{"textgrid.poem.35734": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Gesang und Tanz sei gefeiert,", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Gesang und Tanz sei gefeiert,", "tokens": ["Mit", "Ge\u00b7sang", "und", "Tanz", "sei", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O du Tag, und o Nacht, auch du!", "tokens": ["O", "du", "Tag", ",", "und", "o", "Nacht", ",", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "KON", "FM", "NN", "$,", "ADV", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Denn er k\u00f6mmt, der Fried', und erneuert", "tokens": ["Denn", "er", "k\u00f6mmt", ",", "der", "Fried'", ",", "und", "er\u00b7neu\u00b7ert"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "KON", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Gefild' uns mit Heil und Ruh!", "tokens": ["Die", "Ge\u00b7fild'", "uns", "mit", "Heil", "und", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Von der Grenze kehrt, wer gestritten,", "tokens": ["Von", "der", "Gren\u00b7ze", "kehrt", ",", "wer", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PWS", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit der Eichen Laub' in die H\u00fctten!", "tokens": ["Mit", "der", "Ei\u00b7chen", "Laub'", "in", "die", "H\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "O wie eilt ihr Gang", "tokens": ["O", "wie", "eilt", "ihr", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In der Trommeln Klang,", "tokens": ["In", "der", "Trom\u00b7meln", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "In der H\u00f6rner Get\u00f6n und dem Siegsgesang!", "tokens": ["In", "der", "H\u00f6r\u00b7ner", "Ge\u00b7t\u00f6n", "und", "dem", "Siegs\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wer daheim in Angst sich gegr\u00e4met,", "tokens": ["Wer", "da\u00b7heim", "in", "Angst", "sich", "ge\u00b7gr\u00e4\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O hinaus, und begr\u00fc\u00dft das Heer,", "tokens": ["O", "hin\u00b7aus", ",", "und", "be\u00b7gr\u00fc\u00dft", "das", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mit der Lieb' Umarmung, und nehmet", "tokens": ["Mit", "der", "Lieb'", "Um\u00b7ar\u00b7mung", ",", "und", "neh\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Gep\u00e4ck und das Mordgewehr!", "tokens": ["Das", "Ge\u00b7p\u00e4ck", "und", "das", "Mord\u00b7ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ja er lebt, dein Sohn, du Betr\u00fcbter!", "tokens": ["Ja", "er", "lebt", ",", "dein", "Sohn", ",", "du", "Be\u00b7tr\u00fcb\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ja er lebt, o Braut, dein Geliebter!", "tokens": ["Ja", "er", "lebt", ",", "o", "Braut", ",", "dein", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ja der Vater lebt!", "tokens": ["Ja", "der", "Va\u00b7ter", "lebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wie er sehnend strebt", "tokens": ["Wie", "er", "seh\u00b7nend", "strebt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Nach der Kindelein Schwarm, und vor Freude bebt!", "tokens": ["Nach", "der", "Kin\u00b7de\u00b7lein", "Schwarm", ",", "und", "vor", "Freu\u00b7de", "bebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sei gegr\u00fc\u00dft in heiligen Narben,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "in", "hei\u00b7li\u00b7gen", "Nar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit Triumph uns gegr\u00fc\u00dft, o Held!", "tokens": ["Mit", "Tri\u00b7umph", "uns", "ge\u00b7gr\u00fc\u00dft", ",", "o", "Held", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Triumph auch gr\u00fc\u00dft sie, die starben", "tokens": ["Mit", "Tri\u00b7umph", "auch", "gr\u00fc\u00dft", "sie", ",", "die", "star\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "$,", "PRELS", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "F\u00fcr Gemein' und Altar im Feld!", "tokens": ["F\u00fcr", "Ge\u00b7mein'", "und", "Al\u00b7tar", "im", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Doch verschont, unr\u00fchmliche Z\u00e4hren,", "tokens": ["Doch", "ver\u00b7schont", ",", "un\u00b7r\u00fchm\u00b7li\u00b7che", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die geweihte Gruft zu entehren!", "tokens": ["Die", "ge\u00b7weih\u00b7te", "Gruft", "zu", "en\u00b7teh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Es belohnt, o Wais',", "tokens": ["Es", "be\u00b7lohnt", ",", "o", "Wais'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "FM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und o Witw' und Greis,", "tokens": ["Und", "o", "Wit\u00b7w'", "und", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "NN", "KON", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Es belohnt die Gemein' euch mit Kost und Preis!", "tokens": ["Es", "be\u00b7lohnt", "die", "Ge\u00b7mein'", "euch", "mit", "Kost", "und", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wie umzog uns schwarz das Gewitter", "tokens": ["Wie", "um\u00b7zog", "uns", "schwarz", "das", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Verschwornen zu Fu\u00df und Ro\u00df:", "tokens": ["Der", "Ver\u00b7schwor\u00b7nen", "zu", "Fu\u00df", "und", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Tyrannen Schwarm, und der Ritter,", "tokens": ["Der", "Ty\u00b7ran\u00b7nen", "Schwarm", ",", "und", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein unz\u00e4hlbarer Mietlingstro\u00df!", "tokens": ["Ein", "un\u00b7z\u00e4hl\u00b7ba\u00b7rer", "Miet\u00b7lings\u00b7tro\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ein Hauch verweht das Get\u00fcmmel;", "tokens": ["Doch", "ein", "Hauch", "ver\u00b7weht", "das", "Ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und es strahlt die Sonn' an dem Himmel.", "tokens": ["Und", "es", "strahlt", "die", "Sonn'", "an", "dem", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nun beginnt der Tanz", "tokens": ["Nun", "be\u00b7ginnt", "der", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In dem Eichenkranz", "tokens": ["In", "dem", "Ei\u00b7chen\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Um der Freiheit Altar und des Vaterlands!", "tokens": ["Um", "der", "Frei\u00b7heit", "Al\u00b7tar", "und", "des", "Va\u00b7ter\u00b7lands", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "KON", "ART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Nun erhebt euch, frei der Befehdung,", "tokens": ["Nun", "er\u00b7hebt", "euch", ",", "frei", "der", "Be\u00b7feh\u00b7dung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die Gewerb', und das Land zu baun:", "tokens": ["Die", "Ge\u00b7werb'", ",", "und", "das", "Land", "zu", "baun", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df erbl\u00fchn von Flei\u00df aus Ver\u00f6dung", "tokens": ["Da\u00df", "er\u00b7bl\u00fchn", "von", "Flei\u00df", "aus", "Ve\u00b7r\u00f6\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Verbr\u00fcderten Berg' und Au'n.", "tokens": ["Der", "Ver\u00b7br\u00fc\u00b7der\u00b7ten", "Ber\u00b7g'", "und", "Au'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "NE"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Dem Gebornen pflanzt und dem Gatten;", "tokens": ["Dem", "Ge\u00b7bor\u00b7nen", "pflanzt", "und", "dem", "Gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und der S\u00e4ugling spiel' in dem Schatten!", "tokens": ["Und", "der", "S\u00e4ug\u00b7ling", "spiel'", "in", "dem", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kein Bezwinger schw\u00e4cht", "tokens": ["Kein", "Be\u00b7zwin\u00b7ger", "schw\u00e4cht"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Uns Gesetz und Recht;", "tokens": ["Uns", "Ge\u00b7setz", "und", "Recht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Es gebeut uns kein Herr, es gehorcht kein Knecht!", "tokens": ["Es", "ge\u00b7beut", "uns", "kein", "Herr", ",", "es", "ge\u00b7horcht", "kein", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "O du Vaterland der Gemeine,", "tokens": ["O", "du", "Va\u00b7ter\u00b7land", "der", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die f\u00fcr all' und f\u00fcr einen wirbt,", "tokens": ["Die", "f\u00fcr", "all'", "und", "f\u00fcr", "ei\u00b7nen", "wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "KON", "APPR", "ART", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo f\u00fcr aller Wohl auch der eine", "tokens": ["Wo", "f\u00fcr", "al\u00b7ler", "Wohl", "auch", "der", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "ADV", "ART", "ART"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Entschlossenheit lebt und stirbt!", "tokens": ["Mit", "Ent\u00b7schlos\u00b7sen\u00b7heit", "lebt", "und", "stirbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wir Vereinten schw\u00f6ren dir wieder,", "tokens": ["Wir", "Ver\u00b7ein\u00b7ten", "schw\u00f6\u00b7ren", "dir", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zu beharren frei und wie Br\u00fcder!", "tokens": ["Zu", "be\u00b7har\u00b7ren", "frei", "und", "wie", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "PWAV", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ja mit Herz und Hand", "tokens": ["Ja", "mit", "Herz", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sei gekn\u00fcpft das Band", "tokens": ["Sei", "ge\u00b7kn\u00fcpft", "das", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "F\u00fcr Gemein' und Altar, o du Vaterland!", "tokens": ["F\u00fcr", "Ge\u00b7mein'", "und", "Al\u00b7tar", ",", "o", "du", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "FM", "PPER", 