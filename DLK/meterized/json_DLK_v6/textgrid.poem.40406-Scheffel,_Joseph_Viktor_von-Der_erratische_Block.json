{"textgrid.poem.40406": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der erratische Block", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst ziert' ich, den \u00c4ther durchsp\u00e4hend,", "tokens": ["Einst", "ziert'", "ich", ",", "den", "\u00c4\u00b7ther", "durch\u00b7sp\u00e4\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als Spitze des Urgebirgs Stock,", "tokens": ["Als", "Spit\u00b7ze", "des", "Ur\u00b7ge\u00b7birgs", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ruhm, Hoheit und Stellung verschm\u00e4hend,", "tokens": ["Ruhm", ",", "Ho\u00b7heit", "und", "Stel\u00b7lung", "ver\u00b7schm\u00e4\u00b7hend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ward ich zum erratischen Block.", "tokens": ["Ward", "ich", "zum", "er\u00b7ra\u00b7ti\u00b7schen", "Block", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Man sagt, wenn's dem Denker zu wohl ist,", "tokens": ["Man", "sagt", ",", "wenn's", "dem", "Den\u00b7ker", "zu", "wohl", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "ADV", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wagt er sich kecklich aufs Eis:", "tokens": ["So", "wagt", "er", "sich", "keck\u00b7lich", "aufs", "Eis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mir winkten, wo's kl\u00fcftig und hohl ist,", "tokens": ["Mir", "wink\u00b7ten", ",", "wo's", "kl\u00fcf\u00b7tig", "und", "hohl", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schneejungfraun, verf\u00fchrend und wei\u00df.", "tokens": ["Schnee\u00b7jung\u00b7fraun", ",", "ver\u00b7f\u00fch\u00b7rend", "und", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Doch als ich mit Poltern und L\u00e4rmen", "tokens": ["Doch", "als", "ich", "mit", "Pol\u00b7tern", "und", "L\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Abst\u00fcrzend aufs Firnfeld mich hub,", "tokens": ["Ab\u00b7st\u00fcr\u00b7zend", "aufs", "Firn\u00b7feld", "mich", "hub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Verb\u00fc\u00dft' ich mein jugendlich Schw\u00e4rmen", "tokens": ["Ver\u00b7b\u00fc\u00dft'", "ich", "mein", "ju\u00b7gend\u00b7lich", "Schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit tausendj\u00e4hrigem Schub.", "tokens": ["Mit", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gem", "Schub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Scharf wies mir der Gletscher die Z\u00e4hne:", "tokens": ["Scharf", "wies", "mir", "der", "Glet\u00b7scher", "die", "Z\u00e4h\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u00bbhier, Springinsland, wirst du poliert,", "tokens": ["\u00bb", "hier", ",", "Sprin\u00b7gins\u00b7land", ",", "wirst", "du", "po\u00b7liert", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und im Schutt meiner gro\u00dfen Mor\u00e4ne", "tokens": ["Und", "im", "Schutt", "mei\u00b7ner", "gro\u00b7\u00dfen", "Mo\u00b7r\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Als Fremder talab transportiert.\u00ab", "tokens": ["Als", "Frem\u00b7der", "ta\u00b7lab", "trans\u00b7por\u00b7tiert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Geritzt und gekritzt und geschoben", "tokens": ["Ge\u00b7ritzt", "und", "ge\u00b7kritzt", "und", "ge\u00b7scho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "KON", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Entrollt' ich in spaltige Schluft,", "tokens": ["Ent\u00b7rollt'", "ich", "in", "spal\u00b7ti\u00b7ge", "Schluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ward sto\u00dfweis nach oben gehoben,", "tokens": ["Ward", "sto\u00df\u00b7weis", "nach", "o\u00b7ben", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gew\u00e4lzt und gepufft und geknufft.", "tokens": ["Ge\u00b7w\u00e4lzt", "und", "ge\u00b7pufft", "und", "ge\u00b7knufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Da bleib' einer sauber und munter", "tokens": ["Da", "bleib'", "ei\u00b7ner", "sau\u00b7ber", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In solchem Gerutsch und Geschlamm;", "tokens": ["In", "sol\u00b7chem", "Ge\u00b7rutsch", "und", "Ge\u00b7schlamm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "... Ich kam immer tiefer herunter,", "tokens": ["...", "Ich", "kam", "im\u00b7mer", "tie\u00b7fer", "her\u00b7un\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bis der Eiswall ins Urmeer zerschwamm.", "tokens": ["Bis", "der", "Eis\u00b7wall", "ins", "Ur\u00b7meer", "zer\u00b7schwamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und der spielt die traurigste Rolle,", "tokens": ["Und", "der", "spielt", "die", "trau\u00b7rigs\u00b7te", "Rol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem die Basis mit Grundeis ergeht ...", "tokens": ["Dem", "die", "Ba\u00b7sis", "mit", "Grun\u00b7deis", "er\u00b7geht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ich wurde auf treibender Scholle", "tokens": ["Ich", "wur\u00b7de", "auf", "trei\u00b7ben\u00b7der", "Schol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In des Ozeans Brandung verweht.", "tokens": ["In", "des", "O\u00b7ze\u00b7ans", "Bran\u00b7dung", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Plimp, plump! Da ging ich zugrunde,", "tokens": ["Plimp", ",", "plump", "!", "Da", "ging", "ich", "zu\u00b7grun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lag elend versunken und schlief,", "tokens": ["Lag", "e\u00b7lend", "ver\u00b7sun\u00b7ken", "und", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bis in sp\u00e4t erst erl\u00f6sender Stunde", "tokens": ["Bis", "in", "sp\u00e4t", "erst", "er\u00b7l\u00f6\u00b7sen\u00b7der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich Gletscher und S\u00fcndflut verlief.", "tokens": ["Sich", "Glet\u00b7scher", "und", "S\u00fcnd\u00b7flut", "ver\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Den entw\u00e4sserten Seegrund verkl\u00e4rte", "tokens": ["Den", "ent\u00b7w\u00e4s\u00b7ser\u00b7ten", "See\u00b7grund", "ver\u00b7kl\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die Sonne mit w\u00e4rmerem Strahl,", "tokens": ["Die", "Son\u00b7ne", "mit", "w\u00e4r\u00b7me\u00b7rem", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mit der Rhinozerosherde", "tokens": ["Und", "mit", "der", "Rhi\u00b7no\u00b7ze\u00b7ros\u00b7her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Spazierte der Mammut durchs Tal.", "tokens": ["Spa\u00b7zier\u00b7te", "der", "Mam\u00b7mut", "durchs", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Nun lagern wir Eiszeitschubisten", "tokens": ["Nun", "la\u00b7gern", "wir", "Eis\u00b7zeit\u00b7schu\u00b7bis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nutzbringend als steinerne Saat", "tokens": ["Nutz\u00b7brin\u00b7gend", "als", "stei\u00b7ner\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dienen dem Heiden wie Christen", "tokens": ["Und", "die\u00b7nen", "dem", "Hei\u00b7den", "wie", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als Baustoff f\u00fcr Kirche und Staat.", "tokens": ["Als", "Bau\u00b7stoff", "f\u00fcr", "Kir\u00b7che", "und", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Dies Lied ist zwei Forschern gelungen", "tokens": ["Dies", "Lied", "ist", "zwei", "For\u00b7schern", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "CARD", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Gau zwischen Aare und Reu\u00df;", "tokens": ["Im", "Gau", "zwi\u00b7schen", "Aa\u00b7re", "und", "Reu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "KON", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Wirtshaus, in dem sie es sungen,", "tokens": ["Das", "Wirts\u00b7haus", ",", "in", "dem", "sie", "es", "sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War ganz von erratischem Gneus.", "tokens": ["War", "ganz", "von", "er\u00b7ra\u00b7ti\u00b7schem", "Gneus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Sie sungen es ernst und dramatisch", "tokens": ["Sie", "sun\u00b7gen", "es", "ernst", "und", "dra\u00b7ma\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In die Findlinglandschaft hinein", "tokens": ["In", "die", "Find\u00b7ling\u00b7land\u00b7schaft", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und schoben sich selbst dann erratisch", "tokens": ["Und", "scho\u00b7ben", "sich", "selbst", "dann", "er\u00b7ra\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Holpern und Stolpern vom Wein.", "tokens": ["Mit", "Hol\u00b7pern", "und", "Stol\u00b7pern", "vom", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}