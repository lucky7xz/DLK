{"textgrid.poem.55979": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Orpheus. Eurydike. Hermes", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der Seelen wunderliches Bergwerk.", "tokens": ["Das", "war", "der", "See\u00b7len", "wun\u00b7der\u00b7li\u00b7ches", "Berg\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie stille Silbererze gingen sie", "tokens": ["Wie", "stil\u00b7le", "Sil\u00b7be\u00b7rer\u00b7ze", "gin\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als Adern durch sein Dunkel. Zwischen Wurzeln", "tokens": ["als", "A\u00b7dern", "durch", "sein", "Dun\u00b7kel", ".", "Zwi\u00b7schen", "Wur\u00b7zeln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "entsprang das Blut, das fortgeht zu den Menschen,", "tokens": ["ent\u00b7sprang", "das", "Blut", ",", "das", "fort\u00b7geht", "zu", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und schwer wie Porphyr sah es aus im Dunkel.", "tokens": ["und", "schwer", "wie", "Por\u00b7phyr", "sah", "es", "aus", "im", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sonst war nichts Rotes.", "tokens": ["Sonst", "war", "nichts", "Ro\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Felsen waren da", "tokens": ["Fel\u00b7sen", "wa\u00b7ren", "da"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "und wesenlose W\u00e4lder. Br\u00fccken \u00fcber Leeres", "tokens": ["und", "we\u00b7sen\u00b7lo\u00b7se", "W\u00e4l\u00b7der", ".", "Br\u00fc\u00b7cken", "\u00fc\u00b7ber", "Lee\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und jener gro\u00dfe graue blinde Teich,", "tokens": ["und", "je\u00b7ner", "gro\u00b7\u00dfe", "grau\u00b7e", "blin\u00b7de", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der \u00fcber seinem fernen Grunde hing", "tokens": ["der", "\u00fc\u00b7ber", "sei\u00b7nem", "fer\u00b7nen", "Grun\u00b7de", "hing"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "wie Regenhimmel \u00fcber einer Landschaft.", "tokens": ["wie", "Re\u00b7gen\u00b7him\u00b7mel", "\u00fc\u00b7ber", "ei\u00b7ner", "Land\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und zwischen Wiesen, sanft und voller Langmut,", "tokens": ["Und", "zwi\u00b7schen", "Wie\u00b7sen", ",", "sanft", "und", "vol\u00b7ler", "Lang\u00b7mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "erschien des einen Weges blasser Streifen,", "tokens": ["er\u00b7schien", "des", "ei\u00b7nen", "We\u00b7ges", "blas\u00b7ser", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "wie eine lange Bleiche hingelegt.", "tokens": ["wie", "ei\u00b7ne", "lan\u00b7ge", "Blei\u00b7che", "hin\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und dieses einen Weges kamen sie.", "tokens": ["Und", "die\u00b7ses", "ei\u00b7nen", "We\u00b7ges", "ka\u00b7men", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Voran der schlanke Mann im blauen Mantel,", "tokens": ["Vo\u00b7ran", "der", "schlan\u00b7ke", "Mann", "im", "blau\u00b7en", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der stumm und ungeduldig vor sich aussah.", "tokens": ["der", "stumm", "und", "un\u00b7ge\u00b7dul\u00b7dig", "vor", "sich", "aus\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ohne zu kauen fra\u00df sein Schritt den Weg", "tokens": ["Oh\u00b7ne", "zu", "kau\u00b7en", "fra\u00df", "sein", "Schritt", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "in gro\u00dfen Bissen; seine H\u00e4nde hingen", "tokens": ["in", "gro\u00b7\u00dfen", "Bis\u00b7sen", ";", "sei\u00b7ne", "H\u00e4n\u00b7de", "hin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "schwer und verschlossen aus dem Fall der Falten", "tokens": ["schwer", "und", "ver\u00b7schlos\u00b7sen", "aus", "dem", "Fall", "der", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVPP", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "und wu\u00dften nicht mehr von der leichten Leier,", "tokens": ["und", "wu\u00df\u00b7ten", "nicht", "mehr", "von", "der", "leich\u00b7ten", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "die in die Linke eingewachsen war", "tokens": ["die", "in", "die", "Lin\u00b7ke", "ein\u00b7ge\u00b7wach\u00b7sen", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "wie Rosenranken in den Ast des \u00d6lbaums.", "tokens": ["wie", "Ro\u00b7sen\u00b7ran\u00b7ken", "in", "den", "Ast", "des", "\u00d6l\u00b7baums", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und seine Sinne waren wie entzweit:", "tokens": ["Und", "sei\u00b7ne", "Sin\u00b7ne", "wa\u00b7ren", "wie", "ent\u00b7zweit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "indes der Blick ihm wie ein Hund vorauslief,", "tokens": ["in\u00b7des", "der", "Blick", "ihm", "wie", "ein", "Hund", "vor\u00b7aus\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "umkehrte, kam und immer wieder weit", "tokens": ["um\u00b7kehr\u00b7te", ",", "kam", "und", "im\u00b7mer", "wie\u00b7der", "weit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und wartend an der n\u00e4chsten Wendung stand, \u2013", "tokens": ["und", "war\u00b7tend", "an", "der", "n\u00e4chs\u00b7ten", "Wen\u00b7dung", "stand", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "blieb sein Geh\u00f6r wie ein Geruch zur\u00fcck.", "tokens": ["blieb", "sein", "Ge\u00b7h\u00f6r", "wie", "ein", "Ge\u00b7ruch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Manchmal erschien es ihm als reichte es", "tokens": ["Manch\u00b7mal", "er\u00b7schien", "es", "ihm", "als", "reich\u00b7te", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KOKOM", "VVFIN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "bis an das Gehen jener beiden andern,", "tokens": ["bis", "an", "das", "Ge\u00b7hen", "je\u00b7ner", "bei\u00b7den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PDAT", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "die folgen sollten diesen ganzen Aufstieg.", "tokens": ["die", "fol\u00b7gen", "soll\u00b7ten", "die\u00b7sen", "gan\u00b7zen", "Auf\u00b7stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Dann wieder wars nur seines Steigens Nachklang", "tokens": ["Dann", "wie\u00b7der", "wars", "nur", "sei\u00b7nes", "Stei\u00b7gens", "Nach\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "und seines Mantels Wind was hinter ihm war.", "tokens": ["und", "sei\u00b7nes", "Man\u00b7tels", "Wind", "was", "hin\u00b7ter", "ihm", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PWS", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Er aber sagte sich, sie k\u00e4men doch;", "tokens": ["Er", "a\u00b7ber", "sag\u00b7te", "sich", ",", "sie", "k\u00e4\u00b7men", "doch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "sagte es laut und h\u00f6rte sich verhallen.", "tokens": ["sag\u00b7te", "es", "laut", "und", "h\u00f6r\u00b7te", "sich", "ver\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PRF", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Sie k\u00e4men doch, nur w\u00e4rens zwei", "tokens": ["Sie", "k\u00e4\u00b7men", "doch", ",", "nur", "w\u00e4\u00b7rens", "zwei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "die furchtbar leise gingen. D\u00fcrfte er", "tokens": ["die", "furcht\u00b7bar", "lei\u00b7se", "gin\u00b7gen", ".", "D\u00fcrf\u00b7te", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "$.", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "sich einmal wenden (w\u00e4re das Zur\u00fcckschaun", "tokens": ["sich", "ein\u00b7mal", "wen\u00b7den", "(", "w\u00e4\u00b7re", "das", "Zu\u00b7r\u00fcck\u00b7schaun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "VVINF", "$(", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "nicht die Zersetzung dieses ganzen Werkes,", "tokens": ["nicht", "die", "Zer\u00b7set\u00b7zung", "die\u00b7ses", "gan\u00b7zen", "Wer\u00b7kes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "das erst vollbracht wird), m\u00fc\u00dfte er sie sehen,", "tokens": ["das", "erst", "voll\u00b7bracht", "wird", ")", ",", "m\u00fc\u00df\u00b7te", "er", "sie", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "$(", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "die beiden Leisen, die ihm schweigend nachgehn:", "tokens": ["die", "bei\u00b7den", "Lei\u00b7sen", ",", "die", "ihm", "schwei\u00b7gend", "nach\u00b7gehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Den Gott des Ganges und der weiten Botschaft,", "tokens": ["Den", "Gott", "des", "Gan\u00b7ges", "und", "der", "wei\u00b7ten", "Bot\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Reisehaube \u00fcber hellen Augen,", "tokens": ["die", "Rei\u00b7se\u00b7hau\u00b7be", "\u00fc\u00b7ber", "hel\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den schlanken Stab hertragend vor dem Leibe", "tokens": ["den", "schlan\u00b7ken", "Stab", "her\u00b7tra\u00b7gend", "vor", "dem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und fl\u00fcgelschlagend an den Fu\u00dfgelenken;", "tokens": ["und", "fl\u00fc\u00b7gel\u00b7schla\u00b7gend", "an", "den", "Fu\u00df\u00b7ge\u00b7len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und seiner linken Hand gegeben: ", "tokens": ["und", "sei\u00b7ner", "lin\u00b7ken", "Hand", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die So-geliebte, da\u00df aus einer Leier", "tokens": ["Die", "So\u00b7ge\u00b7lieb\u00b7te", ",", "da\u00df", "aus", "ei\u00b7ner", "Lei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mehr Klage kam als je aus Klagefrauen;", "tokens": ["mehr", "Kla\u00b7ge", "kam", "als", "je", "aus", "Kla\u00b7ge\u00b7frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KOKOM", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df eine Welt aus Klage ward, in der", "tokens": ["da\u00df", "ei\u00b7ne", "Welt", "aus", "Kla\u00b7ge", "ward", ",", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VAFIN", "$,", "APPR", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, 