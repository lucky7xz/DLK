{"dta.poem.11701": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fcckenb\u00fcsser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehochehrw\u00fcrdiger Herr\u201c, so h\u00e4tt' ich gerne geschrieben,", "tokens": ["\u201e", "ho\u00b7ch\u00b7ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Herr", "\u201c", ",", "so", "h\u00e4tt'", "ich", "ger\u00b7ne", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$(", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber die Ehre schien mir fast und die W\u00fcrde zu hoch;", "tokens": ["A\u00b7ber", "die", "Eh\u00b7re", "schien", "mir", "fast", "und", "die", "W\u00fcr\u00b7de", "zu", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "KON", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Euch verdro\u00df inde\u00df mein ", "tokens": ["Euch", "ver\u00b7dro\u00df", "in\u00b7de\u00df", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ueber den Brief; denkt Euch ", "tokens": ["Ue\u00b7ber", "den", "Brief", ";", "denkt", "Euch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}