{"textgrid.poem.55770": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Innerstes wollt Ihr nach au\u00dfen kehren,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Innerstes wollt Ihr nach au\u00dfen kehren,", "tokens": ["Mein", "In\u00b7ners\u00b7tes", "wollt", "Ihr", "nach", "au\u00b7\u00dfen", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Betasten wollt Ihr meiner Seele Schw\u00e4ren,", "tokens": ["Be\u00b7tas\u00b7ten", "wollt", "Ihr", "mei\u00b7ner", "See\u00b7le", "Schw\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durchw\u00fchlen wollt Ihr branderfa\u00dfte Wunden,", "tokens": ["Durch\u00b7w\u00fch\u00b7len", "wollt", "Ihr", "bran\u00b7der\u00b7fa\u00df\u00b7te", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis Ihr zum Sitz der Qual Euch durchgefunden,", "tokens": ["Bis", "Ihr", "zum", "Sitz", "der", "Qual", "Euch", "durch\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erbarmungslos wollt Ihr das Spiel der Nerven", "tokens": ["Er\u00b7bar\u00b7mungs\u00b7los", "wollt", "Ihr", "das", "Spiel", "der", "Ner\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zum wahnwitzw\u00fcsten Wirbeltanz versch\u00e4rfen. \u2013 \u2013", "tokens": ["Zum", "wahn\u00b7witz\u00b7w\u00fcs\u00b7ten", "Wir\u00b7bel\u00b7tanz", "ver\u00b7sch\u00e4r\u00b7fen", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, wenn Ihr k\u00e4mt zu retten und zu heilen,", "tokens": ["Ja", ",", "wenn", "Ihr", "k\u00e4mt", "zu", "ret\u00b7ten", "und", "zu", "hei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr aber wollt Euch an der Qual begeilen,", "tokens": ["Ihr", "a\u00b7ber", "wollt", "Euch", "an", "der", "Qual", "be\u00b7gei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Berauschen wollt Ihr Euch an Martert\u00f6nen", "tokens": ["Be\u00b7rau\u00b7schen", "wollt", "Ihr", "Euch", "an", "Mar\u00b7ter\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Eurer Henker-Wollust wollt Ihr fr\u00f6hnen.", "tokens": ["Und", "Eu\u00b7rer", "Hen\u00b7ker\u00b7Wol\u00b7lust", "wollt", "Ihr", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O h\u00fctet Euch, ich bin ein Andrer worden,", "tokens": ["O", "h\u00fc\u00b7tet", "Euch", ",", "ich", "bin", "ein", "A\u00b7ndrer", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin Keiner mehr vom zahmen Dulderorden;", "tokens": ["Bin", "Kei\u00b7ner", "mehr", "vom", "zah\u00b7men", "Dul\u00b7der\u00b7or\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich la\u00df mich nicht auf Eure Folter schn\u00fcren,", "tokens": ["Ich", "la\u00df", "mich", "nicht", "auf", "Eu\u00b7re", "Fol\u00b7ter", "schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich la\u00df mich nicht durchstochern und durchsp\u00fcren.", "tokens": ["Ich", "la\u00df", "mich", "nicht", "durch\u00b7sto\u00b7chern", "und", "durch\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Fort, fort, hinweg, Ihr ahnt nicht, was Euch droht,", "tokens": ["Fort", ",", "fort", ",", "hin\u00b7weg", ",", "Ihr", "ahnt", "nicht", ",", "was", "Euch", "droht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor meinen Augen flirrt es blutig rot.", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", "flirrt", "es", "blu\u00b7tig", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}