{"textgrid.poem.59502": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Stammbuchblatt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Liebe? Eine zarte Blume,", "tokens": ["Was", "ist", "Lie\u00b7be", "?", "Ei\u00b7ne", "zar\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die zerflattert, wenn die Hand sie pfl\u00fcckt,", "tokens": ["Die", "zer\u00b7flat\u00b7tert", ",", "wenn", "die", "Hand", "sie", "pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine G\u00f6ttin, die im Heiligtume", "tokens": ["Ei\u00b7ne", "G\u00f6t\u00b7tin", ",", "die", "im", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur durch Anschaun Sterbliche begl\u00fcckt,", "tokens": ["Nur", "durch", "An\u00b7schaun", "Sterb\u00b7li\u00b7che", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Eine Biene, die mit leichtem Wallen", "tokens": ["Ei\u00b7ne", "Bie\u00b7ne", ",", "die", "mit", "leich\u00b7tem", "Wal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenig Stunden um die Kelche summt,", "tokens": ["We\u00b7nig", "Stun\u00b7den", "um", "die", "Kel\u00b7che", "summt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Eine Melodie der Nachtigallen,", "tokens": ["Ei\u00b7ne", "Me\u00b7lo\u00b7die", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die nach kurzem Lenz verstummt.", "tokens": ["Die", "nach", "kur\u00b7zem", "Lenz", "ver\u00b7stummt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Was ist Freundschaft, was ist Seeleng\u00fcte,", "tokens": ["Was", "ist", "Freund\u00b7schaft", ",", "was", "ist", "See\u00b7len\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "PWS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Was der Herzen s\u00fc\u00dfe Sympathie?", "tokens": ["Was", "der", "Her\u00b7zen", "s\u00fc\u00b7\u00dfe", "Sym\u00b7pa\u00b7thie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Ach! aus bessern Welten eine Bl\u00fcte,", "tokens": ["Ach", "!", "aus", "bes\u00b7sern", "Wel\u00b7ten", "ei\u00b7ne", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "In der Erden L\u00fcften reift sie nie.", "tokens": ["In", "der", "Er\u00b7den", "L\u00fcf\u00b7ten", "reift", "sie", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Was ist Tugend? in dem Lumpenkittel", "tokens": ["Was", "ist", "Tu\u00b7gend", "?", "in", "dem", "Lum\u00b7pen\u00b7kit\u00b7tel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Predigt sie: ein Nichts ist Ruhm und Gold!", "tokens": ["Pre\u00b7digt", "sie", ":", "ein", "Nichts", "ist", "Ruhm", "und", "Gold", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "PIS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Was ist Wahrheit? in dem Narrenspittel", "tokens": ["Was", "ist", "Wahr\u00b7heit", "?", "in", "dem", "Nar\u00b7ren\u00b7spit\u00b7tel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Reicht man ihr den Gnadensold.", "tokens": ["Reicht", "man", "ihr", "den", "Gna\u00b7den\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}