{"dta.poem.21233": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.  \n  Das Oestereichische Hausz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist der ewge Baum/ der Deutschland schatten gibt/", "tokens": ["Di\u00df", "ist", "der", "ew\u00b7ge", "Baum", "/", "der", "Deutschland", "schat\u00b7ten", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Sich vom stillen ", "tokens": ["Der", "Sich", "vom", "stil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vnd sich den Himmel l\u00e4st vnd keine Welt bedecken;", "tokens": ["Vnd", "sich", "den", "Him\u00b7mel", "l\u00e4st", "vnd", "kei\u00b7ne", "Welt", "be\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der vnter Luna Krei\u00df kein ", "tokens": ["Der", "vn\u00b7ter", "Lu\u00b7na", "Krei\u00df", "kein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN", "PIAT"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was wilt du/ sihst du nicht wie Er veranckert steht?", "tokens": ["Was", "wilt", "du", "/", "sihst", "du", "nicht", "wie", "Er", "ver\u00b7an\u00b7ckert", "steht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$(", "VVFIN", "PPER", "PTKNEG", "PWAV", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "St\u00fcrm\u2019 jmmer rawer ", "tokens": ["St\u00fcrm'", "jm\u00b7mer", "ra\u00b7wer"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du wirst hie keinen Ast von diesem ", "tokens": ["Du", "wirst", "hie", "kei\u00b7nen", "Ast", "von", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ", "tokens": ["Dann"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Dann Vnsrer Reiche Schutz/ sind nicht Gewehr vnd Waffen/", "tokens": ["Dann", "Vns\u00b7rer", "Rei\u00b7che", "Schutz", "/", "sind", "nicht", "Ge\u00b7wehr", "vnd", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "NN", "$(", "VAFIN", "PTKNEG", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey denen ", "tokens": ["Bey", "de\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PRELS"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Di\u00df ist\u2019s/ da\u00df dieser ", "tokens": ["Di\u00df", "ist's", "/", "da\u00df", "die\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "$(", "KOUS", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}}}}}