{"dta.poem.9233": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n Der betr\u00fcbte Abschied.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "So werd ich nun von dir getrieben/", "tokens": ["So", "werd", "ich", "nun", "von", "dir", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du allerliebste Marilis!", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7te", "Ma\u00b7ri\u00b7lis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und soll ich dich vergebens lieben?", "tokens": ["Und", "soll", "ich", "dich", "ver\u00b7ge\u00b7bens", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn dieses ist nun mehr gewi\u00df/", "tokens": ["Denn", "die\u00b7ses", "ist", "nun", "mehr", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach dem ich meinen schlu\u00df gelesen/", "tokens": ["Nach", "dem", "ich", "mei\u00b7nen", "schlu\u00df", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin am l\u00e4ngsten hier gewesen.", "tokens": ["Ich", "bin", "am", "l\u00e4ngs\u00b7ten", "hier", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Nun werd ich dich gesegnen m\u00fcssen/", "tokens": ["Nun", "werd", "ich", "dich", "ge\u00b7seg\u00b7nen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein kind! ich h\u00e4tte zwar vermeint/", "tokens": ["Mein", "kind", "!", "ich", "h\u00e4t\u00b7te", "zwar", "ver\u00b7meint", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wolte deiner noch geniessen:", "tokens": ["Ich", "wol\u00b7te", "dei\u00b7ner", "noch", "ge\u00b7nies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mein verh\u00e4ngni\u00df ist mir feind/", "tokens": ["Doch", "mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "ist", "mir", "feind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und r\u00fccket mir in einer stunde/", "tokens": ["Und", "r\u00fc\u00b7cket", "mir", "in", "ei\u00b7ner", "stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das liebste bi\u00dfgen au\u00df dem munde", "tokens": ["Das", "liebs\u00b7te", "bi\u00df\u00b7gen", "au\u00df", "dem", "mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. F\u00fcrwar ich wolte gerne leiden/", "tokens": ["F\u00fcr\u00b7war", "ich", "wol\u00b7te", "ger\u00b7ne", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was immer zu erleiden steht/", "tokens": ["Was", "im\u00b7mer", "zu", "er\u00b7lei\u00b7den", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und solt ich alle lust vermeiden/", "tokens": ["Und", "solt", "ich", "al\u00b7le", "lust", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir doch sonst zu hertzen geht/", "tokens": ["Die", "mir", "doch", "sonst", "zu", "hert\u00b7zen", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn mir das gl\u00fccke nur verg\u00fcnnte/", "tokens": ["Wenn", "mir", "das", "gl\u00fc\u00b7cke", "nur", "ver\u00b7g\u00fcnn\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich bey dir verbleiben k\u00f6nnte.", "tokens": ["Da\u00df", "ich", "bey", "dir", "ver\u00b7blei\u00b7ben", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "4. Doch hast du irgend meine schmertzen", "tokens": ["Doch", "hast", "du", "ir\u00b7gend", "mei\u00b7ne", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit kalten augen angeschaut/", "tokens": ["Mit", "kal\u00b7ten", "au\u00b7gen", "an\u00b7ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hast du meinem frommen hertzen", "tokens": ["Und", "hast", "du", "mei\u00b7nem", "from\u00b7men", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die redlichkeit nicht zugetraut/", "tokens": ["Die", "red\u00b7lich\u00b7keit", "nicht", "zu\u00b7ge\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gieb mir doch auff solche weise/", "tokens": ["So", "gieb", "mir", "doch", "auff", "sol\u00b7che", "wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PIAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein freundlich ansehn auff die reise.", "tokens": ["Ein", "freund\u00b7lich", "an\u00b7sehn", "auff", "die", "rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Dein leben hat mir wohl gefallen/", "tokens": ["Dein", "le\u00b7ben", "hat", "mir", "wohl", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein sch\u00f6ner wandel war mein liecht:", "tokens": ["Dein", "sch\u00f6\u00b7ner", "wan\u00b7del", "war", "mein", "liecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wol du weist von diesen allen/", "tokens": ["Wie", "wol", "du", "weist", "von", "die\u00b7sen", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "APPR", "PDAT", "PIAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich wie es scheint/ die helffte nicht/", "tokens": ["Gleich", "wie", "es", "scheint", "/", "die", "helff\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$(", "ART", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ich behielt die stillen sorgen", "tokens": ["Denn", "ich", "be\u00b7hielt", "die", "stil\u00b7len", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In meiner engen brust verborgen.", "tokens": ["In", "mei\u00b7ner", "en\u00b7gen", "brust", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Nun mu\u00df ich mir den z\u00fcgel lassen/", "tokens": ["Nun", "mu\u00df", "ich", "mir", "den", "z\u00fc\u00b7gel", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil doch nicht mehr zu hoffen ist:", "tokens": ["Weil", "doch", "nicht", "mehr", "zu", "hof\u00b7fen", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trete schon auff meine strassen/", "tokens": ["Ich", "tre\u00b7te", "schon", "auff", "mei\u00b7ne", "stras\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wei\u00df wem du bescheret bist/", "tokens": ["Wer", "wei\u00df", "wem", "du", "be\u00b7sche\u00b7ret", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PWS", "PPER", "VVFIN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch r\u00fchm ich des gl\u00fcckes gabe/", "tokens": ["Je\u00b7doch", "r\u00fchm", "ich", "des", "gl\u00fc\u00b7ckes", "ga\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Da\u00df ich dich nur gesehen habe.", "tokens": ["Da\u00df", "ich", "dich", "nur", "ge\u00b7se\u00b7hen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Und wirst du dich mit dem verbinden", "tokens": ["Und", "wirst", "du", "dich", "mit", "dem", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PRF", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dein und meine freundschafft kennt/", "tokens": ["Der", "dein", "und", "mei\u00b7ne", "freund\u00b7schafft", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wirst du doch kein hertze sinden", "tokens": ["So", "wirst", "du", "doch", "kein", "hert\u00b7ze", "sin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das in dergleichen flamme brennt/", "tokens": ["Das", "in", "derg\u00b7lei\u00b7chen", "flam\u00b7me", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt mu\u00df ich mich von hinnen lencken/", "tokens": ["Jetzt", "mu\u00df", "ich", "mich", "von", "hin\u00b7nen", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wirst du wohl an mich gedencken.", "tokens": ["Doch", "wirst", "du", "wohl", "an", "mich", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}