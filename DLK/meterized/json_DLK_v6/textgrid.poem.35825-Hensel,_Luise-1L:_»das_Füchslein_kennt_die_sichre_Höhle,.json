{"textgrid.poem.35825": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdas F\u00fcchslein kennt die sichre H\u00f6hle,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas F\u00fcchslein kennt die sichre H\u00f6hle,", "tokens": ["\u00bb", "das", "F\u00fcchs\u00b7lein", "kennt", "die", "sich\u00b7re", "H\u00f6h\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwalbe klebt ihr Nestlein an \u2013", "tokens": ["Die", "Schwal\u00b7be", "klebt", "ihr", "Nes\u00b7tlein", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, zeige meiner m\u00fcden Seele", "tokens": ["O", ",", "zei\u00b7ge", "mei\u00b7ner", "m\u00fc\u00b7den", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Ort auch, wo sie rasten kann!\u00ab", "tokens": ["Den", "Ort", "auch", ",", "wo", "sie", "ras\u00b7ten", "kann", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So fleht' ich mit gerung'nen H\u00e4nden,", "tokens": ["So", "fleht'", "ich", "mit", "ge\u00b7rung'\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hei\u00dfe Thr\u00e4nen flossen drauf;", "tokens": ["Und", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "flos\u00b7sen", "drauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blicke mu\u00dft' ich sehnend wenden", "tokens": ["Die", "Bli\u00b7cke", "mu\u00dft'", "ich", "seh\u00b7nend", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum fernen Himmelszelt hinauf.", "tokens": ["Zum", "fer\u00b7nen", "Him\u00b7mels\u00b7zelt", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Erde schien mir so verlassen,", "tokens": ["Die", "Er\u00b7de", "schien", "mir", "so", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Heiland, meint' ich, sei so fern;", "tokens": ["Der", "Hei\u00b7land", ",", "meint'", "ich", ",", "sei", "so", "fern", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Tag, die Farben wollt' ich hassen", "tokens": ["Den", "Tag", ",", "die", "Far\u00b7ben", "wollt'", "ich", "has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und einzig suchen meinen Herrn.", "tokens": ["Und", "ein\u00b7zig", "su\u00b7chen", "mei\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wu\u00dft' Ihn nirgend doch zu finden,", "tokens": ["Und", "wu\u00dft'", "Ihn", "nir\u00b7gend", "doch", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fragend blickt' ich himmelw\u00e4rts;", "tokens": ["Und", "fra\u00b7gend", "blickt'", "ich", "him\u00b7mel\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "So sah ich Monde, Jahre schwinden,", "tokens": ["So", "sah", "ich", "Mon\u00b7de", ",", "Jah\u00b7re", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ruhe kam nicht in mein Herz.", "tokens": ["Und", "Ru\u00b7he", "kam", "nicht", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie konnt' ich diese Erde lieben,", "tokens": ["Wie", "konnt'", "ich", "die\u00b7se", "Er\u00b7de", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf der ich Ihn nicht wandeln sah?", "tokens": ["Auf", "der", "ich", "Ihn", "nicht", "wan\u00b7deln", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "PTKNEG", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele sucht' Ihn einzig dr\u00fcben", "tokens": ["Die", "See\u00b7le", "sucht'", "Ihn", "ein\u00b7zig", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ahnte nicht, da\u00df Er so nah'.", "tokens": ["Und", "ahn\u00b7te", "nicht", ",", "da\u00df", "Er", "so", "nah'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bis ich von Seiner Kirche h\u00f6rte,", "tokens": ["Bis", "ich", "von", "Sei\u00b7ner", "Kir\u00b7che", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die auf den wahren Fels gebaut,", "tokens": ["Die", "auf", "den", "wah\u00b7ren", "Fels", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bis Sein Geist den Weg mich lehrte", "tokens": ["Und", "bis", "Sein", "Geist", "den", "Weg", "mich", "lehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur hochgelobten Gottesbraut.", "tokens": ["Zur", "hoch\u00b7ge\u00b7lob\u00b7ten", "Got\u00b7tes\u00b7braut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da sehnt' ich mich nach Seinen Gaben", "tokens": ["Da", "sehnt'", "ich", "mich", "nach", "Sei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nach der Kirche Mutterschoo\u00df,", "tokens": ["Und", "nach", "der", "Kir\u00b7che", "Mut\u00b7ter\u00b7schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und konnte doch den Trost nicht haben,", "tokens": ["Und", "konn\u00b7te", "doch", "den", "Trost", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mich ihr heil'ger Arm umschlo\u00df.", "tokens": ["Da\u00df", "mich", "ihr", "heil'\u00b7ger", "Arm", "um\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bis ich beim Anblick Seiner Wunden", "tokens": ["Bis", "ich", "beim", "An\u00b7blick", "Sei\u00b7ner", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein banges Zagen \u00fcberwand \u2013", "tokens": ["Mein", "ban\u00b7ges", "Za\u00b7gen", "\u00fc\u00b7ber\u00b7wand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hatt' ich meinen Freund gefunden", "tokens": ["Da", "hatt'", "ich", "mei\u00b7nen", "Freund", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Mutterhaus und Vaterland. \u2013", "tokens": ["Und", "Mut\u00b7ter\u00b7haus", "und", "Va\u00b7ter\u00b7land", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das F\u00fcchslein ruht in sichrer H\u00f6hle,", "tokens": ["Das", "F\u00fcchs\u00b7lein", "ruht", "in", "sich\u00b7rer", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schw\u00e4lblein froh im Neste thront;", "tokens": ["Das", "Schw\u00e4lb\u00b7lein", "froh", "im", "Nes\u00b7te", "thront", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Dein Altar ist meiner Seele", "tokens": ["Und", "Dein", "Al\u00b7tar", "ist", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Heimath, wo sie friedlich wohnt.", "tokens": ["Die", "Hei\u00b7math", ",", "wo", "sie", "fried\u00b7lich", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}