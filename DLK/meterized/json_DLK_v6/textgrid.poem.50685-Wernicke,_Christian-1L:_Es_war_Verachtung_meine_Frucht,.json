{"textgrid.poem.50685": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war Verachtung meine Frucht,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war Verachtung meine Frucht,", "tokens": ["Es", "war", "Ver\u00b7ach\u00b7tung", "mei\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang' ich Kreon's Gunst in tiefster Demuth sucht';", "tokens": ["So", "lang'", "ich", "Kre\u00b7on's", "Gunst", "in", "tiefs\u00b7ter", "De\u00b7muth", "sucht'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Erreicht' ich unverhofft durch Keckheit Zweck und Ziel.", "tokens": ["Er\u00b7reicht'", "ich", "un\u00b7ver\u00b7hofft", "durch", "Keck\u00b7heit", "Zweck", "und", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er war besorgt f\u00fcr seine Ruh'", "tokens": ["Er", "war", "be\u00b7sorgt", "f\u00fcr", "sei\u00b7ne", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schrieb mir einen Brief voll Anerbietung zu.", "tokens": ["Und", "schrieb", "mir", "ei\u00b7nen", "Brief", "voll", "An\u00b7er\u00b7bie\u00b7tung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich las ihn und erkannt' hierbei,", "tokens": ["Ich", "las", "ihn", "und", "er\u00b7kannt'", "hier\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df eines Narren Gunst gleich seinem Briefe sei, \u2013", "tokens": ["Da\u00df", "ei\u00b7nes", "Nar\u00b7ren", "Gunst", "gleich", "sei\u00b7nem", "Brie\u00b7fe", "sei", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}