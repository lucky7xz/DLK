{"textgrid.poem.57173": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[nun wohne du darin]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wohne DU darin,", "tokens": ["Nun", "woh\u00b7ne", "Du", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "in diesem leeren Hause,", "tokens": ["in", "die\u00b7sem", "lee\u00b7ren", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "aus dem der Welt Gebrause", "tokens": ["aus", "dem", "der", "Welt", "Ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "herausfloh und dahin.", "tokens": ["her\u00b7aus\u00b7floh", "und", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist nun noch mein Sinn, \u2013", "tokens": ["Was", "ist", "nun", "noch", "mein", "Sinn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "als da\u00df auf eine Pause", "tokens": ["als", "da\u00df", "auf", "ei\u00b7ne", "Pau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ich einzig DEINE Klause,", "tokens": ["ich", "ein\u00b7zig", "DeI\u00b7NE", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mein Grund und Ursprung, bin!", "tokens": ["mein", "Grund", "und", "Ur\u00b7sprung", ",", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}