{"textgrid.poem.39840": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: War mir ein heiter Liebesloos gefallen,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War mir ein heiter Liebesloos gefallen,", "tokens": ["War", "mir", "ein", "hei\u00b7ter", "Lie\u00b7bes\u00b7loos", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sel'ger Tanz, ein Gru\u00df auf stillen Auen:", "tokens": ["Ein", "sel'\u00b7ger", "Tanz", ",", "ein", "Gru\u00df", "auf", "stil\u00b7len", "Au\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich eilt' hinweg, ganz dir mich zu vertrauen,", "tokens": ["Ich", "eilt'", "hin\u00b7weg", ",", "ganz", "dir", "mich", "zu", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deine Brust go\u00df ich mein Gl\u00fcck vor Allen.", "tokens": ["In", "dei\u00b7ne", "Brust", "go\u00df", "ich", "mein", "Gl\u00fcck", "vor", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch heute tret' ich aus den theuren Hallen,", "tokens": ["Auch", "heu\u00b7te", "tret'", "ich", "aus", "den", "theu\u00b7ren", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch glaub' ich Sie zu h\u00f6ren und zu schauen;", "tokens": ["Noch", "glaub'", "ich", "Sie", "zu", "h\u00f6\u00b7ren", "und", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun such' ich Dich, und mu\u00df in Still' und Grauen", "tokens": ["Nun", "such'", "ich", "Dich", ",", "und", "mu\u00df", "in", "Still'", "und", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einsam nach deinem frischen Grabe wallen.", "tokens": ["Ein\u00b7sam", "nach", "dei\u00b7nem", "fri\u00b7schen", "Gra\u00b7be", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Die Freude rauschet noch vor meinen Ohren,", "tokens": ["Die", "Freu\u00b7de", "rau\u00b7schet", "noch", "vor", "mei\u00b7nen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und webt mir auf die Lippen s\u00fc\u00dfe Lieder:", "tokens": ["Und", "webt", "mir", "auf", "die", "Lip\u00b7pen", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du schl\u00e4fst, und Antwort hoff' ich, ach, vergebens.", "tokens": ["Du", "schl\u00e4fst", ",", "und", "Ant\u00b7wort", "hoff'", "ich", ",", "ach", ",", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "VVFIN", "PPER", "$,", "ITJ", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O dr\u00e4nge doch aus meiner Brust verloren", "tokens": ["O", "dr\u00e4n\u00b7ge", "doch", "aus", "mei\u00b7ner", "Brust", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinen Todestraum sich mischend, nieder", "tokens": ["In", "dei\u00b7nen", "To\u00b7des\u00b7traum", "sich", "mi\u00b7schend", ",", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$,", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu deiner Gruft ein heitrer Ton des Lebens!", "tokens": ["Zu", "dei\u00b7ner", "Gruft", "ein", "hei\u00b7trer", "Ton", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}