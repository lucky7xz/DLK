{"textgrid.poem.36666": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zweites Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch nicht immer der Berge melanchol'sche", "tokens": ["Doch", "nicht", "im\u00b7mer", "der", "Ber\u00b7ge", "me\u00b7lan\u00b7chol'\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4lderschauer, der Felsschlucht altes Dunkel,", "tokens": ["W\u00e4l\u00b7der\u00b7schau\u00b7er", ",", "der", "Fels\u00b7schlucht", "al\u00b7tes", "Dun\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wo des R\u00e4ubers Auge dem Wandrer lauert,", "tokens": ["Wo", "des", "R\u00e4u\u00b7bers", "Au\u00b7ge", "dem", "Wand\u00b7rer", "lau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der fliegende Dolch so manchen Busen", "tokens": ["Und", "der", "flie\u00b7gen\u00b7de", "Dolch", "so", "man\u00b7chen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "PIAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Von den Qualen des Lebens schon befreiet,", "tokens": ["Von", "den", "Qua\u00b7len", "des", "Le\u00b7bens", "schon", "be\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Doch nicht immer des Bergstroms \u00f6des Brausen", "tokens": ["Doch", "nicht", "im\u00b7mer", "des", "Berg\u00b7stroms", "\u00f6\u00b7des", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und des Sturmes Gespielen, jene Wipfel", "tokens": ["Und", "des", "Stur\u00b7mes", "Ge\u00b7spie\u00b7len", ",", "je\u00b7ne", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "PDAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Uralt rauschender B\u00e4um' und jene Pfade,", "tokens": ["Ur\u00b7alt", "rau\u00b7schen\u00b7der", "B\u00e4um'", "und", "je\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Die nur selten das Maulthier keuchend wandelt,", "tokens": ["Die", "nur", "sel\u00b7ten", "das", "Mault\u00b7hier", "keu\u00b7chend", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "W\u00e4ren meine Gesellschaft. Menschen suchen", "tokens": ["W\u00e4\u00b7ren", "mei\u00b7ne", "Ge\u00b7sell\u00b7schaft", ".", "Men\u00b7schen", "su\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "NN", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Gerne Menschen. Erhab'ne Geister freilich,", "tokens": ["Ger\u00b7ne", "Men\u00b7schen", ".", "Er\u00b7hab'\u00b7ne", "Geis\u00b7ter", "frei\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Sch\u00f6pferische, die Herrscherthrone st\u00fctzen,", "tokens": ["Sch\u00f6p\u00b7fe\u00b7ri\u00b7sche", ",", "die", "Herr\u00b7scher\u00b7thro\u00b7ne", "st\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "V\u00f6lkern, oder den Sternen, des Gedankens", "tokens": ["V\u00f6l\u00b7kern", ",", "o\u00b7der", "den", "Ster\u00b7nen", ",", "des", "Ge\u00b7dan\u00b7kens"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Unergr\u00fcndlichem Werk, ja selbst dem Gotte", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7chem", "Werk", ",", "ja", "selbst", "dem", "Got\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Den er denkt, des Gesetzes Ordnung geben,", "tokens": ["Den", "er", "denkt", ",", "des", "Ge\u00b7set\u00b7zes", "Ord\u00b7nung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Die das All und was in ihm ist, bis zu der", "tokens": ["Die", "das", "All", "und", "was", "in", "ihm", "ist", ",", "bis", "zu", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "PWS", "APPR", "PPER", "VAFIN", "$,", "KOUS", "APPR", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Pflanze treibendem Keim, die weite Sch\u00f6pfung,", "tokens": ["Pflan\u00b7ze", "trei\u00b7ben\u00b7dem", "Keim", ",", "die", "wei\u00b7te", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Die lebend'ge, mit ihrem Geist, mit Anfang", "tokens": ["Die", "le\u00b7ben\u00b7d'\u00b7ge", ",", "mit", "ih\u00b7rem", "Geist", ",", "mit", "An\u00b7fang"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Selbst und Ende, die Alles, was im Raume,", "tokens": ["Selbst", "und", "En\u00b7de", ",", "die", "Al\u00b7les", ",", "was", "im", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$,", "PRELS", "PIS", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Alles, was in der Zeit geschieht und lebet,", "tokens": ["Al\u00b7les", ",", "was", "in", "der", "Zeit", "ge\u00b7schieht", "und", "le\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.21": {"text": "Zaubrern \u00e4hnlich, in Zahl und Chiffern bannen,", "tokens": ["Zau\u00b7brern", "\u00e4hn\u00b7lich", ",", "in", "Zahl", "und", "Chif\u00b7fern", "ban\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Geister auch, die des Bildes ew'ge Sch\u00f6nheit", "tokens": ["Geis\u00b7ter", "auch", ",", "die", "des", "Bil\u00b7des", "ew'\u00b7ge", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "Aus dem Marmor mit Sch\u00f6pferfreiheit rufen,", "tokens": ["Aus", "dem", "Mar\u00b7mor", "mit", "Sch\u00f6p\u00b7fer\u00b7frei\u00b7heit", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Als ob l\u00e4ngst sie vollendet in der rohen", "tokens": ["Als", "ob", "l\u00e4ngst", "sie", "voll\u00b7en\u00b7det", "in", "der", "ro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "PPER", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ird'schen Masse geschlummert, und nun herrlich,", "tokens": ["Ird'\u00b7schen", "Mas\u00b7se", "ge\u00b7schlum\u00b7mert", ",", "und", "nun", "herr\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Wie die Seele dem K\u00f6rper, ihr entstiegen,", "tokens": ["Wie", "die", "See\u00b7le", "dem", "K\u00f6r\u00b7per", ",", "ihr", "ent\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Ferne w\u00e4ren sie mir. Doch wie die Sonne,", "tokens": ["Fer\u00b7ne", "w\u00e4\u00b7ren", "sie", "mir", ".", "Doch", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$.", "KON", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Der unendliche Lichtquell, alles Lebens", "tokens": ["Der", "un\u00b7end\u00b7li\u00b7che", "Licht\u00b7quell", ",", "al\u00b7les", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Heitre Mutter, die Schatten auch erzeuget,", "tokens": ["Heit\u00b7re", "Mut\u00b7ter", ",", "die", "Schat\u00b7ten", "auch", "er\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Folgt dem Genius auch des Schwarzen, Dunkeln,", "tokens": ["Folgt", "dem", "Ge\u00b7nius", "auch", "des", "Schwar\u00b7zen", ",", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,", "ADJA", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.31": {"text": "Allzuviel, und der karge Neid, die grimme", "tokens": ["All\u00b7zu\u00b7viel", ",", "und", "der", "kar\u00b7ge", "Neid", ",", "die", "grim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "$,", "KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Eifersucht und der Bosheit Schlangent\u00e4uschung,", "tokens": ["Ei\u00b7fer\u00b7sucht", "und", "der", "Bos\u00b7heit", "Schlan\u00b7gen\u00b7t\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Alle Martern und Leiden einer k\u00fchnen", "tokens": ["Al\u00b7le", "Mar\u00b7tern", "und", "Lei\u00b7den", "ei\u00b7ner", "k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "ART", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Ruhmbegierigen thatenlust'gen Seele,", "tokens": ["Ruhm\u00b7be\u00b7gie\u00b7ri\u00b7gen", "tha\u00b7ten\u00b7lust'\u00b7gen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Nie mehr tr\u00e4fen sie mich; treulose Herzen", "tokens": ["Nie", "mehr", "tr\u00e4\u00b7fen", "sie", "mich", ";", "treu\u00b7lo\u00b7se", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$.", "ADJA", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "Und eidbr\u00fcchige Freunde w\u00fcrfen nie mehr", "tokens": ["Und", "eid\u00b7br\u00fc\u00b7chi\u00b7ge", "Freun\u00b7de", "w\u00fcr\u00b7fen", "nie", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.37": {"text": "T\u00f6dtlich Gift in die Quelle, die kastal'sche,", "tokens": ["T\u00f6dt\u00b7lich", "Gift", "in", "die", "Quel\u00b7le", ",", "die", "ka\u00b7stal'\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Wo ich sch\u00f6pf' und den ernsten Musen opfre;", "tokens": ["Wo", "ich", "sch\u00f6pf'", "und", "den", "erns\u00b7ten", "Mu\u00b7sen", "opf\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.39": {"text": "Ha\u00df und Kleinmuth bekr\u00e4nzte mir den Altar,", "tokens": ["Ha\u00df", "und", "Klein\u00b7muth", "be\u00b7kr\u00e4nz\u00b7te", "mir", "den", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Wo die Flamm' ich entz\u00fcnde, nicht mit Dornen,", "tokens": ["Wo", "die", "Flamm'", "ich", "ent\u00b7z\u00fcn\u00b7de", ",", "nicht", "mit", "Dor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Statt mit Rosen und s\u00fc\u00dfer Myrt' und Lorbeer;", "tokens": ["Statt", "mit", "Ro\u00b7sen", "und", "s\u00fc\u00b7\u00dfer", "Myrt'", "und", "Lor\u00b7beer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Vor dem Grauen der schicksalsheil'gen Furien", "tokens": ["Vor", "dem", "Grau\u00b7en", "der", "schick\u00b7sals\u00b7heil'\u00b7gen", "Fu\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Furchtsam zitternd, verb\u00e4rg' ihr s\u00fc\u00dfes Antlitz", "tokens": ["Furcht\u00b7sam", "zit\u00b7ternd", ",", "ver\u00b7b\u00e4r\u00b7g'", "ihr", "s\u00fc\u00b7\u00dfes", "Ant\u00b7litz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Mir die fliehende, scheue Grazie nicht mehr;", "tokens": ["Mir", "die", "flie\u00b7hen\u00b7de", ",", "scheu\u00b7e", "Gra\u00b7zie", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.45": {"text": "Lieblich w\u00e4re mein Lied alsdann und lauter", "tokens": ["Lieb\u00b7lich", "w\u00e4\u00b7re", "mein", "Lied", "als\u00b7dann", "und", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "ADV", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.46": {"text": "Wie italischer Aether; meines Lebens", "tokens": ["Wie", "i\u00b7ta\u00b7li\u00b7scher", "A\u00b7e\u00b7ther", ";", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.47": {"text": "Milde sinkende Sonne g\u00f6ss' in diesen", "tokens": ["Mil\u00b7de", "sin\u00b7ken\u00b7de"