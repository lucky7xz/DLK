{"textgrid.poem.64345": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "66. An Lyde", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh mich an und l\u00e4chle, S\u00fc\u00dfe!", "tokens": ["Sieh", "mich", "an", "und", "l\u00e4ch\u00b7le", ",", "S\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mir deine Hand, und k\u00fcsse", "tokens": ["Gieb", "mir", "dei\u00b7ne", "Hand", ",", "und", "k\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen Trauten! Rot und bla\u00df", "tokens": ["Dei\u00b7nen", "Trau\u00b7ten", "!", "Rot", "und", "bla\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallet z\u00e4rtliches Verlangen", "tokens": ["Wal\u00b7let", "z\u00e4rt\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zitternd \u00fcber meine Wangen,", "tokens": ["Zit\u00b7ternd", "\u00fc\u00b7ber", "mei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Wimpern sind mir na\u00df.", "tokens": ["Und", "die", "Wim\u00b7pern", "sind", "mir", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Meine hei\u00dfen Lippen beben;", "tokens": ["Mei\u00b7ne", "hei\u00b7\u00dfen", "Lip\u00b7pen", "be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Atme, Lyde, neues Leben,", "tokens": ["At\u00b7me", ",", "Ly\u00b7de", ",", "neu\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsse Wonne mir hinein!", "tokens": ["K\u00fcs\u00b7se", "Won\u00b7ne", "mir", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lechzend sinken meine Augen;", "tokens": ["Lech\u00b7zend", "sin\u00b7ken", "mei\u00b7ne", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df aus deinem Blick sie saugen", "tokens": ["La\u00df", "aus", "dei\u00b7nem", "Blick", "sie", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Honig, Milch und Labewein!", "tokens": ["Ho\u00b7nig", ",", "Milch", "und", "La\u00b7be\u00b7wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}