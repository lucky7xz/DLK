{"dta.poem.10232": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Kirchhofe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ging regenschwer und sturmbewegt,             ", "tokens": ["Der", "Tag", "ging", "re\u00b7gen\u00b7schwer", "und", "sturm\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich war an manch vergessenem Grab gewesen.", "tokens": ["Ich", "war", "an", "manch", "ver\u00b7ges\u00b7se\u00b7nem", "Grab", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verwittert Stein und Kreuz, die Kr\u00e4nze alt,", "tokens": ["Ver\u00b7wit\u00b7tert", "Stein", "und", "Kreuz", ",", "die", "Kr\u00e4n\u00b7ze", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Namen \u00fcberwachsen, kaum zu lesen.", "tokens": ["Die", "Na\u00b7men", "\u00fc\u00b7ber\u00b7wach\u00b7sen", ",", "kaum", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Tag ging sturmbewegt und regenschwer,", "tokens": ["Der", "Tag", "ging", "sturm\u00b7be\u00b7wegt", "und", "re\u00b7gen\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf allen Gr\u00e4bern fror das Wort: Gewesen.", "tokens": ["Auf", "al\u00b7len", "Gr\u00e4\u00b7bern", "fror", "das", "Wort", ":", "Ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sturmestot die S\u00e4rge schlummerten \u2014", "tokens": ["Wie", "stur\u00b7me\u00b7stot", "die", "S\u00e4r\u00b7ge", "schlum\u00b7mer\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Auf allen Gr\u00e4bern taute still: Genesen.", "tokens": ["Auf", "al\u00b7len", "Gr\u00e4\u00b7bern", "tau\u00b7te", "still", ":", "Ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PTKVZ", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}