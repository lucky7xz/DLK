{"textgrid.poem.66749": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fa\u00df es, Mensch, und wirf zusammen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fa\u00df es, Mensch, und wirf zusammen", "tokens": ["Fa\u00df", "es", ",", "Mensch", ",", "und", "wirf", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "KON", "VVIMP", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles nun in einen Brand,", "tokens": ["Al\u00b7les", "nun", "in", "ei\u00b7nen", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was zur Schw\u00e4che mag verdammen,", "tokens": ["Was", "zur", "Schw\u00e4\u00b7che", "mag", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mit Feigheit dich umwand!", "tokens": ["Was", "mit", "Feig\u00b7heit", "dich", "um\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll dich etwas so bedrohen,", "tokens": ["Soll", "dich", "et\u00b7was", "so", "be\u00b7dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df es willenlos dich beugt?", "tokens": ["Da\u00df", "es", "wil\u00b7len\u00b7los", "dich", "beugt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gib's dem Feuer! La\u00df es lohen!", "tokens": ["Gib's", "dem", "Feu\u00b7er", "!", "La\u00df", "es", "lo\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei der Geist, der selbst sich zeugt!", "tokens": ["Sei", "der", "Geist", ",", "der", "selbst", "sich", "zeugt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Flamme sei die Stunde,", "tokens": ["Dei\u00b7ne", "Flam\u00b7me", "sei", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Wiege der Moment \u2013", "tokens": ["Dei\u00b7ne", "Wie\u00b7ge", "der", "Mo\u00b7ment", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei mit jener Macht im Bunde,", "tokens": ["Sei", "mit", "je\u00b7ner", "Macht", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die kein Recht von gestern kennt!", "tokens": ["Die", "kein", "Recht", "von", "ge\u00b7stern", "kennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "--+---+", "measure": "anapaest.init"}, "line.5": {"text": "Wisse, Schuld wird ungeheuer,", "tokens": ["Wis\u00b7se", ",", "Schuld", "wird", "un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ihr Konto nie zerrei\u00dft \u2013", "tokens": ["Die", "ihr", "Kon\u00b7to", "nie", "zer\u00b7rei\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df es lohen! Gib's dem Feuer!", "tokens": ["La\u00df", "es", "lo\u00b7hen", "!", "Gib's", "dem", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei der Zeuger, sei der Geist!", "tokens": ["Sei", "der", "Zeu\u00b7ger", ",", "sei", "der", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}