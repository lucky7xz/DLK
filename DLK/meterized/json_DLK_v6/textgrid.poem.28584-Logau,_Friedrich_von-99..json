{"textgrid.poem.28584": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Curiosus gr\u00e4mt sich sehr, was ein andrer hat zu leben;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "sehr", ",", "was", "ein", "an\u00b7drer", "hat", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "$,", "PRELS", "ART", "ADJA", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Curiosus gr\u00e4mt sich sehr, was ein andrer hat zu geben;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "sehr", ",", "was", "ein", "an\u00b7drer", "hat", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "$,", "PRELS", "ART", "ADJA", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Curiosus gr\u00e4mt sich sehr, was ein andrer f\u00fchrt f\u00fcr Lehre;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "sehr", ",", "was", "ein", "an\u00b7drer", "f\u00fchrt", "f\u00fcr", "Leh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "$,", "PRELS", "ART", "ADJA", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Curiosus gr\u00e4mt sich sehr, was ein andrer hat f\u00fcr Ehre;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "sehr", ",", "was", "ein", "an\u00b7drer", "hat", "f\u00fcr", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "$,", "PRELS", "ART", "ADJA", "VAFIN", "APPR", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Curiosus gr\u00e4mt sich nichts; hat nicht wol das Brot zu leben;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "nichts", ";", "hat", "nicht", "wol", "das", "Brot", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PIS", "$.", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Curiosus gr\u00e4mt sich nichts; hat viel Schuld und nichts zu geben;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "nichts", ";", "hat", "viel", "Schuld", "und", "nichts", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PIS", "$.", "VAFIN", "PIAT", "NN", "KON", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Curiosus gr\u00e4mt sich nichts; glaubt von Gotte keine Lehre;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "nichts", ";", "glaubt", "von", "Got\u00b7te", "kei\u00b7ne", "Leh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PIS", "$.", "VVFIN", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Curiosus gr\u00e4mt sich nichts; hat viel Schmach und wenig Ehre;", "tokens": ["Cu\u00b7ri\u00b7o\u00b7sus", "gr\u00e4mt", "sich", "nichts", ";", "hat", "viel", "Schmach", "und", "we\u00b7nig", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PIS", "$.", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+---+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Eignen Kummer schickt er fort, kan ihn nicht im Hause leiden;", "tokens": ["Eig\u00b7nen", "Kum\u00b7mer", "schickt", "er", "fort", ",", "kan", "ihn", "nicht", "im", "Hau\u00b7se", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Fremden Kummer h\u00e4lt er an, kan ihn keine Stunde meiden.", "tokens": ["Frem\u00b7den", "Kum\u00b7mer", "h\u00e4lt", "er", "an", ",", "kan", "ihn", "kei\u00b7ne", "Stun\u00b7de", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}