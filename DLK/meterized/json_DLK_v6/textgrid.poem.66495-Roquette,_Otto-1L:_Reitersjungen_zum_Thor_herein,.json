{"textgrid.poem.66495": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Reitersjungen zum Thor herein,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reitersjungen zum Thor herein,", "tokens": ["Rei\u00b7ters\u00b7jun\u00b7gen", "zum", "Thor", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Gar leidige G\u00e4ste zur Stelle!", "tokens": ["Gar", "lei\u00b7di\u00b7ge", "G\u00e4s\u00b7te", "zur", "Stel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mach's Fenster zu, feins M\u00e4gdelein,", "tokens": ["Mach's", "Fens\u00b7ter", "zu", ",", "feins", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deine Augen gl\u00e4nzen zu helle!", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "gl\u00e4n\u00b7zen", "zu", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PTKZU", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber ein Tag, und \u00fcber ein Stund,", "tokens": ["Und", "\u00fc\u00b7ber", "ein", "Tag", ",", "und", "\u00fc\u00b7ber", "ein", "Stund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie band ihr Haar mit Seiden.", "tokens": ["Sie", "band", "ihr", "Haar", "mit", "Sei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lacht' im Tanze von Herzensgrund,", "tokens": ["Sie", "lacht'", "im", "Tan\u00b7ze", "von", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mocht Manche den T\u00e4nzer ihr neiden!", "tokens": ["Mocht", "Man\u00b7che", "den", "T\u00e4n\u00b7zer", "ihr", "nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Im Garten steht ein Apfelbaum,", "tokens": ["Im", "Gar\u00b7ten", "steht", "ein", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Himmel gehn die Sterne:", "tokens": ["Am", "Him\u00b7mel", "gehn", "die", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbherzliebster, mir ist's, ach, wie ein Traum,", "tokens": ["\u00bb", "herz\u00b7liebs\u00b7ter", ",", "mir", "ist's", ",", "ach", ",", "wie", "ein", "Traum", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VAFIN", "$,", "ITJ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Will tr\u00e4umen noch lang' und gerne!\u00ab", "tokens": ["Will", "tr\u00e4u\u00b7men", "noch", "lang'", "und", "ger\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ADV", "KON", "ADV", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und Reitersjungen zum Thor hinaus,", "tokens": ["Und", "Rei\u00b7ters\u00b7jun\u00b7gen", "zum", "Thor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Staub die Trompeten verklingen.", "tokens": ["Im", "Staub", "die", "Trom\u00b7pe\u00b7ten", "ver\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbwas soll ich einsam nun zu Haus?", "tokens": ["\u00bb", "was", "soll", "ich", "ein\u00b7sam", "nun", "zu", "Haus", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz will mir zerspringen!\u00ab", "tokens": ["Das", "Herz", "will", "mir", "zer\u00b7sprin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was schiert sich ein jung frisch Reitersblut?", "tokens": ["Was", "schiert", "sich", "ein", "jung", "frisch", "Rei\u00b7ters\u00b7blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heut gek\u00fc\u00dft, und morgen verlassen!", "tokens": ["Heut", "ge\u00b7k\u00fc\u00dft", ",", "und", "mor\u00b7gen", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der Krieg hat nicht Haus, noch fahrend Gut,", "tokens": ["Der", "Krieg", "hat", "nicht", "Haus", ",", "noch", "fah\u00b7rend", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er nimmt es auf allen Gassen.", "tokens": ["Er", "nimmt", "es", "auf", "al\u00b7len", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Hab' acht, hab' acht, feins M\u00e4gdelein,", "tokens": ["Hab'", "acht", ",", "hab'", "acht", ",", "feins", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "VAFIN", "CARD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was suchst du auf freier Stra\u00dfen?", "tokens": ["Was", "suchst", "du", "auf", "frei\u00b7er", "Stra\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Kriesfeld liegt in blutigem Schein", "tokens": ["Das", "Kries\u00b7feld", "liegt", "in", "blu\u00b7ti\u00b7gem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Voll Elend \u00fcberma\u00dfen!", "tokens": ["Voll", "E\u00b7lend", "\u00fc\u00b7berm\u00b7a\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbich bin daheim verschm\u00e4ht, veracht',", "tokens": ["\u00bb", "ich", "bin", "da\u00b7heim", "ver\u00b7schm\u00e4ht", ",", "ver\u00b7acht'", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bin daheim vertrieben.", "tokens": ["Und", "bin", "da\u00b7heim", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was ich such' im Feld der Schlacht,", "tokens": ["Und", "was", "ich", "such'", "im", "Feld", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das steht bei Gott geschrieben!\u00ab", "tokens": ["Das", "steht", "bei", "Gott", "ge\u00b7schrie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie stund und schrie vor Freud' und Pein,", "tokens": ["Sie", "stund", "und", "schrie", "vor", "Freud'", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knab lag still gebettet.", "tokens": ["Der", "Knab", "lag", "still", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbthu auf die Augen und Lippen dein,", "tokens": ["\u00bb", "thu", "auf", "die", "Au\u00b7gen", "und", "Lip\u00b7pen", "dein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dran Seel' und Sinn ich verwettet!\u00ab", "tokens": ["Dran", "Seel'", "und", "Sinn", "ich", "ver\u00b7wet\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "NN", "KON", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbich hab' dich, und m\u00fc\u00dft' ich betteln gehn", "tokens": ["\u00bb", "ich", "hab'", "dich", ",", "und", "m\u00fc\u00dft'", "ich", "bet\u00b7teln", "gehn"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "$,", "KON", "VMFIN", "PPER", "VVINF", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr dich und mich um Erbarmen!", "tokens": ["F\u00fcr", "dich", "und", "mich", "um", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bist du todt, und ist's geschehn,", "tokens": ["Und", "bist", "du", "todt", ",", "und", "ist's", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab dich in meinen Armen!\u00ab", "tokens": ["Ich", "hab", "dich", "in", "mei\u00b7nen", "Ar\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}