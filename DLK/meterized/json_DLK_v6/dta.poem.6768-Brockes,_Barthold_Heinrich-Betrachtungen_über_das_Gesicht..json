{"dta.poem.6768": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wird ihr Abstand sich zu mindern scheinen;", "tokens": ["So", "wird", "ihr", "Ab\u00b7stand", "sich", "zu", "min\u00b7dern", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch kan unser Fu\u00df dahin nicht gehn.", "tokens": ["Und", "doch", "kan", "un\u00b7ser", "Fu\u00df", "da\u00b7hin", "nicht", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob wir dieselben gleich fast zu ber\u00fchren meinen.", "tokens": ["Ob", "wir", "die\u00b7sel\u00b7ben", "gleich", "fast", "zu", "be\u00b7r\u00fch\u00b7ren", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}