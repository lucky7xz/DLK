{"textgrid.poem.34524": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deine Augen, deine lieben blauen Augen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Augen, deine lieben blauen Augen,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", ",", "dei\u00b7ne", "lie\u00b7ben", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "draus naiven Goldes Sternenblicke leuchten,", "tokens": ["draus", "nai\u00b7ven", "Gol\u00b7des", "Ster\u00b7nen\u00b7bli\u00b7cke", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "deine Augen, so voll tiefen, klaren Glanzes,", "tokens": ["dei\u00b7ne", "Au\u00b7gen", ",", "so", "voll", "tie\u00b7fen", ",", "kla\u00b7ren", "Glan\u00b7zes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "drinnen dennoch \u2013 fl\u00fcchtig und verstohlen \u2013", "tokens": ["drin\u00b7nen", "den\u00b7noch", "\u2013", "fl\u00fcch\u00b7tig", "und", "ver\u00b7stoh\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADJD", "KON", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "mitten unter den frommen M\u00e4dchengedanken", "tokens": ["mit\u00b7ten", "un\u00b7ter", "den", "from\u00b7men", "M\u00e4d\u00b7chen\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "deines ruhig athmenden jungen Busens", "tokens": ["dei\u00b7nes", "ru\u00b7hig", "ath\u00b7men\u00b7den", "jun\u00b7gen", "Bu\u00b7sens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.7": {"text": "pl\u00f6tzlich ein neckisch Irrlicht flackernd aufspringt,", "tokens": ["pl\u00f6tz\u00b7lich", "ein", "ne\u00b7ckisch", "Irr\u00b7licht", "fla\u00b7ckernd", "auf\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJD", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "deine Augen erinnern mich an ein Traumbild:", "tokens": ["dei\u00b7ne", "Au\u00b7gen", "e\u00b7rin\u00b7nern", "mich", "an", "ein", "Traum\u00b7bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "eine Procession von Kindern und Jungfraun,", "tokens": ["ei\u00b7ne", "Pro\u00b7ces\u00b7si\u00b7on", "von", "Kin\u00b7dern", "und", "Jung\u00b7fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "die durch gelbe, gl\u00fchend heisse Felder zog,", "tokens": ["die", "durch", "gel\u00b7be", ",", "gl\u00fc\u00b7hend", "heis\u00b7se", "Fel\u00b7der", "zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "w\u00e4hrend die Sonne grimmig auf sie niederlachte ...", "tokens": ["w\u00e4h\u00b7rend", "die", "Son\u00b7ne", "grim\u00b7mig", "auf", "sie", "nie\u00b7der\u00b7lach\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Zaghaft, mit des jungen Rehwilds scheuen Augen,", "tokens": ["Zag\u00b7haft", ",", "mit", "des", "jun\u00b7gen", "Reh\u00b7wilds", "scheu\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "schweigend, unter dem Schnee der weissen Spitzen,", "tokens": ["schwei\u00b7gend", ",", "un\u00b7ter", "dem", "Schnee", "der", "weis\u00b7sen", "Spit\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "ziehn sie langsam wie ein Mousseline-Nebel", "tokens": ["ziehn", "sie", "lang\u00b7sam", "wie", "ein", "Mous\u00b7se\u00b7li\u00b7ne\u00b7Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "\u00fcber die rosig-silbern leuchtende Strasse", "tokens": ["\u00fc\u00b7ber", "die", "ro\u00b7sig\u00b7sil\u00b7bern", "leuch\u00b7ten\u00b7de", "Stras\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "und wie im Traume singen sie Ave Maria ...", "tokens": ["und", "wie", "im", "Trau\u00b7me", "sin\u00b7gen", "sie", "A\u00b7ve", "Ma\u00b7ria", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "APPRART", "NN", "VVFIN", "PPER", "NE", "NE", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und vor aller Andacht sehn sie gar nicht,", "tokens": ["Und", "vor", "al\u00b7ler", "An\u00b7dacht", "sehn", "sie", "gar", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "wie ein frisiertes \u00c4ffchen, ein N\u00fcsseknacker,", "tokens": ["wie", "ein", "fri\u00b7sier\u00b7tes", "\u00c4ff\u00b7chen", ",", "ein", "N\u00fcs\u00b7se\u00b7kna\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ausstaffiert mit Messhemd und mit Stola", "tokens": ["aus\u00b7staf\u00b7fiert", "mit", "Mess\u00b7hemd", "und", "mit", "Sto\u00b7la"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "APPR", "NE"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "hinter ihnen her mit t\u00fcckischen Blicken springt,", "tokens": ["hin\u00b7ter", "ih\u00b7nen", "her", "mit", "t\u00fc\u00b7cki\u00b7schen", "Bli\u00b7cken", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "schon die Backen bl\u00e4ht, auf die heiligen Kerzen zu pusten \u2013", "tokens": ["schon", "die", "Ba\u00b7cken", "bl\u00e4ht", ",", "auf", "die", "hei\u00b7li\u00b7gen", "Ker\u00b7zen", "zu", "pus\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "w\u00e4hrend es zierlich die reiche Robe vom Boden aufhebt ...", "tokens": ["w\u00e4h\u00b7rend", "es", "zier\u00b7lich", "die", "rei\u00b7che", "Ro\u00b7be", "vom", "Bo\u00b7den", "auf\u00b7hebt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}}}}}