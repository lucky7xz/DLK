{"textgrid.poem.46583": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von f\u00fcnf Br\u00fcdern, o beneidenswerthe]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von f\u00fcnf Br\u00fcdern, o beneidenswerthe", "tokens": ["Von", "f\u00fcnf", "Br\u00fc\u00b7dern", ",", "o", "be\u00b7nei\u00b7dens\u00b7wert\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "CARD", "NN", "$,", "FM", "FM"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwester, warest du umworben;", "tokens": ["Schwes\u00b7ter", ",", "wa\u00b7rest", "du", "um\u00b7wor\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder zu gefallen dir begehrte,", "tokens": ["Je\u00b7der", "zu", "ge\u00b7fal\u00b7len", "dir", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gern f\u00fcr dich entbehrte,", "tokens": ["Gern", "f\u00fcr", "dich", "ent\u00b7behr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "W\u00e4re gern f\u00fcr dich gestorben.", "tokens": ["W\u00e4\u00b7re", "gern", "f\u00fcr", "dich", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Keinem wolltest du den Vorzug geben,", "tokens": ["Kei\u00b7nem", "woll\u00b7test", "du", "den", "Vor\u00b7zug", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jeder Dienst war dir willkommen;", "tokens": ["Je\u00b7der", "Dienst", "war", "dir", "will\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zuletzt nun hast du beim Entschweben", "tokens": ["Doch", "zu\u00b7letzt", "nun", "hast", "du", "beim", "Ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinen j\u00fcngsten eben", "tokens": ["Dei\u00b7nen", "j\u00fcng\u00b7sten", "e\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Als den liebsten mitgenommen.", "tokens": ["Als", "den", "liebs\u00b7ten", "mit\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}