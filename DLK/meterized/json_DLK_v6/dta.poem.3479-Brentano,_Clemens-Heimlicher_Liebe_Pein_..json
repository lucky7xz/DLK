{"dta.poem.3479": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Heimlicher Liebe Pein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz der ist auf die Wanderschaft hin,               ", "tokens": ["Mein", "Schatz", "der", "ist", "auf", "die", "Wan\u00b7der\u00b7schaft", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ich wei\u00df aber nicht, was ich so traurig bin,", "tokens": ["Ich", "wei\u00df", "a\u00b7ber", "nicht", ",", "was", "ich", "so", "trau\u00b7rig", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vielleicht ist er todt, und liegt in guter Ruh,", "tokens": ["Viel\u00b7leicht", "ist", "er", "todt", ",", "und", "liegt", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Drum bring ich meine Zeit so traurig zu.", "tokens": ["Drum", "bring", "ich", "mei\u00b7ne", "Zeit", "so", "trau\u00b7rig", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als ich mit meim Schatz in die Kirch wollte gehn,", "tokens": ["Als", "ich", "mit", "meim", "Schatz", "in", "die", "Kirch", "woll\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Viel falsche falsche Zungen unter der Th\u00fcre stehn,", "tokens": ["Viel", "fal\u00b7sche", "fal\u00b7sche", "Zun\u00b7gen", "un\u00b7ter", "der", "Th\u00fc\u00b7re", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die eine redt dies, die andre redt das,", "tokens": ["Die", "ei\u00b7ne", "redt", "dies", ",", "die", "and\u00b7re", "redt", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PDS", "$,", "PRELS", "PIS", "VVFIN", "PDS", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das macht mir gar oft die A\u00fcgelein na\u00df.", "tokens": ["Das", "macht", "mir", "gar", "oft", "die", "A\u00b7\u00fc\u00b7ge\u00b7lein", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Distel und die Dornen, die stechen also sehr,", "tokens": ["Die", "Dis\u00b7tel", "und", "die", "Dor\u00b7nen", ",", "die", "ste\u00b7chen", "al\u00b7so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die falschen falschen Zungen aber noch viel mehr,", "tokens": ["Die", "fal\u00b7schen", "fal\u00b7schen", "Zun\u00b7gen", "a\u00b7ber", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Feuer auf Erden auch brennet also hei\u00df,", "tokens": ["Kein", "Feu\u00b7er", "auf", "Er\u00b7den", "auch", "bren\u00b7net", "al\u00b7so", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "ADV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als heimliche Liebe, die Niemand nicht wei\u00df.", "tokens": ["Als", "heim\u00b7li\u00b7che", "Lie\u00b7be", ",", "die", "Nie\u00b7mand", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PRELS", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Ach herzlieber Schatz, ich bitte dich noch eins,", "tokens": ["Ach", "herz\u00b7lie\u00b7ber", "Schatz", ",", "ich", "bit\u00b7te", "dich", "noch", "eins", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Du wollest auch bei meiner Begr\u00e4bni\u00df, seyn,", "tokens": ["Du", "wol\u00b7lest", "auch", "bei", "mei\u00b7ner", "Be\u00b7gr\u00e4b\u00b7ni\u00df", ",", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "VAINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bei meiner Begr\u00e4bni\u00df, bis ins k\u00fchle Grab,", "tokens": ["Bei", "mei\u00b7ner", "Be\u00b7gr\u00e4b\u00b7ni\u00df", ",", "bis", "ins", "k\u00fch\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dieweil ich dich so treulich geliebet hab.", "tokens": ["Die\u00b7weil", "ich", "dich", "so", "treu\u00b7lich", "ge\u00b7lie\u00b7bet", "hab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach Gott! was hat mein Vater und Mutter gethan,", "tokens": ["Ach", "Gott", "!", "was", "hat", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie haben mich gezwungen zu einem ehrlichen Mann,", "tokens": ["Sie", "ha\u00b7ben", "mich", "ge\u00b7zwun\u00b7gen", "zu", "ei\u00b7nem", "ehr\u00b7li\u00b7chen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zu einem ehrlichen Mann, den ich nicht geliebt,", "tokens": ["Zu", "ei\u00b7nem", "ehr\u00b7li\u00b7chen", "Mann", ",", "den", "ich", "nicht", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das macht mir ja mein Herz so betr\u00fcbt.", "tokens": ["Das", "macht", "mir", "ja", "mein", "Herz", "so", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}