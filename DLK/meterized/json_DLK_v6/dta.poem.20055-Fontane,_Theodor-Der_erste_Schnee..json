{"dta.poem.20055": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der erste Schnee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne schien, doch Winters N\u00e4h\u2019", "tokens": ["Die", "Son\u00b7ne", "schien", ",", "doch", "Win\u00b7ters", "N\u00e4h'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verrieth ein Flockenpaar;", "tokens": ["Ver\u00b7rieth", "ein", "Flo\u00b7cken\u00b7paar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gleicht das erste Fl\u00f6ckchen Schnee", "tokens": ["Es", "gleicht", "das", "ers\u00b7te", "Fl\u00f6ck\u00b7chen", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ersten wei\u00dfen Haar.", "tokens": ["Dem", "ers\u00b7ten", "wei\u00b7\u00dfen", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch wird \u2014 wie wohl von lieber Hand", "tokens": ["Noch", "wird", "wie", "wohl", "von", "lie\u00b7ber", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "KOKOM", "ADV", "APPR", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der erste Schnee dem Haupt \u2014", "tokens": ["Der", "ers\u00b7te", "Schnee", "dem", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So auch der erste Schnee dem Land", "tokens": ["So", "auch", "der", "ers\u00b7te", "Schnee", "dem", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Sonnenstrahl geraubt.", "tokens": ["Vom", "Son\u00b7nen\u00b7strahl", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch habet Acht! mit einem Mal", "tokens": ["Doch", "ha\u00b7bet", "Acht", "!", "mit", "ei\u00b7nem", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "CARD", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Haupt und Erde wei\u00df,", "tokens": ["Ist", "Haupt", "und", "Er\u00b7de", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Freundeshand und Sonnenstrahl", "tokens": ["Und", "Freun\u00b7des\u00b7hand", "und", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich nicht zu helfen wei\u00df.", "tokens": ["Sich", "nicht", "zu", "hel\u00b7fen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}