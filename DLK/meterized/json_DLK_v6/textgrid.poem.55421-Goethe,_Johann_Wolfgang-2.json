{"textgrid.poem.55421": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man ist gewohnt, da\u00df an den h\u00f6chsten Tagen", "tokens": ["Man", "ist", "ge\u00b7wohnt", ",", "da\u00df", "an", "den", "h\u00f6chs\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "VVPP", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Herrscherthron sich alle V\u00f6lkerschaften", "tokens": ["Zum", "Herr\u00b7scher\u00b7thron", "sich", "al\u00b7le", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach eigner Weise zuversichtlich wagen,", "tokens": ["Nach", "eig\u00b7ner", "Wei\u00b7se", "zu\u00b7ver\u00b7sicht\u00b7lich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mag seltsam auch der Schmuck an ihnen haften.", "tokens": ["Mag", "selt\u00b7sam", "auch", "der", "Schmuck", "an", "ih\u00b7nen", "haf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie denn das \u00c4u\u00dfre sei von Pelz und Kragen,", "tokens": ["Wie", "denn", "das", "\u00c4u\u00df\u00b7re", "sei", "von", "Pelz", "und", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man sieht hindurch die innern Eigenschaften;", "tokens": ["Man", "sieht", "hin\u00b7durch", "die", "in\u00b7nern", "Ei\u00b7gen\u00b7schaf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier bringt nun ein Korsar, zum Schein verwegen,", "tokens": ["Hier", "bringt", "nun", "ein", "Kor\u00b7sar", ",", "zum", "Schein", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Einsiedlerischer Zelle stillen Segen.", "tokens": ["Ein\u00b7sied\u00b7le\u00b7ri\u00b7scher", "Zel\u00b7le", "stil\u00b7len", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}