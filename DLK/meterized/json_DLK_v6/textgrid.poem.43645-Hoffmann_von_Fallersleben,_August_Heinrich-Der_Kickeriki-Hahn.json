{"textgrid.poem.43645": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Kickeriki-Hahn", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir des Morgens fr\u00fch aufstehn", "tokens": ["Wenn", "wir", "des", "Mor\u00b7gens", "fr\u00fch", "auf\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dann nach unsern H\u00fchnern sehn,", "tokens": ["Und", "dann", "nach", "un\u00b7sern", "H\u00fch\u00b7nern", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lock' ich meinen Hahn zu mir:", "tokens": ["Dann", "lock'", "ich", "mei\u00b7nen", "Hahn", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ei lieber Hahn, wie geht es dir?", "tokens": ["Ei", "lie\u00b7ber", "Hahn", ",", "wie", "geht", "es", "dir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ei sag', warum", "tokens": ["Ei", "sag'", ",", "wa\u00b7rum"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bist du so stumm?", "tokens": ["Bist", "du", "so", "stumm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Augen macht er zu alsdann,", "tokens": ["Die", "Au\u00b7gen", "macht", "er", "zu", "als\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil er auswendig Alles kann;", "tokens": ["Weil", "er", "aus\u00b7wen\u00b7dig", "Al\u00b7les", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schl\u00e4gt mit seinen Fl\u00fcgeln schnell", "tokens": ["Er", "schl\u00e4gt", "mit", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kr\u00e4het wohlbedacht und hell,", "tokens": ["Und", "kr\u00e4\u00b7het", "wohl\u00b7be\u00b7dacht", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und irrt sich nie:", "tokens": ["Und", "irrt", "sich", "nie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kickeriki!", "tokens": ["Ki\u00b7cke\u00b7ri\u00b7ki", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+---", "measure": "dactylic.init"}}}}}