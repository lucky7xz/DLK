{"dta.poem.9085": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergeben? Ich? Dir?", "tokens": ["Ver\u00b7ge\u00b7ben", "?", "Ich", "?", "Dir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "$.", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "L\u00e4ngst.", "tokens": ["L\u00e4ngst", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ich thats, noch eh ichs wusste.", "tokens": ["Ich", "thats", ",", "noch", "eh", "ichs", "wuss\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aber vergessen? Vergessen? . . . Ach, wenn ichs k\u00f6nnte!", "tokens": ["A\u00b7ber", "ver\u00b7ges\u00b7sen", "?", "Ver\u00b7ges\u00b7sen", "?", ".", ".", ".", "Ach", ",", "wenn", "ichs", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$.", "NN", "$.", "$.", "$.", "$.", "ITJ", "$,", "KOUS", "PIS", "VMFIN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Oft,", "tokens": ["Oft", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "mitten im hellsten Sonnenschein,", "tokens": ["mit\u00b7ten", "im", "hells\u00b7ten", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "wenn ich fr\u00f6hlich bin und \u201ean nichts denke\u201c,", "tokens": ["wenn", "ich", "fr\u00f6h\u00b7lich", "bin", "und", "\u201e", "an", "nichts", "den\u00b7ke", "\u201c", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "$(", "APPR", "PIS", "VVFIN", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "pl\u00f6tzlich,", "tokens": ["pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "da,", "tokens": ["da", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "grau hockt es vor mir,", "tokens": ["grau", "hockt", "es", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": ". . . wie eine Kr\u00f6te!", "tokens": [".", ".", ".", "wie", "ei\u00b7ne", "Kr\u00f6\u00b7te", "!"], "token_info": ["punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "PWAV", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und Alles, Alles scheint mir wieder schaal. Schaal und trostlos.", "tokens": ["Und", "Al\u00b7les", ",", "Al\u00b7les", "scheint", "mir", "wie\u00b7der", "schaal", ".", "Schaal", "und", "trost\u00b7los", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VVFIN", "PPER", "ADV", "ADJD", "$.", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ganze Leben.", "tokens": ["Das", "gan\u00b7ze", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und ich bin traurig. Traurig \u00fcber dich . . . und mich.", "tokens": ["Und", "ich", "bin", "trau\u00b7rig", ".", "Trau\u00b7rig", "\u00fc\u00b7ber", "dich", ".", ".", ".", "und", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$.", "ADJD", "APPR", "PPER", "$.", "$.", "$.", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}