{"dta.poem.17683": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "305.  Die Liebe Gottes ist wesentlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe gegen Gott steht nicht in s\u00fcssigkeit/", "tokens": ["Die", "Lie\u00b7be", "ge\u00b7gen", "Gott", "steht", "nicht", "in", "s\u00fcs\u00b7sig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "S\u00fcss ist ein zufall nur: sie steht in Wesenheit.", "tokens": ["S\u00fcss", "ist", "ein", "zu\u00b7fall", "nur", ":", "sie", "steht", "in", "We\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}}}}}