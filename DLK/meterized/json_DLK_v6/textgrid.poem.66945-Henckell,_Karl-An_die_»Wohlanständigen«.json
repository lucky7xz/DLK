{"textgrid.poem.66945": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "An die \u00bbWohlanst\u00e4ndigen\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Schicklichkeit wollt die Natur ihr morden,", "tokens": ["Mit", "Schick\u00b7lich\u00b7keit", "wollt", "die", "Na\u00b7tur", "ihr", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das s\u00fchnt sich mit Unerbittlichkeit.", "tokens": ["Das", "s\u00fchnt", "sich", "mit", "Un\u00b7er\u00b7bitt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr seid so verflucht anst\u00e4ndig geworden,", "tokens": ["Ihr", "seid", "so", "ver\u00b7flucht", "an\u00b7st\u00e4n\u00b7dig", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "ADJD", "VAPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich aber d\u00fcrste nach Sittlichkeit.", "tokens": ["Ich", "a\u00b7ber", "d\u00fcrs\u00b7te", "nach", "Sitt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbanst\u00f6\u00dfig\u00ab ist euch der herrliche Wille", "tokens": ["\u00bb", "an\u00b7st\u00f6\u00b7\u00dfig", "\u00ab", "ist", "euch", "der", "herr\u00b7li\u00b7che", "Wil\u00b7le"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$(", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des wahren Menschen, der sich befreit,", "tokens": ["Des", "wah\u00b7ren", "Men\u00b7schen", ",", "der", "sich", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihr tragt auf der Nase die garstige Brille", "tokens": ["Ihr", "tragt", "auf", "der", "Na\u00b7se", "die", "gars\u00b7ti\u00b7ge", "Bril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Der Gouvernante Verlogenheit.", "tokens": ["Der", "Gou\u00b7ver\u00b7nan\u00b7te", "Ver\u00b7lo\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}