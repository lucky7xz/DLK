{"textgrid.poem.66471": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen, mein Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, mein Wald,", "tokens": ["Will\u00b7kom\u00b7men", ",", "mein", "Wald", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gr\u00fcnschattiges Haus!", "tokens": ["Gr\u00fcn\u00b7schat\u00b7ti\u00b7ges", "Haus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durch die Wipfel schon hallt", "tokens": ["Durch", "die", "Wip\u00b7fel", "schon", "hallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mir dein gr\u00fc\u00dfend Gebraus.", "tokens": ["Mir", "dein", "gr\u00fc\u00b7\u00dfend", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie trink' ich in Z\u00fcgen", "tokens": ["Wie", "trink'", "ich", "in", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mich frisch und gesund,", "tokens": ["Mich", "frisch", "und", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Hier athm' ich Gen\u00fcgen", "tokens": ["Hier", "athm'", "ich", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aus Herzensgrund!", "tokens": ["Aus", "Her\u00b7zens\u00b7grund", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Zum grasigen Hang,", "tokens": ["Zum", "gra\u00b7si\u00b7gen", "Hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Aufsteigend vom Thal,", "tokens": ["Auf\u00b7stei\u00b7gend", "vom", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dringt der Glocken Klang", "tokens": ["Dringt", "der", "Glo\u00b7cken", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und des Abends Strahl.", "tokens": ["Und", "des", "A\u00b7bends", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und es rauscht in der Eiche", "tokens": ["Und", "es", "rauscht", "in", "der", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Hochstrebendem Baum", "tokens": ["Hoch\u00b7stre\u00b7ben\u00b7dem", "Baum"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im gr\u00fcnen Bereiche", "tokens": ["Im", "gr\u00fc\u00b7nen", "Be\u00b7rei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ein Liedestraum.", "tokens": ["Ein", "Lie\u00b7des\u00b7traum", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Den Blumen gesellt", "tokens": ["Den", "Blu\u00b7men", "ge\u00b7sellt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Auf Rasen und Moos,", "tokens": ["Auf", "Ra\u00b7sen", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tief schau ich die Welt,", "tokens": ["Tief", "schau", "ich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und den Himmel, wie gro\u00df!", "tokens": ["Und", "den", "Him\u00b7mel", ",", "wie", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und ich tr\u00e4um' im Schweigen", "tokens": ["Und", "ich", "tr\u00e4um'", "im", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Waldschattiger Ruh", "tokens": ["Wald\u00b7schat\u00b7ti\u00b7ger", "Ruh"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Den Himmel mein eigen,", "tokens": ["Den", "Him\u00b7mel", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Erde dazu!", "tokens": ["Die", "Er\u00b7de", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}