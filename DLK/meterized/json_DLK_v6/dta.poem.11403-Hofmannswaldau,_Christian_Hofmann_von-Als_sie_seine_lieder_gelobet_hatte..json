{"dta.poem.11403": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie seine lieder gelobet hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein schwan singt niemahls sch\u00f6ne,", "tokens": ["Ein", "schwan", "singt", "nie\u00b7mahls", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wenn er sterben soll.", "tokens": ["Als", "wenn", "er", "ster\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum, Daphne! lebe wohl!", "tokens": ["Drum", ",", "Daph\u00b7ne", "!", "le\u00b7be", "wohl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn, weil mein klag-geth\u00f6ne", "tokens": ["Denn", ",", "weil", "mein", "klag\u00b7get\u00b7h\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dir itzt so s\u00fcsse klingt, und dein gem\u00fcth erfreut:", "tokens": ["Dir", "itzt", "so", "s\u00fcs\u00b7se", "klingt", ",", "und", "dein", "ge\u00b7m\u00fcth", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So 