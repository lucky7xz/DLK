{"dta.poem.9831": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Michel Probner von Grauden\u00df  \n durch buchstaben-wechsel:  \n Und nur ich male gro\u00dfe proben.  \n C. E.  \n Erkl\u00e4rung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein au\u00dfge\u00fcbter held gr\u00e4bt mit erhitztem muth", "tokens": ["Ein", "au\u00df\u00b7ge\u00b7\u00fcb\u00b7ter", "held", "gr\u00e4bt", "mit", "er\u00b7hitz\u00b7tem", "muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die proben tapffcer faust in seiner feinde blut/", "tokens": ["Die", "pro\u00b7ben", "tapf\u00b7fcer", "faust", "in", "sei\u00b7ner", "fein\u00b7de", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ein gelehrter sucht in klug gesetzten schrifften/", "tokens": ["Und", "ein", "ge\u00b7lehr\u00b7ter", "sucht", "in", "klug", "ge\u00b7setz\u00b7ten", "schriff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPR", "ADJD", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie ich durch mahlen mir ein ehrenmahl zu stifften.", "tokens": ["Wie", "ich", "durch", "mah\u00b7len", "mir", "ein", "eh\u00b7ren\u00b7mahl", "zu", "stiff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "VVINF", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir treffen alle drey das vorgesteckte ziel/", "tokens": ["Wir", "tref\u00b7fen", "al\u00b7le", "drey", "das", "vor\u00b7ge\u00b7steck\u00b7te", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "CARD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ein Darius dort f\u00fcr Alexandern fiel/", "tokens": ["Wenn", "ein", "Da\u00b7ri\u00b7us", "dort", "f\u00fcr", "A\u00b7lex\u00b7an\u00b7dern", "fiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ADV", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und noch ein Grotius auff tausend zungen schwebet/", "tokens": ["Und", "noch", "ein", "Gro\u00b7ti\u00b7us", "auff", "tau\u00b7send", "zun\u00b7gen", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wie mein theurer ruhm in hundert bildern lebet.", "tokens": ["So", "wie", "mein", "theu\u00b7rer", "ruhm", "in", "hun\u00b7dert", "bil\u00b7dern", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das wunderwerck der welt/ Rom/ sage was ich kan/", "tokens": ["Das", "wun\u00b7der\u00b7werck", "der", "welt", "/", "Rom", "/", "sa\u00b7ge", "was", "ich", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "NE", "$(", "VVFIN", "PWS", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es melde Brandenburg/ was meine faust gethan/", "tokens": ["Es", "mel\u00b7de", "Bran\u00b7den\u00b7burg", "/", "was", "mei\u00b7ne", "faust", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es spreche (stehts doch frey) ob nicht f\u00fcr andern allen", "tokens": ["Es", "spre\u00b7che", "(", "stehts", "doch", "frey", ")", "ob", "nicht", "f\u00fcr", "an\u00b7dern", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "ADV", "ADJD", "$(", "KOUS", "PTKNEG", "APPR", "PIS", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nur ich entworffen hab/ was vielen wolgefallen/", "tokens": ["Nur", "ich", "ent\u00b7worf\u00b7fen", "hab", "/", "was", "vie\u00b7len", "wol\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$(", "PWS", "PIS", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ob nicht mein pinsel sich/ es komm auch wer da will/", "tokens": ["Ob", "nicht", "mein", "pin\u00b7sel", "sich", "/", "es", "komm", "auch", "wer", "da", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PRF", "$(", "PPER", "VVFIN", "ADV", "PWS", "ADV", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schon gro\u00df genug gemacht; doch was? ich schweige still!", "tokens": ["Schon", "gro\u00df", "ge\u00b7nug", "ge\u00b7macht", ";", "doch", "was", "?", "ich", "schwei\u00b7ge", "still", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "$.", "KON", "PWS", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein nahme: und nur ich/ ich male grosse proben/", "tokens": ["Mein", "nah\u00b7me", ":", "und", "nur", "ich", "/", "ich", "ma\u00b7le", "gros\u00b7se", "pro\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KON", "ADV", "PPER", "$(", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wird sich/ nebst meiner kunst/ schon von sich selber loben.", "tokens": ["Wird", "sich", "/", "nebst", "mei\u00b7ner", "kunst", "/", "schon", "von", "sich", "sel\u00b7ber", "lo\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "$(", "APPR", "PPOSAT", "NN", "$(", "ADV", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}