{"textgrid.poem.63755": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbein St\u00fcck des Himmels, das zur Erde fiel,", "tokens": ["\u00bb", "ein", "St\u00fcck", "des", "Him\u00b7mels", ",", "das", "zur", "Er\u00b7de", "fiel", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Sch\u00f6pfung Sonntagskind, ein zweites Eden,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Sonn\u00b7tags\u00b7kind", ",", "ein", "zwei\u00b7tes", "E\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Zauberin des Meers, bet\u00f6rend jeden,", "tokens": ["Die", "Zau\u00b7be\u00b7rin", "des", "Meers", ",", "be\u00b7t\u00f6\u00b7rend", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den je vorbeitrug seines Schiffes Kiel;", "tokens": ["Den", "je", "vor\u00b7bei\u00b7trug", "sei\u00b7nes", "Schif\u00b7fes", "Kiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein ew'ger Freudenborn, ein Leidasyl\u00ab \u2013", "tokens": ["Ein", "ew'\u00b7ger", "Freu\u00b7den\u00b7born", ",", "ein", "Lei\u00b7da\u00b7syl", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Freund, genug der \u00fcberspannten Reden!", "tokens": ["O", "Freund", ",", "ge\u00b7nug", "der", "\u00fc\u00b7bers\u00b7pann\u00b7ten", "Re\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die blanke Larve deckt gar arge Sch\u00e4den,", "tokens": ["Die", "blan\u00b7ke", "Lar\u00b7ve", "deckt", "gar", "ar\u00b7ge", "Sch\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gar schn\u00f6der L\u00fcst' und Leidenschaften Spiel!", "tokens": ["Gar", "schn\u00f6\u00b7der", "L\u00fcst'", "und", "Lei\u00b7den\u00b7schaf\u00b7ten", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl mag dies Land des ew'gen Sonnenlichts", "tokens": ["Wohl", "mag", "dies", "Land", "des", "ew'\u00b7gen", "Son\u00b7nen\u00b7lichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Paradies dir d\u00fcnken, zauberhelle,", "tokens": ["Ein", "Pa\u00b7ra\u00b7dies", "dir", "d\u00fcn\u00b7ken", ",", "zau\u00b7ber\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Schlangen locken: kommt und werdet G\u00f6tter!", "tokens": ["Wo", "Schlan\u00b7gen", "lo\u00b7cken", ":", "kommt", "und", "wer\u00b7det", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$.", "VVFIN", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch niemand pflegt im Schwei\u00df des Angesichts", "tokens": ["Doch", "nie\u00b7mand", "pflegt", "im", "Schwei\u00df", "des", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier abzub\u00fc\u00dfen seine S\u00fcndenf\u00e4lle,", "tokens": ["Hier", "ab\u00b7zu\u00b7b\u00fc\u00b7\u00dfen", "sei\u00b7ne", "S\u00fcn\u00b7den\u00b7f\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sehr entbehrlich scheinen Feigenbl\u00e4tter.", "tokens": ["Und", "sehr", "ent\u00b7behr\u00b7lich", "schei\u00b7nen", "Fei\u00b7gen\u00b7bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}