{"textgrid.poem.56118": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gott vermags. Wie aber, sag mir, soll", "tokens": ["Ein", "Gott", "ver\u00b7mags", ".", "Wie", "a\u00b7ber", ",", "sag", "mir", ",", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein Mann ihm folgen durch die schmale Leier?", "tokens": ["ein", "Mann", "ihm", "fol\u00b7gen", "durch", "die", "schma\u00b7le", "Lei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Sinn ist Zwiespalt. An der Kreuzung zweier", "tokens": ["Sein", "Sinn", "ist", "Zwies\u00b7palt", ".", "An", "der", "Kreu\u00b7zung", "zwei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "APPR", "ART", "NN", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herzwege steht kein Tempel f\u00fcr Apoll.", "tokens": ["Herz\u00b7we\u00b7ge", "steht", "kein", "Tem\u00b7pel", "f\u00fcr", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "APPR", "NE", "$."], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Gesang, wie du ihn lehrst, ist nicht Begehr,", "tokens": ["Ge\u00b7sang", ",", "wie", "du", "ihn", "lehrst", ",", "ist", "nicht", "Be\u00b7gehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nicht Werbung um ein endlich noch Erreichtes;", "tokens": ["nicht", "Wer\u00b7bung", "um", "ein", "end\u00b7lich", "noch", "Er\u00b7reich\u00b7tes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "ART", "ADV", "ADV", "NN", "$."], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gesang ist Dasein. F\u00fcr den Gott ein Leichtes.", "tokens": ["Ge\u00b7sang", "ist", "Da\u00b7sein", ".", "F\u00fcr", "den", "Gott", "ein", "Leich\u00b7tes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann aber ", "tokens": ["Wann", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "an unser Sein die Erde und die Sterne?", "tokens": ["an", "un\u00b7ser", "Sein", "die", "Er\u00b7de", "und", "die", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies ", "tokens": ["Dies"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "die Stimme dann den Mund dir aufst\u00f6\u00dft, \u2013 lerne", "tokens": ["die", "Stim\u00b7me", "dann", "den", "Mund", "dir", "auf\u00b7st\u00f6\u00dft", ",", "\u2013", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "vergessen, da\u00df du aufsangst. Das verrinnt.", "tokens": ["ver\u00b7ges\u00b7sen", ",", "da\u00df", "du", "auf\u00b7sangst", ".", "Das", "ver\u00b7rinnt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VVFIN", "$.", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Wahrheit singen, ist ein andrer Hauch.", "tokens": ["In", "Wahr\u00b7heit", "sin\u00b7gen", ",", "ist", "ein", "an\u00b7drer", "Hauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Hauch um nichts. Ein Wehn im Gott. Ein Wind.", "tokens": ["Ein", "Hauch", "um", "nichts", ".", "Ein", "Wehn", "im", "Gott", ".", "Ein", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$.", "ART", "NN", "APPRART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}