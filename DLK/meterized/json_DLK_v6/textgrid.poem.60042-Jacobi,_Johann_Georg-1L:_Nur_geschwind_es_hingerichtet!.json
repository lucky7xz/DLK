{"textgrid.poem.60042": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur geschwind es hingerichtet!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur geschwind es hingerichtet!", "tokens": ["Nur", "ge\u00b7schwind", "es", "hin\u00b7ge\u00b7rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Qu\u00e4len sollst du mir", "tokens": ["Qu\u00e4\u00b7len", "sollst", "du", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht das arme Thier,", "tokens": ["Nicht", "das", "ar\u00b7me", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ob es gleich das Blumenbeet zernichtet;", "tokens": ["Ob", "es", "gleich", "das", "Blu\u00b7men\u00b7beet", "zer\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn von allen Farben hier,", "tokens": ["Denn", "von", "al\u00b7len", "Far\u00b7ben", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche durch einander funkeln,", "tokens": ["Wel\u00b7che", "durch", "ein\u00b7an\u00b7der", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat es keinerley Genu\u00df.", "tokens": ["Hat", "es", "kei\u00b7ner\u00b7ley", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fchlt es doch im Dunkeln,", "tokens": ["W\u00fchlt", "es", "doch", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wo es einmal w\u00fchlen mu\u00df!", "tokens": ["Wo", "es", "ein\u00b7mal", "w\u00fch\u00b7len", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und daneben fehlt ihm das Gesicht:", "tokens": ["Und", "da\u00b7ne\u00b7ben", "fehlt", "ihm", "das", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Unsre Blumen kennt es nicht.", "tokens": ["Uns\u00b7re", "Blu\u00b7men", "kennt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}