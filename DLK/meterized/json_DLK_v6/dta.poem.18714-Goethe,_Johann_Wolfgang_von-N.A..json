{"dta.poem.18714": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Hab\u2019 ich euch denn je gerathen", "tokens": ["Hab'", "ich", "euch", "denn", "je", "ge\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ihr Kriege f\u00fchren solltet?", "tokens": ["Wie", "ihr", "Krie\u00b7ge", "f\u00fch\u00b7ren", "soll\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schalt ich euch nach euren Thaten", "tokens": ["Schalt", "ich", "euch", "nach", "eu\u00b7ren", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ihr Friede schliessen wolltet?", "tokens": ["Wenn", "ihr", "Frie\u00b7de", "schlies\u00b7sen", "woll\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so hab\u2019 ich auch den Fischer", "tokens": ["Und", "so", "hab'", "ich", "auch", "den", "Fi\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig sehen Netze werfen,", "tokens": ["Ru\u00b7hig", "se\u00b7hen", "Net\u00b7ze", "wer\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Brauchte dem gewandten Tischer", "tokens": ["Brauch\u00b7te", "dem", "ge\u00b7wand\u00b7ten", "Ti\u00b7scher"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winkelmaas nicht einzusch\u00e4rfen.", "tokens": ["Win\u00b7kel\u00b7maas", "nicht", "ein\u00b7zu\u00b7sch\u00e4r\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber ihr wollt\u2019 besser wissen", "tokens": ["A\u00b7ber", "ihr", "wollt'", "bes\u00b7ser", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich weiss, der ich bedachte", "tokens": ["Was", "ich", "weiss", ",", "der", "ich", "be\u00b7dach\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Natur, f\u00fcr mich beflissen,", "tokens": ["Was", "Na\u00b7tur", ",", "f\u00fcr", "mich", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon zu meinem Eigen machte.", "tokens": ["Schon", "zu", "mei\u00b7nem", "Ei\u00b7gen", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fchlt ihr euch dergleichen St\u00e4rke,", "tokens": ["F\u00fchlt", "ihr", "euch", "derg\u00b7lei\u00b7chen", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun, so f\u00f6rdert eure Sachen;", "tokens": ["Nun", ",", "so", "f\u00f6r\u00b7dert", "eu\u00b7re", "Sa\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht ihr aber meine Werke,", "tokens": ["Seht", "ihr", "a\u00b7ber", "mei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernet erst: so wollt\u2019 er\u2019s machen.", "tokens": ["Ler\u00b7net", "erst", ":", "so", "wollt'", "er's", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}