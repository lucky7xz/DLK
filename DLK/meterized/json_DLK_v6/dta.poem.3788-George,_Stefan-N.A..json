{"dta.poem.3788": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Die kinder unterm feigenbaum entschlafen", "tokens": ["Die", "kin\u00b7der", "un\u00b7term", "fei\u00b7gen\u00b7baum", "ent\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach unbedachtem seligem verm\u00e4hlen \u00b7", "tokens": ["Nach", "un\u00b7be\u00b7dach\u00b7tem", "se\u00b7li\u00b7gem", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "XY"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich k\u00fcmmerten der kalten v\u00e4ter strafen", "tokens": ["Mich", "k\u00fcm\u00b7mer\u00b7ten", "der", "kal\u00b7ten", "v\u00e4\u00b7ter", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wol! da ich euch den starken tropfen g\u00f6nnte", "tokens": ["Wol", "!", "da", "ich", "euch", "den", "star\u00b7ken", "trop\u00b7fen", "g\u00f6nn\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "PPER", "PRF", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aus meinem treuen ringe der mir diene", "tokens": ["Aus", "mei\u00b7nem", "treu\u00b7en", "rin\u00b7ge", "der", "mir", "die\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "ART", "PPER", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn es bei einer d\u00e4mmerung mir schiene", "tokens": ["Wenn", "es", "bei", "ei\u00b7ner", "d\u00e4m\u00b7me\u00b7rung", "mir", "schie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dass ich die Sterne nicht mehr schauen k\u00f6nnte", "tokens": ["Dass", "ich", "die", "Ster\u00b7ne", "nicht", "mehr", "schau\u00b7en", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Begnadete! da ich euch g\u00fctig nahte", "tokens": ["Be\u00b7gna\u00b7de\u00b7te", "!", "da", "ich", "euch", "g\u00fc\u00b7tig", "nah\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KOUS", "PPER", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kein erwachen euch ein gl\u00fcck ermattet", "tokens": ["Und", "kein", "er\u00b7wa\u00b7chen", "euch", "ein", "gl\u00fcck", "er\u00b7mat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das nur der tr\u00e4um so herrlich euch gestattet", "tokens": ["Das", "nur", "der", "tr\u00e4um", "so", "herr\u00b7lich", "euch", "ge\u00b7stat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ich es jezt aus euren z\u00fcgen rate", "tokens": ["Als", "ich", "es", "jezt", "aus", "eu\u00b7ren", "z\u00fc\u00b7gen", "ra\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}