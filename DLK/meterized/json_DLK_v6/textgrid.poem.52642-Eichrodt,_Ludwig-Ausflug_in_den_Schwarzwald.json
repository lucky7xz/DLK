{"textgrid.poem.52642": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ausflug in den Schwarzwald", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im H\u00f6llenthale drohen", "tokens": ["Im", "H\u00f6l\u00b7len\u00b7tha\u00b7le", "dro\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Felsen hoch herein,", "tokens": ["Die", "Fel\u00b7sen", "hoch", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die schauerlichen sch\u00fcchtern", "tokens": ["Die", "schau\u00b7er\u00b7li\u00b7chen", "sch\u00fcch\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den fr\u00fchen Wandrer ein.", "tokens": ["Den", "fr\u00fc\u00b7hen", "Wand\u00b7rer", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann \u00f6ffnen sich die Berge", "tokens": ["Dann", "\u00f6ff\u00b7nen", "sich", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hohen Ebene zu,", "tokens": ["Der", "ho\u00b7hen", "E\u00b7be\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Tannen und die Matten", "tokens": ["Die", "Tan\u00b7nen", "und", "die", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Prangen in gr\u00fcner Ruh.", "tokens": ["Pran\u00b7gen", "in", "gr\u00fc\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Am dunkelblauen Himmel", "tokens": ["Am", "dun\u00b7kel\u00b7blau\u00b7en", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Milchwei\u00dfe Wolken ziehn,", "tokens": ["Milch\u00b7wei\u00b7\u00dfe", "Wol\u00b7ken", "ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lieblich in wilder Gegend", "tokens": ["Lieb\u00b7lich", "in", "wil\u00b7der", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Himmel und Blumen bl\u00fchn.", "tokens": ["Him\u00b7mel", "und", "Blu\u00b7men", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und ehe die H\u00fcgel schlie\u00dfen", "tokens": ["Und", "e\u00b7he", "die", "H\u00fc\u00b7gel", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Thal mit sanfter H\u00f6h,", "tokens": ["Das", "Thal", "mit", "sanf\u00b7ter", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da spiegelt sich die Sonne", "tokens": ["Da", "spie\u00b7gelt", "sich", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im tiefen Titi-See.", "tokens": ["Im", "tie\u00b7fen", "Ti\u00b7ti\u00b7See", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es baden die m\u00fcden Freunde", "tokens": ["Es", "ba\u00b7den", "die", "m\u00fc\u00b7den", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In seiner frischen Fluth,", "tokens": ["In", "sei\u00b7ner", "fri\u00b7schen", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00e4rken Leib und Seele", "tokens": ["Und", "st\u00e4r\u00b7ken", "Leib", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wandern wieder gut.", "tokens": ["Und", "wan\u00b7dern", "wie\u00b7der", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}