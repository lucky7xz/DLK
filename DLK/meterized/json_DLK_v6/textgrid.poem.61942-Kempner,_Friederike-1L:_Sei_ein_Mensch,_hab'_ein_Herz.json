{"textgrid.poem.61942": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei ein Mensch, hab' ein Herz", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei ein Mensch, hab' ein Herz", "tokens": ["Sei", "ein", "Mensch", ",", "hab'", "ein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Unter Millionen,", "tokens": ["Un\u00b7ter", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie ein Fels, wie ein Stern,", "tokens": ["Wie", "ein", "Fels", ",", "wie", "ein", "Stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Stehe fest, leuchte fern,", "tokens": ["Ste\u00b7he", "fest", ",", "leuch\u00b7te", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Setz' die Welt in Staunen!", "tokens": ["Setz'", "die", "Welt", "in", "Stau\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sei ein Mensch, hab' ein Herz", "tokens": ["Sei", "ein", "Mensch", ",", "hab'", "ein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "F\u00fcr die Millionen,", "tokens": ["F\u00fcr", "die", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn's der Tor auch Wahnsinn nennt,", "tokens": ["Wenn's", "der", "Tor", "auch", "Wahn\u00b7sinn", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er keine Weisheit kennt,", "tokens": ["Weil", "er", "kei\u00b7ne", "Weis\u00b7heit", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kannst Du dr\u00fcber staunen?", "tokens": ["Kannst", "Du", "dr\u00fc\u00b7ber", "stau\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sei ein Geist, sch\u00fcr' die Glut", "tokens": ["Sei", "ein", "Geist", ",", "sch\u00fcr'", "die", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Unter Millionen,", "tokens": ["Un\u00b7ter", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Selber hei\u00df, selber gl\u00fch',", "tokens": ["Sel\u00b7ber", "hei\u00df", ",", "sel\u00b7ber", "gl\u00fch'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "F\u00fcrchte nie, raste nie,", "tokens": ["F\u00fcrch\u00b7te", "nie", ",", "ras\u00b7te", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Setz' die Kraft in Staunen.", "tokens": ["Setz'", "die", "Kraft", "in", "Stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sei ein Geist, sch\u00fcr' die Glut", "tokens": ["Sei", "ein", "Geist", ",", "sch\u00fcr'", "die", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Unter Millionen,", "tokens": ["Un\u00b7ter", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "La\u00df auf Erden eine Spur,", "tokens": ["La\u00df", "auf", "Er\u00b7den", "ei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahne sie und l\u00e4chle nur,", "tokens": ["Ah\u00b7ne", "sie", "und", "l\u00e4ch\u00b7le", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es giebt sichre Kronen.", "tokens": ["Es", "giebt", "sich\u00b7re", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Sei ein Geist, sch\u00fcr' die Glut", "tokens": ["Sei", "ein", "Geist", ",", "sch\u00fcr'", "die", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Unter Millionen,", "tokens": ["Un\u00b7ter", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie am Himmel still ein Stern,", "tokens": ["Wie", "am", "Him\u00b7mel", "still", "ein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirke l\u00e4chelnd, scheide gern,", "tokens": ["Wir\u00b7ke", "l\u00e4\u00b7chelnd", ",", "schei\u00b7de", "gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles wird sich lohnen!", "tokens": ["Al\u00b7les", "wird", "sich", "loh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}