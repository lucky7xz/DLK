{"dta.poem.11640": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auftrag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.85", "nl:0.14"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "In poetischer Epistel", "tokens": ["In", "po\u00b7e\u00b7ti\u00b7scher", "E\u00b7pis\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft ein desperater Wicht", "tokens": ["Ruft", "ein", "des\u00b7pe\u00b7ra\u00b7ter", "Wicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aus dem Ton der h\u00f6chsten Fistel:", "tokens": ["Aus", "dem", "Ton", "der", "h\u00f6chs\u00b7ten", "Fis\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schurke, warum schreibt Er nicht?!", "tokens": ["Schur\u00b7ke", ",", "wa\u00b7rum", "schreibt", "Er", "nicht", "?!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wei\u00df Er doch, es lassen Herzen,", "tokens": ["Wei\u00df", "Er", "doch", ",", "es", "las\u00b7sen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VVINF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die die Liebe angeweht,", "tokens": ["Die", "die", "Lie\u00b7be", "an\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz und gar nicht mit sich scherzen,", "tokens": ["Ganz", "und", "gar", "nicht", "mit", "sich", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PTKNEG", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nun vollends ein Poet!", "tokens": ["Und", "nun", "vol\u00b7lends", "ein", "Po\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ich bin von dem Gelichter,", "tokens": ["Denn", "ich", "bin", "von", "dem", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem der Kopf best\u00e4ndig voll:", "tokens": ["Dem", "der", "Kopf", "be\u00b7st\u00e4n\u00b7dig", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich auch nur halb ein Dichter,", "tokens": ["Bin", "ich", "auch", "nur", "halb", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich doch zur H\u00e4lfte toll.", "tokens": ["Bin", "ich", "doch", "zur", "H\u00e4lf\u00b7te", "toll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Amor hat Ihn mir verpflichtet,", "tokens": ["A\u00b7mor", "hat", "Ihn", "mir", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und f\u00fcrwahr, der durft' es schon,", "tokens": ["Und", "f\u00fcr\u00b7wahr", ",", "der", "durft'", "es", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Mund, der Ihm berichtet,", "tokens": ["Denn", "der", "Mund", ",", "der", "Ihm", "be\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reicht zugleich den Botenlohn.", "tokens": ["Reicht", "zu\u00b7gleich", "den", "Bo\u00b7ten\u00b7lohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pass' Er denn zur guten Stunde,", "tokens": ["Pass'", "Er", "denn", "zur", "gu\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn Sein Schatz durch's L\u00e4dchen schaut", "tokens": ["Wenn", "Sein", "Schatz", "durch's", "L\u00e4d\u00b7chen", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lock' ihr jedes Wort vom Munde,", "tokens": ["Lock'", "ihr", "je\u00b7des", "Wort", "vom", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mein Sch\u00e4tzchen ihr vertraut.", "tokens": ["Das", "mein", "Sch\u00e4tz\u00b7chen", "ihr", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schreib' Er mir dann von dem M\u00e4dchen", "tokens": ["Schreib'", "Er", "mir", "dann", "von", "dem", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein halb Dutzend Bogen voll,", "tokens": ["Ein", "halb", "Dut\u00b7zend", "Bo\u00b7gen", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und daneben ein Trakt\u00e4tchen,", "tokens": ["Und", "da\u00b7ne\u00b7ben", "ein", "Trak\u00b7t\u00e4t\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wie ich mich verhalten soll.", "tokens": ["Wie", "ich", "mich", "ver\u00b7hal\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}