{"dta.poem.2568": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. C. F. u. J. S. P. Hochzeit. Fest den  \n 10. Septemb. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Perle keuscher Zucht und Tugend/", "tokens": ["Per\u00b7le", "keu\u00b7scher", "Zucht", "und", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bild der angenehmen Jugend/", "tokens": ["Bild", "der", "an\u00b7ge\u00b7neh\u00b7men", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Braut was sol es seyn/", "tokens": ["Sch\u00f6\u00b7ne", "Braut", "was", "sol", "es", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PWS", "VMFIN", "PPER", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie ihrer Freyheit Sch\u00e4tze", "tokens": ["Da\u00df", "sie", "ih\u00b7rer", "Frey\u00b7heit", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liefert in das Ehstands Netze/", "tokens": ["Lie\u00b7fert", "in", "das", "Eh\u00b7stands", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und stellt sich gefangen ein?", "tokens": ["Und", "stellt", "sich", "ge\u00b7fan\u00b7gen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Oder wil sie nach den Zeiten", "tokens": ["O\u00b7der", "wil", "sie", "nach", "den", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhre kluge Sinnen leiten/", "tokens": ["Ih\u00b7re", "klu\u00b7ge", "Sin\u00b7nen", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sich jetzt verj\u00fcngt das Jahr/", "tokens": ["Weil", "sich", "jetzt", "ver\u00b7j\u00fcngt", "das", "Jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f\u00e4ngt alles an zu bl\u00fchen?", "tokens": ["Und", "f\u00e4ngt", "al\u00b7les", "an", "zu", "bl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wil sie di\u00df zum Beyspiel ziehen/", "tokens": ["Wil", "sie", "di\u00df", "zum", "Bey\u00b7spiel", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sieh schn\u00e4belt Paar und Paar?", "tokens": ["Wenn", "sieh", "schn\u00e4\u00b7belt", "Paar", "und", "Paar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Mu\u00df die Scho\u00df der reichen Erden", "tokens": ["Mu\u00df", "die", "Scho\u00df", "der", "rei\u00b7chen", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt des Himmels Braut nicht werden", "tokens": ["Jetzt", "des", "Him\u00b7mels", "Braut", "nicht", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "PTKNEG", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie sich mit Blumen schm\u00fcckt?", "tokens": ["Wenn", "sie", "sich", "mit", "Blu\u00b7men", "schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er mit den g\u00f6ldnen Stralen", "tokens": ["Wenn", "er", "mit", "den", "g\u00f6ld\u00b7nen", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pfleget ihre Brust zu mahlen", "tokens": ["Pfle\u00b7get", "ih\u00b7re", "Brust", "zu", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und viel tausend Farben schickt?", "tokens": ["Und", "viel", "tau\u00b7send", "Far\u00b7ben", "schickt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ietzt in dem bunten Lentzen", "tokens": ["Wenn", "ietzt", "in", "dem", "bun\u00b7ten", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tulpen und Narcissen gl\u00e4ntzen/", "tokens": ["Tul\u00b7pen", "und", "Nar\u00b7cis\u00b7sen", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der G\u00e4rten Reichthum prangt/", "tokens": ["Und", "der", "G\u00e4r\u00b7ten", "Reicht\u00b7hum", "prangt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Baum in seinem Kleide/", "tokens": ["Wenn", "der", "Baum", "in", "sei\u00b7nem", "Klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als wie in Schnee-weisser Seide/", "tokens": ["Als", "wie", "in", "Schnee\u00b7weis\u00b7ser", "Sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NE", "NE", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die vollkommne Zier erlangt.", "tokens": ["Die", "voll\u00b7komm\u00b7ne", "Zier", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solte da der Mensch nicht leben/", "tokens": ["Sol\u00b7te", "da", "der", "Mensch", "nicht", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG",