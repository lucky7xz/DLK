{"dta.poem.23520": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Stammbaum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O des Schicksals b\u00f6se T\u00fccke,", "tokens": ["O", "des", "Schick\u00b7sals", "b\u00f6\u00b7se", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O das b\u00f6se Spiel der Zeit!", "tokens": ["O", "das", "b\u00f6\u00b7se", "Spiel", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst wohl sa\u00dfen wir im Gl\u00fccke,", "tokens": ["Einst", "wohl", "sa\u00b7\u00dfen", "wir", "im", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzo sitzen wir im Leid.", "tokens": ["Jet\u00b7zo", "sit\u00b7zen", "wir", "im", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur der Anblick meiner Felder,", "tokens": ["Nur", "der", "An\u00b7blick", "mei\u00b7ner", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Wiesen ist noch mein;", "tokens": ["Mei\u00b7ner", "Wie\u00b7sen", "ist", "noch", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4ngst verkauft sind meine W\u00e4lder,", "tokens": ["L\u00e4ngst", "ver\u00b7kauft", "sind", "mei\u00b7ne", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein Baum blieb mir allein.", "tokens": ["Nur", "ein", "Baum", "blieb", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich diesen Baum umklammre,", "tokens": ["Wenn", "ich", "die\u00b7sen", "Baum", "um\u00b7klamm\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heg' ich Hoffnung, sch\u00f6pf' ich Muth;", "tokens": ["Heg'", "ich", "Hoff\u00b7nung", ",", "sch\u00f6pf'", "ich", "Muth", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich vor ihm wein' und jammre,", "tokens": ["Wenn", "ich", "vor", "ihm", "wein'", "und", "jamm\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchl' ich neue Lebensglut.", "tokens": ["F\u00fchl'", "ich", "neu\u00b7e", "Le\u00b7bens\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein, ich habe nicht vergebens", "tokens": ["Nein", ",", "ich", "ha\u00b7be", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Tage hingebracht!", "tokens": ["Mei\u00b7ne", "Ta\u00b7ge", "hin\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, da steht der Baum des Lebens", "tokens": ["Seht", ",", "da", "steht", "der", "Baum", "des", "Le\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in voller Bl\u00fcthenpracht!", "tokens": ["Und", "in", "vol\u00b7ler", "Bl\u00fc\u00b7then\u00b7pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Menschheit wird begl\u00fccken", "tokens": ["Und", "die", "Menschheit", "wird", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das was ich hienieden that:", "tokens": ["Das", "was", "ich", "hien\u00b7ie\u00b7den", "that", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mit diesen Bl\u00fcthen schm\u00fccken", "tokens": ["Denn", "mit", "die\u00b7sen", "Bl\u00fc\u00b7then", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sich mancher Hof und Staat.", "tokens": ["Wird", "sich", "man\u00b7cher", "Hof", "und", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O du Baum aus altem Samen,", "tokens": ["O", "du", "Baum", "aus", "al\u00b7tem", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie beruhigst du mein Herz!", "tokens": ["Wie", "be\u00b7ru\u00b7higst", "du", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon vor deinem stolzen Namen", "tokens": ["Schon", "vor", "dei\u00b7nem", "stol\u00b7zen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwindet aller Gram und Schmerz.", "tokens": ["Schwin\u00b7det", "al\u00b7ler", "Gram", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gr\u00fcnt, ihr jungen Sommerlatten,", "tokens": ["Gr\u00fcnt", ",", "ihr", "jun\u00b7gen", "Som\u00b7mer\u00b7lat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachst in fr\u00f6hlichem Gedeihn!", "tokens": ["Wachst", "in", "fr\u00f6h\u00b7li\u00b7chem", "Ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in meines Stammbaums Schatten", "tokens": ["Und", "in", "mei\u00b7nes", "Stamm\u00b7baums", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlaf' ich sanft und selig ein.", "tokens": ["Schlaf'", "ich", "sanft", "und", "se\u00b7lig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}