{"textgrid.poem.54333": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist es, misvergn\u00fcgter Geist,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist es, misvergn\u00fcgter Geist,", "tokens": ["Was", "ist", "es", ",", "mis\u00b7ver\u00b7gn\u00fcg\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das deinen Kummer t\u00e4glich mehret,", "tokens": ["Das", "dei\u00b7nen", "Kum\u00b7mer", "t\u00e4g\u00b7lich", "meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Zufriedenheit entrei\u00dft,", "tokens": ["Und", "die", "Zu\u00b7frie\u00b7den\u00b7heit", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deiner Seelen Ruhe st\u00f6ret?", "tokens": ["Und", "dei\u00b7ner", "See\u00b7len", "Ru\u00b7he", "st\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4ltst du das Haupt darum gest\u00fctzt,", "tokens": ["H\u00e4ltst", "du", "das", "Haupt", "da\u00b7rum", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil Ha\u00df und Neid die Pfeile spitzt?", "tokens": ["Weil", "Ha\u00df", "und", "Neid", "die", "Pfei\u00b7le", "spitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie? wird dein Geist dadurch ger\u00fchret?", "tokens": ["Wie", "?", "wird", "dein", "Geist", "da\u00b7durch", "ge\u00b7r\u00fch\u00b7ret", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPOSAT", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! f\u00fcrchte nicht dergleichen Brut,", "tokens": ["Ach", "!", "f\u00fcrch\u00b7te", "nicht", "derg\u00b7lei\u00b7chen", "Brut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PTKNEG", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wo bleibt der starke Heldenmuth,", "tokens": ["Wo", "bleibt", "der", "star\u00b7ke", "Hel\u00b7den\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der die gesetzten Seelen zieret?", "tokens": ["Der", "die", "ge\u00b7setz\u00b7ten", "See\u00b7len", "zie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ists m\u00f6glich, das der schele Neid,", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "das", "der", "sche\u00b7le", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem nur schwache Geister zittern,", "tokens": ["Vor", "dem", "nur", "schwa\u00b7che", "Geis\u00b7ter", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir ebenfals wie jenen dr\u00e4ut,", "tokens": ["Dir", "e\u00b7ben\u00b7fals", "wie", "je\u00b7nen", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOKOM", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deine Gro\u00dfmuth, will ersch\u00fcttern?", "tokens": ["Und", "dei\u00b7ne", "Gro\u00df\u00b7muth", ",", "will", "er\u00b7sch\u00fct\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beth\u00f6rter! gieb die Sorgen an,", "tokens": ["Be\u00b7th\u00f6r\u00b7ter", "!", "gieb", "die", "Sor\u00b7gen", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil Zorn und Misgunst niemals kann", "tokens": ["Weil", "Zorn", "und", "Mis\u00b7gunst", "nie\u00b7mals", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Jugend reines Kleid beflecken.", "tokens": ["Der", "Ju\u00b7gend", "rei\u00b7nes", "Kleid", "be\u00b7fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sucht er auch unsre Fehler auf,", "tokens": ["Sucht", "er", "auch", "uns\u00b7re", "Feh\u00b7ler", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So geben wir nicht Achtung drauf:", "tokens": ["So", "ge\u00b7ben", "wir", "nicht", "Ach\u00b7tung", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sein Schm\u00e4hen mu\u00df uns nicht erschrecken.", "tokens": ["Sein", "Schm\u00e4\u00b7hen", "mu\u00df", "uns", "nicht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf! r\u00fcste dich, bestrittner Geist!", "tokens": ["Auf", "!", "r\u00fcs\u00b7te", "dich", ",", "be\u00b7stritt\u00b7ner", "Geist", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willst du ihm denn die Freude g\u00f6nnen,", "tokens": ["Willst", "du", "ihm", "denn", "die", "Freu\u00b7de", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er dich gar zu Boden schmei\u00dft?", "tokens": ["Da\u00df", "er", "dich", "gar", "zu", "Bo\u00b7den", "schmei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll man dich da wohl weise nennen?", "tokens": ["Soll", "man", "dich", "da", "wohl", "wei\u00b7se", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wenig als der Wellen Wuth", "tokens": ["So", "we\u00b7nig", "als", "der", "Wel\u00b7len", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den harten Felsen Schaden thut,", "tokens": ["Den", "har\u00b7ten", "Fel\u00b7sen", "Scha\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So stark sie vielmals toben, krachen,", "tokens": ["So", "stark", "sie", "viel\u00b7mals", "to\u00b7ben", ",", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wenig kann die Schlangenart,", "tokens": ["So", "we\u00b7nig", "kann", "die", "Schlan\u00b7ge\u00b7nart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die sich mit Zerbers Rachen paart,", "tokens": ["Die", "sich", "mit", "Zer\u00b7bers", "Ra\u00b7chen", "paart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein m\u00e4nnlich Herze sch\u00fcchtern machen.", "tokens": ["Ein", "m\u00e4nn\u00b7lich", "Her\u00b7ze", "sch\u00fcch\u00b7tern", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer bl\u00f6d und niedertr\u00e4chtig ist,", "tokens": ["Wer", "bl\u00f6d", "und", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sich nicht aus dem Staub will wagen,", "tokens": ["Und", "sich", "nicht", "aus", "dem", "Staub", "will", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sich nur, weil er sich vergi\u00dft,", "tokens": ["L\u00e4\u00dft", "sich", "nur", ",", "weil", "er", "sich", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von diesem Feind im Harnisch jagen,", "tokens": ["Von", "die\u00b7sem", "Feind", "im", "Har\u00b7nisch", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er bebt, so bald der hagre Geist", "tokens": ["Er", "bebt", ",", "so", "bald", "der", "hag\u00b7re", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm seine gelben Z\u00e4hne weist,", "tokens": ["Ihm", "sei\u00b7ne", "gel\u00b7ben", "Z\u00e4h\u00b7ne", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sucht aus Furcht sich zu verstecken,", "tokens": ["Und", "sucht", "aus", "Furcht", "sich", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Weiser aber h\u00f6rt in Ruh", "tokens": ["Ein", "Wei\u00b7ser", "a\u00b7ber", "h\u00f6rt", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Drohung ganz gelassen zu,", "tokens": ["Der", "Dro\u00b7hung", "ganz", "ge\u00b7las\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn ihn die Misgunst meynt zu schrecken.", "tokens": ["Wenn", "ihn", "die", "Mis\u00b7gunst", "meynt", "zu", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Warum? dies d\u00fcrre Knochenbild", "tokens": ["Wa\u00b7rum", "?", "dies", "d\u00fcr\u00b7re", "Kno\u00b7chen\u00b7bild"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt selber schimpflich zu verstehen,", "tokens": ["Giebt", "sel\u00b7ber", "schimpf\u00b7lich", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wenig es bey denen gilt,", "tokens": ["Wie", "we\u00b7nig", "es", "bey", "de\u00b7nen", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die sich durch Tugenden erh\u00f6hen.", "tokens": ["Die", "sich", "durch", "Tu\u00b7gen\u00b7den", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Je heftiger er auf sie schilt,", "tokens": ["Je", "hef\u00b7ti\u00b7ger", "er", "auf", "sie", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seinen Muth mit L\u00e4stern k\u00fchlt,", "tokens": ["Und", "sei\u00b7nen", "Muth", "mit", "L\u00e4s\u00b7tern", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Je mehr verr\u00e4th er seine Bl\u00f6sse;", "tokens": ["Je", "mehr", "ver\u00b7r\u00e4\u00b7th", "er", "sei\u00b7ne", "Bl\u00f6s\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Er f\u00e4nde nie Gelegenheit,", "tokens": ["Er", "f\u00e4n\u00b7de", "nie", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wofern, was er mit Gift bestreut,", "tokens": ["Wo\u00b7fern", ",", "was", "er", "mit", "Gift", "be\u00b7streut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er selber in der That bes\u00e4sse.", "tokens": ["Er", "sel\u00b7ber", "in", "der", "That", "be\u00b7s\u00e4s\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Brust worinnen Klugheit wohnt,", "tokens": ["Die", "Brust", "wo\u00b7rin\u00b7nen", "Klug\u00b7heit", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird still und unempfindlich bleiben,", "tokens": ["Wird", "still", "und", "un\u00b7emp\u00b7find\u00b7lich", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn gleich der Neid, der nichts verschont,", "tokens": ["Wenn", "gleich", "der", "Neid", ",", "der", "nichts", "ver\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich sucht an selbiger zu reiben.", "tokens": ["Sich", "sucht", "an", "sel\u00b7bi\u00b7ger", "zu", "rei\u0