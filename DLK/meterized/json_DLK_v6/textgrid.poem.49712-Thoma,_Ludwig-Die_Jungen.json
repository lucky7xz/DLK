{"textgrid.poem.49712": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Jungen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah oft junge Staatsanw\u00e4lte,", "tokens": ["Ich", "sah", "oft", "jun\u00b7ge", "Staats\u00b7an\u00b7w\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die zielbewu\u00dft vor Torheit fliehen,", "tokens": ["Die", "ziel\u00b7be\u00b7wu\u00dft", "vor", "Tor\u00b7heit", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich \u00fcben in Gesinnungsk\u00e4lte,", "tokens": ["Sich", "\u00fc\u00b7ben", "in", "Ge\u00b7sin\u00b7nungs\u00b7k\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u2013 vor sie lernen \u2013 schon erziehen.", "tokens": ["Und", "\u2013", "vor", "sie", "ler\u00b7nen", "\u2013", "schon", "er\u00b7zie\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "PPER", "VVINF", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie haben fr\u00fch, noch eh' sie's kennen,", "tokens": ["Sie", "ha\u00b7ben", "fr\u00fch", ",", "noch", "eh'", "sie's", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADV", "KOUS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Glauben an das Volk verloren,", "tokens": ["Den", "Glau\u00b7ben", "an", "das", "Volk", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem sie vollbewu\u00dft sich trennen", "tokens": ["Von", "dem", "sie", "voll\u00b7be\u00b7wu\u00dft", "sich", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit hohem Sinn und feuchten Ohren.", "tokens": ["Mit", "ho\u00b7hem", "Sinn", "und", "feuch\u00b7ten", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Denkungsart der Untertanen", "tokens": ["Die", "Den\u00b7kungs\u00b7art", "der", "Un\u00b7ter\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht sie zu harten Pessimisten.", "tokens": ["Macht", "sie", "zu", "har\u00b7ten", "Pes\u00b7si\u00b7mis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie folgen ihres Vorteils Fahnen", "tokens": ["Sie", "fol\u00b7gen", "ih\u00b7res", "Vor\u00b7teils", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im neuen Kurs als gute Christen.", "tokens": ["Im", "neu\u00b7en", "Kurs", "als", "gu\u00b7te", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie sprechen schon von \u00bbZiel und Zwecken", "tokens": ["Sie", "spre\u00b7chen", "schon", "von", "\u00bb", "Ziel", "und", "Zwe\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Staats\u00ab, von \u00bbreiflichem Ermessen\u00ab,", "tokens": ["Des", "Staats", "\u00ab", ",", "von", "\u00bb", "reif\u00b7li\u00b7chem", "Er\u00b7mes\u00b7sen", "\u00ab", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "$,", "APPR", "$(", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Mitteln, S\u00fcnder \u00bbabzuschrecken\u00ab,", "tokens": ["Von", "Mit\u00b7teln", ",", "S\u00fcn\u00b7der", "\u00bb", "ab\u00b7zu\u00b7schre\u00b7cken", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$(", "VVIZU", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von \u00bbwohlverstand'nen Interessen\u00ab.", "tokens": ["Von", "\u00bb", "wohl\u00b7ver\u00b7stan\u00b7d'\u00b7nen", "In\u00b7ter\u00b7es\u00b7sen", "\u00ab", "."], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "$(", "ADJA", "NN", "$(", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Von \u00bbfalscher Milde\u00ab h\u00f6rt man Worte", "tokens": ["Von", "\u00bb", "fal\u00b7scher", "Mil\u00b7de", "\u00ab", "h\u00f6rt", "man", "Wor\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$(", "ADJA", "NN", "$(", "VVFIN", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von \u00bbExempel statuieren\u00ab.", "tokens": ["Und", "von", "\u00bb", "Ex\u00b7em\u00b7pel", "sta\u00b7tu\u00b7ie\u00b7ren", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "$(", "NN", "VVFIN", "$(", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie f\u00e4hrt durch eine junge Pforte", "tokens": ["Wie", "f\u00e4hrt", "durch", "ei\u00b7ne", "jun\u00b7ge", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Weisheit in die Welt spazieren!", "tokens": ["Die", "Weis\u00b7heit", "in", "die", "Welt", "spa\u00b7zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Seh' ich so einen jungen Sprecher,", "tokens": ["Seh'", "ich", "so", "ei\u00b7nen", "jun\u00b7gen", "Spre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb Lausbub' und halb Staatserhalter,", "tokens": ["Halb", "Laus\u00b7bub'", "und", "halb", "Staa\u00b7tser\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-++-++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Gef\u00e4llt mir besser ein Verbrecher,", "tokens": ["Ge\u00b7f\u00e4llt", "mir", "bes\u00b7ser", "ein", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als solchen d\u00fcrren Sinns Verwalter.", "tokens": ["Als", "sol\u00b7chen", "d\u00fcr\u00b7ren", "Sinns", "Ver\u00b7wal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}