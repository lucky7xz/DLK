{"textgrid.poem.47748": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[siehst du die dunkelen Flecken im Monde?]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du die dunkelen Flecken im Monde?", "tokens": ["Siehst", "du", "die", "dun\u00b7ke\u00b7len", "Fle\u00b7cken", "im", "Mon\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NE", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kennst du sie, jene von unseren V\u00e4tern", "tokens": ["Kennst", "du", "sie", ",", "je\u00b7ne", "von", "un\u00b7se\u00b7ren", "V\u00e4\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PDS", "APPR", "PPOSAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Lauter und \u00e4cht abstammende M\u00e4hre,", "tokens": ["Lau\u00b7ter", "und", "\u00e4cht", "ab\u00b7stam\u00b7men\u00b7de", "M\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Welche Bedeutung enth\u00fcllet und Ursprung?", "tokens": ["Wel\u00b7che", "Be\u00b7deu\u00b7tung", "ent\u00b7h\u00fcl\u00b7let", "und", "Ur\u00b7sprung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Kannst du sie nicht? ich ertheile Belehr. \u2013", "tokens": ["Kannst", "du", "sie", "nicht", "?", "ich", "er\u00b7thei\u00b7le", "Be\u00b7lehr", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Waren einmal zwei pfiffige Diebe,", "tokens": ["Wa\u00b7ren", "ein\u00b7mal", "zwei", "pfif\u00b7fi\u00b7ge", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denen, indem sie die n\u00e4chtlichen Pfade", "tokens": ["De\u00b7nen", ",", "in\u00b7dem", "sie", "die", "n\u00e4cht\u00b7li\u00b7chen", "Pfa\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Walleten, allzu beleuchtet die Erde", "tokens": ["Wal\u00b7le\u00b7ten", ",", "all\u00b7zu", "be\u00b7leuch\u00b7tet", "die", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PTKA", "ADJD", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Durch des Mondes annoch vollst\u00e4ndiges,", "tokens": ["Durch", "des", "Mon\u00b7des", "an\u00b7noch", "voll\u00b7st\u00e4n\u00b7di\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ohne Befleckung erscheinendes Licht war.", "tokens": ["Oh\u00b7ne", "Be\u00b7fle\u00b7ckung", "er\u00b7schei\u00b7nen\u00b7des", "Licht", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Rathe, was thaten die ganz heillosen?", "tokens": ["Ra\u00b7the", ",", "was", "tha\u00b7ten", "die", "ganz", "heil\u00b7lo\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ART", "ADV", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Stiegen empor mit klebrigem Topfe", "tokens": ["Stie\u00b7gen", "em\u00b7por", "mit", "kleb\u00b7ri\u00b7gem", "Top\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und bestrichen den Mond mit Theer.", "tokens": ["Und", "be\u00b7stri\u00b7chen", "den", "Mond", "mit", "Theer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "D\u00fcster und lichtlos wurde der Arme,", "tokens": ["D\u00fcs\u00b7ter", "und", "licht\u00b7los", "wur\u00b7de", "der", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "All sein freundliches Lachen und Laben", "tokens": ["All", "sein", "freund\u00b7li\u00b7ches", "La\u00b7chen", "und", "La\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Unter die widrige Masse begraben,", "tokens": ["Un\u00b7ter", "die", "wid\u00b7ri\u00b7ge", "Mas\u00b7se", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Und es erschrack \u00fcber seine Verw\u00fcstung", "tokens": ["Und", "es", "er\u00b7schrack", "\u00fc\u00b7ber", "sei\u00b7ne", "Ver\u00b7w\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Alles auf Erden und trauerte schwer.", "tokens": ["Al\u00b7les", "auf", "Er\u00b7den", "und", "trau\u00b7er\u00b7te", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Tapferen aber und m\u00e4chtigen Ringens", "tokens": ["Tap\u00b7fe\u00b7ren", "a\u00b7ber", "und", "m\u00e4ch\u00b7ti\u00b7gen", "Rin\u00b7gens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "Brach mit der Zeit sein Strahl die Verh\u00fcllung,", "tokens": ["Brach", "mit", "der", "Zeit", "sein", "Strahl", "die", "Ver\u00b7h\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Und es erschien der erquickliche wieder,", "tokens": ["Und", "es", "er\u00b7schien", "der", "er\u00b7quick\u00b7li\u00b7che", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Gr\u00fc\u00dfte der alte, der liebliche Glanz", "tokens": ["Gr\u00fc\u00df\u00b7te", "der", "al\u00b7te", ",", "der", "lieb\u00b7li\u00b7che", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.18": {"text": "Doch nicht ganz.", "tokens": ["Doch", "nicht", "ganz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Jene, die Frechen \u2013", "tokens": ["Je\u00b7ne", ",", "die", "Fre\u00b7chen", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Allzu verrucht entartetem Wesen", "tokens": ["All\u00b7zu", "ver\u00b7rucht", "ent\u00b7ar\u00b7te\u00b7tem", "We\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Ein allwarnendes, ewiges Beispiel \u2013", "tokens": ["Ein", "all\u00b7war\u00b7nen\u00b7des", ",", "e\u00b7wi\u00b7ges", "Bei\u00b7spiel", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJA", "NN", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Hangen geblieben mit ihrem Topfe", "tokens": ["Han\u00b7gen", "ge\u00b7blie\u00b7ben", "mit", "ih\u00b7rem", "Top\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "Waren sie beid' am bestrichenen Monde,", "tokens": ["Wa\u00b7ren", "sie", "beid'", "am", "be\u00b7stri\u00b7che\u00b7nen", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.24": {"text": "Hangen \u2013 es sieht's dein Auge \u2013 noch immer,", "tokens": ["Han\u00b7gen", "\u2013", "es", "sieht's", "dein", "Au\u00b7ge", "\u2013", "noch", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.25": {"text": "Tr\u00fcben dem Monde den heiteren Schimmer,", "tokens": ["Tr\u00fc\u00b7ben", "dem", "Mon\u00b7de", "den", "hei\u00b7te\u00b7ren", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.26": {"text": "Bilden die dunklen Figuren in ihm.", "tokens": ["Bil\u00b7den", "die", "dunk\u00b7len", "Fi\u00b7gu\u00b7ren", "in", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}