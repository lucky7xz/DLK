{"dta.poem.8027": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Sieger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Anzuschauen das Turnei,", "tokens": ["An\u00b7zu\u00b7schau\u00b7en", "das", "Tur\u00b7nei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00dfen hundert Frauen droben;", "tokens": ["Sa\u00b7\u00dfen", "hun\u00b7dert", "Frau\u00b7en", "dro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese waren nur das Laub,", "tokens": ["Die\u00b7se", "wa\u00b7ren", "nur", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine F\u00fcrstin war die Rose.", "tokens": ["Mei\u00b7ne", "F\u00fcrs\u00b7tin", "war", "die", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufw\u00e4rts blickt\u2019 ich keck zu ihr,", "tokens": ["Auf\u00b7w\u00e4rts", "blickt'", "ich", "keck", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Adler blickt zur Sonne.", "tokens": ["Wie", "der", "Ad\u00b7ler", "blickt", "zur", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie da meiner Wangen Glut", "tokens": ["Wie", "da", "mei\u00b7ner", "Wan\u00b7gen", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Visier durchbrennen wollte!", "tokens": ["Das", "Vi\u00b7sier", "durch\u00b7bren\u00b7nen", "woll\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Wie des Herzens k\u00fchner Schlag", "tokens": ["Wie", "des", "Her\u00b7zens", "k\u00fch\u00b7ner", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schier den Panzer durchgebrochen!", "tokens": ["Schier", "den", "Pan\u00b7zer", "durch\u00b7ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihrer Blicke sanfter Schein", "tokens": ["Ih\u00b7rer", "Bli\u00b7cke", "sanf\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "War in mir zu wildem Lodern,", "tokens": ["War", "in", "mir", "zu", "wil\u00b7dem", "Lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ihrer Rede mildes Wehn", "tokens": ["Ih\u00b7rer", "Re\u00b7de", "mil\u00b7des", "Wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "War in mir zu Sturmestoben,", "tokens": ["War", "in", "mir", "zu", "Stur\u00b7me\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sie, der sch\u00f6ne Maientag,", "tokens": ["Sie", ",", "der", "sch\u00f6\u00b7ne", "Mai\u00b7en\u00b7tag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "In mir zum Gewitter worden.", "tokens": ["In", "mir", "zum", "Ge\u00b7wit\u00b7ter", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Unaufhaltbar brach ich los,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7bar", "brach", "ich", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sieghaft Alles niederdonnernd.", "tokens": ["Sieg\u00b7haft", "Al\u00b7les", "nie\u00b7der\u00b7don\u00b7nernd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}