{"textgrid.poem.43967": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Damit genung! Es ist vergebens.", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Damit genung! Es ist vergebens.", "tokens": ["Da\u00b7mit", "ge\u00b7nung", "!", "Es", "ist", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Einsamkeit", "tokens": ["In", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Begehrt mein Leid", "tokens": ["Be\u00b7gehrt", "mein", "Leid"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Schlu\u00df des schweren Lebens.", "tokens": ["Den", "Schlu\u00df", "des", "schwe\u00b7ren", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein treues Lieben", "tokens": ["Mein", "treu\u00b7es", "Lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bringt nur Betr\u00fcben", "tokens": ["Bringt", "nur", "Be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und schliest mich mit der stummen Pein", "tokens": ["Und", "schliest", "mich", "mit", "der", "stum\u00b7men", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hier zwischen Berg und Th\u00e4ler ein.", "tokens": ["Hier", "zwi\u00b7schen", "Berg", "und", "Th\u00e4\u00b7ler", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den sanften West bewegt mein Klagen,", "tokens": ["Den", "sanf\u00b7ten", "West", "be\u00b7wegt", "mein", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es rauscht die Bach", "tokens": ["Es", "rauscht", "die", "Bach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Seufzern nach", "tokens": ["Den", "Seuf\u00b7zern", "nach"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Aus Mitleid meiner Plagen;", "tokens": ["Aus", "Mit\u00b7leid", "mei\u00b7ner", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die V\u00f6gel schweigen,", "tokens": ["Die", "V\u00f6\u00b7gel", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Um nur zu zeigen,", "tokens": ["Um", "nur", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df deine sch\u00f6ne Tyranney", "tokens": ["Da\u00df", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch Thieren \u00fcberlegen sey.", "tokens": ["Auch", "Thie\u00b7ren", "\u00fc\u00b7berl\u00b7e\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was soll ich thun? Was soll ich bitten?", "tokens": ["Was", "soll", "ich", "thun", "?", "Was", "soll", "ich", "bit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um H\u00fclf in Noth?", "tokens": ["Um", "H\u00fclf", "in", "Noth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nein, um den Tod.", "tokens": ["Nein", ",", "um", "den", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Den hab ich l\u00e4ngst erlidten;", "tokens": ["Den", "hab", "ich", "l\u00e4ngst", "er\u00b7lid\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn bey dem Triebe", "tokens": ["Denn", "bey", "dem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Verworfner Liebe", "tokens": ["Ver\u00b7worf\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Stirbt jeder mit vermehrter Qual", "tokens": ["Stirbt", "je\u00b7der", "mit", "ver\u00b7mehr\u00b7ter", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Tages mehr als tausendmahl.", "tokens": ["Des", "Ta\u00b7ges", "mehr", "als", "tau\u00b7send\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So sterb auch ich; ja, wenn ich st\u00fcrbe,", "tokens": ["So", "sterb", "auch", "ich", ";", "ja", ",", "wenn", "ich", "st\u00fcr\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "PTKANT", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcst ich doch,", "tokens": ["So", "w\u00fcst", "ich", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df dies mein Joch", "tokens": ["Da\u00df", "dies", "mein", "Joch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zugleich mit mir verd\u00fcrbe;", "tokens": ["Zu\u00b7gleich", "mit", "mir", "ver\u00b7d\u00fcr\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich l\u00e4g und schliefe", "tokens": ["Ich", "l\u00e4g", "und", "schlie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In jener Tiefe,", "tokens": ["In", "je\u00b7ner", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wo keine Last, die mich bedeckt,", "tokens": ["Wo", "kei\u00b7ne", "Last", ",", "die", "mich", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ungebohrne Volck erschr\u00f6ckt.", "tokens": ["Das", "un\u00b7ge\u00b7bohr\u00b7ne", "Volck", "er\u00b7schr\u00f6ckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer sagt mir, ob und wo ich lebe?", "tokens": ["Wer", "sagt", "mir", ",", "ob", "und", "wo", "ich", "le\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kind, in dir,", "tokens": ["Mein", "Kind", ",", "in", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Um das ich hier", "tokens": ["Um", "das", "ich", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mein Blut dem Kummer gebe,", "tokens": ["Mein", "Blut", "dem", "Kum\u00b7mer", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Blut vom Herzen,", "tokens": ["Mein", "Blut", "vom", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Das in den Schmerzen,", "tokens": ["Das", "in", "den", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die dein verstockter Sinn ern\u00e4hrt,", "tokens": ["Die", "dein", "ver\u00b7stock\u00b7ter", "Sinn", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich durch und in sich selbst verzehrt.", "tokens": ["Sich", "durch", "und", "in", "sich", "selbst", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "KON", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich sage viel; doch, Engel, wi\u00dfe,", "tokens": ["Ich", "sa\u00b7ge", "viel", ";", "doch", ",", "En\u00b7gel", ",", "wi\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "$,", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dencke mehr.", "tokens": ["Ich", "den\u00b7cke", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gieb noch Geh\u00f6r", "tokens": ["Gieb", "noch", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADV", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und st\u00e4rcke mich durch K\u00fc\u00dfe!", "tokens": ["Und", "st\u00e4r\u00b7cke", "mich", "durch", "K\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst bringt mein Sterben", "tokens": ["Sonst", "bringt", "mein", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auch dein Verderben", "tokens": ["Auch", "dein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Durch dieses tief geholte Weh,", "tokens": ["Durch", "die\u00b7ses", "tief", "ge\u00b7hol\u00b7te", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dem ich gleich zur Grube geh.", "tokens": ["Mit", "dem", "ich", "gleich", "zur", "Gru\u00b7be", "geh."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPRART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}