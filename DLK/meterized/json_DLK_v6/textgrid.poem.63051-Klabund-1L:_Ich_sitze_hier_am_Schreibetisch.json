{"textgrid.poem.63051": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sitze hier am Schreibetisch", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sitze hier am Schreibetisch", "tokens": ["Ich", "sit\u00b7ze", "hier", "am", "Schrei\u00b7be\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schreibe ein Gedichte,", "tokens": ["Und", "schrei\u00b7be", "ein", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Indem ich in die Tinte wisch", "tokens": ["In\u00b7dem", "ich", "in", "die", "Tin\u00b7te", "wisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mein Gebet verrichte.", "tokens": ["Und", "mein", "Ge\u00b7bet", "ver\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "So giebt sich spiegelnd Vers an Vers", "tokens": ["So", "giebt", "sich", "spie\u00b7gelnd", "Vers", "an", "Vers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In \u00f6lgemuter Gl\u00e4tte.", "tokens": ["In", "\u00f6l\u00b7ge\u00b7mu\u00b7ter", "Gl\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur selten fragt man sich: Wie w\u00e4rs,", "tokens": ["Nur", "sel\u00b7ten", "fragt", "man", "sich", ":", "Wie", "w\u00e4rs", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "PRF", "$.", "PWAV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es mehr ", "tokens": ["Wenn", "es", "mehr"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die Seele tut mir garnicht weh,", "tokens": ["Die", "See\u00b7le", "tut", "mir", "gar\u00b7nicht", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist ganz unbeteiligt.", "tokens": ["Sie", "ist", "ganz", "un\u00b7be\u00b7tei\u00b7ligt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch sich selbst geheiligt.", "tokens": ["Und", "durch", "sich", "selbst", "ge\u00b7hei\u00b7ligt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Abends geh ich mit ihr aus,", "tokens": ["Des", "A\u00b7bends", "geh", "ich", "mit", "ihr", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Knopfloch eine Dalie.", "tokens": ["Im", "Kno\u00b7pfloch", "ei\u00b7ne", "Da\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich selber hei\u00dfe Stanislaus,", "tokens": ["Ich", "sel\u00b7ber", "hei\u00b7\u00dfe", "Sta\u00b7nis\u00b7laus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie aber hei\u00dft Amalie.", "tokens": ["Sie", "a\u00b7ber", "hei\u00dft", "A\u00b7ma\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}