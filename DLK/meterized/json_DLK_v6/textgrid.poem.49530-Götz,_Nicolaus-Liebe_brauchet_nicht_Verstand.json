{"textgrid.poem.49530": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Liebe brauchet nicht Verstand", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Zeiten rei\u00dfte der Verstand", "tokens": ["Vor", "Zei\u00b7ten", "rei\u00df\u00b7te", "der", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Amathunt, wo er die K\u00f6nigin ", "tokens": ["Nach", "A\u00b7mat\u00b7hunt", ",", "wo", "er", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den blinden ", "tokens": ["Den", "blin\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Bey denen er, so gern als ich, geblieben w\u00e4re.", "tokens": ["Bey", "de\u00b7nen", "er", ",", "so", "gern", "als", "ich", ",", "ge\u00b7blie\u00b7ben", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "ADV", "ADV", "KOUS", "PPER", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er bot sich allen an, that artig und galant.", "tokens": ["Er", "bot", "sich", "al\u00b7len", "an", ",", "that", "ar\u00b7tig", "und", "ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "PTKVZ", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer mich zum F\u00fchrer w\u00e4hlt, wird, sprach er, niemahls gleiten:", "tokens": ["Wer", "mich", "zum", "F\u00fch\u00b7rer", "w\u00e4hlt", ",", "wird", ",", "sprach", "er", ",", "nie\u00b7mahls", "glei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich f\u00fchr ihn immer an der Hand! \u2013 \u2013", "tokens": ["Ich", "f\u00fchr", "ihn", "im\u00b7mer", "an", "der", "Hand", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Besonders meinen Sohn, wann er den Bogen spannt!", "tokens": ["Be\u00b7son\u00b7ders", "mei\u00b7nen", "Sohn", ",", "wann", "er", "den", "Bo\u00b7gen", "spannt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die abzuschaffen macht zu viel Beschwerlichkeiten;", "tokens": ["Die", "ab\u00b7zu\u00b7schaf\u00b7fen", "macht", "zu", "viel", "Be\u00b7schwer\u00b7lich\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIZU", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum wandert immerhin zur\u00fcck in euer Land,", "tokens": ["Drum", "wan\u00b7dert", "im\u00b7mer\u00b7hin", "zu\u00b7r\u00fcck", "in", "eu\u00b7er", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein allerliebster Herr Pedant;", "tokens": ["Mein", "al\u00b7ler\u00b7liebs\u00b7ter", "Herr", "Pe\u00b7dant", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann ", "tokens": ["Dann"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}