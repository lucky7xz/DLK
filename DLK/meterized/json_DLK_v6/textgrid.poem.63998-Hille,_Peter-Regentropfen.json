{"textgrid.poem.63998": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Regentropfen", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regentropfen warm und gro\u00df", "tokens": ["Re\u00b7gen\u00b7trop\u00b7fen", "warm", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Machen aus der Nacht sich los,", "tokens": ["Ma\u00b7chen", "aus", "der", "Nacht", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Regentropfen warm und gro\u00df.", "tokens": ["Re\u00b7gen\u00b7trop\u00b7fen", "warm", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da die Nacht steht ganz in Glanz,", "tokens": ["Da", "die", "Nacht", "steht", "ganz", "in", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Einen Augenblick da stand's,", "tokens": ["Ei\u00b7nen", "Au\u00b7gen\u00b7blick", "da", "stan\u00b7d's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Geisterantlitz, da entschwand's.", "tokens": ["Ein", "Geis\u00b7ter\u00b7ant\u00b7litz", ",", "da", "ent\u00b7schwand'", "s."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ART", "NN", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da, ein Blitz hat Licht gemacht,", "tokens": ["Da", ",", "ein", "Blitz", "hat", "Licht", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz in Glanz da stand die Nacht,", "tokens": ["Ganz", "in", "Glanz", "da", "stand", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da, ein Blitz hat Licht gemacht.", "tokens": ["Da", ",", "ein", "Blitz", "hat", "Licht", "ge\u00b7macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Helle wird im Lied das Leid,", "tokens": ["Hel\u00b7le", "wird", "im", "Lied", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtet auf wie ein Geschmeid,", "tokens": ["Leuch\u00b7tet", "auf", "wie", "ein", "Ge\u00b7schmeid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtend wird im Lied das Leid.", "tokens": ["Leuch\u00b7tend", "wird", "im", "Lied", "das", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und da steht es in der Nacht,", "tokens": ["Und", "da", "steht", "es", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Still in seiner Geisterpracht", "tokens": ["Still", "in", "sei\u00b7ner", "Geis\u00b7ter\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht sein Antlitz in der Nacht.", "tokens": ["Steht", "sein", "Ant\u00b7litz", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Liedertropfen warm und gro\u00df", "tokens": ["Lie\u00b7der\u00b7trop\u00b7fen", "warm", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6sen aus dem Leid sich los,", "tokens": ["L\u00f6\u00b7sen", "aus", "dem", "Leid", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liedertropfen warm und gro\u00df.", "tokens": ["Lie\u00b7der\u00b7trop\u00b7fen", "warm", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}