{"textgrid.poem.39923": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Steinlacherin und der Russe", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort steht der fremde Feldhauptmann", "tokens": ["Dort", "steht", "der", "frem\u00b7de", "Feld\u00b7haupt\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den M\u00e4gden zu Gefallen,", "tokens": ["Den", "M\u00e4g\u00b7den", "zu", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht sich keck die Weiber an,", "tokens": ["Er", "sieht", "sich", "keck", "die", "Wei\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die aus der Kirche wallen.", "tokens": ["Die", "aus", "der", "Kir\u00b7che", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4gdlein tritt zuletzt heraus,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "tritt", "zu\u00b7letzt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6nst' im ganzen Flecken,", "tokens": ["Die", "sch\u00f6nst'", "im", "gan\u00b7zen", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schickt die blauen Augen aus,", "tokens": ["Sie", "schickt", "die", "blau\u00b7en", "Au\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruft sie heim vor Schrecken.", "tokens": ["Und", "ruft", "sie", "heim", "vor", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es s\u00e4umt geheimni\u00dfvoll der Flor", "tokens": ["Es", "s\u00e4umt", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "der", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die langen Augenlider,", "tokens": ["Die", "lan\u00b7gen", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es dr\u00e4ngt die keusche Brust hervor", "tokens": ["Es", "dr\u00e4ngt", "die", "keu\u00b7sche", "Brust", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das weiche Scharlachmieder.", "tokens": ["Das", "wei\u00b7che", "Schar\u00b7lach\u00b7mie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf blanken Spitzen lagern sich", "tokens": ["Auf", "blan\u00b7ken", "Spit\u00b7zen", "la\u00b7gern", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Haares braune Flechten,", "tokens": ["Des", "Haa\u00b7res", "brau\u00b7ne", "Flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die linke Hand liegt tugendlich", "tokens": ["Die", "lin\u00b7ke", "Hand", "liegt", "tu\u00b7gend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am G\u00fcrtel auf der rechten.", "tokens": ["Am", "G\u00fcr\u00b7tel", "auf", "der", "rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie schreitet f\u00fcrder mit dem Buch", "tokens": ["Sie", "schrei\u00b7tet", "f\u00fcr\u00b7der", "mit", "dem", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Hause fromm und munter,", "tokens": ["Zu", "Hau\u00b7se", "fromm", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch ferne gl\u00e4nzt das blaue Tuch,", "tokens": ["Noch", "fer\u00b7ne", "gl\u00e4nzt", "das", "blau\u00b7e", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wallt den Leib herunter.", "tokens": ["Es", "wallt", "den", "Leib", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Kriegsmann geht, im Blicke Glut,", "tokens": ["Der", "Kriegs\u00b7mann", "geht", ",", "im", "Bli\u00b7cke", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie tiefdurchgl\u00fchte Kohlen,", "tokens": ["Wie", "tief\u00b7durch\u00b7gl\u00fch\u00b7te", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Wirt befiehlt sein Uebermut,", "tokens": ["Dem", "Wirt", "be\u00b7fiehlt", "sein", "Ue\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die junge Magd zu holen.", "tokens": ["Die", "jun\u00b7ge", "Magd", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die b\u00e4rt'ge Lippe r\u00fchret er", "tokens": ["Die", "b\u00e4rt'\u00b7ge", "Lip\u00b7pe", "r\u00fch\u00b7ret", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zu raschem, kurzem Worte,", "tokens": ["Zu", "ra\u00b7schem", ",", "kur\u00b7zem", "Wor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da tr\u00e4gt der Wirt ein Herz gar schwer", "tokens": ["Da", "tr\u00e4gt", "der", "Wirt", "ein", "Herz", "gar", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seines Nachbars Pforte.", "tokens": ["Zu", "sei\u00b7nes", "Nach\u00b7bars", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der graue Vater h\u00f6rt's mit Harm,", "tokens": ["Der", "grau\u00b7e", "Va\u00b7ter", "h\u00f6rt's", "mit", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat seinen Gram verborgen:", "tokens": ["Hat", "sei\u00b7nen", "Gram", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbkomm,\u00ab spricht er, \u00bbKind, an meinem Arm;", "tokens": ["\u00bb", "komm", ",", "\u00ab", "spricht", "er", ",", "\u00bb", "Kind", ",", "an", "mei\u00b7nem", "Arm", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df den im Himmel sorgen!\u00ab", "tokens": ["La\u00df", "den", "im", "Him\u00b7mel", "sor\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ART", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So f\u00fchrt er sie dem Hause zu,", "tokens": ["So", "f\u00fchrt", "er", "sie", "dem", "Hau\u00b7se", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wappnet sich zum Streite:", "tokens": ["Er", "wapp\u00b7net", "sich", "zum", "Strei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnach meinem Kind, Herr, fragtest du?", "tokens": ["\u00bb", "nach", "mei\u00b7nem", "Kind", ",", "Herr", ",", "frag\u00b7test", "du", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier steht es mir zur Seite.\u00ab", "tokens": ["Hier", "steht", "es", "mir", "zur", "Sei\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Jungfrau lehnt sich an den Greis,", "tokens": ["Die", "Jung\u00b7frau", "lehnt", "sich", "an", "den", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit zagendem Vertrauen,", "tokens": ["Mit", "za\u00b7gen\u00b7dem", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es war an seiner Locken Eis", "tokens": ["Es", "war", "an", "sei\u00b7ner", "Lo\u00b7cken", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Bl\u00fctenhaupt zu schauen.", "tokens": ["Ihr", "Bl\u00fc\u00b7ten\u00b7haupt", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der J\u00fcngling aber stellt sich fern,", "tokens": ["Der", "J\u00fcng\u00b7ling", "a\u00b7ber", "stellt", "sich", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er scheut, sie zu verletzen,", "tokens": ["Er", "scheut", ",", "sie", "zu", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er winkt mit regem Augenstern,", "tokens": ["Er", "winkt", "mit", "re\u00b7gem", "Au\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bis sie sich beide setzen.", "tokens": ["Bis", "sie", "sich", "bei\u00b7de", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann setzt er sich zu unterst an,", "tokens": ["Dann", "setzt", "er", "sich", "zu", "un\u00b7terst", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo er im Sonnenlichte", "tokens": ["Wo", "er", "im", "Son\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich recht ergehn und laben kann", "tokens": ["Sich", "recht", "er\u00b7gehn", "und", "la\u00b7ben", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVINF", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihrem Angesichte.", "tokens": ["Auf", "ih\u00b7rem", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er blickt in ihrer Wangen Blut,", "tokens": ["Er", "blickt", "in", "ih\u00b7rer", "Wan\u00b7gen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer Augen Bl\u00e4ue,", "tokens": ["In", "ih\u00b7rer", "Au\u00b7gen", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Hand ihm auf der Stirne ruht,", "tokens": ["Die", "Hand", "ihm", "auf", "der", "Stir\u00b7ne", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schaut, und schaut auf's Neue.", "tokens": ["Er", "schaut", ",", "und", "schaut", "auf's", "Neu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da weicht aus seiner Brust die Pein,", "tokens": ["Da", "weicht", "aus", "sei\u00b7ner", "Brust", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wird sein Auge milde,", "tokens": ["Da", "wird", "sein", "Au\u00b7ge", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Sinn wird still, sein Herz wird rein", "tokens": ["Sein", "Sinn", "wird", "still", ",", "sein", "Herz", "wird", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Gottes Ebenbilde.", "tokens": ["Vor", "Got\u00b7tes", "E\u00b7ben\u00b7bil\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es l\u00e4\u00dft sein Mund aus rauhem Bart", "tokens": ["Es", "l\u00e4\u00dft", "sein", "Mund", "aus", "rau\u00b7hem", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kindlich L\u00e4cheln schauen,", "tokens": ["Ein", "kind\u00b7lich", "L\u00e4\u00b7cheln", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bethr\u00e4nte Blicke weben zart", "tokens": ["Be\u00b7thr\u00e4n\u00b7te", "Bli\u00b7cke", "we\u00b7ben", "zart"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich unter dunkeln Brauen.", "tokens": ["Sich", "un\u00b7ter", "dun\u00b7keln", "Brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Dann steht er auf und rei\u00dft sich los,", "tokens": ["Dann", "steht", "er", "auf", "und", "rei\u00dft", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Langt nach des Vaters H\u00e4nden,", "tokens": ["Langt", "nach", "des", "Va\u00b7ters", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warf einen Ring ihm in den Schoos,", "tokens": ["Warf", "ei\u00b7nen", "Ring", "ihm", "in", "den", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und th\u00e4t sich schweigend wenden.", "tokens": ["Und", "th\u00e4t", "sich", "schwei\u00b7gend", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+