{"dta.poem.15725": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der erste Urwohnsitz der Menschen mit vier Fl\u00fcssen,", "tokens": ["Der", "ers\u00b7te", "Ur\u00b7wohn\u00b7sitz", "der", "Men\u00b7schen", "mit", "vier", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jetzt noch alle Welt von dort bew\u00e4ssern m\u00fcssen,", "tokens": ["Die", "jetzt", "noch", "al\u00b7le", "Welt", "von", "dort", "be\u00b7w\u00e4s\u00b7sern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "APPR", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als den sich mehrenden zu enge ward das Haus,", "tokens": ["Als", "den", "sich", "meh\u00b7ren\u00b7den", "zu", "en\u00b7ge", "ward", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "ADV", "APPR", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da zogen wie die Str\u00f6m' auch vier Geschlechter aus.", "tokens": ["Da", "zo\u00b7gen", "wie", "die", "Str\u00f6m'", "auch", "vier", "Ge\u00b7schlech\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Voll klarem Weltverstand, entgegen zog das eine", "tokens": ["Voll", "kla\u00b7rem", "Welt\u00b7ver\u00b7stand", ",", "ent\u00b7ge\u00b7gen", "zog", "das", "ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "PTKVZ", "VVFIN", "ART", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sonne, da\u00df ihr Licht stets fr\u00fcher ihm erscheine.", "tokens": ["Der", "Son\u00b7ne", ",", "da\u00df", "ihr", "Licht", "stets", "fr\u00fc\u00b7her", "ihm", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Voll r\u00fcst'ger Thatkraft zog nach Westen hin das andre,", "tokens": ["Voll", "r\u00fcst'\u00b7ger", "That\u00b7kraft", "zog", "nach", "Wes\u00b7ten", "hin", "das", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mit der Sonne Lauf es alle Welt durchwandre.", "tokens": ["Da\u00df", "mit", "der", "Son\u00b7ne", "Lauf", "es", "al\u00b7le", "Welt", "durch\u00b7wand\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Entflammt von Sonnendurst, zum hei\u00dfen S\u00fcden zog", "tokens": ["Ent\u00b7flammt", "von", "Son\u00b7nen\u00b7durst", ",", "zum", "hei\u00b7\u00dfen", "S\u00fc\u00b7den", "zog"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dritte, wo es voll von Glanz und Glut sich sog.", "tokens": ["Das", "drit\u00b7te", ",", "wo", "es", "voll", "von", "Glanz", "und", "Glut", "sich", "so\u00b7g."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "$,", "PWAV", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "PRF", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit starrem Sinne nahm das vierte seine Reise", "tokens": ["Mit", "star\u00b7rem", "Sin\u00b7ne", "nahm", "das", "vier\u00b7te", "sei\u00b7ne", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem festen Nordstrom zu, wo es erstarrt' im Eise.", "tokens": ["Dem", "fes\u00b7ten", "Nord\u00b7strom", "zu", ",", "wo", "es", "er\u00b7starrt'", "im", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "So auseinander sind aus einem Sitz gewichen", "tokens": ["So", "aus\u00b7ein\u00b7an\u00b7der", "sind", "aus", "ei\u00b7nem", "Sitz", "ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die vier, un\u00e4hnlich nun gleich ihren Himmelsstrichen.", "tokens": ["Die", "vier", ",", "un\u00b7\u00e4hn\u00b7lich", "nun", "gleich", "ih\u00b7ren", "Him\u00b7mels\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die wechselseitige Erinnrung ist geschwunden,", "tokens": ["Die", "wech\u00b7sel\u00b7sei\u00b7ti\u00b7ge", "E\u00b7rinn\u00b7rung", "ist", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch h\u00e4lt auf Erden sie des Himmels Macht verbunden.", "tokens": ["Doch", "h\u00e4lt", "auf", "Er\u00b7den", "sie", "des", "Him\u00b7mels", "Macht", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Vom Ost zum Westen ist die Sonne stets gezogen,", "tokens": ["Vom", "Ost", "zum", "Wes\u00b7ten", "ist", "die", "Son\u00b7ne", "stets", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zwischen S\u00fcd und Nord gespannt ein Regenbogen.", "tokens": ["Und", "zwi\u00b7schen", "S\u00fcd", "und", "Nord", "ge\u00b7spannt", "ein", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}