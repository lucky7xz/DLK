{"textgrid.poem.42276": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An die Bienen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr wissen, holde Bienen,", "tokens": ["Wollt", "ihr", "wis\u00b7sen", ",", "hol\u00b7de", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr s\u00fc\u00dfe Beute liebt,", "tokens": ["Die", "ihr", "s\u00fc\u00b7\u00dfe", "Beu\u00b7te", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo es mehr, als hier im Gr\u00fcnen,", "tokens": ["Wo", "es", "mehr", ",", "als", "hier", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "KOUS", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Honigreiche Blumen gibt?", "tokens": ["Ho\u00b7ni\u00b7grei\u00b7che", "Blu\u00b7men", "gibt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Statt die tausend auszunippen,", "tokens": ["Statt", "die", "tau\u00b7send", "aus\u00b7zu\u00b7nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "CARD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die euch Florens Milde beut,", "tokens": ["Die", "euch", "Flo\u00b7rens", "Mil\u00b7de", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Saugt aus Amaryllis Lippen", "tokens": ["Saugt", "aus", "A\u00b7ma\u00b7ryl\u00b7lis", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller tausend S\u00fc\u00dfigkeit.", "tokens": ["Al\u00b7ler", "tau\u00b7send", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Florens sch\u00f6ne Kinder r\u00f6tet", "tokens": ["Flo\u00b7rens", "sch\u00f6\u00b7ne", "Kin\u00b7der", "r\u00f6\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur der Fr\u00fchlingssonne Licht:", "tokens": ["Nur", "der", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", "Licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amaryllis Blumen t\u00f6tet", "tokens": ["A\u00b7ma\u00b7ryl\u00b7lis", "Blu\u00b7men", "t\u00f6\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch der strenge Winter nicht.", "tokens": ["Auch", "der", "stren\u00b7ge", "Win\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener ausgeleerte H\u00fclle", "tokens": ["Je\u00b7ner", "aus\u00b7ge\u00b7leer\u00b7te", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird nicht wieder angef\u00fcllt:", "tokens": ["Wird", "nicht", "wie\u00b7der", "an\u00b7ge\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber nie versiegt die F\u00fclle,", "tokens": ["A\u00b7ber", "nie", "ver\u00b7siegt", "die", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die aus diesem Kelche quillt.", "tokens": ["Die", "aus", "die\u00b7sem", "Kel\u00b7che", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eins, nur Eins sei euch geklaget!", "tokens": ["Eins", ",", "nur", "Eins", "sei", "euch", "ge\u00b7kla\u00b7get", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Eh' ihr auf dies Purpurrot", "tokens": ["Eh'", "ihr", "auf", "dies", "Pur\u00b7pur\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure seidnen Fl\u00fcgel waget,", "tokens": ["Eu\u00b7re", "seid\u00b7nen", "Fl\u00fc\u00b7gel", "wa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt, ihr Lieben, was euch droht!", "tokens": ["H\u00f6rt", ",", "ihr", "Lie\u00b7ben", ",", "was", "euch", "droht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, ein hei\u00dfer Ku\u00df hat neulich", "tokens": ["Ach", ",", "ein", "hei\u00b7\u00dfer", "Ku\u00df", "hat", "neu\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Gefahr mir kund gemacht.", "tokens": ["Die", "Ge\u00b7fahr", "mir", "kund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt die Fl\u00fcgel, warn' ich treulich,", "tokens": ["Nehmt", "die", "Fl\u00fc\u00b7gel", ",", "warn'", "ich", "treu\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja vor dieser Glut in acht!", "tokens": ["Ja", "vor", "die\u00b7ser", "Glut", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PDAT", "NN", "APPR", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}