{"textgrid.poem.65415": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Goldperlen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcbe Regentropfen fielen drau\u00dfen in dein Lockenhaar.", "tokens": ["Tr\u00fc\u00b7be", "Re\u00b7gen\u00b7trop\u00b7fen", "fie\u00b7len", "drau\u00b7\u00dfen", "in", "dein", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sch\u00fcttle sie mir in die H\u00e4nde, Perlen sind es goldenklar.", "tokens": ["Sch\u00fctt\u00b7le", "sie", "mir", "in", "die", "H\u00e4n\u00b7de", ",", "Per\u00b7len", "sind", "es", "gol\u00b7den\u00b7klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}