{"textgrid.poem.58309": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "347. Ein eng'lisches Schauspiel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist es nicht so sch\u00f6n", "tokens": ["Wie", "ist", "es", "nicht", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und lieblich anzusehn,", "tokens": ["Und", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn des Gerechten Geist mit Gott vergn\u00fcget,", "tokens": ["Wenn", "des", "Ge\u00b7rech\u00b7ten", "Geist", "mit", "Gott", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sanft und still in Schmerz und Leiden lieget", "tokens": ["So", "sanft", "und", "still", "in", "Schmerz", "und", "Lei\u00b7den", "lie\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und k\u00fcsset seines Vaters gute Hand,", "tokens": ["Und", "k\u00fcs\u00b7set", "sei\u00b7nes", "Va\u00b7ters", "gu\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die ihm das Kreuz aus Liebe zugesandt!", "tokens": ["Die", "ihm", "das", "Kreuz", "aus", "Lie\u00b7be", "zu\u00b7ge\u00b7sandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}