{"textgrid.poem.25621": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Peter Wichtig", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinen Epigrammen, sagt Herr Wichtig,", "tokens": ["In", "mei\u00b7nen", "E\u00b7pig\u00b7ram\u00b7men", ",", "sagt", "Herr", "Wich\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sey f\u00fcr den Spott der Gegenstand zu klein.", "tokens": ["Sey", "f\u00fcr", "den", "Spott", "der", "Ge\u00b7gen\u00b7stand", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von allen ist der Tadel zwar nicht richtig,", "tokens": ["Von", "al\u00b7len", "ist", "der", "Ta\u00b7del", "zwar", "nicht", "rich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch r\u00e4um' ich's gern von diesem ein.", "tokens": ["Doch", "r\u00e4um'", "ich's", "gern", "von", "die\u00b7sem", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "PDAT", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}