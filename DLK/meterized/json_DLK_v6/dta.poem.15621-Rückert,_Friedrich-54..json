{"dta.poem.15621": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Daheim im stillen Haus die Seele war befangen,", "tokens": ["Da\u00b7heim", "im", "stil\u00b7len", "Haus", "die", "See\u00b7le", "war", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Derweil der Geist hinaus war in die Welt gegangen.", "tokens": ["Der\u00b7weil", "der", "Geist", "hin\u00b7aus", "war", "in", "die", "Welt", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APZR", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die K\u00f6rperwelt hindurch drang er zur Geisterwelt,", "tokens": ["Die", "K\u00f6r\u00b7per\u00b7welt", "hin\u00b7durch", "drang", "er", "zur", "Geis\u00b7ter\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und dachte kaum zur\u00fcck zur Seel' im stillen Zelt.", "tokens": ["Und", "dach\u00b7te", "kaum", "zu\u00b7r\u00fcck", "zur", "Seel'", "im", "stil\u00b7len", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch als er durch die Welt gekommen war ein St\u00fcck,", "tokens": ["Doch", "als", "er", "durch", "die", "Welt", "ge\u00b7kom\u00b7men", "war", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nahm mit dem Reis'ertrag er seinen Weg zur\u00fcck.", "tokens": ["Nahm", "mit", "dem", "Reis'\u00b7er\u00b7trag", "er", "sei\u00b7nen", "Weg", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er kam und fand die Seel' am Webstuhl eingeschlafen,", "tokens": ["Er", "kam", "und", "fand", "die", "Seel'", "am", "Web\u00b7stuhl", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit erz\u00fcrntem Wort begann er sie zu strafen.", "tokens": ["Und", "mit", "er\u00b7z\u00fcrn\u00b7tem", "Wort", "be\u00b7gann", "er", "sie", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mit Seelenruhe doch die Seele sich erhob", "tokens": ["Mit", "See\u00b7len\u00b7ru\u00b7he", "doch", "die", "See\u00b7le", "sich", "er\u00b7hob"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4chelte: Sieh her! ich schlief nicht, sondern wob.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", ":", "Sieh", "her", "!", "ich", "schlief", "nicht", ",", "son\u00b7dern", "wob", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er sah; gewachsen war im Schlaf das aufgezogene", "tokens": ["Er", "sah", ";", "ge\u00b7wach\u00b7sen", "war", "im", "Schlaf", "das", "auf\u00b7ge\u00b7zo\u00b7ge\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVPP", "VAFIN", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Gewebe wunderbar; so glaubt' ihr der Betrogene.", "tokens": ["Ge\u00b7we\u00b7be", "wun\u00b7der\u00b7bar", ";", "so", "glaubt'", "ihr", "der", "Be\u00b7tro\u00b7ge\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}}}}}