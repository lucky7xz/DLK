{"dta.poem.19876": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Zum Spaz,  \n der sich auf dem Saal gefangen hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Bons dies, Herr Spaz! Ei, seht doch mal!               ", "tokens": ["Bons", "dies", ",", "Herr", "Spaz", "!", "Ei", ",", "seht", "doch", "mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "NN", "NE", "$.", "NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wilkommen hier auf meinem Saal!", "tokens": ["Wil\u00b7kom\u00b7men", "hier", "auf", "mei\u00b7nem", "Saal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist gefangen, sieht er wol?", "tokens": ["Er", "ist", "ge\u00b7fan\u00b7gen", ",", "sieht", "er", "wol", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stelt\u2019 er sich auch noch so tol,", "tokens": ["Und", "stelt'", "er", "sich", "auch", "noch", "so", "tol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fl\u00f6g\u2019 er ewig, kreuz und queer,", "tokens": ["Und", "fl\u00f6g'", "er", "e\u00b7wig", ",", "kreuz", "und", "que\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach allen Fenstern hin und her,", "tokens": ["Nach", "al\u00b7len", "Fens\u00b7tern", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zerbr\u00e4ch\u2019 auch Schnabel sich und Kopf,", "tokens": ["Zer\u00b7br\u00e4ch'", "auch", "Schna\u00b7bel", "sich", "und", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PRF", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er ist gefangen, armer Tropf!", "tokens": ["Er", "ist", "ge\u00b7fan\u00b7gen", ",", "ar\u00b7mer", "Tropf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich sein Despot! und er mein Sklav!", "tokens": ["Ich", "sein", "Des\u00b7pot", "!", "und", "er", "mein", "Sklav", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "KON", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er sey Prinz, Junker, oder Graf,", "tokens": ["Er", "sey", "Prinz", ",", "Jun\u00b7ker", ",", "o\u00b7der", "Graf", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NE", "$,", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bei seinem Spazvolk! \u2014 H\u00f6r\u2019 er nun,", "tokens": ["Bei", "sei\u00b7nem", "Spaz\u00b7volk", "!", "H\u00f6r'", "er", "nun", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Was all\u2019 ich mit ihm k\u00f6nte thun.", "tokens": ["Was", "all'", "ich", "mit", "ihm", "k\u00f6n\u00b7te", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zerzupfen, rupfen, Hals umdrehn \u2014", "tokens": ["Zer\u00b7zup\u00b7fen", ",", "rup\u00b7fen", ",", "Hals", "um\u00b7drehn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da wird nicht Hund noch Hahn nach kr\u00e4hn \u2014", "tokens": ["Da", "wird", "nicht", "Hund", "noch", "Hahn", "nach", "kr\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zerschlagen ihn, mit einem Hieb\u2019,", "tokens": ["Zer\u00b7schla\u00b7gen", "ihn", ",", "mit", "ei\u00b7nem", "Hieb'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und das mit Recht, Herr Galgendieb!", "tokens": ["Und", "das", "mit", "Recht", ",", "Herr", "Gal\u00b7gen\u00b7dieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Weis er die Kirschen, die verschmizt,", "tokens": ["Weis", "er", "die", "Kir\u00b7schen", ",", "die", "ver\u00b7schmizt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er vor dem Maul mir wegstipizt?", "tokens": ["Er", "vor", "dem", "Maul", "mir", "weg\u00b7sti\u00b7pizt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Auch w\u00fcrd\u2019 es F\u00fcrstenkurzweil\u2019 seyn,", "tokens": ["Auch", "w\u00fcrd'", "es", "F\u00fcrs\u00b7ten\u00b7kurz\u00b7weil'", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Liess\u2019 ich den Kater Lips herein.", "tokens": ["Liess'", "ich", "den", "Ka\u00b7ter", "Lips", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wenn ich ja \u00fcbergn\u00e4dig w\u00e4r\u2019,", "tokens": ["Wenn", "ich", "ja", "\u00fc\u00b7berg\u00b7n\u00e4\u00b7dig", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So holt\u2019 ich eine scharfe Scheer\u2019,", "tokens": ["So", "holt'", "ich", "ei\u00b7ne", "schar\u00b7fe", "Scheer'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und schnitt\u2019 ihm ab die Fl\u00fcgelein,", "tokens": ["Und", "schnitt'", "ihm", "ab", "die", "Fl\u00fc\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Zusamt dem kecken Schw\u00e4nzelein.", "tokens": ["Zu\u00b7samt", "dem", "ke\u00b7cken", "Schw\u00e4n\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dann m\u00fcst\u2019 er unter Bett\u2019 und Bank", "tokens": ["Dann", "m\u00fcst'", "er", "un\u00b7ter", "Bett'", "und", "Bank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Im Staube flattern lebenslang. \u2014", "tokens": ["Im", "Stau\u00b7be", "flat\u00b7tern", "le\u00b7bens\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "He! B\u00fcrschgen, wie ist ihm zu Sin? \u2014", "tokens": ["He", "!", "B\u00fcrschgen", ",", "wie", "ist", "ihm", "zu", "Sin", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "PWAV", "VAFIN", "PPER", "APPR", "NE", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Doch, seh\u2019 er, da\u00df ein Mensch ich bin!", "tokens": ["Doch", ",", "seh'", "er", ",", "da\u00df", "ein", "Mensch", "ich", "bin", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ich lass\u2019 ihn wieder frank und frei.", "tokens": ["Ich", "lass'", "ihn", "wie\u00b7der", "frank", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Doch da\u00df stets eingedenk ihm sey,", "tokens": ["Doch", "da\u00df", "stets", "ein\u00b7ge\u00b7denk", "ihm", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die Freiheit sey ein g\u00fcldner Schaz,", "tokens": ["Die", "Frei\u00b7heit", "sey", "ein", "g\u00fcld\u00b7ner", "Schaz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "So hudelt man ihn erst, Herr Spaz,", "tokens": ["So", "hu\u00b7delt", "man", "ihn", "erst", ",", "Herr", "Spaz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und scheucht ihn hin und her husch! husch!", "tokens": ["Und", "scheucht", "ihn", "hin", "und", "her", "husch", "!", "husch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "ADJD", "$.", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Nun Fenster auf! hinaus zu Busch!", "tokens": ["Nun", "Fens\u00b7ter", "auf", "!", "hin\u00b7aus", "zu", "Busch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "$.", "APZR", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}