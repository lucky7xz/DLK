{"textgrid.poem.50443": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "31. An Strephon", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, ", "tokens": ["Hier", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Dass ich mich niemahls mehr verbinde", "tokens": ["Dass", "ich", "mich", "nie\u00b7mahls", "mehr", "ver\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer ", "tokens": ["Mit", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Weil sie im ", "tokens": ["Weil", "sie", "im"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Gnug, dass ", "tokens": ["Gnug", ",", "dass"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Die annoch ", "tokens": ["Die", "an\u00b7noch"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nur eben, doch mir unbewusst,", "tokens": ["Nur", "e\u00b7ben", ",", "doch", "mir", "un\u00b7be\u00b7wusst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Und ich, was von ", "tokens": ["Und", "ich", ",", "was", "von"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Hinfort weiss ich von keinem Triebe,", "tokens": ["Hin\u00b7fort", "weiss", "ich", "von", "kei\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als der mich f\u00fchrt, zu deiner Gunst:", "tokens": ["Als", "der", "mich", "f\u00fchrt", ",", "zu", "dei\u00b7ner", "Gunst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und untern Weibern ist sie ", "tokens": ["Und", "un\u00b7tern", "Wei\u00b7bern", "ist", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und ich halt' itzt in mindrer Acht'", "tokens": ["Und", "ich", "halt'", "itzt", "in", "mind\u00b7rer", "Acht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}