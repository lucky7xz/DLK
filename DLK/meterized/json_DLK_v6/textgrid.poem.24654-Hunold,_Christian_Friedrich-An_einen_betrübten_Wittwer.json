{"textgrid.poem.24654": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen betr\u00fcbten Wittwer", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch-Edler/ wenn dein Schatz den Gang der Welt itzt geht/", "tokens": ["Hoch\u00b7Ed\u00b7ler", "/", "wenn", "dein", "Schatz", "den", "Gang", "der", "Welt", "itzt", "geht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dir doch allezeit vor deinen Augen steht:", "tokens": ["Und", "dir", "doch", "al\u00b7le\u00b7zeit", "vor", "dei\u00b7nen", "Au\u00b7gen", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird die/ die dich nie als nur im Sterben kr\u00e4ncket/", "tokens": ["So", "wird", "die", "/", "die", "dich", "nie", "als", "nur", "im", "Ster\u00b7ben", "kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "$(", "PRELS", "PPER", "ADV", "KOKOM", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht in die Gruft so wohl als in dein Hertz gesencket.", "tokens": ["Nicht", "in", "die", "Gruft", "so", "wohl", "als", "in", "dein", "Hertz", "ge\u00b7sen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "ADV", "KOKOM", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach nur in deine Brust: da ist die Ruhestatt", "tokens": ["Ach", "nur", "in", "dei\u00b7ne", "Brust", ":", "da", "ist", "die", "Ru\u00b7he\u00b7statt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des frommen Ehgemahls/ die dich erfreuet hat;", "tokens": ["Des", "from\u00b7men", "Eh\u00b7ge\u00b7mahls", "/", "die", "dich", "er\u00b7freu\u00b7et", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel als Salomo an einer Frau gepriesen/", "tokens": ["So", "viel", "als", "Sa\u00b7lo\u00b7mo", "an", "ei\u00b7ner", "Frau", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "NE", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat/ da\u00df es m\u00f6glich sey/ dir Gott an Ihr erwiesen.", "tokens": ["Hat", "/", "da\u00df", "es", "m\u00f6g\u00b7lich", "sey", "/", "dir", "Gott", "an", "Ihr", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "PPER", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fromm und auch angenehm/ wie Sara Tugendhafft/", "tokens": ["Fromm", "und", "auch", "an\u00b7ge\u00b7nehm", "/", "wie", "Sa\u00b7ra", "Tu\u00b7gend\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ADJD", "$(", "KOKOM", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Rahel/ deren Flei\u00df im Hause Nutzen schafft.", "tokens": ["Wie", "Ra\u00b7hel", "/", "de\u00b7ren", "Flei\u00df", "im", "Hau\u00b7se", "Nut\u00b7zen", "schafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "PRELAT", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die dir das Gl\u00fcck gewehrt/ was ", "tokens": ["Die", "dir", "das", "Gl\u00fcck", "ge\u00b7wehrt", "/", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$(", "PWS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und auch ", "tokens": ["Und", "auch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "So denckest du an sie auch jeden Augenblick/", "tokens": ["So", "den\u00b7ckest", "du", "an", "sie", "auch", "je\u00b7den", "Au\u00b7gen\u00b7blick", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gehst mit ihr zur Grufft/ sie geht mit dir zur\u00fcck/", "tokens": ["Du", "gehst", "mit", "ihr", "zur", "Grufft", "/", "sie", "geht", "mit", "dir", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "$(", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist sie stets um dich/ und lernet dich erkennen/", "tokens": ["So", "ist", "sie", "stets", "um", "dich", "/", "und", "ler\u00b7net", "dich", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$(", "KON", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Treu-verm\u00e4hlte nie der Tod vermag zu trennen.", "tokens": ["Da\u00df", "Treu\u00b7ver\u00b7m\u00e4hl\u00b7te", "nie", "der", "Tod", "ver\u00b7mag", "zu", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie aber ist sie tod? du siehest sie nicht mehr.", "tokens": ["Wie", "a\u00b7ber", "ist", "sie", "tod", "?", "du", "sie\u00b7hest", "sie", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dennoch f\u00fchlet sie dein z\u00e4rtlich Hertz zusehr:", "tokens": ["Und", "den\u00b7noch", "f\u00fch\u00b7let", "sie", "dein", "z\u00e4rt\u00b7lich", "Hertz", "zu\u00b7sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sind die Augen dir ob der Entfernung tr\u00fcbe/", "tokens": ["Es", "sind", "die", "Au\u00b7gen", "dir", "ob", "der", "Ent\u00b7fer\u00b7nung", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und deine Brust verbleibt das Grab getreuer Liebe.", "tokens": ["Und", "dei\u00b7ne", "Brust", "ver\u00b7bleibt", "das", "Grab", "ge\u00b7treu\u00b7er", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hoch-Edler/ dein Gem\u00fcth kr\u00e4nckt so die Liebe nur.", "tokens": ["Hoch\u00b7Ed\u00b7ler", "/", "dein", "Ge\u00b7m\u00fcth", "kr\u00e4nckt", "so", "die", "Lie\u00b7be", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch die Vernunft in dir ergreifft des Himmels Spur/", "tokens": ["Doch", "die", "Ver\u00b7nunft", "in", "dir", "er\u00b7greifft", "des", "Him\u00b7mels", "Spur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bey der Seeligen sich ebenfalls zu sehen/", "tokens": ["Und", "bey", "der", "See\u00b7li\u00b7gen", "sich", "e\u00b7ben\u00b7falls", "zu", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So will sie auch mit ihr zum dritten Himmel gehen.", "tokens": ["So", "will", "sie", "auch", "mit", "ihr", "zum", "drit\u00b7ten", "Him\u00b7mel", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dein Glaubens-Auge sieht/ wie ihre Seele lebt/", "tokens": ["Dein", "Glau\u00b7bens\u00b7Au\u00b7ge", "sieht", "/", "wie", "ih\u00b7re", "See\u00b7le", "lebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie man nur ihre Quaal/ sie aber nicht begr\u00e4bt/", "tokens": ["Wie", "man", "nur", "ih\u00b7re", "Qua\u00b7al", "/", "sie", "a\u00b7ber", "nicht", "be\u00b7gr\u00e4bt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPOSAT", "NN", "$(", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie/ wenn in deiner Brust ihr Angedencken lieget/", "tokens": ["Wie", "/", "wenn", "in", "dei\u00b7ner", "Brust", "ihr", "An\u00b7ge\u00b7den\u00b7cken", "lie\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich kein so grosser Schmertz bey ihrer Lust besieget.", "tokens": ["Dich", "kein", "so", "gros\u00b7ser", "Schmertz", "bey", "ih\u00b7rer", "Lust", "be\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie/ wenn dein Hertz sie liebt/ es dieses nicht vergi\u00dft/", "tokens": ["Wie", "/", "wenn", "dein", "Hertz", "sie", "liebt", "/", "es", "die\u00b7ses", "nicht", "ver\u00b7gi\u00dft", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$(", "PPER", "PDS", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihr in dieser Welt annoch am liebsten ist:", "tokens": ["Was", "ihr", "in", "die\u00b7ser", "Welt", "an\u00b7noch", "am", "liebs\u00b7ten", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "ADV", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist/ vor Schmertzen dich nicht selber begraben/", "tokens": ["Das", "ist", "/", "vor", "Schmert\u00b7zen", "dich", "nicht", "sel\u00b7ber", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "APPR", "NN", "PPER", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und in den Kindern sie noch allzeit haben.", "tokens": ["Und", "in", "den", "Kin\u00b7dern", "sie", "noch", "all\u00b7zeit", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du Hochvern\u00fcnftger Mann/ du kennest mehr/ als wir", "tokens": ["Du", "Hoch\u00b7ver\u00b7n\u00fcnft\u00b7ger", "Mann", "/", "du", "ken\u00b7nest", "mehr", "/", "als", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Gott im Creutze liebt/ und wie das Kraut allhier/", "tokens": ["Wie", "Gott", "im", "Creut\u00b7ze", "liebt", "/", "und", "wie", "das", "Kraut", "all\u00b7hier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "$(", "KON", "PWAV", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das man in ", "tokens": ["Das", "man", "in"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PIS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Sey Christliche Gedult/ die man in dir erkennet.", "tokens": ["Sey", "Christ\u00b7li\u00b7che", "Ge\u00b7dult", "/", "die", "man", "in", "dir", "er\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$(", "PRELS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wir w\u00fcnschen dieses nur: der Himmel st\u00e4rcke dich/", "tokens": ["Wir", "w\u00fcn\u00b7schen", "die\u00b7ses", "nur", ":", "der", "Him\u00b7mel", "st\u00e4r\u00b7cke", "dich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fclle fort dein Hau\u00df mit Seegen mildiglich/", "tokens": ["Und", "f\u00fcl\u00b7le", "fort", "dein", "Hau\u00df", "mit", "See\u00b7gen", "mil\u00b7dig\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df wenn in deiner Brust itzt Freud und Lust begraben/", "tokens": ["Da\u00df", "wenn", "in", "dei\u00b7ner", "Brust", "itzt", "Freud", "und", "Lust", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN", "ADV", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie auch da wiederum die Auferstehung haben.", "tokens": ["Sie", "auch", "da", "wie\u00b7de\u00b7rum", "die", "Auf\u00b7er\u00b7ste\u00b7hung", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}