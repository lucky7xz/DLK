{"textgrid.poem.37212": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4ufer an den Vollmond", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum mein lieber Mond, sieht Er", "tokens": ["Wa\u00b7rum", "mein", "lie\u00b7ber", "Mond", ",", "sieht", "Er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hoch und kalt auf mich daher?", "tokens": ["So", "hoch", "und", "kalt", "auf", "mich", "da\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PRF", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wol nicht seiner V\u00f6lle wegen?", "tokens": ["Doch", "wol", "nicht", "sei\u00b7ner", "V\u00f6l\u00b7le", "we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O da bin ich ihm \u00fcberlegen:", "tokens": ["O", "da", "bin", "ich", "ihm", "\u00fc\u00b7berl\u00b7e\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Er, mein lieber, wei\u00df Er wol?", "tokens": ["Denn", "Er", ",", "mein", "lie\u00b7ber", ",", "wei\u00df", "Er", "wol", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADV", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Einmal nur im Monat voll!", "tokens": ["Ist", "Ein\u00b7mal", "nur", "im", "Mo\u00b7nat", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}