{"textgrid.poem.65083": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Stunde, die nicht Wachen mehr,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stunde, die nicht Wachen mehr,", "tokens": ["Die", "Stun\u00b7de", ",", "die", "nicht", "Wa\u00b7chen", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und noch des Schlafes Gut nicht spendet,", "tokens": ["Und", "noch", "des", "Schla\u00b7fes", "Gut", "nicht", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist's, die aller Foltern Heer", "tokens": ["Sie", "ist's", ",", "die", "al\u00b7ler", "Fol\u00b7tern", "Heer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00e4th'risch gegen mich entsendet.", "tokens": ["Ver\u00b7r\u00e4th'\u00b7risch", "ge\u00b7gen", "mich", "ent\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob mich der Blitz des Himmels trifft,", "tokens": ["Ob", "mich", "der", "Blitz", "des", "Him\u00b7mels", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen biet' ich den Gewittern!", "tokens": ["Will\u00b7kom\u00b7men", "biet'", "ich", "den", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch jener Stunde schleichend Gift", "tokens": ["Doch", "je\u00b7ner", "Stun\u00b7de", "schlei\u00b7chend", "Gift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht mein gest\u00e4hltes Herz erzittern.", "tokens": ["Macht", "mein", "ge\u00b7st\u00e4hl\u00b7tes", "Herz", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sind die Waffen hingelegt,", "tokens": ["Da", "sind", "die", "Waf\u00b7fen", "hin\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Womit ich muthig k\u00e4mpf' am Tage,", "tokens": ["Wo\u00b7mit", "ich", "mut\u00b7hig", "k\u00e4mpf'", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die hingeschied'ne Sehnsucht regt", "tokens": ["Die", "hin\u00b7ge\u00b7schie\u00b7d'\u00b7ne", "Sehn\u00b7sucht", "regt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich leis' in ihrem Sarkophage.", "tokens": ["Sich", "leis'", "in", "ih\u00b7rem", "Sar\u00b7ko\u00b7pha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da strahlt voll schmerzenreicher Huld", "tokens": ["Da", "strahlt", "voll", "schmer\u00b7zen\u00b7rei\u00b7cher", "Huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Aug auf mich im Liebesscheine", "tokens": ["Dein", "Aug", "auf", "mich", "im", "Lie\u00b7bes\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fragt mich: Wessen ist die Schuld,", "tokens": ["Und", "fragt", "mich", ":", "Wes\u00b7sen", "ist", "die", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du nicht mein, du ewig Meine?", "tokens": ["Da\u00df", "du", "nicht", "mein", ",", "du", "e\u00b7wig", "Mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "$,", "PPER", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da \u00fcbert\u00f6nt dein s\u00fc\u00dfer Laut", "tokens": ["Da", "\u00fc\u00b7ber\u00b7t\u00f6nt", "dein", "s\u00fc\u00b7\u00dfer", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens tosende Orkane,", "tokens": ["Des", "Le\u00b7bens", "to\u00b7sen\u00b7de", "Or\u00b7ka\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fragt: Du meine Geistesbraut!", "tokens": ["Und", "fragt", ":", "Du", "mei\u00b7ne", "Geis\u00b7tes\u00b7braut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liebst du mich auch, wie ich es ahne?", "tokens": ["Liebst", "du", "mich", "auch", ",", "wie", "ich", "es", "ah\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So ruft's, und jeder Natter Stich,", "tokens": ["So", "ruft's", ",", "und", "je\u00b7der", "Nat\u00b7ter", "Stich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das tiefe Elend ist vergessen!", "tokens": ["Das", "tie\u00b7fe", "E\u00b7lend", "ist", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In wilder Inbrunst m\u00f6cht' ich dich", "tokens": ["In", "wil\u00b7der", "In\u00b7brunst", "m\u00f6cht'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinen Busen jauchzend pressen.", "tokens": ["An", "mei\u00b7nen", "Bu\u00b7sen", "jauch\u00b7zend", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein Geist flammt auf zu Gottes Lob,", "tokens": ["Mein", "Geist", "flammt", "auf", "zu", "Got\u00b7tes", "Lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem Aug' gl\u00e4nzt eine Z\u00e4hre,", "tokens": ["In", "mei\u00b7nem", "Aug'", "gl\u00e4nzt", "ei\u00b7ne", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herz erbebt und schwillt, als ob", "tokens": ["Mein", "Herz", "er\u00b7bebt", "und", "schwillt", ",", "als", "ob"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,", "KOKOM", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich nicht schon l\u00e4ngst gestorben w\u00e4re.", "tokens": ["Ich", "nicht", "schon", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Hinweg du b\u00f6ser Spuk! zur\u00fcck,", "tokens": ["Hin\u00b7weg", "du", "b\u00f6\u00b7ser", "Spuk", "!", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "$.", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du b\u00e4ngster von den Schrecken allen,", "tokens": ["Du", "b\u00e4ngs\u00b7ter", "von", "den", "Schre\u00b7cken", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um ewig unerreichbar Gl\u00fcck", "tokens": ["Um", "e\u00b7wig", "un\u00b7er\u00b7reich\u00b7bar", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ruh'los, ein irrer Geist, zu wallen!", "tokens": ["Ruh'\u00b7los", ",", "ein", "ir\u00b7rer", "Geist", ",", "zu", "wal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}