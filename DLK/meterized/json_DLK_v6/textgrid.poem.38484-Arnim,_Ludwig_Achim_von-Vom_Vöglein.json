{"textgrid.poem.38484": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Vom V\u00f6glein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df dich Gott mein lieb Regerl!", "tokens": ["Gr\u00fc\u00df", "dich", "Gott", "mein", "lieb", "Re\u00b7gerl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich komm aus dem Wald,", "tokens": ["Ich", "komm", "aus", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hab gefangen ein sch\u00f6ns V\u00f6gerl,", "tokens": ["Hab", "ge\u00b7fan\u00b7gen", "ein", "sch\u00f6ns", "V\u00f6\u00b7gerl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Entwischt w\u00e4r mirs bald.", "tokens": ["Ent\u00b7wischt", "w\u00e4r", "mirs", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich th\u00e4t dirs gern schenken,", "tokens": ["Ich", "th\u00e4t", "dirs", "gern", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nimms an, sey so gut,", "tokens": ["Nimms", "an", ",", "sey", "so", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Es wird dich nicht kr\u00e4nken,", "tokens": ["Es", "wird", "dich", "nicht", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Weils sch\u00f6n singen thut.", "tokens": ["Weils", "sch\u00f6n", "sin\u00b7gen", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ey du mein liebs Regerl,", "tokens": ["Ey", "du", "mein", "liebs", "Re\u00b7gerl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ich bitt dich um ein Gnad,", "tokens": ["Ich", "bitt", "dich", "um", "ein", "Gnad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschaff doch dem V\u00f6gerl", "tokens": ["Ver\u00b7schaff", "doch", "dem", "V\u00f6\u00b7gerl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein H\u00e4usle von Drath,", "tokens": ["Ein", "H\u00e4us\u00b7le", "von", "Drath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Thu auch nicht vergessen,", "tokens": ["Thu", "auch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ein Tr\u00f6gerl zum Trank,", "tokens": ["Ein", "Tr\u00f6\u00b7gerl", "zum", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ein Tr\u00f6gerl zum Fressen,", "tokens": ["Ein", "Tr\u00f6\u00b7gerl", "zum", "Fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Da\u00df 's dir nit wird krank.", "tokens": ["Da\u00df", "'s", "dir", "nit", "wird", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}