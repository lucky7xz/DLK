{"dta.poem.11717": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist, drum denkt er; denkt, drum spricht er, und ein Wort,", "tokens": ["Gott", "ist", ",", "drum", "denkt", "er", ";", "denkt", ",", "drum", "spricht", "er", ",", "und", "ein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PAV", "VVFIN", "PPER", "$.", "VVFIN", "$,", "PAV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er es denkt und spricht, so stehts geschaffen dort.", "tokens": ["Wie", "er", "es", "denkt", "und", "spricht", ",", "so", "stehts", "ge\u00b7schaf\u00b7fen", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADV", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist und denkest auch, du denkst und sprichst, allein", "tokens": ["Du", "bist", "und", "den\u00b7kest", "auch", ",", "du", "denkst", "und", "sprichst", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Wesen ist das Wort, es ist ein Bild und Schein.", "tokens": ["Kein", "We\u00b7sen", "ist", "das", "Wort", ",", "es", "ist", "ein", "Bild", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das macht: du sprichst nur nach, du denkst nur nach, du bist", "tokens": ["Das", "macht", ":", "du", "sprichst", "nur", "nach", ",", "du", "denkst", "nur", "nach", ",", "du", "bist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur nach dem Ersten, der dir vorspricht, denkt und ist.", "tokens": ["Nur", "nach", "dem", "Ers\u00b7ten", ",", "der", "dir", "vor\u00b7spricht", ",", "denkt", "und", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}