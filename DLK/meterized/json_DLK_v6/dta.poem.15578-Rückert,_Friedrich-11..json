{"dta.poem.15578": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Schuldbuch ist sein eigenes Gewissen,", "tokens": ["Des", "Men\u00b7schen", "Schuld\u00b7buch", "ist", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin durchstrichen wird kein Blatt, noch ausgerissen.", "tokens": ["Da\u00b7rin", "durch\u00b7stri\u00b7chen", "wird", "kein", "Blatt", ",", "noch", "aus\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VAFIN", "PIAT", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Schuldner kann darin nicht tilgen seine Schuld,", "tokens": ["Der", "Schuld\u00b7ner", "kann", "da\u00b7rin", "nicht", "til\u00b7gen", "sei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PAV", "PTKNEG", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur danken kann er, wenn sie tilgt des Schuldherrn Huld.", "tokens": ["Nur", "dan\u00b7ken", "kann", "er", ",", "wenn", "sie", "tilgt", "des", "Schuld\u00b7herrn", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In deinem Schuldbuch kannst du tilgen, was dir ist", "tokens": ["In", "dei\u00b7nem", "Schuld\u00b7buch", "kannst", "du", "til\u00b7gen", ",", "was", "dir", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,", "PWS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein andrer schuldig, nicht was du ihm schuldig bist.", "tokens": ["Ein", "an\u00b7drer", "schul\u00b7dig", ",", "nicht", "was", "du", "ihm", "schul\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "$,", "PTKNEG", "PWS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}