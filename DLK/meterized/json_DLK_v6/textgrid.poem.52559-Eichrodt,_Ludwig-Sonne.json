{"textgrid.poem.52559": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder kam die Sonne", "tokens": ["Und", "wie\u00b7der", "kam", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins gro\u00dfe Thal des Rheins,", "tokens": ["Ins", "gro\u00b7\u00dfe", "Thal", "des", "Rheins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat gebracht die Wonne", "tokens": ["Und", "hat", "ge\u00b7bracht", "die", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des warmen Sonnenscheins.", "tokens": ["Des", "war\u00b7men", "Son\u00b7nen\u00b7scheins", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie kam herf\u00fcr die Berge", "tokens": ["Sie", "kam", "her\u00b7f\u00fcr", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und z\u00fcndete ins Gras,", "tokens": ["Und", "z\u00fcn\u00b7de\u00b7te", "ins", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre ersten Werke", "tokens": ["Und", "ih\u00b7re", "ers\u00b7ten", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen waren das.", "tokens": ["Die", "Blu\u00b7men", "wa\u00b7ren", "das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie sie durchs Geb\u00fcsche", "tokens": ["Und", "wie", "sie", "durchs", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die B\u00e4chlein aufgek\u00fc\u00dft,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "auf\u00b7ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schwammen auf die Fische", "tokens": ["Da", "schwam\u00b7men", "auf", "die", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und haben sie gegr\u00fc\u00dft.", "tokens": ["Und", "ha\u00b7ben", "sie", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie sie aus den B\u00e4umen", "tokens": ["Und", "wie", "sie", "aus", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das junge Laub gelockt,", "tokens": ["Das", "jun\u00b7ge", "Laub", "ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat in den gr\u00fcnen R\u00e4umen", "tokens": ["Hat", "in", "den", "gr\u00fc\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Lied und Laut gestockt.", "tokens": ["Kein", "Lied", "und", "Laut", "ge\u00b7stockt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie sie ganz den Winter", "tokens": ["Und", "wie", "sie", "ganz", "den", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verjagt mit ihrem Strahl \u2013", "tokens": ["Ver\u00b7jagt", "mit", "ih\u00b7rem", "Strahl", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6nen Menschenkinder", "tokens": ["Die", "sch\u00f6\u00b7nen", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinsprangen in das Thal.", "tokens": ["Hin\u00b7spran\u00b7gen", "in", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}