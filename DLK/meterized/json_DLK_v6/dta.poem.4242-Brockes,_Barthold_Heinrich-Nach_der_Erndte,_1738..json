{"dta.poem.4242": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Erndte,  \n  1738.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! mein Wunsch ist eingetroffen,", "tokens": ["Gott", "Lob", "!", "mein", "Wunsch", "ist", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh\u2019 die Vorwerks-Th\u00fcren offen,", "tokens": ["Ich", "seh'", "die", "Vor\u00b7werks\u00b7T\u00b7h\u00fc\u00b7ren", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein letztes Korn wird eingef\u00fchrt;", "tokens": ["Mein", "letz\u00b7tes", "Korn", "wird", "ein\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Obgleich durch gar zu milden Regen,", "tokens": ["Ob\u00b7gleich", "durch", "gar", "zu", "mil\u00b7den", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den man in dieser Zeit versp\u00fchrt,", "tokens": ["Den", "man", "in", "die\u00b7ser", "Zeit", "ver\u00b7sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl mancher sein Getrayd verliehrt.", "tokens": ["Wohl", "man\u00b7cher", "sein", "Ge\u00b7trayd", "ver\u00b7liehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sprich denn, mein Geist, ob, dieserwegen,", "tokens": ["Sprich", "denn", ",", "mein", "Geist", ",", "ob", ",", "die\u00b7ser\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "KOUS", "$,", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem, Der mir wieder so viel Segen", "tokens": ["Dem", ",", "Der", "mir", "wie\u00b7der", "so", "viel", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ART", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aufs neu geschenkt, nicht Dank geb\u00fchrt?", "tokens": ["Aufs", "neu", "ge\u00b7schenkt", ",", "nicht", "Dank", "ge\u00b7b\u00fchrt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVPP", "$,", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft uns unsern Sch\u00f6pfer preisen", "tokens": ["La\u00dft", "uns", "un\u00b7sern", "Sch\u00f6p\u00b7fer", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die Fr\u00fcchte, die uns speisen,", "tokens": ["F\u00fcr", "die", "Fr\u00fcch\u00b7te", ",", "die", "uns", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, im Dank, Dem Ehr\u2019 erweisen,", "tokens": ["Und", ",", "im", "Dank", ",", "Dem", "Ehr'", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sie schuff und stetig schafft,", "tokens": ["Der", "sie", "schuff", "und", "ste\u00b7tig", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der in ihren zarten Saft", "tokens": ["Der", "in", "ih\u00b7ren", "zar\u00b7ten", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine solche Wunder-Kraft", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "Wun\u00b7der\u00b7Kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eingefl\u00f6sset und gesenket,", "tokens": ["Ein\u00b7ge\u00b7fl\u00f6s\u00b7set", "und", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie uns noch itzt geschenket.", "tokens": ["Und", "sie", "uns", "noch", "itzt", "ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich hebe denn itzt H\u00e4nd\u2019 und Augen zu Dir, Quell aller", "tokens": ["Ich", "he\u00b7be", "denn", "itzt", "H\u00e4nd'", "und", "Au\u00b7gen", "zu", "Dir", ",", "Quell", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "KON", "NN", "APPR", "PPER", "$,", "NN", "PIAT"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Gaben, auf,", "tokens": ["Ga\u00b7ben", ",", "auf", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und danke Dir, da\u00df der Natur so stet- so ordentlicher", "tokens": ["Und", "dan\u00b7ke", "Dir", ",", "da\u00df", "der", "Na\u00b7tur", "so", "stet", "so", "or\u00b7dent\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "TRUNC", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Lauf,", "tokens": ["Lauf", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Den Du allein, durch Deine Lieb\u2019 und Deine weise Wunder-", "tokens": ["Den", "Du", "al\u00b7lein", ",", "durch", "Dei\u00b7ne", "Lieb'", "und", "Dei\u00b7ne", "wei\u00b7se", "Wun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Macht,", "tokens": ["Macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Geordnet und bisher erhalten, das, was so Thier\u2019 als Men-", "tokens": ["Ge\u00b7ord\u00b7net", "und", "bis\u00b7her", "er\u00b7hal\u00b7ten", ",", "das", ",", "was", "so", "Thier'", "als", "Men"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "VVPP", "$,", "PDS", "$,", "PRELS", "ADV", "NN", "KOKOM", "TRUNC"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "schen n\u00e4hrt,", "tokens": ["schen", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Uns, blo\u00df durch Dein so gn\u00e4digs Wollen, nun abermahl", "tokens": ["Uns", ",", "blo\u00df", "durch", "Dein", "so", "gn\u00e4\u00b7digs", "Wol\u00b7len", ",", "nun", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADV", "APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "durch Dich beschehrt.", "tokens": ["durch", "Dich", "be\u00b7schehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ach gieb, wenn wir dasselbe n\u00fctzen, geniessen, brauchen und", "tokens": ["Ach", "gieb", ",", "wenn", "wir", "das\u00b7sel\u00b7be", "n\u00fct\u00b7zen", ",", "ge\u00b7nies\u00b7sen", ",", "brau\u00b7chen", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "PPER", "PDAT", "VVINF", "$,", "VVPP", "$,", "VVFIN", "KON"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "verzehren,", "tokens": ["ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Da\u00df es mit frohem Dank geschehe, und wir, in unsrer Lust,", "tokens": ["Da\u00df", "es", "mit", "fro\u00b7hem", "Dank", "ge\u00b7sche\u00b7he", ",", "und", "wir", ",", "in", "uns\u00b7rer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,", "KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Dich ehren!", "tokens": ["Dich", "eh\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}