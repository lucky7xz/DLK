{"textgrid.poem.25734": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ruhm ist nur ein sch\u00f6ner Traum!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ruhm ist nur ein sch\u00f6ner Traum!", "tokens": ["Der", "Ruhm", "ist", "nur", "ein", "sch\u00f6\u00b7ner", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du ihn tr\u00e4umest, wird er kaum", "tokens": ["Wenn", "du", "ihn", "tr\u00e4u\u00b7mest", ",", "wird", "er", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00fcdigkeit am Morgen lohnen.", "tokens": ["Die", "M\u00fc\u00b7dig\u00b7keit", "am", "Mor\u00b7gen", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ruhm ist ein verzognes Kind,", "tokens": ["Der", "Ruhm", "ist", "ein", "ver\u00b7zog\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es n\u00e4rrt den Vater zwar, doch ist der Vater blind,", "tokens": ["Es", "n\u00e4rrt", "den", "Va\u00b7ter", "zwar", ",", "doch", "ist", "der", "Va\u00b7ter", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und spielt mit ihm \u2013 um bunte Bohnen.", "tokens": ["Und", "spielt", "mit", "ihm", "\u2013", "um", "bun\u00b7te", "Boh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr diesen Sohn hat Tag und Nacht", "tokens": ["F\u00fcr", "die\u00b7sen", "Sohn", "hat", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Herz gesorgt, dein Kopf gewacht,", "tokens": ["Dein", "Herz", "ge\u00b7sorgt", ",", "dein", "Kopf", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihn gro\u00df zu ziehn; itzt mu\u00df er wandern.", "tokens": ["Ihn", "gro\u00df", "zu", "ziehn", ";", "itzt", "mu\u00df", "er", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$.", "ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ha! wie dein Busen steigt und f\u00e4llt,", "tokens": ["Ha", "!", "wie", "dein", "Bu\u00b7sen", "steigt", "und", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Ob ihn ein G\u00f6nner k\u00fc\u00dft, ein R\u00e4uber Netze stellt!", "tokens": ["Ob", "ihn", "ein", "G\u00f6n\u00b7ner", "k\u00fc\u00dft", ",", "ein", "R\u00e4u\u00b7ber", "Net\u00b7ze", "stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was habt ihr Einer von dem Andern?", "tokens": ["Was", "habt", "ihr", "Ei\u00b7ner", "von", "dem", "An\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie wenig strahlt von seinem Gl\u00fcck'", "tokens": ["Wie", "we\u00b7nig", "strahlt", "von", "sei\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Aus weiter Fern' auf dich zur\u00fcck!", "tokens": ["Aus", "wei\u00b7ter", "Fern'", "auf", "dich", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Selbst deinem Nachbar wird nicht tr\u00e4umen,", "tokens": ["Selbst", "dei\u00b7nem", "Nach\u00b7bar", "wird", "nicht", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df, fern von dir, ein Sohn noch lebt,", "tokens": ["Da\u00df", ",", "fern", "von", "dir", ",", "ein", "Sohn", "noch", "lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "APPR", "PPER", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den, wenn sein Nam' auch dir nicht auf der Lippe schwebt,", "tokens": ["Den", ",", "wenn", "sein", "Nam'", "auch", "dir", "nicht", "auf", "der", "Lip\u00b7pe", "schwebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch alle Reimer ba\u00df bereimen!", "tokens": ["Doch", "al\u00b7le", "Rei\u00b7mer", "ba\u00df", "be\u00b7rei\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ist Ruhm allein der Preis am Ziel',", "tokens": ["Ist", "Ruhm", "al\u00b7lein", "der", "Preis", "am", "Ziel'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So ist es etwas, doch nicht viel;", "tokens": ["So", "ist", "es", "et\u00b7was", ",", "doch", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Denn, wenn ein J\u00fcngling ihn ", "tokens": ["Denn", ",", "wenn", "ein", "J\u00fcng\u00b7ling", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "So wahrt den Beutel! H\u00fctet Euch,", "tokens": ["So", "wahrt", "den", "Beu\u00b7tel", "!", "H\u00fc\u00b7tet", "Euch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df unter Einem Dach' Ihr nie mit ihm zugleich", "tokens": ["Da\u00df", "un\u00b7ter", "Ei\u00b7nem", "Da\u00b7ch'", "Ihr", "nie", "mit", "ihm", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "PPER", "ADV"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Ohn' Euren Degen \u00fcbernachtet.", "tokens": ["Ohn'", "Eu\u00b7ren", "De\u00b7gen", "\u00fc\u00b7bern\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Doch, \u00fcbersteh' der K\u00fcnstler Pest,", "tokens": ["Doch", ",", "\u00fc\u00b7ber\u00b7steh'", "der", "K\u00fcnst\u00b7ler", "Pest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und halt', als Mann, den Proteus fest:", "tokens": ["Und", "halt'", ",", "als", "Mann", ",", "den", "Pro\u00b7teus", "fest", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Vor\u00fcber ist die Sch\u00e4ferstunde!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "die", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Mehr als das Ganze, war auch hier", "tokens": ["Mehr", "als", "das", "Gan\u00b7ze", ",", "war", "auch", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "$,", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die H\u00e4lfte. So erlischt der Durst nach Wein in dir", "tokens": ["Die", "H\u00e4lf\u00b7te", ".", "So", "er\u00b7lischt", "der", "Durst", "nach", "Wein", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Am ersten vor des Fasses Spunde.", "tokens": ["Am", "ers\u00b7ten", "vor", "des", "Fas\u00b7ses", "Spun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wie? Ist denn alles Unbestand?", "tokens": ["Wie", "?", "Ist", "denn", "al\u00b7les", "Un\u00b7be\u00b7stand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und ringt der Weise nur, f\u00fcr Tand", "tokens": ["Und", "ringt", "der", "Wei\u00b7se", "nur", ",", "f\u00fcr", "Tand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Den Kranz zu achten, der die Scheitel", "tokens": ["Den", "Kranz", "zu", "ach\u00b7ten", ",", "der", "die", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Nach langem saurem Kampfe schm\u00fcckt?", "tokens": ["Nach", "lan\u00b7gem", "sau\u00b7rem", "Kamp\u00b7fe", "schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Kaum hat er noch auf ihn mit L\u00e4cheln hingeblickt:", "tokens": ["Kaum", "hat", "er", "noch", "auf", "ihn", "mit", "L\u00e4\u00b7cheln", "hin\u00b7ge\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Itzt seufzt er schon: Auch du bist eitel!", "tokens": ["Itzt", "seufzt", "er", "schon", ":", "Auch", "du", "bist", "ei\u00b7tel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wenn Ruhm auch eitel ist: wohlan!", "tokens": ["Wenn", "Ruhm", "auch", "ei\u00b7tel", "ist", ":", "wo\u00b7hlan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "VAFIN", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Was spornt zu schweren K\u00e4mpfen an?", "tokens": ["Was", "spornt", "zu", "schwe\u00b7ren", "K\u00e4mp\u00b7fen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Das Gold? \u2013 Wirst du den K\u00e4mpfer sch\u00e4tzen,", "tokens": ["Das", "Gold", "?", "\u2013", "Wirst", "du", "den", "K\u00e4mp\u00b7fer", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Wenn gleich auch seine gro\u00dfe That?", "tokens": ["Wenn", "gleich", "auch", "sei\u00b7ne", "gro\u00b7\u00dfe", "That", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und ihn dem Manne, Freund, der arm denselben Pfad", "tokens": ["Und", "ihn", "dem", "Man\u00b7ne", ",", "Freund", ",", "der", "arm", "den\u00b7sel\u00b7ben", "Pfad"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "$,", "NN", "$,", "PRELS", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Aus Tugend ging, zur Seite setzen?", "tokens": ["Aus", "Tu\u00b7gend", "ging", ",", "zur", "Sei\u00b7te", "set\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Was von der Zeit als Eigenthum", "tokens": ["Was", "von", "der", "Zeit", "als", "Ei\u00b7gen\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Uns \u00fcbrig ist, soll nicht dem Ruhm',", "tokens": ["Uns", "\u00fcb\u00b7rig", "ist", ",", "soll", "nicht", "dem", "Ruhm'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "VMFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Nicht Peru's Minen angeh\u00f6ren.", "tokens": ["Nicht", "Pe\u00b7ru's", "Mi\u00b7nen", "an\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Des Bechers Klang soll selten nur,", "tokens": ["Des", "Be\u00b7chers", "Klang", "soll", "sel\u00b7ten", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Doch \u00f6ftrer das Geschw\u00e4tz des Bachs der Hirtenflur,", "tokens": ["Doch", "\u00f6ft\u00b7rer", "das", "Ge\u00b7schw\u00e4tz", "des", "Bachs", "der", "Hir\u00b7ten\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Uns, K\u00e4mpfer f\u00fcr die Zukunft! st\u00f6ren.", "tokens": ["Uns", ",", "K\u00e4mp\u00b7fer", "f\u00fcr", "die", "Zu\u00b7kunft", "!", "st\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NN", "APPR", "ART", "NN", "$.", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Kein Panzer d\u00fcnk' uns noch zu schwer,", "tokens": ["Kein", "Pan\u00b7zer", "d\u00fcnk'", "uns", "noch", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Zu tief kein Stand, zu lang kein Speer,", "tokens": ["Zu", "tief", "kein", "Stand", ",", "zu", "lang", "kein", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PIAT", "NN", "$,", "PTKA", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Den s\u00fc\u00dfen Dank davon zu tragen!", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Dank", "da\u00b7von", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Doch thut es, Freund, nicht immer Roth,", "tokens": ["Doch", "thut", "es", ",", "Freund", ",", "nicht", "im\u00b7mer", "Roth", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "So lang kein offner Helm mit einer Fehde droht,", "tokens": ["So", "lang", "kein", "off\u00b7ner", "Helm", "mit", "ei\u00b7ner", "Feh\u00b7de", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Gleich das Visir zur\u00fcckzuschlagen.", "tokens": ["Gleich", "das", "Vi\u00b7sir", "zu\u00b7r\u00fcck\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.55": {"text": "Wer mi\u00dft sich k\u00fchn mit diesem Gl\u00fcck',", "tokens": ["Wer", "mi\u00dft", "sich", "k\u00fchn", "mit", "die\u00b7sem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Wenn alle Thaten unserm Blick'", "tokens": ["Wenn", "al\u00b7le", "Tha\u00b7ten", "un\u00b7serm", "Blick'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Einst sterbend noch vor\u00fcber gehen?", "tokens": ["Einst", "ster\u00b7bend", "noch", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Sie bleiben hier, und wirken fort,", "tokens": ["Sie", "blei\u00b7ben", "hier", ",", "und", "wir\u00b7ken", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Sind wir gleich l\u00e4ngstens Staub! Und o! wir werden dort", "tokens": ["Sind", "wir", "gleich", "l\u00e4ngs\u00b7tens", "Staub", "!", "Und", "o", "!", "wir", "wer\u00b7den", "dort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "NN", "$.", "KON", "FM", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Mit sch\u00e4rferm Blick' die Folge sehen.", "tokens": ["Mit", "sch\u00e4r\u00b7ferm", "Blick'", "die", "Fol\u00b7ge", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", 