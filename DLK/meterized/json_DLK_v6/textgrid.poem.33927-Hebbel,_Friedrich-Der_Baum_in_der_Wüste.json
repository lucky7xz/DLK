{"textgrid.poem.33927": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum in der W\u00fcste", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Baum im W\u00fcstensand,", "tokens": ["Es", "steht", "ein", "Baum", "im", "W\u00fcs\u00b7ten\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der einzige, der dort gedieh;", "tokens": ["Der", "ein\u00b7zi\u00b7ge", ",", "der", "dort", "ge\u00b7dieh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die Sonne hat ihn fast verbrannt,", "tokens": ["Die", "Son\u00b7ne", "hat", "ihn", "fast", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Regen tr\u00e4nkt den durst'gen nie.", "tokens": ["Der", "Re\u00b7gen", "tr\u00e4nkt", "den", "dur\u00b7st'\u00b7gen", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In seiner falben Krone h\u00e4ngt", "tokens": ["In", "sei\u00b7ner", "fal\u00b7ben", "Kro\u00b7ne", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00fcrzig eine Frucht voll Saft,", "tokens": ["Ge\u00b7w\u00fcr\u00b7zig", "ei\u00b7ne", "Frucht", "voll", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat sein Mark hinein gedr\u00e4ngt,", "tokens": ["Er", "hat", "sein", "Mark", "hin\u00b7ein", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leben, seine h\u00f6chste Kraft.", "tokens": ["Sein", "Le\u00b7ben", ",", "sei\u00b7ne", "h\u00f6chs\u00b7te", "Kraft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Stunde, wo sie, \u00fcberschwer,", "tokens": ["Die", "Stun\u00b7de", ",", "wo", "sie", ",", "\u00fc\u00b7bersc\u00b7hwer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Boden fallen mu\u00df, ist nah',", "tokens": ["Zu", "Bo\u00b7den", "fal\u00b7len", "mu\u00df", ",", "ist", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es zieht kein Wanderer daher,", "tokens": ["Es", "zieht", "kein", "Wan\u00b7de\u00b7rer", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr ihn selbst ist sie nicht da.", "tokens": ["Und", "f\u00fcr", "ihn", "selbst", "ist", "sie", "nicht", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}