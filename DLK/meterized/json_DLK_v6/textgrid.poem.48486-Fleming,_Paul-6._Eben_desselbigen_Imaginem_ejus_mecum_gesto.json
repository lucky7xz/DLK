{"textgrid.poem.48486": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Eben desselbigen Imaginem ejus mecum gesto", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Bildn\u00fc\u00df, der ich folg', ist mir in mich geschrieben.", "tokens": ["Das", "Bild\u00b7n\u00fc\u00df", ",", "der", "ich", "fol\u00b7g'", ",", "ist", "mir", "in", "mich", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Wesen, die Gestalt, das Lachen und fortan", "tokens": ["Das", "We\u00b7sen", ",", "die", "Ge\u00b7stalt", ",", "das", "La\u00b7chen", "und", "for\u00b7tan"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "steht stets in meinem Sinn' und ist darinnen blieben,", "tokens": ["steht", "stets", "in", "mei\u00b7nem", "Sinn'", "und", "ist", "da\u00b7rin\u00b7nen", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "KON", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gebildet von der Zeit, da ich sie erst blickt' an.", "tokens": ["ge\u00b7bil\u00b7det", "von", "der", "Zeit", ",", "da", "ich", "sie", "erst", "blickt'", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich trag' es, wo ich geh'. Und hab' ich ein Verlangen", "tokens": ["Ich", "trag'", "es", ",", "wo", "ich", "geh'", ".", "Und", "hab'", "ich", "ein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$.", "KON", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nach einer vollen Lust, so denk' ich, was sie tut.", "tokens": ["nach", "ei\u00b7ner", "vol\u00b7len", "Lust", ",", "so", "denk'", "ich", ",", "was", "sie", "tut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn seh' ich sie vor mir. Will ich sie denn umfangen,", "tokens": ["Denn", "seh'", "ich", "sie", "vor", "mir", ".", "Will", "ich", "sie", "denn", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PPER", "$.", "VMFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so giebt ihr rechtes Bild heraus mein treuer Mut.", "tokens": ["so", "giebt", "ihr", "rech\u00b7tes", "Bild", "he\u00b7raus", "mein", "treu\u00b7er", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APZR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}