{"textgrid.poem.24858": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Adele", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich nicht allein, Adele,", "tokens": ["La\u00df", "mich", "nicht", "al\u00b7lein", ",", "A\u00b7de\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "$,", "NE", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nicht in weiter Welt allein!", "tokens": ["Nicht", "in", "wei\u00b7ter", "Welt", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnen will ich meine Seele,", "tokens": ["Son\u00b7nen", "will", "ich", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weib, in deines Auges Schein.", "tokens": ["Weib", ",", "in", "dei\u00b7nes", "Au\u00b7ges", "Schein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leg in meine deine Rechte,", "tokens": ["Leg", "in", "mei\u00b7ne", "dei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df an Ader Ader wallt!", "tokens": ["Da\u00df", "an", "A\u00b7der", "A\u00b7der", "wallt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaurig drau\u00dfen sind die N\u00e4chte,", "tokens": ["Schau\u00b7rig", "drau\u00b7\u00dfen", "sind", "die", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Tage o wie kalt!", "tokens": ["Und", "die", "Ta\u00b7ge", "o", "wie", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "FM", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In des Menschenschwarms Gew\u00fchle", "tokens": ["In", "des", "Men\u00b7schen\u00b7schwarms", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steh' ich da bet\u00e4ubt und bang;", "tokens": ["Steh'", "ich", "da", "be\u00b7t\u00e4ubt", "und", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nur Einer mit mir f\u00fchle,", "tokens": ["Da\u00df", "nur", "Ei\u00b7ner", "mit", "mir", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fruchtlos ist mein Herzensdrang.", "tokens": ["Frucht\u00b7los", "ist", "mein", "Her\u00b7zens\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Natur mich zu vertrauen,", "tokens": ["Der", "Na\u00b7tur", "mich", "zu", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streif' ich durch Gebirg und Wald;", "tokens": ["Streif'", "ich", "durch", "Ge\u00b7birg", "und", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zur\u00fcck von ihr treibt Grauen", "tokens": ["Doch", "zu\u00b7r\u00fcck", "von", "ihr", "treibt", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In mich selbst mich wieder bald.", "tokens": ["In", "mich", "selbst", "mich", "wie\u00b7der", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ob das Herz in Freude schlage,", "tokens": ["Ob", "das", "Herz", "in", "Freu\u00b7de", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es in Verzweiflung bricht:", "tokens": ["Ob", "es", "in", "Ver\u00b7zwei\u00b7flung", "bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Taub ist sie f\u00fcr unsre Klage,", "tokens": ["Taub", "ist", "sie", "f\u00fcr", "uns\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Lust versteht sie nicht.", "tokens": ["Uns\u00b7re", "Lust", "ver\u00b7steht", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihre welken Bl\u00e4tter streut sie", "tokens": ["Ih\u00b7re", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "streut", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teilnahmslos auf unsre Gruft;", "tokens": ["Teil\u00b7nahms\u00b7los", "auf", "uns\u00b7re", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Nur aus unserm Staub erneut sie", "tokens": ["Nur", "aus", "un\u00b7serm", "Staub", "er\u00b7neut", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Lenze Bl\u00fctenduft.", "tokens": ["Ih\u00b7rer", "Len\u00b7ze", "Bl\u00fc\u00b7ten\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00df mich nicht allein, Adele,", "tokens": ["La\u00df", "mich", "nicht", "al\u00b7lein", ",", "A\u00b7de\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "$,", "NE", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nicht in weiter Welt allein!", "tokens": ["Nicht", "in", "wei\u00b7ter", "Welt", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnen will ich meine Seele,", "tokens": ["Son\u00b7nen", "will", "ich", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weib, in deines Auges Schein!", "tokens": ["Weib", ",", "in", "dei\u00b7nes", "Au\u00b7ges", "Schein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}