{"textgrid.poem.57991": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "29. Zu Haus wird's besser gehen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei Pilger, werd nicht m\u00fcd, halt noch ein wenig aus", "tokens": ["Ei", "Pil\u00b7ger", ",", "werd", "nicht", "m\u00fcd", ",", "halt", "noch", "ein", "we\u00b7nig", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VAFIN", "PTKNEG", "ADJD", "$,", "VVFIN", "ADV", "ART", "PIS", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bleibe abgesp\u00e4nt von dem, was mu\u00df verschwinden!", "tokens": ["Und", "blei\u00b7be", "ab\u00b7ge\u00b7sp\u00e4nt", "von", "dem", ",", "was", "mu\u00df", "ver\u00b7schwin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "ART", "$,", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Zeit k\u00fcrzt immer ab, bald kommst du ja nach Haus,", "tokens": ["Die", "Zeit", "k\u00fcrzt", "im\u00b7mer", "ab", ",", "bald", "kommst", "du", "ja", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wirst du, was du w\u00fcnschst, bei deinem Vater finden.", "tokens": ["Da", "wirst", "du", ",", "was", "du", "w\u00fcnschst", ",", "bei", "dei\u00b7nem", "Va\u00b7ter", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}