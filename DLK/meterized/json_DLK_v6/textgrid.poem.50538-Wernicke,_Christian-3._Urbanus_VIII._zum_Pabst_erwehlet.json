{"textgrid.poem.50538": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "3. Urbanus VIII. zum Pabst erwehlet", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der f\u00fchrt' in seinem Wapen, ", "tokens": ["Der", "f\u00fchrt'", "in", "sei\u00b7nem", "Wa\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hatt' im ", "tokens": ["Und", "hatt'", "im"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Als schon ", "tokens": ["Als", "schon"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Weil aber dem ", "tokens": ["Weil", "a\u00b7ber", "dem"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "So macht' er diesen Vers bekant:", "tokens": ["So", "macht'", "er", "die\u00b7sen", "Vers", "be\u00b7kant", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Pabst, der gerne jedermann", "tokens": ["Der", "Pabst", ",", "der", "ger\u00b7ne", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Freund' hat, schlug hierauf den sch\u00f6nen Endschluss an:", "tokens": ["Zum", "Freund'", "hat", ",", "schlug", "hier\u00b7auf", "den", "sch\u00f6\u00b7nen", "End\u00b7schluss", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "VVFIN", "PAV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}