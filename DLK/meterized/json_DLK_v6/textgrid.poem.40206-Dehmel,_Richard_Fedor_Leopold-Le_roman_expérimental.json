iambic.tetra"}, "line.4": {"text": "thut Alle ihr aus Einem Loch.", "tokens": ["thut", "Al\u00b7le", "ihr", "aus", "Ei\u00b7nem", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}