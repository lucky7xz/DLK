{"dta.poem.23690": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Arbeit f\u00fcr Doris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebstes M&#228;dchen, sei nicht m&#252;\u00dfig!", "tokens": ["Liebs\u00b7tes", "M", "&#228;", "dchen", ",", "sei", "nicht", "m", "&#252;", "\u00dfig", "!"], "token_info": ["word", "word", "XML_entity", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NN", "NE", "$(", "VVINF", "$,", "VAFIN", "PTKNEG", "ADJD", "$(", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrchtest du dich denn f\u00fcr Arbeit?", "tokens": ["F\u00fcrch\u00b7test", "du", "dich", "denn", "f\u00fcr", "Ar\u00b7beit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df dich doch dazu nicht treiben,", "tokens": ["La\u00df", "dich", "doch", "da\u00b7zu", "nicht", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wir sind zur M\u00fch erschaffen!", "tokens": ["Denn", "wir", "sind", "zur", "M\u00fch", "er\u00b7schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Komm, du must nicht m\u00fc\u00dfig gehen,", "tokens": ["Komm", ",", "du", "must", "nicht", "m\u00fc\u00b7\u00dfig", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm, du must f\u00fcr Arbeit schwitzen!", "tokens": ["Komm", ",", "du", "must", "f\u00fcr", "Ar\u00b7beit", "schwit\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebstes M\u00e4dchen, sei nicht m\u00fc\u00dfig!", "tokens": ["Liebs\u00b7tes", "M\u00e4d\u00b7chen", ",", "sei", "nicht", "m\u00fc\u00b7\u00dfig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebstes M\u00e4dchen, gib mir K\u00fcsse!", "tokens": ["Liebs\u00b7tes", "M\u00e4d\u00b7chen", ",", "gib", "mir", "K\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gib mir hundert, gib mir tausend,", "tokens": ["Gib", "mir", "hun\u00b7dert", ",", "gib", "mir", "tau\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "$,", "VVIMP", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gib mir neun und neunzig tausend,", "tokens": ["Gib", "mir", "neun", "und", "neun\u00b7zig", "tau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "KON", "CARD", "CARD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gib mir volle hundert tausend!", "tokens": ["Gib", "mir", "vol\u00b7le", "hun\u00b7dert", "tau\u00b7send", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "CARD", "CARD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "K\u00fcsse, bis ich nicht mehr z\u00e4le,", "tokens": ["K\u00fcs\u00b7se", ",", "bis", "ich", "nicht", "mehr", "z\u00e4\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00fcsse heute, k\u00fcsse morgen;", "tokens": ["K\u00fcs\u00b7se", "heu\u00b7te", ",", "k\u00fcs\u00b7se", "mor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Denn du solst nichts thun als k\u00fcssen.", "tokens": ["Denn", "du", "solst", "nichts", "thun", "als", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "VVINF", "KOKOM", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}