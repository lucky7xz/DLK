{"textgrid.poem.30352": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Grund, worauff ein Thron sein festes stehen fand,", "tokens": ["Der", "Grund", ",", "wo\u00b7rauff", "ein", "Thron", "sein", "fes\u00b7tes", "ste\u00b7hen", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PPOSAT", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist (was man auch sonst sagt) ein richtiger Verstand;", "tokens": ["Ist", "(", "was", "man", "auch", "sonst", "sagt", ")", "ein", "rich\u00b7ti\u00b7ger", "Ver\u00b7stand", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PWS", "PIS", "ADV", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um den bat Salomo; da den er kunte haben,", "tokens": ["Um", "den", "bat", "Sa\u00b7lo\u00b7mo", ";", "da", "den", "er", "kun\u00b7te", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "VVFIN", "NE", "$.", "KOUS", "ART", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da fehlt ihm sonsten nichts an k\u00f6niglichen Gaben.", "tokens": ["Da", "fehlt", "ihm", "sons\u00b7ten", "nichts", "an", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}