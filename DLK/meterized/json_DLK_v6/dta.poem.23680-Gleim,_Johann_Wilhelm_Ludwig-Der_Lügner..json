{"dta.poem.23680": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der L\u00fcgner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J&#252;ngst sprach ein frommer Pfarrer:", "tokens": ["J", "&#252;", "ngst", "sprach", "ein", "from\u00b7mer", "Pfar\u00b7rer", ":"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin ein Feind von M\u00e4dchen.", "tokens": ["Ich", "bin", "ein", "Feind", "von", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein mich deucht, er l\u00fcget.", "tokens": ["Al\u00b7lein", "mich", "deucht", ",", "er", "l\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn gestern, nach der Kirche,", "tokens": ["Denn", "ge\u00b7stern", ",", "nach", "der", "Kir\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wollt er ein M\u00e4dchen trauen,", "tokens": ["Wollt", "er", "ein", "M\u00e4d\u00b7chen", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da trat er zu dem M\u00e4dchen,", "tokens": ["Da", "trat", "er", "zu", "dem", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schielte nach dem Busen.", "tokens": ["Und", "schiel\u00b7te", "nach", "dem", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}