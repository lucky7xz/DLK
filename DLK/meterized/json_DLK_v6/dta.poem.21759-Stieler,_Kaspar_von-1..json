{"dta.poem.21759": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die g\u00f6ldne Nacht-Laterne", "tokens": ["Die", "g\u00f6ld\u00b7ne", "Nacht\u00b7La\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit ihrem Sternen-Chorr", "tokens": ["mit", "ih\u00b7rem", "Ster\u00b7nen\u00b7Chorr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "erstarret und h\u00f6ret gerne/", "tokens": ["er\u00b7star\u00b7ret", "und", "h\u00f6\u00b7ret", "ger\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wie s\u00fc\u00df der Filidor", "tokens": ["wie", "s\u00fc\u00df", "der", "Fi\u00b7li\u00b7dor"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "auff seiner Fl\u00f6ten spielet/", "tokens": ["auff", "sei\u00b7ner", "Fl\u00f6\u00b7ten", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "auch so/ da\u00df alle Welt", "tokens": ["auch", "so", "/", "da\u00df", "al\u00b7le", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "KOUS", "PIAT", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "die s\u00fcsse Flamme f\u00fchlet", "tokens": ["die", "s\u00fcs\u00b7se", "Flam\u00b7me", "f\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Lieben ist ein Kriegen", "tokens": ["Das", "Lie\u00b7ben", "ist", "ein", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und zwar ein s\u00fcsser Streit.", "tokens": ["und", "zwar", "ein", "s\u00fcs\u00b7ser", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer nicht wil unten liegen/", "tokens": ["Wer", "nicht", "wil", "un\u00b7ten", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der mu\u00df in Freundligkeit", "tokens": ["der", "mu\u00df", "in", "Freund\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "tag-t\u00e4glich nur verschiessen", "tokens": ["tag\u00b7t\u00e4g\u00b7lich", "nur", "ver\u00b7schies\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "vor Pulver/ Lipppen-safft.", "tokens": ["vor", "Pul\u00b7ver", "/", "Lipp\u00b7pen\u00b7\u00b7safft", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit lautern kurzen Spiessen", "tokens": ["Mit", "lau\u00b7tern", "kur\u00b7zen", "Spies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "wird hier der Sieg geschafft.", "tokens": ["wird", "hier", "der", "Sieg", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Filidor/ dein Singen", "tokens": ["Mein", "Fi\u00b7li\u00b7dor", "/", "dein", "Sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gefiel dem Mavors wohl/", "tokens": ["ge\u00b7fiel", "dem", "Ma\u00b7vors", "wohl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der Pindus must\u2019 erklingen/", "tokens": ["der", "Pin\u00b7dus", "must'", "er\u00b7klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "auch so/ da\u00df selbst der Pohl/", "tokens": ["auch", "so", "/", "da\u00df", "selbst", "der", "Pohl", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "du nahmst den Feder-kiel", "tokens": ["du", "nahmst", "den", "Fe\u00b7der\u00b7kiel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und schriebst von s\u00fcssen schn\u00e4beln", "tokens": ["und", "schriebst", "von", "s\u00fcs\u00b7sen", "schn\u00e4\u00b7beln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und von dem Venus-Spiel\u2019.", "tokens": ["und", "von", "dem", "Ve\u00b7nus\u00b7Spiel'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ei! la\u00df dich weiter h\u00f6ren/", "tokens": ["Ei", "!", "la\u00df", "dich", "wei\u00b7ter", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mein s\u00fcsser Filidor/", "tokens": ["mein", "s\u00fcs\u00b7ser", "Fi\u00b7li\u00b7dor", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "du kanst den Krieg verst\u00f6ren.", "tokens": ["du", "kanst", "den", "Krieg", "ver\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mars h\u00e4lt die Faust empor/", "tokens": ["Mars", "h\u00e4lt", "die", "Faust", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und wil der Venus schenken", "tokens": ["und", "wil", "der", "Ve\u00b7nus", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die Blut bespr\u00fctzte Fahn\u2019/", "tokens": ["die", "Blut", "be\u00b7spr\u00fctz\u00b7te", "Fahn'", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "er wil nicht mehr gedencken", "tokens": ["er", "wil", "nicht", "mehr", "ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "an Schwerdt und Pusikan.", "tokens": ["an", "Schwerdt", "und", "Pu\u00b7si\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum schreib doch nur was kekker/", "tokens": ["Drum", "schreib", "doch", "nur", "was", "kek\u00b7ker", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "PWS", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Edler Filidor/", "tokens": ["Mein", "Ed\u00b7ler", "Fi\u00b7li\u00b7dor", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dir spricht der kleine Lekker", "tokens": ["dir", "spricht", "der", "klei\u00b7ne", "Lek\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "was heimlich in das Ohr.", "tokens": ["was", "heim\u00b7lich", "in", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du must ja nichts verschweigen", "tokens": ["Du", "must", "ja", "nichts", "ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man nimt dich an zum Zeugen", "tokens": ["Man", "nimt", "dich", "an", "zum", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "von solcher s\u00fcssen Schlacht!", "tokens": ["von", "sol\u00b7cher", "s\u00fcs\u00b7sen", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}