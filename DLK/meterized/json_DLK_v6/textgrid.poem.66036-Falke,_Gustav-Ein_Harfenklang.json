{"textgrid.poem.66036": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Harfenklang", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wind, im dunklen Laube w\u00fchlend, bringt", "tokens": ["Der", "Wind", ",", "im", "dunk\u00b7len", "Lau\u00b7be", "w\u00fch\u00b7lend", ",", "bringt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu mir den Ruf der wachen Nachtigallen:", "tokens": ["Zu", "mir", "den", "Ruf", "der", "wa\u00b7chen", "Nach\u00b7ti\u00b7gal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dazwischen: welch ein Ton? Ein Fremdes singt.", "tokens": ["Da\u00b7zwi\u00b7schen", ":", "welch", "ein", "Ton", "?", "Ein", "Frem\u00b7des", "singt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "PWAT", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Woher die Stimmen, die bald sacht,", "tokens": ["Wo\u00b7her", "die", "Stim\u00b7men", ",", "die", "bald", "sacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald schwer aufklingen aus der Nacht", "tokens": ["Bald", "schwer", "auf\u00b7klin\u00b7gen", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jetzt wie in sich selbst verhallen?", "tokens": ["Und", "jetzt", "wie", "in", "sich", "selbst", "ver\u00b7hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der wei\u00dfe Apfelzweig,", "tokens": ["Der", "wei\u00b7\u00dfe", "Ap\u00b7fel\u00b7zweig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der sich vor meinem offnen Fenster wiegt,", "tokens": ["Der", "sich", "vor", "mei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ans Glas die feuchten Bl\u00fcten schmiegt,", "tokens": ["Ans", "Glas", "die", "feuch\u00b7ten", "Bl\u00fc\u00b7ten", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gl\u00e4nzt m\u00e4rchenhaft im Vollmondlicht,", "tokens": ["Gl\u00e4nzt", "m\u00e4r\u00b7chen\u00b7haft", "im", "Voll\u00b7mond\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und heilig schimmern B\u00fcsche, Beet und Steig,", "tokens": ["Und", "hei\u00b7lig", "schim\u00b7mern", "B\u00fc\u00b7sche", ",", "Beet", "und", "Steig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mein Blick ist fassungslos geweitet:", "tokens": ["Mein", "Blick", "ist", "fas\u00b7sungs\u00b7los", "ge\u00b7wei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O welches hohe Fest ist hier bereitet", "tokens": ["O", "wel\u00b7ches", "ho\u00b7he", "Fest", "ist", "hier", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "ADJA", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den feinen Seelen, die in Tr\u00e4umen leben", "tokens": ["Den", "fei\u00b7nen", "See\u00b7len", ",", "die", "in", "Tr\u00e4u\u00b7men", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unter jedem leisen Ton erbeben,", "tokens": ["Und", "un\u00b7ter", "je\u00b7dem", "lei\u00b7sen", "Ton", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der von der Harfe der Gottheit klingt und k\u00fcndet,", "tokens": ["Der", "von", "der", "Har\u00b7fe", "der", "Got\u00b7theit", "klingt", "und", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Da\u00df sie noch immer", "tokens": ["Da\u00df", "sie", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Zum alten Spiel die flei\u00dfigen Finger r\u00fcndet", "tokens": ["Zum", "al\u00b7ten", "Spiel", "die", "flei\u00b7\u00dfi\u00b7gen", "Fin\u00b7ger", "r\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und noch zu Ende nicht ihr Lied gebracht.", "tokens": ["Und", "noch", "zu", "En\u00b7de", "nicht", "ihr", "Lied", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie endets nimmer,", "tokens": ["Sie", "en\u00b7dets", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Horch, welch ein Klang der Liebe durch die Nacht!", "tokens": ["Horch", ",", "welch", "ein", "Klang", "der", "Lie\u00b7be", "durch", "die", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}