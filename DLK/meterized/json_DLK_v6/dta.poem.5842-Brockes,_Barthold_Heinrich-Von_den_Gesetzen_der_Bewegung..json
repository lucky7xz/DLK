{"dta.poem.5842": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sehr die Welt an Aenderungen reich:", "tokens": ["Wie", "sehr", "die", "Welt", "an", "A\u00b7en\u00b7de\u00b7run\u00b7gen", "reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie viel Verschiedenheit die vielen C\u00f6rper hegen:", "tokens": ["Wie", "viel", "Ver\u00b7schie\u00b7den\u00b7heit", "die", "vie\u00b7len", "C\u00f6r\u00b7per", "he\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie unterschiedentlich wir sie zu nennen pflegen;", "tokens": ["Wie", "un\u00b7ter\u00b7schie\u00b7dent\u00b7lich", "wir", "sie", "zu", "nen\u00b7nen", "pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So ist jedoch ihr Wesen gleich.", "tokens": ["So", "ist", "je\u00b7doch", "ihr", "We\u00b7sen", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man kennt in ihnen blos allein", "tokens": ["Man", "kennt", "in", "ih\u00b7nen", "blos", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschiedne Grad von einerley Bewegen.", "tokens": ["Ver\u00b7schied\u00b7ne", "Grad", "von", "ei\u00b7ner\u00b7ley", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch die Materie kau nicht ge\u00e4ndert seyn,", "tokens": ["Auch", "die", "Ma\u00b7te\u00b7rie", "kau", "nicht", "ge\u00b7\u00e4n\u00b7dert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Als blos in so weit nur,", "tokens": ["Als", "blos", "in", "so", "weit", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADV", "ADJD", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Da\u00df sie auf andre Art getheilet ist. Es bleibet,", "tokens": ["Da\u00df", "sie", "auf", "and\u00b7re", "Art", "ge\u00b7thei\u00b7let", "ist", ".", "Es", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "VAFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ohn da\u00df es etwas hintertreibet,", "tokens": ["Ohn", "da\u00df", "es", "et\u00b7was", "hin\u00b7ter\u00b7trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In ihr, zu aller Zeit, dieselbige Natur.", "tokens": ["In", "ihr", ",", "zu", "al\u00b7ler", "Zeit", ",", "die\u00b7sel\u00b7bi\u00b7ge", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PIAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}