{"textgrid.poem.43564": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Weidenfl\u00f6te", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jacob, wenn du Weiden schneidest,", "tokens": ["Ja\u00b7cob", ",", "wenn", "du", "Wei\u00b7den", "schnei\u00b7dest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O so denk' an mich auch dann!", "tokens": ["O", "so", "denk'", "an", "mich", "auch", "dann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneid' mir eine Weidenfl\u00f6te,", "tokens": ["Schneid'", "mir", "ei\u00b7ne", "Wei\u00b7den\u00b7fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich mir was spielen kann.", "tokens": ["Da\u00df", "ich", "mir", "was", "spie\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lieber Jacob, ja du thust es,", "tokens": ["Lie\u00b7ber", "Ja\u00b7cob", ",", "ja", "du", "thust", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NE", "$,", "ADV", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Weil du mir so gut doch bist.", "tokens": ["Weil", "du", "mir", "so", "gut", "doch", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich keine Weidenfl\u00f6te,", "tokens": ["Hab'", "ich", "kei\u00b7ne", "Wei\u00b7den\u00b7fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df ich kaum, da\u00df Fr\u00fchling ist.", "tokens": ["Wei\u00df", "ich", "kaum", ",", "da\u00df", "Fr\u00fch\u00b7ling", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der stillen Zeit am Abend", "tokens": ["In", "der", "stil\u00b7len", "Zeit", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh' ich auf und ab am Thor,", "tokens": ["Geh'", "ich", "auf", "und", "ab", "am", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "KON", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spiel' auf meiner Weidenfl\u00f6te", "tokens": ["Spiel'", "auf", "mei\u00b7ner", "Wei\u00b7den\u00b7fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedermann ein St\u00fcckchen vor.", "tokens": ["Je\u00b7der\u00b7mann", "ein", "St\u00fcck\u00b7chen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mutter kommet mit dem Vater,", "tokens": ["Mut\u00b7ter", "kom\u00b7met", "mit", "dem", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Schwestern alle drei:", "tokens": ["Und", "die", "Schwes\u00b7tern", "al\u00b7le", "drei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedem spiel' ich dann ein St\u00fcckchen,", "tokens": ["Je\u00b7dem", "spiel'", "ich", "dann", "ein", "St\u00fcck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber meiner Mutter zwei.", "tokens": ["A\u00b7ber", "mei\u00b7ner", "Mut\u00b7ter", "zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}