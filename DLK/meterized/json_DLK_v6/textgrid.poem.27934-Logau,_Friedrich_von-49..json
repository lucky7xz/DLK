{"textgrid.poem.27934": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mars ist ein Gewissens-Mann,", "tokens": ["Mars", "ist", "ein", "Ge\u00b7wis\u00b7sens\u00b7Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich nimmt der Menschheit an;", "tokens": ["Der", "sich", "nimmt", "der", "Menschheit", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schl\u00e4gt er Menschen h\u00e4uffig nieder,", "tokens": ["Schl\u00e4gt", "er", "Men\u00b7schen", "h\u00e4uf\u00b7fig", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeugt er Menschen h\u00e4uffig wieder.", "tokens": ["Zeugt", "er", "Men\u00b7schen", "h\u00e4uf\u00b7fig", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}