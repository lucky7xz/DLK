{"textgrid.poem.59974": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "O wag es doch nur einen Tag!", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, mein Volk, mit Trommelschlag", "tokens": ["Frisch", "auf", ",", "mein", "Volk", ",", "mit", "Trom\u00b7mel\u00b7schlag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Zorneswetterschein!", "tokens": ["Im", "Zor\u00b7nes\u00b7wet\u00b7ter\u00b7schein", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wag es doch, nur ", "tokens": ["O", "wag", "es", "doch", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und ob der Sieg vor Sternenlicht", "tokens": ["Und", "ob", "der", "Sieg", "vor", "Ster\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Feinde schon geh\u00f6rt \u2013", "tokens": ["Dem", "Fein\u00b7de", "schon", "ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ein Herz, das sich emp\u00f6rt.", "tokens": ["Ein", "Herz", ",", "das", "sich", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wart in deiner tiefen Not", "tokens": ["O", "wart", "in", "dei\u00b7ner", "tie\u00b7fen", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf keinen Ehebund;", "tokens": ["Auf", "kei\u00b7nen", "E\u00b7he\u00b7bund", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer liebt, der gehet in den Tod", "tokens": ["Wer", "liebt", ",", "der", "ge\u00b7het", "in", "den", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr eine Sch\u00e4ferstund:", "tokens": ["F\u00fcr", "ei\u00b7ne", "Sch\u00e4\u00b7fer\u00b7stund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wer die Ketten knirschend trug,", "tokens": ["Und", "wer", "die", "Ket\u00b7ten", "knir\u00b7schend", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem ist das Sterben Lust", "tokens": ["Dem", "ist", "das", "Ster\u00b7ben", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Aus unterdr\u00fcckter Brust.", "tokens": ["Aus", "un\u00b7ter\u00b7dr\u00fcck\u00b7ter", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df deine Weisen fort und fort", "tokens": ["La\u00df", "dei\u00b7ne", "Wei\u00b7sen", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Tod und Schrecken sehn,", "tokens": ["Nur", "Tod", "und", "Schre\u00b7cken", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Volk soll vor Prophetenwort", "tokens": ["Dem", "Volk", "soll", "vor", "Pro\u00b7phe\u00b7ten\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ruf der Ehre gehn.", "tokens": ["Der", "Ruf", "der", "Eh\u00b7re", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Horch auf, der letzte W\u00fcrfel f\u00e4llt,", "tokens": ["Horch", "auf", ",", "der", "letz\u00b7te", "W\u00fcr\u00b7fel", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Abend, er ist nah,", "tokens": ["Dein", "A\u00b7bend", ",", "er", "ist", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "In deiner Gr\u00f6\u00dfe da!", "tokens": ["In", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O tilg nur ", "tokens": ["O", "tilg", "nur"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "ADV"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Aus deiner Sklaverei,", "tokens": ["Aus", "dei\u00b7ner", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zeig dem grollenden Geschick,", "tokens": ["Und", "zeig", "dem", "grol\u00b7len\u00b7den", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie nicht ewig sei;", "tokens": ["Da\u00df", "sie", "nicht", "e\u00b7wig", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erwach aus deinem b\u00f6sen Traum:", "tokens": ["Er\u00b7wach", "aus", "dei\u00b7nem", "b\u00f6\u00b7sen", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Reif ist, die du gesucht,", "tokens": ["Reif", "ist", ",", "die", "du", "ge\u00b7sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und sch\u00fcttle nicht zu sp\u00e4t vom Baum,", "tokens": ["Und", "sch\u00fctt\u00b7le", "nicht", "zu", "sp\u00e4t", "vom", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sie gefault, die Frucht.", "tokens": ["Wenn", "sie", "ge\u00b7fault", ",", "die", "Frucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wach auf! wach auf! die Morgenluft", "tokens": ["Wach", "auf", "!", "wach", "auf", "!", "die", "Mor\u00b7gen\u00b7luft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "ADJD", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt mahnend an dein Ohr \u2013", "tokens": ["Schl\u00e4gt", "mah\u00b7nend", "an", "dein", "Ohr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus deiner tausendj\u00e4hr'gen Gruft", "tokens": ["Aus", "dei\u00b7ner", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gen", "Gruft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Empor, mein Volk, empor!", "tokens": ["Em\u00b7por", ",", "mein", "Volk", ",", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df kommen, was da kommen mag:", "tokens": ["La\u00df", "kom\u00b7men", ",", "was", "da", "kom\u00b7men", "mag", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Blitz auf, ein Wetterschein!", "tokens": ["Blitz", "auf", ",", "ein", "Wet\u00b7ter\u00b7schein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und wag's, und w\u00e4r's nur ", "tokens": ["Und", "wag's", ",", "und", "w\u00e4r's", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KON", "VAFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein freies Volk zu sein!", "tokens": ["Ein", "frei\u00b7es", "Volk", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}