{"textgrid.poem.24689": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer Halle hat er mich empfangen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Halle hat er mich empfangen,", "tokens": ["In", "ei\u00b7ner", "Hal\u00b7le", "hat", "er", "mich", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die r\u00e4tselhaft mich \u00e4ngstet mit Gewalt,", "tokens": ["Die", "r\u00e4t\u00b7sel\u00b7haft", "mich", "\u00e4ngs\u00b7tet", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von s\u00fc\u00dfen D\u00fcften widerlich durchwallt:", "tokens": ["Von", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7ten", "wi\u00b7der\u00b7lich", "durch\u00b7wallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da h\u00e4ngen fremde V\u00f6gel, bunte Schlangen.", "tokens": ["Da", "h\u00e4n\u00b7gen", "frem\u00b7de", "V\u00f6\u00b7gel", ",", "bun\u00b7te", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Tor f\u00e4llt zu, des Lebens Laut verhallt,", "tokens": ["Das", "Tor", "f\u00e4llt", "zu", ",", "des", "Le\u00b7bens", "Laut", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Seele Atmen hemmt ein dumpfes Bangen,", "tokens": ["Der", "See\u00b7le", "At\u00b7men", "hemmt", "ein", "dum\u00b7pfes", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zaubertrunk h\u00e4lt jeden Sinn befangen", "tokens": ["Ein", "Zau\u00b7ber\u00b7trunk", "h\u00e4lt", "je\u00b7den", "Sinn", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles fl\u00fcchtet hilflos, ohne Halt.", "tokens": ["Und", "al\u00b7les", "fl\u00fcch\u00b7tet", "hil\u00b7flos", ",", "oh\u00b7ne", "Halt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er aber ist nicht wie er immer war,", "tokens": ["Er", "a\u00b7ber", "ist", "nicht", "wie", "er", "im\u00b7mer", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Auge bannt und fremd ist Stirn und Haar.", "tokens": ["Sein", "Au\u00b7ge", "bannt", "und", "fremd", "ist", "Stirn", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von seinen Worten, den unscheinbar leisen,", "tokens": ["Von", "sei\u00b7nen", "Wor\u00b7ten", ",", "den", "un\u00b7schein\u00b7bar", "lei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geht eine Herrschaft aus und ein Verf\u00fchren,", "tokens": ["Geht", "ei\u00b7ne", "Herr\u00b7schaft", "aus", "und", "ein", "Ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er macht die leere Luft beengend kreisen", "tokens": ["Er", "macht", "die", "lee\u00b7re", "Luft", "be\u00b7en\u00b7gend", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und er kann t\u00f6ten, ohne zu ber\u00fchren.", "tokens": ["Und", "er", "kann", "t\u00f6\u00b7ten", ",", "oh\u00b7ne", "zu", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}