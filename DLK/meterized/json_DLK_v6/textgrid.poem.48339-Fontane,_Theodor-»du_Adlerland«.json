{"textgrid.poem.48339": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbdu Adlerland\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Adlerland, das seiner Schwingen R\u00e4nder", "tokens": ["Du", "Ad\u00b7ler\u00b7land", ",", "das", "sei\u00b7ner", "Schwin\u00b7gen", "R\u00e4n\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Links in den Rhein, rechts in den Njemen taucht,", "tokens": ["Links", "in", "den", "Rhein", ",", "rechts", "in", "den", "Nje\u00b7men", "taucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Du Zukunftsland, du Hoffnung deutscher L\u00e4nder,", "tokens": ["Du", "Zu\u00b7kunfts\u00b7land", ",", "du", "Hoff\u00b7nung", "deut\u00b7scher", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das, um zu siegen, nur zu ", "tokens": ["Das", ",", "um", "zu", "sie\u00b7gen", ",", "nur", "zu"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "KOUI", "PTKZU", "VVINF", "$,", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Zu Fl\u00fcgen, h\u00f6hern, vollern,", "tokens": ["Zu", "Fl\u00fc\u00b7gen", ",", "h\u00f6\u00b7hern", ",", "vol\u00b7lern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Raff auf dich, Land der Zollern,", "tokens": ["Raff", "auf", "dich", ",", "Land", "der", "Zol\u00b7lern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Non soli cedo tr\u00e4gst du auf der Brust,", "tokens": ["Non", "so\u00b7li", "ce\u00b7do", "tr\u00e4gst", "du", "auf", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Drum ", "tokens": ["Drum"], "token_info": ["word"], "pos": ["PAV"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Aus kleinem Anfang bist du aufgesprossen,", "tokens": ["Aus", "klei\u00b7nem", "An\u00b7fang", "bist", "du", "auf\u00b7ge\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du letztes Glied an deutschen Reiches Rumpf,", "tokens": ["Du", "letz\u00b7tes", "Glied", "an", "deut\u00b7schen", "Rei\u00b7ches", "Rumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du (einst der Spott begl\u00fcckterer Genossen),", "tokens": ["Du", "(", "einst", "der", "Spott", "be\u00b7gl\u00fcck\u00b7te\u00b7rer", "Ge\u00b7nos\u00b7sen", ")", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$(", "ADV", "ART", "NN", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du stiegst empor aus Wald und Sand und Sumpf;", "tokens": ["Du", "stiegst", "em\u00b7por", "aus", "Wald", "und", "Sand", "und", "Sumpf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gott hat dich aufgerichtet,", "tokens": ["Gott", "hat", "dich", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sumpf, Wald, sie sind gelichtet,", "tokens": ["Sumpf", ",", "Wald", ",", "sie", "sind", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein drang die ", "tokens": ["Ein", "drang", "die"], "token_info": ["word", "word", "word"], "pos": ["ART", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Schuf Land aus Sumpf, die ", "tokens": ["Schuf", "Land", "aus", "Sumpf", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "APPR", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ja, heller Sonnenschein hat dich geboren:", "tokens": ["Ja", ",", "hel\u00b7ler", "Son\u00b7nen\u00b7schein", "hat", "dich", "ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewissensfreiheit, Mut, Gesetz und Recht,", "tokens": ["Ge\u00b7wis\u00b7sens\u00b7frei\u00b7heit", ",", "Mut", ",", "Ge\u00b7setz", "und", "Recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O gib die alte Triebkraft nicht verloren,", "tokens": ["O", "gib", "die", "al\u00b7te", "Trieb\u00b7kraft", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermach sie neu dem kommenden Geschlecht \u2013", "tokens": ["Ver\u00b7mach", "sie", "neu", "dem", "kom\u00b7men\u00b7den", "Ge\u00b7schlecht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von deinen Edelsteinen", "tokens": ["Von", "dei\u00b7nen", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aufopfre keinen, keinen,", "tokens": ["Auf\u00b7opf\u00b7re", "kei\u00b7nen", ",", "kei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIAT", "$,", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor allem doch in Schild und Krone dein", "tokens": ["Vor", "al\u00b7lem", "doch", "in", "Schild", "und", "Kro\u00b7ne", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "APPR", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "So vieles f\u00e4llt (es kommt zu sp\u00e4t die Reue),", "tokens": ["So", "vie\u00b7les", "f\u00e4llt", "(", "es", "kommt", "zu", "sp\u00e4t", "die", "Reu\u00b7e", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$(", "PPER", "VVFIN", "PTKA", "ADJD", "ART", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du gl\u00fccklich Land, so Gott will, stehest fest,", "tokens": ["Du", "gl\u00fcck\u00b7lich", "Land", ",", "so", "Gott", "will", ",", "ste\u00b7hest", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "ADV", "NN", "VMFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du stehest fest, weil noch in alter Treue", "tokens": ["Du", "ste\u00b7hest", "fest", ",", "weil", "noch", "in", "al\u00b7ter", "Treu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein F\u00fcrst, dein Volk, keins von dem andern l\u00e4\u00dft;", "tokens": ["Dein", "F\u00fcrst", ",", "dein", "Volk", ",", "keins", "von", "dem", "an\u00b7dern", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PIAT", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So war's in alten Tagen,", "tokens": ["So", "wa\u00b7r's", "in", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "So sei's bei neuem Wagen,", "tokens": ["So", "sei's", "bei", "neu\u00b7em", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann komm', was mag, ob Ost, ob Westen droh',", "tokens": ["Dann", "komm'", ",", "was", "mag", ",", "ob", "Ost", ",", "ob", "Wes\u00b7ten", "droh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VMFIN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der letzte Trumpf bleibt ", "tokens": ["Der", "letz\u00b7te", "Trumpf", "bleibt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nur Eintracht siegt! o wahr dich vor dem Hadern,", "tokens": ["Nur", "Ein\u00b7tracht", "siegt", "!", "o", "wahr", "dich", "vor", "dem", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$.", "FM", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch freue dich wettstreitender Partei'n,", "tokens": ["Doch", "freu\u00b7e", "dich", "wett\u00b7strei\u00b7ten\u00b7der", "Par\u00b7tei'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie lockern nicht, sie festigen die Quadern,", "tokens": ["Sie", "lo\u00b7ckern", "nicht", ",", "sie", "fes\u00b7ti\u00b7gen", "die", "Qua\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Aus Freiheit und aus Treue", "tokens": ["Aus", "Frei\u00b7heit", "und", "aus", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprie\u00dft immer Sieg aufs neue.", "tokens": ["Sprie\u00dft", "im\u00b7mer", "Sieg", "aufs", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.8": {"text": "Wirst siegen du, du deutsches Zukunftsland.", "tokens": ["Wirst", "sie\u00b7gen", "du", ",", "du", "deut\u00b7sches", "Zu\u00b7kunfts\u00b7land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}