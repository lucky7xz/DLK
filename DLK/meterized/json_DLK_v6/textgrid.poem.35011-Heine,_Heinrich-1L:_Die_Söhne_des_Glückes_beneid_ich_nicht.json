{"textgrid.poem.35011": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die S\u00f6hne des Gl\u00fcckes beneid ich nicht", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00f6hne des Gl\u00fcckes beneid ich nicht", "tokens": ["Die", "S\u00f6h\u00b7ne", "des", "Gl\u00fc\u00b7ckes", "be\u00b7neid", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ob ihrem Leben, beneiden", "tokens": ["Ob", "ih\u00b7rem", "Le\u00b7ben", ",", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Will ich sie nur ob ihrem Tod,", "tokens": ["Will", "ich", "sie", "nur", "ob", "ih\u00b7rem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem schmerzlos raschen Verscheiden.", "tokens": ["Dem", "schmerz\u00b7los", "ra\u00b7schen", "Ver\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Prachtgewand, das Haupt bekr\u00e4nzt,", "tokens": ["Im", "Pracht\u00b7ge\u00b7wand", ",", "das", "Haupt", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Lachen auf der Lippe,", "tokens": ["Und", "La\u00b7chen", "auf", "der", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sitzen sie froh beim Lebensbankett \u2013", "tokens": ["Sit\u00b7zen", "sie", "froh", "beim", "Le\u00b7bens\u00b7ban\u00b7kett", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da trifft sie j\u00e4hlings die Hippe.", "tokens": ["Da", "trifft", "sie", "j\u00e4h\u00b7lings", "die", "Hip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Festkleid und mit Rosen geschm\u00fcckt,", "tokens": ["Im", "Fest\u00b7kleid", "und", "mit", "Ro\u00b7sen", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die noch wie lebend bl\u00fchten,", "tokens": ["Die", "noch", "wie", "le\u00b7bend", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gelangen in das Schattenreich", "tokens": ["Ge\u00b7lan\u00b7gen", "in", "das", "Schat\u00b7ten\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fortunas Favoriten.", "tokens": ["For\u00b7tu\u00b7nas", "Fa\u00b7vo\u00b7ri\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nie hatte Siechtum sie entstellt,", "tokens": ["Nie", "hat\u00b7te", "Siech\u00b7tum", "sie", "ent\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind Tote von guter Miene,", "tokens": ["Sind", "To\u00b7te", "von", "gu\u00b7ter", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und huldreich empf\u00e4ngt sie an ihrem Hof", "tokens": ["Und", "huld\u00b7reich", "emp\u00b7f\u00e4ngt", "sie", "an", "ih\u00b7rem", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zarewna Proserpine.", "tokens": ["Za\u00b7rew\u00b7na", "Pro\u00b7ser\u00b7pi\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Wie sehr mu\u00df ich beneiden ihr Los!", "tokens": ["Wie", "sehr", "mu\u00df", "ich", "be\u00b7nei\u00b7den", "ihr", "Los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schon sieben Jahre mit herben,", "tokens": ["Schon", "sie\u00b7ben", "Jah\u00b7re", "mit", "her\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Qualvollen Gebresten w\u00e4lz ich mich", "tokens": ["Qual\u00b7vol\u00b7len", "Ge\u00b7bres\u00b7ten", "w\u00e4lz", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Boden, und kann nicht sterben!", "tokens": ["Am", "Bo\u00b7den", ",", "und", "kann", "nicht", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "O Gott, verk\u00fcrze meine Qual,", "tokens": ["O", "Gott", ",", "ver\u00b7k\u00fcr\u00b7ze", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit man mich bald begrabe;", "tokens": ["Da\u00b7mit", "man", "mich", "bald", "be\u00b7gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Du wei\u00dft ja, da\u00df ich kein Talent", "tokens": ["Du", "wei\u00dft", "ja", ",", "da\u00df", "ich", "kein", "Ta\u00b7lent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Martyrtume habe.", "tokens": ["Zum", "Mar\u00b7tyr\u00b7tu\u00b7me", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ob deiner Inkonsequenz, o Herr,", "tokens": ["Ob", "dei\u00b7ner", "In\u00b7kon\u00b7se\u00b7quenz", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erlaube, da\u00df ich staune:", "tokens": ["Er\u00b7lau\u00b7be", ",", "da\u00df", "ich", "stau\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du schufest den fr\u00f6hlichsten Dichter, und raubst", "tokens": ["Du", "schu\u00b7fest", "den", "fr\u00f6h\u00b7lichs\u00b7ten", "Dich\u00b7ter", ",", "und", "raubst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihm jetzt seine gute Laune.", "tokens": ["Ihm", "jetzt", "sei\u00b7ne", "gu\u00b7te", "Lau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Schmerz verdampft den heitern Sinn", "tokens": ["Der", "Schmerz", "ver\u00b7dampft", "den", "hei\u00b7tern", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht mich melancholisch;", "tokens": ["Und", "macht", "mich", "me\u00b7lan\u00b7cho\u00b7lisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nimmt nicht der traurige Spa\u00df ein End',", "tokens": ["Nimmt", "nicht", "der", "trau\u00b7ri\u00b7ge", "Spa\u00df", "ein", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "So werd ich am Ende katholisch.", "tokens": ["So", "werd", "ich", "am", "En\u00b7de", "ka\u00b7tho\u00b7lisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich heule dir dann die Ohren voll,", "tokens": ["Ich", "heu\u00b7le", "dir", "dann", "die", "Oh\u00b7ren", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie andre gute Christen \u2013", "tokens": ["Wie", "and\u00b7re", "gu\u00b7te", "Chris\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Miserere! Verloren geht", "tokens": ["O", "Mi\u00b7se\u00b7re\u00b7re", "!", "Ver\u00b7lo\u00b7ren", "geht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "VVFIN"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der beste der Humoristen!", "tokens": ["Der", "bes\u00b7te", "der", "Hu\u00b7mo\u00b7ris\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}