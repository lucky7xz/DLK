{"textgrid.poem.44883": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein Adel mehr, um einen wen'ger wieder]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Adel mehr, um einen wen'ger wieder,", "tokens": ["Ein", "A\u00b7del", "mehr", ",", "um", "ei\u00b7nen", "wen'\u00b7ger", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUI", "ART", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was liegt nicht in der M\u00f6glichkeit Bereich!", "tokens": ["Was", "liegt", "nicht", "in", "der", "M\u00f6g\u00b7lich\u00b7keit", "Be\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Deinhartstein erh\u00f6ht, Zedlitz erniedert,", "tokens": ["De\u00b7in\u00b7hart\u00b7stein", "er\u00b7h\u00f6ht", ",", "Zed\u00b7litz", "er\u00b7nie\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sind sie denn sich gleich.", "tokens": ["So", "sind", "sie", "denn", "sich", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}