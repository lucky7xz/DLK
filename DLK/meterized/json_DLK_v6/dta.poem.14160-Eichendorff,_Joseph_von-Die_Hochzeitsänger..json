{"dta.poem.14160": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeits\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt ist Schmau\u00df und Tanz zerstoben,", "tokens": ["Jetzt", "ist", "Schmau\u00df", "und", "Tanz", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer stiller wird's im Haus,", "tokens": ["Im\u00b7mer", "stil\u00b7ler", "wird's", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die M\u00e4gde putzen oben", "tokens": ["Und", "die", "M\u00e4g\u00b7de", "put\u00b7zen", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle lust'gen Kerzen aus.", "tokens": ["Al\u00b7le", "lust'\u00b7gen", "Ker\u00b7zen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wir blasen recht mit Rasen", "tokens": ["Doch", "wir", "bla\u00b7sen", "recht", "mit", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder in sein Instrument,", "tokens": ["Je\u00b7der", "in", "sein", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6cht' in meinem Grimm ausblasen", "tokens": ["M\u00f6cht'", "in", "mei\u00b7nem", "Grimm", "aus\u00b7bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Stern' am Firmament!", "tokens": ["Al\u00b7le", "Stern'", "am", "Fir\u00b7ma\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}