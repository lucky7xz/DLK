{"dta.poem.7198": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Dactylisch Sonnet  \n an den  \n Edlen vnd Weltber\u00fchmten Herrn  \n August Buchnern/  \n Vber die Erfindung der Dactylischen vnd  \n Anap\u00e4stischen Verse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6ret die Lieder wie artlich sie klingen/", "tokens": ["H\u00f6\u00b7ret", "die", "Lie\u00b7der", "wie", "art\u00b7lich", "sie", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ADJD", "PPER", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Welche Herr ", "tokens": ["Wel\u00b7che", "Herr"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Echo sich selbsten in jhnen verliebt/", "tokens": ["E\u00b7cho", "sich", "selbs\u00b7ten", "in", "jh\u00b7nen", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wolte sie gerne mit fre\u00fcden nachsingen/", "tokens": ["Wol\u00b7te", "sie", "ger\u00b7ne", "mit", "fre\u00fc\u00b7den", "nach\u00b7sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Vbet sich stetig die Stimme zu schwingen/", "tokens": ["Vbet", "sich", "ste\u00b7tig", "die", "Stim\u00b7me", "zu", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aber in dem sie noch hefftig betr\u00fcbt/", "tokens": ["A\u00b7ber", "in", "dem", "sie", "noch", "heff\u00b7tig", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nicht mehr als halbe gebrochne wort giebt;", "tokens": ["Nicht", "mehr", "als", "hal\u00b7be", "ge\u00b7broch\u00b7ne", "wort", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00e4lder vnd Felder dem toone nachspringen.", "tokens": ["W\u00e4l\u00b7der", "vnd", "Fel\u00b7der", "dem", "too\u00b7ne", "nach\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVIZU", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Buchner/ so l\u00e4ngste\u0303 vnsterblich gemacht/", "tokens": ["Buch\u00b7ner", "/", "so", "l\u00e4ngs\u00b7t\u1ebd", "vns\u00b7terb\u00b7lich", "ge\u00b7macht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Jtzo mann \u00e4hnlich den G\u00f6ttern Jhn acht/", "tokens": ["Jt\u00b7zo", "mann", "\u00e4hn\u00b7lich", "den", "G\u00f6t\u00b7tern", "Jhn", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "PPER", "CARD", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weil er Dactylisch zu singen erfunden:", "tokens": ["Weil", "er", "Dac\u00b7ty\u00b7lisch", "zu", "sin\u00b7gen", "er\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKZU", "VVINF", "VVPP", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ph\u00f6bus verwundert sich selbsten ob Jhn/", "tokens": ["Ph\u00f6\u00b7bus", "ver\u00b7wun\u00b7dert", "sich", "selbs\u00b7ten", "ob", "Jhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "KOUS", "PPER", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Orphe\u00fcs die Seiten mu\u00df anders aufzihn/", "tokens": ["Or\u00b7phe\u00fcs", "die", "Sei\u00b7ten", "mu\u00df", "an\u00b7ders", "auf\u00b7zihn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Cicero schweiget vnd lieget gebunden.", "tokens": ["Ci\u00b7ce\u00b7ro", "schwei\u00b7get", "vnd", "lie\u00b7get", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}