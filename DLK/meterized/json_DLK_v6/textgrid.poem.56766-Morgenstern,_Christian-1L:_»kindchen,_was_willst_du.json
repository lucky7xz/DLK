{"textgrid.poem.56766": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbkindchen, was willst du", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkindchen, was willst du", "tokens": ["\u00bb", "kind\u00b7chen", ",", "was", "willst", "du"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVINF", "$,", "PWS", "VMFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "erwachen zum Leben?", "tokens": ["er\u00b7wa\u00b7chen", "zum", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Komm mit mir,", "tokens": ["Komm", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "dir ist besser so!", "tokens": ["dir", "ist", "bes\u00b7ser", "so", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den Kampf zu bestehn,", "tokens": ["Den", "Kampf", "zu", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "hast du nicht Kraft,", "tokens": ["hast", "du", "nicht", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "komm, leg dein K\u00f6pfchen", "tokens": ["komm", ",", "leg", "dein", "K\u00f6pf\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NE", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "an meine Brust,", "tokens": ["an", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "sieh doch,", "tokens": ["sieh", "doch", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "mein Mantel ist warm und gut!", "tokens": ["mein", "Man\u00b7tel", "ist", "warm", "und", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Komm, Kindchen,", "tokens": ["Komm", ",", "Kind\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "wir bitten den Wind;", "tokens": ["wir", "bit\u00b7ten", "den", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "der tr\u00e4gt uns hin\u00fcber", "tokens": ["der", "tr\u00e4gt", "uns", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPER", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "in meinen Garten;", "tokens": ["in", "mei\u00b7nen", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "da will ich dich betten", "tokens": ["da", "will", "ich", "dich", "bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "ins gr\u00fcne Gras ...", "tokens": ["ins", "gr\u00fc\u00b7ne", "Gras", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und wenn eine Zeit vergangen ist,", "tokens": ["Und", "wenn", "ei\u00b7ne", "Zeit", "ver\u00b7gan\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "dann wirdst du Blume und Schmetterling,", "tokens": ["dann", "wirdst", "du", "Blu\u00b7me", "und", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "bl\u00fchende Blume, gl\u00fchender Schmetterling ...!", "tokens": ["bl\u00fc\u00b7hen\u00b7de", "Blu\u00b7me", ",", "gl\u00fc\u00b7hen\u00b7der", "Schmet\u00b7ter\u00b7ling", "...", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Nicht wahr, nun willst du?", "tokens": ["Nicht", "wahr", ",", "nun", "willst", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "VMFIN", "PPER", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Komm, kleines Herz!", "tokens": ["Komm", ",", "klei\u00b7nes", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Dir ist besser so!\u00ab", "tokens": ["Dir", "ist", "bes\u00b7ser", "so", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}