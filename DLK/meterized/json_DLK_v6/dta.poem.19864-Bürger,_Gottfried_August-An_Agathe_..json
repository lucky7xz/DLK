{"dta.poem.19864": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An Agathe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem nasgeweinten Schleier               ", "tokens": ["Mit", "dem", "nas\u00b7ge\u00b7wein\u00b7ten", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wisch\u2019 ich meine Z\u00e4ren ab;", "tokens": ["Wisch'", "ich", "mei\u00b7ne", "Z\u00e4\u00b7ren", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Auge schauet freier,", "tokens": ["Und", "mein", "Au\u00b7ge", "schau\u00b7et", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das Leben, bis aus Grab.", "tokens": ["Durch", "das", "Le\u00b7ben", ",", "bis", "aus", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geist erhabner Prophezeihung,", "tokens": ["Geist", "er\u00b7hab\u00b7ner", "Pro\u00b7phe\u00b7zei\u00b7hung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Geist erleuchtet mich!", "tokens": ["Got\u00b7tes", "Geist", "er\u00b7leuch\u00b7tet", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensodem zur Erneuung", "tokens": ["Le\u00b7ben\u00b7so\u00b7dem", "zur", "Er\u00b7neu\u00b7ung"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Weht gewis auch \u00fcber mich.", "tokens": ["Weht", "ge\u00b7wis", "auch", "\u00fc\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedes Drangsal dieses Lebens,", "tokens": ["Je\u00b7des", "Dran\u00b7gsal", "die\u00b7ses", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So dein weiches Herz gedr\u00fckt,", "tokens": ["So", "dein", "wei\u00b7ches", "Herz", "ge\u00b7dr\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeuget, da\u00df du nicht vergebens", "tokens": ["Zeu\u00b7get", ",", "da\u00df", "du", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft nach Trost hinaus geblikt.", "tokens": ["Oft", "nach", "Trost", "hin\u00b7aus", "ge\u00b7blikt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APZR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein! Nicht schwelgenden Gew\u00fcrmen", "tokens": ["Nein", "!", "Nicht", "schwel\u00b7gen\u00b7den", "Ge\u00b7w\u00fcr\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig \u00fcberlasner Raub,", "tokens": ["E\u00b7wig", "\u00fc\u00b7ber\u00b7las\u00b7ner", "Raub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein Spiel den Erdenst\u00fcrmen", "tokens": ["Noch", "ein", "Spiel", "den", "Er\u00b7den\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibet guter Herzen Staub.", "tokens": ["Blei\u00b7bet", "gu\u00b7ter", "Her\u00b7zen", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}