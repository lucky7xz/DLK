{"textgrid.poem.28235": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da der g\u00f6ldne Sonnen-Wagen", "tokens": ["Da", "der", "g\u00f6ld\u00b7ne", "Son\u00b7nen\u00b7Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Fr\u00fchlings-Zucker bringt getragen,", "tokens": ["Fr\u00fch\u00b7lings\u00b7Zu\u00b7cker", "bringt", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die s\u00fcssen Zwillings-K\u00fcsse", "tokens": ["Da\u00df", "die", "s\u00fcs\u00b7sen", "Zwil\u00b7lings\u00b7K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tag und N\u00e4chte machen s\u00fcsse,", "tokens": ["Tag", "und", "N\u00e4ch\u00b7te", "ma\u00b7chen", "s\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da der Himmel g\u00fctig lachet,", "tokens": ["Da", "der", "Him\u00b7mel", "g\u00fc\u00b7tig", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da die Erde Schm\u00fcncke machet,", "tokens": ["Da", "die", "Er\u00b7de", "Schm\u00fcn\u00b7cke", "ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da sich Feld und Wiesen mahlen,", "tokens": ["Da", "sich", "Feld", "und", "Wie\u00b7sen", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da der B\u00e4ume H\u00e4upter pralen,", "tokens": ["Da", "der", "B\u00e4u\u00b7me", "H\u00e4up\u00b7ter", "pra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da die Brunnen Silber gissen,", "tokens": ["Da", "die", "Brun\u00b7nen", "Sil\u00b7ber", "gis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da mit funckeln B\u00e4che flissen,", "tokens": ["Da", "mit", "fun\u00b7ckeln", "B\u00e4\u00b7che", "flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da die Vogel Lieder singen,", "tokens": ["Da", "die", "Vo\u00b7gel", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Da die Fische Spr\u00fcnge springen,", "tokens": ["Da", "die", "Fi\u00b7sche", "Spr\u00fcn\u00b7ge", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da f\u00fcr Freuden alles wiebelt,", "tokens": ["Da", "f\u00fcr", "Freu\u00b7den", "al\u00b7les", "wie\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da mit gleichem gleiches liebelt:", "tokens": ["Da", "mit", "glei\u00b7chem", "glei\u00b7ches", "lie\u00b7belt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "O, so mu\u00df f\u00fcr tr\u00fcbem kr\u00e4ncken", "tokens": ["O", ",", "so", "mu\u00df", "f\u00fcr", "tr\u00fc\u00b7bem", "kr\u00e4n\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Blo\u00df der Mensch die Stirne sencken!", "tokens": ["Blo\u00df", "der", "Mensch", "die", "Stir\u00b7ne", "sen\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Weil zumal bey Fr\u00fchlings-L\u00fcsten", "tokens": ["Weil", "zu\u00b7mal", "bey", "Fr\u00fch\u00b7lings\u00b7L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.18": {"text": "Mars erfrischet sein verw\u00fcsten,", "tokens": ["Mars", "er\u00b7fri\u00b7schet", "sein", "ver\u00b7w\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da er di\u00df f\u00fcr Lust erkennet,", "tokens": ["Da", "er", "di\u00df", "f\u00fcr", "Lust", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wann er raubet, sch\u00e4ndet, brennet.", "tokens": ["Wann", "er", "rau\u00b7bet", ",", "sch\u00e4n\u00b7det", ",", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}