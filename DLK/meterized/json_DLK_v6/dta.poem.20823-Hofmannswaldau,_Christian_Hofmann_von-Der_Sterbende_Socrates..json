{"dta.poem.20823": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wie der so in den Schrancken rennet/", "tokens": ["Wie", "der", "so", "in", "den", "Schran\u00b7cken", "ren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihm den Prei\u00df zu haben traut/", "tokens": ["Und", "ihm", "den", "Prei\u00df", "zu", "ha\u00b7ben", "traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKZU", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Volck und L\u00e4uffer um sich schaut/", "tokens": ["Viel", "Volck", "und", "L\u00e4uf\u00b7fer", "um", "sich", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df wo sich Bahn und Reuter trennet.", "tokens": ["Bi\u00df", "wo", "sich", "Bahn", "und", "Reu\u00b7ter", "tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "PRF", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So plagte mich auch dieser Trieb/", "tokens": ["So", "plag\u00b7te", "mich", "auch", "die\u00b7ser", "Trieb", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war mir leid/ es war mir lieb/", "tokens": ["Es", "war", "mir", "leid", "/", "es", "war", "mir", "lieb", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich muste nur die Feder fassen/", "tokens": ["Ich", "mus\u00b7te", "nur", "die", "Fe\u00b7der", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er sprach mir unaufh\u00f6rlich zu", "tokens": ["Er", "sprach", "mir", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das s\u00fcsse Wesen nicht zu lassen;", "tokens": ["Das", "s\u00fcs\u00b7se", "We\u00b7sen", "nicht", "zu", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich hatte weder Rast noch Ruh.", "tokens": ["Ich", "hat\u00b7te", "we\u00b7der", "Rast", "noch", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}