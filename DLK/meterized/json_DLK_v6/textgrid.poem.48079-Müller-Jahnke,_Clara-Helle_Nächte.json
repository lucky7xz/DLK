{"textgrid.poem.48079": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Helle N\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du, wie tief schon die Sonne steht", "tokens": ["Siehst", "du", ",", "wie", "tief", "schon", "die", "Son\u00b7ne", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "und wie so rot ihr Licht?!", "tokens": ["und", "wie", "so", "rot", "ihr", "Licht", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob sie in funkelnden Wassern zergeht,", "tokens": ["Ob", "sie", "in", "fun\u00b7keln\u00b7den", "Was\u00b7sern", "zer\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "uns beiden stirbt sie nicht.", "tokens": ["uns", "bei\u00b7den", "stirbt", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns leuchtet die Nacht, die niedersinkt", "tokens": ["Uns", "leuch\u00b7tet", "die", "Nacht", ",", "die", "nie\u00b7der\u00b7sinkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und ladet zum letzten Genu\u00df \u2013 \u2013", "tokens": ["und", "la\u00b7det", "zum", "letz\u00b7ten", "Ge\u00b7nu\u00df", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "und unsre lebendige Seele ertrinkt", "tokens": ["und", "uns\u00b7re", "le\u00b7ben\u00b7di\u00b7ge", "See\u00b7le", "er\u00b7trinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "jauchzend im Sch\u00f6pferku\u00df!", "tokens": ["jauch\u00b7zend", "im", "Sch\u00f6p\u00b7fer\u00b7ku\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du und ich, wir beide", "tokens": ["Du", "und", "ich", ",", "wir", "bei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "KON", "PPER", "$,", "PPER", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "tr\u00e4umen in trunkner Nacht.", "tokens": ["tr\u00e4u\u00b7men", "in", "trunk\u00b7ner", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von verbla\u00dfter Seide", "tokens": ["Von", "ver\u00b7bla\u00df\u00b7ter", "Sei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "sind wir \u00fcberdacht.", "tokens": ["sind", "wir", "\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ein Flimmern wie vom Tage", "tokens": ["Ein", "Flim\u00b7mern", "wie", "vom", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "flie\u00dft um den schwarzen Tann \u2013", "tokens": ["flie\u00dft", "um", "den", "schwar\u00b7zen", "Tann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "eine blasse s\u00fc\u00dfe Sage", "tokens": ["ei\u00b7ne", "blas\u00b7se", "s\u00fc\u00b7\u00dfe", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "sieht uns lachend an.", "tokens": ["sieht", "uns", "la\u00b7chend", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sie singt: \u00bbwenn zwei sich finden,", "tokens": ["Sie", "singt", ":", "\u00bb", "wenn", "zwei", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "KOUS", "CARD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die sich von je geh\u00f6rt,", "tokens": ["die", "sich", "von", "je", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ein Leuchten soll es k\u00fcnden,", "tokens": ["ein", "Leuch\u00b7ten", "soll", "es", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "das keine Nacht zerst\u00f6rt.", "tokens": ["das", "kei\u00b7ne", "Nacht", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Singen soll es sagen,", "tokens": ["Ein", "Sin\u00b7gen", "soll", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "das nicht im Sturme zerrinnt\u00ab \u2013", "tokens": ["das", "nicht", "im", "Stur\u00b7me", "zer\u00b7rinnt", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PTKNEG", "APPRART", "NN", "VVPP", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "und in den Syringenhagen", "tokens": ["und", "in", "den", "Sy\u00b7rin\u00b7gen\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "s\u00e4uselt Mittsommerwind.", "tokens": ["s\u00e4u\u00b7selt", "Mitt\u00b7som\u00b7mer\u00b7wind", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}