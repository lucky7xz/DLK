{"textgrid.poem.38604": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lebenslied", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommen und Scheiden,", "tokens": ["Kom\u00b7men", "und", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Suchen und Meiden,", "tokens": ["Su\u00b7chen", "und", "Mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "F\u00fcrchten und Sehnen,", "tokens": ["F\u00fcrch\u00b7ten", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zweifeln und W\u00e4hnen,", "tokens": ["Zwei\u00b7feln", "und", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Armuth und F\u00fclle, Ver\u00f6dung und Pracht", "tokens": ["Ar\u00b7muth", "und", "F\u00fcl\u00b7le", ",", "Ve\u00b7r\u00f6\u00b7dung", "und", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Wechseln auf Erden, wie D\u00e4mmrung und Nacht!", "tokens": ["Wech\u00b7seln", "auf", "Er\u00b7den", ",", "wie", "D\u00e4mm\u00b7rung", "und", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PWAV", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Fruchtlos hienieden", "tokens": ["Frucht\u00b7los", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ringst du nach Frieden!", "tokens": ["Ringst", "du", "nach", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "T\u00e4uschende Schimmer", "tokens": ["T\u00e4u\u00b7schen\u00b7de", "Schim\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Winken dir immer;", "tokens": ["Win\u00b7ken", "dir", "im\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Doch, wie die Furchen des gleitenden Kahns,", "tokens": ["Doch", ",", "wie", "die", "Fur\u00b7chen", "des", "glei\u00b7ten\u00b7den", "Kahns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schwinden die Zaubergebilde des Wahns!", "tokens": ["Schwin\u00b7den", "die", "Zau\u00b7ber\u00b7ge\u00b7bil\u00b7de", "des", "Wahns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf zu der Sterne", "tokens": ["Auf", "zu", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Leuchtender Ferne", "tokens": ["Leuch\u00b7ten\u00b7der", "Fer\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Blicke vom Staube", "tokens": ["Bli\u00b7cke", "vom", "Stau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Muthig der Glaube:", "tokens": ["Mut\u00b7hig", "der", "Glau\u00b7be", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dort nur verkn\u00fcpft ein unsterbliches Band", "tokens": ["Dort", "nur", "ver\u00b7kn\u00fcpft", "ein", "uns\u00b7terb\u00b7li\u00b7ches", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Wahrheit und Frieden, Verein und Bestand!", "tokens": ["Wahr\u00b7heit", "und", "Frie\u00b7den", ",", "Ver\u00b7ein", "und", "Be\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "G\u00fcnstige Fluthen", "tokens": ["G\u00fcns\u00b7ti\u00b7ge", "Flut\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tragen die Guten,", "tokens": ["Tra\u00b7gen", "die", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "F\u00f6rdern die Braven", "tokens": ["F\u00f6r\u00b7dern", "die", "Bra\u00b7ven"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sicher zum Hafen,", "tokens": ["Si\u00b7cher", "zum", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und, ein harmonisch verklingendes Lied,", "tokens": ["Und", ",", "ein", "har\u00b7mo\u00b7nisch", "ver\u00b7klin\u00b7gen\u00b7des", "Lied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Schlie\u00dft sich das Leben dem edlen Gem\u00fcth!", "tokens": ["Schlie\u00dft", "sich", "das", "Le\u00b7ben", "dem", "ed\u00b7len", "Ge\u00b7m\u00fcth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4nnlich zu leiden,", "tokens": ["M\u00e4nn\u00b7lich", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kraftvoll zu meiden,", "tokens": ["Kraft\u00b7voll", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "K\u00fchn zu verachten,", "tokens": ["K\u00fchn", "zu", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bleib' unser Trachten!", "tokens": ["Bleib'", "un\u00b7ser", "Trach\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bleib' unser K\u00e4mpfen! in eherner Brust", "tokens": ["Bleib'", "un\u00b7ser", "K\u00e4mp\u00b7fen", "!", "in", "e\u00b7her\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Uns des unstr\u00e4flichen Willens bewu\u00dft!", "tokens": ["Uns", "des", "uns\u00b7tr\u00e4f\u00b7li\u00b7chen", "Wil\u00b7lens", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}