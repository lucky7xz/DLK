{"dta.poem.21360": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hochaufgestapelt im dritten Saal", "tokens": ["Hoch\u00b7auf\u00b7ge\u00b7sta\u00b7pelt", "im", "drit\u00b7ten", "Saal"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind Schwerter, Streit\u00e4xte, Speere,", "tokens": ["Sind", "Schwer\u00b7ter", ",", "Strei\u00b7t\u00e4x\u00b7te", ",", "Spee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Harnische, Helme, von Silber und Stahl,", "tokens": ["Har\u00b7ni\u00b7sche", ",", "Hel\u00b7me", ",", "von", "Sil\u00b7ber", "und", "Stahl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Altfr\u00e4nkische Feuergewehre.", "tokens": ["Alt\u00b7fr\u00e4n\u00b7ki\u00b7sche", "Feu\u00b7er\u00b7ge\u00b7weh\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sehr wenig Kanonen, jedoch genug", "tokens": ["Sehr", "we\u00b7nig", "Ka\u00b7no\u00b7nen", ",", "je\u00b7doch", "ge\u00b7nug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um eine Trophee zu bilden.", "tokens": ["Um", "ei\u00b7ne", "Tro\u00b7phee", "zu", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hoch ragt daraus eine Fahne hervor,", "tokens": ["Hoch", "ragt", "da\u00b7raus", "ei\u00b7ne", "Fah\u00b7ne", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Farbe ist schwarz-roth-g\u00fclden.", "tokens": ["Die", "Far\u00b7be", "ist", "schwa\u00b7rz\u00b7ro\u00b7th\u00b7g\u00fcl\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Kaiser bewohnt den vierten Saal.", "tokens": ["Der", "Kai\u00b7ser", "be\u00b7wohnt", "den", "vier\u00b7ten", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schon seit Jahrhunderten sitzt er", "tokens": ["Schon", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "sitzt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auf steinernem Stuhl, am steinernen Tisch,", "tokens": ["Auf", "stei\u00b7ner\u00b7nem", "Stuhl", ",", "am", "stei\u00b7ner\u00b7nen", "Tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Haupt auf den Armen st\u00fctzt er.", "tokens": ["Das", "Haupt", "auf", "den", "Ar\u00b7men", "st\u00fctzt", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sein Bart, der bis zur Erde wuchs,", "tokens": ["Sein", "Bart", ",", "der", "bis", "zur", "Er\u00b7de", "wuchs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist roth wie Feuerflammen,", "tokens": ["Ist", "roth", "wie", "Feu\u00b7er\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zuweilen zwinkert er mit dem Aug\u2019,", "tokens": ["Zu\u00b7wei\u00b7len", "zwin\u00b7kert", "er", "mit", "dem", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zieht manchmal die Braunen zusammen.", "tokens": ["Zieht", "manch\u00b7mal", "die", "Brau\u00b7nen", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}