{"dta.poem.8416": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An ein M\u00e4dchen,  \n das am Frohnleichnamsfest ein  \n Marienbild trug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Denk' ich, M\u00e4dchen, auch an dich;", "tokens": ["Denk'", "ich", ",", "M\u00e4d\u00b7chen", ",", "auch", "an", "dich", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die hellen Sehnsuchtstr\u00e4nen gleiten,", "tokens": ["Und", "die", "hel\u00b7len", "Sehn\u00b7sucht\u00b7str\u00e4\u00b7nen", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Seele w\u00f6lket sich.", "tokens": ["Und", "die", "See\u00b7le", "w\u00f6l\u00b7ket", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sittsam war dein Aug, voll M\u00e4dchenmilde,", "tokens": ["Sitt\u00b7sam", "war", "dein", "Aug", ",", "voll", "M\u00e4d\u00b7chen\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der die Andacht Reize lieh,", "tokens": ["Der", "die", "An\u00b7dacht", "Rei\u00b7ze", "lieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wich vom sch\u00f6nen Muttergottesbilde,", "tokens": ["Wich", "vom", "sch\u00f6\u00b7nen", "Mut\u00b7ter\u00b7got\u00b7tes\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wich vom Christuskinde nie.", "tokens": ["Wich", "vom", "Chris\u00b7tus\u00b7kin\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manche Z\u00e4hre floss von deinen Wangen,", "tokens": ["Man\u00b7che", "Z\u00e4h\u00b7re", "floss", "von", "dei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie der Thau von Rosen rinnt,", "tokens": ["Wie", "der", "Thau", "von", "Ro\u00b7sen", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blieb izt am Marienbilde hangen,", "tokens": ["Blieb", "izt", "am", "Ma\u00b7ri\u00b7en\u00b7bil\u00b7de", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Rann izt auf das Christuskind.", "tokens": ["Rann", "izt", "auf", "das", "Chris\u00b7tus\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine junge morgenrothbestreute", "tokens": ["Ei\u00b7ne", "jun\u00b7ge", "mor\u00b7gen\u00b7roth\u00b7be\u00b7streu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Silberblum' im Paradies", "tokens": ["Sil\u00b7ber\u00b7blum'", "im", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warst du, hehr, wie die Gebenedeite,", "tokens": ["Warst", "du", ",", "hehr", ",", "wie", "die", "Ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die dein Arm dem Volke wies!", "tokens": ["Die", "dein", "Arm", "dem", "Vol\u00b7ke", "wies", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bange Sehnsucht, banges s\u00fcsses Klopfen", "tokens": ["Ban\u00b7ge", "Sehn\u00b7sucht", ",", "ban\u00b7ges", "s\u00fcs\u00b7ses", "Klop\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schauerte durch meinen Geist.", "tokens": ["Schau\u00b7er\u00b7te", "durch", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kostet' ich des Stromes einen Tropfen,", "tokens": ["Kos\u00b7tet'", "ich", "des", "Stro\u00b7mes", "ei\u00b7nen", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der am Stule Gottes fleusst?", "tokens": ["Der", "am", "Stu\u00b7le", "Got\u00b7tes", "fleusst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trunken kniet' ich, wann der Reigen kniete,", "tokens": ["Trun\u00b7ken", "kniet'", "ich", ",", "wann", "der", "Rei\u00b7gen", "knie\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Betend, himmelan gef\u00fchrt,", "tokens": ["Be\u00b7tend", ",", "him\u00b7me\u00b7lan", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsste manche Knosp' und manche Bl\u00fcte,", "tokens": ["K\u00fcss\u00b7te", "man\u00b7che", "Knosp'", "und", "man\u00b7che", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die dein wallend Kleid ber\u00fchrt.", "tokens": ["Die", "dein", "wal\u00b7lend", "Kleid", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lebe, lebe deine Pilgertage,", "tokens": ["Le\u00b7be", ",", "le\u00b7be", "dei\u00b7ne", "Pil\u00b7ger\u00b7ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gutes M\u00e4dchen, flitterlos,", "tokens": ["Gu\u00b7tes", "M\u00e4d\u00b7chen", ",", "flit\u00b7ter\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann komm' ein Himmelsbot', und trage", "tokens": ["Und", "dann", "komm'", "ein", "Him\u00b7mels\u00b7bot'", ",", "und", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Seel' in Gottes Schooss!", "tokens": ["Dei\u00b7ne", "Seel'", "in", "Got\u00b7tes", "Scho\u00b7oss", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der Heiland l\u00e4chl' auf seinem Throne,", "tokens": ["Und", "der", "Hei\u00b7land", "l\u00e4chl'", "auf", "sei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann du dich dem Throne nahst;", "tokens": ["Wann", "du", "dich", "dem", "Thro\u00b7ne", "nahst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Maria bringe dir die Krone,", "tokens": ["Und", "Ma\u00b7ria", "brin\u00b7ge", "dir", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die du oft in Tr\u00e4umen sahst!", "tokens": ["Die", "du", "oft", "in", "Tr\u00e4u\u00b7men", "sahst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Gebe dir ein Lichtgewand! Vom Throne,", "tokens": ["Ge\u00b7be", "dir", "ein", "Licht\u00b7ge\u00b7wand", "!", "Vom", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Welten Richter thront,", "tokens": ["Wo", "der", "Wel\u00b7ten", "Rich\u00b7ter", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weh's her\u00fcber: Frommes M\u00e4dchen, wohne,", "tokens": ["Weh's", "her\u00b7\u00fc\u00b7ber", ":", "From\u00b7mes", "M\u00e4d\u00b7chen", ",", "woh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$.", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo die fromme Laura wohnt!", "tokens": ["Wo", "die", "from\u00b7me", "Lau\u00b7ra", "wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}