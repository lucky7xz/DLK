{"textgrid.poem.54100": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Frau spricht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja . . . da w\u00e4r nun also wieder einer . . .", "tokens": ["Ja", ".", ".", ".", "da", "w\u00e4r", "nun", "al\u00b7so", "wie\u00b7der", "ei\u00b7ner", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKANT", "$.", "$.", "$.", "ADV", "VAFIN", "ADV", "ADV", "ADV", "PIS", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "das ist komisch!", "tokens": ["das", "ist", "ko\u00b7misch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vor f\u00fcnf Jahren, da war meiner;", "tokens": ["Vor", "f\u00fcnf", "Jah\u00b7ren", ",", "da", "war", "mei\u00b7ner", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dann war eine ganze Weile keiner . . .", "tokens": ["dann", "war", "ei\u00b7ne", "gan\u00b7ze", "Wei\u00b7le", "kei\u00b7ner", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PIS", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und jetzt geht ein Mann in meiner Wohnung um,", "tokens": ["Und", "jetzt", "geht", "ein", "Mann", "in", "mei\u00b7ner", "Woh\u00b7nung", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "findet manches, was ich sage, dumm;", "tokens": ["fin\u00b7det", "man\u00b7ches", ",", "was", "ich", "sa\u00b7ge", ",", "dumm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "lobt und tadelt, spricht vom Daseinszwecke", "tokens": ["lobt", "und", "ta\u00b7delt", ",", "spricht", "vom", "Da\u00b7se\u00b7ins\u00b7zwe\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "und macht auf das Tischtuch Kaffeeflecke \u2013", "tokens": ["und", "macht", "auf", "das", "Tischtuch", "Kaf\u00b7fee\u00b7fle\u00b7cke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ist das alles n\u00f6tig \u2013?", "tokens": ["Ist", "das", "al\u00b7les", "n\u00f6\u00b7tig", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "PIS", "ADJD", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ja . . . er sorgt. Und liebt. Und ists ein tr\u00fcber", "tokens": ["Ja", ".", ".", ".", "er", "sorgt", ".", "Und", "liebt", ".", "Und", "ists", "ein", "tr\u00fc\u00b7ber"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "$.", "$.", "PPER", "VVFIN", "$.", "KON", "VVFIN", "$.", "KON", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Morgen, reich ich meine Hand hin\u00fcber . . .", "tokens": ["Mor\u00b7gen", ",", "reich", "ich", "mei\u00b7ne", "Hand", "hin\u00b7\u00fc\u00b7ber", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "PPOSAT", "NN", "ADV", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das ist komisch:", "tokens": ["Das", "ist", "ko\u00b7misch", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "M\u00e4nner . . . so in allen ihren Posen . . .", "tokens": ["M\u00e4n\u00b7ner", ".", ".", ".", "so", "in", "al\u00b7len", "ih\u00b7ren", "Po\u00b7sen", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und fr\u00fchmorgens, in den Unterhosen . . .", "tokens": ["Und", "fr\u00fch\u00b7mor\u00b7gens", ",", "in", "den", "Un\u00b7ter\u00b7ho\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Pl\u00f6tzlich wohnt da einer auch in meiner Seele.", "tokens": ["Pl\u00f6tz\u00b7lich", "wohnt", "da", "ei\u00b7ner", "auch", "in", "mei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Qu\u00e4lt mich; liebt mich; will, da\u00df ich ihn qu\u00e4le;", "tokens": ["Qu\u00e4lt", "mich", ";", "liebt", "mich", ";", "will", ",", "da\u00df", "ich", "ihn", "qu\u00e4\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "dreht mein Leben anders, lastet, l\u00e4\u00dft mich fliegen \u2013", "tokens": ["dreht", "mein", "Le\u00b7ben", "an\u00b7ders", ",", "las\u00b7tet", ",", "l\u00e4\u00dft", "mich", "flie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "siegt, und weil ich klug bin, la\u00df ich mich besiegen . . .", "tokens": ["siegt", ",", "und", "weil", "ich", "klug", "bin", ",", "la\u00df", "ich", "mich", "be\u00b7sie\u00b7gen", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "$,", "KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Habe ich das n\u00f6tig \u2013?", "tokens": ["Ha\u00b7be", "ich", "das", "n\u00f6\u00b7tig", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich war ausgeglichen. Bleiben wir allein,", "tokens": ["Ich", "war", "aus\u00b7ge\u00b7gli\u00b7chen", ".", "Blei\u00b7ben", "wir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": " . . . komisch . . .", "tokens": [".", ".", ".", "ko\u00b7misch", ".", ".", "."], "token_info": ["punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "ADJD", "$.", "$.", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "sind wir stolz. So sollt es immer sein!", "tokens": ["sind", "wir", "stolz", ".", "So", "sollt", "es", "im\u00b7mer", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "ADV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Flackerts aber, knistern kleine Flammen,", "tokens": ["Fla\u00b7ckerts", "a\u00b7ber", ",", "knis\u00b7tern", "klei\u00b7ne", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "f\u00e4llt das alles j\u00e4h in sich zusammen.", "tokens": ["f\u00e4llt", "das", "al\u00b7les", "j\u00e4h", "in", "sich", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Er braucht uns. Und wir, wir brauchen ihn.", "tokens": ["Er", "braucht", "uns", ".", "Und", "wir", ",", "wir", "brau\u00b7chen", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Liebe ist: Erf\u00fcllung, Last und Medizin.", "tokens": ["Lie\u00b7be", "ist", ":", "Er\u00b7f\u00fcl\u00b7lung", ",", "Last", "und", "Me\u00b7di\u00b7zin", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Denn ein Mann ist Mann und Gott und Kind,", "tokens": ["Denn", "ein", "Mann", "ist", "Mann", "und", "Gott", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "weil wir so sehr H\u00e4lfte sind.", "tokens": ["weil", "wir", "so", "sehr", "H\u00e4lf\u00b7te", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Aber das ist schlie\u00dflich \u00fcberall:", "tokens": ["A\u00b7ber", "das", "ist", "schlie\u00df\u00b7lich", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "der erste Mann ist stets ein Ungl\u00fccksfall.", "tokens": ["der", "ers\u00b7te", "Mann", "ist", "stets", "ein", "Un\u00b7gl\u00fccks\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die wahre Erkenntnis liegt unbestritten", "tokens": ["Die", "wah\u00b7re", "Er\u00b7kennt\u00b7nis", "liegt", "un\u00b7be\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "etwa zwischen dem zweiten und dem dritten.", "tokens": ["et\u00b7wa", "zwi\u00b7schen", "dem", "zwei\u00b7ten", "und", "dem", "drit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dann wei\u00dft du. Vom Wissen wird man nicht satt,", "tokens": ["Dann", "wei\u00dft", "du", ".", "Vom", "Wis\u00b7sen", "wird", "man", "nicht", "satt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "APPRART", "NN", "VAFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "aber notd\u00fcrftig zufrieden, mit dem, was man hat,", "tokens": ["a\u00b7ber", "not\u00b7d\u00fcrf\u00b7tig", "zu\u00b7frie\u00b7den", ",", "mit", "dem", ",", "was", "man", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "APPR", "ART", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}