{"textgrid.poem.40382": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Maschinenbauer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann Maschinbauer werden;", "tokens": ["Ich", "kann", "Ma\u00b7schin\u00b7bau\u00b7er", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da str\u00e4ubt sich manchem das Haar.", "tokens": ["da", "str\u00e4ubt", "sich", "man\u00b7chem", "das", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das ist viel toller als M\u00e4rchenspuk,", "tokens": ["Das", "ist", "viel", "tol\u00b7ler", "als", "M\u00e4r\u00b7chen\u00b7spuk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "da hausen wirklich wahr", "tokens": ["da", "hau\u00b7sen", "wirk\u00b7lich", "wahr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "tausend Zauberkr\u00e4fte.", "tokens": ["tau\u00b7send", "Zau\u00b7ber\u00b7kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die toben, wirbeln, krachen", "tokens": ["Die", "to\u00b7ben", ",", "wir\u00b7beln", ",", "kra\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit Kolben, Kurbeln, Gelenken,", "tokens": ["mit", "Kol\u00b7ben", ",", "Kur\u00b7beln", ",", "Ge\u00b7len\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "mit feuerschnaubenden Rachen,", "tokens": ["mit", "feu\u00b7er\u00b7schnau\u00b7ben\u00b7den", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "man mu\u00df an die H\u00f6lle denken,", "tokens": ["man", "mu\u00df", "an", "die", "H\u00f6l\u00b7le", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "an die gro\u00dfen Tiere der Urzeit.", "tokens": ["an", "die", "gro\u00b7\u00dfen", "Tie\u00b7re", "der", "Ur\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und sind viel st\u00e4rker als Riesen;", "tokens": ["Und", "sind", "viel", "st\u00e4r\u00b7ker", "als", "Rie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "was k\u00f6nnen sie alles tun!", "tokens": ["was", "k\u00f6n\u00b7nen", "sie", "al\u00b7les", "tun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bergwerke bohren, Dampfschiffe treiben,", "tokens": ["Berg\u00b7wer\u00b7ke", "boh\u00b7ren", ",", "Dampf\u00b7schif\u00b7fe", "trei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bahn brechen mit eisernen Schuhn;", "tokens": ["Bahn", "bre\u00b7chen", "mit", "ei\u00b7ser\u00b7nen", "Schuhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "++--+--+", "measure": "prosodiakos"}, "line.5": {"text": "weh dem, der ihnen zu nah tritt!", "tokens": ["weh", "dem", ",", "der", "ih\u00b7nen", "zu", "nah", "tritt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schnurstracks rei\u00dft Schwungrad und Riemen", "tokens": ["Schnur\u00b7stracks", "rei\u00dft", "Schwun\u00b7grad", "und", "Rie\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "die t\u00e4ppische Hand in Fetzen.", "tokens": ["die", "t\u00e4p\u00b7pi\u00b7sche", "Hand", "in", "Fet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mit solchen Unget\u00fcmen", "tokens": ["Mit", "sol\u00b7chen", "Un\u00b7ge\u00b7t\u00fc\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "auf guten Fu\u00df sich setzen", "tokens": ["auf", "gu\u00b7ten", "Fu\u00df", "sich", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "lernt nur ein kleiner Held.", "tokens": ["lernt", "nur", "ein", "klei\u00b7ner", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}