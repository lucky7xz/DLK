{"dta.poem.10171": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Eis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach nimm, o Mensch, des Sch\u00f6pfers Lieb und Macht,", "tokens": ["Ach", "nimm", ",", "o", "Mensch", ",", "des", "Sch\u00f6p\u00b7fers", "Lieb", "und", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "FM", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der iede Creatur, mehr als man glaubt, geniesset,", "tokens": ["Der", "ie\u00b7de", "Crea\u00b7tur", ",", "mehr", "als", "man", "glaubt", ",", "ge\u00b7nies\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ADV", "KOUS", "PIS", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am schroffen Eis\u2019 aufs neu in Acht!", "tokens": ["Am", "schrof\u00b7fen", "Eis'", "aufs", "neu", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJD", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da es, trotz seiner H\u00e4rtigkeit,", "tokens": ["Da", "es", ",", "trotz", "sei\u00b7ner", "H\u00e4r\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch von so seltener Beschaffenheit,", "tokens": ["Doch", "von", "so", "sel\u00b7te\u00b7ner", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df es gar leicht zerschmeltzt und schnell zerfliesset.", "tokens": ["Da\u00df", "es", "gar", "leicht", "zer\u00b7schmeltzt", "und", "schnell", "zer\u00b7flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn, da es Felsen-hart, es Felsen-gleich auch", "tokens": ["Wenn", ",", "da", "es", "Fel\u00b7sen\u00b7hart", ",", "es", "Fel\u00b7sen\u00b7gleich", "auch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "NN", "$,", "PPER", "NE", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und es die W\u00e4rme nicht so leicht zerst\u00f6hrte;", "tokens": ["Und", "es", "die", "W\u00e4r\u00b7me", "nicht", "so", "leicht", "zer\u00b7st\u00f6hr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welch Elend w\u00fcrde nicht im Wasser, auf der Erden,", "tokens": ["Welch", "E\u00b7lend", "w\u00fcr\u00b7de", "nicht", "im", "Was\u00b7ser", ",", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von ieder Creatur empfunden werden!", "tokens": ["Von", "ie\u00b7der", "Crea\u00b7tur", "emp\u00b7fun\u00b7den", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn ihr demnach, wie schnell das h\u00e4rtest\u2019 Eis zergehet,", "tokens": ["Wenn", "ihr", "dem\u00b7nach", ",", "wie", "schnell", "das", "h\u00e4r\u00b7test'", "Eis", "zer\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch des Zephirs Hauch zu Wasser wird, ersehet,", "tokens": ["Und", "durch", "des", "Ze\u00b7phirs", "Hauch", "zu", "Was\u00b7ser", "wird", ",", "er\u00b7se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch wie die H\u00e4rte sich so leicht entsteinet;", "tokens": ["Auch", "wie", "die", "H\u00e4r\u00b7te", "sich", "so", "leicht", "ent\u00b7stei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach so gedenckt dabey:", "tokens": ["Ach", "so", "ge\u00b7denckt", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df die\u00df, mehr als man leider meinet,", "tokens": ["Da\u00df", "die\u00df", ",", "mehr", "als", "man", "lei\u00b7der", "mei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "ADV", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wunder-Werck des weisen Sch\u00f6pfers sey.", "tokens": ["Ein", "Wun\u00b7der\u00b7\u00b7Werck", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}