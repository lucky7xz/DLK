{"textgrid.poem.40894": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Einzug", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat das Herz des Menschen ganz eigne L\u00e4nderkarten!", "tokens": ["Es", "hat", "das", "Herz", "des", "Men\u00b7schen", "ganz", "eig\u00b7ne", "L\u00e4n\u00b7der\u00b7kar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Stelle, wo ihm Liebes begegnet auf seinen Fahrten,", "tokens": ["Die", "Stel\u00b7le", ",", "wo", "ihm", "Lie\u00b7bes", "be\u00b7geg\u00b7net", "auf", "sei\u00b7nen", "Fahr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bezeichnet ihm schon ferne ein heitrer, heller Stern,", "tokens": ["Be\u00b7zeich\u00b7net", "ihm", "schon", "fer\u00b7ne", "ein", "hei\u00b7trer", ",", "hel\u00b7ler", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie ihn gesehn die Weisen einst ob der Krippe des Herrn.", "tokens": ["Wie", "ihn", "ge\u00b7sehn", "die", "Wei\u00b7sen", "einst", "ob", "der", "Krip\u00b7pe", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ART", "NN", "ADV", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie bist du, Stern, so funkelnd ob Augsburg mir zu schaun,", "tokens": ["Wie", "bist", "du", ",", "Stern", ",", "so", "fun\u00b7kelnd", "ob", "Augs\u00b7burg", "mir", "zu", "schaun", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "VVPP", "KOUS", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie Treu' im Blick der M\u00e4nner, wie Huld im Aug' der Fraun,", "tokens": ["Wie", "Treu'", "im", "Blick", "der", "M\u00e4n\u00b7ner", ",", "wie", "Huld", "im", "Aug'", "der", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ART", "NN", "$,", "PWAV", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wehm\u00fcthig Leuchten sendend den Tagen, die verglommen,", "tokens": ["Weh\u00b7m\u00fct\u00b7hig", "Leuch\u00b7ten", "sen\u00b7dend", "den", "Ta\u00b7gen", ",", "die", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein s\u00fc\u00df Verhei\u00dfen streuend auf Tage, die noch kommen!", "tokens": ["Ein", "s\u00fc\u00df", "Ver\u00b7hei\u00b7\u00dfen", "streu\u00b7end", "auf", "Ta\u00b7ge", ",", "die", "noch", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Max sprach's zum Kreis der Treuen, die mit ihm fr\u00f6hlich ritten,", "tokens": ["Max", "sprach's", "zum", "Kreis", "der", "Treu\u00b7en", ",", "die", "mit", "ihm", "fr\u00f6h\u00b7lich", "rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Lechfeld lag vor ihnen, die liebe Stadt inmitten.", "tokens": ["Das", "Lech\u00b7feld", "lag", "vor", "ih\u00b7nen", ",", "die", "lie\u00b7be", "Stadt", "in\u00b7mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbwas blinkt dort durchs Geh\u00f6lze, als ob's ein Lager w\u00e4re?", "tokens": ["\u00bb", "was", "blinkt", "dort", "durchs", "Ge\u00b7h\u00f6l\u00b7ze", ",", "als", "ob's", "ein", "La\u00b7ger", "w\u00e4\u00b7re", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "NE", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wohl gar der Egypterherzog mit seinem Zigeunerheere?\u00ab", "tokens": ["Wohl", "gar", "der", "E\u00b7gyp\u00b7ter\u00b7her\u00b7zog", "mit", "sei\u00b7nem", "Zi\u00b7geu\u00b7ner\u00b7hee\u00b7re", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NE", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Herr Kunze darauf erwidert: \u00bbWenn recht mein Auge sah,", "tokens": ["Herr", "Kun\u00b7ze", "da\u00b7rauf", "er\u00b7wi\u00b7dert", ":", "\u00bb", "Wenn", "recht", "mein", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PAV", "VVPP", "$.", "$(", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wohl lagert Herzog Amors Zigeunerv\u00f6lklein da;", "tokens": ["Wohl", "la\u00b7gert", "Her\u00b7zog", "A\u00b7mors", "Zi\u00b7geu\u00b7ner\u00b7v\u00f6l\u00b7klein", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch scheint's nicht fest im Wandern, die F\u00fc\u00dfchen sind schon wund,", "tokens": ["Doch", "scheint's", "nicht", "fest", "im", "Wan\u00b7dern", ",", "die", "F\u00fc\u00df\u00b7chen", "sind", "schon", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was Wunder? ", "tokens": ["Was", "Wun\u00b7der", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "O seht das seltne Lager! Die Lanzen sind Nadelspitzen,", "tokens": ["O", "seht", "das", "selt\u00b7ne", "La\u00b7ger", "!", "Die", "Lan\u00b7zen", "sind", "Na\u00b7del\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als Schilder, geh\u00e4ngt an B\u00e4ume, rings Spiegel und Spiegelchen blitzen,", "tokens": ["Als", "Schil\u00b7der", ",", "ge\u00b7h\u00e4ngt", "an", "B\u00e4u\u00b7me", ",", "rings", "Spie\u00b7gel", "und", "Spie\u00b7gel\u00b7chen", "blit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Viel Pfeile in braunen, blauen und schwarzen K\u00f6chern der Augen,", "tokens": ["Viel", "Pfei\u00b7le", "in", "brau\u00b7nen", ",", "blau\u00b7en", "und", "schwar\u00b7zen", "K\u00f6\u00b7chern", "der", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "$,", "ADJA", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als grob und leicht Gesch\u00fctze die Zungen und Z\u00fcngelchen taugen!", "tokens": ["Als", "grob", "und", "leicht", "Ge\u00b7sch\u00fct\u00b7ze", "die", "Zun\u00b7gen", "und", "Z\u00fcn\u00b7gel\u00b7chen", "tau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und hat das Herz des Menschen ganz eigne L\u00e4nderkarten,", "tokens": ["Und", "hat", "das", "Herz", "des", "Men\u00b7schen", "ganz", "eig\u00b7ne", "L\u00e4n\u00b7der\u00b7kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mu\u00dft' ihnen zum Kometen dein heller Stern entarten", "tokens": ["Mu\u00dft'", "ih\u00b7nen", "zum", "Ko\u00b7me\u00b7ten", "dein", "hel\u00b7ler", "Stern", "ent\u00b7ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als des Profo\u00dfen Ruthe, im Zorn ob Augsburg lohend,", "tokens": ["Als", "des", "Pro\u00b7fo\u00b7\u00dfen", "Ru\u00b7the", ",", "im", "Zorn", "ob", "Augs\u00b7burg", "lo\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$,", "APPRART", "NN", "KOUS", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Uns\u00fc\u00dfen Abschieds mahnend und b\u00f6se R\u00fcckkehr drohend!\u00ab", "tokens": ["Un\u00b7s\u00fc\u00b7\u00dfen", "Ab\u00b7schieds", "mah\u00b7nend", "und", "b\u00f6\u00b7se", "R\u00fcck\u00b7kehr", "dro\u00b7hend", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Da fa\u00dft der Fr\u00e4ulein eines des Kaisers Z\u00fcgel leise:", "tokens": ["Da", "fa\u00dft", "der", "Fr\u00e4u\u00b7lein", "ei\u00b7nes", "des", "Kai\u00b7sers", "Z\u00fc\u00b7gel", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbgestatt' in deinem Schutze, Herr, uns die Heimatreise,", "tokens": ["\u00bb", "ge\u00b7statt'", "in", "dei\u00b7nem", "Schut\u00b7ze", ",", "Herr", ",", "uns", "die", "Hei\u00b7mat\u00b7rei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Heimf\u00fchre die T\u00f6chter wieder dem weisen Magistrat,", "tokens": ["Heim\u00b7f\u00fch\u00b7re", "die", "T\u00f6ch\u00b7ter", "wie\u00b7der", "dem", "wei\u00b7sen", "Ma\u00b7gist\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Schwestern seinen S\u00f6hnen, die Kinder der Vaterstadt!\u00ab", "tokens": ["Die", "Schwes\u00b7tern", "sei\u00b7nen", "S\u00f6h\u00b7nen", ",", "die", "Kin\u00b7der", "der", "Va\u00b7ter\u00b7stadt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Da klammerten sich die M\u00e4gdlein an B\u00fcgel ihm und Zaum,", "tokens": ["Da", "klam\u00b7mer\u00b7ten", "sich", "die", "M\u00e4gd\u00b7lein", "an", "B\u00fc\u00b7gel", "ihm", "und", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An M\u00e4hn' und Schweif des Rosses und an des Mantels Saum.", "tokens": ["An", "M\u00e4hn'", "und", "Schweif", "des", "Ros\u00b7ses", "und", "an", "des", "Man\u00b7tels", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Kaiser l\u00e4\u00dft's geschehen, er denkt nur still bei sich:", "tokens": ["Der", "Kai\u00b7ser", "l\u00e4\u00dft's", "ge\u00b7sche\u00b7hen", ",", "er", "denkt", "nur", "still", "bei", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "PRF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Euch wird mein Purpur sch\u00fctzen, mein graues Haar sch\u00fctzt mich!", "tokens": ["Euch", "wird", "mein", "Pur\u00b7pur", "sch\u00fct\u00b7zen", ",", "mein", "grau\u00b7es", "Haar", "sch\u00fctzt", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "So ritt der Zug von dannen. Herr Kunz ritt hinterdrein", "tokens": ["So", "ritt", "der", "Zug", "von", "dan\u00b7nen", ".", "Herr", "Kunz", "ritt", "hin\u00b7ter\u00b7drein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$.", "NN", "NE", "VVFIN", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und trieb ein buntes Denken, zu laut fast mocht es sein:", "tokens": ["Und", "trieb", "ein", "bun\u00b7tes", "Den\u00b7ken", ",", "zu", "laut", "fast", "mocht", "es", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "PTKA", "ADJD", "ADV", "VVFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbo Max, du seltner J\u00e4ger! Sieh, was sich f\u00fcr V\u00f6glein fingen,", "tokens": ["\u00bb", "o", "Max", ",", "du", "selt\u00b7ner", "J\u00e4\u00b7ger", "!", "Sieh", ",", "was", "sich", "f\u00fcr", "V\u00f6\u00b7glein", "fin\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "PPER", "ADJA", "NN", "$.", "NE", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Dir, lustig zappelnd und flatternd, in Garn und Ro\u00dfhaarschlingen!", "tokens": ["Dir", ",", "lus\u00b7tig", "zap\u00b7pelnd", "und", "flat\u00b7ternd", ",", "in", "Garn", "und", "Ro\u00df\u00b7haar\u00b7schlin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "VVPP", "KON", "VVPP", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "O Max, du seltner G\u00e4rtner! Schm\u00fcckst du zum Rosenturnei", "tokens": ["O", "Max", ",", "du", "selt\u00b7ner", "G\u00e4rt\u00b7ner", "!", "Schm\u00fcckst", "du", "zum", "Ro\u00b7sen\u00b7tur\u00b7nei"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "ADJA", "NN", "$.