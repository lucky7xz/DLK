{"textgrid.poem.43922": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich untersteh mich, dir, galant- und treues Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich untersteh mich, dir, galant- und treues Kind,", "tokens": ["Ich", "un\u00b7ter\u00b7steh", "mich", ",", "dir", ",", "ga\u00b7lant", "und", "treu\u00b7es", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein schlecht geseztes Lied verwegen darzureichen;", "tokens": ["Ein", "schlecht", "ge\u00b7sez\u00b7tes", "Lied", "ver\u00b7we\u00b7gen", "dar\u00b7zu\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch weil dein Schlu\u00df und Wort sein sch\u00f6nster Inhalt sind,", "tokens": ["Doch", "weil", "dein", "Schlu\u00df", "und", "Wort", "sein", "sch\u00f6ns\u00b7ter", "In\u00b7halt", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird ein holder Blick auf de\u00dfen Zeilen streichen.", "tokens": ["So", "wird", "ein", "hol\u00b7der", "Blick", "auf", "de\u00b7\u00dfen", "Zei\u00b7len", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Die Dinte scheint sehr bleich. Was macht es? Sie erschrickt,", "tokens": ["Die", "Din\u00b7te", "scheint", "sehr", "bleich", ".", "Was", "macht", "es", "?", "Sie", "er\u00b7schrickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$.", "PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit solcher Klimperey dein Auge zu beschweren;", "tokens": ["Mit", "sol\u00b7cher", "Klim\u00b7pe\u00b7rey", "dein", "Au\u00b7ge", "zu", "be\u00b7schwe\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Auge, de\u00dfen Strahl so scharf als Sonnen blickt,", "tokens": ["Dein", "Au\u00b7ge", ",", "de\u00b7\u00dfen", "Strahl", "so", "scharf", "als", "Son\u00b7nen", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "ADV", "ADJD", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan leicht den fahlen Saft wie mich in Brand verkehren.", "tokens": ["Kan", "leicht", "den", "fah\u00b7len", "Saft", "wie", "mich", "in", "Brand", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "ADJA", "NN", "KOKOM", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ist auch dies Papier in etwas schmal und klein,", "tokens": ["Und", "ist", "auch", "dies", "Pa\u00b7pier", "in", "et\u00b7was", "schmal", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PDS", "NN", "APPR", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So scheint mir doch der Stern von deiner Sanftmuth gr\u00f6\u00dfer;", "tokens": ["So", "scheint", "mir", "doch", "der", "Stern", "von", "dei\u00b7ner", "Sanft\u00b7muth", "gr\u00f6\u00b7\u00dfer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Daher versprech ich mir ein g\u00fctiges Verzeihn,", "tokens": ["Da\u00b7her", "ver\u00b7sprech", "ich", "mir", "ein", "g\u00fc\u00b7ti\u00b7ges", "Ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum tadle nicht die Schrift, mein Herz ist desto be\u00dfer.", "tokens": ["Drum", "tad\u00b7le", "nicht", "die", "Schrift", ",", "mein", "Herz", "ist", "des\u00b7to", "be\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}}}}