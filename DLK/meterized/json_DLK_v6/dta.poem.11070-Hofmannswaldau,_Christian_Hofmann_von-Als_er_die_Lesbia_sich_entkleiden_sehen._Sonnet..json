{"dta.poem.11070": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er die Lesbia sich entkleiden sehen.  \n  Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die saubre Lesbia sa\u00df mit geschrenckten f\u00fcssen,", "tokens": ["Die", "saub\u00b7re", "Les\u00b7bia", "sa\u00df", "mit", "ge\u00b7schrenck\u00b7ten", "f\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jhr netter finger war um schuh und str\u00fcmpff bem\u00fcht.", "tokens": ["Ihr", "net\u00b7ter", "fin\u00b7ger", "war", "um", "schuh", "und", "str\u00fcmpff", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier konnt\u2019 ich, was sie doch sonst jedem aug entzieht,", "tokens": ["Hier", "konnt'", "ich", ",", "was", "sie", "doch", "sonst", "je\u00b7dem", "aug", "ent\u00b7zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch einen k\u00fchnen blick in stiller lust geniessen.", "tokens": ["Durch", "ei\u00b7nen", "k\u00fch\u00b7nen", "blick", "in", "stil\u00b7ler", "lust", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die seide hatte kaum dem marmel weichen m\u00fcssen,", "tokens": ["Die", "sei\u00b7de", "hat\u00b7te", "kaum", "dem", "mar\u00b7mel", "wei\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als sich der leichte rock von ihrem leibe schied;", "tokens": ["Als", "sich", "der", "leich\u00b7te", "rock", "von", "ih\u00b7rem", "lei\u00b7be", "schied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch als die sichre hand die weisse brust verrieth,", "tokens": ["Doch", "als", "die", "sich\u00b7re", "hand", "die", "weis\u00b7se", "brust", "ver\u00b7rieth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ward ich unvermerckt in ohnmacht hingerissen:", "tokens": ["So", "ward", "ich", "un\u00b7ver\u00b7merckt", "in", "ohn\u00b7macht", "hin\u00b7ge\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die augen suncken hin, die beine wurden matt.", "tokens": ["Die", "au\u00b7gen", "sun\u00b7cken", "hin", ",", "die", "bei\u00b7ne", "wur\u00b7den", "matt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die nackte Silvia stieg sicher in das bad,", "tokens": ["Die", "nack\u00b7te", "Sil\u00b7via", "stieg", "si\u00b7cher", "in", "das", "bad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Eh ich mein auge konnt\u2019 aus der verwirrung f\u00fchren.", "tokens": ["Eh", "ich", "mein", "au\u00b7ge", "konnt'", "aus", "der", "ver\u00b7wir\u00b7rung", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und also ward sie mir kein gantz entdecktes land.", "tokens": ["Und", "al\u00b7so", "ward", "sie", "mir", "kein", "gantz", "ent\u00b7deck\u00b7tes", "land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "PIAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch hat die blosse brust mir so viel krafft entwandt;", "tokens": ["Doch", "hat", "die", "blos\u00b7se", "brust", "mir", "so", "viel", "krafft", "ent\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Was w\u00fcrde Silvius nicht in der schos verliehren.", "tokens": ["Was", "w\u00fcr\u00b7de", "Sil\u00b7vius", "nicht", "in", "der", "schos", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "PTKNEG", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}