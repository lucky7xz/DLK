ord", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Oft zum Virgil, noch \u00f6fter zum M\u00e4cen.", "tokens": ["Oft", "zum", "Vir\u00b7gil", ",", "noch", "\u00f6f\u00b7ter", "zum", "M\u00e4\u00b7cen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Sein Ha\u00df entehrt. Warum? Weil Seine Gunst", "tokens": ["Sein", "Ha\u00df", "ent\u00b7ehrt", ".", "Wa\u00b7rum", "?", "Weil", "Sei\u00b7ne", "Gunst"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "PWAV", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kaum weniger, als mancher Pfalzgraf, adelt.", "tokens": ["Kaum", "we\u00b7ni\u00b7ger", ",", "als", "man\u00b7cher", "Pfalz\u00b7graf", ",", "a\u00b7delt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Er versteht, wie meisterliche Kunst", "tokens": ["Nur", "Er", "ver\u00b7steht", ",", "wie", "meis\u00b7ter\u00b7li\u00b7che", "Kunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Zeilen lobt, in ganzen Bl\u00e4ttern tadelt.", "tokens": ["In", "Zei\u00b7len", "lobt", ",", "in", "gan\u00b7zen", "Bl\u00e4t\u00b7tern", "ta\u00b7delt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein Ausspruch nur, der stets die Regel trifft,", "tokens": ["Sein", "Aus\u00b7spruch", "nur", ",", "der", "stets", "die", "Re\u00b7gel", "trifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Entscheidet schnell den Werth von jeder Schrift.", "tokens": ["Ent\u00b7schei\u00b7det", "schnell", "den", "Werth", "von", "je\u00b7der", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Die Ungeduld der Fremden, Ihn zu schaun,", "tokens": ["Die", "Un\u00b7ge\u00b7duld", "der", "Frem\u00b7den", ",", "Ihn", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Spornt ihren Fu\u00df auf den gelehrten Reisen.", "tokens": ["Spornt", "ih\u00b7ren", "Fu\u00df", "auf", "den", "ge\u00b7lehr\u00b7ten", "Rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie m\u00fcssen sich aus Seinem Mund erbaun,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "sich", "aus", "Sei\u00b7nem", "Mund", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Ihm, Ihm selbst, sich und ihr Stammbuch weisen,", "tokens": ["Und", "Ihm", ",", "Ihm", "selbst", ",", "sich", "und", "ihr", "Stamm\u00b7buch", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "ADV", "$,", "PRF", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergleichen Ihn mit Seinem Kupferstich,", "tokens": ["Ver\u00b7glei\u00b7chen", "Ihn", "mit", "Sei\u00b7nem", "Kup\u00b7fer\u00b7stich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sehn, wie Er lacht, freun und empfehlen sich.", "tokens": ["Sehn", ",", "wie", "Er", "lacht", ",", "freun", "und", "emp\u00b7feh\u00b7len", "sich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF", "KON", "VVFIN", "PRF", "$."], "meter": "+--+++-+-+", "measure": "dactylic.init"}}, "stanza.15": {"line.1": {"text": "Er lehrt die Welt. Sein Ton, Sein Vorrang steigt,", "tokens": ["Er", "lehrt", "die", "Welt", ".", "Sein", "Ton", ",", "Sein", "Vor\u00b7rang", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Seine Stirn umstrahlt der Glanz der Ehre.", "tokens": ["Und", "Sei\u00b7ne", "Stirn", "um\u00b7strahlt", "der", "Glanz", "der", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das, was Er sagt, und das, was Er verschweigt,", "tokens": ["Das", ",", "was", "Er", "sagt", ",", "und", "das", ",", "was", "Er", "ver\u00b7schweigt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist, wie ein Licht und Nebel Seiner Lehre,", "tokens": ["Ist", ",", "wie", "ein", "Licht", "und", "Ne\u00b7bel", "Sei\u00b7ner", "Leh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das, wann Er will, der Schl\u00fcsse Band entdeckt,", "tokens": ["Das", ",", "wann", "Er", "will", ",", "der", "Schl\u00fcs\u00b7se", "Band", "ent\u00b7deckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der, wann Er mu\u00df, des Bandes Grund versteckt.", "tokens": ["Der", ",", "wann", "Er", "mu\u00df", ",", "des", "Ban\u00b7des", "Grund", "ver\u00b7steckt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.16": {"line.1": {"text": "Der K\u00f6rper Stoff, was ihre Kraft erh\u00e4lt,", "tokens": ["Der", "K\u00f6r\u00b7per", "Stoff", ",", "was", "ih\u00b7re", "Kraft", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie jede wirkt, sieht Er von allen Seiten.", "tokens": ["Wie", "je\u00b7de", "wirkt", ",", "sieht", "Er", "von", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Witz durchstreift sogar die Geisterwelt,", "tokens": ["Sein", "Witz", "durch\u00b7streift", "so\u00b7gar", "die", "Geis\u00b7ter\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das dunkle Land entlegner M\u00f6glichkeiten,", "tokens": ["Das", "dunk\u00b7le", "Land", "ent\u00b7leg\u00b7ner", "M\u00f6g\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sp\u00e4het dort mehr Dinge seltner Art,", "tokens": ["Und", "sp\u00e4\u00b7het", "dort", "mehr", "Din\u00b7ge", "selt\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als ein Uly\u00df bei seiner H\u00f6llenfahrt.", "tokens": ["Als", "ein", "U\u00b7ly\u00df", "bei", "sei\u00b7ner", "H\u00f6l\u00b7len\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.17": {"line.1": {"text": "Der Wahrheit Reich macht Er sich unterthan.", "tokens": ["Der", "Wahr\u00b7heit", "Reich", "macht", "Er", "sich", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er herrscht allein, mit sieggewohnten Sch\u00e4tzen.", "tokens": ["Er", "herrscht", "al\u00b7lein", ",", "mit", "sieg\u00b7ge\u00b7wohn\u00b7ten", "Sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Emp\u00f6ret sich des Zweiflers kecker Wahn,", "tokens": ["Em\u00b7p\u00f6\u00b7ret", "sich", "des", "Zweif\u00b7lers", "ke\u00b7cker", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So kann doch das Sein Ansehn nicht verletzt.", "tokens": ["So", "kann", "doch", "das", "Sein", "An\u00b7sehn", "nicht", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umsonst erregt ein Aeol Sturm und Flut:", "tokens": ["Um\u00b7sonst", "er\u00b7regt", "ein", "A\u00b7e\u00b7ol", "Sturm", "und", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Neptun erscheint, 