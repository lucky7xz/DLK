{"textgrid.poem.41244": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gelehrte", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt ist der, zu dem sein Vater spricht:", "tokens": ["Be\u00b7gl\u00fcckt", "ist", "der", ",", "zu", "dem", "sein", "Va\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sohn, sei gelehrt! und der den Vater h\u00f6ret,", "tokens": ["Sohn", ",", "sei", "ge\u00b7lehrt", "!", "und", "der", "den", "Va\u00b7ter", "h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "VVPP", "$.", "KON", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, nur auf Ruhm, auf Meisterschaft erpicht,", "tokens": ["Und", ",", "nur", "auf", "Ruhm", ",", "auf", "Meis\u00b7ter\u00b7schaft", "er\u00b7picht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald vieles lernt, und endlich alles lehret,", "tokens": ["Bald", "vie\u00b7les", "lernt", ",", "und", "end\u00b7lich", "al\u00b7les", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "KON", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit gleichem Muth bejahet und verneint,", "tokens": ["Mit", "glei\u00b7chem", "Muth", "be\u00b7ja\u00b7het", "und", "ver\u00b7neint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Beweisen darf, und zu beweisen scheint.", "tokens": ["Be\u00b7wei\u00b7sen", "darf", ",", "und", "zu", "be\u00b7wei\u00b7sen", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "KON", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein Ernst verschm\u00e4ht, was H\u00f6fen stets gefiel:", "tokens": ["Sein", "Ernst", "ver\u00b7schm\u00e4ht", ",", "was", "H\u00f6\u00b7fen", "stets", "ge\u00b7fiel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PWS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Ueberflu\u00df geschm\u00fcckter Freudenfeste,", "tokens": ["Den", "Ue\u00b7berf\u00b7lu\u00df", "ge\u00b7schm\u00fcck\u00b7ter", "Freu\u00b7den\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die fr\u00fche Jagd, den sp\u00e4ten Tanz, das Spiel,", "tokens": ["Die", "fr\u00fc\u00b7he", "Jagd", ",", "den", "sp\u00e4\u00b7ten", "Tanz", ",", "das", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Nachtgepr\u00e4ng' erleuchteter Pal\u00e4ste,", "tokens": ["Das", "Nacht\u00b7ge\u00b7pr\u00e4ng'", "er\u00b7leuch\u00b7te\u00b7ter", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Masken Scherz, wo Mummerei und List", "tokens": ["Der", "Mas\u00b7ken", "Scherz", ",", "wo", "Mum\u00b7me\u00b7rei", "und", "List"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verliebte paart, Gepaarten g\u00fcnstig ist.", "tokens": ["Ver\u00b7lieb\u00b7te", "paart", ",", "Ge\u00b7paar\u00b7ten", "g\u00fcns\u00b7tig", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihn reizen nie der Waffen Glanz und Pracht,", "tokens": ["Ihn", "rei\u00b7zen", "nie", "der", "Waf\u00b7fen", "Glanz", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Edlen Muth, der Enkel tapfrer Ahnen,", "tokens": ["Der", "Ed\u00b7len", "Muth", ",", "der", "En\u00b7kel", "tapf\u00b7rer", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Helden Lust, die feuervolle Schlacht,", "tokens": ["Der", "Hel\u00b7den", "Lust", ",", "die", "feu\u00b7er\u00b7vol\u00b7le", "Schlacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der stolze Sieg, der Ruhm erfocht'ner Fahnen.", "tokens": ["Der", "stol\u00b7ze", "Sieg", ",", "der", "Ruhm", "er\u00b7focht'\u00b7ner", "Fah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Kriegsgeschrei, das donnernde Metall,", "tokens": ["Das", "Kriegs\u00b7ge\u00b7schrei", ",", "das", "don\u00b7nern\u00b7de", "Me\u00b7tall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der k\u00fchne Sturm, und der erstiegne Wall.", "tokens": ["Der", "k\u00fch\u00b7ne", "Sturm", ",", "und", "der", "er\u00b7sti\u00b7eg\u00b7ne", "Wall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Er mehrt auch nicht den zu geheimen Rath,", "tokens": ["Er", "mehrt", "auch", "nicht", "den", "zu", "ge\u00b7hei\u00b7men", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der um den Thron erhabner F\u00fcrsten sitzet,", "tokens": ["Der", "um", "den", "Thron", "er\u00b7hab\u00b7ner", "F\u00fcrs\u00b7ten", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, sonder Ihn, den anvertrauten Staat", "tokens": ["Und", ",", "son\u00b7der", "Ihn", ",", "den", "an\u00b7ver\u00b7trau\u00b7ten", "Staat"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KON", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bewacht, versorgt, erweitert und besch\u00fctzet.", "tokens": ["Be\u00b7wacht", ",", "ver\u00b7sorgt", ",", "er\u00b7wei\u00b7tert", "und", "be\u00b7sch\u00fct\u00b7zet", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er will, Er kann (wie oft trifft beides ein!)", "tokens": ["Er", "will", ",", "Er", "kann", "(", "wie", "oft", "trifft", "bei\u00b7des", "ein", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$(", "KOKOM", "ADV", "VVFIN", "PIS", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Cineas von einem Pyrrhus sein.", "tokens": ["Kein", "Ci\u00b7ne\u00b7as", "von", "ei\u00b7nem", "Pyrr\u00b7hus", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was Ihn bem\u00fcht, verherrlicht und erg\u00f6tzt,", "tokens": ["Was", "Ihn", "be\u00b7m\u00fcht", ",", "ver\u00b7herr\u00b7licht", "und", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind weder Pracht, noch Kriegs- und Staatsgesch\u00e4fte:", "tokens": ["Sind", "we\u00b7der", "Pracht", ",", "noch", "Kriegs", "und", "Staats\u00b7ge\u00b7sch\u00e4f\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "$,", "ADV", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist ein Buch, das er selbst aufgesetzt,", "tokens": ["Es", "ist", "ein", "Buch", ",", "das", "er", "selbst", "auf\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es ist ein Schatz von Ihm beschriebner Hefte,", "tokens": ["Es", "ist", "ein", "Schatz", "von", "Ihm", "be\u00b7schrieb\u00b7ner", "Hef\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Kupferstich, der Ihn, mit Recht, entz\u00fcckt,", "tokens": ["Ein", "Kup\u00b7fer\u00b7stich", ",", "der", "Ihn", ",", "mit", "Recht", ",", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In dem Er Sich, mit Ruhm verbr\u00e4mt, erblickt.", "tokens": ["In", "dem", "Er", "Sich", ",", "mit", "Ruhm", "ver\u00b7br\u00e4mt", ",", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "$,", "APPR", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es ist Sein Krieg ein schwerer Federkrieg,", "tokens": ["Es", "ist", "Sein", "Krieg", "ein", "schwe\u00b7rer", "Fe\u00b7der\u00b7krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dem durch Ihn Beweise stehn und fallen;", "tokens": ["In", "dem", "durch", "Ihn", "Be\u00b7wei\u00b7se", "stehn", "und", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Er betritt, auf den erhaltnen Sieg,", "tokens": ["Und", "Er", "be\u00b7tritt", ",", "auf", "den", "er\u00b7halt\u00b7nen", "Sieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Helden gleich, des Ehrentempels Hallen,", "tokens": ["Den", "Hel\u00b7den", "gleich", ",", "des", "Eh\u00b7ren\u00b7tem\u00b7pels", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und stellet dort Sich Seiner Leser Schaar,", "tokens": ["Und", "stel\u00b7let", "dort", "Sich", "Sei\u00b7ner", "Le\u00b7ser", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Setzerzunft, und den Verlegern dar.", "tokens": ["Der", "Set\u00b7zer\u00b7zunft", ",", "und", "den", "Ver\u00b7le\u00b7gern", "dar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ja! dreifach gro\u00df und furchtbar ist der Mann,", "tokens": ["Ja", "!", "drei\u00b7fach", "gro\u00df", "und", "furcht\u00b7bar", "ist", "der", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der muthig schreibt, bis Neid und Gegner schwinden.", "tokens": ["Der", "mut\u00b7hig", "schreibt", ",", "bis", "Neid", "und", "Geg\u00b7ner", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er trifft in Sich mehr, als neun Musen, an,", "tokens": ["Er", "trifft", "in", "Sich", "mehr", ",", "als", "neun", "Mu\u00b7sen", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "$,", "KOUS", "CARD", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er wird in Sich mehr, als den Ph\u00f6bus, finden,", "tokens": ["Er", "wird", "in", "Sich", "mehr", ",", "als", "den", "Ph\u00f6\u00b7bus", ",", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ist im Streit, wie Ajax beim Homer,", "tokens": ["Und", "ist", "im", "Streit", ",", "wie", "A\u00b7jax", "beim", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "$,", "PWAV", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des Heeres Schutz, ja selbst ein ganzes Heer.", "tokens": ["Des", "Hee\u00b7res", "Schutz", ",", "ja", "selbst", "ein", "gan\u00b7zes", "Heer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Erw\u00fcnschter Preis gelehrter Ritterschaft!", "tokens": ["Er\u00b7w\u00fcnschter", "Preis", "ge\u00b7lehr\u00b7ter", "Rit\u00b7ter\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dein Lorbeer kr\u00f6nt den, so der Muth erhoben:", "tokens": ["Dein", "Lor\u00b7beer", "kr\u00f6nt", "den", ",", "so", "der", "Muth", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch braucht auch der nicht stets der Waffen Kraft;", "tokens": ["Doch", "braucht", "auch", "der", "nicht", "stets", "der", "Waf\u00b7fen", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er lobet auch, damit ihn andre loben,", "tokens": ["Er", "lo\u00b7bet", "auch", ",", "da\u00b7mit", "ihn", "and\u00b7re", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und lohnt dem Ruhm, den er im Lenz erh\u00e4lt,", "tokens": ["Und", "lohnt", "dem", "Ruhm", ",", "den", "er", "im", "Lenz", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Gegenruhm, noch eh' die Bl\u00fcte f\u00e4llt.", "tokens": ["Mit", "Ge\u00b7gen\u00b7ruhm", ",", "noch", "eh'", "die", "Bl\u00fc\u00b7te", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es keimt und spro\u00dft die Saat der Dankbarkeit", "tokens": ["Es", "keimt", "und", "spro\u00dft", "die", "Saat", "der", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Zeitungen, und w\u00e4chst in Monatsschriften.", "tokens": ["In", "Zei\u00b7tun\u00b7gen", ",", "und", "w\u00e4chst", "in", "Mo\u00b7nats\u00b7schrif\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein werther Freund belehrt die Folgezeit,", "tokens": ["Ein", "wert\u00b7her", "Freund", "be\u00b7lehrt", "die", "Fol\u00b7ge\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zeigt uns selbst, wie viel wir Gutes stiften,", "tokens": ["Und", "zeigt", "uns", "selbst", ",", "wie", "viel", "wir", "Gu\u00b7tes", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dich ermahnt sein s\u00fc\u00dfes Lobgedicht,", "tokens": ["Und", "dich", "er\u00b7mahnt", "sein", "s\u00fc\u00b7\u00dfes", "Lob\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Germanien! zu der Bewundrungspflicht.", "tokens": ["Ger\u00b7ma\u00b7ni\u00b7en", "!", "zu", "der", "Be\u00b7wund\u00b7rungs\u00b7pflicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Oft ist der Ruhm, der Schriftverfasser hebt,", "tokens": ["Oft", "ist", "der", "Ruhm", ",", "der", "Schrift\u00b7ver\u00b7fas\u00b7ser", "hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Urspr\u00fcnglich schwach; doch hilft die Gunst ihm weiter.", "tokens": ["Ur\u00b7spr\u00fcng\u00b7lich", "schwach", ";", "doch", "hilft", "die", "Gunst", "ihm", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der G\u00f6nner Huld, nach der die Zuschrift strebt,", "tokens": ["Der", "G\u00f6n\u00b7ner", "Huld", ",", "nach", "der", "die", "Zu\u00b7schrift", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Macht Kleine gro\u00df, und dunkle Namen heiter,", "tokens": ["Macht", "Klei\u00b7ne", "gro\u00df", ",", "und", "dunk\u00b7le", "Na\u00b7men", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJD", "$,", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wer zuerst um Nachsicht bitten mu\u00df,", "tokens": ["Und", "wer", "zu\u00b7erst", "um", "Nach\u00b7sicht", "bit\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gebeut zuletzt, und ist ein Pansophus.", "tokens": ["Ge\u00b7beut", "zu\u00b7letzt", ",", "und", "ist", "ein", "Pan\u00b7so\u00b7phus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So wie ein Bach, der tr\u00e4g' und d\u00fcrftig quillt,", "tokens": ["So", "wie", "ein", "Bach", ",", "der", "tr\u00e4g'", "und", "d\u00fcrf\u00b7tig", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Kies und Schlamm tr\u00fcb und ver\u00e4chtlich flie\u00dfet,", "tokens": ["Durch", "Kies", "und", "Schlamm", "tr\u00fcb", "und", "ver\u00b7\u00e4cht\u00b7lich", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich kr\u00fcmmt und schleicht, von fremden Wassern schwillt,", "tokens": ["Sich", "kr\u00fcmmt", "und", "schleicht", ",", "von", "frem\u00b7den", "Was\u00b7sern", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann rauscht und gl\u00e4nzt, sich stolz ins Land ergie\u00dfet,", "tokens": ["Dann", "rauscht", "und", "gl\u00e4nzt", ",", "sich", "stolz", "ins", "Land", "er\u00b7gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dort B\u00e4chen folgt, hier B\u00e4che selbst regiert,", "tokens": ["Dort", "B\u00e4\u00b7chen", "folgt", ",", "hier", "B\u00e4\u00b7che", "selbst", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und endlich gar des Stromes Namen f\u00fchrt.", "tokens": ["Und", "end\u00b7lich", "gar", "des", "Stro\u00b