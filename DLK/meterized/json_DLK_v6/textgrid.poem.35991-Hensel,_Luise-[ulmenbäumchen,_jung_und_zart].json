{"textgrid.poem.35991": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[ulmenb\u00e4umchen, jung und zart]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist gewi\u00df von guter Art.", "tokens": ["Ist", "ge\u00b7wi\u00df", "von", "gu\u00b7ter", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drum tut sein in deinem Garten", "tokens": ["Drum", "tut", "sein", "in", "dei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch der G\u00e4rtner warten.", "tokens": ["Auch", "der", "G\u00e4rt\u00b7ner", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ulmenb\u00e4umchen wird zum Baum", "tokens": ["Ul\u00b7men\u00b7b\u00e4um\u00b7chen", "wird", "zum", "Baum"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in deines Gartens Raum,", "tokens": ["Hier", "in", "dei\u00b7nes", "Gar\u00b7tens", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann will es vor den St\u00fcrmen", "tokens": ["Und", "dann", "will", "es", "vor", "den", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauher Zeit dich schirmen.", "tokens": ["Rau\u00b7her", "Zeit", "dich", "schir\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}