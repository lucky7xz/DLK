{"dta.poem.10986": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tzendienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht, in Gottes Creaturen, sein' Allmacht, Lieb und\nWeisheit ehrt,", "tokens": ["Wer", "nicht", ",", "in", "Got\u00b7tes", "Crea\u00b7tu\u00b7ren", ",", "sein'", "All\u00b7macht", ",", "Lieb", "und", "Weis\u00b7heit", "ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "$,", "APPR", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und nichts zu seinem Preise denkt, wenn er die Wunder sieht", "tokens": ["Und", "nichts", "zu", "sei\u00b7nem", "Prei\u00b7se", "denkt", ",", "wenn", "er", "die", "Wun\u00b7der", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "In welchem er sich offenbart, fast f\u00fchlbar und fast sichtbarlich,", "tokens": ["In", "wel\u00b7chem", "er", "sich", "of\u00b7fen\u00b7bart", ",", "fast", "f\u00fchl\u00b7bar", "und", "fast", "sicht\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+--++-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Scheint nimmermehr ein guter Christ,", "tokens": ["Scheint", "nim\u00b7mer\u00b7mehr", "ein", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wo nicht gar ein Atheist,", "tokens": ["Und", ",", "wo", "nicht", "gar", "ein", "A\u00b7theist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein G\u00f6tzendiener wenigstens (von einer Gottheit, die er sich,", "tokens": ["Ein", "G\u00f6t\u00b7zen\u00b7die\u00b7ner", "we\u00b7nigs\u00b7tens", "(", "von", "ei\u00b7ner", "Got\u00b7theit", ",", "die", "er", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Zu seinem Besten, selbst erdacht,", "tokens": ["Zu", "sei\u00b7nem", "Bes\u00b7ten", ",", "selbst", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die gleichsam blo\u00df f\u00fcr ihn allein,", "tokens": ["Die", "gleich\u00b7sam", "blo\u00df", "f\u00fcr", "ihn", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erschuf und wirket, was sie wirket, ihn segnet und ihn selig", "tokens": ["Er\u00b7schuf", "und", "wir\u00b7ket", ",", "was", "sie", "wir\u00b7ket", ",", "ihn", "seg\u00b7net", "und", "ihn", "se\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "PPER", "ADJD"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Ja, mit Verstand und Sinnen, doch Verstand- und Sinnen-", "tokens": ["Ja", ",", "mit", "Ver\u00b7stand", "und", "Sin\u00b7nen", ",", "doch", "Ver\u00b7stan\u00b7d", "und", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "NN", "KON", "NN", "$,", "ADV", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}