{"textgrid.poem.30016": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thraso hat nechst Krieg gef\u00fchrt mit den V\u00f6lckern, die da fliegen,", "tokens": ["Thra\u00b7so", "hat", "nechst", "Krieg", "ge\u00b7f\u00fchrt", "mit", "den", "V\u00f6l\u00b7ckern", ",", "die", "da", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "VVPP", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Das bey sechtzig tausent Mann ihm zun F\u00fcssen blieben liegen;", "tokens": ["Das", "bey", "secht\u00b7zig", "tau\u00b7sent", "Mann", "ihm", "zun", "F\u00fcs\u00b7sen", "blie\u00b7ben", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "CARD", "CARD", "NN", "PPER", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn er hat viel Vogel-Leim au\u00df Musqueten au\u00dfgeschossen,", "tokens": ["Denn", "er", "hat", "viel", "Vo\u00b7gel\u00b7Leim", "au\u00df", "Mus\u00b7que\u00b7ten", "au\u00df\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der an Federn seinem Feind ist behangen und zerflossen.", "tokens": ["Der", "an", "Fe\u00b7dern", "sei\u00b7nem", "Feind", "ist", "be\u00b7han\u00b7gen", "und", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wer von ihnen fiel herab, diesen stach wie wilden H\u00fcnnern", "tokens": ["Wer", "von", "ih\u00b7nen", "fiel", "her\u00b7ab", ",", "die\u00b7sen", "stach", "wie", "wil\u00b7den", "H\u00fcn\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "ADV", "$,", "PDS", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Ihre Federn er ins Hirn. Last euch dieses wol erinnern", "tokens": ["Ih\u00b7re", "Fe\u00b7dern", "er", "ins", "Hirn", ".", "Last", "euch", "die\u00b7ses", "wol", "e\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "$.", "VVFIN", "PPER", "PDS", "ADV", "VVINF"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Ihr, die ihr zu Felde dient, da\u00df ihr wisset recht zu kriegen,", "tokens": ["Ihr", ",", "die", "ihr", "zu", "Fel\u00b7de", "dient", ",", "da\u00df", "ihr", "wis\u00b7set", "recht", "zu", "krie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Waffen auch zu f\u00fchren recht wider Leute, die da f-l\u00fcgen.", "tokens": ["Waf\u00b7fen", "auch", "zu", "f\u00fch\u00b7ren", "recht", "wi\u00b7der", "Leu\u00b7te", ",", "die", "da", "f\u00b7l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "ADJD", "APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-++-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}