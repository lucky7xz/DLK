{"textgrid.poem.61417": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchl zu deinem Verstand spricht jegliche Lehre; sie bleibt dir", "tokens": ["K\u00fchl", "zu", "dei\u00b7nem", "Ver\u00b7stand", "spricht", "jeg\u00b7li\u00b7che", "Leh\u00b7re", ";", "sie", "bleibt", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ewig ein Totes, sobald fremd sie von au\u00dfen dir kommt.", "tokens": ["E\u00b7wig", "ein", "To\u00b7tes", ",", "so\u00b7bald", "fremd", "sie", "von", "au\u00b7\u00dfen", "dir", "kommt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "APPR", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Was dir ein anderer gibt, und w\u00e4r' es das K\u00f6stlichste, frommt nicht,", "tokens": ["Was", "dir", "ein", "an\u00b7de\u00b7rer", "gibt", ",", "und", "w\u00e4r'", "es", "das", "K\u00f6st\u00b7lichs\u00b7te", ",", "frommt", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "VVFIN", "$,", "KON", "VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wenn du den schlafenden Klang tief in der Seele nicht trugst.", "tokens": ["Wenn", "du", "den", "schla\u00b7fen\u00b7den", "Klang", "tief", "in", "der", "See\u00b7le", "nicht", "trugst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wunder begreifen sich nicht, du mu\u00dft sie im Innern erleben,", "tokens": ["Wun\u00b7der", "be\u00b7grei\u00b7fen", "sich", "nicht", ",", "du", "mu\u00dft", "sie", "im", "In\u00b7nern", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Jeglicher Glaub' ist ein Wahn, den du nicht selber erfuhrst;", "tokens": ["Jeg\u00b7li\u00b7cher", "Glaub'", "ist", "ein", "Wahn", ",", "den", "du", "nicht", "sel\u00b7ber", "er\u00b7fuhrst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nur was selbst du erkennst als ein G\u00f6ttliches, das dir herabkam,", "tokens": ["Nur", "was", "selbst", "du", "er\u00b7kennst", "als", "ein", "G\u00f6tt\u00b7li\u00b7ches", ",", "das", "dir", "her\u00b7ab\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Hat, ein lebendiger Hauch, dich zu verwandeln die Macht.", "tokens": ["Hat", ",", "ein", "le\u00b7ben\u00b7di\u00b7ger", "Hauch", ",", "dich", "zu", "ver\u00b7wan\u00b7deln", "die", "Macht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}