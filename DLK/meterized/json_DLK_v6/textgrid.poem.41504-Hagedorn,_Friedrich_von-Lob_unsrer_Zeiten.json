{"textgrid.poem.41504": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lob unsrer Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Tadler, schweigt! ich will der Welt", "tokens": ["Ihr", "Tad\u00b7ler", ",", "schweigt", "!", "ich", "will", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Vorzug unsrer Zeiten melden.", "tokens": ["Den", "Vor\u00b7zug", "uns\u00b7rer", "Zei\u00b7ten", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O wi\u00dft, wohin mein Blick nur f\u00e4llt,", "tokens": ["O", "wi\u00dft", ",", "wo\u00b7hin", "mein", "Blick", "nur", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jedem Stand' entdeck' ich Helden.", "tokens": ["In", "je\u00b7dem", "Stand'", "ent\u00b7deck'", "ich", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will der Menschen Lob besingen", "tokens": ["Ich", "will", "der", "Men\u00b7schen", "Lob", "be\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schenke meiner Lieder Schall", "tokens": ["Und", "schen\u00b7ke", "mei\u00b7ner", "Lie\u00b7der", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem tonbegier'gen Wiederhall;", "tokens": ["Dem", "ton\u00b7be\u00b7gier'\u00b7gen", "Wie\u00b7der\u00b7hall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Plaudrer mag ihn weiter bringen.", "tokens": ["Der", "Plaud\u00b7rer", "mag", "ihn", "wei\u00b7ter", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du tausendz\u00fcngiges Ger\u00fccht,", "tokens": ["Du", "tau\u00b7send\u00b7z\u00fcn\u00b7gi\u00b7ges", "Ge\u00b7r\u00fccht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erm\u00fcde nie im Ruhm der Zeiten;", "tokens": ["Er\u00b7m\u00fc\u00b7de", "nie", "im", "Ruhm", "der", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verschweige ja von ihnen nicht", "tokens": ["Ver\u00b7schwei\u00b7ge", "ja", "von", "ih\u00b7nen", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hunderttausend Trefflichkeiten!", "tokens": ["Die", "hun\u00b7dert\u00b7tau\u00b7send", "Treff\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Priester lebt nach seiner Lehre;", "tokens": ["Der", "Pries\u00b7ter", "lebt", "nach", "sei\u00b7ner", "Leh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Papst ist noch der Knechte Knecht;", "tokens": ["Der", "Papst", "ist", "noch", "der", "Knech\u00b7te", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Feldherr suchet nichts als Recht;", "tokens": ["Der", "Feld\u00b7herr", "su\u00b7chet", "nichts", "als", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Handelsherr nur Treu' und Ehre.", "tokens": ["Der", "Han\u00b7dels\u00b7herr", "nur", "Treu'", "und", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nichts \u00fcbertrifft die starke Zahl", "tokens": ["Nichts", "\u00fc\u00b7bert\u00b7rifft", "die", "star\u00b7ke", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewissenhafter Advocaten,", "tokens": ["Ge\u00b7wis\u00b7sen\u00b7haf\u00b7ter", "Ad\u00b7vo\u00b7ca\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die alle Jahre kaum einmal", "tokens": ["Die", "al\u00b7le", "Jah\u00b7re", "kaum", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rechte der Partei verrathen.", "tokens": ["Die", "Rech\u00b7te", "der", "Par\u00b7tei", "ver\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wollte nicht die Aerzte preisen?", "tokens": ["Wer", "woll\u00b7te", "nicht", "die", "A\u00b7erz\u00b7te", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Stets bleibt's der Kranken Eigenschaft,", "tokens": ["Stets", "bleibt's", "der", "Kran\u00b7ken", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df alle der Recepte Kraft,", "tokens": ["Da\u00df", "al\u00b7le", "der", "Re\u00b7cep\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lebendig oder todt, beweisen.", "tokens": ["Le\u00b7ben\u00b7dig", "o\u00b7der", "todt", ",", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wie reich ist die gelehrte Welt", "tokens": ["Wie", "reich", "ist", "die", "ge\u00b7lehr\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Wissenschaft und gro\u00dfen Geistern!", "tokens": ["An", "Wis\u00b7sen\u00b7schaft", "und", "gro\u00b7\u00dfen", "Geis\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Dank, den ihr Bem\u00fchn erh\u00e4lt,", "tokens": ["Den", "Dank", ",", "den", "ihr", "Be\u00b7m\u00fchn", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darf Momus, unberufen, meistern.", "tokens": ["Darf", "Mo\u00b7mus", ",", "un\u00b7be\u00b7ru\u00b7fen", ",", "meis\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "NE", "$,", "ADJD", "$,", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er will sich an Scribenten reiben,", "tokens": ["Er", "will", "sich", "an", "Scri\u00b7ben\u00b7ten", "rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Nur weil er selbst kein Lob gewinnt,", "tokens": ["Nur", "weil", "er", "selbst", "kein", "Lob", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sagt, da\u00df sie zu sittsam sind,", "tokens": ["Und", "sagt", ",", "da\u00df", "sie", "zu", "sitt\u00b7sam", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu sp\u00e4t und viel zu wenig schreiben.", "tokens": ["Zu", "sp\u00e4t", "und", "viel", "zu", "we\u00b7nig", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "ADV", "PTKA", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was gr\u00fcnt euch f\u00fcr ein Lorbeerhain,", "tokens": ["Was", "gr\u00fcnt", "euch", "f\u00fcr", "ein", "Lor\u00b7beer\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Monarchen, Herrscher, Sieger, Retter!", "tokens": ["Mon\u00b7ar\u00b7chen", ",", "Herr\u00b7scher", ",", "Sie\u00b7ger", ",", "Ret\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! k\u00f6nntet ihr unsterblich sein,", "tokens": ["Ach", "!", "k\u00f6nn\u00b7tet", "ihr", "uns\u00b7terb\u00b7lich", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchlaucht'ge F\u00fcrsten, ihr w\u00e4r't G\u00f6tter.", "tokens": ["Durch\u00b7laucht'\u00b7ge", "F\u00fcrs\u00b7ten", ",", "ihr", "w\u00e4r't", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wer kann doch eure Tugend fassen", "tokens": ["Wer", "kann", "doch", "eu\u00b7re", "Tu\u00b7gend", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und eurer Gaben Wechselstreit?", "tokens": ["Und", "eu\u00b7rer", "Ga\u00b7ben", "Wech\u00b7sel\u00b7streit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr habt nichts als die Dankbarkeit", "tokens": ["Ihr", "habt", "nichts", "als", "die", "Dank\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und die Geduld uns \u00fcberlassen.", "tokens": ["Und", "die", "Ge\u00b7duld", "uns", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Staatsmann, der an W\u00fcrden gro\u00df,", "tokens": ["Der", "Staats\u00b7mann", ",", "der", "an", "W\u00fcr\u00b7den", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ungleich gr\u00f6\u00dfer an Verstande,", "tokens": ["Doch", "un\u00b7gleich", "gr\u00f6\u00b7\u00dfer", "an", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sitzt jedem K\u00f6nig in dem Schoo\u00df", "tokens": ["Sitzt", "je\u00b7dem", "K\u00f6\u00b7nig", "in", "dem", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und findet sich in jedem Lande.", "tokens": ["Und", "fin\u00b7det", "sich", "in", "je\u00b7dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Regenten wissen zu regieren!", "tokens": ["Re\u00b7gen\u00b7ten", "wis\u00b7sen", "zu", "re\u00b7gie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die Kunst zu herrschen lernt sich bald;", "tokens": ["Die", "Kunst", "zu", "herr\u00b7schen", "lernt", "sich", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn alles steckt in der Gewalt", "tokens": ["Denn", "al\u00b7les", "steckt", "in", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der H\u00e4nde, die den Scepter f\u00fchren.", "tokens": ["Der", "H\u00e4n\u00b7de", ",", "die", "den", "Scep\u00b7ter", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Britte, der die Fremden sch\u00e4tzt,", "tokens": ["Der", "Brit\u00b7te", ",", "der", "die", "Frem\u00b7den", "sch\u00e4tzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will einem jeden sich verbinden;", "tokens": ["Will", "ei\u00b7nem", "je\u00b7den", "sich", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PIAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der stille Franzmann \u00fcbersetzt,", "tokens": ["Der", "stil\u00b7le", "Franz\u00b7mann", "\u00fc\u00b7bers\u00b7etzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir muntern Deutschen, wir erfinden.", "tokens": ["Wir", "mun\u00b7tern", "Deut\u00b7schen", ",", "wir", "er\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lobt in Iberiens Provinzen", "tokens": ["Lobt", "in", "I\u00b7be\u00b7ri\u00b7ens", "Pro\u00b7vin\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Scherz, Freiheit, Wahrheit, Demuth, Flei\u00df;", "tokens": ["Scherz", ",", "Frei\u00b7heit", ",", "Wahr\u00b7heit", ",", "De\u00b7muth", ",", "Flei\u00df", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lobt auch der Belgen steten Schwei\u00df", "tokens": ["Lobt", "auch", "der", "Bel\u00b7gen", "ste\u00b7ten", "Schwei\u00df"], "token_info":