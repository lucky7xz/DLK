{"dta.poem.973": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Ii", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht nahm ich acht auf dich in meiner bahn                 ", "tokens": ["Nicht", "nahm", "ich", "acht", "auf", "dich", "in", "mei\u00b7ner", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "CARD", "APPR", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In zeiten feucht und falb worin der wahn                 ", "tokens": ["In", "zei\u00b7ten", "feucht", "und", "falb", "wo\u00b7rin", "der", "wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des suchens fragens sich verlor               ", "tokens": ["Des", "su\u00b7chens", "fra\u00b7gens", "sich", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kann jemand in den zeiten feucht und falb                 ", "tokens": ["Kann", "je\u00b7mand", "in", "den", "zei\u00b7ten", "feucht", "und", "falb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am dunklen thore harren meinethalb?                 ", "tokens": ["Am", "dunk\u00b7len", "tho\u00b7re", "har\u00b7ren", "mei\u00b7net\u00b7halb", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun denk ich dein weil unterm dunklen thor               ", "tokens": ["Nun", "denk", "ich", "dein", "weil", "un\u00b7term", "dunk\u00b7len", "thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo &#228;ngstend s&#228;ule und gem&#228;uer knarrt                 ", "tokens": ["Wo", "&#228;", "ngs\u00b7tend", "s", "&#228;", "u\u00b7le", "und", "gem", "&#228;", "uer", "knarrt"], "token_info": ["word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PWAV", "$(", "FM", "FM", "FM", "FM", "KON", "ADJA", "$(", "ADJD", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du meinethalben mein geharrt                 ", "tokens": ["Du", "mei\u00b7net\u00b7hal\u00b7ben", "mein", "ge\u00b7harrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als niemand ging und als es schweigsam fror.               ", "tokens": ["Als", "nie\u00b7mand", "ging", "und", "als", "es", "schweig\u00b7sam", "fror", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}