{"textgrid.poem.55713": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: O lasse mein Flehn Dich erweichen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O lasse mein Flehn Dich erweichen", "tokens": ["O", "las\u00b7se", "mein", "Flehn", "Dich", "er\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kehr' zu den Deinen zur\u00fcck.", "tokens": ["Und", "kehr'", "zu", "den", "Dei\u00b7nen", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weitab dort, im Schatten der Eichen,", "tokens": ["Weit\u00b7ab", "dort", ",", "im", "Schat\u00b7ten", "der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "An schilfrohr-umfl\u00fcsterten Teichen,", "tokens": ["An", "schilf\u00b7rohr\u00b7um\u00b7fl\u00fcs\u00b7ter\u00b7ten", "Tei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dort bl\u00fcht Dir vielleicht noch ein Gl\u00fcck.", "tokens": ["Dort", "bl\u00fcht", "Dir", "viel\u00b7leicht", "noch", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch hier, in dem dumpfen Gew\u00fchle", "tokens": ["Doch", "hier", ",", "in", "dem", "dum\u00b7pfen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Stadt, in der S\u00fcnde Revier,", "tokens": ["Der", "Stadt", ",", "in", "der", "S\u00fcn\u00b7de", "Re\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Du Reine, Waldesk\u00fchle,", "tokens": ["Du", "Rei\u00b7ne", ",", "Wal\u00b7des\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sprich: was willst Du hier?", "tokens": ["O", "sprich", ":", "was", "willst", "Du", "hier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du wirst Deine Jugend vers\u00e4umen", "tokens": ["Du", "wirst", "Dei\u00b7ne", "Ju\u00b7gend", "ver\u00b7s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Deiner Seele Heil", "tokens": ["Und", "Dei\u00b7ner", "See\u00b7le", "Heil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In nervenerschlaffenden Tr\u00e4umen,", "tokens": ["In", "ner\u00b7ve\u00b7ner\u00b7schlaf\u00b7fen\u00b7den", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In Wonnen, wollustgeil,", "tokens": ["In", "Won\u00b7nen", ",", "wol\u00b7lust\u00b7geil", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du wirst, mit erloschenen Blicken,", "tokens": ["Du", "wirst", ",", "mit", "er\u00b7lo\u00b7sche\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Nacht entgegenziehn,", "tokens": ["Der", "Nacht", "ent\u00b7ge\u00b7gen\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wirst Deine Qualen ersticken", "tokens": ["Und", "wirst", "Dei\u00b7ne", "Qua\u00b7len", "er\u00b7sti\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Gleich mir in Absinth und Morphin.", "tokens": ["Gleich", "mir", "in", "Ab\u00b7sinth", "und", "Mor\u00b7phin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "O lasse mein Flehn Dich erweichen", "tokens": ["O", "las\u00b7se", "mein", "Flehn", "Dich", "er\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kehr' zu den Deinen zur\u00fcck.", "tokens": ["Und", "kehr'", "zu", "den", "De