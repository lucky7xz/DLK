{"textgrid.poem.32795": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df immer, Unschuld, meinen Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df immer, Unschuld, meinen Sinn,", "tokens": ["La\u00df", "im\u00b7mer", ",", "Un\u00b7schuld", ",", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir immer heilig sein!", "tokens": ["Dir", "im\u00b7mer", "hei\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du weihtest mich zur Priesterin", "tokens": ["Du", "weih\u00b7test", "mich", "zur", "Pries\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deinem Altar ein.", "tokens": ["An", "dei\u00b7nem", "Al\u00b7tar", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein falsches Licht", "tokens": ["Dein", "fal\u00b7sches", "Licht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Verdunkle nicht,", "tokens": ["Ver\u00b7dunk\u00b7le", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "O G\u00f6ttin, mir dein Angesicht!", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "mir", "dein", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie hab' ich dich so manchesmal", "tokens": ["Wie", "hab'", "ich", "dich", "so", "man\u00b7ches\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Innersten gef\u00fchlt,", "tokens": ["Im", "In\u00b7ners\u00b7ten", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn ich im stillen Lindenthal", "tokens": ["Wenn", "ich", "im", "stil\u00b7len", "Lin\u00b7dent\u00b7hal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem Lamm gespielt!", "tokens": ["Mit", "dei\u00b7nem", "Lamm", "ge\u00b7spielt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da schicktest du", "tokens": ["Da", "schick\u00b7test", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mir Himmelsruh'", "tokens": ["Mir", "Him\u00b7mels\u00b7ruh'"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und deine lieben T\u00e4ubchen zu!", "tokens": ["Und", "dei\u00b7ne", "lie\u00b7ben", "T\u00e4ub\u00b7chen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df immer frische Blumen mir", "tokens": ["La\u00df", "im\u00b7mer", "fri\u00b7sche", "Blu\u00b7men", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinem Pfade bl\u00fchn!", "tokens": ["Auf", "mei\u00b7nem", "Pfa\u00b7de", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00e4nge sie in Kr\u00e4nzen dir", "tokens": ["Ich", "h\u00e4n\u00b7ge", "sie", "in", "Kr\u00e4n\u00b7zen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deinem Altar hin.", "tokens": ["An", "dei\u00b7nem", "Al\u00b7tar", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Saitenklang", "tokens": ["Mit", "Sai\u00b7ten\u00b7klang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und Lobgesang", "tokens": ["Und", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bring' ich dir immer Herzensdank.", "tokens": ["Bring'", "ich", "dir", "im\u00b7mer", "Her\u00b7zens\u00b7dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn langsamer, dem Grabe zu", "tokens": ["Wenn", "lang\u00b7sa\u00b7mer", ",", "dem", "Gra\u00b7be", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJD", "$,", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich meine Schritte nahn;", "tokens": ["Sich", "mei\u00b7ne", "Schrit\u00b7te", "nahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann, G\u00f6ttin, unterst\u00fctze du", "tokens": ["Dann", ",", "G\u00f6t\u00b7tin", ",", "un\u00b7ter\u00b7st\u00fct\u00b7ze", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich auf der letzten Bahn!", "tokens": ["Mich", "auf", "der", "letz\u00b7ten", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lach Frieden mir!", "tokens": ["Lach", "Frie\u00b7den", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich dank' es dir,", "tokens": ["Ich", "dank'", "es", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und folg' in deinem Himmel dir!", "tokens": ["Und", "fol\u00b7g'", "in", "dei\u00b7nem", "Him\u00b7mel", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}}}}