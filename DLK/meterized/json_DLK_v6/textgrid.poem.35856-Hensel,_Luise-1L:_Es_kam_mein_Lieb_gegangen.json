{"textgrid.poem.35856": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kam mein Lieb gegangen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam mein Lieb gegangen", "tokens": ["Es", "kam", "mein", "Lieb", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf gr\u00fcnem Rasengrund. \u2013", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Ra\u00b7sen\u00b7grund", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie leuchten Seine Wangen!", "tokens": ["Wie", "leuch\u00b7ten", "Sei\u00b7ne", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie lacht Sein Purpurmund! \u2013", "tokens": ["Wie", "lacht", "Sein", "Pur\u00b7pur\u00b7mund", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Augen, lichte Sonnen,", "tokens": ["Die", "Au\u00b7gen", ",", "lich\u00b7te", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie strahlten mir in's Herz,", "tokens": ["Sie", "strahl\u00b7ten", "mir", "in's", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das brach ob solchen Wonnen", "tokens": ["Das", "brach", "ob", "sol\u00b7chen", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und brach ob solchem Schmerz.", "tokens": ["Und", "brach", "ob", "sol\u00b7chem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ganz gab sich's Ihm zu eigen", "tokens": ["Ganz", "gab", "sich's", "Ihm", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In s\u00fc\u00dfem Liebestod,", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Lie\u00b7be\u00b7stod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Er mit holdem Neigen", "tokens": ["Als", "Er", "mit", "hol\u00b7dem", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ersten Ku\u00df mir bot.", "tokens": ["Den", "ers\u00b7ten", "Ku\u00df", "mir", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie bl\u00fchte rings die Oede,", "tokens": ["Wie", "bl\u00fch\u00b7te", "rings", "die", "O\u00b7e\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie t\u00f6nte rings Gesang,", "tokens": ["Wie", "t\u00f6n\u00b7te", "rings", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Seine s\u00fc\u00dfe Rede", "tokens": ["Als", "Sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Re\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meine Seele klang!", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bei Seiner Liebe Kosen", "tokens": ["Bei", "Sei\u00b7ner", "Lie\u00b7be", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Morgensonnenstrahl,", "tokens": ["Im", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie flammten hoch die Rosen, \u2013", "tokens": ["Wie", "flamm\u00b7ten", "hoch", "die", "Ro\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie jauchzten Berg und Thal! \u2013", "tokens": ["Wie", "jauchz\u00b7ten", "Berg", "und", "Thal", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch weh! wie bald entschwunden", "tokens": ["Doch", "weh", "!", "wie", "bald", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "PWAV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War meine sel'ge Lust.", "tokens": ["War", "mei\u00b7ne", "sel'\u00b7ge", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich ging, der Liebe Wunden", "tokens": ["Ich", "ging", ",", "der", "Lie\u00b7be", "Wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In sehnsuchtsvoller Brust.", "tokens": ["In", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}