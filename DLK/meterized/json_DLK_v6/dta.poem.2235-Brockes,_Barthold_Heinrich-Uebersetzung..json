{"dta.poem.2235": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wunderlich verf\u00e4hrt der Mensch mit allem, was", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "ver\u00b7f\u00e4hrt", "der", "Mensch", "mit", "al\u00b7lem", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "APPR", "PIS", "$,", "PWS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihm hier beschieden,", "tokens": ["ihm", "hier", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit einem Nichts ist er zuweilen, doch mehrentheils mit", "tokens": ["Mit", "ei\u00b7nem", "Nichts", "ist", "er", "zu\u00b7wei\u00b7len", ",", "doch", "meh\u00b7ren\u00b7theils", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "APPR"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nichts zufrieden.", "tokens": ["Nichts", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}