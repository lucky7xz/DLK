{"textgrid.poem.59384": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch eilt der helle Strom hinunter.", "tokens": ["Frisch", "eilt", "der", "hel\u00b7le", "Strom", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drauf ziehn viel bunte Schifflein munter,", "tokens": ["Drauf", "ziehn", "viel", "bun\u00b7te", "Schif\u00b7flein", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Strom und Schiff und bunte Scheine,", "tokens": ["Und", "Strom", "und", "Schiff", "und", "bun\u00b7te", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie fragen alle: was ich weine?", "tokens": ["Sie", "fra\u00b7gen", "al\u00b7le", ":", "was", "ich", "wei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir ist so wohl, mir ist so weh,", "tokens": ["Mir", "ist", "so", "wohl", ",", "mir", "ist", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich den Fr\u00fchling fahren seh.", "tokens": ["Wie", "ich", "den", "Fr\u00fch\u00b7ling", "fah\u00b7ren", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel Lenze sitz ich schon da oben,", "tokens": ["Viel", "Len\u00b7ze", "sitz", "ich", "schon", "da", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVIMP", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Regenbogen steht im Land erhoben", "tokens": ["Ein", "Re\u00b7gen\u00b7bo\u00b7gen", "steht", "im", "Land", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch die T\u00e4ler, Wiesen, Wogen", "tokens": ["Und", "durch", "die", "T\u00e4\u00b7ler", ",", "Wie\u00b7sen", ",", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Still, wie ein fernes Lied, gezogen,", "tokens": ["Still", ",", "wie", "ein", "fer\u00b7nes", "Lied", ",", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schifft immerfort dein himmlisch Bild \u2013", "tokens": ["Schifft", "im\u00b7mer\u00b7fort", "dein", "himm\u00b7lisch", "Bild", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Strom und Schiff nie stille hielt.", "tokens": ["Doch", "Strom", "und", "Schiff", "nie", "stil\u00b7le", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}