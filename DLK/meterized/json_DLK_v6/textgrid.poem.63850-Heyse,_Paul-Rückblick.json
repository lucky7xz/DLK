{"textgrid.poem.63850": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckblick", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn es wahr ist, jung schon m\u00fcsse sterben,", "tokens": ["Wenn", "es", "wahr", "ist", ",", "jung", "schon", "m\u00fcs\u00b7se", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADJD", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wen die G\u00f6tter lieben, bin ich dankbar,", "tokens": ["Wen", "die", "G\u00f6t\u00b7ter", "lie\u00b7ben", ",", "bin", "ich", "dank\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Da\u00df sie nicht zum Liebling mich erkoren.", "tokens": ["Da\u00df", "sie", "nicht", "zum", "Lieb\u00b7ling", "mich", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zwar, schon nach des Schauspiels erstem Aufzug", "tokens": ["Zwar", ",", "schon", "nach", "des", "Schau\u00b7spiels", "ers\u00b7tem", "Auf\u00b7zug"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Heimzugehn, empfiehlt sich, wenn ein R\u00fchrst\u00fcck", "tokens": ["Heim\u00b7zu\u00b7gehn", ",", "emp\u00b7fiehlt", "sich", ",", "wenn", "ein", "R\u00fchr\u00b7st\u00fcck"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oder Alltagspuppenspiel tragiert wird.", "tokens": ["O\u00b7der", "All\u00b7tags\u00b7pup\u00b7pen\u00b7spiel", "tra\u00b7giert", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VAFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Doch, ob meistens auch des St\u00fcckes Fabel", "tokens": ["Doch", ",", "ob", "meis\u00b7tens", "auch", "des", "St\u00fc\u00b7ckes", "Fa\u00b7bel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schal und d\u00fcrftig, ist's doch unterhaltsam,", "tokens": ["Schal", "und", "d\u00fcrf\u00b7tig", ",", "ist's", "doch", "un\u00b7ter\u00b7halt\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Zuzuschau'n dem bunten Szenenwechsel", "tokens": ["Zu\u00b7zu\u00b7schau'n", "dem", "bun\u00b7ten", "Sze\u00b7nen\u00b7wech\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Dieser Posse, die wir Leben nennen,", "tokens": ["Die\u00b7ser", "Pos\u00b7se", ",", "die", "wir", "Le\u00b7ben", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Bis der Vorhang f\u00e4llt, und dann mit G\u00e4hnen,", "tokens": ["Bis", "der", "Vor\u00b7hang", "f\u00e4llt", ",", "und", "dann", "mit", "G\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Unerbaut zwar, doch an Wahrheit reicher", "tokens": ["Un\u00b7er\u00b7baut", "zwar", ",", "doch", "an", "Wahr\u00b7heit", "rei\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "ADV", "APPR", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Uns zu Bett zu legen.", "tokens": ["Uns", "zu", "Bett", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Manchmal freilich", "tokens": ["Manch\u00b7mal", "frei\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Geht ein m\u00e4chtig Trauerspiel in Szene.", "tokens": ["Geht", "ein", "m\u00e4ch\u00b7tig", "Trau\u00b7er\u00b7spiel", "in", "Sze\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "APPR", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Dann f\u00fcrwahr verlohnt sich's, auszuharren", "tokens": ["Dann", "f\u00fcr\u00b7wahr", "ver\u00b7lohnt", "sich's", ",", "aus\u00b7zu\u00b7har\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "$,", "VVIZU"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Bis zum Abschlu\u00df und von Furcht und Mitleid", "tokens": ["Bis", "zum", "Ab\u00b7schlu\u00df", "und", "von", "Furcht", "und", "Mit\u00b7leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Tiefbewegt im Innern zu erschaudern.", "tokens": ["Tief\u00b7be\u00b7wegt", "im", "In\u00b7nern", "zu", "er\u00b7schau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Gleicherma\u00dfen, wessen Erdendasein", "tokens": ["Glei\u00b7cher\u00b7ma\u00b7\u00dfen", ",", "wes\u00b7sen", "Er\u00b7den\u00b7da\u00b7sein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "T\u00e4tig wirksam wie ein Strom verflossen,", "tokens": ["T\u00e4\u00b7tig", "wirk\u00b7sam", "wie", "ein", "Strom", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Der befruchtet rings das Land und stolze", "tokens": ["Der", "be\u00b7fruch\u00b7tet", "rings", "das", "Land", "und", "stol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Schiffe trug, dem ziemt es kaum zu klagen,", "tokens": ["Schif\u00b7fe", "trug", ",", "dem", "ziemt", "es", "kaum", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Nicht die Lampen wert sei die Kom\u00f6die.", "tokens": ["Nicht", "die", "Lam\u00b7pen", "wert", "sei", "die", "Ko\u00b7m\u00f6\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und nun vollends, wem das Gl\u00fcck verg\u00f6nnt ward,", "tokens": ["Und", "nun", "vol\u00b7lends", ",", "wem", "das", "Gl\u00fcck", "ver\u00b7g\u00f6nnt", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PWS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Im Parterre zu sitzen, w\u00e4hrend droben", "tokens": ["Im", "Par\u00b7ter\u00b7re", "zu", "sit\u00b7zen", ",", "w\u00e4h\u00b7rend", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Aufgef\u00fchrt ward eine Welthistorie,", "tokens": ["Auf\u00b7ge\u00b7f\u00fchrt", "ward", "ei\u00b7ne", "Welt\u00b7his\u00b7to\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Gr\u00f6\u00dfer als sie je ein Dichter tr\u00e4umte:", "tokens": ["Gr\u00f6\u00b7\u00dfer", "als", "sie", "je", "ein", "Dich\u00b7ter", "tr\u00e4um\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Ein Volks Erstehn aus \u00fcberlanger", "tokens": ["Ein", "Volks", "Er\u00b7stehn", "aus", "\u00fc\u00b7berl\u00b7an\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Schmach zu h\u00f6chster Herrschermacht und Glorie,", "tokens": ["Schmach", "zu", "h\u00f6chs\u00b7ter", "Herr\u00b7scher\u00b7macht", "und", "Glo\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Wer den hohen Genius, der vollbracht so", "tokens": ["Wer", "den", "ho\u00b7hen", "Ge\u00b7nius", ",", "der", "voll\u00b7bracht", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Traumhaft Herrliches, mit Augen schaute,", "tokens": ["Traum\u00b7haft", "Herr\u00b7li\u00b7ches", ",", "mit", "Au\u00b7gen", "schau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Ja die Hand ihm leibhaft dr\u00fccken durfte,", "tokens": ["Ja", "die", "Hand", "ihm", "leib\u00b7haft", "dr\u00fc\u00b7cken", "durf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Der wird keinen \u00bbG\u00f6tterliebling\u00ab neiden", "tokens": ["Der", "wird", "kei\u00b7nen", "\u00bb", "G\u00f6t\u00b7ter\u00b7lieb\u00b7ling", "\u00ab", "nei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PIAT", "$(", "NN", "$(", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Um den fr\u00fchen Heimgang, der den Anblick", "tokens": ["Um", "den", "fr\u00fc\u00b7hen", "Heim\u00b7gang", ",", "der", "den", "An\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Solchen Wunders ihn verschlafen lassen.", "tokens": ["Sol\u00b7chen", "Wun\u00b7ders", "ihn", "ver\u00b7schla\u00b7fen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Nein, im Fr\u00f6steln seines Greisenwinters", "tokens": ["Nein", ",", "im", "Fr\u00f6s\u00b7teln", "sei\u00b7nes", "Grei\u00b7sen\u00b7win\u00b7ters"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Wird sein Herz ergl\u00fchn, so oft Erinnrung", "tokens": ["Wird", "sein", "Herz", "er\u00b7gl\u00fchn", ",", "so", "oft", "E\u00b7rinn\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$,", "ADV", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Ihm den Fr\u00fchhauch jener Zeit zur\u00fcckbringt.", "tokens": ["Ihm", "den", "Fr\u00fch\u00b7hauch", "je\u00b7ner", "Zeit", "zu\u00b7r\u00fcck\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}