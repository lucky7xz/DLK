{"textgrid.poem.41692": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kennst du so tief wie ich des Leidens S\u00fc\u00dfigkeiten,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du so tief wie ich des Leidens S\u00fc\u00dfigkeiten,", "tokens": ["Kennst", "du", "so", "tief", "wie", "ich", "des", "Lei\u00b7dens", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sagt man auch von dir: \u00bbSeht, welch ein Sonderling!\u00ab?", "tokens": ["Und", "sagt", "man", "auch", "von", "dir", ":", "\u00bb", "Seht", ",", "welch", "ein", "Son\u00b7der\u00b7ling", "!", "\u00ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "PPER", "$.", "$(", "VVFIN", "$,", "PWAT", "ART", "NN", "$.", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u2013 Ich lag im Sterben. In der Brust, der todgeweihten,", "tokens": ["\u2013", "Ich", "lag", "im", "Ster\u00b7ben", ".", "In", "der", "Brust", ",", "der", "tod\u00b7ge\u00b7weih\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "$.", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Rang Schrecken und der Wunsch zum Tod, ein seltsam Ding.", "tokens": ["Rang", "Schre\u00b7cken", "und", "der", "Wunsch", "zum", "Tod", ",", "ein", "selt\u00b7sam", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Angst f\u00fchlt' und Hoffnung ich, jedoch kein Widerstreiten.", "tokens": ["Angst", "f\u00fchlt'", "und", "Hoff\u00b7nung", "ich", ",", "je\u00b7doch", "kein", "Wi\u00b7der\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "PPER", "$,", "ADV", "PIAT", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Je mehr der schlimme Sand der Uhr zu Ende ging,", "tokens": ["Je", "mehr", "der", "schlim\u00b7me", "Sand", "der", "Uhr", "zu", "En\u00b7de", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je sch\u00e4rfer f\u00fchlte ich der Folter Herrlichkeiten;", "tokens": ["Je", "sch\u00e4r\u00b7fer", "f\u00fchl\u00b7te", "ich", "der", "Fol\u00b7ter", "Herr\u00b7lich\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Herz entri\u00df sich ganz der Welt, die es umfing.", "tokens": ["Mein", "Herz", "ent\u00b7ri\u00df", "sich", "ganz", "der", "Welt", ",", "die", "es", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein F\u00fchlen glich dem Wunsch von schaubegiergen Kindern,", "tokens": ["Mein", "F\u00fch\u00b7len", "glich", "dem", "Wunsch", "von", "schau\u00b7be\u00b7gier\u00b7gen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Vorhangsfalten feind, die unsre Blicke hindern ...", "tokens": ["Den", "Vor\u00b7hangs\u00b7fal\u00b7ten", "feind", ",", "die", "uns\u00b7re", "Bli\u00b7cke", "hin\u00b7dern", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis da\u00df die Wahrheit sich enth\u00fcllte, kalt und bla\u00df:", "tokens": ["Bis", "da\u00df", "die", "Wahr\u00b7heit", "sich", "ent\u00b7h\u00fcll\u00b7te", ",", "kalt", "und", "bla\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Tot war ich ohne Schreck. Und mich umgab der Schimmer", "tokens": ["Tot", "war", "ich", "oh\u00b7ne", "Schreck", ".", "Und", "mich", "um\u00b7gab", "der", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "$.", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des grausen Morgenrots. \u2013 Was! ist es nichts als das!", "tokens": ["Des", "grau\u00b7sen", "Mor\u00b7gen\u00b7rots", ".", "\u2013", "Was", "!", "ist", "es", "nichts", "als", "das", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PWS", "$.", "VAFIN", "PPER", "PIS", "KOKOM", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Vorhang war entschwebt ... ich wartete noch immer.", "tokens": ["Der", "Vor\u00b7hang", "war", "ent\u00b7schwebt", "...", "ich", "war\u00b7te\u00b7te", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}