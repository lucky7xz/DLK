{"dta.poem.2320": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Versuch  \n von den Eigenschaften  \n eines  \n  Kunstrichters.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198534", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weste S\u00e4useln soll im Liede wiederhallen,", "tokens": ["Der", "Wes\u00b7te", "S\u00e4u\u00b7seln", "soll", "im", "Lie\u00b7de", "wie\u00b7der\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine sanfte Fluth in sanften Th\u00f6nen wallen,", "tokens": ["Und", "ei\u00b7ne", "sanf\u00b7te", "Fluth", "in", "sanf\u00b7ten", "Th\u00f6\u00b7nen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wenn sie tobt und braust, so stell\u2019 es unsrem Ohr", "tokens": ["Doch", "wenn", "sie", "tobt", "und", "braust", ",", "so", "stell'", "es", "uns\u00b7rem", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Sturm im Verse selbst mit Lerm und Prasseln vor.", "tokens": ["Ein", "Sturm", "im", "Ver\u00b7se", "selbst", "mit", "Lerm", "und", "Pras\u00b7seln", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}