{"textgrid.poem.26023": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Einst werden Sonn' und Sterne kalt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liegst so gut in meinem Arm,", "tokens": ["Du", "liegst", "so", "gut", "in", "mei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gut ruht nur in mir mein Herz.", "tokens": ["So", "gut", "ruht", "nur", "in", "mir", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir schweben wie das Feuer fort", "tokens": ["Wir", "schwe\u00b7ben", "wie", "das", "Feu\u00b7er", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leben nur der K\u00fcsse Leben.", "tokens": ["Und", "le\u00b7ben", "nur", "der", "K\u00fcs\u00b7se", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Einst werden Sonn' und Sterne kalt,", "tokens": ["Einst", "wer\u00b7den", "Sonn'", "und", "Ster\u00b7ne", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns hat der Tod vergessen m\u00fcssen,", "tokens": ["Uns", "hat", "der", "Tod", "ver\u00b7ges\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und tausend, tausend Jahre alt", "tokens": ["Und", "tau\u00b7send", ",", "tau\u00b7send", "Jah\u00b7re", "alt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Leben wir noch in jungen K\u00fcssen.", "tokens": ["Le\u00b7ben", "wir", "noch", "in", "jun\u00b7gen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}