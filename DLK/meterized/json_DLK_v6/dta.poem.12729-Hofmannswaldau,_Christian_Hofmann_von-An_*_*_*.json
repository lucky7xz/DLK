{"dta.poem.12729": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An * * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedweder vers ist dir zu harte;", "tokens": ["Jed\u00b7we\u00b7der", "vers", "ist", "dir", "zu", "har\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut aber man", "tokens": ["Schaut", "a\u00b7ber", "man"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein dichten an,", "tokens": ["Dein", "dich\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So klingt es zehnmal h\u00e4rter.", "tokens": ["So", "klingt", "es", "zehn\u00b7mal", "h\u00e4r\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der eine vers hat sieben f\u00fc\u00dfe,", "tokens": ["Der", "ei\u00b7ne", "vers", "hat", "sie\u00b7ben", "f\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "CARD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der andre hinckt, der dritte zu viel risse,", "tokens": ["Der", "and\u00b7re", "hinckt", ",", "der", "drit\u00b7te", "zu", "viel", "ris\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ART", "ADJA", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da sind die sylben unrecht angebracht.", "tokens": ["Da", "sind", "die", "syl\u00b7ben", "un\u00b7recht", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch weist du, wie man jenes besser macht,", "tokens": ["Doch", "weist", "du", ",", "wie", "man", "je\u00b7nes", "bes\u00b7ser", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PIS", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gieb nur die f\u00fc\u00dfe weg den lahmen bettel-leuten;", "tokens": ["Gieb", "nur", "die", "f\u00fc\u00b7\u00dfe", "weg", "den", "lah\u00b7men", "bet\u00b7tel\u00b7leu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die werden dein versehn doch noch zum besten deuten.", "tokens": ["Die", "wer\u00b7den", "dein", "ver\u00b7sehn", "doch", "noch", "zum", "bes\u00b7ten", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}