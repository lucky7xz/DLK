{"textgrid.poem.24958": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Drei Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich aus ihrer Ruhestatt", "tokens": ["N\u00e4cht\u00b7lich", "aus", "ih\u00b7rer", "Ru\u00b7he\u00b7statt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Steigen drei deutsche Dichter;", "tokens": ["Stei\u00b7gen", "drei", "deut\u00b7sche", "Dich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Klagend schaun sie mich an und matt,", "tokens": ["Kla\u00b7gend", "schaun", "sie", "mich", "an", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Blasse Totengesichter.", "tokens": ["Blas\u00b7se", "To\u00b7ten\u00b7ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Deutsche Mutter, wie warst du so karg", "tokens": ["Deut\u00b7sche", "Mut\u00b7ter", ",", "wie", "warst", "du", "so", "karg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Deinen S\u00f6hnen im Leben;", "tokens": ["Dei\u00b7nen", "S\u00f6h\u00b7nen", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Nichts als die Wiege, den Gram und den Sarg", "tokens": ["Nichts", "als", "die", "Wie\u00b7ge", ",", "den", "Gram", "und", "den", "Sarg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Hast du den Edlen gegeben.", "tokens": ["Hast", "du", "den", "Ed\u00b7len", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort den trauerverh\u00fcllten Geist,", "tokens": ["Dort", "den", "trau\u00b7er\u00b7ver\u00b7h\u00fcll\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kennst du ihn? Gieb mir Kunde!", "tokens": ["Kennst", "du", "ihn", "?", "Gieb", "mir", "Kun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ueber der m\u00e4chtigen Stirne weist", "tokens": ["Ue\u00b7ber", "der", "m\u00e4ch\u00b7ti\u00b7gen", "Stir\u00b7ne", "weist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Er die klaffende Wunde.", "tokens": ["Er", "die", "klaf\u00b7fen\u00b7de", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Kummer um dich, der sein Leben geknickt,", "tokens": ["Kum\u00b7mer", "um", "dich", ",", "der", "sein", "Le\u00b7ben", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Trieb ihn hinab zu den Toten;", "tokens": ["Trieb", "ihn", "hin\u00b7ab", "zu", "den", "To\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "St\u00e4rker, wie er dich wieder erblickt,", "tokens": ["St\u00e4r\u00b7ker", ",", "wie", "er", "dich", "wie\u00b7der", "er\u00b7blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rieseln die Tropfen, die roten.", "tokens": ["Rie\u00b7seln", "die", "Trop\u00b7fen", ",", "die", "ro\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Und der zweite, die Locken zerrauft,", "tokens": ["Und", "der", "zwei\u00b7te", ",", "die", "Lo\u00b7cken", "zer\u00b7rauft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wei\u00df die M\u00e4r zu erz\u00e4hlen,", "tokens": ["Wei\u00df", "die", "M\u00e4r", "zu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wie du die eigenen S\u00f6hne verkauft", "tokens": ["Wie", "du", "die", "ei\u00b7ge\u00b7nen", "S\u00f6h\u00b7ne", "ver\u00b7kauft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An die M\u00e4kler der Seelen.", "tokens": ["An", "die", "M\u00e4k\u00b7ler", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "In den W\u00e4ldern des Westens voll Gram", "tokens": ["In", "den", "W\u00e4l\u00b7dern", "des", "Wes\u00b7tens", "voll", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Irrte der Fremdling verloren;", "tokens": ["Irr\u00b7te", "der", "Fremd\u00b7ling", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Selbst den Wilden verschwieg er vor Scham,", "tokens": ["Selbst", "den", "Wil\u00b7den", "ver\u00b7schwieg", "er", "vor", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Welches Land ihn geboren.", "tokens": ["Wel\u00b7ches", "Land", "ihn", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Und der dritte mit starrem Blick,", "tokens": ["Und", "der", "drit\u00b7te", "mit", "star\u00b7rem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aber den Z\u00fcgen der Griechen,", "tokens": ["A\u00b7ber", "den", "Z\u00fc\u00b7gen", "der", "Grie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Stammelt verst\u00f6rt: Warum, Geschick,", "tokens": ["Stam\u00b7melt", "ver\u00b7st\u00f6rt", ":", "Wa\u00b7rum", ",", "Ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$.", "PWAV", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mu\u00dft' ich in Deutschland siechen?", "tokens": ["Mu\u00dft'", "ich", "in", "Deutschland", "sie\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "VVINF", "$."], "meter": "+-+++-", "measure": "unknown.measure.tetra"}}, "stanza.8": {"line.1": {"text": "Schon in der Wiege traf ihn der Fluch,", "tokens": ["Schon", "in", "der", "Wie\u00b7ge", "traf", "ihn", "der", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der sich am J\u00fcngling erf\u00fcllte,", "tokens": ["Der", "sich", "am", "J\u00fcng\u00b7ling", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis mit des Wahnsinns Schleiertuch", "tokens": ["Bis", "mit", "des", "Wahn\u00b7sinns", "Schlei\u00b7er\u00b7tuch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mild ihn der Himmel umh\u00fcllte.", "tokens": ["Mild", "ihn", "der", "Him\u00b7mel", "um\u00b7h\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Das sind die drei, die im Trauerchor", "tokens": ["Das", "sind", "die", "drei", ",", "die", "im", "Trau\u00b7er\u00b7chor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "CARD", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "N\u00e4chtlich den Reigen schlingen!", "tokens": ["N\u00e4cht\u00b7lich", "den", "Rei\u00b7gen", "schlin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sage, wie t\u00f6nt dir das Lied ins Ohr,", "tokens": ["Sa\u00b7ge", ",", "wie", "t\u00f6nt", "dir", "das", "Lied", "ins", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mutter, das sie dir singen?", "tokens": ["Mut\u00b7ter", ",", "das", "sie", "dir", "sin\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Deutsche Mutter, verbirg dein Gesicht!", "tokens": ["Deut\u00b7sche", "Mut\u00b7ter", ",", "ver\u00b7birg", "dein", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nicht mit marmornen Platten,", "tokens": ["Nicht", "mit", "mar\u00b7mor\u00b7nen", "Plat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und mit dem Lorbeer auf Gr\u00e4bern nicht", "tokens": ["Und", "mit", "dem", "Lor\u00b7beer", "auf", "Gr\u00e4\u00b7bern", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "PTKNEG"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "S\u00fchnst du die z\u00fcrnenden Schatten.", "tokens": ["S\u00fchnst", "du", "die", "z\u00fcr\u00b7nen\u00b7den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}