{"dta.poem.933": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Umkreisen wir den stillen teich               ", "tokens": ["Um\u00b7krei\u00b7sen", "wir", "den", "stil\u00b7len", "teich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den die wasserwege m&#252;nden               ", "tokens": ["In", "den", "die", "was\u00b7ser\u00b7we\u00b7ge", "m", "&#252;", "nden"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "ART", "FM", "FM", "$(", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du suchst mich heiter zu ergr&#252;nden               ", "tokens": ["Du", "suchst", "mich", "hei\u00b7ter", "zu", "ergr", "&#252;", "nden"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wind umweht uns fr&#252;hlingsweich             ", "tokens": ["Ein", "wind", "um\u00b7weht", "uns", "fr", "&#252;", "hlings\u00b7weich"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$(", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die bl&#228;tter die den boden gilben               ", "tokens": ["Die", "bl", "&#228;", "tter", "die", "den", "bo\u00b7den", "gil\u00b7ben"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ADJD", "ART", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbreiten neuen wohlgeruch               ", "tokens": ["Ver\u00b7brei\u00b7ten", "neu\u00b7en", "wohl\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du sprichst mir nach in klugen silben               ", "tokens": ["Du", "sprichst", "mir", "nach", "in", "klu\u00b7gen", "sil\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mich erfreut im bunten buch             ", "tokens": ["Was", "mich", "er\u00b7freut", "im", "bun\u00b7ten", "buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch weisst du auch vom tiefen gl&#252;cke               ", "tokens": ["Doch", "weisst", "du", "auch", "vom", "tie\u00b7fen", "gl", "&#252;", "cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$(", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch&#228;tzest du die stumme thr&#228;ne               ", "tokens": ["Und", "sch", "&#228;", "tzest", "du", "die", "stum\u00b7me", "thr", "&#228;", "ne"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "ADJD", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das auge schattend auf der br&#252;cke               ", "tokens": ["Das", "au\u00b7ge", "schat\u00b7tend", "auf", "der", "br", "&#252;", "cke"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$(", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verfolgest du den zug der schw&#228;ne.             ", "tokens": ["Ver\u00b7fol\u00b7gest", "du", "den", "zug", "der", "schw", "&#228;", "ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}