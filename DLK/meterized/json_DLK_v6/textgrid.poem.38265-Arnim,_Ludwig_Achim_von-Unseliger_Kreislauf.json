{"textgrid.poem.38265": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Unseliger Kreislauf", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl t\u00e4glich will erscheinen", "tokens": ["Wohl", "t\u00e4g\u00b7lich", "will", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6ne Morgenr\u00f6th,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6th", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Thau mu\u00df nieder weinen,", "tokens": ["Den", "Thau", "mu\u00df", "nie\u00b7der", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die weis bekleidet geht,", "tokens": ["Die", "weis", "be\u00b7klei\u00b7det", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Luna ist sie genannt;", "tokens": ["Lu\u00b7na", "ist", "sie", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schneeweis thut sie uns leuchten,", "tokens": ["Schnee\u00b7weis", "thut", "sie", "uns", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Macht uns den Tag bekannt.", "tokens": ["Macht", "uns", "den", "Tag", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber ihr in Wonne", "tokens": ["Und", "\u00fc\u00b7ber", "ihr", "in", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ph\u00f6bus mit Gold bekleidt,", "tokens": ["Ph\u00f6\u00b7bus", "mit", "Gold", "be\u00b7kleidt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das ist die Liebessonne,", "tokens": ["Das", "ist", "die", "Lie\u00b7bes\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alle Welt erfreut;", "tokens": ["Die", "al\u00b7le", "Welt", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jedoch ihr klarer Schein", "tokens": ["Je\u00b7doch", "ihr", "kla\u00b7rer", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll mich nicht gar abwenden,", "tokens": ["Soll", "mich", "nicht", "gar", "ab\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wohl von dem Trauren mein.", "tokens": ["Wohl", "von", "dem", "Trau\u00b7ren", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt auf ihr Sturmwind alle,", "tokens": ["H\u00f6rt", "auf", "ihr", "Sturm\u00b7wind", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wehn vom Himmelsschild,", "tokens": ["Die", "wehn", "vom", "Him\u00b7mels\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist in Sinn gefallen", "tokens": ["Mir", "ist", "in", "Sinn", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein adeliches Bild;", "tokens": ["Ein", "a\u00b7de\u00b7li\u00b7ches", "Bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6flich und tugendreich,", "tokens": ["H\u00f6f\u00b7lich", "und", "tu\u00b7gen\u00b7dreich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Selbst Absalon mu\u00df weichen,", "tokens": ["Selbst", "Ab\u00b7sa\u00b7lon", "mu\u00df", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An Sch\u00f6nheit ihm nichts gleich.", "tokens": ["An", "Sch\u00f6n\u00b7heit", "ihm", "nichts", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Orpheus, der konnte zwingen", "tokens": ["Or\u00b7pheus", ",", "der", "konn\u00b7te", "zwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "VMFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "die wilde Thier im Wald,", "tokens": ["die", "wil\u00b7de", "Thier", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Harfen und sein Singen", "tokens": ["Sein", "Har\u00b7fen", "und", "sein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lockt sie zusammen bald;", "tokens": ["Lockt", "sie", "zu\u00b7sam\u00b7men", "bald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Wild in Fels und Stein", "tokens": ["Das", "Wild", "in", "Fels", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "H\u00f6rt wohl das tiefe Klagen", "tokens": ["H\u00f6rt", "wohl", "das", "tie\u00b7fe", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und gro\u00dfe Trauren mein.", "tokens": ["Und", "gro\u00b7\u00dfe", "Trau\u00b7ren", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00df Orpheus' Saiten hallen,", "tokens": ["S\u00fc\u00df", "Or\u00b7pheus'", "Sai\u00b7ten", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bitter meine Stimm", "tokens": ["Und", "bit\u00b7ter", "mei\u00b7ne", "Stimm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In armer Lieb mu\u00df schallen;", "tokens": ["In", "ar\u00b7mer", "Lieb", "mu\u00df", "schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Venus, la\u00df den Grimm,", "tokens": ["O", "Ve\u00b7nus", ",", "la\u00df", "den", "Grimm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch Lieb des Buhlen dein,", "tokens": ["Durch", "Lieb", "des", "Buh\u00b7len", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Send meinem kranken Herzen", "tokens": ["Send", "mei\u00b7nem", "kran\u00b7ken", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch bald der H\u00fclfe Schein.", "tokens": ["Doch", "bald", "der", "H\u00fcl\u00b7fe", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In mir h\u00f6rt man stets schlagen", "tokens": ["In", "mir", "h\u00f6rt", "man", "stets", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "ADV", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ein unruhige Uhr,", "tokens": ["Ein", "un\u00b7ru\u00b7hi\u00b7ge", "Uhr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder Schlag will klagen", "tokens": ["Und", "je\u00b7der", "Schlag", "will", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um spr\u00f6de Sch\u00f6nheit nur;", "tokens": ["Um", "spr\u00f6\u00b7de", "Sch\u00f6n\u00b7heit", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hoffnung die Uhr zieht auf,", "tokens": ["Hoff\u00b7nung", "die", "Uhr", "zieht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "So geht sie ewig, ewig", "tokens": ["So", "geht", "sie", "e\u00b7wig", ",", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den schmerzlich bittern Lauf.", "tokens": ["Den", "schmerz\u00b7lich", "bit\u00b7tern", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es rennen alle Bronnen", "tokens": ["Es", "ren\u00b7nen", "al\u00b7le", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zusammen in das Meer,", "tokens": ["Zu\u00b7sam\u00b7men", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sind sie hingeronnen,", "tokens": ["Und", "sind", "sie", "hin\u00b7ge\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So kehren sie daher;", "tokens": ["So", "keh\u00b7ren", "sie", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So auch die Seufzer mein", "tokens": ["So", "auch", "die", "Seuf\u00b7zer", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ziehn aus betr\u00fcbtem Herzen,", "tokens": ["Ziehn", "aus", "be\u00b7tr\u00fcb\u00b7tem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und kehren wieder drein.", "tokens": ["Und", "keh\u00b7ren", "wie\u00b7der", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und sterbend schon in Leiden,", "tokens": ["Und", "ster\u00b7bend", "schon", "in", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bitt ich dich auch allein,", "tokens": ["Bitt", "ich", "dich", "auch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wollst mein Herz ausschneiden,", "tokens": ["Du", "wollst", "mein", "Herz", "aus\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und legen in einen Stein;", "tokens": ["Und", "le\u00b7gen", "in", "ei\u00b7nen", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Damit anzeig ich blos,", "tokens": ["Da\u00b7mit", "an\u00b7zeig", "ich", "blos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df dich ein Stein gebohren,", "tokens": ["Da\u00df", "dich", "ein", "Stein", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und nicht des Weibes Schoos.", "tokens": ["Und", "nicht", "des", "Wei\u00b7bes", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "F\u00fcr's andre lasse bauen", "tokens": ["F\u00fcr's", "and\u00b7re", "las\u00b7se", "bau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Gitter ob dem Stein,", "tokens": ["Ein", "Git\u00b7ter", "ob", "dem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jeder k\u00f6nne schauen", "tokens": ["Da\u00df", "je\u00b7der", "k\u00f6n\u00b7ne", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das elend Herze mein;", "tokens": ["Das", "e\u00b7lend", "Her\u00b7ze", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Amor vor der Zeit", "tokens": ["Dem", "A\u00b7mor", "vor", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Lieb und heimlich Leiden", "tokens": ["Durch", "Lieb", "und", "heim\u00b7lich", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Genommen all sein Freud.", "tokens": ["Ge\u00b7nom\u00b7men", "all", "sein", "Freud", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zum dritten ich begehre", "tokens": ["Zum", "drit\u00b7ten", "ich", "be\u00b7geh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begleite mich ins Grab,", "tokens": ["Be\u00b7glei\u00b7te", "mich", "ins", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kr\u00e4nzlein mir verehre,", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "mir", "ver\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von bitterm Kraut Schabab;", "tokens": ["Von", "bit\u00b7term", "Kraut", "Scha\u00b7bab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Leb wohl dies Kraut bedeut,", "tokens": ["Leb", "wohl", "dies", "Kraut", "be\u00b7deut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum wird es auch wohl billig", "tokens": ["Drum", "wird", "es", "auch", "wohl", "bil\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An meinen Leib gestreut.", "tokens": ["An", "mei\u00b7nen", "Leib", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zulezt ich noch begehre,", "tokens": ["Zu\u00b7lezt", "ich", "noch", "be\u00b7geh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du mir trauren sollt,", "tokens": ["Da\u00df", "du", "mir", "trau\u00b7ren", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Veilbraun mir zur Ehre,", "tokens": ["In", "Veil\u00b7braun", "mir", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Farbe war ich hold;", "tokens": ["Der", "Far\u00b7be", "war", "ich", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trug sie im Leben mein,", "tokens": ["Trug", "sie", "im", "Le\u00b7ben", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Veilbraun will nichts bedeuten,", "tokens": ["Veil\u00b7braun", "will", "nichts", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als Lieb und heimlich Pein.", "tokens": ["Als", "Lieb", "und", "heim\u00b7lich", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}