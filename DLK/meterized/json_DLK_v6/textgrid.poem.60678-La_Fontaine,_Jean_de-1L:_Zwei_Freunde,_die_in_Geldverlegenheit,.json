{"textgrid.poem.60678": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Freunde, die in Geldverlegenheit,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Freunde, die in Geldverlegenheit,", "tokens": ["Zwei", "Freun\u00b7de", ",", "die", "in", "Geld\u00b7ver\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verkauften Nachbar K\u00fcrschner eine B\u00e4renhaut.", "tokens": ["Ver\u00b7kauf\u00b7ten", "Nach\u00b7bar", "K\u00fcrschner", "ei\u00b7ne", "B\u00e4\u00b7ren\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zwar hatten sie den B\u00e4ren nur geschaut,", "tokens": ["Zwar", "hat\u00b7ten", "sie", "den", "B\u00e4\u00b7ren", "nur", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch meinten sie, vom Fang sei's nicht mehr weit.", "tokens": ["Doch", "mein\u00b7ten", "sie", ",", "vom", "Fang", "sei's", "nicht", "mehr", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPRART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch hatten sie dem K\u00fcrschner anvertraut,", "tokens": ["Auch", "hat\u00b7ten", "sie", "dem", "K\u00fcrschner", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df solch ein Riesenexemplar von B\u00e4r", "tokens": ["Da\u00df", "solch", "ein", "Rie\u00b7sen\u00b7ex\u00b7emp\u00b7lar", "von", "B\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wohl nicht zum zweitenmal zu finden w\u00e4r.", "tokens": ["Wohl", "nicht", "zum", "zwei\u00b7ten\u00b7mal", "zu", "fin\u00b7den", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPRART", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Winter, sagten sie, der werde heuer kalt,", "tokens": ["Der", "Win\u00b7ter", ",", "sag\u00b7ten", "sie", ",", "der", "wer\u00b7de", "heu\u00b7er", "kalt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und f\u00fcr zwei M\u00e4ntel reiche wohl das Fell.", "tokens": ["Und", "f\u00fcr", "zwei", "M\u00e4n\u00b7tel", "rei\u00b7che", "wohl", "das", "Fell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie lobten ihren B\u00e4ren dergestalt,", "tokens": ["Sie", "lob\u00b7ten", "ih\u00b7ren", "B\u00e4\u00b7ren", "der\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Als h\u00e4tten sie ihn schon zur Stell.", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", "ihn", "schon", "zur", "Stell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In sp\u00e4testens zwei Tagen wollten sie ihn bringen.", "tokens": ["In", "sp\u00e4\u00b7tes\u00b7tens", "zwei", "Ta\u00b7gen", "woll\u00b7ten", "sie", "ihn", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "CARD", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie nehmen ihren Lohn und eilen in den Wald", "tokens": ["Sie", "neh\u00b7men", "ih\u00b7ren", "Lohn", "und", "ei\u00b7len", "in", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und finden auch den B\u00e4ren bald,", "tokens": ["Und", "fin\u00b7den", "auch", "den", "B\u00e4\u00b7ren", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der schnurgerade auf sie zugelaufen kommt.", "tokens": ["Der", "schnur\u00b7ge\u00b7ra\u00b7de", "auf", "sie", "zu\u00b7ge\u00b7lau\u00b7fen", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man f\u00fchlt, hier gibt es kein Entspringen!", "tokens": ["Man", "f\u00fchlt", ",", "hier", "gibt", "es", "kein", "Ent\u00b7sprin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der eine findet, was ihm frommt,", "tokens": ["Der", "ei\u00b7ne", "fin\u00b7det", ",", "was", "ihm", "frommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und klettert hoch in einen Baum hinein.", "tokens": ["Und", "klet\u00b7tert", "hoch", "in", "ei\u00b7nen", "Baum", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der andre, der kein Glied mehr r\u00fchren kann,", "tokens": ["Der", "and\u00b7re", ",", "der", "kein", "Glied", "mehr", "r\u00fch\u00b7ren", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wirft sich zur Erde, h\u00e4lt den Atem an", "tokens": ["Wirft", "sich", "zur", "Er\u00b7de", ",", "h\u00e4lt", "den", "A\u00b7tem", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.21": {"text": "Und stellt sich tot; denn grade f\u00e4llt ihm ein,", "tokens": ["Und", "stellt", "sich", "tot", ";", "denn", "gra\u00b7de", "f\u00e4llt", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "$.", "KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Da\u00df oft die Rede geht: der B\u00e4r verschont", "tokens": ["Da\u00df", "oft", "die", "Re\u00b7de", "geht", ":", "der", "B\u00e4r", "ver\u00b7schont"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Den regungslosen Leib, in dem kein Leben wohnt.", "tokens": ["Den", "re\u00b7gungs\u00b7lo\u00b7sen", "Leib", ",", "in", "dem", "kein", "Le\u00b7ben", "wohnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.24": {"text": "Und Meister Petz f\u00e4llt wirklich drauf herein.", "tokens": ["Und", "Meis\u00b7ter", "Petz", "f\u00e4llt", "wirk\u00b7lich", "drauf", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Er sieht den K\u00f6rper, dem das Lebenslicht entflohn", "tokens": ["Er", "sieht", "den", "K\u00f6r\u00b7per", ",", "dem", "das", "Le\u00b7bens\u00b7licht", "ent\u00b7flohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und denkt: Der Mann ist tot \u2013 wennschon", "tokens": ["Und", "denkt", ":", "Der", "Mann", "ist", "tot", "\u2013", "wenn\u00b7schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$(", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es m\u00f6glich ist, da\u00df er sich nur verstellt.", "tokens": ["Es", "m\u00f6g\u00b7lich", "ist", ",", "da\u00df", "er", "sich", "nur", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Weshalb er es f\u00fcr ratsam h\u00e4lt,", "tokens": ["We\u00b7shalb", "er", "es", "f\u00fcr", "rat\u00b7sam", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Den Leichnam umzudrehn und zu bef\u00fchlen", "tokens": ["Den", "Leich\u00b7nam", "um\u00b7zu\u00b7drehn", "und", "zu", "be\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVIZU", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und ihm die Schnauze ins Gesicht zu w\u00fchlen,", "tokens": ["Und", "ihm", "die", "Schnau\u00b7ze", "ins", "Ge\u00b7sicht", "zu", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Um festzustellen, ob der Atem fort.", "tokens": ["Um", "fest\u00b7zu\u00b7stel\u00b7len", ",", "ob", "der", "A\u00b7tem", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "\u00bbwahrhaftig, eine Leiche! Wie sie stinkt!\u00ab", "tokens": ["\u00bb", "wahr\u00b7haf\u00b7tig", ",", "ei\u00b7ne", "Lei\u00b7che", "!", "Wie", "sie", "stinkt", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ART", "NN", "$.", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Brummt er, als er von dannen hinkt.", "tokens": ["Brummt", "er", ",", "als", "er", "von", "dan\u00b7nen", "hinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Der andre der Gesellen dort,", "tokens": ["Der", "and\u00b7re", "der", "Ge\u00b7sel\u00b7len", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Der auf dem Baum, steigt nun herunter,", "tokens": ["Der", "auf", "dem", "Baum", ",", "steigt", "nun", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "L\u00e4uft hin zu seinem Freund, begr\u00fc\u00dft ihn munter", "tokens": ["L\u00e4uft", "hin", "zu", "sei\u00b7nem", "Freund", ",", "be\u00b7gr\u00fc\u00dft", "ihn", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und meint entz\u00fcckt, wie sch\u00f6n es sei,", "tokens": ["Und", "meint", "ent\u00b7z\u00fcckt", ",", "wie", "sch\u00f6n", "es", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df die entsetzliche Gefahr vorbei", "tokens": ["Da\u00df", "die", "ent\u00b7setz\u00b7li\u00b7che", "Ge\u00b7fahr", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und ihre Qualen nur in etwas Angst bestanden.", "tokens": ["Und", "ih\u00b7re", "Qua\u00b7len", "nur", "in", "et\u00b7was", "Angst", "be\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "\u00bbja, allerdings,\u00ab f\u00fcgt er hinzu, \u00bbdas Fell des B\u00e4ren...", "tokens": ["\u00bb", "ja", ",", "al\u00b7ler\u00b7dings", ",", "\u00ab", "f\u00fcgt", "er", "hin\u00b7zu", ",", "\u00bb", "das", "Fell", "des", "B\u00e4\u00b7ren", "..."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "$,", "$(", "VVFIN", "PPER", "PTKVZ", "$,", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch bitte, willst du mir erkl\u00e4ren,", "tokens": ["Doch", "bit\u00b7te", ",", "willst", "du", "mir", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Welch ein Geheimnis dir Herr Petz gestanden,", "tokens": ["Welch", "ein", "Ge\u00b7heim\u00b7nis", "dir", "Herr", "Petz", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Als er so nah dir ins Gesicht geschaut?\u00ab", "tokens": ["Als", "er", "so", "nah", "dir", "ins", "Ge\u00b7sicht", "ge\u00b7schaut", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "\u00bber sagte nur, man solle nie die Haut", "tokens": ["\u00bb", "er", "sag\u00b7te", "nur", ",", "man", "sol\u00b7le", "nie", "die", "Haut"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "PIS", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Des B\u00e4ren schon zu Markte tragen,", "tokens": ["Des", "B\u00e4\u00b7ren", "schon", "zu", "Mark\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Eh man nicht ihn, den B\u00e4r, erschlagen.\u00ab", "tokens": ["Eh", "man", "nicht", "ihn", ",", "den", "B\u00e4r", ",", "er\u00b7schla\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$.", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}}}}