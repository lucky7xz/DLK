{"textgrid.poem.57687": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warm sind die leisen L\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warm sind die leisen L\u00fcfte,", "tokens": ["Warm", "sind", "die", "lei\u00b7sen", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Zweige sind bl\u00fctenschwer,", "tokens": ["Die", "Zwei\u00b7ge", "sind", "bl\u00fc\u00b7ten\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unstet treibt auf dem Rasen", "tokens": ["Un\u00b7stet", "treibt", "auf", "dem", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ein welkes Blatt einher.", "tokens": ["Ein", "wel\u00b7kes", "Blatt", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus den bl\u00fchenden Veilchen", "tokens": ["Aus", "den", "bl\u00fc\u00b7hen\u00b7den", "Veil\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "St\u00f6\u00dft es der Wind in den Sand,", "tokens": ["St\u00f6\u00dft", "es", "der", "Wind", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "So war einst meine Seele", "tokens": ["So", "war", "einst", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eh sie die deinige fand.", "tokens": ["Eh", "sie", "die", "dei\u00b7ni\u00b7ge", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PPOSS", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}