{"textgrid.poem.33292": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Lied, in Abwesenheit des Geliebten zu singen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Teuthold, mein Trauter, ist gangen von hier,", "tokens": ["Teut\u00b7hold", ",", "mein", "Trau\u00b7ter", ",", "ist", "gan\u00b7gen", "von", "hier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "APPR", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "W\u00e4lder und Berge verbergen ihn mir;", "tokens": ["W\u00e4l\u00b7der", "und", "Ber\u00b7ge", "ver\u00b7ber\u00b7gen", "ihn", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sonst wohl erzielte noch fern ihn mein Blick:", "tokens": ["Sonst", "wohl", "er\u00b7ziel\u00b7te", "noch", "fern", "ihn", "mein", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Winkt' ich, dann winkt' er mir wieder zur\u00fcck.", "tokens": ["Winkt'", "ich", ",", "dann", "winkt'", "er", "mir", "wie\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00e4h' ich ihn jetzt des Maienmonds freu'n,", "tokens": ["S\u00e4h'", "ich", "ihn", "jetzt", "des", "Mai\u00b7en\u00b7monds", "freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "W\u00e4re die H\u00e4lfte der Freuden auch mein;", "tokens": ["W\u00e4\u00b7re", "die", "H\u00e4lf\u00b7te", "der", "Freu\u00b7den", "auch", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "PPOSAT", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Pfl\u00fcckt' er ein Bl\u00fcmchen, so pfl\u00fcckt' er es mir;", "tokens": ["Pfl\u00fcckt'", "er", "ein", "Bl\u00fcm\u00b7chen", ",", "so", "pfl\u00fcckt'", "er", "es", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "S\u00e4ng' er ein Liedchen, so s\u00e4ng' er es mir.", "tokens": ["S\u00e4ng'", "er", "ein", "Lied\u00b7chen", ",", "so", "s\u00e4ng'", "er", "es", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00e4h' ich ihn wandeln im traulichen Wald,", "tokens": ["S\u00e4h'", "ich", "ihn", "wan\u00b7deln", "im", "trau\u00b7li\u00b7chen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "H\u00f6rt' ich des Sehnenden Seufzen gar bald:", "tokens": ["H\u00f6rt'", "ich", "des", "Seh\u00b7nen\u00b7den", "Seuf\u00b7zen", "gar", "bald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Liebend, allliebend umfing ich ihn dann,", "tokens": ["Lie\u00b7bend", ",", "all\u00b7lie\u00b7bend", "um\u00b7fing", "ich", "ihn", "dann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Schmiegt' an den Trauten mich inniglich an.", "tokens": ["Schmiegt'", "an", "den", "Trau\u00b7ten", "mich", "in\u00b7nig\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4tt' ich, o h\u00e4tt' ich doch Feengewalt,", "tokens": ["H\u00e4tt'", "ich", ",", "o", "h\u00e4tt'", "ich", "doch", "Fe\u00b7en\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Mich zu verwandeln in jede Gestalt,", "tokens": ["Mich", "zu", "ver\u00b7wan\u00b7deln", "in", "je\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich ihm spielen manch' wunderlich Spiel,", "tokens": ["K\u00f6nnt'", "ich", "ihm", "spie\u00b7len", "man\u00b7ch'", "wun\u00b7der\u00b7lich", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "O, wie gen\u00f6ss' ich der Freuden so viel!", "tokens": ["O", ",", "wie", "ge\u00b7n\u00f6\u00b7ss'", "ich", "der", "Freu\u00b7den", "so", "viel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Ging' er stilldenkend am k\u00fchlenden Bach,", "tokens": ["Ging'", "er", "still\u00b7den\u00b7kend", "am", "k\u00fch\u00b7len\u00b7den", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schw\u00e4mm' ihm ein Bl\u00fcmchen Vergi\u00dfmeinnicht nach;", "tokens": ["Schw\u00e4mm'", "ihm", "ein", "Bl\u00fcm\u00b7chen", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Hascht' er das Bl\u00fcmchen, und n\u00e4hm' es zu sich,", "tokens": ["Hascht'", "er", "das", "Bl\u00fcm\u00b7chen", ",", "und", "n\u00e4hm'", "es", "zu", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "PRF", "$,"], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "H\u00e4tt' er in liebenden H\u00e4nden dann mich.", "tokens": ["H\u00e4tt'", "er", "in", "lie\u00b7ben\u00b7den", "H\u00e4n\u00b7den", "dann", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Sucht er im Schatten der Linde sich Ruh'", "tokens": ["Sucht", "er", "im", "Schat\u00b7ten", "der", "Lin\u00b7de", "sich", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NE", "PRF", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Deckt' ich mit duftenden Bl\u00e4ttern ihn zu;", "tokens": ["Deckt'", "ich", "mit", "duf\u00b7ten\u00b7den", "Bl\u00e4t\u00b7tern", "ihn", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ging er auf Blumengefilden einher,", "tokens": ["Ging", "er", "auf", "Blu\u00b7men\u00b7ge\u00b7fil\u00b7den", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Fl\u00f6g' ich als Schmetterling rund um ihn her.", "tokens": ["Fl\u00f6g'", "ich", "als", "Schmet\u00b7ter\u00b7ling", "rund", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "F\u00fcgt' er zu B\u00fcchern in's K\u00e4mmerlein sich,", "tokens": ["F\u00fcgt'", "er", "zu", "B\u00fc\u00b7chern", "in's", "K\u00e4m\u00b7mer\u00b7lein", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "PRF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Setzt' ich an's Fenster als Nachtigall mich,", "tokens": ["Setzt'", "ich", "an's", "Fens\u00b7ter", "als", "Nach\u00b7ti\u00b7gall", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KOUS", "NN", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "S\u00e4nge sein eigenes Liedchen ihm vor:", "tokens": ["S\u00e4n\u00b7ge", "sein", "ei\u00b7ge\u00b7nes", "Lied\u00b7chen", "ihm", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "W\u00fcrd' er nicht lauschen und spitzen sein Ohr?", "tokens": ["W\u00fcrd'", "er", "nicht", "lau\u00b7schen", "und", "spit\u00b7zen", "sein", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Br\u00e4chte mein liebendes, sehnendes Ach", "tokens": ["Br\u00e4ch\u00b7te", "mein", "lie\u00b7ben\u00b7des", ",", "seh\u00b7nen\u00b7des", "Ach"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Doch ein gef\u00e4lliger Zephyr ihm nach!", "tokens": ["Doch", "ein", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", "Ze\u00b7phyr", "ihm", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4re nur leicht und gefl\u00fcgelt mein Ku\u00df,", "tokens": ["W\u00e4\u00b7re", "nur", "leicht", "und", "ge\u00b7fl\u00fc\u00b7gelt", "mein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Br\u00e4cht' er wohl st\u00fcndlich ihm freundlichen Gru\u00df.", "tokens": ["Br\u00e4cht'", "er", "wohl", "st\u00fcnd\u00b7lich", "ihm", "freund\u00b7li\u00b7chen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}