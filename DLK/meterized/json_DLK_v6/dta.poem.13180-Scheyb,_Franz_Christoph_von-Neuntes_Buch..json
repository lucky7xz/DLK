{"dta.poem.13180": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem Thalia mir von diesem Helden sprach,", "tokens": ["In\u00b7dem", "Tha\u00b7lia", "mir", "von", "die\u00b7sem", "Hel\u00b7den", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "510Bewog mein Auge sich dem Frauenzimmer nach:", "tokens": ["mein", "Au\u00b7ge", "sich", "dem", "Frau\u00b7en\u00b7zim\u00b7mer", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald h\u00f6rt\u2019 ich ihr Gespr\u00e4ch, bald sah\u2019 ich durch die Reihen,", "tokens": ["Bald", "h\u00f6rt'", "ich", "ihr", "Ge\u00b7spr\u00e4ch", ",", "bald", "sah'", "ich", "durch", "die", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und liesse meinen Sinn durch ihre Tracht zerstreuen.", "tokens": ["Und", "lies\u00b7se", "mei\u00b7nen", "Sinn", "durch", "ih\u00b7re", "Tracht", "zer\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer hat doch, fiel mir ein, die bunte Kleider-Pracht,", "tokens": ["Wer", "hat", "doch", ",", "fiel", "mir", "ein", ",", "die", "bun\u00b7te", "Klei\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Farben Lieblichkeit, den Seiden-Stoff erdacht?", "tokens": ["Der", "Far\u00b7ben", "Lieb\u00b7lich\u00b7keit", ",", "den", "Sei\u00b7den\u00b7Stoff", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "515Die doch der Sch\u00f6nheit nichts zu besserm Vortheil geben;", "tokens": ["doch", "der", "Sch\u00f6n\u00b7heit", "nichts", "zu", "bes\u00b7serm", "Vor\u00b7theil", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIS", "PTKZU", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Vielmehr dadurch sich selbst in ihrem Pomp erheben.", "tokens": ["Viel\u00b7mehr", "da\u00b7durch", "sich", "selbst", "in", "ih\u00b7rem", "Pomp", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So kostbar jene sich in ihrem Prangen wies,", "tokens": ["So", "kost\u00b7bar", "je\u00b7ne", "sich", "in", "ih\u00b7rem", "Pran\u00b7gen", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So war nichts, das an ihr sch\u00f6n oder reizend lie\u00df\u2019.", "tokens": ["So", "war", "nichts", ",", "das", "an", "ihr", "sch\u00f6n", "o\u00b7der", "rei\u00b7zend", "lie\u00df'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PRELS", "APPR", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wogegen andere nur mit dem Anblick spielten,", "tokens": ["Wo\u00b7ge\u00b7gen", "an\u00b7de\u00b7re", "nur", "mit", "dem", "An\u00b7blick", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "520Doch ohne Pracht ein Aug in der Entz\u00fcckung hielten.", "tokens": ["oh\u00b7ne", "Pracht", "ein", "Aug", "in", "der", "Ent\u00b7z\u00fc\u00b7ckung", "hiel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Was Wunder, wann am Hof, dacht\u2019 ich, ein Streit entsteht;", "tokens": ["Was", "Wun\u00b7der", ",", "wann", "am", "Hof", ",", "dacht'", "ich", ",", "ein", "Streit", "ent\u00b7steht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil man die Sch\u00f6nen sucht, die minderen verschm\u00e4ht;", "tokens": ["Weil", "man", "die", "Sch\u00f6\u00b7nen", "sucht", ",", "die", "min\u00b7de\u00b7ren", "ver\u00b7schm\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich wei\u00df nicht, wie mein Aug und Sinn besch\u00e4ftigt waren;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mein", "Aug", "und", "Sinn", "be\u00b7sch\u00e4f\u00b7tigt", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOKOM", "PPOSAT", "NN", "KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich mu\u00dft auf jeden Blick was pr\u00e4chtigers erfahren.", "tokens": ["Ich", "mu\u00dft", "auf", "je\u00b7den", "Blick", "was", "pr\u00e4ch\u00b7ti\u00b7gers", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}