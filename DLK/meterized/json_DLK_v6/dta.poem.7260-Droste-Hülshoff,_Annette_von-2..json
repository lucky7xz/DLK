{"dta.poem.7260": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "An bl\u00fchender Akazie lehnt ein blonder bleicher Mann,", "tokens": ["An", "bl\u00fc\u00b7hen\u00b7der", "A\u00b7ka\u00b7zie", "lehnt", "ein", "blon\u00b7der", "blei\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sehr mangelt ihm der Sitz, allein die Kinder spielen dran,", "tokens": ["Sehr", "man\u00b7gelt", "ihm", "der", "Sitz", ",", "al\u00b7lein", "die", "Kin\u00b7der", "spie\u00b7len", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "So schreibt er stehend, immer Ball und Peitschenhieb ge-", "tokens": ["So", "schreibt", "er", "ste\u00b7hend", ",", "im\u00b7mer", "Ball", "und", "Peit\u00b7schen\u00b7hieb", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "w\u00e4rt'gend,", "tokens": ["w\u00e4rt'\u00b7gend", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Schnellfingrig f\u00fcr die Druckerei den L\u00fcckenb\u00fc\u00dfer fert'gend.", "tokens": ["Schnell\u00b7fing\u00b7rig", "f\u00fcr", "die", "Dru\u00b7cke\u00b7rei", "den", "L\u00fc\u00b7cken\u00b7b\u00fc\u00b7\u00dfer", "fert'\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "\u201ein Osten steigt das junge Licht, es rauscht im Eichenhain,", "tokens": ["\u201e", "in", "Os\u00b7ten", "steigt", "das", "jun\u00b7ge", "Licht", ",", "es", "rauscht", "im", "Ei\u00b7chen\u00b7hain", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Schon schlang der alte Erebus die alten Schatten ein,", "tokens": ["Schon", "schlang", "der", "al\u00b7te", "E\u00b7re\u00b7bus", "die", "al\u00b7ten", "Schat\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Des Geistes Siegel sind gel\u00f6st, der Aether aufgeschlossen,", "tokens": ["Des", "Geis\u00b7tes", "Sie\u00b7gel", "sind", "ge\u00b7l\u00f6st", ",", "der", "A\u00b7e\u00b7ther", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Und aus vermorschter Dogmen Staub lebend'ge Cedern sprossen.", "tokens": ["Und", "aus", "ver\u00b7morschter", "Dog\u00b7men", "Staub", "le\u00b7ben\u00b7d'\u00b7ge", "Ce\u00b7dern", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-++-+--+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "O Geistesfessel, h\u00e4rter du als jemals ein Tyrann,", "tokens": ["O", "Geis\u00b7tes\u00b7fes\u00b7sel", ",", "h\u00e4r\u00b7ter", "du", "als", "je\u00b7mals", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "PPER", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Geschlagen um des Sclaven Leib, du tausendj\u00e4hr'ger Bann!", "tokens": ["Ge\u00b7schla\u00b7gen", "um", "des", "Scla\u00b7ven", "Leib", ",", "du", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ger", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Geheim doch sicher hat der Rost genagt an deinem Ringe,", "tokens": ["Ge\u00b7heim", "doch", "si\u00b7cher", "hat", "der", "Rost", "ge\u00b7nagt", "an", "dei\u00b7nem", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Nun wackelt er und f\u00fcrchtet sich vor jedes Knaben Klinge!", "tokens": ["Nun", "wa\u00b7ckelt", "er", "und", "f\u00fcrch\u00b7tet", "sich", "vor", "je\u00b7des", "Kna\u00b7ben", "Klin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Hin ist die Zeit wo ein Gespenst im B\u00fc\u00dfermantel schlich,", "tokens": ["Hin", "ist", "die", "Zeit", "wo", "ein", "Ge\u00b7spenst", "im", "B\u00fc\u00b7\u00dfer\u00b7man\u00b7tel", "schlich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PWAV", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "In seinen Bettelsack des Deutschen Gold und Ehre strich,", "tokens": ["In", "sei\u00b7nen", "Bet\u00b7tel\u00b7sack", "des", "Deut\u00b7schen", "Gold", "und", "Eh\u00b7re", "strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wo Greise, Schulmonarchen gleich, die stumpfe Gei\u00dfel", "tokens": ["Wo", "Grei\u00b7se", ",", "Schul\u00b7mon\u00b7ar\u00b7chen", "gleich", ",", "die", "stump\u00b7fe", "Gei\u00b7\u00dfel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "schwenkten,", "tokens": ["schwenk\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Des Sonnenrosses Zaum dem Grab verfallne H\u00e4nde lenkten.", "tokens": ["Des", "Son\u00b7nen\u00b7ros\u00b7ses", "Zaum", "dem", "Grab", "ver\u00b7fall\u00b7ne", "H\u00e4n\u00b7de", "lenk\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Nicht wird im zarten Kinde mehr des Mannes Keim erstickt,", "tokens": ["Nicht", "wird", "im", "zar\u00b7ten", "Kin\u00b7de", "mehr", "des", "Man\u00b7nes", "Keim", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "APPRART", "ADJA", "NN", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Frei schie\u00dft die Eichenlode, unbeengt und ungeknickt;", "tokens": ["Frei", "schie\u00dft", "die", "Ei\u00b7chen\u00b7lo\u00b7de", ",", "un\u00b7be\u00b7engt", "und", "un\u00b7ge\u00b7knickt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Was mehr als Wissen, wirkender als Gaben, die zerst\u00fcckelt \u2014", "tokens": ["Was", "mehr", "als", "Wis\u00b7sen", ",", "wir\u00b7ken\u00b7der", "als", "Ga\u00b7ben", ",", "die", "zer\u00b7st\u00fc\u00b7ckelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "NN", "$,", "PRELS", "KOUS", "NN", "$,", "PRELS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Des kr\u00e4ftgen Wollens Einheit wird im jungen Mark ent-", "tokens": ["Des", "kr\u00e4ft\u00b7gen", "Wol\u00b7lens", "Ein\u00b7heit", "wird", "im", "jun\u00b7gen", "Mark", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wickelt.", "tokens": ["wi\u00b7ckelt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Und dennoch ri\u00df das Document vom schn\u00f6den Seelenkauf", "tokens": ["Und", "den\u00b7noch", "ri\u00df", "das", "Do\u00b7cu\u00b7ment", "vom", "schn\u00f6\u00b7den", "See\u00b7len\u00b7kauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Durch deutsche Hand, durch unsre Hand, die, nach Egyptens", "tokens": ["Durch", "deut\u00b7sche", "Hand", ",", "durch", "uns\u00b7re", "Hand", ",", "die", ",", "nach", "E\u00b7gyp\u00b7tens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PRELS", "$,", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch immer stark genug den Brand an's Bagnothor zu", "tokens": ["Noch", "im\u00b7mer", "stark", "ge\u00b7nug", "den", "Brand", "an's", "Bag\u00b7no\u00b7thor", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch ihr, die ihr den ganzen Saft der Muttererde trinkt,", "tokens": ["Doch", "ihr", ",", "die", "ihr", "den", "gan\u00b7zen", "Saft", "der", "Mut\u00b7ter\u00b7er\u00b7de", "trinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "An deren Zweig das erste Blatt schon wie Smaragde blinkt,", "tokens": ["An", "de\u00b7ren", "Zweig", "das", "ers\u00b7te", "Blatt", "schon", "wie", "Sma\u00b7rag\u00b7de", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "ADV", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr!\u201c \u2014 unser Dichter stutzt \u2014 er h\u00f6rt an den Hollunder-", "tokens": ["Ihr", "!", "\u201c", "un\u00b7ser", "Dich\u00b7ter", "stutzt", "er", "h\u00f6rt", "an", "den", "Hol\u00b7lun\u00b7der"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$.", "$(", "$(", "PPOSAT", "NN", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Erstlingsreis, den G\u00f6ttinger, wie eine Walze keuchen.", "tokens": ["Sein", "Erst\u00b7lings\u00b7reis", ",", "den", "G\u00f6t\u00b7tin\u00b7ger", ",", "wie", "ei\u00b7ne", "Wal\u00b7ze", "keu\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Und auf der Bank \u2014 sein Manuscript \u2014 o Pest! sein Dichter-", "tokens": ["Und", "auf", "der", "Bank", "sein", "Ma\u00b7nus\u00b7cript", "o", "Pest", "!", "sein", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "FM", "FM", "$.", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort fliegt er, droben in der Luft, als langer Drachen-", "tokens": ["Dort", "fliegt", "er", ",", "dro\u00b7ben", "in", "der", "Luft", ",", "als", "lan\u00b7ger", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und \u2014 was? ein Gu\u00df? \u2014 bei Gott, da h\u00e4ngt der Bub',", "tokens": ["Und", "was", "?", "ein", "Gu\u00df", "?", "bei", "Gott", ",", "da", "h\u00e4ngt", "der", "Bub'", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "$.", "ART", "NN", "$.", "$(", "APPR", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Am Ast, und leert den Wasserkrug auf seines Vaters Glatze!", "tokens": ["Am", "Ast", ",", "und", "leert", "den", "Was\u00b7ser\u00b7krug", "auf", "sei\u00b7nes", "Va\u00b7ters", "Glat\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}