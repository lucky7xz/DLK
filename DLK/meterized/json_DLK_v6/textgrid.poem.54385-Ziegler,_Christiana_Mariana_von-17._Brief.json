{"textgrid.poem.54385": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "17. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund! dein h\u00f6flich seyn ermuntert mich zum Dichten.", "tokens": ["Mein", "Freund", "!", "dein", "h\u00f6f\u00b7lich", "seyn", "er\u00b7mun\u00b7tert", "mich", "zum", "Dich\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "ADJD", "VAINF", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du fragst, so mu\u00df ich dir von meiner Noth berichten:", "tokens": ["Du", "fragst", ",", "so", "mu\u00df", "ich", "dir", "von", "mei\u00b7ner", "Noth", "be\u00b7rich\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schon gestern legt ich mich, der Frost befiel das Herz.", "tokens": ["Schon", "ge\u00b7stern", "legt", "ich", "mich", ",", "der", "Frost", "be\u00b7fiel", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was empfand ich nicht f\u00fcr ungewohnten Schmerz!", "tokens": ["Und", "was", "emp\u00b7fand", "ich", "nicht", "f\u00fcr", "un\u00b7ge\u00b7wohn\u00b7ten", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Fiebers heisse Glut durchzog mir alle Glieder;", "tokens": ["Des", "Fie\u00b7bers", "heis\u00b7se", "Glut", "durch\u00b7zog", "mir", "al\u00b7le", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch Hoffnung und Geduld erholt ich mich doch wieder.", "tokens": ["Durch", "Hoff\u00b7nung", "und", "Ge\u00b7duld", "er\u00b7holt", "ich", "mich", "doch", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Seele leidet nicht, der Schmerz trifft nur den Leib;", "tokens": ["Die", "See\u00b7le", "lei\u00b7det", "nicht", ",", "der", "Schmerz", "trifft", "nur", "den", "Leib", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier mu\u00df man m\u00e4nnlich seyn, und kein verzagtes Weib.", "tokens": ["Hier", "mu\u00df", "man", "m\u00e4nn\u00b7lich", "seyn", ",", "und", "kein", "ver\u00b7zag\u00b7tes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VAINF", "$,", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das w\u00e4r ein sch\u00f6ner Streich, betr\u00e4f es unsre Nymphen,", "tokens": ["Das", "w\u00e4r", "ein", "sch\u00f6\u00b7ner", "Streich", ",", "be\u00b7tr\u00e4f", "es", "uns\u00b7re", "Nym\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.10": {"text": "Sie w\u00fcrden vor Verdru\u00df die M\u00e4uler trefflich r\u00fcmpfen.", "tokens": ["Sie", "w\u00fcr\u00b7den", "vor", "Ver\u00b7dru\u00df", "die", "M\u00e4u\u00b7ler", "treff\u00b7lich", "r\u00fcmp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer s\u00e4sse bey dem Rahm, n\u00e4hm Complimenten ein?", "tokens": ["Wer", "s\u00e4s\u00b7se", "bey", "dem", "Rahm", ",", "n\u00e4hm", "Com\u00b7pli\u00b7men\u00b7ten", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die Sonne traurte selbst um ihren Gegenschein.", "tokens": ["Die", "Son\u00b7ne", "traur\u00b7te", "selbst", "um", "ih\u00b7ren", "Ge\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dergleichen Unfall darf auch Staxen nicht befallen.", "tokens": ["Derg\u00b7lei\u00b7chen", "Un\u00b7fall", "darf", "auch", "Sta\u00b7xen", "nicht", "be\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VMFIN", "ADV", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo h\u00f6rte man den Ku\u00df von seinen Lippen schallen?", "tokens": ["Wo", "h\u00f6r\u00b7te", "man", "den", "Ku\u00df", "von", "sei\u00b7nen", "Lip\u00b7pen", "schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Gott! wie gr\u00e4mte sich die alte Sch\u00e4fermagd!", "tokens": ["Mein", "Gott", "!", "wie", "gr\u00e4m\u00b7te", "sich", "die", "al\u00b7te", "Sch\u00e4\u00b7fer\u00b7magd", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von andern w\u00fcrd ihr nicht was sch\u00f6nes vorgesagt.", "tokens": ["Von", "an\u00b7dern", "w\u00fcrd", "ihr", "nicht", "was", "sch\u00f6\u00b7nes", "vor\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "PTKNEG", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein Freund, mein werther Freund, mein Trost bey allen Plagen", "tokens": ["Mein", "Freund", ",", "mein", "wert\u00b7her", "Freund", ",", "mein", "Trost", "bey", "al\u00b7len", "Pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein unverf\u00e4lschter Sinn, dein Mitleid, dein Beklagen", "tokens": ["Dein", "un\u00b7ver\u00b7f\u00e4lschter", "Sinn", ",", "dein", "Mit\u00b7leid", ",", "dein", "Be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.19": {"text": "Ertheilt mir Muth und Kraft, ich trotze aller Noth", "tokens": ["Er\u00b7theilt", "mir", "Muth", "und", "Kraft", ",", "ich", "trot\u00b7ze", "al\u00b7ler", "Noth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und kann nicht sch\u00fcchtern seyn wenn mir das Fieber droht.", "tokens": ["Und", "kann", "nicht", "sch\u00fcch\u00b7tern", "seyn", "wenn", "mir", "das", "Fie\u00b7ber", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich brauche schon den Arzt, und seine Arzeneyen;", "tokens": ["Ich", "brau\u00b7che", "schon", "den", "Arzt", ",", "und", "sei\u00b7ne", "Ar\u00b7ze\u00b7ne\u00b7yen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.22": {"text": "Der soll den K\u00f6rper auch von seinem Schmerz befreyen.", "tokens": ["Der", "soll", "den", "K\u00f6r\u00b7per", "auch", "von", "sei\u00b7nem", "Schmerz", "be\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.23": {"text": "An statt des Dankes nimm itzt die Versicherung hin,", "tokens": ["An", "statt", "des", "Dan\u00b7kes", "nimm", "itzt", "die", "Ver\u00b7si\u00b7che\u00b7rung", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVIMP", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Da\u00df ich dir stets geneigt und deine Freundinn bin.", "tokens": ["Da\u00df", "ich", "dir", "stets", "ge\u00b7neigt", "und", "dei\u00b7ne", "Freun\u00b7dinn", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}