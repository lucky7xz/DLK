{"textgrid.poem.49031": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "An Nemesis", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist's, das mitten", "tokens": ["Was", "ist's", ",", "das", "mit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Freudenfluge scharf und bitter,", "tokens": ["Im", "Freu\u00b7den\u00b7flu\u00b7ge", "scharf", "und", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Winters strengem Odem gleich, das Herz", "tokens": ["Des", "Win\u00b7ters", "stren\u00b7gem", "O\u00b7dem", "gleich", ",", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Belastet und im trunkensten Genusse", "tokens": ["Be\u00b7las\u00b7tet", "und", "im", "trun\u00b7kens\u00b7ten", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Den Fl\u00fcgel l\u00e4hmt? Ist's vom Orcus", "tokens": ["Den", "Fl\u00fc\u00b7gel", "l\u00e4hmt", "?", "Ist's", "vom", "Or\u00b7cus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Hohn? Ha, oder ist's der Klang", "tokens": ["Der", "Hohn", "?", "Ha", ",", "o\u00b7der", "ist's", "der", "Klang"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von deinem furchtbarn Ma\u00df, o Nemesis,", "tokens": ["Von", "dei\u00b7nem", "furcht\u00b7barn", "Ma\u00df", ",", "o", "Ne\u00b7me\u00b7sis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vor blinden, \u00fcber\u00fcppigen Begierden warnend?", "tokens": ["Vor", "blin\u00b7den", ",", "\u00fc\u00b7be\u00b7r\u00fcp\u00b7pi\u00b7gen", "Be\u00b7gier\u00b7den", "war\u00b7nend", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn o, zu ausgelassner Mut ist f\u00fcrchterlich.", "tokens": ["Denn", "o", ",", "zu", "aus\u00b7ge\u00b7lass\u00b7ner", "Mut", "ist", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$,", "APPR", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir flehen, flehen,", "tokens": ["Wir", "fle\u00b7hen", ",", "fle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "O Nemesis, zu dir!", "tokens": ["O", "Ne\u00b7me\u00b7sis", ",", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhell' die d\u00fcstre Wolke, die zu schwer", "tokens": ["Er\u00b7hell'", "die", "d\u00fcst\u00b7re", "Wol\u00b7ke", ",", "die", "zu", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PTKA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Aug' des Sterblichen umh\u00fcllet!", "tokens": ["Das", "Aug'", "des", "Sterb\u00b7li\u00b7chen", "um\u00b7h\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O zeig' uns klar die sich're Bahn,", "tokens": ["O", "zeig'", "uns", "klar", "die", "sich'\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhabne G\u00f6ttin, die du m\u00e4chtig", "tokens": ["Er\u00b7hab\u00b7ne", "G\u00f6t\u00b7tin", ",", "die", "du", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Athos Gipfel standest einst,", "tokens": ["Auf", "A\u00b7thos", "Gip\u00b7fel", "stan\u00b7dest", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und furchtbar deinen Stab bei Marathona", "tokens": ["Und", "furcht\u00b7bar", "dei\u00b7nen", "Stab", "bei", "Ma\u00b7ra\u00b7tho\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Und Salamis erhubest: Brich,", "tokens": ["Und", "Sa\u00b7la\u00b7mis", "er\u00b7hu\u00b7best", ":", "Brich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O brich die schwere Kett' entzwei, zerschlage", "tokens": ["O", "brich", "die", "schwe\u00b7re", "Kett'", "ent\u00b7zwei", ",", "zer\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Unterdr\u00fccker Vorsatz und Gewalt!", "tokens": ["Der", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", "Vor\u00b7satz", "und", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}