{"textgrid.poem.54264": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du lebest, Tyrsis, recht mit Schlemmern um die Wette,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lebest, Tyrsis, recht mit Schlemmern um die Wette,", "tokens": ["Du", "le\u00b7best", ",", "Tyr\u00b7sis", ",", "recht", "mit", "Schlem\u00b7mern", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$,", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Frist, s\u00e4uffst, verspielst, verkauffst die Kleider und das Bette,", "tokens": ["Frist", ",", "s\u00e4uffst", ",", "ver\u00b7spielst", ",", "ver\u00b7kauffst", "die", "Klei\u00b7der", "und", "das", "Bet\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du schluckest alles di\u00df in Schlung und Gurgel ein,", "tokens": ["Du", "schlu\u00b7ckest", "al\u00b7les", "di\u00df", "in", "Schlung", "und", "Gur\u00b7gel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PDS", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00e4r es auch ein Hau\u00df, so tr\u00e4nckst du es hinein.", "tokens": ["Und", "w\u00e4r", "es", "auch", "ein", "Hau\u00df", ",", "so", "tr\u00e4n\u00b7ckst", "du", "es", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}