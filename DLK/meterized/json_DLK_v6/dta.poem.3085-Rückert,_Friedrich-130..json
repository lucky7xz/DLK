{"dta.poem.3085": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "130.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unbillig klagest du, zu wenig sei dir kund", "tokens": ["Un\u00b7bil\u00b7lig", "kla\u00b7gest", "du", ",", "zu", "we\u00b7nig", "sei", "dir", "kund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PTKA", "PIS", "VAFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Dinge dieser Welt geheimnisreicher Grund.", "tokens": ["Der", "Din\u00b7ge", "die\u00b7ser", "Welt", "ge\u00b7heim\u00b7nis\u00b7rei\u00b7cher", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die n\u00e4chsten Gr\u00fcnde nur der Dinge siehst du nicht,", "tokens": ["Die", "n\u00e4chs\u00b7ten", "Gr\u00fcn\u00b7de", "nur", "der", "Din\u00b7ge", "siehst", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den letzten h\u00f6chsten Grund f\u00fchlst du mit Zuversicht.", "tokens": ["Den", "letz\u00b7ten", "h\u00f6chs\u00b7ten", "Grund", "f\u00fchlst", "du", "mit", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du f\u00fchlst, die Kette reicht von Gott zu dir hernieder,", "tokens": ["Du", "f\u00fchlst", ",", "die", "Ket\u00b7te", "reicht", "von", "Gott", "zu", "dir", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur in der Mitte siehst du nicht die Mittelglieder.", "tokens": ["Nur", "in", "der", "Mit\u00b7te", "siehst", "du", "nicht", "die", "Mit\u00b7tel\u00b7glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was brauchst du sie zu sehn? Du f\u00fchlst der Kette Zug,", "tokens": ["Was", "brauchst", "du", "sie", "zu", "sehn", "?", "Du", "f\u00fchlst", "der", "Ket\u00b7te", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An der dich durch die Welt Gott zieht, das ist genug.", "tokens": ["An", "der", "dich", "durch", "die", "Welt", "Gott", "zieht", ",", "das", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}