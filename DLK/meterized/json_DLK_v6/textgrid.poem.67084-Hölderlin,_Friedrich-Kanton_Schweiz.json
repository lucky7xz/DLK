eter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Lieber! wie war uns da! \u2013 bei solchem Mahle begehret", "tokens": ["Lie\u00b7ber", "!", "wie", "war", "uns", "da", "!", "\u2013", "bei", "sol\u00b7chem", "Mah\u00b7le", "be\u00b7ge\u00b7hret"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$.", "$(", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.42": {"text": "Nichts auf Erden die Brust, und alle Kr\u00e4fte gedeihen.", "tokens": ["Nichts", "auf", "Er\u00b7den", "die", "Brust", ",", "und", "al\u00b7le", "Kr\u00e4f\u00b7te", "ge\u00b7dei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Lieber! er schwand so schnell, der k\u00f6stliche Tag; in der k\u00fchlen", "tokens": ["Lie\u00b7ber", "!", "er", "schwand", "so", "schnell", ",", "der", "k\u00f6st\u00b7li\u00b7che", "Tag", ";", "in", "der", "k\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}}, "stanza.10": {"line.1": {"text": "D\u00e4mmerung schieden wir; an den Heiligt\u00fcmern der Freiheit", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "schie\u00b7den", "wir", ";", "an", "den", "Hei\u00b7lig\u00b7t\u00fc\u00b7mern", "der", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wallten wir dann vorbei in frommer seliger Stille,", "tokens": ["Wall\u00b7ten", "wir", "dann", "vor\u00b7bei", "in", "from\u00b7mer", "se\u00b7li\u00b7ger", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Fa\u00dften sie tief ins Herz, und segneten sie, und schieden!", "tokens": ["Fa\u00df\u00b7ten", "sie", "tief", "ins", "Herz", ",", "und", "seg\u00b7ne\u00b7ten", "sie", ",", "und", "schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "$,", "KON", "VVINF", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}}, "stanza.11": {"line.1": {"text": "Lebt dann wohl, ihr Gl\u00fccklichen dort! im friedsamen Tale", "tokens": ["Lebt", "dann", "wohl", ",", "ihr", "Gl\u00fcck\u00b7li\u00b7chen", "dort", "!", "im", "fried\u00b7sa\u00b7men", "Ta\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "ADV", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}}, "stanza.12": {"line.1": {"text": "Lebe wohl, du St\u00e4tte des Schwurs!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "St\u00e4t\u00b7te", "des", "Schwurs", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als in heiliger Nacht der ernste Bund dich besuchte.", "tokens": ["Als", "in", "hei\u00b7li\u00b7ger", "Nacht", "der", "erns\u00b7te", "Bund", "dich", "be\u00b7such\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Herrlich Gebirg! wo der bleiche Tyrann den Knechten vergebens,", "tokens": ["Herr\u00b7lich", "Ge\u00b7birg", "!", "wo", "der", "blei\u00b7che", "Ty\u00b7rann", "den", "Knech\u00b7ten", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Zahm und schmeichlerisch Mut gebot \u2013 zu gewaltig erhub sich", "tokens": ["Zahm", "und", "schmeich\u00b7le\u00b7risch", "Mut", "ge\u00b7bot", "\u2013", "zu", "ge\u00b7wal\u00b7tig", "er\u00b7hub", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "NN", "VVFIN", "$(", "PTKA", "ADJD", "VVFIN", "PRF"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Wider den Trotz die gerechte, die unerbittliche Rache \u2013", "tokens": ["Wi\u00b7der", "den", "Trotz", "die", "ge\u00b7rech\u00b7te", ",", "die", "un\u00b7er\u00b7bitt\u00b7li\u00b7che", "Ra\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Lebe wohl, du herrlich Gebirg", "tokens": ["Le\u00b7be", "wohl", ",", "du", "herr\u00b7lich", "Ge\u00b7birg"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJD", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Opferblut \u2013 es wehrte der Tr\u00e4ne der einsame Vater.", "tokens": ["Op\u00b7fer\u00b7blut", "\u2013", "es", "wehr\u00b7te", "der", "Tr\u00e4\u00b7ne", "der", "ein\u00b7sa\u00b7me", "Va\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Schlummre sanft, du Heldengebein! o schliefen auch wir dort", "tokens": ["Schlumm\u00b7re", "sanft", ",", "du", "Hel\u00b7den\u00b7ge\u00b7bein", "!", "o", "schlie\u00b7fen", "auch", "wir", "dort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "PPER", "NN", "$.", "FM", "VVFIN", "ADV", "PPER", "ADV"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Deinen eisernen Schlaf, dem Vaterlande geopfert,", "tokens": ["Dei\u00b7nen", "ei\u00b7ser\u00b7nen", "Schlaf", ",", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "ge\u00b7o\u00b7pfert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Walthers Gesellen und Tells, im sch\u00f6nen Kampfe der Freiheit!", "tokens": ["Walt\u00b7hers", "Ge\u00b7sel\u00b7len", "und", "Tells", ",", "im", "sch\u00f6\u00b7nen", "Kamp\u00b7fe", "der", "Frei\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}}, "stanza.13": {"line.1": {"text": "K\u00f6nnt ich dein vergessen, o Land, der g\u00f6ttlichen Freiheit!", "tokens": ["K\u00f6nnt", "ich", "dein", "ver\u00b7ges\u0