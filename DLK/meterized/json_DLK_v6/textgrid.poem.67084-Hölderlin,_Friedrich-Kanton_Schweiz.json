{"textgrid.poem.67084": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kanton Schweiz", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, in erm\u00fcdender Ruh, im bitters\u00fc\u00dfen Verlangen,", "tokens": ["Hier", ",", "in", "er\u00b7m\u00fc\u00b7den\u00b7der", "Ruh", ",", "im", "bit\u00b7ter\u00b7s\u00fc\u00b7\u00dfen", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Da zu sein, wo mein Herz, und jeder be\u00dfre Gedank ist,", "tokens": ["Da", "zu", "sein", ",", "wo", "mein", "Herz", ",", "und", "je\u00b7der", "be\u00df\u00b7re", "Ge\u00b7dank", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VAINF", "$,", "PWAV", "PPOSAT", "NN", "$,", "KON", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Reichet doch Erinnerung mir den zaubrischen Becher", "tokens": ["Rei\u00b7chet", "doch", "E\u00b7rin\u00b7ne\u00b7rung", "mir", "den", "zaub\u00b7ri\u00b7schen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Sch\u00e4umend und voll, und hoher Genu\u00df der kehrenden Bilder", "tokens": ["Sch\u00e4u\u00b7mend", "und", "voll", ",", "und", "ho\u00b7her", "Ge\u00b7nu\u00df", "der", "keh\u00b7ren\u00b7den", "Bil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "KON", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Weckt die schlummernden Fittige mir zu trautem Gesange.", "tokens": ["Weckt", "die", "schlum\u00b7mern\u00b7den", "Fit\u00b7ti\u00b7ge", "mir", "zu", "trau\u00b7tem", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Bruder! dir gab ein Gott der Liebe g\u00f6ttlichen Funken,", "tokens": ["Bru\u00b7der", "!", "dir", "gab", "ein", "Gott", "der", "Lie\u00b7be", "g\u00f6tt\u00b7li\u00b7chen", "Fun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "Zarten gel\u00e4uterten Sinn, zu ersp\u00e4hn, was herrlich und sch\u00f6n ist;", "tokens": ["Zar\u00b7ten", "ge\u00b7l\u00e4u\u00b7ter\u00b7ten", "Sinn", ",", "zu", "er\u00b7sp\u00e4hn", ",", "was", "herr\u00b7lich", "und", "sch\u00f6n", "ist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Stolzer Freiheit gl\u00fchet dein Herz, und kindlicher Einfalt \u2013", "tokens": ["Stol\u00b7zer", "Frei\u00b7heit", "gl\u00fc\u00b7het", "dein", "Herz", ",", "und", "kind\u00b7li\u00b7cher", "Ein\u00b7falt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN", "$("], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Bruder! komm und koste mit mir des zaubrischen Bechers.", "tokens": ["Bru\u00b7der", "!", "komm", "und", "kos\u00b7te", "mit", "mir", "des", "zaub\u00b7ri\u00b7schen", "Be\u00b7chers", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort, wo der Abendstrahl die Westgew\u00f6lke verg\u00fcldet,", "tokens": ["Dort", ",", "wo", "der", "A\u00b7bend\u00b7strahl", "die", "West\u00b7ge\u00b7w\u00f6l\u00b7ke", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Dorthin wende den Blick, und weine die Tr\u00e4ne der Sehnsucht!", "tokens": ["Dor\u00b7thin", "wen\u00b7de", "den", "Blick", ",", "und", "wei\u00b7ne", "die", "Tr\u00e4\u00b7ne", "der", "Sehn\u00b7sucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ach! dort wandelten wir! dort flog und schwelgte das Auge", "tokens": ["Ach", "!", "dort", "wan\u00b7del\u00b7ten", "wir", "!", "dort", "flog", "und", "schwelg\u00b7te", "das", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+----+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unter den Herrlichkeiten umher! \u2013 wie dehnte der Busen,", "tokens": ["Un\u00b7ter", "den", "Herr\u00b7lich\u00b7kei\u00b7ten", "um\u00b7her", "!", "\u2013", "wie", "dehn\u00b7te", "der", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "$(", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Diesen Himmel zu fassen, sich aus! \u2013 wie brannte die Wange", "tokens": ["Die\u00b7sen", "Him\u00b7mel", "zu", "fas\u00b7sen", ",", "sich", "aus", "!", "\u2013", "wie", "brann\u00b7te", "die", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,", "PRF", "PTKVZ", "$.", "$(", "PWAV", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "S\u00fc\u00df von Morgenl\u00fcften gek\u00fchlt, als unter Ges\u00e4ngen", "tokens": ["S\u00fc\u00df", "von", "Mor\u00b7gen\u00b7l\u00fcf\u00b7ten", "ge\u00b7k\u00fchlt", ",", "als", "un\u00b7ter", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Lieber! wie dr\u00fccktest du mir die hei\u00dfe, zitternde Rechte,", "tokens": ["Lie\u00b7ber", "!", "wie", "dr\u00fcck\u00b7test", "du", "mir", "die", "hei\u00b7\u00dfe", ",", "zit\u00b7tern\u00b7de", "Rech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "PWAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Sahst so gl\u00fchend und ernst mich an im donnernden Rheinsturz!", "tokens": ["Sahst", "so", "gl\u00fc\u00b7hend", "und", "ernst", "mich", "an", "im", "don\u00b7nern\u00b7den", "Rhein\u00b7sturz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Aber selig, wie du, o Tag am Quelle der Freiheit!", "tokens": ["A\u00b7ber", "se\u00b7lig", ",", "wie", "du", ",", "o", "Tag", "am", "Quel\u00b7le", "der", "Frei\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "$,", "FM", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Festlich, wie du, sank keiner auf uns vom rosigen Himmel.", "tokens": ["Fest\u00b7lich", ",", "wie", "du", ",", "sank", "kei\u00b7ner", "auf", "uns", "vom", "ro\u00b7si\u00b7gen", "Him\u00b7mel", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "$,", "VVFIN", "PIS", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}}, "stanza.7": {"line.1": {"text": "Ahndung schwellte das Herz. Schon war des feiernden Klosters", "tokens": ["Ahn\u00b7dung", "schwell\u00b7te", "das", "Herz", ".", "Schon", "war", "des", "fei\u00b7ern\u00b7den", "Klos\u00b7ters"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}}, "stanza.8": {"line.1": {"text": "Ernste Glocke verhallt. Schon schwanden die friedlichen H\u00fctten", "tokens": ["Erns\u00b7te", "Glo\u00b7cke", "ver\u00b7hallt", ".", "Schon", "schwan\u00b7den", "die", "fried\u00b7li\u00b7chen", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Rund an Blumenh\u00fcgeln umher, am rollenden Gie\u00dfbach,", "tokens": ["Rund", "an", "Blu\u00b7men\u00b7h\u00fc\u00b7geln", "um\u00b7her", ",", "am", "rol\u00b7len\u00b7den", "Gie\u00df\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Unter Fichten im Tal, wo dem Ahn in heiliger Urzeit", "tokens": ["Un\u00b7ter", "Fich\u00b7ten", "im", "Tal", ",", "wo", "dem", "Ahn", "in", "hei\u00b7li\u00b7ger", "Ur\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "F\u00fcglich deuchte der Grund zum Erbe gen\u00fcgsamer Enkel.", "tokens": ["F\u00fcg\u00b7lich", "deuch\u00b7te", "der", "Grund", "zum", "Er\u00b7be", "ge\u00b7n\u00fcg\u00b7sa\u00b7mer", "En\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Schaurig und k\u00fchl empfing uns die Nacht in ewigen W\u00e4ldern,", "tokens": ["Schau\u00b7rig", "und", "k\u00fchl", "emp\u00b7fing", "uns", "die", "Nacht", "in", "e\u00b7wi\u00b7gen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und wir klommen hinauf am furchtbarherrlichen", "tokens": ["Und", "wir", "klom\u00b7men", "hin\u00b7auf", "am", "furcht\u00b7bar\u00b7herr\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "N\u00e4chtlicher immer wards und enger im Riesengeb\u00fcrge.", "tokens": ["N\u00e4cht\u00b7li\u00b7cher", "im\u00b7mer", "wards", "und", "en\u00b7ger", "im", "Rie\u00b7sen\u00b7ge\u00b7b\u00fcr\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "J\u00e4her herunter hing der Pfad zu den einsamen Wallern.", "tokens": ["J\u00e4\u00b7her", "her\u00b7un\u00b7ter", "hing", "der", "Pfad", "zu", "den", "ein\u00b7sa\u00b7men", "Wal\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Dicht zur Rechten donnert hinab der z\u00fcrnende Waldstrom:", "tokens": ["Dicht", "zur", "Rech\u00b7ten", "don\u00b7nert", "hin\u00b7ab", "der", "z\u00fcr\u00b7nen\u00b7de", "Wald\u00b7strom", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Nur sein Donner berauscht den Sinn. Die sch\u00e4umenden Wogen", "tokens": ["Nur", "sein", "Don\u00b7ner", "be\u00b7rauscht", "den", "Sinn", ".", "Die", "sch\u00e4u\u00b7men\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Birgt uns Felsengestr\u00e4uch, und modernde Tannen am Abhang,", "tokens": ["Birgt", "uns", "Fel\u00b7sen\u00b7ge\u00b7str\u00e4uch", ",", "und", "mo\u00b7dern\u00b7de", "Tan\u00b7nen", "am", "Ab\u00b7hang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Vom Orkane gest\u00fcrzt. \u2013 Nun tagte die Nacht am Gebirge", "tokens": ["Vom", "Or\u00b7ka\u00b7ne", "ge\u00b7st\u00fcrzt", ".", "\u2013", "Nun", "tag\u00b7te", "die", "Nacht", "am", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Schaurig und wundersam, wie Heldengeister am Lego,", "tokens": ["Schau\u00b7rig", "und", "wun\u00b7der\u00b7sam", ",", "wie", "Hel\u00b7den\u00b7geis\u00b7ter", "am", "Le\u00b7go", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.14": {"text": "W\u00e4lzten sich k\u00e4mpfende Wolken heran auf schneeiger Heide.", "tokens": ["W\u00e4lz\u00b7ten", "sich", "k\u00e4mp\u00b7fen\u00b7de", "Wol\u00b7ken", "he\u00b7ran", "auf", "schne\u00b7e\u00b7i\u00b7ger", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+---+-+--+-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Sturm und Frost entschwebte der Kluft. Vom Sturme getragen", "tokens": ["Sturm", "und", "Frost", "ent\u00b7schweb\u00b7te", "der", "Kluft", ".", "Vom", "Stur\u00b7me", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VVPP"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Schrie und st\u00fcrzte der Aar, die Beut im Tale zu haschen.", "tokens": ["Schrie", "und", "st\u00fcrz\u00b7te", "der", "Aar", ",", "die", "Beut", "im", "Ta\u00b7le", "zu", "ha\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Und der Wolken H\u00fclle zerri\u00df, und im ehernen Panzer", "tokens": ["Und", "der", "Wol\u00b7ken", "H\u00fcl\u00b7le", "zer\u00b7ri\u00df", ",", "und", "im", "e\u00b7her\u00b7nen", "Pan\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Kam die Riesin heran, die majest\u00e4tische Myten.", "tokens": ["Kam", "die", "Rie\u00b7sin", "he\u00b7ran", ",", "die", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7sche", "My\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "ART", "ADJ