{"textgrid.poem.50920": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Wandersegen", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist es doch betr\u00fcbt zu klagen,", "tokens": ["So", "ist", "es", "doch", "be\u00b7tr\u00fcbt", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn deutsche M\u00fctter den Rhein hinab,", "tokens": ["Wenn", "deut\u00b7sche", "M\u00fct\u00b7ter", "den", "Rhein", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hinab und \u00fcber des Meeres Grab", "tokens": ["Hin\u00b7ab", "und", "\u00fc\u00b7ber", "des", "Mee\u00b7res", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die zarten Wickelkindlein tragen,", "tokens": ["Die", "zar\u00b7ten", "Wi\u00b7ckel\u00b7kin\u00b7dlein", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nach freier L\u00e4nder Gestaden hin,", "tokens": ["Nach", "frei\u00b7er", "L\u00e4n\u00b7der", "Ge\u00b7sta\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Indes die M\u00e4nner auf weiten Wegen,", "tokens": ["In\u00b7des", "die", "M\u00e4n\u00b7ner", "auf", "wei\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Getrennt, bek\u00fcmmert zum Ziele fliehn!", "tokens": ["Ge\u00b7trennt", ",", "be\u00b7k\u00fcm\u00b7mert", "zum", "Zie\u00b7le", "fliehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich streue meinen leichten Segen,", "tokens": ["Ich", "streu\u00b7e", "mei\u00b7nen", "leich\u00b7ten", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Fast trauernd, in dein Frauenherz:", "tokens": ["Fast", "trau\u00b7ernd", ",", "in", "dein", "Frau\u00b7en\u00b7herz", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Fahr gl\u00fccklich denn rheinniederw\u00e4rts", "tokens": ["Fahr", "gl\u00fcck\u00b7lich", "denn", "rhein\u00b7nie\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und finde Leute in allen Reichen,", "tokens": ["Und", "fin\u00b7de", "Leu\u00b7te", "in", "al\u00b7len", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die gute Milch dem Kindlein reichen,", "tokens": ["Die", "gu\u00b7te", "Milch", "dem", "Kin\u00b7dlein", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und auf den Schiffen, wenn es schreit,", "tokens": ["Und", "auf", "den", "Schif\u00b7fen", ",", "wenn", "es", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Publikum, das ihm verzeiht!", "tokens": ["Ein", "Pub\u00b7li\u00b7kum", ",", "das", "ihm", "ver\u00b7zeiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des Reimes wegen, als ein Schweizer,", "tokens": ["Des", "Rei\u00b7mes", "we\u00b7gen", ",", "als", "ein", "Schwei\u00b7zer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "W\u00fcnsch ich dir einen n\u00fcchternen Heizer,", "tokens": ["W\u00fcnsch", "ich", "dir", "ei\u00b7nen", "n\u00fcch\u00b7ter\u00b7nen", "Hei\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Der da vorsichtig, sanft und lind", "tokens": ["Der", "da", "vor\u00b7sich\u00b7tig", ",", "sanft", "und", "lind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das Schiff dich tragen l\u00e4\u00dft mit dem Kind!", "tokens": ["Das", "Schiff", "dich", "tra\u00b7gen", "l\u00e4\u00dft", "mit", "dem", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Ich w\u00fcnsche, da\u00df alles, was sehenswert,", "tokens": ["Ich", "w\u00fcn\u00b7sche", ",", "da\u00df", "al\u00b7les", ",", "was", "se\u00b7hens\u00b7wert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "$,", "PWS", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Die sch\u00f6nsten Seiten zu dir kehrt,", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Sei\u00b7ten", "zu", "dir", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Vor deinem Fu\u00df frisch Rasengr\u00fcn,", "tokens": ["Vor", "dei\u00b7nem", "Fu\u00df", "frisch", "Ra\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dem Auge freundlicher Sterne Gl\u00fchn,", "tokens": ["Dem", "Au\u00b7ge", "freund\u00b7li\u00b7cher", "Ster\u00b7ne", "Gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "In deine H\u00e4nde wei\u00dfes Brot", "tokens": ["In", "dei\u00b7ne", "H\u00e4n\u00b7de", "wei\u00b7\u00dfes", "Brot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und alle Tag Morgen- und Abendrot!", "tokens": ["Und", "al\u00b7le", "Tag", "Mor\u00b7gen", "und", "A\u00b7ben\u00b7drot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "TRUNC", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Indes sei deinem Mann der Wein", "tokens": ["In\u00b7des", "sei", "dei\u00b7nem", "Mann", "der", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "All\u00fcberall s\u00fc\u00df, stark und rein!", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "s\u00fc\u00df", ",", "stark", "und", "rein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Rhein will keinen Wunsch ich sagen,", "tokens": ["Vom", "Rhein", "will", "kei\u00b7nen", "Wunsch", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird ger\u00fchrt und treu dich tragen;", "tokens": ["Er", "wird", "ge\u00b7r\u00fchrt", "und", "treu", "dich", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch das Meer sei ohne Gefahr!", "tokens": ["Je\u00b7doch", "das", "Meer", "sei", "oh\u00b7ne", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und wo ihr hinkommt, frisch und klar,", "tokens": ["Und", "wo", "ihr", "hin\u00b7kommt", ",", "frisch", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Blumen umgeben, vergn\u00fcgt und rein", "tokens": ["Von", "Blu\u00b7men", "um\u00b7ge\u00b7ben", ",", "ver\u00b7gn\u00fcgt", "und", "rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVPP", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "M\u00fcssen alle Brunnen und Quellen sein!", "tokens": ["M\u00fcs\u00b7sen", "al\u00b7le", "Brun\u00b7nen", "und", "Quel\u00b7len", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und weil die Guten dieser Erden", "tokens": ["Und", "weil", "die", "Gu\u00b7ten", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch eine Weile wandern werden,", "tokens": ["Noch", "ei\u00b7ne", "Wei\u00b7le", "wan\u00b7dern", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So mache die Ferne das Herz euch satt", "tokens": ["So", "ma\u00b7che", "die", "Fer\u00b7ne", "das", "Herz", "euch", "satt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Mit allem Besten, was sie hat;", "tokens": ["Mit", "al\u00b7lem", "Bes\u00b7ten", ",", "was", "sie", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie f\u00fclle freundlich euch die Truh", "tokens": ["Sie", "f\u00fcl\u00b7le", "freund\u00b7lich", "euch", "die", "Truh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und geb euch leichte Sorgen am Tag,", "tokens": ["Und", "geb", "euch", "leich\u00b7te", "Sor\u00b7gen", "am", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Am Abend Nachtigallenschlag,", "tokens": ["Am", "A\u00b7bend", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zur Nachtzeit aber die goldene Ruh;", "tokens": ["Zur", "Nacht\u00b7zeit", "a\u00b7ber", "die", "gol\u00b7de\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Des Sommers Frucht, des Fr\u00fchlings Zier,", "tokens": ["Des", "Som\u00b7mers", "Frucht", ",", "des", "Fr\u00fch\u00b7lings", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In England immer vom besten Bier,", "tokens": ["In", "En\u00b7gland", "im\u00b7mer", "vom", "bes\u00b7ten", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Den Fisch im Wasser, den Vogel der Luft \u2013", "tokens": ["Den", "Fisch", "im", "Was\u00b7ser", ",", "den", "Vo\u00b7gel", "der", "Luft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Nur keinen Boden zu einer Gruft:", "tokens": ["Nur", "kei\u00b7nen", "Bo\u00b7den", "zu", "ei\u00b7ner", "Gruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Denn in der Heimat sollt ihr sterben", "tokens": ["Denn", "in", "der", "Hei\u00b7mat", "sollt", "ihr", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und euren Kindern die Freiheit vererben!", "tokens": ["Und", "eu\u00b7ren", "Kin\u00b7dern", "die", "Frei\u00b7heit", "ver\u00b7er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}