{"textgrid.poem.46086": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o wie ist der Epheu treu!]", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie ist der Epheu treu!", "tokens": ["O", "wie", "ist", "der", "E\u00b7pheu", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann er sich nicht selbst erheben", "tokens": ["Kann", "er", "sich", "nicht", "selbst", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann er gleich dem Wein nicht reben,", "tokens": ["Kann", "er", "gleich", "dem", "Wein", "nicht", "re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann er doch so liebend ranken", "tokens": ["Kann", "er", "doch", "so", "lie\u00b7bend", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An den Armen an den Kranken", "tokens": ["An", "den", "Ar\u00b7men", "an", "den", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf zum wahren Weinstock streben!", "tokens": ["Auf", "zum", "wah\u00b7ren", "Wein\u00b7stock", "stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O wie ist der Epheu gut", "tokens": ["O", "wie", "ist", "der", "E\u00b7pheu", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo er nur ein bi\u00dfchen ruht", "tokens": ["Wo", "er", "nur", "ein", "bi\u00df\u00b7chen", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Gleich die W\u00fcrzelchen fest klammern,", "tokens": ["Gleich", "die", "W\u00fcr\u00b7zel\u00b7chen", "fest", "klam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Da\u00df die Trennung ihn mu\u00df jammern,", "tokens": ["Da\u00df", "die", "Tren\u00b7nung", "ihn", "mu\u00df", "jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie ist der Epheu treu,", "tokens": ["O", "wie", "ist", "der", "E\u00b7pheu", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Grabesurne bricht", "tokens": ["Wenn", "die", "Gra\u00b7be\u00b7sur\u00b7ne", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sie doch der Epheu nicht", "tokens": ["L\u00e4\u00dft", "sie", "doch", "der", "E\u00b7pheu", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bindet um die Asche fest die Scherben,", "tokens": ["Bin\u00b7det", "um", "die", "A\u00b7sche", "fest", "die", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O wie ist ", "tokens": ["O", "wie", "ist"], "token_info": ["word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aus der W\u00fcste steigt er auf", "tokens": ["Aus", "der", "W\u00fcs\u00b7te", "steigt", "er", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die Braut die sich auf den Geliebten st\u00fctzet.", "tokens": ["Wie", "die", "Braut", "die", "sich", "auf", "den", "Ge\u00b7lieb\u00b7ten", "st\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "O wie ist ", "tokens": ["O", "wie", "ist"], "token_info": ["word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Von der Wurzel losgeschnitten", "tokens": ["Von", "der", "Wur\u00b7zel", "los\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden Wurzeln seine Zweige", "tokens": ["Wer\u00b7den", "Wur\u00b7zeln", "sei\u00b7ne", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nie von jenem weiche", "tokens": ["Da\u00df", "er", "nie", "von", "je\u00b7nem", "wei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er einmal hat umarmt.", "tokens": ["Was", "er", "ein\u00b7mal", "hat", "um\u00b7armt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O wie ist der Epheu sinnend", "tokens": ["O", "wie", "ist", "der", "E\u00b7pheu", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das was er sinnet minnend,", "tokens": ["Und", "das", "was", "er", "sin\u00b7net", "min\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um das Kreuz schlingt er die Triebe.", "tokens": ["Um", "das", "Kreuz", "schlingt", "er", "die", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der W\u00fcste lag ein Stein", "tokens": ["In", "der", "W\u00fcs\u00b7te", "lag", "ein", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So allein, allein, allein", "tokens": ["So", "al\u00b7lein", ",", "al\u00b7lein", ",", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kam der Epheu z\u00e4h und kraus", "tokens": ["Kam", "der", "E\u00b7pheu", "z\u00e4h", "und", "kraus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Baute drum ein gr\u00fcnes Haus", "tokens": ["Bau\u00b7te", "drum", "ein", "gr\u00fc\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Immergr\u00fcn ist er geblieben", "tokens": ["Im\u00b7mer\u00b7gr\u00fcn", "ist", "er", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sollte ihn der Stein nicht lieben.", "tokens": ["Soll\u00b7te", "ihn", "der", "Stein", "nicht", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}