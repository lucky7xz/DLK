{"textgrid.poem.67442": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Apollo", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df Apollo der Sch\u00e4fer nicht mehr am lieblichen Peneus", "tokens": ["Ach", ",", "da\u00df", "A\u00b7pol\u00b7lo", "der", "Sch\u00e4\u00b7fer", "nicht", "mehr", "am", "lieb\u00b7li\u00b7chen", "Pe\u00b7neus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "NE", "ART", "NN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Weidet! da\u00df er so bald wieder die Erde verlie\u00df!", "tokens": ["Wei\u00b7det", "!", "da\u00df", "er", "so", "bald", "wie\u00b7der", "die", "Er\u00b7de", "ver\u00b7lie\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Glaub, er verlie\u00df sie nicht! Er weidet die Heerde der Menschen;", "tokens": ["Glaub", ",", "er", "ver\u00b7lie\u00df", "sie", "nicht", "!", "Er", "wei\u00b7det", "die", "Heer\u00b7de", "der", "Men\u00b7schen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Nur ein barbarisches Ohr h\u00f6rt nicht des Hirten Gesang.", "tokens": ["Nur", "ein", "bar\u00b7ba\u00b7ri\u00b7sches", "Ohr", "h\u00f6rt", "nicht", "des", "Hir\u00b7ten", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "----+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}