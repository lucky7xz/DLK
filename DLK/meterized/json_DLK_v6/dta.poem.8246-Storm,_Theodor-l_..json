{"dta.poem.8246": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Was zu scheu um Klang zu geben,", "tokens": ["Was", "zu", "scheu", "um", "Klang", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was zu lieblich zum Entstehen,", "tokens": ["Was", "zu", "lieb\u00b7lich", "zum", "Ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was geboren zum Vergehen,", "tokens": ["Was", "ge\u00b7bo\u00b7ren", "zum", "Ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die Monde nimmer bieten,", "tokens": ["Was", "die", "Mon\u00b7de", "nim\u00b7mer", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NE", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosen aus verwelkten Bl\u00fcthen,", "tokens": ["Ro\u00b7sen", "aus", "ver\u00b7welk\u00b7ten", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Thr\u00e4nen dann aus jungem Leide", "tokens": ["Thr\u00e4\u00b7nen", "dann", "aus", "jun\u00b7gem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Klang verlorner Freude.", "tokens": ["Und", "ein", "Klang", "ver\u00b7lor\u00b7ner", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}