{"textgrid.poem.24522": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Music", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht alle Lust ist gut. Soll sie dir n\u00fctzlich seyn.", "tokens": ["Nicht", "al\u00b7le", "Lust", "ist", "gut", ".", "Soll", "sie", "dir", "n\u00fctz\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ADJD", "$.", "VMFIN", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wehle die ", "tokens": ["So", "weh\u00b7le", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das nicht zu \u00fcppig ist/ nicht aus der Ruh in Pein/", "tokens": ["Das", "nicht", "zu", "\u00fcp\u00b7pig", "ist", "/", "nicht", "aus", "der", "Ruh", "in", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PTKA", "ADJD", "VAFIN", "$(", "PTKNEG", "APPR", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur aus der Unruh dich in Ruhe wei\u00df zu setzen.", "tokens": ["Nur", "aus", "der", "Un\u00b7ruh", "dich", "in", "Ru\u00b7he", "wei\u00df", "zu", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Hertz sey Felsen-Art/ ", "tokens": ["Dein", "Hertz", "sey", "Fel\u00b7sen\u00b7Art", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und g\u00f6nnt der Sterblichkeit den Vorschmack jener Luft.", "tokens": ["Und", "g\u00f6nnt", "der", "Sterb\u00b7lich\u00b7keit", "den", "Vor\u00b7schmack", "je\u00b7ner", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}