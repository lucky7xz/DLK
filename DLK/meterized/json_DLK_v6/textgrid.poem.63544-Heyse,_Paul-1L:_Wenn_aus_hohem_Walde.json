{"textgrid.poem.63544": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn aus hohem Walde", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus hohem Walde", "tokens": ["Wenn", "aus", "ho\u00b7hem", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mondenschimmer quillt,", "tokens": ["Mon\u00b7den\u00b7schim\u00b7mer", "quillt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf die lichte Halde", "tokens": ["Auf", "die", "lich\u00b7te", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wagt sich vor das Wild.", "tokens": ["Wagt", "sich", "vor", "das", "Wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt in irrer Klage", "tokens": ["Jetzt", "in", "ir\u00b7rer", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wird die Sehnsucht laut,", "tokens": ["Wird", "die", "Sehn\u00b7sucht", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die dem hellen Tage", "tokens": ["Die", "dem", "hel\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nicht ihr Leid vertraut.", "tokens": ["Nicht", "ihr", "Leid", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ruft der Hirsch die Hinde,", "tokens": ["Ruft", "der", "Hirsch", "die", "Hin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ach, sie h\u00f6rt ihn bald,", "tokens": ["Ach", ",", "sie", "h\u00f6rt", "ihn", "bald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn umsonst im Winde", "tokens": ["Wenn", "um\u00b7sonst", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mein Gesang verhallt!", "tokens": ["Mein", "Ge\u00b7sang", "ver\u00b7hallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}