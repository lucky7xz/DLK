{"textgrid.poem.32040": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "115. Die Nachreu kommt zu sp\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da Gott auf Erden ging, ward er fast nicht geacht;", "tokens": ["Da", "Gott", "auf", "Er\u00b7den", "ging", ",", "ward", "er", "fast", "nicht", "ge\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nun er im Himmel ist, beklagt ihn jedermann,", "tokens": ["Nun", "er", "im", "Him\u00b7mel", "ist", ",", "be\u00b7klagt", "ihn", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ihm nicht gr\u00f6\u00dfer Ehr ist worden angetan.", "tokens": ["Da\u00df", "ihm", "nicht", "gr\u00f6\u00b7\u00dfer", "Ehr", "ist", "wor\u00b7den", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJA", "NN", "VAFIN", "VAPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So t\u00f6richt ist die Welt, da\u00df sies nicht vorbedacht! ", "tokens": ["So", "t\u00f6\u00b7richt", "ist", "die", "Welt", ",", "da\u00df", "sies", "nicht", "vor\u00b7be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}