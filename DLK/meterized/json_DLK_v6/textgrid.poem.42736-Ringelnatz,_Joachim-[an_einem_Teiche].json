{"textgrid.poem.42736": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[an einem Teiche]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem Teiche", "tokens": ["An", "ei\u00b7nem", "Tei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schlich eine Schleiche,", "tokens": ["Schlich", "ei\u00b7ne", "Schlei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Eine Blindschleiche sogar.", "tokens": ["Ei\u00b7ne", "Blind\u00b7schlei\u00b7che", "so\u00b7gar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da trieb ein Etwas ans Ufer im Wind.", "tokens": ["Da", "trieb", "ein", "Et\u00b7was", "ans", "U\u00b7fer", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Schleiche sah nicht, was es war,", "tokens": ["Die", "Schlei\u00b7che", "sah", "nicht", ",", "was", "es", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn sie war blind.", "tokens": ["Denn", "sie", "war", "blind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.8": {"text": "Das dunkle Etwas aber war die Kindsleiche", "tokens": ["Das", "dunk\u00b7le", "Et\u00b7was", "a\u00b7ber", "war", "die", "Kinds\u00b7lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Einer Blindschleiche.", "tokens": ["Ei\u00b7ner", "Blind\u00b7schlei\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}