{"textgrid.poem.63072": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Lebenslauf", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geboren ward Klabund,", "tokens": ["Ge\u00b7bo\u00b7ren", "ward", "Kla\u00b7bund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da war er achtzehn Jahre", "tokens": ["Da", "war", "er", "acht\u00b7zehn", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hatte blonde Haare", "tokens": ["Und", "hat\u00b7te", "blon\u00b7de", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und war gesund.", "tokens": ["Und", "war", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Doch als er starb, ein Trott,", "tokens": ["Doch", "als", "er", "starb", ",", "ein", "Trott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "War er zwei Jahre \u00e4lter,", "tokens": ["War", "er", "zwei", "Jah\u00b7re", "\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein morscher Lustbeh\u00e4lter,", "tokens": ["Ein", "mor\u00b7scher", "Lust\u00b7be\u00b7h\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So stieg er aufs Schafott.", "tokens": ["So", "stieg", "er", "aufs", "Scha\u00b7fott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Er bracht ein Zwilling um....", "tokens": ["Er", "bracht", "ein", "Zwil\u00b7ling", "um", "...."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "(das M\u00e4dchen war vom Lande", "tokens": ["(", "das", "M\u00e4d\u00b7chen", "war", "vom", "Lan\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kam dadurch in Schande", "tokens": ["Und", "kam", "da\u00b7durch", "in", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ins Delirium.)", "tokens": ["Und", "ins", "De\u00b7li\u00b7ri\u00b7um", ".", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}