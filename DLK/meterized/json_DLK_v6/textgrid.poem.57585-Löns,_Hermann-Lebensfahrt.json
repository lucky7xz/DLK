{"textgrid.poem.57585": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Lebensfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nordwind streichelt die Wellen,", "tokens": ["Der", "Nord\u00b7wind", "strei\u00b7chelt", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Gr\u00fcnwasser sinken und schwellen,", "tokens": ["Die", "Gr\u00fcn\u00b7was\u00b7ser", "sin\u00b7ken", "und", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Sonne prallt goldig und hei\u00df", "tokens": ["Die", "Son\u00b7ne", "prallt", "gol\u00b7dig", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Auf unsern lautjubelnden Kreis.", "tokens": ["Auf", "un\u00b7sern", "laut\u00b7ju\u00b7beln\u00b7den", "Kreis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Mit M\u00e4dchen, vollbl\u00fctig wie Rosen,", "tokens": ["Mit", "M\u00e4d\u00b7chen", ",", "voll\u00b7bl\u00fc\u00b7tig", "wie", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Starkarmige Frohburschen kosen,", "tokens": ["Star\u00b7kar\u00b7mi\u00b7ge", "Froh\u00b7bur\u00b7schen", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wein, Lachen und lauter Gesang", "tokens": ["Wein", ",", "La\u00b7chen", "und", "lau\u00b7ter", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und pl\u00e4tschernder Meerwellen Klang.", "tokens": ["Und", "pl\u00e4t\u00b7schern\u00b7der", "Meer\u00b7wel\u00b7len", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ich sitze stumm in der Mitte,", "tokens": ["Ich", "sit\u00b7ze", "stumm", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie immer der paarlose Dritte,", "tokens": ["Wie", "im\u00b7mer", "der", "paar\u00b7lo\u00b7se", "Drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich denke an Klippe und Riff", "tokens": ["Ich", "den\u00b7ke", "an", "Klip\u00b7pe", "und", "Riff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und unser schwachplankiges Schiff.", "tokens": ["Und", "un\u00b7ser", "schwach\u00b7plan\u00b7ki\u00b7ges", "Schiff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wildfremd noch waren wir gestern,", "tokens": ["Wild\u00b7fremd", "noch", "wa\u00b7ren", "wir", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lieben sich heute wie Schwestern,", "tokens": ["Und", "lie\u00b7ben", "sich", "heu\u00b7te", "wie", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "KOKOM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und morgen im maigr\u00fcnen Wald", "tokens": ["Und", "mor\u00b7gen", "im", "mai\u00b7gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vielleicht die Pistole schon knallt.", "tokens": ["Viel\u00b7leicht", "die", "Pis\u00b7to\u00b7le", "schon", "knallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Wasser sich dehnen und bl\u00e4hen", "tokens": ["Die", "Was\u00b7ser", "sich", "deh\u00b7nen", "und", "bl\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Um klippige, zackige H\u00f6hen,", "tokens": ["Um", "klip\u00b7pi\u00b7ge", ",", "za\u00b7cki\u00b7ge", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein greller, landsuchender Schrei,", "tokens": ["Ein", "grel\u00b7ler", ",", "land\u00b7su\u00b7chen\u00b7der", "Schrei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und Lachen und Lieben vorbei.", "tokens": ["Und", "La\u00b7chen", "und", "Lie\u00b7ben", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich einziger wurde gerettet,", "tokens": ["Ich", "ein\u00b7zi\u00b7ger", "wur\u00b7de", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf meerfeuchtem Sande gebettet,", "tokens": ["Auf", "meer\u00b7feuch\u00b7tem", "San\u00b7de", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Im Tang eine schneewei\u00dfe Hand \u2013", "tokens": ["Im", "Tang", "ei\u00b7ne", "schnee\u00b7wei\u00b7\u00dfe", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich habe sie einmal gekannt.", "tokens": ["Ich", "ha\u00b7be", "sie", "ein\u00b7mal", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Ich pfeife mein sorgloses Liedchen:", "tokens": ["Ich", "pfei\u00b7fe", "mein", "sorg\u00b7lo\u00b7ses", "Lied\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Natur, du k\u00fchltest dein M\u00fctchen,", "tokens": ["Na\u00b7tur", ",", "du", "k\u00fchl\u00b7test", "dein", "M\u00fct\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer wei\u00df, ob mich morgen nicht auch", "tokens": ["Wer", "wei\u00df", ",", "ob", "mich", "mor\u00b7gen", "nicht", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Fortpustet ein giftiger Hauch.", "tokens": ["Fort\u00b7pus\u00b7tet", "ein", "gif\u00b7ti\u00b7ger", "Hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}