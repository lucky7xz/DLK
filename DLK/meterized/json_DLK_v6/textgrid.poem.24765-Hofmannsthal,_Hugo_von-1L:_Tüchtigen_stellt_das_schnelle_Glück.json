{"textgrid.poem.24765": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00fcchtigen stellt das schnelle Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00fcchtigen stellt das schnelle Gl\u00fcck", "tokens": ["T\u00fcch\u00b7ti\u00b7gen", "stellt", "das", "schnel\u00b7le", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hoch empor, wo er gebiete,", "tokens": ["Hoch", "em\u00b7por", ",", "wo", "er", "ge\u00b7bie\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vielen zum Nutzen, vielen zum Leid,", "tokens": ["Vie\u00b7len", "zum", "Nut\u00b7zen", ",", "vie\u00b7len", "zum", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PIAT", "APPRART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und es h\u00e4ngen sich viele an ihn,", "tokens": ["Und", "es", "h\u00e4n\u00b7gen", "sich", "vie\u00b7le", "an", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PIS", "APPR", "PPER", "$,"], "meter": "--+--+---", "measure": "anapaest.di.plus"}, "line.5": {"text": "Neiden ihn viele,", "tokens": ["Nei\u00b7den", "ihn", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und ihn umschmeichelt was da gemein ist.", "tokens": ["Und", "ihn", "um\u00b7schmei\u00b7chelt", "was", "da", "ge\u00b7mein", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PWS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er aber, droben,", "tokens": ["Er", "a\u00b7ber", ",", "dro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Suchet sich selber, welchem er diene", "tokens": ["Su\u00b7chet", "sich", "sel\u00b7ber", ",", "wel\u00b7chem", "er", "die\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "$,", "PRELS", "PPER", "PDS"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Von den Geistern, welchem strengen,", "tokens": ["Von", "den", "Geis\u00b7tern", ",", "wel\u00b7chem", "stren\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem wird er \u00e4hnlich", "tokens": ["Und", "dem", "wird", "er", "\u00e4hn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und verdient sich den Glanz", "tokens": ["Und", "ver\u00b7dient", "sich", "den", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und Stab des Gebietens,", "tokens": ["Und", "Stab", "des", "Ge\u00b7bie\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den dereinst das schnelle Gl\u00fcck ihm zuwarf,", "tokens": ["Den", "de\u00b7reinst", "das", "schnel\u00b7le", "Gl\u00fcck", "ihm", "zu\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Und k\u00e4mpft es aus,", "tokens": ["Und", "k\u00e4mpft", "es", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Unabl\u00e4ssig,", "tokens": ["Un\u00b7ab\u00b7l\u00e4s\u00b7sig", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Tagaus, tagein,", "tokens": ["Ta\u00b7gaus", ",", "ta\u00b7gein", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Jahr um Jahr,", "tokens": ["Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Und waltet des Amtes", "tokens": ["Und", "wal\u00b7tet", "des", "Am\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Wesenhaft,", "tokens": ["We\u00b7sen\u00b7haft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Und ihn gr\u00fc\u00dft,", "tokens": ["Und", "ihn", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Wo M\u00e4nner seiner gedenken,", "tokens": ["Wo", "M\u00e4n\u00b7ner", "sei\u00b7ner", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Ein sch\u00f6nes Wort:", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Bew\u00e4hrung.", "tokens": ["Be\u00b7w\u00e4h\u00b7rung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}