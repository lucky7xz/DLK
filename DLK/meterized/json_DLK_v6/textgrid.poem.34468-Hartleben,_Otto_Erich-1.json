{"textgrid.poem.34468": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Sommerabend, im trocknenden Heu,", "tokens": ["Am", "Som\u00b7mer\u00b7a\u00b7bend", ",", "im", "trock\u00b7nen\u00b7den", "Heu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "an des Berges duftigem Wiesenhang", "tokens": ["an", "des", "Ber\u00b7ges", "duf\u00b7ti\u00b7gem", "Wie\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "liegt still ein Kind und schaut hinauf", "tokens": ["liegt", "still", "ein", "Kind", "und", "schaut", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit m\u00e4rchentr\u00e4umenden Augen:", "tokens": ["mit", "m\u00e4r\u00b7chen\u00b7tr\u00e4u\u00b7men\u00b7den", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Weiche Wolkenfl\u00f6ckchen, ihr da droben,", "tokens": ["Wei\u00b7che", "Wol\u00b7ken\u00b7fl\u00f6ck\u00b7chen", ",", "ihr", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "von der Sonne rosig angehaucht,", "tokens": ["von", "der", "Son\u00b7ne", "ro\u00b7sig", "an\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "o ihr wundersch\u00f6nen Himmelssch\u00e4fchen,", "tokens": ["o", "ihr", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Him\u00b7mels\u00b7sch\u00e4f\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "heute weidet euch der liebe Gott!", "tokens": ["heu\u00b7te", "wei\u00b7det", "euch", "der", "lie\u00b7be", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}