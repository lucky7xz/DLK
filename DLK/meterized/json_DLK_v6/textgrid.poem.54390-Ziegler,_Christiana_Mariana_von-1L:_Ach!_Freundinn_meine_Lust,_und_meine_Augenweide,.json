{"textgrid.poem.54390": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! Freundinn meine Lust, und meine Augenweide,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! Freundinn meine Lust, und meine Augenweide,", "tokens": ["Ach", "!", "Freun\u00b7dinn", "mei\u00b7ne", "Lust", ",", "und", "mei\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein alles auf der Welt, mit der ich itzo leide;", "tokens": ["Mein", "al\u00b7les", "auf", "der", "Welt", ",", "mit", "der", "ich", "it\u00b7zo", "lei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor Schrecken zittert mir noch Herze, Hand, und Fu\u00df,", "tokens": ["Vor", "Schre\u00b7cken", "zit\u00b7tert", "mir", "noch", "Her\u00b7ze", ",", "Hand", ",", "und", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich kenne mich fast nicht vor Unmuth und Verdru\u00df;", "tokens": ["Ich", "ken\u00b7ne", "mich", "fast", "nicht", "vor", "Un\u00b7muth", "und", "Ver\u00b7dru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Schmerz der dich bef\u00e4llt, wirkt auch in meiner Seele.", "tokens": ["Der", "Schmerz", "der", "dich", "be\u00b7f\u00e4llt", ",", "wirkt", "auch", "in", "mei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich kann nicht bey dir seyn, das macht da\u00df ich mich qu\u00e4le.", "tokens": ["Ich", "kann", "nicht", "bey", "dir", "seyn", ",", "das", "macht", "da\u00df", "ich", "mich", "qu\u00e4\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VAINF", "$,", "PDS", "VVFIN", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer pflegt dich so wie ich? was ist dein Zeitvertreib?", "tokens": ["Wer", "pflegt", "dich", "so", "wie", "ich", "?", "was", "ist", "dein", "Zeit\u00b7ver\u00b7treib", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer sorget vor den Arzt, vor deinen matten Leib?", "tokens": ["Wer", "sor\u00b7get", "vor", "den", "Arzt", ",", "vor", "dei\u00b7nen", "mat\u00b7ten", "Leib", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer suchet Kr\u00e4uter auf, die kranke Brust zu heilen?", "tokens": ["Wer", "su\u00b7chet", "Kr\u00e4u\u00b7ter", "auf", ",", "die", "kran\u00b7ke", "Brust", "zu", "hei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O k\u00f6nnt ich mich so gleich nur in zwey Theile theilen,", "tokens": ["O", "k\u00f6nnt", "ich", "mich", "so", "gleich", "nur", "in", "zwey", "Thei\u00b7le", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich setzte mich gewi\u00df an deine Lagerstatt,", "tokens": ["Ich", "setz\u00b7te", "mich", "ge\u00b7wi\u00df", "an", "dei\u00b7ne", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich spr\u00e4ch dir freundlich zu, und w\u00e4rst du noch so matt,", "tokens": ["Ich", "spr\u00e4ch", "dir", "freund\u00b7lich", "zu", ",", "und", "w\u00e4rst", "du", "noch", "so", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie z\u00e4rtlich wollt ich dich von Grund der Seelen pflegen!", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "wollt", "ich", "dich", "von", "Grund", "der", "See\u00b7len", "pfle\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PRF", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein Schicksal la\u00df dich doch, ach la\u00df dich doch bewegen,", "tokens": ["Mein", "Schick\u00b7sal", "la\u00df", "dich", "doch", ",", "ach", "la\u00df", "dich", "doch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PPER", "ADV", "$,", "XY", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und merke dieses mal auf meinen Jammerthon:", "tokens": ["Und", "mer\u00b7ke", "die\u00b7ses", "mal", "auf", "mei\u00b7nen", "Jam\u00b7mer\u00b7thon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schenk mir die Freundinn noch, als meiner Treue Lohn.", "tokens": ["Schenk", "mir", "die", "Freun\u00b7dinn", "noch", ",", "als", "mei\u00b7ner", "Treu\u00b7e", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sag, warum wolltest du so schrecklich auf mich toben?", "tokens": ["Sag", ",", "wa\u00b7rum", "woll\u00b7test", "du", "so", "schreck\u00b7lich", "auf", "mich", "to\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich habe mich ja nie bey ihrer Gunst erhoben.", "tokens": ["Ich", "ha\u00b7be", "mich", "ja", "nie", "bey", "ih\u00b7rer", "Gunst", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du kennst die stille Lust die mir daraus entstand.", "tokens": ["Du", "kennst", "die", "stil\u00b7le", "Lust", "die", "mir", "da\u00b7raus", "ent\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wir schworen beyderseits mit aufgehobner Hand:", "tokens": ["Wir", "schwo\u00b7ren", "bey\u00b7der\u00b7seits", "mit", "auf\u00b7ge\u00b7hob\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nichts trennet unsre Brust, nichts trennet unsre Liebe,", "tokens": ["Nichts", "tren\u00b7net", "uns\u00b7re", "Brust", ",", "nichts", "tren\u00b7net", "uns\u00b7re", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und so vermehrten sich die reinen Freundschaftstriebe.", "tokens": ["Und", "so", "ver\u00b7mehr\u00b7ten", "sich", "die", "rei\u00b7nen", "Freund\u00b7schaft\u00b7strie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dein Unfall ist zu gro\u00df, das Leid so dir geschehn", "tokens": ["Dein", "Un\u00b7fall", "ist", "zu", "gro\u00df", ",", "das", "Leid", "so", "dir", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mu\u00df ich, ob gleich entfernt, vor meinen Augen sehn.", "tokens": ["Mu\u00df", "ich", ",", "ob", "gleich", "ent\u00b7fernt", ",", "vor", "mei\u00b7nen", "Au\u00b7gen", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ADV", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da unterh\u00e4ltst mich stets mit hunderttausend Klagen.", "tokens": ["Da", "un\u00b7ter\u00b7h\u00e4ltst", "mich", "stets", "mit", "hun\u00b7dert\u00b7tau\u00b7send", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch la\u00df dir auch ein Wort von deiner Freundinn sagen;", "tokens": ["Doch", "la\u00df", "dir", "auch", "ein", "Wort", "von", "dei\u00b7ner", "Freun\u00b7dinn", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nimm dich ja wohl in acht, hier gilt kein stoisch seyn:", "tokens": ["Nimm", "dich", "ja", "wohl", "in", "acht", ",", "hier", "gilt", "kein", "sto\u00b7isch", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "CARD", "$,", "ADV", "VVFIN", "PIAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn nimmt das Fieber erst den ganzen K\u00f6rper ein,", "tokens": ["Denn", "nimmt", "das", "Fie\u00b7ber", "erst", "den", "gan\u00b7zen", "K\u00f6r\u00b7per", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So mu\u00df der schwache Leib sich in die Federn h\u00fcllen.", "tokens": ["So", "mu\u00df", "der", "schwa\u00b7che", "Leib", "sich", "in", "die", "Fe\u00b7dern", "h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Erf\u00fclle dieses mal nur meinen Raht und Willen.", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "die\u00b7ses", "mal", "nur", "mei\u00b7nen", "Raht", "und", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gesetzt ein schneller Schmerz spr\u00e4ch dir das Leben ab,", "tokens": ["Ge\u00b7setzt", "ein", "schnel\u00b7ler", "Schmerz", "spr\u00e4ch", "dir", "das", "Le\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So n\u00e4hmst du mich gewi\u00df zugleich mit dir ins Grab.", "tokens": ["So", "n\u00e4hmst", "du", "mich", "ge\u00b7wi\u00df", "zu\u00b7gleich", "mit", "dir", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Tod scherzt warlich nicht, wie die Erfahrung lehret.", "tokens": ["Der", "Tod", "scherzt", "war\u00b7lich", "nicht", ",", "wie", "die", "Er\u00b7fah\u00b7rung", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So schone Freundinn dich vor die so dich verehret.", "tokens": ["So", "scho\u00b7ne", "Freun\u00b7dinn", "dich", "vor", "die", "so", "dich", "ver\u00b7eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "APPR", "ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Furcht w\u00e4r nicht so gro\u00df; allein ich weis zu wohl,", "tokens": ["Die", "Furcht", "w\u00e4r", "nicht", "so", "gro\u00df", ";", "al\u00b7lein", "ich", "weis", "zu", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "ADV", "PPER", "PTKVZ", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was ich bey deinem Fall zugleich verlieren soll.", "tokens": ["Was", "ich", "bey", "dei\u00b7nem", "Fall", "zu\u00b7gleich", "ver\u00b7lie\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Denn dein Verlust ist nicht mit etwas zu ersetzen.", "tokens": ["Denn", "dein", "Ver\u00b7lust", "ist", "nicht", "mit", "et\u00b7was", "zu", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wer deinen Werth erkennt, der weis ihn nicht zu sch\u00e4tzen.", "tokens": ["Wer", "dei\u00b7nen", "Werth", "er\u00b7kennt", ",", "der", "weis", "ihn", "nicht", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PTKVZ", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}