{"textgrid.poem.60323": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Vision", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Himmel strahlt der Mond so klar,", "tokens": ["Vom", "Him\u00b7mel", "strahlt", "der", "Mond", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greif aus, o Rappe, greif!", "tokens": ["Greif", "aus", ",", "o", "Rap\u00b7pe", ",", "greif", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NE", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Winde fliegt des Reiters Haar,", "tokens": ["Im", "Win\u00b7de", "fliegt", "des", "Rei\u00b7ters", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Rosses Mahn und Schweif.", "tokens": ["Des", "Ros\u00b7ses", "Mahn", "und", "Schweif", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf seinem Hut der Reiter tr\u00e4gt", "tokens": ["Auf", "sei\u00b7nem", "Hut", "der", "Rei\u00b7ter", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemsbart und Federnputz,", "tokens": ["Gems\u00b7bart", "und", "Fe\u00b7dern\u00b7putz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein schmerzliches Gel\u00e4chter schl\u00e4gt", "tokens": ["Ein", "schmerz\u00b7li\u00b7ches", "Ge\u00b7l\u00e4ch\u00b7ter", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er auf und schwingt den Stutz.", "tokens": ["Er", "auf", "und", "schwingt", "den", "Stutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Reiter sprengt um Mitternacht", "tokens": ["Der", "Rei\u00b7ter", "sprengt", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Land Tirol, allein;", "tokens": ["Durchs", "Land", "Ti\u00b7rol", ",", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Waldstrom braust und st\u00fcrzt mit Macht,", "tokens": ["Der", "Wald\u00b7strom", "braust", "und", "st\u00fcrzt", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Reiter holt ihn ein.", "tokens": ["Der", "Rei\u00b7ter", "holt", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Schneegans dort hoch oben ruft", "tokens": ["Die", "Schnee\u00b7gans", "dort", "hoch", "o\u00b7ben", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr schnatternd Wanderlied,", "tokens": ["Ihr", "schnat\u00b7ternd", "Wan\u00b7der\u00b7lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell zieht der Vogel in der Luft,", "tokens": ["Schnell", "zieht", "der", "Vo\u00b7gel", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Reiter schneller flieht.", "tokens": ["Der", "Rei\u00b7ter", "schnel\u00b7ler", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schnell ist der Wolkenschatten Flucht,", "tokens": ["Schnell", "ist", "der", "Wol\u00b7ken\u00b7schat\u00b7ten", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Reiter schneller noch,", "tokens": ["Der", "Rei\u00b7ter", "schnel\u00b7ler", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum braust er in der tiefen Schlucht,", "tokens": ["Kaum", "braust", "er", "in", "der", "tie\u00b7fen", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon auch am Gipfel hoch.", "tokens": ["Schon", "auch", "am", "Gip\u00b7fel", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo das Gebein der Helden liegt,", "tokens": ["Wo", "das", "Ge\u00b7bein", "der", "Hel\u00b7den", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibt er dem Ro\u00df die Sporn,", "tokens": ["Gibt", "er", "dem", "Ro\u00df", "die", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An den verge\u00dfnen Gr\u00e4bern fliegt", "tokens": ["An", "den", "ver\u00b7ge\u00df\u00b7nen", "Gr\u00e4\u00b7bern", "fliegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wild vorbei im Zorn.", "tokens": ["Er", "wild", "vor\u00b7bei", "im", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Am Wege dort ein Kruzifix,", "tokens": ["Am", "We\u00b7ge", "dort", "ein", "Kru\u00b7zi\u00b7fix", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ungl\u00fccks Herberg, ragt,", "tokens": ["Des", "Un\u00b7gl\u00fccks", "Her\u00b7berg", ",", "ragt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seitw\u00e4rts gewandten finstern Blicks", "tokens": ["Seit\u00b7w\u00e4rts", "ge\u00b7wand\u00b7ten", "fins\u00b7tern", "Blicks"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorbei der Reiter jagt.", "tokens": ["Vor\u00b7bei", "der", "Rei\u00b7ter", "jagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So reitet er durchs Land Tirol", "tokens": ["So", "rei\u00b7tet", "er", "durchs", "Land", "Ti\u00b7rol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruft so bang, so schwer:", "tokens": ["Und", "ruft", "so", "bang", ",", "so", "schwer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein sch\u00f6nes Land, leb wohl! leb wohl!", "tokens": ["\u00bb", "mein", "sch\u00f6\u00b7nes", "Land", ",", "leb", "wohl", "!", "leb", "wohl", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ADV", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du siehst mich nimmermehr!\u00ab", "tokens": ["Du", "siehst", "mich", "nim\u00b7mer\u00b7mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das letzte Heldengrab zerrei\u00dft,", "tokens": ["Das", "letz\u00b7te", "Hel\u00b7den\u00b7grab", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Reiter st\u00fcrzt hinein,", "tokens": ["Der", "Rei\u00b7ter", "st\u00fcrzt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Grab zu. Verschwunden ist der Geist", "tokens": ["Grab", "zu", ".", "Ver\u00b7schwun\u00b7den", "ist", "der", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "NN", "VAFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von Achtzehnhundertneun.", "tokens": ["Von", "Acht\u00b7zehn\u00b7hun\u00b7dert\u00b7neun", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}