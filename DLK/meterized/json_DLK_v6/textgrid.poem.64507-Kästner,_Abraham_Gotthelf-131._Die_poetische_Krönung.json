{"textgrid.poem.64507": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "131. Die poetische Kr\u00f6nung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, Gott der Dichter, mu\u00df ich's klagen,", "tokens": ["Dir", ",", "Gott", "der", "Dich\u00b7ter", ",", "mu\u00df", "ich's", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach Hermann: ", "tokens": ["Sprach", "Her\u00b7mann", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und singt ein schl\u00e4frig Lied von mir.", "tokens": ["Und", "singt", "ein", "schl\u00e4f\u00b7rig", "Lied", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey ruhig, hat Apoll gesprochen,", "tokens": ["Sey", "ru\u00b7hig", ",", "hat", "A\u00b7poll", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "NE", "VVPP", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Der Frevel ist bereits gerochen,", "tokens": ["Der", "Fre\u00b7vel", "ist", "be\u00b7reits", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}