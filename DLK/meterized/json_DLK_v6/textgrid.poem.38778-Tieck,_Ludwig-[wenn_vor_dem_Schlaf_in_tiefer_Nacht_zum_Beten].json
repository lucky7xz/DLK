{"textgrid.poem.38778": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[wenn vor dem Schlaf in tiefer Nacht zum Beten]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn vor dem Schlaf in tiefer Nacht zum Beten", "tokens": ["Wenn", "vor", "dem", "Schlaf", "in", "tie\u00b7fer", "Nacht", "zum", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz inbr\u00fcnstig kehrt, ein heilges Weinen", "tokens": ["Mein", "Herz", "in\u00b7br\u00fcns\u00b7tig", "kehrt", ",", "ein", "heil\u00b7ges", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich sehnet nach den stillen Todesst\u00e4ten,", "tokens": ["Sich", "seh\u00b7net", "nach", "den", "stil\u00b7len", "To\u00b7des\u00b7st\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nahn Engel, die mit mildem Licht erscheinen.", "tokens": ["Nahn", "En\u00b7gel", ",", "die", "mit", "mil\u00b7dem", "Licht", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ich den Blick versteh, mu\u00df vor mich treten", "tokens": ["Wie", "ich", "den", "Blick", "ver\u00b7steh", ",", "mu\u00df", "vor", "mich", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein holdes Bild, dein s\u00fcsses Blicken, meinen", "tokens": ["Dein", "hol\u00b7des", "Bild", ",", "dein", "s\u00fcs\u00b7ses", "Bli\u00b7cken", ",", "mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Thr\u00e4nen du l\u00e4chelst, mir ist als umwehten", "tokens": ["Thr\u00e4\u00b7nen", "du", "l\u00e4\u00b7chelst", ",", "mir", "ist", "als", "um\u00b7weh\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "KOKOM", "VVFIN"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Mich Himmelsd\u00fcft' in Paradieseshainen.", "tokens": ["Mich", "Him\u00b7mels\u00b7d\u00fcft'", "in", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7hai\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schlummernd streck' ich die H\u00e4nd in sch\u00f6ner Trauer,", "tokens": ["Schlum\u00b7mernd", "streck'", "ich", "die", "H\u00e4nd", "in", "sch\u00f6\u00b7ner", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Im Herzen bist du, quillst aus allen Thr\u00e4nen,", "tokens": ["Im", "Her\u00b7zen", "bist", "du", ",", "quillst", "aus", "al\u00b7len", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$,", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun wollen Arme dich und Busen fassen;", "tokens": ["Nun", "wol\u00b7len", "Ar\u00b7me", "dich", "und", "Bu\u00b7sen", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du weichst mit stillem Wink wie Nebelschauer:", "tokens": ["Du", "weichst", "mit", "stil\u00b7lem", "Wink", "wie", "Ne\u00b7bel\u00b7schau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stets blieb des Lebens Sch\u00f6nstes mir ein W\u00e4hnen,", "tokens": ["Stets", "blieb", "des", "Le\u00b7bens", "Sch\u00f6ns\u00b7tes", "mir", "ein", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es zog hinweg, hat einsam mich gelassen.", "tokens": ["Es", "zog", "hin\u00b7weg", ",", "hat", "ein\u00b7sam", "mich", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}