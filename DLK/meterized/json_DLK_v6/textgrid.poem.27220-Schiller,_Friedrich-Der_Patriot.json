{"textgrid.poem.27220": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Patriot", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Verfassung sich \u00fcberall bilde! Wie sehr ists zu w\u00fcnschen,", "tokens": ["Da\u00df", "Ver\u00b7fas\u00b7sung", "sich", "\u00fc\u00b7be\u00b7rall", "bil\u00b7de", "!", "Wie", "sehr", "ists", "zu", "w\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADV", "VVFIN", "$.", "PWAV", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Aber ihr Schw\u00e4tzer verhelft uns zu Verfassungen nicht!", "tokens": ["A\u00b7ber", "ihr", "Schw\u00e4t\u00b7zer", "ver\u00b7helft", "uns", "zu", "Ver\u00b7fas\u00b7sun\u00b7gen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKNEG", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}}}}}