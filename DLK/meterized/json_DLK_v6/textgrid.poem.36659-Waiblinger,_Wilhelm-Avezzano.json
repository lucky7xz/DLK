{"textgrid.poem.36659": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Avezzano", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundliche H\u00fcgel umgeben das Dorf, den dankbaren Boden", "tokens": ["Freund\u00b7li\u00b7che", "H\u00fc\u00b7gel", "um\u00b7ge\u00b7ben", "das", "Dorf", ",", "den", "dank\u00b7ba\u00b7ren", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hat ein flei\u00dfiger Schlag s\u00fcdlicher Leutchen bepflanzt.", "tokens": ["Hat", "ein", "flei\u00b7\u00dfi\u00b7ger", "Schlag", "s\u00fcd\u00b7li\u00b7cher", "Leut\u00b7chen", "be\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Heimathlich f\u00fchl' ich mich hier, und kehr' ich nach Hause zum Heerde,", "tokens": ["Hei\u00b7math\u00b7lich", "f\u00fchl'", "ich", "mich", "hier", ",", "und", "kehr'", "ich", "nach", "Hau\u00b7se", "zum", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sagt mir ein M\u00e4dchen, dir bl\u00fcht nirgends ein heimathlich Gl\u00fcck.", "tokens": ["Sagt", "mir", "ein", "M\u00e4d\u00b7chen", ",", "dir", "bl\u00fcht", "nir\u00b7gends", "ein", "hei\u00b7math\u00b7lich", "Gl\u00fcck", "."], "token_info": ["word", "word", "