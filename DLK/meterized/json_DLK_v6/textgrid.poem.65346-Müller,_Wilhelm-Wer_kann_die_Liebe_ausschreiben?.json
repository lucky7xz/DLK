{"textgrid.poem.65346": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wer kann die Liebe ausschreiben?", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4ren Fl\u00fcss' und Meere Tinte, w\u00e4r' der Himmel mein Papier,", "tokens": ["W\u00e4\u00b7ren", "Fl\u00fcss'", "und", "Mee\u00b7re", "Tin\u00b7te", ",", "w\u00e4r'", "der", "Him\u00b7mel", "mein", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "NE", "$,", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "W\u00fcchsen Federn wie die \u00c4hren auf der weiten Erde mir,", "tokens": ["W\u00fcch\u00b7sen", "Fe\u00b7dern", "wie", "die", "\u00c4h\u00b7ren", "auf", "der", "wei\u00b7ten", "Er\u00b7de", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "H\u00fclfen mir die Engel schreiben um die Wette Tag und Nacht,", "tokens": ["H\u00fcl\u00b7fen", "mir", "die", "En\u00b7gel", "schrei\u00b7ben", "um", "die", "Wet\u00b7te", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sag', wann w\u00e4r' es ausgeschrieben, was die Lieb' in mir gedacht?", "tokens": ["Sag'", ",", "wann", "w\u00e4r'", "es", "aus\u00b7ge\u00b7schrie\u00b7ben", ",", "was", "die", "Lieb'", "in", "mir", "ge\u00b7dacht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}