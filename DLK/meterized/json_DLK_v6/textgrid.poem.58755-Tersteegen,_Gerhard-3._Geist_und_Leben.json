{"textgrid.poem.58755": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "3. Geist und Leben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein Geist, nicht grob sichtbar,", "tokens": ["Du", "bist", "ein", "Geist", ",", "nicht", "grob", "sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Denken und ein Leben gar,", "tokens": ["Ein", "Den\u00b7ken", "und", "ein", "Le\u00b7ben", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das unbeschr\u00e4nkt, frei, ganz vollkommen,", "tokens": ["Das", "un\u00b7be\u00b7schr\u00e4nkt", ",", "frei", ",", "ganz", "voll\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der allen Geistern Leben gibst", "tokens": ["Der", "al\u00b7len", "Geis\u00b7tern", "Le\u00b7ben", "gibst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und den, der geistlich lebet, liebst,", "tokens": ["Und", "den", ",", "der", "geist\u00b7lich", "le\u00b7bet", ",", "liebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Herz der Grobheit ist entnommen;", "tokens": ["Des", "Herz", "der", "Grob\u00b7heit", "ist", "ent\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sehn nur Schildereien hier,", "tokens": ["Wir", "sehn", "nur", "Schil\u00b7de\u00b7rei\u00b7en", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Geist und Wahrheit dient man dir,", "tokens": ["Im", "Geist", "und", "Wahr\u00b7heit", "dient", "man", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}