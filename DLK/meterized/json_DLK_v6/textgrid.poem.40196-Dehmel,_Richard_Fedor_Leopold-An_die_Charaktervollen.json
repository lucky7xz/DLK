{"textgrid.poem.40196": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "An die Charaktervollen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr meint, da\u00df ernst und fest ihr seid,", "tokens": ["Ihr", "meint", ",", "da\u00df", "ernst", "und", "fest", "ihr", "seid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "KON", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nur weil ihr kalt und streng euch stellt? \u2013", "tokens": ["nur", "weil", "ihr", "kalt", "und", "streng", "euch", "stellt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "KON", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wahre braucht kein k\u00fcnstlich Kleid,", "tokens": ["Das", "Wah\u00b7re", "braucht", "kein", "k\u00fcnst\u00b7lich", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wahrer Ernst schaut heiter in die Welt!", "tokens": ["und", "wah\u00b7rer", "Ernst", "schaut", "hei\u00b7ter", "in", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}