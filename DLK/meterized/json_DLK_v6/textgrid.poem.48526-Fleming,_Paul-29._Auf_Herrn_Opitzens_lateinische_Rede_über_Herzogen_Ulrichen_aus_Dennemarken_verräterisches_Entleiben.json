{"textgrid.poem.48526": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "29. Auf Herrn Opitzens lateinische Rede \u00fcber Herzogen Ulrichen aus Dennemarken verr\u00e4terisches Entleiben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich las, ich \u00fcberlas, ich las es zehnmal wieder:", "tokens": ["Ich", "las", ",", "ich", "\u00fc\u00b7ber\u00b7las", ",", "ich", "las", "es", "zehn\u00b7mal", "wie\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "es war ein gleicher Geist der Reden wie der Lieder.", "tokens": ["es", "war", "ein", "glei\u00b7cher", "Geist", "der", "Re\u00b7den", "wie", "der", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Streicht nur den Titul aus, wo seiner wird gedacht!", "tokens": ["Streicht", "nur", "den", "Ti\u00b7tul", "aus", ",", "wo", "sei\u00b7ner", "wird", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$,", "PWAV", "PPOSAT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein' iede Zeile sagt, da\u00df Opitz sie gemacht.", "tokens": ["Ein'", "ie\u00b7de", "Zei\u00b7le", "sagt", ",", "da\u00df", "O\u00b7pitz", "sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word",