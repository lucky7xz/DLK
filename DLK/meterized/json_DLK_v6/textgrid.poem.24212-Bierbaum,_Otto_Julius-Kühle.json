{"textgrid.poem.24212": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles das ist nur ein Tr\u00e4umen,", "tokens": ["Al\u00b7les", "das", "ist", "nur", "ein", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und ich sollte nie erwachen:", "tokens": ["Und", "ich", "soll\u00b7te", "nie", "er\u00b7wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das w\u00e4r sch\u00f6n.", "tokens": ["Das", "w\u00e4r", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Denn der Tag hat kalte Farben,", "tokens": ["Denn", "der", "Tag", "hat", "kal\u00b7te", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und die Wahrheit geht in Wolle,", "tokens": ["Und", "die", "Wahr\u00b7heit", "geht", "in", "Wol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rauh und grau.", "tokens": ["Rauh", "und", "grau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wirklichkeit, die alte Vettel,", "tokens": ["Wirk\u00b7lich\u00b7keit", ",", "die", "al\u00b7te", "Vet\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcckt schon ihre Klapperschere", "tokens": ["Z\u00fcckt", "schon", "ih\u00b7re", "Klap\u00b7per\u00b7sche\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie grinst:", "tokens": ["Und", "sie", "grinst", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Weg die bunten Seidenb\u00e4nder,", "tokens": ["Weg", "die", "bun\u00b7ten", "Sei\u00b7den\u00b7b\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg die langen Ringellocken,", "tokens": ["Weg", "die", "lan\u00b7gen", "Rin\u00b7gel\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weg den Tand!", "tokens": ["Weg", "den", "Tand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Und ein kurzer Krampf im Herzen", "tokens": ["Und", "ein", "kur\u00b7zer", "Krampf", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das alte b\u00f6se Lachen:", "tokens": ["Und", "das", "al\u00b7te", "b\u00f6\u00b7se", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du wohl?", "tokens": ["Siehst", "du", "wohl", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}