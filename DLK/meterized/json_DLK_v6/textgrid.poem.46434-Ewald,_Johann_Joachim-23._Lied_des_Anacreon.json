{"textgrid.poem.46434": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "23. Lied des Anacreon", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Sterblichen ihr Leben", "tokens": ["Wenn", "Sterb\u00b7li\u00b7chen", "ihr", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Reichthum l\u00e4nger w\u00fcrde", "tokens": ["Durch", "Reicht\u00b7hum", "l\u00e4n\u00b7ger", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So h\u00e4ufft' ich selber Sch\u00e4tze", "tokens": ["So", "h\u00e4ufft'", "ich", "sel\u00b7ber", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wenn der Tod denn k\u00e4me,", "tokens": ["Und", "wenn", "der", "Tod", "denn", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "G\u00e4b' ich ihm Gold damit er", "tokens": ["G\u00e4b'", "ich", "ihm", "Gold", "da\u00b7mit", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "NN", "KOUS", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich leben lie\u00df' und gienge.", "tokens": ["Mich", "le\u00b7ben", "lie\u00df'", "und", "gien\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch da wir nicht das Leben", "tokens": ["Doch", "da", "wir", "nicht", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch Gold erkauffen k\u00f6nnen,", "tokens": ["Durch", "Gold", "er\u00b7kauf\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da mich der Tod nicht schonet,", "tokens": ["Da", "mich", "der", "Tod", "nicht", "scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Was gr\u00e4m' ich mich, was seufz' ich?", "tokens": ["Was", "gr\u00e4m'", "ich", "mich", ",", "was", "seuf\u00b7z'", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was soll mir Gold und Reichthum?", "tokens": ["Was", "soll", "mir", "Gold", "und", "Reicht\u00b7hum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Viel lieber will ich trinken,", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die besten Weine trinken,", "tokens": ["Die", "bes\u00b7ten", "Wei\u00b7ne", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ich will mit Freunden lachen", "tokens": ["Ich", "will", "mit", "Freun\u00b7den", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und auf dem sanften Rasen", "tokens": ["Und", "auf", "dem", "sanf\u00b7ten", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ein sch\u00f6nes Kind umarmen.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Kind", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}