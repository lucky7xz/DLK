{"textgrid.poem.62150": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ihr wi\u00dft wohl, wen ich meine]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wi\u00dft wohl, wen ich meine,", "tokens": ["Ihr", "wi\u00dft", "wohl", ",", "wen", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stadt liegt an der Seine,", "tokens": ["Die", "Stadt", "liegt", "an", "der", "Sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entschieden ist's die sch\u00f6nste Stadt,", "tokens": ["Ent\u00b7schie\u00b7den", "ist's", "die", "sch\u00f6ns\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die man wohl je gesehen hat,", "tokens": ["Die", "man", "wohl", "je", "ge\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Haar ist lang und ist auch dicht,", "tokens": ["Ihr", "Haar", "ist", "lang", "und", "ist", "auch", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat ein wunderbar Gesicht,", "tokens": ["Sie", "hat", "ein", "wun\u00b7der\u00b7bar", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zauberhaft wie ein Gedicht", "tokens": ["Und", "zau\u00b7ber\u00b7haft", "wie", "ein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Laut zu meinem Herzen spricht,", "tokens": ["Ihr", "Laut", "zu", "mei\u00b7nem", "Her\u00b7zen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du kennst ach, die Geschichte nicht,", "tokens": ["Du", "kennst", "ach", ",", "die", "Ge\u00b7schich\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "XY", "$,", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie das Herz ihr brach und bricht", "tokens": ["Und", "wie", "das", "Herz", "ihr", "brach", "und", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mond mit rotem Scheine", "tokens": ["Der", "Mond", "mit", "ro\u00b7tem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beleuchtet Stadt und Seine.", "tokens": ["Be\u00b7leuch\u00b7tet", "Stadt", "und", "Sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}