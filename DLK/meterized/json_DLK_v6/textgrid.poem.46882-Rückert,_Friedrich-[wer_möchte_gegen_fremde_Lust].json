{"textgrid.poem.46882": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wer m\u00f6chte gegen fremde Lust]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer m\u00f6chte gegen fremde Lust", "tokens": ["Wer", "m\u00f6ch\u00b7te", "ge\u00b7gen", "frem\u00b7de", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertauschen eigne Leiden?", "tokens": ["Ver\u00b7tau\u00b7schen", "eig\u00b7ne", "Lei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dr\u00fcck eine Tochter an die Brust", "tokens": ["Dr\u00fcck", "ei\u00b7ne", "Toch\u00b7ter", "an", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer will, ich will's nicht neiden.", "tokens": ["Wer", "will", ",", "ich", "will's", "nicht", "nei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich will am Schmerz und am Verlust", "tokens": ["Ich", "will", "am", "Schmerz", "und", "am", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie am Besitz mich weiden;", "tokens": ["Wie", "am", "Be\u00b7sitz", "mich", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich habe doch, mir ist's bewu\u00dft,", "tokens": ["Ich", "ha\u00b7be", "doch", ",", "mir", "ist's", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da sch\u00f6nste Kind von beiden:", "tokens": ["Da", "sch\u00f6ns\u00b7te", "Kind", "von", "bei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nicht m\u00f6cht' ich gegen fremde Lust", "tokens": ["Nicht", "m\u00f6cht'", "ich", "ge\u00b7gen", "frem\u00b7de", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vertauschen meine Leiden", "tokens": ["Ver\u00b7tau\u00b7schen", "mei\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}