{"dta.poem.83": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Salinden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will wie Hercules der H\u00f6llen Riegel brechen/", "tokens": ["Ich", "will", "wie", "Her\u00b7cu\u00b7les", "der", "H\u00f6l\u00b7len", "Rie\u00b7gel", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich will/ wenn ", "tokens": ["Ich", "will", "/", "wenn"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich will wie ", "tokens": ["Ich", "will", "wie"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich will mit ", "tokens": ["Ich", "will", "mit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich will den ", "tokens": ["Ich", "will", "den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Durch mich soll Babilon den Thurm gebauet sehn/", "tokens": ["Durch", "mich", "soll", "Ba\u00b7bi\u00b7lon", "den", "Thurm", "ge\u00b7bau\u00b7et", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "NE", "ART", "NN", "VVPP", "VVINF", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Des Himmels St\u00fcrmung soll von meiner Hand geschehn.", "tokens": ["Des", "Him\u00b7mels", "St\u00fcr\u00b7mung", "soll", "von", "mei\u00b7ner", "Hand", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich will in Wein verkehrn das Wasser in den B\u00e4chen.", "tokens": ["Ich", "will", "in", "Wein", "ver\u00b7kehrn", "das", "Was\u00b7ser", "in", "den", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich will ", "tokens": ["Ich", "will"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Ich will den grossen Weg zur blauem H\u00f6he messen.", "tokens": ["Ich", "will", "den", "gros\u00b7sen", "Weg", "zur", "blau\u00b7em", "H\u00f6\u00b7he", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich will das weite Meer in einem Eymer f\u00fcllen/", "tokens": ["Ich", "will", "das", "wei\u00b7te", "Meer", "in", "ei\u00b7nem", "Ey\u00b7mer", "f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich will den heissen Durst mit warmen Ertze stillen.", "tokens": ["Ich", "will", "den", "heis\u00b7sen", "Durst", "mit", "war\u00b7men", "Ert\u00b7ze", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dis alles/ und noch mehr soll meine Arbeit seyn/", "tokens": ["Dis", "al\u00b7les", "/", "und", "noch", "mehr", "soll", "mei\u00b7ne", "Ar\u00b7beit", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "KON", "ADV", "ADV", "VMFIN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn mich ", "tokens": ["Wenn", "mich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}