{"textgrid.poem.25001": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lichter schon werden die Reben der Laube,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lichter schon werden die Reben der Laube,", "tokens": ["Lich\u00b7ter", "schon", "wer\u00b7den", "die", "Re\u00b7ben", "der", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Drunter im Lenz wir, im Herbste geruht,", "tokens": ["Drun\u00b7ter", "im", "Lenz", "wir", ",", "im", "Herbs\u00b7te", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und, die wir reifen gesehen, die Traube,", "tokens": ["Und", ",", "die", "wir", "rei\u00b7fen", "ge\u00b7se\u00b7hen", ",", "die", "Trau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VVFIN", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Str\u00f6mt auf die Kelter die goldene Flut.", "tokens": ["Str\u00f6mt", "auf", "die", "Kel\u00b7ter", "die", "gol\u00b7de\u00b7ne", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald als Wein in feurigen Wogen", "tokens": ["Bald", "als", "Wein", "in", "feu\u00b7ri\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gie\u00dfen wird sie die Glut des August,", "tokens": ["Gie\u00b7\u00dfen", "wird", "sie", "die", "Glut", "des", "Au\u00b7gust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die sie am flammenden Mittag gesogen,", "tokens": ["Die", "sie", "am", "flam\u00b7men\u00b7den", "Mit\u00b7tag", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns beim Dezemberfrost in die Brust.", "tokens": ["Uns", "beim", "De\u00b7zem\u00b7berf\u00b7rost", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "So um Verlornes wie sollten wir klagen?", "tokens": ["So", "um", "Ver\u00b7lor\u00b7nes", "wie", "soll\u00b7ten", "wir", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KOKOM", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Immer vom Liede der Nachtigall", "tokens": ["Im\u00b7mer", "vom", "Lie\u00b7de", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "T\u00f6nt aus den wonnigen Junitagen", "tokens": ["T\u00f6nt", "aus", "den", "won\u00b7ni\u00b7gen", "Ju\u00b7ni\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Uns in der Seele der Wiederhall.", "tokens": ["Uns", "in", "der", "See\u00b7le", "der", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Gingen alle zu Grab, die uns teuer \u2013", "tokens": ["Gin\u00b7gen", "al\u00b7le", "zu", "Grab", ",", "die", "uns", "teu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von der Liebe, die wir geliebt,", "tokens": ["Von", "der", "Lie\u00b7be", ",", "die", "wir", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ewig erf\u00fcllt uns das w\u00e4rmende Feuer,", "tokens": ["E\u00b7wig", "er\u00b7f\u00fcllt", "uns", "das", "w\u00e4r\u00b7men\u00b7de", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ob auch das Leben zu Asche zerstiebt!", "tokens": ["Ob", "auch", "das", "Le\u00b7ben", "zu", "A\u00b7sche", "zer\u00b7stiebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}