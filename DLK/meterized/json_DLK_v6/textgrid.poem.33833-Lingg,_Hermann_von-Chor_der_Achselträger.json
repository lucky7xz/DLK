{"textgrid.poem.33833": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Chor der Achseltr\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wir schm\u00e4hten, la\u00dft uns loben,", "tokens": ["Was", "wir", "schm\u00e4h\u00b7ten", ",", "la\u00dft", "uns", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was wir lobten, sei geschm\u00e4ht,", "tokens": ["Was", "wir", "lob\u00b7ten", ",", "sei", "ge\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil vielleicht der Wind von oben", "tokens": ["Weil", "viel\u00b7leicht", "der", "Wind", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Anders heut als gestern weht.", "tokens": ["An\u00b7ders", "heut", "als", "ge\u00b7stern", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Leise, Freunde, leise treten", "tokens": ["Lei\u00b7se", ",", "Freun\u00b7de", ",", "lei\u00b7se", "tre\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ziemt dem Achseltr\u00e4gerchor.", "tokens": ["Ziemt", "dem", "Ach\u00b7sel\u00b7tr\u00e4\u00b7ger\u00b7chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist es Zeit, um anzubeten?", "tokens": ["Ist", "es", "Zeit", ",", "um", "an\u00b7zu\u00b7be\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "KOUI", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lauschet mit gespanntem Ohr!", "tokens": ["Lau\u00b7schet", "mit", "ge\u00b7spann\u00b7tem", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es Zeit zum Lispeln, S\u00e4useln,", "tokens": ["Ist", "es", "Zeit", "zum", "Lis\u00b7peln", ",", "S\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPRART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder rauh die Stirn zu kr\u00e4useln?", "tokens": ["O\u00b7der", "rauh", "die", "Stirn", "zu", "kr\u00e4u\u00b7seln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, es geht gewi\u00df was vor!", "tokens": ["Ach", ",", "es", "geht", "ge\u00b7wi\u00df", "was", "vor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "PWS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer es w\u00fc\u00dfte, ob ein Tr\u00e4nchen,", "tokens": ["Wer", "es", "w\u00fc\u00df\u00b7te", ",", "ob", "ein", "Tr\u00e4n\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ein L\u00e4cheln Vorschrift ist?", "tokens": ["Ob", "ein", "L\u00e4\u00b7cheln", "Vor\u00b7schrift", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alt und rostig ist das F\u00e4hnchen,", "tokens": ["Alt", "und", "ros\u00b7tig", "ist", "das", "F\u00e4hn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Welches sich zu drehn vergi\u00dft!", "tokens": ["Wel\u00b7ches", "sich", "zu", "drehn", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor dem Unsinn, vor der Fadheit", "tokens": ["Vor", "dem", "Un\u00b7sinn", ",", "vor", "der", "Fad\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Legen wir uns auf den Bauch,", "tokens": ["Le\u00b7gen", "wir", "uns", "auf", "den", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn verha\u00dft ist uns Geradheit,", "tokens": ["Denn", "ver\u00b7ha\u00dft", "ist", "uns", "Ge\u00b7rad\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wahrheit ist nur Dunst und Rauch.", "tokens": ["Wahr\u00b7heit", "ist", "nur", "Dunst", "und", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wahrheit ist allein die Gunst;", "tokens": ["Wahr\u00b7heit", "ist", "al\u00b7lein", "die", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Sie zu haschen, sie zu fassen,", "tokens": ["Sie", "zu", "ha\u00b7schen", ",", "sie", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ewig nicht mehr auszulassen,", "tokens": ["E\u00b7wig", "nicht", "mehr", "aus\u00b7zu\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ist der Menschen h\u00f6chste Kunst.", "tokens": ["Ist", "der", "Men\u00b7schen", "h\u00f6chs\u00b7te", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, wie schwierig ist's, zu treffen,", "tokens": ["Ach", ",", "wie", "schwie\u00b7rig", "ist's", ",", "zu", "tref\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob die Segel einzureffen", "tokens": ["Ob", "die", "Se\u00b7gel", "ein\u00b7zu\u00b7ref\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder auszuspannen sind!", "tokens": ["O\u00b7der", "aus\u00b7zu\u00b7span\u00b7nen", "sind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll man tadeln, soll man r\u00fchmen?", "tokens": ["Soll", "man", "ta\u00b7deln", ",", "soll", "man", "r\u00fch\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll man Schmeichelei verbl\u00fcmen,", "tokens": ["Soll", "man", "Schmei\u00b7che\u00b7lei", "ver\u00b7bl\u00fc\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll man taub sein oder blind?", "tokens": ["Soll", "man", "taub", "sein", "o\u00b7der", "blind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gut ist's immer, sich verneigen,", "tokens": ["Gut", "ist's", "im\u00b7mer", ",", "sich", "ver\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "$,", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch gef\u00e4hrlich ist selbst schweigen,", "tokens": ["Doch", "ge\u00b7f\u00e4hr\u00b7lich", "ist", "selbst", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn man Abscheu zeigen soll,", "tokens": ["Wenn", "man", "Ab\u00b7scheu", "zei\u00b7gen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Fl\u00fcsternd deutlich anzuspielen,", "tokens": ["Fl\u00fcs\u00b7ternd", "deut\u00b7lich", "an\u00b7zu\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Links und rechts zugleich zu schielen", "tokens": ["Links", "und", "rechts", "zu\u00b7gleich", "zu", "schie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hier ein ", "tokens": ["Hier", "ein"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "O wie bangt uns vor den Wahlen,", "tokens": ["O", "wie", "bangt", "uns", "vor", "den", "Wah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Frage wird entstehn:", "tokens": ["Wenn", "die", "Fra\u00b7ge", "wird", "ent\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll man mit den Liberalen", "tokens": ["Soll", "man", "mit", "den", "Li\u00b7be\u00b7ra\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder mit den Klerikalen,", "tokens": ["O\u00b7der", "mit", "den", "Kle\u00b7ri\u00b7ka\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder gar mit beiden gehn?", "tokens": ["O\u00b7der", "gar", "mit", "bei\u00b7den", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, wem beides w\u00e4r' erreichbar,", "tokens": ["Ach", ",", "wem", "bei\u00b7des", "w\u00e4r'", "er\u00b7reich\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Einem Gott w\u00e4r' der vergleichbar!", "tokens": ["Ei\u00b7nem", "Gott", "w\u00e4r'", "der", "ver\u00b7gleich\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wie beneiden wir dich schon,", "tokens": ["Wie", "be\u00b7nei\u00b7den", "wir", "dich", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Krokodil, um deine Tr\u00e4ne,", "tokens": ["Kro\u00b7ko\u00b7dil", ",", "um", "dei\u00b7ne", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um dein Winseln dich, Hy\u00e4ne,", "tokens": ["Um", "dein", "Win\u00b7seln", "dich", ",", "Hy\u00b7\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erst dich, Cham\u00e4leon!", "tokens": ["Und", "erst", "dich", ",", "Cha\u00b7m\u00e4\u00b7le\u00b7on", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}