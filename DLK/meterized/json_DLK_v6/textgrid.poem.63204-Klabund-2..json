{"textgrid.poem.63204": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann ich denn noch Verse singen,", "tokens": ["Kann", "ich", "denn", "noch", "Ver\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich hinter St\u00e4ben sitze?", "tokens": ["Wo", "ich", "hin\u00b7ter", "St\u00e4\u00b7ben", "sit\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Donner donnre, Blitze blitze,", "tokens": ["Don\u00b7ner", "donn\u00b7re", ",", "Blit\u00b7ze", "blit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Wand will nicht zerspringen.", "tokens": ["Und", "die", "Wand", "will", "nicht", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ginge doch die T\u00fcr und k\u00e4me", "tokens": ["Gin\u00b7ge", "doch", "die", "T\u00fcr", "und", "k\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine frauliche Gestalt,", "tokens": ["Ei\u00b7ne", "frau\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mich bei den H\u00e4nden n\u00e4hme,", "tokens": ["Die", "mich", "bei", "den", "H\u00e4n\u00b7den", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie sei M\u00e4dchen oder alt.", "tokens": ["Sie", "sei", "M\u00e4d\u00b7chen", "o\u00b7der", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn der Tisch sich doch belebte,", "tokens": ["Wenn", "der", "Tisch", "sich", "doch", "be\u00b7leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn mein Mantel mich umfinge!", "tokens": ["Wenn", "mein", "Man\u00b7tel", "mich", "um\u00b7fin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dieses Kissen an mir hinge,", "tokens": ["Die\u00b7ses", "Kis\u00b7sen", "an", "mir", "hin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dieses Bildnis \u2013 wenn es lebte!", "tokens": ["Die\u00b7ses", "Bild\u00b7nis", "\u2013", "wenn", "es", "leb\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}