{"textgrid.poem.54985": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Spr\u00f6de", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem reinsten Fr\u00fchlingsmorgen", "tokens": ["An", "dem", "reins\u00b7ten", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging die Sch\u00e4ferin und sang,", "tokens": ["Ging", "die", "Sch\u00e4\u00b7fe\u00b7rin", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jung und sch\u00f6n und ohne Sorgen,", "tokens": ["Jung", "und", "sch\u00f6n", "und", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es durch die Felder klang,", "tokens": ["Da\u00df", "es", "durch", "die", "Fel\u00b7der", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So la la! le ralla!", "tokens": ["So", "la", "la", "!", "le", "ral\u00b7la", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "$.", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Thyrsis bot ihr f\u00fcr ein M\u00e4ulchen", "tokens": ["Thyr\u00b7sis", "bot", "ihr", "f\u00fcr", "ein", "M\u00e4ul\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwei, drei Sch\u00e4fchen gleich am Ort,", "tokens": ["Zwei", ",", "drei", "Sch\u00e4f\u00b7chen", "gleich", "am", "Ort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "CARD", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schalkhaft blickte sie ein Weilchen;", "tokens": ["Schalk\u00b7haft", "blick\u00b7te", "sie", "ein", "Weil\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch sie sang und lachte fort,", "tokens": ["Doch", "sie", "sang", "und", "lach\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "So la la! le ralla!", "tokens": ["So", "la", "la", "!", "le", "ral\u00b7la", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "$.", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ein andrer bot ihr B\u00e4nder,", "tokens": ["Und", "ein", "an\u00b7drer", "bot", "ihr", "B\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der dritte bot sein Herz;", "tokens": ["Und", "der", "drit\u00b7te", "bot", "sein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Doch sie trieb mit Herz und B\u00e4ndern", "tokens": ["Doch", "sie", "trieb", "mit", "Herz", "und", "B\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So wie mit den L\u00e4mmern Scherz,", "tokens": ["So", "wie", "mit", "den", "L\u00e4m\u00b7mern", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nur la la! le ralla!", "tokens": ["Nur", "la", "la", "!", "le", "ral\u00b7la", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "$.", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}