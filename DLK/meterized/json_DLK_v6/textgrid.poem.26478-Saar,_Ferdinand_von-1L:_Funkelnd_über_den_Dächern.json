{"textgrid.poem.26478": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Funkelnd \u00fcber den D\u00e4chern", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Funkelnd \u00fcber den D\u00e4chern", "tokens": ["Fun\u00b7kelnd", "\u00fc\u00b7ber", "den", "D\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Liegt der hei\u00dfe Strahl;", "tokens": ["Liegt", "der", "hei\u00b7\u00dfe", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, kein L\u00fcften, kein F\u00e4chern", "tokens": ["Ach", ",", "kein", "L\u00fcf\u00b7ten", ",", "kein", "F\u00e4\u00b7chern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Lindert die sengende Qual.", "tokens": ["Lin\u00b7dert", "die", "sen\u00b7gen\u00b7de", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stumm in der H\u00e4user Schatten", "tokens": ["Stumm", "in", "der", "H\u00e4u\u00b7ser", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gehen die Menschen hin;", "tokens": ["Ge\u00b7hen", "die", "Men\u00b7schen", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von W\u00e4ldern und gr\u00fcnen Matten", "tokens": ["Von", "W\u00e4l\u00b7dern", "und", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Tr\u00e4umt ihr lechzender Sinn.", "tokens": ["Tr\u00e4umt", "ihr", "lech\u00b7zen\u00b7der", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Leiser rollen die Wagen,", "tokens": ["Lei\u00b7ser", "rol\u00b7len", "die", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Pl\u00e4tschert der Brunnen Fluth;", "tokens": ["Pl\u00e4t\u00b7schert", "der", "Brun\u00b7nen", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In solchen schlummernden Tagen", "tokens": ["In", "sol\u00b7chen", "schlum\u00b7mern\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Selbst die Liebe ruht.", "tokens": ["Selbst", "die", "Lie\u00b7be", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Einsam im weiten Raume", "tokens": ["Ein\u00b7sam", "im", "wei\u00b7ten", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schlummerst auch du, mein Herz,", "tokens": ["Schlum\u00b7merst", "auch", "du", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und leis' nur wie im Traume", "tokens": ["Und", "leis'", "nur", "wie", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchzuckt dich der Sehnsucht Schmerz.", "tokens": ["Durch\u00b7zuckt", "dich", "der", "Sehn\u00b7sucht", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}}}}