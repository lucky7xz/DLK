{"textgrid.poem.64102": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Nemesis", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6tter lieben was bescheiden:", "tokens": ["Die", "G\u00f6t\u00b7ter", "lie\u00b7ben", "was", "be\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie segnen reich das Werk der Pflicht:", "tokens": ["Sie", "seg\u00b7nen", "reich", "das", "Werk", "der", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Stolze wollen sie nicht leiden,", "tokens": ["Das", "Stol\u00b7ze", "wol\u00b7len", "sie", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sich vom heil'gen Ma\u00df will scheiden \u2013", "tokens": ["Das", "sich", "vom", "heil'\u00b7gen", "Ma\u00df", "will", "schei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch neidisch sind die G\u00f6tter nicht.", "tokens": ["Doch", "nei\u00b7disch", "sind", "die", "G\u00f6t\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Pfl\u00fcger, der die Frucht der Erde", "tokens": ["Dem", "Pfl\u00fc\u00b7ger", ",", "der", "die", "Frucht", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit stillem Flei\u00dfe schwer gewinnt,", "tokens": ["Mit", "stil\u00b7lem", "Flei\u00b7\u00dfe", "schwer", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er am fromm bekr\u00e4nzten Herde", "tokens": ["Wenn", "er", "am", "fromm", "be\u00b7kr\u00e4nz\u00b7ten", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Weine l\u00f6set die Beschwerde, \u2013", "tokens": ["Im", "Wei\u00b7ne", "l\u00f6\u00b7set", "die", "Be\u00b7schwer\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm sind die G\u00f6tter hold gesinnt.", "tokens": ["Ihm", "sind", "die", "G\u00f6t\u00b7ter", "hold", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Schiffer, der den Dioskuren", "tokens": ["Den", "Schif\u00b7fer", ",", "der", "den", "Dios\u00b7ku\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vertraut und nicht dem eignen Mast", "tokens": ["Ver\u00b7traut", "und", "nicht", "dem", "eig\u00b7nen", "Mast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, landet er an fremden Fluren,", "tokens": ["Und", ",", "lan\u00b7det", "er", "an", "frem\u00b7den", "Flu\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den G\u00f6ttern dankt, die mit ihm fuhren, \u2013", "tokens": ["Den", "G\u00f6t\u00b7tern", "dankt", ",", "die", "mit", "ihm", "fuh\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist kein Gott, der solchen ha\u00dft.", "tokens": ["Es", "ist", "kein", "Gott", ",", "der", "sol\u00b7chen", "ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch der die freien L\u00fcfte wollte", "tokens": ["Doch", "der", "die", "frei\u00b7en", "L\u00fcf\u00b7te", "woll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich unterwerfen: Ikarus,", "tokens": ["Sich", "un\u00b7ter\u00b7wer\u00b7fen", ":", "I\u00b7ka\u00b7rus", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PRF", "VVINF", "$.", "NE", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Er wagte, was der Mensch nicht sollte,", "tokens": ["Er", "wag\u00b7te", ",", "was", "der", "Mensch", "nicht", "soll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihm der Gott des \u00c4thers grollte,", "tokens": ["Da\u00df", "ihm", "der", "Gott", "des", "\u00c4\u00b7thers", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und warf ihn in den Tartarus.", "tokens": ["Und", "warf", "ihn", "in", "den", "Tar\u00b7ta\u00b7rus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu stolz hat Niobe gesprochen,", "tokens": ["Zu", "stolz", "hat", "Ni\u00b7o\u00b7be", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu sicher Kr\u00f6sos sich gesonnt,", "tokens": ["Zu", "si\u00b7cher", "Kr\u00f6\u00b7sos", "sich", "ge\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Antigone das Recht gebrochen,", "tokens": ["An\u00b7ti\u00b7go\u00b7ne", "das", "Recht", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und Xerxes hat das Land durchstochen", "tokens": ["Und", "Xe\u00b7rxes", "hat", "das", "Land", "durch\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcberbr\u00fcckt den Hellespont!", "tokens": ["Und", "\u00fc\u00b7berb\u00b7r\u00fcckt", "den", "Hel\u00b7les\u00b7pont", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und Pha\u00ebthon, der staubgeboren", "tokens": ["Und", "Pha\u00ebthon", ",", "der", "staub\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Gott des Lichtes griff ins Amt,", "tokens": ["Dem", "Gott", "des", "Lich\u00b7tes", "griff", "ins", "Amt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Prometheus, der um Menschentoren", "tokens": ["Pro\u00b7me\u00b7theus", ",", "der", "um", "Men\u00b7schen\u00b7to\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Den Bund der G\u00f6tter abgeschworen, \u2013", "tokens": ["Den", "Bund", "der", "G\u00f6t\u00b7ter", "ab\u00b7ge\u00b7schwo\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erlegen sind sie allgesamt.", "tokens": ["Er\u00b7le\u00b7gen", "sind", "sie", "all\u00b7ge\u00b7samt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn, wer mit ungez\u00e4hmten Sinnen", "tokens": ["Denn", ",", "wer", "mit", "un\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ew'gen Ordnungen verga\u00df,", "tokens": ["Der", "ew'\u00b7gen", "Ord\u00b7nun\u00b7gen", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Unerh\u00f6rte zu gewinnen,", "tokens": ["Das", "Un\u00b7er\u00b7h\u00f6r\u00b7te", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Unerlaubte zu beginnen", "tokens": ["Das", "Un\u00b7er\u00b7laub\u00b7te", "zu", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich k\u00fchnen \u00dcbermuts verma\u00df, \u2013", "tokens": ["Sich", "k\u00fch\u00b7nen", "\u00dc\u00b7ber\u00b7muts", "ver\u00b7ma\u00df", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den st\u00fcrzen sie, die Allgerechten,", "tokens": ["Den", "st\u00fcr\u00b7zen", "sie", ",", "die", "All\u00b7ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ew'ge Nacht und Finsternis:", "tokens": ["In", "ew'\u00b7ge", "Nacht", "und", "Fins\u00b7ter\u00b7nis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Streng ob den Guten und den Schlechten", "tokens": ["Streng", "ob", "den", "Gu\u00b7ten", "und", "den", "Schlech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herrscht, m\u00e4chtig \u00fcber allen M\u00e4chten,", "tokens": ["Herrscht", ",", "m\u00e4ch\u00b7tig", "\u00fc\u00b7ber", "al\u00b7len", "M\u00e4ch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die h\u00f6chste G\u00f6ttin: \u2013 Nemesis.", "tokens": ["Die", "h\u00f6chs\u00b7te", "G\u00f6t\u00b7tin", ":", "\u2013", "Ne\u00b7me\u00b7sis", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}