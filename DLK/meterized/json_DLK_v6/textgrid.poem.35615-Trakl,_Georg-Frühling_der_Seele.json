{"textgrid.poem.35615": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufschrei im Schlaf; durch schwarze Gassen st\u00fcrzt der Wind,", "tokens": ["Auf\u00b7schrei", "im", "Schlaf", ";", "durch", "schwar\u00b7ze", "Gas\u00b7sen", "st\u00fcrzt", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Blau des Fr\u00fchlings winkt durch brechendes Ge\u00e4st,", "tokens": ["Das", "Blau", "des", "Fr\u00fch\u00b7lings", "winkt", "durch", "bre\u00b7chen\u00b7des", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Purpurner Nachttau und es erl\u00f6schen rings die Sterne.", "tokens": ["Pur\u00b7pur\u00b7ner", "Nacht\u00b7tau", "und", "es", "er\u00b7l\u00f6\u00b7schen", "rings", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--++-+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Gr\u00fcnlich d\u00e4mmert der Flu\u00df, silbern die alten Alleen", "tokens": ["Gr\u00fcn\u00b7lich", "d\u00e4m\u00b7mert", "der", "Flu\u00df", ",", "sil\u00b7bern", "die", "al\u00b7ten", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und die T\u00fcrme der Stadt. O sanfte Trunkenheit", "tokens": ["Und", "die", "T\u00fcr\u00b7me", "der", "Stadt", ".", "O", "sanf\u00b7te", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$.", "NE", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Im gleitenden Kahn und die dunklen Rufe der Amsel", "tokens": ["Im", "glei\u00b7ten\u00b7den", "Kahn", "und", "die", "dunk\u00b7len", "Ru\u00b7fe", "der", "Am\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "In kindlichen G\u00e4rten. Schon lichtet sich der rosige Flor.", "tokens": ["In", "kind\u00b7li\u00b7chen", "G\u00e4r\u00b7ten", ".", "Schon", "lich\u00b7tet", "sich", "der", "ro\u00b7si\u00b7ge", "Flor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+----+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Feierlich rauschen die Wasser. O die feuchten Schatten der Au,", "tokens": ["Fei\u00b7er\u00b7lich", "rau\u00b7schen", "die", "Was\u00b7ser", ".", "O", "die", "feuch\u00b7ten", "Schat\u00b7ten", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "NE", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das schreitende Tier; Gr\u00fcnendes, Bl\u00fctengezweig", "tokens": ["Das", "schrei\u00b7ten\u00b7de", "Tier", ";", "Gr\u00fc\u00b7nen\u00b7des", ",", "Bl\u00fc\u00b7ten\u00b7ge\u00b7zweig"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "NN"], "meter": "-+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "R\u00fchrt die kristallene Stirne; schimmernder Schaukelkahn.", "tokens": ["R\u00fchrt", "die", "kris\u00b7tal\u00b7le\u00b7ne", "Stir\u00b7ne", ";", "schim\u00b7mern\u00b7der", "Schau\u00b7kel\u00b7kahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Leise t\u00f6nt die Sonne im Rosengew\u00f6lk am H\u00fcgel.", "tokens": ["Lei\u00b7se", "t\u00f6nt", "die", "Son\u00b7ne", "im", "Ro\u00b7sen\u00b7ge\u00b7w\u00f6lk", "am", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Gro\u00df ist die Stille des Tannenwalds, die ernsten Schatten am Flu\u00df.", "tokens": ["Gro\u00df", "ist", "die", "Stil\u00b7le", "des", "Tan\u00b7nen\u00b7walds", ",", "die", "erns\u00b7ten", "Schat\u00b7ten", "am", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Reinheit! Reinheit! Wo sind die furchtbaren Pfade des Todes,", "tokens": ["Rein\u00b7heit", "!", "Rein\u00b7heit", "!", "Wo", "sind", "die", "furcht\u00b7ba\u00b7ren", "Pfa\u00b7de", "des", "To\u00b7des", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PWAV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Des grauen steinernen Schweigens, die Felsen der Nacht", "tokens": ["Des", "grau\u00b7en", "stei\u00b7ner\u00b7nen", "Schwei\u00b7gens", ",", "die", "Fel\u00b7sen", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und die friedlosen Schatten? Strahlender Sonnenabgrund.", "tokens": ["Und", "die", "fried\u00b7lo\u00b7sen", "Schat\u00b7ten", "?", "Strah\u00b7len\u00b7der", "Son\u00b7nen\u00b7ab\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "NN", "NN", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Schwester, da ich dich fand an einsamer Lichtung", "tokens": ["Schwes\u00b7ter", ",", "da", "ich", "dich", "fand", "an", "ein\u00b7sa\u00b7mer", "Lich\u00b7tung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Des Waldes und Mittag war und gro\u00df das Schweigen des Tiers;", "tokens": ["Des", "Wal\u00b7des", "und", "Mit\u00b7tag", "war", "und", "gro\u00df", "das", "Schwei\u00b7gen", "des", "Tiers", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "KON", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wei\u00dfe unter wilder Eiche, und es bl\u00fchte silbern der Dorn.", "tokens": ["Wei\u00b7\u00dfe", "un\u00b7ter", "wil\u00b7der", "Ei\u00b7che", ",", "und", "es", "bl\u00fch\u00b7te", "sil\u00b7bern", "der", "Dorn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.4": {"text": "Gewaltiges Sterben und die singende Flamme im Herzen.", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ges", "Ster\u00b7ben", "und", "die", "sin\u00b7gen\u00b7de", "Flam\u00b7me", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Dunkler umflie\u00dfen die Wasser die sch\u00f6nen Spiele der Fische.", "tokens": ["Dunk\u00b7ler", "um\u00b7flie\u00b7\u00dfen", "die", "Was\u00b7ser", "die", "sch\u00f6\u00b7nen", "Spie\u00b7le", "der", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Stunde der Trauer, Schweigender Anblick der Sonne;", "tokens": ["Stun\u00b7de", "der", "Trau\u00b7er", ",", "Schwei\u00b7gen\u00b7der", "An\u00b7blick", "der", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Es ist die Seele ein Fremdes auf Erden. Geistlich d\u00e4mmert", "tokens": ["Es", "ist", "die", "See\u00b7le", "ein", "Frem\u00b7des", "auf", "Er\u00b7den", ".", "Geist\u00b7lich", "d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bl\u00e4ue \u00fcber dem verhauenen Wald und es l\u00e4utet", "tokens": ["Bl\u00e4u\u00b7e", "\u00fc\u00b7ber", "dem", "ver\u00b7hau\u00b7e\u00b7nen", "Wald", "und", "es", "l\u00e4u\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "PPER", "VVFIN"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Lange eine dunkle Glocke im Dorf; friedlich Geleit.", "tokens": ["Lan\u00b7ge", "ei\u00b7ne", "dunk\u00b7le", "Glo\u00b7cke", "im", "Dorf", ";", "fried\u00b7lich", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "ADJD", "NN", "$."], "meter": "+-+-+-+--++--+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Stille bl\u00fcht die Myrthe \u00fcber den wei\u00dfen Lidern des Toten.", "tokens": ["Stil\u00b7le", "bl\u00fcht", "die", "Myr\u00b7the", "\u00fc\u00b7ber", "den", "wei\u00b7\u00dfen", "Li\u00b7dern", "des", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Leise t\u00f6nen die Wasser im sinkenden Nachmittag", "tokens": ["Lei\u00b7se", "t\u00f6\u00b7nen", "die", "Was\u00b7ser", "im", "sin\u00b7ken\u00b7den", "Nach\u00b7mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und es gr\u00fcnet dunkler die Wildnis am Ufer, Freude im rosigen Wind;", "tokens": ["Und", "es", "gr\u00fc\u00b7net", "dunk\u00b7ler", "die", "Wild\u00b7nis", "am", "U\u00b7fer", ",", "Freu\u00b7de", "im", "ro\u00b7si\u00b7gen", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "ART", "NN", "APPRART", "NN", "$,", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+--+--+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der sanfte Gesang des Bruders am Abendh\u00fcgel.", "tokens": ["Der", "sanf\u00b7te", "Ge\u00b7sang", "des", "Bru\u00b7ders", "am", "A\u00b7bend\u00b7h\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}