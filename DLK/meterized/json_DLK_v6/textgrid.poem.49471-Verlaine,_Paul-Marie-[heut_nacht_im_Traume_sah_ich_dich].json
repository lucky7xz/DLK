{"textgrid.poem.49471": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[heut nacht im Traume sah ich dich]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut nacht im Traume sah ich dich,", "tokens": ["Heut", "nacht", "im", "Trau\u00b7me", "sah", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du dehntest dich mit leisem Lachen", "tokens": ["Du", "dehn\u00b7test", "dich", "mit", "lei\u00b7sem", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schw\u00e4tztest gurrend tausend Sachen.", "tokens": ["Und", "schw\u00e4tz\u00b7test", "gur\u00b7rend", "tau\u00b7send", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kostete gleich Fr\u00fcchten dich,", "tokens": ["Ich", "kos\u00b7te\u00b7te", "gleich", "Fr\u00fcch\u00b7ten", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich mit durst'ger Lippe k\u00fcsste", "tokens": ["Wie", "ich", "mit", "dur\u00b7st'\u00b7ger", "Lip\u00b7pe", "k\u00fcss\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Berg, Tal und H\u00fcgel, Wang' und Br\u00fcste.", "tokens": ["Berg", ",", "Tal", "und", "H\u00fc\u00b7gel", ",", "Wang'", "und", "Br\u00fcs\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich war von einer Biegsamkeit,", "tokens": ["Ich", "war", "von", "ei\u00b7ner", "Biegs\u00b7am\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wirklich man bewundern musste,", "tokens": ["Die", "wirk\u00b7lich", "man", "be\u00b7wun\u00b7dern", "muss\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Herrgott, welche Kraft, welche Puste!", "tokens": ["Herr\u00b7gott", ",", "wel\u00b7che", "Kraft", ",", "wel\u00b7che", "Pus\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und du, Geliebte, zur selben Zeit,", "tokens": ["Und", "du", ",", "Ge\u00b7lieb\u00b7te", ",", "zur", "sel\u00b7ben", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welche Puste und Kraft, welche Schnelle", "tokens": ["Wel\u00b7che", "Pus\u00b7te", "und", "Kraft", ",", "wel\u00b7che", "Schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "KON", "NN", "$,", "PWAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und Biegsamkeit der Gazelle.", "tokens": ["Und", "Biegs\u00b7am\u00b7keit", "der", "Ga\u00b7zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Am Morgen gab's in deinem Arm,", "tokens": ["Am", "Mor\u00b7gen", "gab's", "in", "dei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur viel vollkomm'ner, im Erwachen", "tokens": ["Nur", "viel", "voll\u00b7kom\u00b7m'\u00b7ner", ",", "im", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "$,", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Genau dieselben s\u00fcssen Sachen.", "tokens": ["Ge\u00b7nau", "die\u00b7sel\u00b7ben", "s\u00fcs\u00b7sen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}