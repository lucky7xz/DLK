{"dta.poem.9728": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie nimmt ihm die hertzen zehen mit  \n der dame weg.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur frisch! das spiel ist mein/ so vieler hertzen wegen/", "tokens": ["Nur", "frisch", "!", "das", "spiel", "ist", "mein", "/", "so", "vie\u00b7ler", "hert\u00b7zen", "we\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PDS", "VVFIN", "VAFIN", "PPOSAT", "$(", "ADV", "PIAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagt mir: welch hertz ist wol zehn hertzen \u00fcberlegen?", "tokens": ["Sagt", "mir", ":", "welch", "hertz", "ist", "wol", "zehn", "hert\u00b7zen", "\u00fc\u00b7berl\u00b7e\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWAT", "NN", "VAFIN", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie aber? sticht man mich? halt! doch ich mu\u00df\u2019s verschmertzen/", "tokens": ["Wie", "a\u00b7ber", "?", "sticht", "man", "mich", "?", "halt", "!", "doch", "ich", "mu\u00df's", "ver\u00b7schmert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VVFIN", "PIS", "PRF", "$.", "VVFIN", "$.", "KON", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein damen hertz gilt mehr/ als zehen andre hertzen.", "tokens": ["Ein", "da\u00b7men",