{"dta.poem.14698": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vesper .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt', ich l\u00e4g' begraben,", "tokens": ["Ich", "wollt'", ",", "ich", "l\u00e4g'", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und \u00fcber mir rauscht' weit", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "rauscht'", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Linde jeden Abend", "tokens": ["Die", "Lin\u00b7de", "je\u00b7den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von der alten, sch\u00f6nen Zeit!", "tokens": ["Von", "der", "al\u00b7ten", ",", "sch\u00f6\u00b7nen", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}