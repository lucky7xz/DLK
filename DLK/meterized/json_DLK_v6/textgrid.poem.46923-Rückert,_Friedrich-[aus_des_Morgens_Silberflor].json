{"textgrid.poem.46923": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[aus des Morgens Silberflor]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Morgens Silberflor", "tokens": ["Aus", "des", "Mor\u00b7gens", "Sil\u00b7ber\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt die goldne Sonn' hervor", "tokens": ["Tritt", "die", "gold\u00b7ne", "Sonn'", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelnd wie sie niederschaut,", "tokens": ["L\u00e4\u00b7chelnd", "wie", "sie", "nie\u00b7der\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaut die Welt zu ihr empor.", "tokens": ["Schaut", "die", "Welt", "zu", "ihr", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Blumen bl\u00fchen auf,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "bl\u00fc\u00b7hen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aufw\u00e4rts singt der V\u00f6gel Chor.", "tokens": ["Auf\u00b7w\u00e4rts", "singt", "der", "V\u00f6\u00b7gel", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Gebet der Sch\u00f6pfung steigt", "tokens": ["Das", "Ge\u00b7bet", "der", "Sch\u00f6p\u00b7fung", "steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu des Freudensch\u00f6pfers Ohr.", "tokens": ["Zu", "des", "Freu\u00b7den\u00b7sch\u00f6p\u00b7fers", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Alles Leben hoffet neu", "tokens": ["Al\u00b7les", "Le\u00b7ben", "hof\u00b7fet", "neu"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und vergi\u00dft, was es verlor;", "tokens": ["Und", "ver\u00b7gi\u00dft", ",", "was", "es", "ver\u00b7lor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und der Blick des Tages w\u00e4rmt,", "tokens": ["Und", "der", "Blick", "des", "Ta\u00b7ges", "w\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Was im Frost der N\u00e4chte fror.", "tokens": ["Was", "im", "Frost", "der", "N\u00e4ch\u00b7te", "fror", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Aufgeschlossen ist, o Herz,", "tokens": ["Auf\u00b7ge\u00b7schlos\u00b7sen", "ist", ",", "o", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der Erh\u00f6rung goldnes Thor.", "tokens": ["Der", "Er\u00b7h\u00f6\u00b7rung", "gold\u00b7nes", "Thor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Schleu\u00df dich auf, und trag auch du", "tokens": ["Schleu\u00df", "dich", "auf", ",", "und", "trag", "auch", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Neue Lebensw\u00fcnsche vor!", "tokens": ["Neu\u00b7e", "Le\u00b7bens\u00b7w\u00fcn\u00b7sche", "vor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}