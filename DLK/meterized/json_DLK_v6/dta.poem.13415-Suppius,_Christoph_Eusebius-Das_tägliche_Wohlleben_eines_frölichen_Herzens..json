{"dta.poem.13415": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Das t\u00e4gliche Wohlleben  \n eines fr\u00f6lichen Herzens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Beutel ist sehr schlecht bestellt,", "tokens": ["Mein", "Beu\u00b7tel", "ist", "sehr", "schlecht", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein leerer Magen brummt und bellt,", "tokens": ["Sein", "lee\u00b7rer", "Ma\u00b7gen", "brummt", "und", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tisch ist ledig von Gerichten;", "tokens": ["Der", "Tisch", "ist", "le\u00b7dig", "von", "Ge\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Glas und der October-Saft", "tokens": ["Das", "Glas", "und", "der", "Oc\u00b7to\u00b7ber\u00b7Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stehn in gar schlechter Nachbarschaft,", "tokens": ["Stehn", "in", "gar", "schlech\u00b7ter", "Nach\u00b7bar\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und beeder Zwiespalt ist nicht eben leicht zu schlichten.", "tokens": ["Und", "bee\u00b7der", "Zwies\u00b7palt", "ist", "nicht", "e\u00b7ben", "leicht", "zu", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch f\u00e4llt mich Durst und Hunger an,", "tokens": ["Doch", "f\u00e4llt", "mich", "Durst", "und", "Hun\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwey Feinde stehn f\u00fcr einen Mann,", "tokens": ["Zwey", "Fein\u00b7de", "stehn", "f\u00fcr", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinen K\u00f6rper einzudringen;", "tokens": ["In", "mei\u00b7nen", "K\u00f6r\u00b7per", "ein\u00b7zu\u00b7drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie folgen billig dem Geschrey,", "tokens": ["Sie", "fol\u00b7gen", "bil\u00b7lig", "dem", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als ob da offne Tafel sey,", "tokens": ["Als", "ob", "da", "off\u00b7ne", "Ta\u00b7fel", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo man um Mittagszeit h\u00f6rt die Trompeten klingen.", "tokens": ["Wo", "man", "um", "Mit\u00b7tags\u00b7zeit", "h\u00f6rt", "die", "Trom\u00b7pe\u00b7ten", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wiewohl die\u00df trifft nur immer ein,", "tokens": ["Wie\u00b7wohl", "die\u00df", "trifft", "nur", "im\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo f\u00fcrstliche Pall\u00e4ste seyn,", "tokens": ["Wo", "f\u00fcrst\u00b7li\u00b7che", "Pal\u00b7l\u00e4s\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht aber bey dergleichen Leuten,", "tokens": ["Nicht", "a\u00b7ber", "bey", "derg\u00b7lei\u00b7chen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die man nur mit Verachtung speist,", "tokens": ["Die", "man", "nur", "mit", "Ver\u00b7ach\u00b7tung", "speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die der Hochmuth P\u00f6bel hei\u00dft,", "tokens": ["Und", "die", "der", "Hoch\u00b7muth", "P\u00f6\u00b7bel", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die bey der Menschlichkeit die Schlacken nur bedeuten.", "tokens": ["Die", "bey", "der", "Menschlich\u00b7keit", "die", "Schla\u00b7cken", "nur", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Drum will ich auf mein Bestes sehn,", "tokens": ["Drum", "will", "ich", "auf", "mein", "Bes\u00b7tes", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hin zu meines gleichen gehn,", "tokens": ["Und", "hin", "zu", "mei\u00b7nes", "glei\u00b7chen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo keine solche L\u00fcfte streichen,", "tokens": ["Wo", "kei\u00b7ne", "sol\u00b7che", "L\u00fcf\u00b7te", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sich kein Durst noch Hunger findt,", "tokens": ["Wo", "sich", "kein", "Durst", "noch", "Hun\u00b7ger", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dieweil da keine Menschen sind,", "tokens": ["Die\u00b7weil", "da", "kei\u00b7ne", "Men\u00b7schen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da wohnt nur ich, du, er, wir, ihr, sie, und desgleichen.", "tokens": ["Da", "wohnt", "nur", "ich", ",", "du", ",", "er", ",", "wir", ",", "ihr", ",", "sie", ",", "und", "des\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PPER", "$,", "PPER", "$,", "PPER", "$,", "PPER", "$,", "PPER", "$,", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Allein da schallt kein hohler Ton,", "tokens": ["Al\u00b7lein", "da", "schallt", "kein", "hoh\u00b7ler", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sich der Vater und der Sohn", "tokens": ["Wo", "sich", "der", "Va\u00b7ter", "und", "der", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey Salz und Brod die Zeit verk\u00fcrzen;", "tokens": ["Bey", "Salz", "und", "Brod", "die", "Zeit", "ver\u00b7k\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man das Jahr lang Fasten h\u00e4lt,", "tokens": ["Wo", "man", "das", "Jahr", "lang", "Fas\u00b7ten", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Mager zwar zum Koch bestellt,", "tokens": ["Ist", "Ma\u00b7ger", "zwar", "zum", "Koch", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jedoch der Hunger kan auch Kohl u. Bohnen w\u00fcrzen.", "tokens": ["Je\u00b7doch", "der", "Hun\u00b7ger", "kan", "auch", "Kohl", "u.", "Boh\u00b7nen", "w\u00fcr\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ADV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Auch macht die Einigkeit allein", "tokens": ["Auch", "macht", "die", "Ei\u00b7nig\u00b7keit", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Wasser da den guten Wein,", "tokens": ["Aus", "Was\u00b7ser", "da", "den", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Krug und Gerstensaft sind Br\u00fcder,", "tokens": ["Der", "Krug", "und", "Gers\u00b7ten\u00b7saft", "sind", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Stroh- und gr\u00fcn bemoostes Dach", "tokens": ["Ein", "Stroh", "und", "gr\u00fcn", "be\u00b7moos\u00b7tes", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verstattet kein gepresstes Ach!", "tokens": ["Ver\u00b7stat\u00b7tet", "kein", "ge\u00b7press\u00b7tes", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Raum erschallet nur von frohe\u0303 Jauchzern wieder.", "tokens": ["Sein", "Raum", "er\u00b7schal\u00b7let", "nur", "von", "froh\u1ebd", "Jauch\u00b7zern", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Gesundheit geht da aus und ein,", "tokens": ["Ge\u00b7sund\u00b7heit", "geht", "da", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt alt und jung in einen Reihn,", "tokens": ["Stellt", "alt", "und", "jung", "in", "ei\u00b7nen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00dft Junge lieben, Alte trinken,", "tokens": ["Hei\u00dft", "Jun\u00b7ge", "lie\u00b7ben", ",", "Al\u00b7te", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie thun es bis zur Lustigkeit,", "tokens": ["Sie", "thun", "es", "bis", "zur", "Lus\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr Podagra und sein Geleit", "tokens": ["Herr", "Po\u00b7da\u00b7gra", "und", "sein", "Ge\u00b7leit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf aus den St\u00e4dten nicht in diese Zechen hinken.", "tokens": ["Darf", "aus", "den", "St\u00e4d\u00b7ten", "nicht", "in", "die\u00b7se", "Ze\u00b7chen", "hin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Empfindet man nun keine Noth,", "tokens": ["Emp\u00b7fin\u00b7det", "man", "nun", "kei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Kesselbier, Salz, Fleisch und Brodt,", "tokens": ["Wo", "Kes\u00b7sel\u00b7bier", ",", "Salz", ",", "Fleisch", "und", "Brodt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein Gerichte Kohl mit Liebe,", "tokens": ["Und", "ein", "Ge\u00b7rich\u00b7te", "Kohl", "mit", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So zieh ich ohne viel Ger\u00e4usch", "tokens": ["So", "zieh", "ich", "oh\u00b7ne", "viel", "Ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Mastvieh und von Ochsenfleisch,", "tokens": ["Vom", "Mast\u00b7vieh", "und", "von", "Och\u00b7sen\u00b7fleisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das in der Kehlen oft aus Abgunst stecken blie-", "tokens": ["Das", "in", "der", "Keh\u00b7len", "oft", "aus", "Ab\u00b7gunst", "ste\u00b7cken", "blie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Weil ich zufriednes Herzens bin,", "tokens": ["Weil", "ich", "zu\u00b7fried\u00b7nes", "Her\u00b7zens", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich zu meines gleichen hin,", "tokens": ["Will", "ich", "zu", "mei\u00b7nes", "glei\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht nach hohen Dingen trachten;", "tokens": ["Und", "nicht", "nach", "ho\u00b7hen", "Din\u00b7gen", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da bin ich immer angenehm,", "tokens": ["Da", "bin", "ich", "im\u00b7mer", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da f\u00e4llt mir niemahls unbeqvem,", "tokens": ["Da", "f\u00e4llt", "mir", "nie\u00b7mahls", "un\u00b7be\u00b7qvem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn oft der rothe Thon den schwarzen will ver-", "tokens": ["Wenn", "oft", "der", "ro\u00b7the", "Thon", "den", "schwar\u00b7zen", "will", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "VVINF", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Tr\u00e4gt gleich das Feld nicht Knaster da,", "tokens": ["Tr\u00e4gt", "gleich", "das", "Feld", "nicht", "Knas\u00b7ter", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Bohnen aus Arabia,", "tokens": ["Und", "Boh\u00b7nen", "aus", "A\u00b7ra\u00b7bia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So kann gebrannte Gerste k\u00fchlen,", "tokens": ["So", "kann", "ge\u00b7brann\u00b7te", "Gers\u00b7te", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unsern Fluhren, die man baut,", "tokens": ["In", "un\u00b7sern", "Fluh\u00b7ren", ",", "die", "man", "baut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4chst manch gesundes Tobackskraut,", "tokens": ["W\u00e4chst", "manch", "ge\u00b7sun\u00b7des", "To\u00b7backs\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch schwitzt kein Sklave da in denen Zucker-", "tokens": ["Doch", "schwitzt", "kein", "Skla\u00b7ve", "da", "in", "de\u00b7nen", "Zu\u00b7cke\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADV", "APPR", "PRELS", "TRUNC"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Hingegen siehet man ein Bild,", "tokens": ["Hin\u00b7ge\u00b7gen", "sie\u00b7het", "man", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wenn man die leere Pfeise f\u00fcllt,", "tokens": ["Wenn", "man", "die", "lee\u00b7re", "Pfei\u00b7se", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einsam mit Vergn\u00fcgen schmauchet,", "tokens": ["Und", "ein\u00b7sam", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "schmau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie unser Leben bald verstreicht,", "tokens": ["Wie", "un\u00b7ser", "Le\u00b7ben", "bald", "ver\u00b7streicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie schnell die Jugend von uns weicht,", "tokens": ["Wie", "schnell", "die", "Ju\u00b7gend", "von", "uns", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie bald, wie gar zu bald das Lebens-Oel ver-", "tokens": ["Wie", "bald", ",", "wie", "gar", "zu", "bald", "das", "Le\u00b7bens\u00b7O\u00b7el", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADV", "APPR", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Auch kann man da durchs Gitter sehn,", "tokens": ["Auch", "kann", "man", "da", "durchs", "Git\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich die armen W\u00fcrmer bl\u00e4hn,", "tokens": ["Wie", "sich", "die", "ar\u00b7men", "W\u00fcr\u00b7mer", "bl\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die aus dem Erdenklos gestiegen,", "tokens": ["Die", "aus", "dem", "Er\u00b7denk\u00b7los", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deren Herzen, wie der Wind,", "tokens": ["In", "de\u00b7ren", "Her\u00b7zen", ",", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Leidenschaften rege sind", "tokens": ["Die", "Lei\u00b7den\u00b7schaf\u00b7ten", "re\u00b7ge", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum K\u00e4mpfen, bis sie gar die Tugenden besie-", "tokens": ["Zum", "K\u00e4mp\u00b7fen", ",", "bis", "sie", "gar", "die", "Tu\u00b7gen\u00b7den", "be\u00b7sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Sie sind in stetem Ueberflu\u00df,", "tokens": ["Sie", "sind", "in", "ste\u00b7tem", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob man darum leben mu\u00df,", "tokens": ["Als", "ob", "man", "da\u00b7rum", "le\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selten mit sich selbst zufrieden;", "tokens": ["Und", "sel\u00b7ten", "mit", "sich", "selbst", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie machen sich das Leben schwer.", "tokens": ["Sie", "ma\u00b7chen", "sich", "das", "Le\u00b7ben", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O liebes Salzfa\u00df! komm du her,", "tokens": ["O", "lie\u00b7bes", "Salz\u00b7fa\u00df", "!", "komm", "du", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey dir ist mir allein ein sch\u00f6ner Theil beschie-", "tokens": ["Bey", "dir", "ist", "mir", "al\u00b7lein", "ein", "sch\u00f6\u00b7ner", "Theil", "be\u00b7schie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Wie ist mir doch so herzlich wohl,", "tokens": ["Wie", "ist", "mir", "doch", "so", "herz\u00b7lich", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich bey selbstgepflanzten Kohl", "tokens": ["Da\u00df", "ich", "bey", "selbst\u00b7ge\u00b7pflanz\u00b7ten", "Kohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mir selbst soll \u00fcberlassen werden!", "tokens": ["Mir", "selbst", "soll", "\u00fc\u00b7ber\u00b7las\u00b7sen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fort! packe dich, verkehrte Welt!", "tokens": ["Fort", "!", "pa\u00b7cke", "dich", ",", "ver\u00b7kehr\u00b7te", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zufrieden ist mein Herz gestellt,", "tokens": ["Zu\u00b7frie\u00b7den", "ist", "mein", "Herz", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich macht Gen\u00fcgsamkeit zum B\u00fcrger dieser", "tokens": ["Mich", "macht", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "zum", "B\u00fcr\u00b7ger", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Mein fr\u00f6lich Herz, erhole dich!", "tokens": ["Mein", "fr\u00f6\u00b7lich", "Herz", ",", "er\u00b7ho\u00b7le", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da Kummer, Angst und Sorgen sich", "tokens": ["Da", "Kum\u00b7mer", ",", "Angst", "und", "Sor\u00b7gen", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht dorten mit dir hin begeben,", "tokens": ["Nicht", "dor\u00b7ten", "mit", "dir", "hin", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo die gela\u00dfne Ruhe winkt,", "tokens": ["Wo", "die", "ge\u00b7la\u00df\u00b7ne", "Ru\u00b7he", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo deines Vaters Salzfa\u00df blinkt,", "tokens": ["Wo", "dei\u00b7nes", "Va\u00b7ters", "Salz\u00b7fa\u00df", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ist die Einsamkeit das rechte Freudenleben.", "tokens": ["Da", "ist", "die", "Ein\u00b7sam\u00b7keit", "das", "rech\u00b7te", "Freu\u00b7den\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}