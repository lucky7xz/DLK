{"dta.poem.21512": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Pygm\u00e4en .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist todt, da gro\u00dfe Helden schufen,               ", "tokens": ["Die", "Zeit", "ist", "todt", ",", "da", "gro\u00b7\u00dfe", "Hel\u00b7den", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit der Fackel der Begeisterung,", "tokens": ["Die", "mit", "der", "Fa\u00b7ckel", "der", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit k\u00fchn erhabenem Gedankenschwung", "tokens": ["Mit", "k\u00fchn", "er\u00b7ha\u00b7be\u00b7nem", "Ge\u00b7dan\u00b7ken\u00b7schwung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens florumh\u00fcllte Stufen", "tokens": ["Des", "Le\u00b7bens", "flo\u00b7rum\u00b7h\u00fcll\u00b7te", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und weiter \u2014 weiter bis zum Gipfel klommen,", "tokens": ["Und", "wei\u00b7ter", "wei\u00b7ter", "bis", "zum", "Gip\u00b7fel", "klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo ihnen vor den sehgewalt\u2019gen Blicken", "tokens": ["Wo", "ih\u00b7nen", "vor", "den", "seh\u00b7ge\u00b7walt'\u00b7gen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jach barst der Vorhang mitten in zwei St\u00fccken \u2014", "tokens": ["Jach", "barst", "der", "Vor\u00b7hang", "mit\u00b7ten", "in", "zwei", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADV", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo \u00fcber sie der Friede dann gekommen!", "tokens": ["Wo", "\u00fc\u00b7ber", "sie", "der", "Frie\u00b7de", "dann", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Zeit ist todt \u2014 die Zeit der gro\u00dfen Seelen \u2014", "tokens": ["Die", "Zeit", "ist", "todt", "die", "Zeit", "der", "gro\u00b7\u00dfen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir sind ein \u00e4rmlich Volk nur von Pygm\u00e4en, \u2026", "tokens": ["Wir", "sind", "ein", "\u00e4rm\u00b7lich", "Volk", "nur", "von", "Pyg\u00b7m\u00e4\u00b7en", ",", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "ADV", "APPR", "NE", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich mit ihrer Afterweisheit frevelnd bl\u00e4hen", "tokens": ["Die", "sich", "mit", "ih\u00b7rer", "Af\u00b7ter\u00b7weis\u00b7heit", "fre\u00b7velnd", "bl\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dreist sich mit der L\u00fcge Schmutz verm\u00e4hlen \u2014", "tokens": ["Und", "dreist", "sich", "mit", "der", "L\u00fc\u00b7ge", "Schmutz", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit jener L\u00fcge, die da Prunk und Kronen", "tokens": ["Mit", "je\u00b7ner", "L\u00fc\u00b7ge", ",", "die", "da", "Prunk", "und", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um leere Sch\u00e4del flicht \u2014 um schmale Stirnen", "tokens": ["Um", "lee\u00b7re", "Sch\u00e4\u00b7del", "flicht", "um", "schma\u00b7le", "Stir\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das Diadem der Gottentstammtheit schlingt \u2014", "tokens": ["Das", "Di\u00b7a\u00b7dem", "der", "Got\u00b7tent\u00b7stamm\u00b7theit", "schlingt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Weihrauchduft ohnm\u00e4cht\u2019gen G\u00f6tzen bringt!", "tokens": ["Die", "Weih\u00b7rauch\u00b7duft", "ohn\u00b7m\u00e4cht'\u00b7gen", "G\u00f6t\u00b7zen", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was wir vollbringen, thun wir nach Schablonen,", "tokens": ["Was", "wir", "voll\u00b7brin\u00b7gen", ",", "thun", "wir", "nach", "Scha\u00b7blo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und uns\u2019re Herzen schrei\u2019n nach Gold und Dirnen \u2014", "tokens": ["Und", "un\u00b7s'\u00b7re", "Her\u00b7zen", "schrei'n", "nach", "Gold", "und", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Keinen giebt\u2019s, der tief im Herzen tr\u00fcge", "tokens": ["Und", "Kei\u00b7nen", "giebt's", ",", "der", "tief", "im", "Her\u00b7zen", "tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wir knieen ", "tokens": ["Wir", "kni\u00b7e\u00b7en"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Und singen unserer Freiheit Sterbelieder!             ", "tokens": ["Und", "sin\u00b7gen", "un\u00b7se\u00b7rer", "Frei\u00b7heit", "Ster\u00b7be\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}