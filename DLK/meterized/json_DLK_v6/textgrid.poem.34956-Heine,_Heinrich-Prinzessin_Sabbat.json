{"textgrid.poem.34956": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Prinzessin Sabbat", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Arabiens M\u00e4rchenbuche", "tokens": ["In", "A\u00b7ra\u00b7biens", "M\u00e4r\u00b7chen\u00b7bu\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sehen wir verw\u00fcnschte Prinzen,", "tokens": ["Se\u00b7hen", "wir", "ver\u00b7w\u00fcnschte", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die zuzeiten ihre sch\u00f6ne", "tokens": ["Die", "zu\u00b7zei\u00b7ten", "ih\u00b7re", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Urgestalt zur\u00fcckgewinnen:", "tokens": ["Ur\u00b7ge\u00b7stalt", "zu\u00b7r\u00fcck\u00b7ge\u00b7win\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das behaarte Ungeheuer", "tokens": ["Das", "be\u00b7haar\u00b7te", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein K\u00f6nigsohn geworden;", "tokens": ["Ist", "ein", "K\u00f6\u00b7nig\u00b7sohn", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmuckreich gl\u00e4nzend angekleidet,", "tokens": ["Schmuck\u00b7reich", "gl\u00e4n\u00b7zend", "an\u00b7ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch verliebt die Fl\u00f6te blasend.", "tokens": ["Auch", "ver\u00b7liebt", "die", "Fl\u00f6\u00b7te", "bla\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch die Zauberfrist zerrinnt,", "tokens": ["Doch", "die", "Zau\u00b7ber\u00b7frist", "zer\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wir schauen pl\u00f6tzlich wieder", "tokens": ["Und", "wir", "schau\u00b7en", "pl\u00f6tz\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine k\u00f6nigliche Hoheit", "tokens": ["Sei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Ho\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ein Unget\u00fcm verzottelt.", "tokens": ["In", "ein", "Un\u00b7ge\u00b7t\u00fcm", "ver\u00b7zot\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einen Prinzen solchen Schicksals", "tokens": ["Ei\u00b7nen", "Prin\u00b7zen", "sol\u00b7chen", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt mein Lied. Er ist gehei\u00dfen", "tokens": ["Singt", "mein", "Lied", ".", "Er", "ist", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Israel. Ihn hat verwandelt", "tokens": ["Is\u00b7rael", ".", "Ihn", "hat", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hexenspruch in einen Hund.", "tokens": ["He\u00b7xen\u00b7spruch", "in", "ei\u00b7nen", "Hund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hund mit h\u00fcndischen Gedanken,", "tokens": ["Hund", "mit", "h\u00fcn\u00b7di\u00b7schen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6tert er die ganze Woche", "tokens": ["K\u00f6\u00b7tert", "er", "die", "gan\u00b7ze", "Wo\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des Lebens Kot und Kehricht,", "tokens": ["Durch", "des", "Le\u00b7bens", "Kot", "und", "Keh\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gassenbuben zum Gesp\u00f6tte.", "tokens": ["Gas\u00b7sen\u00b7bu\u00b7ben", "zum", "Ge\u00b7sp\u00f6t\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber jeden Freitagabend,", "tokens": ["A\u00b7ber", "je\u00b7den", "Frei\u00b7tag\u00b7a\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der D\u00e4mmrungstunde, pl\u00f6tzlich", "tokens": ["In", "der", "D\u00e4mm\u00b7rungs\u00b7tun\u00b7de", ",", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weicht der Zauber, und der Hund", "tokens": ["Weicht", "der", "Zau\u00b7ber", ",", "und", "der", "Hund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird aufs neu' ein menschlich Wesen.", "tokens": ["Wird", "aufs", "neu'", "ein", "menschlich", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "ART", "ADJD", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Mensch mit menschlichen Gef\u00fchlen,", "tokens": ["Mensch", "mit", "menschli\u00b7chen", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mit erhobnem Haupt und Herzen,", "tokens": ["Mit", "er\u00b7hob\u00b7nem", "Haupt", "und", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Festlich, reinlich schier gekleidet,", "tokens": ["Fest\u00b7lich", ",", "rein\u00b7lich", "schier", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt er in des Vaters Halle.", "tokens": ["Tritt", "er", "in", "des", "Va\u00b7ters", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbsei gegr\u00fc\u00dft, geliebte Halle", "tokens": ["\u00bb", "sei", "ge\u00b7gr\u00fc\u00dft", ",", "ge\u00b7lieb\u00b7te", "Hal\u00b7le"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "VVPP", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines k\u00f6niglichen Vaters!", "tokens": ["Mei\u00b7nes", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Va\u00b7ters", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zelte Jakobs, eure heil'gen", "tokens": ["Zel\u00b7te", "Ja\u00b7kobs", ",", "eu\u00b7re", "heil'\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "$,", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingangspfosten k\u00fc\u00dft mein Mund!\u00ab", "tokens": ["Ein\u00b7gangs\u00b7pfos\u00b7ten", "k\u00fc\u00dft", "mein", "Mund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Durch das Haus geheimnisvoll", "tokens": ["Durch", "das", "Haus", "ge\u00b7heim\u00b7nis\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht ein Wispern und ein Weben,", "tokens": ["Zieht", "ein", "Wis\u00b7pern", "und", "ein", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der unsichtbare Hausherr", "tokens": ["Und", "der", "un\u00b7sicht\u00b7ba\u00b7re", "Haus\u00b7herr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "---+--++", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Atmet schaurig in der Stille.", "tokens": ["At\u00b7met", "schau\u00b7rig", "in", "der", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Stille! Nur der Seneschall", "tokens": ["Stil\u00b7le", "!", "Nur", "der", "Se\u00b7ne\u00b7schall"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "(vulgo Synagogendiener)", "tokens": ["(", "vul\u00b7go", "Syn\u00b7a\u00b7go\u00b7gen\u00b7die\u00b7ner", ")"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Springt gesch\u00e4ftig auf und nieder,", "tokens": ["Springt", "ge\u00b7sch\u00e4f\u00b7tig", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Lampen anzuz\u00fcnden.", "tokens": ["Um", "die", "Lam\u00b7pen", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Trostverhei\u00dfend goldne Lichter,", "tokens": ["Trost\u00b7ver\u00b7hei\u00b7\u00dfend", "gold\u00b7ne", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie gl\u00e4nzen, wie sie glimmern!", "tokens": ["Wie", "sie", "gl\u00e4n\u00b7zen", ",", "wie", "sie", "glim\u00b7mern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolz aufflackern auch die Kerzen", "tokens": ["Stolz", "auf\u00b7fla\u00b7ckern", "auch", "die", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Br\u00fcstung des Almemors.", "tokens": ["Auf", "der", "Br\u00fcs\u00b7tung", "des", "Al\u00b7me\u00b7mors", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Vor dem Schreine, der die Thora", "tokens": ["Vor", "dem", "Schrei\u00b7ne", ",", "der", "die", "Tho\u00b7ra"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufbewahret und verh\u00e4ngt ist", "tokens": ["Auf\u00b7be\u00b7wah\u00b7ret", "und", "ver\u00b7h\u00e4ngt", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVPP", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der kostbar seidnen Decke,", "tokens": ["Mit", "der", "kost\u00b7bar", "seid\u00b7nen", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die von Edelsteinen funkelt \u2013", "tokens": ["Die", "von", "E\u00b7del\u00b7stei\u00b7nen", "fun\u00b7kelt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dort an seinem Betpultst\u00e4nder", "tokens": ["Dort", "an", "sei\u00b7nem", "Bet\u00b7pult\u00b7st\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht schon der Gemeindes\u00e4nger;", "tokens": ["Steht", "schon", "der", "Ge\u00b7mein\u00b7de\u00b7s\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schmuckes M\u00e4nnchen, das sein schwarzes", "tokens": ["Schmu\u00b7ckes", "M\u00e4nn\u00b7chen", ",", "das", "sein", "schwar\u00b7zes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4ntelchen kokett geachselt.", "tokens": ["M\u00e4n\u00b7tel\u00b7chen", "ko\u00b7kett", "geach\u00b7selt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.14": {"line.1": {"text": "Um die wei\u00dfe Hand zu zeigen,", "tokens": ["Um", "die", "wei\u00b7\u00dfe", "Hand", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haspelt er am Halse, seltsam", "tokens": ["Has\u00b7pelt", "er", "am", "Hal\u00b7se", ",", "selt\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An die Schl\u00e4f' den Zeigefinger,", "tokens": ["An", "die", "Schl\u00e4f'", "den", "Zei\u00b7ge\u00b7fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Kehl' den Daumen dr\u00fcckend.", "tokens": ["An", "die", "Kehl'", "den", "Dau\u00b7men", "dr\u00fc\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Tr\u00e4llert vor sich hin ganz leise,", "tokens": ["Tr\u00e4l\u00b7lert", "vor", "sich", "hin", "ganz", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis er endlich lautaufjubelnd", "tokens": ["Bis", "er", "end\u00b7lich", "laut\u00b7auf\u00b7ju\u00b7belnd"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Stimm' erhebt und singt:", "tokens": ["Sei\u00b7ne", "Stimm'", "er\u00b7hebt", "und", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bblecho Daudi Likras Kalle!", "tokens": ["\u00bb", "le\u00b7cho", "Dau\u00b7di", 