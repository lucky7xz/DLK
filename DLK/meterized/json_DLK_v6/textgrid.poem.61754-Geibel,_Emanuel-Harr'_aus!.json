{"textgrid.poem.61754": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Harr' aus!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00fcrmt im rauhen Kleid von Eisen", "tokens": ["Es", "st\u00fcrmt", "im", "rau\u00b7hen", "Kleid", "von", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschwingten Schritts dahin die Zeit,", "tokens": ["Be\u00b7schwing\u00b7ten", "Schritts", "da\u00b7hin", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum, da\u00df sie dir und deinen Weisen", "tokens": ["Kaum", ",", "da\u00df", "sie", "dir", "und", "dei\u00b7nen", "Wei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ohr noch leiht.", "tokens": ["Ein", "Ohr", "noch", "leiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Umbraust von ihrer Gleise Dr\u00f6hnen,", "tokens": ["Um\u00b7braust", "von", "ih\u00b7rer", "Glei\u00b7se", "Dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihres Marktes ew'ger Hast,", "tokens": ["Von", "ih\u00b7res", "Mark\u00b7tes", "ew'\u00b7ger", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie f\u00e4nde sie zum Dienst des Sch\u00f6nen", "tokens": ["Wie", "f\u00e4n\u00b7de", "sie", "zum", "Dienst", "des", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die heitre Rast!", "tokens": ["Die", "heit\u00b7re", "Rast", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wie ging' in selbstverge\u00dfner Freude", "tokens": ["Wie", "ging'", "in", "selbst\u00b7ver\u00b7ge\u00df\u00b7ner", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz ihr auf beim Fl\u00f6tenlaut,", "tokens": ["Das", "Herz", "ihr", "auf", "beim", "Fl\u00f6\u00b7ten\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schallend zu des Staats Geb\u00e4ude", "tokens": ["Die", "schal\u00b7lend", "zu", "des", "Staats", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Quadern haut!", "tokens": ["Die", "Qua\u00b7dern", "haut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Dem Stoff erst ringt sie ab, dem festen,", "tokens": ["Dem", "Stoff", "erst", "ringt", "sie", "ab", ",", "dem", "fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Werk, dran unsre Sehnsucht h\u00e4ngt;", "tokens": ["Das", "Werk", ",", "dran", "uns\u00b7re", "Sehn\u00b7sucht", "h\u00e4ngt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So murre nicht, da\u00df auch die Besten", "tokens": ["So", "mur\u00b7re", "nicht", ",", "da\u00df", "auch", "die", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stoff bef\u00e4ngt,", "tokens": ["Der", "Stoff", "be\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und da\u00df ihr Blick, vom Schaugepr\u00e4nge", "tokens": ["Und", "da\u00df", "ihr", "Blick", ",", "vom", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerstreut, das alle Sinne reizt,", "tokens": ["Zer\u00b7streut", ",", "das", "al\u00b7le", "Sin\u00b7ne", "reizt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor\u00fcberschweift, wo keusche Strenge", "tokens": ["Vor\u00b7\u00fc\u00b7bersc\u00b7hweift", ",", "wo", "keu\u00b7sche", "Stren\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Farben geizt.", "tokens": ["Mit", "Far\u00b7ben", "geizt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Willst du den m\u00fcden Werkmann schelten,", "tokens": ["Willst", "du", "den", "m\u00fc\u00b7den", "Werk\u00b7mann", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den rasch unechter Prunk besticht?", "tokens": ["Den", "rasch", "un\u00b7ech\u00b7ter", "Prunk", "be\u00b7sticht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur la\u00df sein Ma\u00df f\u00fcr dich nicht gelten", "tokens": ["Nur", "la\u00df", "sein", "Ma\u00df", "f\u00fcr", "dich", "nicht", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "APPR", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dein Gedicht.", "tokens": ["Und", "dein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Dem Gott gehorchend, der die Leier", "tokens": ["Dem", "Gott", "ge\u00b7hor\u00b7chend", ",", "der", "die", "Lei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir weihte, harr' in Treuen aus!", "tokens": ["Dir", "weih\u00b7te", ",", "harr'", "in", "Treu\u00b7en", "aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es folgen Wochen goldner Feier", "tokens": ["Es", "fol\u00b7gen", "Wo\u00b7chen", "gold\u00b7ner", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zeit des Baus.", "tokens": ["Der", "Zeit", "des", "Baus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Da\u00df dann ein sp\u00e4ter Kranz dir werde,", "tokens": ["Da\u00df", "dann", "ein", "sp\u00e4\u00b7ter", "Kranz", "dir", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergi\u00df des Tages fl\u00fccht'ge Gunst", "tokens": ["Ver\u00b7gi\u00df", "des", "Ta\u00b7ges", "fl\u00fccht'\u00b7ge", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und opfre standhaft fort am Herde", "tokens": ["Und", "opf\u00b7re", "stand\u00b7haft", "fort", "am", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der reinen Kunst!", "tokens": ["Der", "rei\u00b7nen", "Kunst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}