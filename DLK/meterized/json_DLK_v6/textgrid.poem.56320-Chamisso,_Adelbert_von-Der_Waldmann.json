{"textgrid.poem.56320": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Waldmann", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wandrer eilt das Tal hinauf,", "tokens": ["Der", "Wand\u00b7rer", "eilt", "das", "Tal", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er steigert fast den Schritt zum Lauf,", "tokens": ["Er", "stei\u00b7gert", "fast", "den", "Schritt", "zum", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Pfad ist steil, die Nacht bricht ein,", "tokens": ["Der", "Pfad", "ist", "steil", ",", "die", "Nacht", "bricht", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie Sonne sinkt in blut'gem Schein,", "tokens": ["Sie", "Son\u00b7ne", "sinkt", "in", "blut'\u00b7gem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nebel ziehn um den Drachenstein.", "tokens": ["Die", "Ne\u00b7bel", "ziehn", "um", "den", "Dra\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wie er bald das Dorf erreicht,", "tokens": ["Und", "wie", "er", "bald", "das", "Dorf", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein seltsam Bild vor\u00fcber schleicht,", "tokens": ["Ein", "selt\u00b7sam", "Bild", "vor\u00b7\u00fc\u00b7ber", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gespenstisch fast, unheimlicher Gast; \u2013", "tokens": ["Ge\u00b7spens\u00b7tisch", "fast", ",", "un\u00b7heim\u00b7li\u00b7cher", "Gast", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dr\u00fcckt ihn annoch des Lebens Last?", "tokens": ["Dr\u00fcckt", "ihn", "an\u00b7noch", "des", "Le\u00b7bens", "Last", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Gew\u00e4hrt das Grab ihm keine Rast?", "tokens": ["Ge\u00b7w\u00e4hrt", "das", "Grab", "ihm", "kei\u00b7ne", "Rast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbihr friedlichen Leute, was zaget ihr,", "tokens": ["\u00bb", "ihr", "fried\u00b7li\u00b7chen", "Leu\u00b7te", ",", "was", "za\u00b7get", "ihr", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und kreuziget euch, und zittert schier?\u00ab \u2013", "tokens": ["Und", "kreu\u00b7zi\u00b7get", "euch", ",", "und", "zit\u00b7tert", "schier", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJD", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ob mir das Haar zu Berge steigt,", "tokens": ["Ob", "mir", "das", "Haar", "zu", "Ber\u00b7ge", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sag's dir an, wenn alles schweigt:", "tokens": ["Ich", "sag's", "dir", "an", ",", "wenn", "al\u00b7les", "schweigt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es hat der Waldmann sich gezeigt.", "tokens": ["Es", "hat", "der", "Wald\u00b7mann", "sich", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbder Waldmann?\u00ab \u2013 Ja. Du wirst nicht bleich,", "tokens": ["\u00bb", "der", "Wald\u00b7mann", "?", "\u00ab", "\u2013", "Ja", ".", "Du", "wirst", "nicht", "bleich", ","], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "$(", "PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist hier fremd, ich dacht es gleich;", "tokens": ["Du", "bist", "hier", "fremd", ",", "ich", "dacht", "es", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein achtzigj\u00e4hr'ger Mann,", "tokens": ["Ich", "bin", "ein", "acht\u00b7zig\u00b7j\u00e4hr'\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und war ein Kind, als sich's entspann,", "tokens": ["Und", "war", "ein", "Kind", ",", "als", "sich's", "ent\u00b7spann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin's, der Kunde geben kann.", "tokens": ["Ich", "bin's", ",", "der", "Kun\u00b7de", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Drachenburg stand dazumal", "tokens": ["Die", "Dra\u00b7chen\u00b7burg", "stand", "da\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stolz funkelnd noch im Sonnenstrahl;", "tokens": ["Stolz", "fun\u00b7kelnd", "noch", "im", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lebte der Graf in Herrlichkeit,", "tokens": ["Da", "leb\u00b7te", "der", "Graf", "in", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei ihm, bewundert weit und breit,", "tokens": ["Bei", "ihm", ",", "be\u00b7wun\u00b7dert", "weit", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das junge Fr\u00e4ulein Adelheid.", "tokens": ["Das", "jun\u00b7ge", "Fr\u00e4u\u00b7lein", "A\u00b7del\u00b7heid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Schreiber Waldmann, h\u00f6flicher Art,", "tokens": ["Der", "Schrei\u00b7ber", "Wald\u00b7mann", ",", "h\u00f6f\u00b7li\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Tr\u00fcbsinnig, bla\u00df und hochgelahrt,", "tokens": ["Tr\u00fcb\u00b7sin\u00b7nig", ",", "bla\u00df", "und", "hoch\u00b7ge\u00b7lahrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erfreute sich der Gunst des Herrn;", "tokens": ["Er\u00b7freu\u00b7te", "sich", "der", "Gunst", "des", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sah das Fr\u00e4ulein nur zu gern,", "tokens": ["Er", "sah", "das", "Fr\u00e4u\u00b7lein", "nur", "zu", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der Versucher blieb nicht fern.", "tokens": ["Und", "der", "Ver\u00b7su\u00b7cher", "blieb", "nicht", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu reden wie er, kein andrer verstund;", "tokens": ["Zu", "re\u00b7den", "wie", "er", ",", "kein", "an\u00b7drer", "ver\u00b7stund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "PPER", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er webte fein mit falschem Mund", "tokens": ["Er", "web\u00b7te", "fein", "mit", "fal\u00b7schem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Netz, womit er sie umschlang;", "tokens": ["Das", "Netz", ",", "wo\u00b7mit", "er", "sie", "um\u00b7schlang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sprach von Lieb, er sprach von Rang,", "tokens": ["Er", "sprach", "von", "Lieb", ",", "er", "sprach", "von", "Rang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von freier Wahl und hartem Zwang;", "tokens": ["Von", "frei\u00b7er", "Wahl", "und", "har\u00b7tem", "Zwang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von Gott und Christo nebenbei,", "tokens": ["Von", "Gott", "und", "Chris\u00b7to", "ne\u00b7ben\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und S\u00fcndenhaftes allerlei;", "tokens": ["Und", "S\u00fcn\u00b7den\u00b7haf\u00b7tes", "al\u00b7ler\u00b7lei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hat er sie best\u00fcrmt, geplagt,", "tokens": ["So", "hat", "er", "sie", "be\u00b7st\u00fcrmt", ",", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gequ\u00e4lt, umgarnt, sei's Gott geklagt,", "tokens": ["Ge\u00b7qu\u00e4lt", ",", "um\u00b7garnt", ",", "sei's", "Gott", "ge\u00b7klagt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis sie ihm Liebe zugesagt.", "tokens": ["Bis", "sie", "ihm", "Lie\u00b7be", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sp\u00e4t ward's dem Vater hinterbracht,", "tokens": ["Sp\u00e4t", "ward's", "dem", "Va\u00b7ter", "hin\u00b7ter\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Zorn, sein Mitleid sind erwacht;", "tokens": ["Sein", "Zorn", ",", "sein", "Mit\u00b7leid", "sind", "er\u00b7wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Kind Erbarmen bei ihm fand,", "tokens": ["Sein", "Kind", "Er\u00b7bar\u00b7men", "bei", "ihm", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der falsche Schreiber ward verbannt,", "tokens": ["Der", "fal\u00b7sche", "Schrei\u00b7ber", "ward", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Leibesstrafe von Burg und Land.", "tokens": ["Bei", "Lei\u00b7bes\u00b7stra\u00b7fe", "von", "Burg", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Sch\u00f6n Adelheid in Tr\u00e4nen zerflo\u00df,", "tokens": ["Sch\u00f6n", "A\u00b7del\u00b7heid", "in", "Tr\u00e4\u00b7nen", "zer\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Waldmann aber irrt' um das Schlo\u00df;", "tokens": ["Der", "Wald\u00b7mann", "a\u00b7ber", "irrt'", "um", "das", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er kannt nicht Ruh, er wu\u00dft nicht Rat,", "tokens": ["Er", "kannt", "nicht", "Ruh", ",", "er", "wu\u00dft", "nicht", "Rat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er w\u00fctete, br\u00fctete fr\u00fch und spat,", "tokens": ["Er", "w\u00fc\u00b7te\u00b7te", ",", "br\u00fc\u00b7te\u00b7te", "fr\u00fch", "und", "spat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und sann auf schauerliche Tat.", "tokens": ["Und", "sann", "auf", "schau\u00b7er\u00b7li\u00b7che", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Er sandt ihr heimlich einen Brief,", "tokens": ["Er", "sandt", "ihr", "heim\u00b7lich", "ei\u00b7nen", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovor es kalt sie \u00fcberlief:", "tokens": ["Wo\u00b7vor", "es", "kalt", "sie", "\u00fc\u00b7berl\u00b7ief", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter":