{"dta.poem.12681": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die wohllust.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die wohllust k\u00f6nten wir weit besser wehlust heissen,", "tokens": ["Die", "wohl\u00b7lust", "k\u00f6n\u00b7ten", "wir", "weit", "bes\u00b7ser", "weh\u00b7lust", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil man am ende mu\u00df in saure \u00e4pffel beissen.", "tokens": ["Weil", "man", "am", "en\u00b7de", "mu\u00df", "in", "sau\u00b7re", "\u00e4pf\u00b7fel", "beis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Adam und Eva sich furchtsam verstackten,", "tokens": ["Als", "A\u00b7dam", "und", "E\u00b7va", "sich", "furcht\u00b7sam", "ver\u00b7stack\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da nahmen sie ein feigen-blat,", "tokens": ["Da", "nah\u00b7men", "sie", "ein", "fei\u00b7gen\u00b7blat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit sie die jenige schande bedackten,", "tokens": ["Da\u00b7mit", "sie", "die", "je\u00b7ni\u00b7ge", "schan\u00b7de", "be\u00b7dack\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die ihre schuld entdecket hat;", "tokens": ["Die", "ih\u00b7re", "schuld", "ent\u00b7de\u00b7cket", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch unsere jungfern entbl\u00f6\u00dfen die schande.", "tokens": ["Doch", "un\u00b7se\u00b7re", "jung\u00b7fern", "ent\u00b7bl\u00f6\u00b7\u00dfen", "die", "schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "ART", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Was macht es? es wachsen nicht feigen im lande.", "tokens": ["Was", "macht", "es", "?", "es", "wach\u00b7sen", "nicht", "fei\u00b7gen", "im", "lan\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}