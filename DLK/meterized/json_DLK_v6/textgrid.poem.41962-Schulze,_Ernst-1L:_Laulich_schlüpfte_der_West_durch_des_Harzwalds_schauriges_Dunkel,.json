{"textgrid.poem.41962": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Laulich schl\u00fcpfte der West durch des Harzwalds schauriges Dunkel,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laulich schl\u00fcpfte der West durch des Harzwalds schauriges Dunkel,", "tokens": ["Lau\u00b7lich", "schl\u00fcpf\u00b7te", "der", "West", "durch", "des", "Harz\u00b7walds", "schau\u00b7ri\u00b7ges", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ueber den felsigen H\u00f6hn spielte das Abendgew\u00f6lk;", "tokens": ["Ue\u00b7ber", "den", "fel\u00b7si\u00b7gen", "H\u00f6hn", "spiel\u00b7te", "das", "A\u00b7bend\u00b7ge\u00b7w\u00f6lk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Sehnsucht rieselt' im Quell, und im Berghain s\u00e4uselte Sehnsucht,", "tokens": ["Sehn\u00b7sucht", "rie\u00b7selt'", "im", "Quell", ",", "und", "im", "Berg\u00b7hain", "s\u00e4u\u00b7sel\u00b7te", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "KON", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sehnsucht wiegte sich her auf dem entfernten Gel\u00e4ut;", "tokens": ["Sehn\u00b7sucht", "wieg\u00b7te", "sich", "her", "auf", "dem", "ent\u00b7fern\u00b7ten", "Ge\u00b7l\u00e4ut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APZR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Br\u00e4utlich entschl\u00fcpfte mit zagendem Fu\u00df dem Himmel die D\u00e4mmrung,", "tokens": ["Br\u00e4ut\u00b7lich", "ent\u00b7schl\u00fcpf\u00b7te", "mit", "za\u00b7gen\u00b7dem", "Fu\u00df", "dem", "Him\u00b7mel", "die", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Ihren fr\u00fchesten Ku\u00df feyerte lispelnd der Hain.", "tokens": ["Ih\u00b7ren", "fr\u00fc\u00b7hes\u00b7ten", "Ku\u00df", "fe\u00b7yer\u00b7te", "lis\u00b7pelnd", "der", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Siehe, da wandelten wir durch des Thalgrunds buschichte Kr\u00fcmmung,", "tokens": ["Sie\u00b7he", ",", "da", "wan\u00b7del\u00b7ten", "wir", "durch", "des", "Thal\u00b7grunds", "bu\u00b7schich\u00b7te", "Kr\u00fcm\u00b7mung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Unter dem s\u00e4uselnden Dach dunkeler Buchen dahin.", "tokens": ["Un\u00b7ter", "dem", "s\u00e4u\u00b7seln\u00b7den", "Dach", "dun\u00b7ke\u00b7ler", "Bu\u00b7chen", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PAV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Um den entragenden Fels wand aufw\u00e4rts-strebend der Pfad sich,", "tokens": ["Um", "den", "ent\u00b7ra\u00b7gen\u00b7den", "Fels", "wand", "auf\u00b7w\u00e4rts\u00b7stre\u00b7bend", "der", "Pfad", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "PRF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und wild rauschte der Bach durch das zerri\u00dfne Gestein.", "tokens": ["Und", "wild", "rauschte", "der", "Bach", "durch", "das", "zer\u00b7ri\u00df\u00b7ne", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ach, wir wagten es kaum uns anzublicken; denn Mi\u00dfgunst", "tokens": ["Ach", ",", "wir", "wag\u00b7ten", "es", "kaum", "uns", "an\u00b7zu\u00b7bli\u00b7cken", ";", "denn", "Mi\u00df\u00b7gunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "PPER", "VVIZU", "$.", "KON", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Schlich sich und Eifersucht hinter uns leise daher.", "tokens": ["Schlich", "sich", "und", "Ei\u00b7fer\u00b7sucht", "hin\u00b7ter", "uns", "lei\u00b7se", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "KON", "NN", "APPR", "PPER", "ADJD", "PAV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.13": {"text": "Lauernde Hyder, du w\u00e4hnst, es sinke, von deines Mundes", "tokens": ["Lau\u00b7ern\u00b7de", "Hy\u00b7der", ",", "du", "w\u00e4hnst", ",", "es", "sin\u00b7ke", ",", "von", "dei\u00b7nes", "Mun\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Giftigem Hauche ber\u00fchrt, welkend die Liebe dahin!", "tokens": ["Gif\u00b7ti\u00b7gem", "Hau\u00b7che", "be\u00b7r\u00fchrt", ",", "wel\u00b7kend", "die", "Lie\u00b7be", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "PAV", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Aber es ist umsonst der Triumph! Hoch flattert der Ph\u00f6nix", "tokens": ["A\u00b7ber", "es", "ist", "um\u00b7sonst", "der", "Tri\u00b7umph", "!", "Hoch", "flat\u00b7tert", "der", "Ph\u00f6\u00b7nix"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "$.", "ADJD", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-++--+-", "measure": "iambic.septa.invert"}, "line.16": {"text": "Ueber der Asche; der Schmerz n\u00e4hret die Liebe wie Thau.", "tokens": ["Ue\u00b7ber", "der", "A\u00b7sche", ";", "der", "Schmerz", "n\u00e4h\u00b7ret", "die", "Lie\u00b7be", "wie", "Thau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "KOKOM", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Blumen pfl\u00fcckte die Reizende sich, der d\u00fcrftigen Felsh\u00f6hn", "tokens": ["Blu\u00b7men", "pfl\u00fcck\u00b7te", "die", "Rei\u00b7zen\u00b7de", "sich", ",", "der", "d\u00fcrf\u00b7ti\u00b7gen", "Fels\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PRF", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Einfach bl\u00fchenden Schmuck, suchte die Bl\u00fcthe des Klees,", "tokens": ["Ein\u00b7fach", "bl\u00fc\u00b7hen\u00b7den", "Schmuck", ",", "such\u00b7te", "die", "Bl\u00fc\u00b7the", "des", "Klees", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Brach Stiefm\u00fctterchen sich, und, des unscheinbaren Geschenks froh,", "tokens": ["Brach", "Stief\u00b7m\u00fct\u00b7ter\u00b7chen", "sich", ",", "und", ",", "des", "un\u00b7schein\u00b7ba\u00b7ren", "Ge\u00b7schenks", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "$,", "KON", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "--+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Schm\u00fcckte sie Busen und Haar, ach, mit dem gl\u00fccklichen Raub.", "tokens": ["Schm\u00fcck\u00b7te", "sie", "Bu\u00b7sen", "und", "Haar", ",", "ach", ",", "mit", "dem", "gl\u00fcck\u00b7li\u00b7chen", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "ITJ", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Siehe, da kr\u00e4nzten die Quelle Vergi\u00dfmeinnicht, und behende", "tokens": ["Sie\u00b7he", ",", "da", "kr\u00e4nz\u00b7ten", "die", "Quel\u00b7le", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "und", "be\u00b7hen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,", "KON", "ADJA"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.22": {"text": "Stieg ich hinab und brach froh das bedeutende Blau.", "tokens": ["Stieg", "ich", "hin\u00b7ab", "und", "brach", "froh", "das", "be\u00b7deu\u00b7ten\u00b7de", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.23": {"text": "Wirf, so sprach ich mit fl\u00fcsterndem Laut und nahte behutsam,", "tokens": ["Wirf", ",", "so", "sprach", "ich", "mit", "fl\u00fcs\u00b7tern\u00b7dem", "Laut", "und", "nah\u00b7te", "be\u00b7hut\u00b7sam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Wirf die Blumen hinweg, sch\u00f6nere bl\u00fchten f\u00fcr dich,", "tokens": ["Wirf", "die", "Blu\u00b7men", "hin\u00b7weg", ",", "sch\u00f6\u00b7ne\u00b7re", "bl\u00fch\u00b7ten", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "ADJA", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Sch\u00f6nere bl\u00fchten f\u00fcr mich; o nimm sie; kennst du sie, Freundin?", "tokens": ["Sch\u00f6\u00b7ne\u00b7re", "bl\u00fch\u00b7ten", "f\u00fcr", "mich", ";", "o", "nimm", "sie", ";", "kennst", "du", "sie", ",", "Freun\u00b7din", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "FM", "VVIMP", "PPER", "$.", "VVFIN", "PPER", "PPER", "$,", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Holde, vergi\u00df mein nicht! La\u00df sie am Herzen dir bl\u00fchn!", "tokens": ["Hol\u00b7de", ",", "ver\u00b7gi\u00df", "mein", "nicht", "!", "La\u00df", "sie", "am", "Her\u00b7zen", "dir", "bl\u00fchn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPOSAT", "PTKNEG", "$.", "VVIMP", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Und sie nahte mit dankendem Blick und winkte Gew\u00e4hrung,", "tokens": ["Und", "sie", "nah\u00b7te", "mit", "dan\u00b7ken\u00b7dem", "Blick", "und", "wink\u00b7te", "Ge\u00b7w\u00e4h\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Drauf, abweichend vom Pfad, nahte dem Haine sie sich,", "tokens": ["Drauf", ",", "ab\u00b7wei\u00b7chend", "vom", "Pfad", ",", "nah\u00b7te", "dem", "Hai\u00b7ne", "sie", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJD", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PRF", "$,"], "meter": "-++--+---+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Da\u00df unmerklich ein Zweig von der Brust abstreifte den Feldstrau\u00df,", "tokens": ["Da\u00df", "un\u00b7mer\u00b7klich", "ein", "Zweig", "von", "der", "Brust", "ab\u00b7streif\u00b7te", "den", "Felds\u00b7trau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Und den zertretenen Schmuck, klagte die Heuchlerin jetzt.", "tokens": ["Und", "den", "zer\u00b7tre\u00b7te\u00b7nen", "Schmuck", ",", "klag\u00b7te", "die", "Heuch\u00b7le\u00b7rin", "jetzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Mit nachl\u00e4ssiger Hand nun barg mein zartes Geschenk sie,", "tokens": ["Mit", "nach\u00b7l\u00e4s\u00b7si\u00b7ger", "Hand", "nun", "barg", "mein", "zar\u00b7tes", "Ge\u00b7schenk", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Doch mit bedeutendem Wink, an der erbebenden Brust,", "tokens": ["Doch", "mit", "be\u00b7deu\u00b7ten\u00b7dem", "Wink", ",", "an", "der", "er\u00b7be\u00b7ben\u00b7den", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Und stolz thronte der Strau\u00df und bl\u00fchete \u00fcppiger: doch bald", "tokens": ["Und", "stolz", "thron\u00b7te", "der", "Strau\u00df", "und", "bl\u00fc\u00b7he\u00b7te", "\u00fcp\u00b7pi\u00b7ger", ":", "doch", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NE", "KON", "VVFIN", "ADJD", "$.", "ADV", "ADV"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Sanken, von heimlicher Gluth welkend, die Bl\u00e4tter dahin.", "tokens": ["San\u00b7ken", ",", "von", "heim\u00b7li\u00b7cher", "Gluth", "wel\u00b7kend", ",", "die", "Bl\u00e4t\u00b7ter", "da\u00b7hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "PAV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Fort nun wandelten wir in dem Graun des k\u00fchleren Nachthauchs,", "tokens": ["Fort", "nun", "wan\u00b7del\u00b7ten", "wir", "in", "dem", "Graun", "des", "k\u00fch\u00b7le\u00b7ren", "Nacht\u00b7hauchs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Und mir st\u00e4rkten des Hains dichtere Schatten den Muth.", "tokens": ["Und", "mir", "st\u00e4rk\u00b7ten", "des", "Hains", "dich\u00b7te\u00b7re", "Schat\u00b7ten", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.37": {"text": "Gieb, so fl\u00fcstert' ich jetzt, o gieb ein einziges Bl\u00fcmchen", "tokens": ["Gieb", ",", "so", "fl\u00fcs\u00b7tert'", "ich", "jetzt", ",", "o", "gieb", "ein", "ein\u00b7zi\u00b7ges", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Nur, und die Bitte, die ich flehete, gieb sie zur\u00fcck!", "tokens": ["Nur", ",", "und", "die", "Bit\u00b7te", ",", "die", "ich", "fle\u00b7he\u00b7te", ",", "gieb", "sie", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Ach, nie flieht die Erinnerung mich des seligen Abends,", "tokens": ["Ach", ",", "nie", "flieht", "die", "E\u00b7rin\u00b7ne\u00b7rung", "mich", "des", "se\u00b7li\u00b7gen", "A\u00b7bends", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Doch viel sch\u00f6ner noch ist's, hab' ich ein bindendes Pfand.", "tokens": ["Doch", "viel", "sch\u00f6\u00b7ner", "noch", "ist's", ",", "hab'", "ich", "ein", "bin\u00b7den\u00b7des", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Sieh, schon welkten die Blumen dahin, so h\u00f6rt ich es lispeln,", "tokens": ["Sieh", ",", "schon", "welk\u00b7ten", "die", "Blu\u00b7men", "da\u00b7hin", ",", "so", "h\u00f6rt", "ich", "es", "lis\u00b7peln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "PAV", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "--+--+--+-+--++", "measure": "anapaest.tri.plus"}, "line.42": {"text": "Und der Erinnerung frommt nimmer ein welkes Geschenk;", "tokens": ["Und", "der", "E\u00b7rin\u00b7ne\u00b7rung", "frommt", "nim\u00b7mer", "ein", "wel\u00b7kes", "Ge\u00b7schenk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Sch\u00f6nere pfl\u00fcck' ich und frische dir einst; stets bl\u00fchet der Kranz dann,", "tokens": ["Sch\u00f6\u00b7ne\u00b7re", "pfl\u00fcck", "ich", "und", "fri\u00b7sche", "dir", "einst", ";", "stets", "bl\u00fc\u00b7het", "der", "Kranz", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJA", "PPER", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Wenn mitleidig ein Gott heimliche W\u00fcnsche gew\u00e4hrt.", "tokens": ["Wenn", "mit\u00b7lei\u00b7dig", "ein", "Gott", "heim\u00b7li\u00b7che", "W\u00fcn\u00b7sche", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.45": {"text": "Also sprach sie und nahete leis' und dr\u00fcckte mit Beben", "tokens": ["Al\u00b7so", "sprach", "sie", "und", "na\u00b7he\u00b7te", "leis'", "und", "dr\u00fcck\u00b7te", "mit", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.46": {"text": "Sanft die gebotene Hand an die ergl\u00fchende Brust.", "tokens": ["Sanft", "die", "ge\u00b