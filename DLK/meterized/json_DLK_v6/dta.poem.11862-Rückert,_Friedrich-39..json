{"dta.poem.11862": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen in dem Korn, sie k\u00f6nnen dich nicht n\u00e4hren;", "tokens": ["Die", "Blu\u00b7men", "in", "dem", "Korn", ",", "sie", "k\u00f6n\u00b7nen", "dich", "nicht", "n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Orte wo sie bl\u00fchn, da k\u00f6nnten wachsen Aehren.", "tokens": ["Am", "Or\u00b7te", "wo", "sie", "bl\u00fchn", ",", "da", "k\u00f6nn\u00b7ten", "wach\u00b7sen", "A\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWAV", "PPER", "VVINF", "$,", "KOUS", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die andern Aehren auch, die wachsenden daneben,", "tokens": ["Die", "an\u00b7dern", "A\u00b7eh\u00b7ren", "auch", ",", "die", "wach\u00b7sen\u00b7den", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "ADJD", "PAV", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zertreten Knaben dir, die nach den Blumen streben.", "tokens": ["Zer\u00b7tre\u00b7ten", "Kna\u00b7ben", "dir", ",", "die", "nach", "den", "Blu\u00b7men", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem Nachbar sind verschont die blumenlosen Saaten;", "tokens": ["Dem", "Nach\u00b7bar", "sind", "ver\u00b7schont", "die", "blu\u00b7men\u00b7lo\u00b7sen", "Saa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So \u00fcbel hat dich Gott mit diesem Schmuck berathen.", "tokens": ["So", "\u00fc\u00b7bel", "hat", "dich", "Gott", "mit", "die\u00b7sem", "Schmuck", "be\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}