{"dta.poem.9340": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvII .  \n Ein Muster Gemeiner Art/  \n so sich mit Weiblichen Versen anf\u00e4het.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was unser Rund die gro\u00dfe Welt \u00fcmbgreiffet/", "tokens": ["Was", "un\u00b7ser", "Rund", "die", "gro\u00b7\u00dfe", "Welt", "\u00fcmb\u00b7greif\u00b7fet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "was durch das licht der rothen Sonnen reiffet/", "tokens": ["was", "durch", "das", "licht", "der", "ro\u00b7then", "Son\u00b7nen", "reif\u00b7fet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "was geht und steht/ was lebet/ schleicht und kreucht/", "tokens": ["was", "geht", "und", "steht", "/", "was", "le\u00b7bet", "/", "schleicht", "und", "kreucht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$(", "PWS", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "hatt seine zeit/ so weit der Himmel reicht.", "tokens": ["hatt", "sei\u00b7ne", "zeit", "/", "so", "weit", "der", "Him\u00b7mel", "reicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An dieses licht der Welt gebohren werden/", "tokens": ["An", "die\u00b7ses", "licht", "der", "Welt", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und wieder\u00fcmb genommen von der erden", "tokens": ["und", "wie\u00b7de\u00b7r\u00fcmb", "ge\u00b7nom\u00b7men", "von", "der", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "hatt seine zeit; Zunehmen und vergehn/", "tokens": ["hatt", "sei\u00b7ne", "zeit", ";", "Zu\u00b7neh\u00b7men", "und", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "verwelcken auch und voll in bl\u00fchen stehn/", "tokens": ["ver\u00b7wel\u00b7cken", "auch", "und", "voll", "in", "bl\u00fc\u00b7hen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADJD", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "hatt seine zeit; Das rotten/ pflantzen/ hauen/", "tokens": ["hatt", "sei\u00b7ne", "zeit", ";", "Das", "rot\u00b7ten", "/", "pflant\u00b7zen", "/", "hau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "das w\u00fcrgen auch/ das heilen/ brechen/ bauen/", "tokens": ["das", "w\u00fcr\u00b7gen", "auch", "/", "das", "hei\u00b7len", "/", "bre\u00b7chen", "/", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$(", "PDS", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "hatt seine zeit in klag\u2019 und lachen seyn/", "tokens": ["hatt", "sei\u00b7ne", "zeit", "in", "klag'", "und", "la\u00b7chen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "VVFIN", "KON", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "der tantz/ das leid/ das wohler gehn/ die pein/", "tokens": ["der", "tantz", "/", "das", "leid", "/", "das", "woh\u00b7ler", "gehn", "/", "die", "pein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "$(", "ART", "ADJA", "VVINF", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "hatt seine zeit; stein-suchen/ steine streuen/", "tokens": ["hatt", "sei\u00b7ne", "zeit", ";", "stein\u00b7su\u00b7chen", "/", "stei\u00b7ne", "streu\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "ADJA", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "das hertzen auch/ mit schertzen sich erfreuen/", "tokens": ["das", "hert\u00b7zen", "auch", "/", "mit", "schert\u00b7zen", "sich", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "APPR", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "von k\u00fcsson weit und schertzen ferne seyn/", "tokens": ["von", "k\u00fcs\u00b7son", "weit", "und", "schert\u00b7zen", "fer\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "KON", "VVFIN", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "hatt seine zeit, verliehren/ samlen ein.", "tokens": ["hatt", "sei\u00b7ne", "zeit", ",", "ver\u00b7lieh\u00b7ren", "/", "sam\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VVINF", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zurei\u00dfen auch/ zunehen/ werffen/ halten/", "tokens": ["Zu\u00b7rei\u00b7\u00dfen", "auch", "/", "zu\u00b7ne\u00b7hen", "/", "werf\u00b7fen", "/", "hal\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$(", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "hatt seine zeit; veri\u00fcngen und veralten/", "tokens": ["hatt", "sei\u00b7ne", "zeit", ";", "ve\u00b7ri\u00fcn\u00b7gen", "und", "ver\u00b7al\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "das lieben auch und hassen fried und streit/", "tokens": ["das", "lie\u00b7ben", "auch", "und", "has\u00b7sen", "fried", "und", "streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "ja alles hatt bestimmte stund\u2019 und zeit. Pred. Sal 3", "tokens": ["ja", "al\u00b7les", "hatt", "be\u00b7stimm\u00b7te", "stund'", "und", "zeit", ".", "Pred", ".", "Sal", "3"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "number"], "pos": ["ADV", "PIS", "VAFIN", "ADJA", "NN", "KON", "NN", "$.", "NE", "$.", "VMFIN", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}