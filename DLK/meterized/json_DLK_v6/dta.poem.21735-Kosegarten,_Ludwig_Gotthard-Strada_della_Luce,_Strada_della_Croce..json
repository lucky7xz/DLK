{"dta.poem.21735": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Strada della Luce, Strada della Croce.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch das Kreuz zum Glanz!", "tokens": ["Durch", "das", "Kreuz", "zum", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wem geb\u00fchrt der Kranz?", "tokens": ["Wem", "ge\u00b7b\u00fchrt", "der", "Kranz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer getrost gelitten,", "tokens": ["Wer", "ge\u00b7trost", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wer mit Kraft gestritten,", "tokens": ["Wer", "mit", "Kraft", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dem geb\u00fchrt der Kranz.", "tokens": ["Dem", "ge\u00b7b\u00fchrt", "der", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Durch das Kreuz zum Glanz!", "tokens": ["Durch", "das", "Kreuz", "zum", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Durch die Nacht zum Licht!", "tokens": ["Durch", "die", "Nacht", "zum", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Herz, verzage nicht,", "tokens": ["Herz", ",", "ver\u00b7za\u00b7ge", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob von Nacht umschauert,", "tokens": ["Ob", "von", "Nacht", "um\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Erd' und Himmel trauert \u2014", "tokens": ["Erd'", "und", "Him\u00b7mel", "trau\u00b7ert"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schau der Ost ergl\u00fcht,", "tokens": ["Schau", "der", "Ost", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und das Dunkel flieht.", "tokens": ["Und", "das", "Dun\u00b7kel", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Durch den Schweiss zum Schlaf!", "tokens": ["Durch", "den", "Schweiss", "zum", "Schlaf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Stach die Sonne, traf", "tokens": ["Stach", "die", "Son\u00b7ne", ",", "traf"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dich des Mittags Schw\u00fcle;", "tokens": ["Dich", "des", "Mit\u00b7tags", "Schw\u00fc\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Abends lullt die K\u00fchle", "tokens": ["A\u00b7bends", "lullt", "die", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In gew\u00fcnschten Schlaf,", "tokens": ["In", "ge\u00b7w\u00fcnschten", "Schlaf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wen die Schw\u00fcle traf.", "tokens": ["Wen", "die", "Schw\u00fc\u00b7le", "traf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Durch den Kampf zum Sieg!", "tokens": ["Durch", "den", "Kampf", "zum", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Krieg geboten, Krieg", "tokens": ["Krieg", "ge\u00b7bo\u00b7ten", ",", "Krieg"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVPP", "$,", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sey der Welt, der Schn\u00f6den!", "tokens": ["Sey", "der", "Welt", ",", "der", "Schn\u00f6\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schande deckt den Bl\u00f6den.", "tokens": ["Schan\u00b7de", "deckt", "den", "Bl\u00f6\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Drum sey bis zum Sieg", "tokens": ["Drum", "sey", "bis", "zum", "Sieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Krieg die Losung, Krieg!", "tokens": ["Krieg", "die", "Lo\u00b7sung", ",", "Krieg", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Durch den Krieg zum Kranz!", "tokens": ["Durch", "den", "Krieg", "zum", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Durch das Kreuz zum Glanz!", "tokens": ["Durch", "das", "Kreuz", "zum", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Durch Gehenna's Grauen", "tokens": ["Durch", "Ge\u00b7hen\u00b7na's", "Grau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hin zu Zions Auen!", "tokens": ["Hin", "zu", "Zi\u00b7ons", "Au\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Selig, wer beharrt,", "tokens": ["Se\u00b7lig", ",", "wer", "be\u00b7harrt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ob das Herz auch starrt!", "tokens": ["Ob", "das", "Herz", "auch", "starrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}