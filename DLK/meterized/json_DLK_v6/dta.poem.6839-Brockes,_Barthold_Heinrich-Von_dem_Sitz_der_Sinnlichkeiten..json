{"dta.poem.6839": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist, ist in der Seel: doch haben unsre Sinnen", "tokens": ["Was", "ist", ",", "ist", "in", "der", "Seel", ":", "doch", "ha\u00b7ben", "uns\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "VAFIN", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sitz in einem Punct des Hirns von innen,", "tokens": ["Den", "Sitz", "in", "ei\u00b7nem", "Punct", "des", "Hirns", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der werth, da\u00df man ihm wohl beachtet und erkennt.", "tokens": ["Der", "werth", ",", "da\u00df", "man", "ihm", "wohl", "be\u00b7ach\u00b7tet", "und", "er\u00b7kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "PIS", "PPER", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wie ein allgemein und n\u00f6thig Instrument,", "tokens": ["Als", "wie", "ein", "all\u00b7ge\u00b7mein", "und", "n\u00f6\u00b7thig", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "(dis ist sein Vorzugs-Recht und eigentlichs Gesch\u00e4ffte)", "tokens": ["(", "dis", "ist", "sein", "Vor\u00b7zugs\u00b7Recht", "und", "ei\u00b7gent\u00b7lichs", "Ge\u00b7sch\u00e4ff\u00b7te", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vereint und theilt er ein der Sinnen Kr\u00e4ffte.", "tokens": ["Ver\u00b7eint", "und", "theilt", "er", "ein", "der", "Sin\u00b7nen", "Kr\u00e4ff\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ART", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Nase f\u00fchlet nie den Eindruck von dem Licht,", "tokens": ["Die", "Na\u00b7se", "f\u00fch\u00b7let", "nie", "den", "Ein\u00b7druck", "von", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Auge sp\u00fcrt das Riechen nicht,", "tokens": ["Das", "Au\u00b7ge", "sp\u00fcrt", "das", "Rie\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Ohr ist nicht geschickt zu schmecken,", "tokens": ["Das", "Ohr", "ist", "nicht", "ge\u00b7schickt", "zu", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In jedem Sinn kan man sein eigen Amt entdecken.", "tokens": ["In", "je\u00b7dem", "Sinn", "kan", "man", "sein", "ei\u00b7gen", "Amt", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PIS", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein noch mehr, man wei\u00df, die Nase selber kan", "tokens": ["Al\u00b7lein", "noch", "mehr", ",", "man", "wei\u00df", ",", "die", "Na\u00b7se", "sel\u00b7ber", "kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "PIS", "VVFIN", "$,", "ART", "NN", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht riechen, auch der Ton schl\u00e4gt an das Ohr nicht an.", "tokens": ["Nicht", "rie\u00b7chen", ",", "auch", "der", "Ton", "schl\u00e4gt", "an", "das", "Ohr", "nicht", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So r\u00fchren Dinge, welche Licht,", "tokens": ["So", "r\u00fch\u00b7ren", "Din\u00b7ge", ",", "wel\u00b7che", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Auch unser Auge nicht,", "tokens": ["Auch", "un\u00b7ser", "Au\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Noch unsre Zunge die, so schmackhafft seyn.", "tokens": ["Noch", "uns\u00b7re", "Zun\u00b7ge", "die", ",", "so", "schmack\u00b7hafft", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "$,", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Drum mu\u00df sich in der Seele finden", "tokens": ["Drum", "mu\u00df", "sich", "in", "der", "See\u00b7le", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Instrument, das allgemein,", "tokens": ["Ein", "Inst\u00b7ru\u00b7ment", ",", "das", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das eintzig h\u00f6ren kan und sehn,", "tokens": ["Das", "eint\u00b7zig", "h\u00f6\u00b7ren", "kan", "und", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Auch schmecken und verstehn,", "tokens": ["Auch", "schme\u00b7cken", "und", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}