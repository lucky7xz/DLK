{"textgrid.poem.33636": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Vergessen und verlassen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur deine Locken k\u00fc\u00dft der Wind,", "tokens": ["Nur", "dei\u00b7ne", "Lo\u00b7cken", "k\u00fc\u00dft", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst ist es ringsum stille Nacht.", "tokens": ["Sonst", "ist", "es", "ring\u00b7sum", "stil\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Mainachtregen haucht gelind,", "tokens": ["Ein", "Mai\u00b7nach\u00b7tre\u00b7gen", "haucht", "ge\u00b7lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Licht ergl\u00e4nzt, kein Stern erwacht,", "tokens": ["Kein", "Licht", "er\u00b7gl\u00e4nzt", ",", "kein", "Stern", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur deine Locken k\u00fc\u00dft der Wind.", "tokens": ["Nur", "dei\u00b7ne", "Lo\u00b7cken", "k\u00fc\u00dft", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was blickst du einsam in die Nacht,", "tokens": ["Was", "blickst", "du", "ein\u00b7sam", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du armes, allverlassnes Kind?", "tokens": ["Du", "ar\u00b7mes", ",", "all\u00b7ver\u00b7lass\u00b7nes", "Kind", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein L\u00e4cheln hat einst mir gelacht \u2013", "tokens": ["Dein", "L\u00e4\u00b7cheln", "hat", "einst", "mir", "ge\u00b7lacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Licht ergl\u00e4nzt, kein Stern erwacht,", "tokens": ["Kein", "Licht", "er\u00b7gl\u00e4nzt", ",", "kein", "Stern", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur deine Locken k\u00fc\u00dft der Wind.", "tokens": ["Nur", "dei\u00b7ne", "Lo\u00b7cken", "k\u00fc\u00dft", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}