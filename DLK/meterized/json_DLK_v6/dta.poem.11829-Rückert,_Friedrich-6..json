{"dta.poem.11829": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wasser tr\u00e4gt den Mann, wenn er zu schwimmen wei\u00df;", "tokens": ["Das", "Was\u00b7ser", "tr\u00e4gt", "den", "Mann", ",", "wenn", "er", "zu", "schwim\u00b7men", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So n\u00e4hrt das Land ihn, wenn er brauchet seinen Flei\u00df.", "tokens": ["So", "n\u00e4hrt", "das", "Land", "ihn", ",", "wenn", "er", "brau\u00b7chet", "sei\u00b7nen", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer Bein' und Arme nicht lernt in die rechte Lage", "tokens": ["Wer", "Bein'", "und", "Ar\u00b7me", "nicht", "lernt", "in", "die", "rech\u00b7te", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "TRUNC", "KON", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu bringen, hoffe nicht da\u00df ihn das Wasser trage.", "tokens": ["Zu", "brin\u00b7gen", ",", "hof\u00b7fe", "nicht", "da\u00df", "ihn", "das", "Was\u00b7ser", "tra\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PTKNEG", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und also wer geschickt nicht reget Fu\u00df und Hand,", "tokens": ["Und", "al\u00b7so", "wer", "ge\u00b7schickt", "nicht", "re\u00b7get", "Fu\u00df", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "VVFIN", "PTKNEG", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreib' es sich selber zu, wenn ihn nicht tr\u00e4gt das Land.", "tokens": ["Schreib'", "es", "sich", "sel\u00b7ber", "zu", ",", "wenn", "ihn", "nicht", "tr\u00e4gt", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Gleichschwer von Leib sind zwei, der eine regt die Glieder", "tokens": ["Gleich\u00b7schwer", "von", "Leib", "sind", "zwei", ",", "der", "ei\u00b7ne", "regt", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "CARD", "$,", "PRELS", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schwimmt, der andre sinkt wie ein Bleiklumpen nieder.", "tokens": ["Und", "schwimmt", ",", "der", "and\u00b7re", "sinkt", "wie", "ein", "Blei\u00b7klum\u00b7pen", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}}, "stanza.5": {"line.1": {"text": "So sind auch zwei gleichstark, der eine braucht die Kraft", "tokens": ["So", "sind", "auch", "zwei", "gleichs\u00b7tark", ",", "der", "ei\u00b7ne", "braucht", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "CARD", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lebt, der andere lebt auch, doch kummerhaft.", "tokens": ["Und", "lebt", ",", "der", "an\u00b7de\u00b7re", "lebt", "auch", ",", "doch", "kum\u00b7mer\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}