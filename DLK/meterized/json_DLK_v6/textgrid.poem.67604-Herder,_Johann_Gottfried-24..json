{"textgrid.poem.67604": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerstreute hund- und hirtenlose Heerde,", "tokens": ["Zer\u00b7streu\u00b7te", "hun\u00b7d", "und", "hir\u00b7ten\u00b7lo\u00b7se", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weh Dir! da br\u00fcllt ein Leu.", "tokens": ["Weh", "Dir", "!", "da", "br\u00fcllt", "ein", "Leu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwo sind nun unsre Hirten?", "tokens": ["\u00bb", "wo", "sind", "nun", "uns\u00b7re", "Hir\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, wie wir uns verirrten!", "tokens": ["Ach", ",", "wie", "wir", "uns", "ver\u00b7irr\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der sel'ge Hund, er war so treu", "tokens": ["Der", "sel'\u00b7ge", "Hund", ",", "er", "war", "so", "treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stark dabei,", "tokens": ["Und", "stark", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und wir ergaben, dumme Heerde,", "tokens": ["Und", "wir", "er\u00b7ga\u00b7ben", ",", "dum\u00b7me", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Wolf ihn! Nun vorbei!", "tokens": ["Dem", "Wolf", "ihn", "!", "Nun", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "$.", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da kommt der Leu!\u00ab", "tokens": ["Da", "kommt", "der", "Leu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr Deutsche, wo ist Euer Hu\u00df", "tokens": ["Ihr", "Deut\u00b7sche", ",", "wo", "ist", "Eu\u00b7er", "Hu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Sickingen und Hutten blieben?", "tokens": ["Und", "Si\u00b7ckin\u00b7gen", "und", "Hut\u00b7ten", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sind aufgerieben!", "tokens": ["Sind", "auf\u00b7ge\u00b7rie\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der deutschen Freiheit Morgengru\u00df!", "tokens": ["Der", "deut\u00b7schen", "Frei\u00b7heit", "Mor\u00b7gen\u00b7gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwir wollen unsre Rednerslust,", "tokens": ["\u00bb", "wir", "wol\u00b7len", "uns\u00b7re", "Red\u00b7ners\u00b7lust", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Widder, an ihn senden;", "tokens": ["Den", "Wid\u00b7der", ",", "an", "ihn", "sen\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist von guten Lenden", "tokens": ["Er", "ist", "von", "gu\u00b7ten", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und breiter Brust", "tokens": ["Und", "brei\u00b7ter", "Brust"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und bellt nicht so und treibt nicht so,", "tokens": ["Und", "bellt", "nicht", "so", "und", "treibt", "nicht", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Hund und Hirte thaten.\u00ab", "tokens": ["Als", "Hund", "und", "Hir\u00b7te", "tha\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "De\u00df seid Ihr wohlberathen,", "tokens": ["De\u00df", "seid", "Ihr", "wohl\u00b7be\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "De\u00df werdet froh!", "tokens": ["De\u00df", "wer\u00b7det", "froh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der Wollen-Cicero", "tokens": ["Der", "Wol\u00b7len\u00b7Ci\u00b7ce\u00b7ro"], "token_info": ["word", "word"], "pos": ["ART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Trabt zitternd stolz entgegen", "tokens": ["Trabt", "zit\u00b7ternd", "stolz", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Der br\u00fcll'nden Majest\u00e4t.", "tokens": ["Der", "br\u00fcll'n\u00b7den", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "\u00bbda will ich recht, hilft's Gott, mein Exercitium", "tokens": ["\u00bb", "da", "will", "ich", "recht", ",", "hilft's", "Gott", ",", "mein", "Ex\u00b7er\u00b7ci\u00b7tium"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "$,", "VVFIN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Als Rednerpatriot ablegen", "tokens": ["Als", "Red\u00b7ner\u00b7pat\u00b7ri\u00b7ot", "ab\u00b7le\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "\u203aheil Dir, o K\u00f6nig! Welche Erde", "tokens": ["\u203a", "heil", "Dir", ",", "o", "K\u00f6\u00b7nig", "!", "Wel\u00b7che", "Er\u00b7de"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "PPER", "$,", "FM", "NN", "$.", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dich nur empf\u00e4ht,", "tokens": ["Dich", "nur", "em\u00b7pf\u00e4ht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Die weiht Dir Segen! \u2013 \u2013", "tokens": ["Die", "weiht", "Dir", "Se\u00b7gen", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$.", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Damit ich aber jetzt, o sanfte Majest\u00e4t,", "tokens": ["Da\u00b7mit", "ich", "a\u00b7ber", "jetzt", ",", "o", "sanf\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auf meine Heerde,", "tokens": ["Auf", "mei\u00b7ne", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Deretwegen", "tokens": ["De\u00b7ret\u00b7we\u00b7gen"], "token_info": ["word"], "pos": ["PDS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Ich denn \u2013\u2039\u00ab L\u00f6wens Angesicht", "tokens": ["Ich", "denn", "\u2013", "\u2039", "\u00ab", "L\u00f6\u00b7wens", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["PPER", "ADV", "$(", "$(", "$(", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Macht augenblicks ihn stumm.", "tokens": ["Macht", "au\u00b7gen\u00b7blicks", "ihn", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "\u00bbnun, Seine Heerde \u2013", "tokens": ["\u00bb", "nun", ",", "Sei\u00b7ne", "Heer\u00b7de", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Herr Demosthen, und weiter kann Er nicht", "tokens": ["Herr", "De\u00b7mos\u00b7then", ",", "und", "wei\u00b7ter", "kann", "Er", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "ADV", "VMFIN", "PPER", "PTKNEG"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Im Exercitium?", "tokens": ["Im", "Ex\u00b7er\u00b7ci\u00b7tium", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Heraus nur! Seine Heerde", "tokens": ["He\u00b7raus", "nur", "!", "Sei\u00b7ne", "Heer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Weiht Uns von ferne Segen", "tokens": ["Weiht", "Uns", "von", "fer\u00b7ne", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und will Uns nicht!", "tokens": ["Und", "will", "Uns", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Wir aber wollen ", "tokens": ["Wir", "a\u00b7ber", "wol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Und geben Uns die M\u00fch", "tokens": ["Und", "ge\u00b7ben", "Uns", "die", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Und kommen, Unsern Segen", "tokens": ["Und", "kom\u00b7men", ",", "Un\u00b7sern", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Geruhend H\u00f6chst Wir selbst ihr nah vors Angesicht", "tokens": ["Ge\u00b7ru\u00b7hend", "H\u00f6chst", "Wir", "selbst", "ihr", "nah", "vors", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In Gnaden vorzulegen.\u00ab", "tokens": ["In", "Gna\u00b7den", "vor\u00b7zu\u00b7le\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVIZU", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schulciceronen", "tokens": ["Schul\u00b7ci\u00b7ce\u00b7ro\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der neuen Rednerei", "tokens": ["Der", "neu\u00b7en", "Red\u00b7ne\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An Rectorats- und Galat\u00e4gen,", "tokens": ["An", "Rec\u00b7to\u00b7rats", "und", "Ga\u00b7la\u00b7t\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Redners Munde Heil und Segen,", "tokens": ["In", "Red\u00b7ners", "Mun\u00b7de", "Heil", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Musik dabei!", "tokens": ["Mu\u00b7sik", "da\u00b7bei", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und auf Kathederthronen,", "tokens": ["Und", "auf", "Ka\u00b7the\u00b7der\u00b7thro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ihr Ciceronen,", "tokens": ["Ihr", "Ci\u00b7ce\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Seid Ihr nicht frei???", "tokens": ["Seid", "Ihr", "nicht", "frei", "???"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbwend' Er Sich hinter Uns, Ambassadeur!\u00ab", "tokens": ["\u00bb", "wend'", "Er", "Sich", "hin\u00b7ter", "Uns", ",", "Am\u00b7bas\u00b7sa\u00b7deur", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "PRF", "APPR", "PPER", "$,", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und sieht ihn quer", "tokens": ["Und", "sieht", "ihn", "quer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erschrecklich an. Der Bock geht ohn' Beschwer", "tokens": ["Er\u00b7schreck\u00b7lich", "an", ".", "Der", "Bock", "geht", "ohn'", "Be\u00b7schwer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ART", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinter ihm her.", "tokens": ["Hin\u00b7ter", "ihm", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Leu schritt weiter hin.", "tokens": ["Der", "Leu", "schritt", "wei\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbsieh, wie sie fliehn!", "tokens": ["\u00bb", "sieh", ",", "wie", "sie", "fliehn", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Dazu nun, b\u00e4rt'ger Herr, verschoneten Wir Ihn.", "tokens": ["Da\u00b7zu", "nun", ",", "b\u00e4rt'\u00b7ger", "Herr", ",", "ver\u00b7scho\u00b7ne\u00b7ten", "Wir", "Ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geh Er, und sag Er: ", "tokens": ["Geh", "Er", ",", "und", "sag", "Er", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und f\u00fchr Er sie uns her!\u00ab", "tokens": ["Und", "f\u00fchr", "Er", "sie", "uns", "her", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "V