{"dta.poem.18510": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-thr\u00e4nen bey dem grabe Hn.  \n  D.  Valent. Alberti.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeihet mir/ ihr theuresten gebeine/", "tokens": ["Ver\u00b7zei\u00b7het", "mir", "/", "ihr", "theu\u00b7res\u00b7ten", "ge\u00b7bei\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Da\u00df ich euch zwar mit hundert ach beweine/", "tokens": ["Da\u00df", "ich", "euch", "zwar", "mit", "hun\u00b7dert", "ach", "be\u00b7wei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "CARD", "XY", "XY", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht aber itzt nach w\u00fcrden r\u00fchmen kan:", "tokens": ["Nicht", "a\u00b7ber", "itzt", "nach", "w\u00fcr\u00b7den", "r\u00fch\u00b7men", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "VAFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es kan mein hertz nicht euer thun erwegen/", "tokens": ["Es", "kan", "mein", "hertz", "nicht", "eu\u00b7er", "thun", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ohn neue pein und thr\u00e4nen zu erregen/", "tokens": ["Ohn", "neu\u00b7e", "pein", "und", "thr\u00e4\u00b7nen", "zu", "er\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und euer lob steht frembden besser an.", "tokens": ["Und", "eu\u00b7er", "lob", "steht", "fremb\u00b7den", "bes\u00b7ser", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich darff von nichts als von betr\u00fcbni\u00df sagen.", "tokens": ["Ich", "darff", "von", "nichts", "als", "von", "be\u00b7tr\u00fcb\u00b7ni\u00df", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "KOKOM", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein schwacher mund soll andern helffen klagen/", "tokens": ["Mein", "schwa\u00b7cher", "mund", "soll", "an\u00b7dern", "helf\u00b7fen", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der heute kaum zu trost kan offen stehn.", "tokens": ["Der", "heu\u00b7te", "kaum", "zu", "trost", "kan", "of\u00b7fen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kommt kirch und schul/ die dieser tod betr\u00fcbet/", "tokens": ["Kommt", "kirch", "und", "schul", "/", "die", "die\u00b7ser", "tod", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "ADJD", "$(", "ART", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Verla\u00dfnes hau\u00df/ und die er sonst geliebet/", "tokens": ["Ver\u00b7la\u00df\u00b7nes", "hau\u00df", "/", "und", "die", "er", "sonst", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Last euer weh durch meine lippen gehn.", "tokens": ["Last", "eu\u00b7er", "weh", "durch", "mei\u00b7ne", "lip\u00b7pen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die kirche klagt: es f\u00e4llt ein grosser Lehrer/", "tokens": ["Die", "kir\u00b7che", "klagt", ":", "es", "f\u00e4llt", "ein", "gros\u00b7ser", "Leh\u00b7rer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der warheit schild/ der gottesfurcht verehrer/", "tokens": ["Der", "war\u00b7heit", "schild", "/", "der", "got\u00b7tes\u00b7furcht", "ver\u00b7eh\u00b7rer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein pfeiler sinckt/ der von den st\u00e4rcksten war;", "tokens": ["Ein", "pfei\u00b7ler", "sinckt", "/", "der", "von", "den", "st\u00e4rcks\u00b7ten", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "APPR", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mein heiligthum hat einen bruch erlitten/", "tokens": ["Mein", "hei\u00b7lig\u00b7thum", "hat", "ei\u00b7nen", "bruch", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der f\u00fcr mein heyl mit mund und hand gestritten/", "tokens": ["Der", "f\u00fcr", "mein", "heyl", "mit", "mund", "und", "hand", "ge\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Liegt itzt erstarrt auff seiner toden-baar.", "tokens": ["Liegt", "itzt", "er\u00b7starrt", "auff", "sei\u00b7ner", "to\u00b7den\u00b7baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ach menschen thut/ ach thut noch heute busse/", "tokens": ["Ach", "men\u00b7schen", "thut", "/", "ach", "thut", "noch", "heu\u00b7te", "bus\u00b7se", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "$(", "ADV", "VVFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Des hauses wohl beruht auf schwachen fusse/", "tokens": ["Des", "hau\u00b7ses", "wohl", "be\u00b7ruht", "auf", "schwa\u00b7chen", "fus\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wenn sich der fall an seine st\u00fctzen macht.", "tokens": ["Wenn", "sich", "der", "fall", "an", "sei\u00b7ne", "st\u00fct\u00b7zen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wenn berge schon sich pflegen zu verstecken/", "tokens": ["Wenn", "ber\u00b7ge", "schon", "sich", "pfle\u00b7gen", "zu", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "PRF", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Will meist das land ein dunckler nebel decken:", "tokens": ["Will", "meist", "das", "land", "ein", "dunck\u00b7ler", "ne\u00b7bel", "de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Jtzt weicht ein berg/ nehmt euer licht in acht.", "tokens": ["Jtzt", "weicht", "ein", "berg", "/", "nehmt", "eu\u00b7er", "licht", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Der Musen-sitz an diesem werthen orte/", "tokens": ["Der", "Mu\u00b7sen\u00b7sitz", "an", "die\u00b7sem", "wert\u00b7hen", "or\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Zeigt auch sein leid durch klagen-volle worte/", "tokens": ["Zeigt", "auch", "sein", "leid", "durch", "kla\u00b7gen\u00b7vol\u00b7le", "wor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und spricht: dein witz hat mir so manches kind", "tokens": ["Und", "spricht", ":", "dein", "witz", "hat", "mir", "so", "man\u00b7ches", "kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und noch vielmehr von fremden unterrichtet.", "tokens": ["Und", "noch", "viel\u00b7mehr", "von", "frem\u00b7den", "un\u00b7ter\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Wie viel sind dir vor deinen flei\u00df verpflichtet/", "tokens": ["Wie", "viel", "sind", "dir", "vor", "dei\u00b7nen", "flei\u00df", "ver\u00b7pflich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die itzt geehrt und grosse leute sind!", "tokens": ["Die", "itzt", "ge\u00b7ehrt", "und", "gros\u00b7se", "leu\u00b7te", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Doch dieser fall/ der andre nur verletzet/", "tokens": ["Doch", "die\u00b7ser", "fall", "/", "der", "and\u00b7re", "nur", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$(", "ART", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Hat leider dir gantz t\u00f6dlich zugesetzet/", "tokens": ["Hat", "lei\u00b7der", "dir", "gantz", "t\u00f6d\u00b7lich", "zu\u00b7ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "O kl\u00e4gliches und jammerreiches hau\u00df!", "tokens": ["O", "kl\u00e4g\u00b7li\u00b7ches", "und", "jam\u00b7mer\u00b7rei\u00b7ches", "hau\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der thr\u00e4nen na\u00df/ das sonst viel treue sinnen", "tokens": ["Der", "thr\u00e4\u00b7nen", "na\u00df", "/", "das", "sonst", "viel", "treu\u00b7e", "sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$(", "PDS", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Umb ihr gesicht mit tropffen lassen rinnen/", "tokens": ["Umb", "ihr", "ge\u00b7sicht", "mit", "tropf\u00b7fen", "las\u00b7sen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVPP", "APPR", "VVINF", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Bricht ja bey dir mit vollen str\u00f6men aus.", "tokens": ["Bricht", "ja", "bey", "dir", "mit", "vol\u00b7len", "str\u00f6\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Wer dich besucht/ dem schallt in seinen ohren:", "tokens": ["Wer", "dich", "be\u00b7sucht", "/", "dem", "schallt", "in", "sei\u00b7nen", "oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "ART", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Es ist zu viel/ es ist zu fr\u00fch verlohren:", "tokens": ["Es", "ist", "zu", "viel", "/", "es", "ist", "zu", "fr\u00fch", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "PIS", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ein tr\u00fcber tag entf\u00fchrt ein solches haupt/", "tokens": ["Ein", "tr\u00fc\u00b7ber", "tag", "ent\u00b7f\u00fchrt", "ein", "sol\u00b7ches", "haupt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Das uns sonst nichs als freuden-tage machte/", "tokens": ["Das", "uns", "sonst", "nichs", "als", "freu\u00b7den\u00b7ta\u00b7ge", "mach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und noch viel jahr uns zu versorgen dachte/", "tokens": ["Und", "noch", "viel", "jahr", "uns", "zu", "ver\u00b7sor\u00b7gen", "dach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Wir sind ein schiff/ dem man den mast geraubt.", "tokens": ["Wir", "sind", "ein", "schiff", "/", "dem", "man", "den", "mast", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "$(", "ART", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und sihe doch bey deiner noth und schmertzen/", "tokens": ["Und", "si\u00b7he", "doch", "bey", "dei\u00b7ner", "noth", "und", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "APPR", "PPOSAT", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Verge\u00df ich fast was meinem eignen hertzen", "tokens": ["Ver\u00b7ge\u00df", "ich", "fast", "was", "mei\u00b7nem", "eig\u00b7nen", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Die gr\u00f6ste pein und marter angethan.", "tokens": ["Die", "gr\u00f6s\u00b7te", "pein", "und", "mar\u00b7ter", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Dein herber stand und deines creutzes flammen/", "tokens": ["Dein", "her\u00b7ber", "stand", "und", "dei\u00b7nes", "creut\u00b7zes", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos