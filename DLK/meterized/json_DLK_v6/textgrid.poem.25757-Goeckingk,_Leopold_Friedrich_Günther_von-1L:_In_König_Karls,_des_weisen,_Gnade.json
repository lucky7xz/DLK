 "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.95": {"text": "Den n\u00e4chsten Freitag gleich entscheiden,", "tokens": ["Den", "n\u00e4chs\u00b7ten", "Frei\u00b7tag", "gleich", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.96": {"text": "Und wenn Ihr schuldig seyd!\u00ab \u2013", "tokens": ["Und", "wenn", "Ihr", "schul\u00b7dig", "seyd", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.97": {"text": "Karl drohte mit den Augenbrauen", "tokens": ["Karl", "droh\u00b7te", "mit", "den", "Au\u00b7gen\u00b7brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.98": {"text": "Dem M\u00f6rder noch, und hie\u00df ihn gehn.", "tokens": ["Dem", "M\u00f6r\u00b7der", "noch", ",", "und", "hie\u00df", "ihn", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.99": {"text": "Die Insel unsrer lieben Frauen,", "tokens": ["Die", "In\u00b7sel", "uns\u00b7rer", "lie\u00b7ben", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.100": {"text": "Zum Kampfplatz' ausersehn,", "tokens": ["Zum", "Kampf\u00b7platz'", "au\u00b7ser\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line