{"textgrid.poem.25757": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In K\u00f6nig Karls, des weisen, Gnade", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In K\u00f6nig Karls, des weisen, Gnade", "tokens": ["In", "K\u00f6\u00b7nig", "Karls", ",", "des", "wei\u00b7sen", ",", "Gna\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "NE", "$,", "ART", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wuchs Aubry von Montdidier,", "tokens": ["Wuchs", "Aub\u00b7ry", "von", "Mont\u00b7di\u00b7dier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gleich einem Oelbaum' am Gestade", "tokens": ["Gleich", "ei\u00b7nem", "O\u00b7el\u00b7baum'", "am", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Marne, in die H\u00f6h'.", "tokens": ["Der", "Mar\u00b7ne", ",", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn er, kein Schmeichler und kein Zwitter", "tokens": ["Denn", "er", ",", "kein", "Schmeich\u00b7ler", "und", "kein", "Zwit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Schurk' und Biedermann,", "tokens": ["Von", "Schurk'", "und", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hing eifriger als alle Ritter", "tokens": ["Hing", "eif\u00b7ri\u00b7ger", "als", "al\u00b7le", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bei Hof, der Wahrheit an.", "tokens": ["Bei", "Hof", ",", "der", "Wahr\u00b7heit", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Schnell sah der Ritter von Macaire", "tokens": ["Schnell", "sah", "der", "Rit\u00b7ter", "von", "Ma\u00b7cai\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Im Glanz' des Throns den Liebling bl\u00fchn,", "tokens": ["Im", "Glanz'", "des", "Throns", "den", "Lieb\u00b7ling", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und er, der gern gewesen w\u00e4re,", "tokens": ["Und", "er", ",", "der", "gern", "ge\u00b7we\u00b7sen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Was, ohne sein Bem\u00fchn,", "tokens": ["Was", ",", "oh\u00b7ne", "sein", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Jetzt Aubry war, legt' diesem Schlingen,", "tokens": ["Jetzt", "Aub\u00b7ry", "war", ",", "legt'", "die\u00b7sem", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Fein, wie der Neid sie flicht,", "tokens": ["Fein", ",", "wie", "der", "Neid", "sie", "flicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Und grub ihm Gruben, doch gelingen", "tokens": ["Und", "grub", "ihm", "Gru\u00b7ben", ",", "doch", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wollt' alle List ihm nicht.", "tokens": ["Wollt'", "al\u00b7le", "List", "ihm", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Von einem Jagdhund' nur begleitet,", "tokens": ["Von", "ei\u00b7nem", "Jagd\u00b7hund'", "nur", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ging Aubry einstens in den Wald", "tokens": ["Ging", "Aub\u00b7ry", "eins\u00b7tens", "in", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Von Bondy. Siehe! pl\u00f6tzlich reitet", "tokens": ["Von", "Bon\u00b7dy", ".", "Sie\u00b7he", "!", "pl\u00f6tz\u00b7lich", "rei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$.", "VVIMP", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sein Feind daher. \u00bbHalt! Halt!", "tokens": ["Sein", "Feind", "da\u00b7her", ".", "\u00bb", "Halt", "!", "Halt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "$.", "$(", "VVIMP", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Du Memme!\u00ab rief er. Aubry kannte", "tokens": ["Du", "Mem\u00b7me", "!", "\u00ab", "rief", "er", ".", "Aub\u00b7ry", "kann\u00b7te"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$.", "$(", "VVFIN", "PPER", "$.", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Stimm', und hielt's f\u00fcr Scherz;", "tokens": ["Die", "Stimm'", ",", "und", "hielt's", "f\u00fcr", "Scherz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Doch jener zog sein Schwert und rannte", "tokens": ["Doch", "je\u00b7ner", "zog", "sein", "Schwert", "und", "rann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die Spitz' in Aubry's Herz.", "tokens": ["Die", "Spitz'", "in", "Au\u00b7bry's", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Noch warm verscharrt' er Aubry's Leiche,", "tokens": ["Noch", "warm", "ver\u00b7scharrt'", "er", "Au\u00b7bry's", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Bedeckte den blutrothen Ort", "tokens": ["Be\u00b7deck\u00b7te", "den", "blut\u00b7ro\u00b7then", "Ort"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Mit Erde, Rasen und Gestr\u00e4uche", "tokens": ["Mit", "Er\u00b7de", ",", "Ra\u00b7sen", "und", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Sorgf\u00e4ltig, und ritt fort.", "tokens": ["Sorg\u00b7f\u00e4l\u00b7tig", ",", "und", "ritt", "fort", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Der Hund, fast in den letzten Z\u00fcgen,", "tokens": ["Der", "Hund", ",", "fast", "in", "den", "letz\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Bedeckt mit Wunden, blieb", "tokens": ["Be\u00b7deckt", "mit", "Wun\u00b7den", ",", "blieb"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Auf Aubry's Grabe winselnd liegen,", "tokens": ["Auf", "Au\u00b7bry's", "Gra\u00b7be", "win\u00b7selnd", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Bis Hunger fort ihn trieb.", "tokens": ["Bis", "Hun\u00b7ger", "fort", "ihn", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Von Aubry's Sippschaft fast vergessen,", "tokens": ["Von", "Au\u00b7bry's", "Sipp\u00b7schaft", "fast", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Kam Herkul mager nach Paris;", "tokens": ["Kam", "Her\u00b7kul", "ma\u00b7ger", "nach", "Pa\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Sie gab ihm gern ihr eignes Essen,", "tokens": ["Sie", "gab", "ihm", "gern", "ihr", "eig\u00b7nes", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Doch, hastig schlingend, lie\u00df", "tokens": ["Doch", ",", "has\u00b7tig", "schlin\u00b7gend", ",", "lie\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADJD", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Er halb es stehn, und rannte wieder", "tokens": ["Er", "halb", "es", "stehn", ",", "und", "rann\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "PPER", "VVINF", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "In Bondy's finstern Hain,", "tokens": ["In", "Bon\u00b7dy's", "fins\u00b7tern", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Legt' auf der Gruft des Herrn sich nieder,", "tokens": ["Legt'", "auf", "der", "Gruft", "des", "Herrn", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und kratzt' ein Loch hinein.", "tokens": ["Und", "kratzt'", "ein", "Loch", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "So trieb er's lange Zeit. Man sp\u00fcrte", "tokens": ["So", "trieb", "er's", "lan\u00b7ge", "Zeit", ".", "Man", "sp\u00fcr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADJA", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Des Hundes F\u00e4hrte nach, und fand", "tokens": ["Des", "Hun\u00b7des", "F\u00e4hr\u00b7te", "nach", ",", "und", "fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Tief im Geh\u00f6lz', wohin sie f\u00fchrte,", "tokens": ["Tief", "im", "Ge\u00b7h\u00f6lz'", ",", "wo\u00b7hin", "sie", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.44": {"text": "Den Hund auf seinem Stand'.", "tokens": ["Den", "Hund", "auf", "sei\u00b7nem", "Stand'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Als man die Stelle voll Gestr\u00e4uche", "tokens": ["Als", "man", "die", "Stel\u00b7le", "voll", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "ADJD", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und frisch gegraben sah,", "tokens": ["Und", "frisch", "ge\u00b7gra\u00b7ben", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Grub man sie auf, und Aubry's Leiche", "tokens": ["Grub", "man", "sie", "auf", ",", "und", "Au\u00b7bry's", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "PTKVZ", "$,", "KON", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Lag halb verweset da.", "tokens": ["Lag", "halb", "ver\u00b7we\u00b7set", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Man fuhr sie nach Paris. Die Ohren", "tokens": ["Man", "fuhr", "sie", "nach", "Pa\u00b7ris", ".", "Die", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NE", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Gesenkt, lief Herkul nebenher.", "tokens": ["Ge\u00b7senkt", ",", "lief", "Her\u00b7kul", "ne\u00b7ben\u00b7her", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.51": {"text": "Schon alle Hoffnung war verloren,", "tokens": ["Schon", "al\u00b7le", "Hoff\u00b7nung", "war", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Je zu entdecken, wer", "tokens": ["Je", "zu", "ent\u00b7de\u00b7cken", ",", "wer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PWS"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.53": {"text": "Der M\u00f6rder sey? Da packt, voll Rache,", "tokens": ["Der", "M\u00f6r\u00b7der", "sey", "?", "Da", "packt", ",", "voll", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Einst Herkul seinen Mann", "tokens": ["Einst", "Her\u00b7kul", "sei\u00b7nen", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Im Kreis der Armbrustsch\u00fctzenwache", "tokens": ["Im", "Kreis", "der", "Arm\u00b7brust\u00b7sch\u00fct\u00b7zen\u00b7wa\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Des K\u00f6nigs, grimmig an.", "tokens": ["Des", "K\u00f6\u00b7nigs", ",", "grim\u00b7mig", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Was schlagen konnte, schlug den Treuen,", "tokens": ["Was", "schla\u00b7gen", "konn\u00b7te", ",", "schlug", "den", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Der seines Herren M\u00f6rder bi\u00df,", "tokens": ["Der", "sei\u00b7nes", "Her\u00b7ren", "M\u00f6r\u00b7der", "bi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Doch immer fa\u00dft' er ihn von neuen,", "tokens": ["Doch", "im\u00b7mer", "fa\u00dft'", "er", "ihn", "von", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Bis man hinweg ihn ri\u00df.", "tokens": ["Bis", "man", "hin\u00b7weg", "ihn", "ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "In allen H\u00e4usern, allen Gassen,", "tokens": ["In", "al\u00b7len", "H\u00e4u\u00b7sern", ",", "al\u00b7len", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Sucht' er den Feind nun auf,", "tokens": ["Sucht'", "er", "den", "Feind", "nun", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.63": {"text": "Und konnt' er ihn nach Wunsch nicht fassen,", "tokens": ["Und", "konnt'", "er", "ihn", "nach", "Wunsch", "nicht", "fas\u00b7sen", ","], "toke