{"dta.poem.5339": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fragen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hoffe, liebster Freund, du werdest auf mein Fragen", "tokens": ["Ich", "hof\u00b7fe", ",", "liebs\u00b7ter", "Freund", ",", "du", "wer\u00b7dest", "auf", "mein", "Fra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie du pflegst, mir deine Meinung sagen:", "tokens": ["So", "wie", "du", "pflegst", ",", "mir", "dei\u00b7ne", "Mei\u00b7nung", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nur ein Mensch aus andern, mehrern, auch min-", "tokens": ["Wenn", "nur", "ein", "Mensch", "aus", "an\u00b7dern", ",", "meh\u00b7rern", ",", "auch", "min"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "$,", "ADV", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ja wenn nur ausser ihm ein Umstand sich anders, als", "tokens": ["Ja", "wenn", "nur", "aus\u00b7ser", "ihm", "ein", "Um\u00b7stand", "sich", "an\u00b7ders", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "KOUS", "ADV", "APPR", "PPER", "ART", "NN", "PRF", "ADV", "$,", "KOUS"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ob er nicht gantz auf andre Weise, bald mehr, und auch", "tokens": ["Ob", "er", "nicht", "gantz", "auf", "and\u00b7re", "Wei\u00b7se", ",", "bald", "mehr", ",", "und", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "$,", "KON", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Gedencken, thun und reden w\u00fcrde, als er jetzt dencket,", "tokens": ["Ge\u00b7den\u00b7cken", ",", "thun", "und", "re\u00b7den", "w\u00fcr\u00b7de", ",", "als", "er", "jetzt", "den\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "KON", "VVINF", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re, wie gew\u00f6hnlich, an, was du mir jetzo vorgetragen", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "wie", "ge\u00b7w\u00f6hn\u00b7lich", ",", "an", ",", "was", "du", "mir", "jet\u00b7zo", "vor\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "$,", "PTKVZ", "$,", "PWS", "PPER", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und wirst du deine Antwort finden, da ich dich werde", "tokens": ["Und", "wirst", "du", "dei\u00b7ne", "Ant\u00b7wort", "fin\u00b7den", ",", "da", "ich", "dich", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "PPER", "PPER", "VAFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sprich, wenn in eines Menschen Hertzen, und seines", "tokens": ["Sprich", ",", "wenn", "in", "ei\u00b7nes", "Men\u00b7schen", "Hert\u00b7zen", ",", "und", "sei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,", "KON", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das allerseltsahmste Gemisch, so m\u00f6glich fast, sich fin-", "tokens": ["Das", "al\u00b7ler\u00b7selt\u00b7sahms\u00b7te", "Ge\u00b7misch", ",", "so", "m\u00f6g\u00b7lich", "fast", ",", "sich", "fin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ADV", "$,", "PRF", "TRUNC"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ob nicht ein solcher Mensch dennoch, wenn er sich ", "tokens": ["Ob", "nicht", "ein", "sol\u00b7cher", "Mensch", "den\u00b7noch", ",", "wenn", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "PIAT", "NN", "ADV", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Nicht wenigstens um ", "tokens": ["Nicht", "we\u00b7nigs\u00b7tens", "um"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}