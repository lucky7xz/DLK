{"textgrid.poem.37525": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Individualit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mal so, da\u00df ich so bin.", "tokens": ["Es", "ist", "mal", "so", ",", "da\u00df", "ich", "so", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df selber nicht warum.", "tokens": ["Wei\u00df", "sel\u00b7ber", "nicht", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier ist die Schenke. Ich bin drin", "tokens": ["Hier", "ist", "die", "Schen\u00b7ke", ".", "Ich", "bin", "drin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke mir: Dideldum!", "tokens": ["Und", "den\u00b7ke", "mir", ":", "Di\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df das so ist, das tut mir leid.", "tokens": ["Da\u00df", "das", "so", "ist", ",", "das", "tut", "mir", "leid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Individuum", "tokens": ["Mein", "In\u00b7di\u00b7vi\u00b7du\u00b7um"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat aber mal die Eigenheit,", "tokens": ["Hat", "a\u00b7ber", "mal", "die", "Ei\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum denk ich mir: Dideldum!", "tokens": ["Drum", "denk", "ich", "mir", ":", "Di\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$.", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und schaut die Jungfer Kellnerin", "tokens": ["Und", "schaut", "die", "Jung\u00b7fer", "Kell\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich auch nach mir nicht um;", "tokens": ["Sich", "auch", "nach", "mir", "nicht", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df ja doch, wie sch\u00f6n ich bin,", "tokens": ["Ich", "wei\u00df", "ja", "doch", ",", "wie", "sch\u00f6n", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke mir: Dideldum!", "tokens": ["Und", "den\u00b7ke", "mir", ":", "Di\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und s\u00e4\u00dfe einer da abseit", "tokens": ["Und", "s\u00e4\u00b7\u00dfe", "ei\u00b7ner", "da", "ab\u00b7seit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADV", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Knurren und Gebrumm", "tokens": ["Mit", "Knur\u00b7ren", "und", "Ge\u00b7brumm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meint, ich w\u00e4re nicht gescheit,", "tokens": ["Und", "meint", ",", "ich", "w\u00e4\u00b7re", "nicht", "ge\u00b7scheit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So denk ich mir: Dideldum!", "tokens": ["So", "denk", "ich", "mir", ":", "Di\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch kommt mir wer daher und spricht,", "tokens": ["Doch", "kommt", "mir", "wer", "da\u00b7her", "und", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWS", "PAV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00e4re gar nicht frumm", "tokens": ["Ich", "w\u00e4\u00b7re", "gar", "nicht", "frumm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00e4tte keine Tugend nicht,", "tokens": ["Und", "h\u00e4t\u00b7te", "kei\u00b7ne", "Tu\u00b7gend", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das nehm ich krumm. \u2013 Dideldum!", "tokens": ["Das", "nehm", "ich", "krumm", ".", "\u2013", "Di\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$.", "$(", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}