{"textgrid.poem.28561": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr einen guten Mann sind alle Zeiten gut,", "tokens": ["F\u00fcr", "ei\u00b7nen", "gu\u00b7ten", "Mann", "sind", "al\u00b7le", "Zei\u00b7ten", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil niemals b\u00f6ses er, und b\u00f6ses ihm nichts thut;", "tokens": ["Weil", "nie\u00b7mals", "b\u00f6\u00b7ses", "er", ",", "und", "b\u00f6\u00b7ses", "ihm", "nichts", "thut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "$,", "KON", "ADJA", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er f\u00fchrt durch beydes Gl\u00fcck nur immer einen Mut.", "tokens": ["Er", "f\u00fchrt", "durch", "bey\u00b7des", "Gl\u00fcck", "nur", "im\u00b7mer", "ei\u00b7nen", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}