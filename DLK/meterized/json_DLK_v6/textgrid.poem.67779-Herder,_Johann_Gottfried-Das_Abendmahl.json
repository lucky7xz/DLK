{"textgrid.poem.67779": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Abendmahl", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jener Nacht, in der Verrath", "tokens": ["In", "je\u00b7ner", "Nacht", ",", "in", "der", "Ver\u00b7rath"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Tod sich unserm Herren naht',", "tokens": ["Und", "Tod", "sich", "un\u00b7serm", "Her\u00b7ren", "naht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nahm Christus, brach ein d\u00fcrres Brod", "tokens": ["Nahm", "Chris\u00b7tus", ",", "brach", "ein", "d\u00fcr\u00b7res", "Brod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: \u00bbSo geh' ich in den Tod!", "tokens": ["Und", "sprach", ":", "\u00bb", "So", "geh'", "ich", "in", "den", "Tod", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zerbrochen wird das Weizenkorn,", "tokens": ["Zer\u00b7bro\u00b7chen", "wird", "das", "Wei\u00b7zen\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leib; doch ist es nicht verlor'n;", "tokens": ["Mein", "Leib", ";", "doch", "ist", "es", "nicht", "ver\u00b7lor'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sprie\u00dft empor; nehmt diesen Trank,", "tokens": ["Es", "sprie\u00dft", "em\u00b7por", ";", "nehmt", "die\u00b7sen", "Trank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kelch zu neuem Lobgesang!", "tokens": ["Den", "Kelch", "zu", "neu\u00b7em", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie des unschuld'gen Lammes Blut,", "tokens": ["Wie", "des", "un\u00b7schuld'\u00b7gen", "Lam\u00b7mes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gew\u00e4hrt mein Tod ein neues Gut,", "tokens": ["Ge\u00b7w\u00e4hrt", "mein", "Tod", "ein", "neu\u00b7es", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Befreiung von der Sklaverei;", "tokens": ["Be\u00b7frei\u00b7ung", "von", "der", "Skla\u00b7ve\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dankt und genie\u00dft! Ihr werdet frei.", "tokens": ["Dankt", "und", "ge\u00b7nie\u00dft", "!", "Ihr", "wer\u00b7det", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar werd' ich in der Sterblichkeit", "tokens": ["Zwar", "werd'", "ich", "in", "der", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mit genie\u00dfen diese Zeit,", "tokens": ["Nicht", "mit", "ge\u00b7nie\u00b7\u00dfen", "die\u00b7se", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein sie kommt; dann denket mein!", "tokens": ["Al\u00b7lein", "sie", "kommt", ";", "dann", "den\u00b7ket", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist wird immer bei Euch sein.", "tokens": ["Mein", "Geist", "wird", "im\u00b7mer", "bei", "Euch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Reiche Gottes sehen wir", "tokens": ["Im", "Rei\u00b7che", "Got\u00b7tes", "se\u00b7hen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns wieder; Ihr hienieden, Ihr,", "tokens": ["Uns", "wie\u00b7der", ";", "Ihr", "hien\u00b7ie\u00b7den", ",", "Ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$.", "PPER", "ADV", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So oft Ihr dieses Mahl genie\u00dft,", "tokens": ["So", "oft", "Ihr", "die\u00b7ses", "Mahl", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denkt meiner, der stets um Euch ist!\u00ab", "tokens": ["Denkt", "mei\u00b7ner", ",", "der", "stets", "um", "Euch", "ist", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "$,", "PRELS", "ADV", "APPR", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir denken Dein, o Menschenfreund,", "tokens": ["Wir", "den\u00b7ken", "Dein", ",", "o", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich mit uns so ganz vereint,", "tokens": ["Der", "sich", "mit", "uns", "so", "ganz", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "De\u00df Sache unsre Sache war,", "tokens": ["De\u00df", "Sa\u00b7che", "uns\u00b7re", "Sa\u00b7che", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sterbend Leben uns gebar.", "tokens": ["Der", "ster\u00b7bend", "Le\u00b7ben", "uns", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir denken Dein, gro\u00dfm\u00fcthig Lamm,", "tokens": ["Wir", "den\u00b7ken", "Dein", ",", "gro\u00df\u00b7m\u00fct\u00b7hig", "Lamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hohn und Undank auf sich nahm,", "tokens": ["Das", "Hohn", "und", "Un\u00b7dank", "auf", "sich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schmerz und Schmach und Tod vergi\u00dft,", "tokens": ["Der", "Schmerz", "und", "Schmach", "und", "Tod", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil er der Menschen Retter ist.", "tokens": ["Weil", "er", "der", "Men\u00b7schen", "Ret\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wir denken Dein, der hier und dort", "tokens": ["Wir", "den\u00b7ken", "Dein", ",", "der", "hier", "und", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "PRELS", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit uns will eins sein fort und fort,", "tokens": ["Mit", "uns", "will", "eins", "sein", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "VAINF", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Matten, den Gefallnen hebt", "tokens": ["Den", "Mat\u00b7ten", ",", "den", "Ge\u00b7fall\u00b7nen", "hebt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Seinen Herzen lebt.", "tokens": ["Und", "in", "der", "Sei\u00b7nen", "Her\u00b7zen", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir treten ein in Deinen Bund;", "tokens": ["Wir", "tre\u00b7ten", "ein", "in", "Dei\u00b7nen", "Bund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch sterbend machtest Du ihn kund:", "tokens": ["Noch", "ster\u00b7bend", "mach\u00b7test", "Du", "ihn", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Befreiung war Dein edler Zweck,", "tokens": ["Be\u00b7frei\u00b7ung", "war", "Dein", "ed\u00b7ler", "Zweck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufopferung zu ihm der Weg.", "tokens": ["Auf\u00b7op\u00b7fe\u00b7rung", "zu", "ihm", "der", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer nur sich selbst, nicht Andern lebt,", "tokens": ["Wer", "nur", "sich", "selbst", ",", "nicht", "An\u00b7dern", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "ADV", "$,", "PTKNEG", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schlamm der L\u00fcste sich begr\u00e4bt", "tokens": ["Im", "Schlamm", "der", "L\u00fcs\u00b7te", "sich", "be\u00b7gr\u00e4bt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Freund ist alter Heuchelei,", "tokens": ["Und", "Freund", "ist", "al\u00b7ter", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist nicht diesem Bunde treu.", "tokens": ["Der", "ist", "nicht", "die\u00b7sem", "Bun\u00b7de", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wem Wahrheit \u00fcber Alles gilt,", "tokens": ["Wem", "Wahr\u00b7heit", "\u00fc\u00b7ber", "Al\u00b7les", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem Menschenheil die Seele f\u00fcllt,", "tokens": ["Wem", "Men\u00b7schen\u00b7heil", "die", "See\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Andre wirkend sich vergi\u00dft,", "tokens": ["F\u00fcr", "And\u00b7re", "wir\u00b7kend", "sich", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist nach Deinem Sinn ein Christ.", "tokens": ["Der", "ist", "nach", "Dei\u00b7nem", "Sinn", "ein", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}