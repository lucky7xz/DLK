-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eilt er von seiner Herrscherin", "tokens": ["Eilt", "er", "von", "sei\u00b7ner", "Herr\u00b7sche\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Augenblick zur Hofstatt hin,", "tokens": ["Den", "Au\u00b7gen\u00b7blick", "zur", "Hof\u00b7statt", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie aber auch den Augenblick", "tokens": ["Sie", "a\u00b7ber", "auch", "den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In ihres Cleons Arm zur\u00fcck,", "tokens": ["In", "ih\u00b7res", "Cleons", "Arm", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Der damals, als Leander kam,", "tokens": ["Der", "da\u00b7mals", ",", "als", "Le\u00b7an\u00b7der", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zum Winkel seine Zuflucht nahm.", "tokens": ["Zum", "Win\u00b7kel", "sei\u00b7ne", "Zu\u00b7flucht", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O sch\u00f6nes Beispiel gleicher Triebe!", "tokens": ["O", "sch\u00f6\u00b7nes", "Bei\u00b7spiel", "glei\u00b7cher", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O wahres Muster heut'ger Liebe!", "tokens": ["O", "wah\u00b7res", "Mus\u00b7ter", "heut'\u00b7ger", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}