{"textgrid.poem.41408": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Gegenliebe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom schweren Dienst der Eitelkeit,", "tokens": ["Vom", "schwe\u00b7ren", "Dienst", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von theuren Freunden voller Neid,", "tokens": ["Von", "theu\u00b7ren", "Freun\u00b7den", "vol\u00b7ler", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Henkern unsrer Lebenszeit,", "tokens": ["Den", "Hen\u00b7kern", "uns\u00b7rer", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eil' ich den Freuden und der Ruh'", "tokens": ["Eil'", "ich", "den", "Freu\u00b7den", "und", "der", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An deinem vollen Busen zu.", "tokens": ["An", "dei\u00b7nem", "vol\u00b7len", "Bu\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df jetzt mein Herz von dir erlernen,", "tokens": ["La\u00df", "jetzt", "mein", "Herz", "von", "dir", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sorgen scherzend zu entfernen.", "tokens": ["Die", "Sor\u00b7gen", "scher\u00b7zend", "zu", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum ird'schen Himmel w\u00fcnscht' es sich", "tokens": ["Zum", "ird'\u00b7schen", "Him\u00b7mel", "w\u00fcnscht'", "es", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur dies dein Schlafgemach, und dich.", "tokens": ["Nur", "dies", "dein", "Schlaf\u00b7ge\u00b7mach", ",", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDS", "PPOSAT", "NN", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Gott der Liebe schlie\u00df' uns ein;", "tokens": ["Der", "Gott", "der", "Lie\u00b7be", "schlie\u00df'", "uns", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sonst komme niemand! er allein", "tokens": ["Sonst", "kom\u00b7me", "nie\u00b7mand", "!", "er", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$.", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Soll Pf\u00f6rtner, Zeug' und H\u00fcter sein.", "tokens": ["Soll", "Pf\u00f6rt\u00b7ner", ",", "Zeug'", "und", "H\u00fc\u00b7ter", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich seh' den unzufried'nen Haufen", "tokens": ["Ich", "seh'", "den", "un\u00b7zu\u00b7frie\u00b7d'\u00b7nen", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach H\u00f6fen und Pal\u00e4sten laufen,", "tokens": ["Nach", "H\u00f6\u00b7fen", "und", "Pa\u00b7l\u00e4s\u00b7ten", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Gold und Schmelz und helle Pracht", "tokens": ["Wo", "Gold", "und", "Schmelz", "und", "hel\u00b7le", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefahr und Knechtschaft schimmernd macht.", "tokens": ["Ge\u00b7fahr", "und", "Knecht\u00b7schaft", "schim\u00b7mernd", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch will auch ich von deinen Knieen", "tokens": ["Doch", "will", "auch", "ich", "von", "dei\u00b7nen", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zu solchem Sitz der Ehrsucht fliehen,", "tokens": ["Zu", "sol\u00b7chem", "Sitz", "der", "Ehr\u00b7sucht", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnsch' ich mir ein h\u00f6her Gl\u00fcck,", "tokens": ["Und", "w\u00fcn\u00b7sch'", "ich", "mir", "ein", "h\u00f6\u00b7her", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Als dieses L\u00e4cheln, diesen Blick,", "tokens": ["Als", "die\u00b7ses", "L\u00e4\u00b7cheln", ",", "die\u00b7sen", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So folge Qual und Ungemach", "tokens": ["So", "fol\u00b7ge", "Qual", "und", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Meineid zur Bestrafung nach;", "tokens": ["Dem", "Mei\u00b7neid", "zur", "Be\u00b7stra\u00b7fung", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und, da\u00df der Fluch vollkommen sei,", "tokens": ["Und", ",", "da\u00df", "der", "Fluch", "voll\u00b7kom\u00b7men", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Seh' ich mich gro\u00df, dich ungetreu!", "tokens": ["Seh'", "ich", "mich", "gro\u00df", ",", "dich", "un\u00b7ge\u00b7treu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So zeigt, mit Schw\u00fcren und mit K\u00fcssen", "tokens": ["So", "zeigt", ",", "mit", "Schw\u00fc\u00b7ren", "und", "mit", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leander, wie man heftig liebt,", "tokens": ["Le\u00b7an\u00b7der", ",", "wie", "man", "hef\u00b7tig", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem, als bezaubert hingerissen,", "tokens": ["Dem", ",", "als", "be\u00b7zau\u00b7bert", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00f6ne dies zur Antwort gibt:", "tokens": ["Die", "Sch\u00f6\u00b7ne", "dies", "zur", "Ant\u00b7wort", "gibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was kann mich auf der Welt betr\u00fcben.", "tokens": ["Was", "kann", "mich", "auf", "der", "Welt", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Willst du, mein Schatz, mich ewig lieben?", "tokens": ["Willst", "du", ",", "mein", "Schatz", ",", "mich", "e\u00b7wig", "lie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du, dessen Huld mich stolz gemacht,", "tokens": ["Du", ",", "des\u00b7sen", "Huld", "mich", "stolz", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Wunsch bei Tag, und Traum bei Nacht", "tokens": ["Mein", "Wunsch", "bei", "Tag", ",", "und", "Traum", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O w\u00fcrde, wie ich dir geneigt,", "tokens": ["O", "w\u00fcr\u00b7de", ",", "wie", "ich", "dir", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch mehr, als Weibermuth, bezeugt!", "tokens": ["Durch", "mehr", ",", "als", "Wei\u00b7ber\u00b7muth", ",", "be\u00b7zeugt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich schrecket nichts, denn, dir zu gut,", "tokens": ["Mich", "schre\u00b7cket", "nichts", ",", "denn", ",", "dir", "zu", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KON", "$,", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vergie\u00dft Elmira gern ihr Blut,", "tokens": ["Ver\u00b7gie\u00dft", "El\u00b7mi\u00b7ra", "gern", "ihr", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn ihre Grabschrift nur erz\u00e4hlt,", "tokens": ["Wenn", "ih\u00b7re", "Grab\u00b7schrift", "nur", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df sie den Tod f\u00fcr dich erw\u00e4hlt.", "tokens": ["Da\u00df", "sie", "den", "Tod", "f\u00fcr", "dich", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hofft meine Sehnsucht nicht vergebens,", "tokens": ["Hofft", "mei\u00b7ne", "Sehn\u00b7sucht", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Trost und Kleinod meines Lebens,", "tokens": ["Du", "Trost", "und", "Klei\u00b7nod", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So trennt den Bund der Z\u00e4rtlichkeit", "tokens": ["So", "trennt", "den", "Bund", "der", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein steigend Gl\u00fcck, kein st\u00fcrzend Leid.", "tokens": ["Kein", "stei\u00b7gend", "Gl\u00fcck", ",", "kein", "st\u00fcr\u00b7zend", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sollten Sch\u00e4tze, Reich' und Kronen", "tokens": ["Und", "soll\u00b7ten", "Sch\u00e4t\u00b7ze", ",", "Reich'", "und", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wechsel tausendfach belohnen;", "tokens": ["Den", "Wech\u00b7sel", "tau\u00b7send\u00b7fach", "be\u00b7loh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So hei\u00df' ich, aus getreuem Sinn,", "tokens": ["So", "hei\u00df'", "ich", ",", "aus", "ge\u00b7treu\u00b7em", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit lieber deine Buhlerin,", "tokens": ["Weit", "lie\u00b7ber", "dei\u00b7ne", "Buh\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als eine gro\u00dfe K\u00f6nigin.", "tokens": ["Als", "ei\u00b7ne", "gro\u00b7\u00dfe", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie viel ist mir an dir verliehn!", "tokens": ["Wie", "viel", "ist", "mir", "an", "dir", "ver\u00b7liehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wird mein Verlangen nicht zu k\u00fchn,", "tokens": ["Wird", "mein", "Ver\u00b7lan\u00b7gen", "nicht", "zu", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So m\u00fcssen sich noch unsre Schatten,", "tokens": ["So", "m\u00fcs\u00b7sen", "sich", "noch", "uns\u00b7re", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit wiederholter Eintracht, gatten.", "tokens": ["Mit", "wie\u00b7der\u00b7hol\u00b7ter", "Ein\u00b7tracht", ",", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr G\u00f6tter! scheint's euch selbst nicht sch\u00f6n,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "scheint's", "euch", "selbst", "nicht", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwo Seelen so vereint zu sehn?", "tokens": ["Zwo", "See\u00b7len", "so", "ver\u00b7eint", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"lin