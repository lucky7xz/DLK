{"textgrid.poem.57389": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter Blumen, im Dufte des r\u00f6thlichen Abends, in frohes", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter Blumen, im Dufte des r\u00f6thlichen Abends, in frohes", "tokens": ["Un\u00b7ter", "Blu\u00b7men", ",", "im", "Duf\u00b7te", "des", "r\u00f6th\u00b7li\u00b7chen", "A\u00b7bends", ",", "in", "fro\u00b7hes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Lebens Genuss,", "tokens": ["Le\u00b7bens", "Ge\u00b7nuss", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das, mit gl\u00fccklicher T\u00e4uschung, zu jugendlichem sich dichtet,", "tokens": ["Das", ",", "mit", "gl\u00fcck\u00b7li\u00b7cher", "T\u00e4u\u00b7schung", ",", "zu", "ju\u00b7gend\u00b7li\u00b7chem", "sich", "dich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "PRF", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ruh' ich, und denke den Tod.", "tokens": ["Ruh'", "ich", ",", "und", "den\u00b7ke", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wer schon \u00f6fter als siebzigmal die Lenze verbl\u00fchn, sich", "tokens": ["Wer", "schon", "\u00f6f\u00b7ter", "als", "sieb\u00b7zig\u00b7mal", "die", "Len\u00b7ze", "ver\u00b7bl\u00fchn", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "VVINF", "$,", "PRF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Immer einsamer sah,", "tokens": ["Im\u00b7mer", "ein\u00b7sa\u00b7mer", "sah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Solle der Vergesser des Todes seyn, des Geleiters", "tokens": ["Sol\u00b7le", "der", "Ver\u00b7ges\u00b7ser", "des", "To\u00b7des", "seyn", ",", "des", "Ge\u00b7lei\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$,", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "In die sch\u00f6nere Welt?", "tokens": ["In", "die", "sch\u00f6\u00b7ne\u00b7re", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "W\u00fcnschet' ich mir den Beginn zu erleben des neuen Jahrhunderts;", "tokens": ["W\u00fcn\u00b7schet'", "ich", "mir", "den", "Be\u00b7ginn", "zu", "er\u00b7le\u00b7ben", "des", "neu\u00b7en", "Jahr\u00b7hun\u00b7derts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "W\u00e4re der Wunsch nicht ein Thor?", "tokens": ["W\u00e4\u00b7re", "der", "Wunsch", "nicht", "ein", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Denn oft s\u00e4umet, zwischen dem Tod' und dem Leben, ein Schlummer.", "tokens": ["Denn", "oft", "s\u00e4u\u00b7met", ",", "zwi\u00b7schen", "dem", "Tod'", "und", "dem", "Le\u00b7ben", ",", "ein", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Leben; ist nicht Leben, nicht Tod!", "tokens": ["Le\u00b7ben", ";", "ist", "nicht", "Le\u00b7ben", ",", "nicht", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Und wie w\u00fcrde das mich bew\u00f6lken, der immer sich jedem", "tokens": ["Und", "wie", "w\u00fcr\u00b7de", "das", "mich", "be\u00b7w\u00f6l\u00b7ken", ",", "der", "im\u00b7mer", "sich", "je\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "PDS", "PRF", "VVINF", "$,", "PRELS", "ADV", "PRF", "PIAT"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Schlummer entriss.", "tokens": ["Schlum\u00b7mer", "ent\u00b7riss", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Trennung von den Geliebten, o k\u00f6nt' ich deiner vergessen;", "tokens": ["Tren\u00b7nung", "von", "den", "Ge\u00b7lieb\u00b7ten", ",", "o", "k\u00f6nt'", "ich", "dei\u00b7ner", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "FM", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "So verg\u00e4ss' ich des Todes mit dir.", "tokens": ["So", "ver\u00b7g\u00e4ss'", "ich", "des", "To\u00b7des", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Doch nichts schreckliches hat der Gestorbne. Nicht den verwesten", "tokens": ["Doch", "nichts", "schreck\u00b7li\u00b7ches", "hat", "der", "Ge\u00b7storb\u00b7ne", ".", "Nicht", "den", "ver\u00b7wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "ADJA", "VAFIN", "ART", "NN", "$.", "PTKNEG", "ART", "ADJA"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Sehen wir, sehn nicht Gebein;", "tokens": ["Se\u00b7hen", "wir", ",", "sehn", "nicht", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Stumme Gestalt nur erblicken wir, bleiche. Ist denn des Mayes", "tokens": ["Stum\u00b7me", "Ge\u00b7stalt", "nur", "er\u00b7bli\u00b7cken", "wir", ",", "blei\u00b7che", ".", "Ist", "denn", "des", "Ma\u00b7yes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "$.", "VAFIN", "ADV", "ART", "NN"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.20": {"text": "Blume nicht auch, und die Lilie weiss?", "tokens": ["Blu\u00b7me", "nicht", "auch", ",", "und", "die", "Li\u00b7lie", "weiss", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Und entfloh nicht die Seele des blumen\u00e4hnlichen Todten", "tokens": ["Und", "ent\u00b7floh", "nicht", "die", "See\u00b7le", "des", "blu\u00b7men\u00b7\u00e4hn\u00b7li\u00b7chen", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "In die Gefilde des Lichts,", "tokens": ["In", "die", "Ge\u00b7fil\u00b7de", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Zu den Bewohnern des Abendsterns, der Winzerin, Maja's,", "tokens": ["Zu", "den", "Be\u00b7woh\u00b7nern", "des", "A\u00b7bends\u00b7terns", ",", "der", "Win\u00b7ze\u00b7rin", ",", "Ma\u00b7ja's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "NE", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.24": {"text": "Oder Apollo's empor,", "tokens": ["O\u00b7der", "A\u00b7pol\u00b7lo's", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Zu des Arktur, Zynosura's, des S\u00edrius, oder der \u00c4hre,", "tokens": ["Zu", "des", "Ark\u00b7tur", ",", "Zynos\u00b7ura's", ",", "des", "S\u00edri\u00b7us", ",", "o\u00b7der", "der", "\u00c4h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "$,", "ART", "NE", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Asteropens, Zeleno's empor?", "tokens": ["As\u00b7te\u00b7ro\u00b7pens", ",", "Ze\u00b7le\u00b7no's", "em\u00b7por", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Oder vielleicht zu jenes Kometen! der flammend vor Eile,", "tokens": ["O\u00b7der", "viel\u00b7leicht", "zu", "je\u00b7nes", "Ko\u00b7me\u00b7ten", "!", "der", "flam\u00b7mend", "vor", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "$.", "ART", "ADJD", "APPR", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Einst um die Sonne sich schwang,", "tokens": ["Einst", "um", "die", "Son\u00b7ne", "sich", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Welche der sch\u00f6neren, die der Erde strahlet, ihn sandte", "tokens": ["Wel\u00b7che", "der", "sch\u00f6\u00b7ne\u00b7ren", ",", "die", "der", "Er\u00b7de", "strah\u00b7let", ",", "ihn", "sand\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "ADJA", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Auf der unendlichen Bahn.", "tokens": ["Auf", "der", "un\u00b7end\u00b7li\u00b7chen", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Gl\u00e4nzender flog der Komet, und beynah der sendenden Sonne", "tokens": ["Gl\u00e4n\u00b7zen\u00b7der", "flog", "der", "Ko\u00b7met", ",", "und", "bey\u00b7nah", "der", "sen\u00b7den\u00b7den", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.32": {"text": "Unaufhaltbar, so schnell", "tokens": ["Un\u00b7auf\u00b7halt\u00b7bar", ",", "so", "schnell"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Schwang der liebende sich. Er liebt die Erde Wie freut er,", "tokens": ["Schwang", "der", "lie\u00b7ben\u00b7de", "sich", ".", "Er", "liebt", "die", "Er\u00b7de", "Wie", "freut", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PRF", "$.", "PPER", "VVFIN", "ART", "NN", "PWAV", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Als er endlich n\u00e4her ihr schwebt,", "tokens": ["Als", "er", "end\u00b7lich", "n\u00e4\u00b7her", "ihr", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Da sich des Wiedersehns! Zu der Erde schallt ihm die Stimme", "tokens": ["Da", "sich", "des", "Wie\u00b7der\u00b7sehns", "!", "Zu", "der", "Er\u00b7de", "schallt", "ihm", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Aus den jungen Hainen hinab,", "tokens": ["Aus", "den", "jun\u00b7gen", "Hai\u00b7nen", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Aus den Thalen der H\u00fcgel, der Berge nicht; und die Winde", "tokens": ["Aus", "den", "Tha\u00b7len", "der", "H\u00fc\u00b7gel", ",", "der", "Ber\u00b7ge", "nicht", ";", "und", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "$.", "KON", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.38": {"text": "Heisst er mit leiserem Fittige wehn:", "tokens": ["Heisst", "er", "mit", "lei\u00b7se\u00b7rem", "Fit\u00b7ti\u00b7ge", "wehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.39": {"text": "Alle St\u00fcrme sind ihm verstumt, und am ehernen Ufer", "tokens": ["Al\u00b7le", "St\u00fcr\u00b7me", "sind", "ihm", "ver\u00b7stumt", ",", "und", "am", "e\u00b7her\u00b7nen", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Schweigt das geebnete Meer.", "tokens": ["Schweigt", "das", "geeb\u00b7ne\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}