{"textgrid.poem.61068": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des M\u00fcden Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verglommen ist das Abendrot,", "tokens": ["Ver\u00b7glom\u00b7men", "ist", "das", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da t\u00f6nt ein fernes Klingen;", "tokens": ["Da", "t\u00f6nt", "ein", "fer\u00b7nes", "Klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich glaube fast, das ist der Tod,", "tokens": ["Ich", "glau\u00b7be", "fast", ",", "das", "ist", "der", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der will in Schlaf mich singen.", "tokens": ["Der", "will", "in", "Schlaf", "mich", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O singe nur zu,", "tokens": ["O", "sin\u00b7ge", "nur", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.6": {"text": "Du Spielmann du!", "tokens": ["Du", "Spiel\u00b7mann", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du sollst mir Frieden bringen.", "tokens": ["Du", "sollst", "mir", "Frie\u00b7den", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein weiches Bette der Rasen gibt,", "tokens": ["Ein", "wei\u00b7ches", "Bet\u00b7te", "der", "Ra\u00b7sen", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es s\u00e4useln so k\u00fchl die Zypressen,", "tokens": ["Es", "s\u00e4u\u00b7seln", "so", "k\u00fchl", "die", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und was ich gelebt, und was ich geliebt,", "tokens": ["Und", "was", "ich", "ge\u00b7lebt", ",", "und", "was", "ich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich will es alles vergessen.", "tokens": ["Ich", "will", "es", "al\u00b7les", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Keinen Ruhm, kein Gl\u00fcck", "tokens": ["Kei\u00b7nen", "Ruhm", ",", "kein", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "La\u00df ich zur\u00fcck,", "tokens": ["La\u00df", "ich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hab' nichts als Schmerzen besessen.", "tokens": ["Hab'", "nichts", "als", "Schmer\u00b7zen", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So fahr denn wohl, du arge Welt", "tokens": ["So", "fahr", "denn", "wohl", ",", "du", "ar\u00b7ge", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen bunten Sch\u00e4umen!", "tokens": ["Mit", "dei\u00b7nen", "bun\u00b7ten", "Sch\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was dich erg\u00f6tzt, was dir gef\u00e4llt,", "tokens": ["Was", "dich", "er\u00b7g\u00f6tzt", ",", "was", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gern will ich's vers\u00e4umen!", "tokens": ["Wie", "gern", "will", "ich's", "ver\u00b7s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon wehet die Nacht", "tokens": ["Schon", "we\u00b7het", "die", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mich an so sacht;", "tokens": ["Mich", "an", "so", "sacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nun la\u00dft mich ruhn und tr\u00e4umen.", "tokens": ["Nun", "la\u00dft", "mich", "ruhn", "und", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}