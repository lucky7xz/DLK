{"dta.poem.18176": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Beantwortete Frage: Wo gut  \n zu wohnen sei?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir leben in der Welt, im Land", "tokens": ["Wir", "le\u00b7ben", "in", "der", "Welt", ",", "im", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Eitelkeit,", "tokens": ["der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Darin die Menschen sind, bald", "tokens": ["Da\u00b7rin", "die", "Men\u00b7schen", "sind", ",", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "ART", "NN", "VAFIN", "$,", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "hie, bald da, zerstreut:", "tokens": ["hie", ",", "bald", "da", ",", "zer\u00b7streut", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "$,", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der runde Erdenball ist weit", "tokens": ["Der", "run\u00b7de", "Er\u00b7den\u00b7ball", "ist", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und breit zertheilet,", "tokens": ["und", "breit", "zer\u00b7thei\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da sich ein ieder Mensch in sei-", "tokens": ["Da", "sich", "ein", "ie\u00b7der", "Mensch", "in", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "PIAT", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "nen Punct verweilet.", "tokens": ["nen", "Punct", "ver\u00b7wei\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Der eine wohnet gern in ienen Morgenland;", "tokens": ["Der", "ei\u00b7ne", "woh\u00b7net", "gern", "in", "ie\u00b7nen", "Mor\u00b7gen\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der andre klaget da, da\u00df er im heissen Sand", "tokens": ["Der", "and\u00b7re", "kla\u00b7get", "da", ",", "da\u00df", "er", "im", "heis\u00b7sen", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den stets beschwei\u00dften Fu\u00df, in diesen Weltstrich", "tokens": ["Den", "stets", "be\u00b7schwei\u00df\u00b7ten", "Fu\u00df", ",", "in", "die\u00b7sen", "Welt\u00b7strich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "brennte,", "tokens": ["brenn\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Wenn er Arabiens entflammten Sand durch-", "tokens": ["Wenn", "er", "A\u00b7ra\u00b7biens", "ent\u00b7flamm\u00b7ten", "Sand", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "rennte.", "tokens": ["renn\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Der Ort, wo man erzeugt, die erste Lufft genie\u00dft,", "tokens": ["Der", "Ort", ",", "wo", "man", "er\u00b7zeugt", ",", "die", "ers\u00b7te", "Lufft", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist sonder Zweiffel gut, wenn da das Gl\u00fckke", "tokens": ["Ist", "son\u00b7der", "Zweif\u00b7fel", "gut", ",", "wenn", "da", "das", "Gl\u00fck\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "ADJD", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "sprie\u00dft", "tokens": ["sprie\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Das unser Sin verlangt. Die Erde ist des HErren:", "tokens": ["Das", "un\u00b7ser", "Sin", "ver\u00b7langt", ".", "Die", "Er\u00b7de", "ist", "des", "Her\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn wir nach ", "tokens": ["Wenn", "wir", "nach"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "w\u00e4ren;", "tokens": ["w\u00e4\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "So m\u00fcst es uns auch gut, bei seinen Gnaden-", "tokens": ["So", "m\u00fcst", "es", "uns", "auch", "gut", ",", "bei", "sei\u00b7nen", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "schein,", "tokens": ["schein", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Am kalten Norderpol, als wie in S\u00fcden sein.", "tokens": ["Am", "kal\u00b7ten", "Nor\u00b7der\u00b7pol", ",", "als", "wie", "in", "S\u00fc\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "KOKOM", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Welt ist nur ein Haus, darin die L\u00e4nder,", "tokens": ["Die", "Welt", "ist", "nur", "ein", "Haus", ",", "da\u00b7rin", "die", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Zimmer", "tokens": ["Zim\u00b7mer"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Dem einen scheint dies gut, dem andern ienes\u2019,", "tokens": ["Dem", "ei\u00b7nen", "scheint", "dies", "gut", ",", "dem", "an\u00b7dern", "ie\u00b7nes'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PDS", "ADJD", "$,", "PRELS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "schlimmer,", "tokens": ["schlim\u00b7mer", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Nachdem die Neigung f\u00e4llt; nachdem die Lebens-", "tokens": ["Nach\u00b7dem", "die", "Nei\u00b7gung", "f\u00e4llt", ";", "nach\u00b7dem", "die", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Art,", "tokens": ["Art", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Sich mit den Gegenden, allwo man wohnt, ver-", "tokens": ["Sich", "mit", "den", "Ge\u00b7gen\u00b7den", ",", "all\u00b7wo", "man", "wohnt", ",", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.31": {"text": "paart,", "tokens": ["paart", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Darauf kommt alles an: Und will man dies ge-", "tokens": ["Da\u00b7rauf", "kommt", "al\u00b7les", "an", ":", "Und", "will", "man", "dies", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "PTKVZ", "$.", "KON", "VMFIN", "PIS", "PDS", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "stehen", "tokens": ["ste\u00b7hen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.34": {"text": "So kann man auch hiebei des H\u00f6chsten Weisheit", "tokens": ["So", "kann", "man", "auch", "hie\u00b7bei", "des", "H\u00f6chs\u00b7ten", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "sehen.", "tokens": ["se\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Die Welt ist abgetheilt, in manches Reich und", "tokens": ["Die", "Welt", "ist", "ab\u00b7ge\u00b7theilt", ",", "in", "man\u00b7ches", "Reich", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "APPR", "PIAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Land,", "tokens": ["Land", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Das menschliche Geschlecht hat durchs Gesellschaffts", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7schlecht", "hat", "durchs", "Ge\u00b7sell\u00b7schaffts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Band", "tokens": ["Band"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.40": {"text": "Sie wiederum verkn\u00fcpft, indem sie allzusammen,", "tokens": ["Sie", "wie\u00b7de\u00b7rum", "ver\u00b7kn\u00fcpft", ",", "in\u00b7dem", "sie", "all\u00b7zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch all aus einem Blut, von einem Ort her-", "tokens": ["Doch", "all", "aus", "ei\u00b7nem", "Blut", ",", "von", "ei\u00b7nem", "Ort", "her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "stammen.", "tokens": ["stam\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Ein ieder der vergn\u00fcgt, lebt allemahl begl\u00fckt,", "tokens": ["Ein", "ie\u00b7der", "der", "ver\u00b7gn\u00fcgt", ",", "lebt", "al\u00b7le\u00b7mahl", "be\u00b7gl\u00fckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "VVPP", "$,", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wo ihn die Vorsehung hat weislich hinger\u00fckt.", "tokens": ["Wo", "ihn", "die", "Vor\u00b7se\u00b7hung", "hat", "weis\u00b7lich", "hin\u00b7ge\u00b7r\u00fckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dient er nur seinem ", "tokens": ["Dient", "er", "nur", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "So mag es in der Stadt; so mag es auf dem Lande", "tokens": ["So", "mag", "es", "in", "der", "Stadt", ";", "so", "mag", "es", "auf", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Auf Bergen, in dem Wald, in Feld und Th\u00e4lern", "tokens": ["Auf", "Ber\u00b7gen", ",", "in", "dem", "Wald", ",", "in", "Feld", "und", "Th\u00e4\u00b7lern"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "sein:", "tokens": ["sein", ":"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "-", "measure": "single.down"}, "line.49": {"text": "So denket die Vernunfft, doch der Geschmak spricht:", "tokens": ["So", "den\u00b7ket", "die", "Ver\u00b7nunfft", ",", "doch", "der", "Ge\u00b7schmak", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Nein.", "tokens": ["Nein", "."], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.51": {"text": "Der die Gesellschafft liebt, und des Get\u00fcmmels", "tokens": ["Der", "die", "Ge\u00b7sell\u00b7schafft", "liebt", ",", "und", "des", "Ge\u00b7t\u00fcm\u00b7mels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Rauschen,", "tokens": ["Rau\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.53": {"text": "Der wird nicht leicht die Stadt, mit einem Dorf", "tokens": ["Der", "wird", "nicht", "leicht", "die", "Stadt", ",", "mit", "ei\u00b7nem", "Dorf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "vertauschen.", "tokens": ["ver\u00b7tau\u00b7schen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.55": {"text": "Wer eine freie Lufft und stilles Leben liebt,", "tokens": ["Wer", "ei\u00b7ne", "frei\u00b7e", "Lufft", "und", "stil\u00b7les", "Le\u00b7ben", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Der w\u00e4hlt ein freies Land daf\u00fcr er St\u00e4dte giebt", "tokens": ["Der", "w\u00e4hlt", "ein", "frei\u00b7es", "Land", "da\u00b7f\u00fcr", "er", "St\u00e4d\u00b7te", "giebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PAV", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Wer hat von Beiden recht? Sie finden alle Beide,", "tokens": ["Wer", "hat", "von", "Bei\u00b7den", "recht", "?", "Sie", "fin\u00b7den", "al\u00b7le", "Bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PIAT", "NN", "$.", "PPER", "VVFIN", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Der eine hie, der da, f\u00fcr sein Gem\u00fcthe, Freude.", "tokens": ["Der", "ei\u00b7ne", "hie", ",", "der", "da", ",", "f\u00fcr", "sein", "Ge\u00b7m\u00fc\u00b7the", ",", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", 