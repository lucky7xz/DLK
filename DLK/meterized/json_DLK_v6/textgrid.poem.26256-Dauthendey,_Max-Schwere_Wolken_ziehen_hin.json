{"textgrid.poem.26256": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Schwere Wolken ziehen hin", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wage kaum zu atmen mehr,", "tokens": ["Wa\u00b7ge", "kaum", "zu", "at\u00b7men", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rundum geht der Tod einher,", "tokens": ["Run\u00b7dum", "geht", "der", "Tod", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Viele Schwellen bleiben leer.", "tokens": ["Vie\u00b7le", "Schwel\u00b7len", "blei\u00b7ben", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Viele Augen schlossen sich,", "tokens": ["Vie\u00b7le", "Au\u00b7gen", "schlos\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes ward ein dunkler Strich,", "tokens": ["Je\u00b7des", "ward", "ein", "dunk\u00b7ler", "Strich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als das L\u00e4cheln bleich entwich.", "tokens": ["Als", "das", "L\u00e4\u00b7cheln", "bleich", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwere Wolken ziehen hin,", "tokens": ["Schwe\u00b7re", "Wol\u00b7ken", "zie\u00b7hen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher dort ich \u00e4hnlich bin,", "tokens": ["Man\u00b7cher", "dort", "ich", "\u00e4hn\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ngen ernste Schatten drin.", "tokens": ["H\u00e4n\u00b7gen", "erns\u00b7te", "Schat\u00b7ten", "drin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}