{"dta.poem.11716": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Meer der Sch\u00f6pfung schwamm zuerst die Lotosblume,", "tokens": ["Im", "Meer", "der", "Sch\u00f6p\u00b7fung", "schwamm", "zu\u00b7erst", "die", "Lo\u00b7tos\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die w\u00f6lbte ihren Kelch gleich einem Heiligthume.", "tokens": ["Die", "w\u00f6lb\u00b7te", "ih\u00b7ren", "Kelch", "gleich", "ei\u00b7nem", "Hei\u00b7lig\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Heiligthume lag der Geist wie unter Zelten,", "tokens": ["Im", "Hei\u00b7lig\u00b7thu\u00b7me", "lag", "der", "Geist", "wie", "un\u00b7ter", "Zel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4chelte im Traum, er tr\u00e4umte k\u00fcnft'ge Welten.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "im", "Traum", ",", "er", "tr\u00e4um\u00b7te", "k\u00fcnft'\u00b7ge", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als sich entfaltete darob die Blum' in Wonne,", "tokens": ["Als", "sich", "ent\u00b7fal\u00b7te\u00b7te", "da\u00b7rob", "die", "Blum'", "in", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ging aus der Blum' ein Glanz, und ward das Licht der Sonne.", "tokens": ["Ging", "aus", "der", "Blum'", "ein", "Glanz", ",", "und", "ward", "das", "Licht", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aufstieg ein Duft, ein Hauch, und ward zu Aetherrauch,", "tokens": ["Auf\u00b7stieg", "ein", "Duft", ",", "ein", "Hauch", ",", "und", "ward", "zu", "A\u00b7e\u00b7ther\u00b7rauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ward feuchte Fr\u00fchlingsluft und Wolkenhimmel auch.", "tokens": ["Ward", "feuch\u00b7te", "Fr\u00fch\u00b7lings\u00b7luft", "und", "Wol\u00b7ken\u00b7him\u00b7mel", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Bl\u00e4ttchen ri\u00df sich los als Schmetterling-Cicade,", "tokens": ["Ein", "Bl\u00e4tt\u00b7chen", "ri\u00df", "sich", "los", "als", "Schmet\u00b7ter\u00b7ling\u00b7Ci\u00b7ca\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und flog der Lebenswelt noch unbekannte Pfade.", "tokens": ["Und", "flog", "der", "Le\u00b7bens\u00b7welt", "noch", "un\u00b7be\u00b7kann\u00b7te", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im Kelche br\u00fctend sa\u00df ein vogelgleich Gebild.", "tokens": ["Im", "Kel\u00b7che", "br\u00fc\u00b7tend", "sa\u00df", "ein", "vo\u00b7gel\u00b7gleich", "Ge\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fl\u00fcgel hobs und schwang sich in des Seyns Gefild,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "hobs", "und", "schwang", "sich", "in", "des", "Seyns", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie k\u00e4mpften in der Luft, und bunt stob manche Feder,", "tokens": ["Sie", "k\u00e4mpf\u00b7ten", "in", "der", "Luft", ",", "und", "bunt", "stob", "man\u00b7che", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein eigenes Geschlecht Luftg\u00e4nger ward aus jeder.", "tokens": ["Ein", "ei\u00b7ge\u00b7nes", "Ge\u00b7schlecht", "Luft\u00b7g\u00e4n\u00b7ger", "ward", "aus", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch au\u00dfen an dem Kelch die Schuppe wasserfrisch", "tokens": ["Doch", "au\u00b7\u00dfen", "an", "dem", "Kelch", "die", "Schup\u00b7pe", "was\u00b7ser\u00b7frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Abtrennte sich und ward halb Krokodil, halb Fisch.", "tokens": ["Ab\u00b7trenn\u00b7te", "sich", "und", "ward", "halb", "Kro\u00b7ko\u00b7dil", ",", "halb", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VAFIN", "ADJD", "NN", "$,", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Der Fisch entschwomm zum Strand der Zukunft voll Begier,", "tokens": ["Der", "Fisch", "ent\u00b7schwomm", "zum", "Strand", "der", "Zu\u00b7kunft", "voll", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stieg dort halb ans Land, ganz als vierf\u00fc\u00df'ges Thier.", "tokens": ["Und", "stieg", "dort", "halb", "ans", "Land", ",", "ganz", "als", "vier\u00b7f\u00fc\u00df'\u00b7ges", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Lotoswiege schwankt, es g\u00e4hrt der Wasserschaum,", "tokens": ["Die", "Lo\u00b7tos\u00b7wie\u00b7ge", "schwankt", ",", "es", "g\u00e4hrt", "der", "Was\u00b7ser\u00b7schaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist erwacht und sieht die Sch\u00f6pfung, seinen Traum.", "tokens": ["Der", "Geist", "er\u00b7wacht", "und", "sieht", "die", "Sch\u00f6p\u00b7fung", ",", "sei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Er sprach: Ich tr\u00e4umte das, doch nun will ich im Wachen", "tokens": ["Er", "sprach", ":", "Ich", "tr\u00e4um\u00b7te", "das", ",", "doch", "nun", "will", "ich", "im", "Wa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PDS", "$,", "ADV", "ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Traumwelt wachen Herrn, den Menschen selber machen.", "tokens": ["Der", "Traum\u00b7welt", "wa\u00b7chen", "Herrn", ",", "den", "Men\u00b7schen", "sel\u00b7ber", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}