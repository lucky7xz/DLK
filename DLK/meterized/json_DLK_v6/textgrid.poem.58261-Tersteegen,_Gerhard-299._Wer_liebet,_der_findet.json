{"textgrid.poem.58261": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "299. Wer liebet, der findet", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man suchet Gott, man wirkt, man l\u00e4uft herum;", "tokens": ["Man", "su\u00b7chet", "Gott", ",", "man", "wirkt", ",", "man", "l\u00e4uft", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach liebet nur, la\u00dft, was zerstreuet, fahren!", "tokens": ["Ach", "lie\u00b7bet", "nur", ",", "la\u00dft", ",", "was", "zer\u00b7streu\u00b7et", ",", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$,", "VVFIN", "$,", "PWS", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ein stiller Geist ist Gottes Heiligtum,", "tokens": ["Ein", "stil\u00b7ler", "Geist", "ist", "Got\u00b7tes", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da will er sich der Einfalt offenbaren.", "tokens": ["Da", "will", "er", "sich", "der", "Ein\u00b7falt", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}