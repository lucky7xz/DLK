{"dta.poem.20675": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Gebet eines Kindes .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aller Menschen Vater! h\u00f6re,                 ", "tokens": ["Al\u00b7ler", "Men\u00b7schen", "Va\u00b7ter", "!", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Merk auf mich dein lallend Kind,", "tokens": ["Merk", "auf", "mich", "dein", "lal\u00b7lend", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb mir deinen Geist, und lehre", "tokens": ["Gieb", "mir", "dei\u00b7nen", "Geist", ",", "und", "leh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich, was deine Wege sind.", "tokens": ["Mich", ",", "was", "dei\u00b7ne", "We\u00b7ge", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich zu f\u00fcrchten, dich zu scheuen,", "tokens": ["Dich", "zu", "f\u00fcrch\u00b7ten", ",", "dich", "zu", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich zu lieben und in dir", "tokens": ["Dich", "zu", "lie\u00b7ben", "und", "in", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "KON", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich der sch\u00f6nen Welt zu freuen,", "tokens": ["Mich", "der", "sch\u00f6\u00b7nen", "Welt", "zu", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6pfer, dies verleihe mir!", "tokens": ["Sch\u00f6p\u00b7fer", ",", "dies", "ver\u00b7lei\u00b7he", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meinen Eltern Ehre geben,", "tokens": ["Mei\u00b7nen", "El\u00b7tern", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrem Wink gehorsam seyn,", "tokens": ["Ih\u00b7rem", "Wink", "ge\u00b7hor\u00b7sam", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir und ihnen dankbar leben,", "tokens": ["Dir", "und", "ih\u00b7nen", "dank\u00b7bar", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Tadel, fromm und rein;", "tokens": ["Oh\u00b7ne", "Ta\u00b7del", ",", "fromm", "und", "rein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vater, die\u00df sind meine Pflichten;", "tokens": ["Va\u00b7ter", ",", "die\u00df", "sind", "mei\u00b7ne", "Pflich\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ich wachse wie ein Baum,", "tokens": ["Ach", ",", "ich", "wach\u00b7se", "wie", "ein", "Baum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der gepflanzet ward zu Fr\u00fcchten", "tokens": ["Der", "ge\u00b7pflan\u00b7zet", "ward", "zu", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVPP", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Gartens besten Raum.", "tokens": ["In", "des", "Gar\u00b7tens", "bes\u00b7ten", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df mich gute Fr\u00fcchte tragen,", "tokens": ["La\u00df", "mich", "gu\u00b7te", "Fr\u00fcch\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, du pr\u00fcfest Herz und Sinn,", "tokens": ["Herr", ",", "du", "pr\u00fc\u00b7fest", "Herz", "und", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ich in der Zukunft Tagen", "tokens": ["Ob", "ich", "in", "der", "Zu\u00b7kunft", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tugendhaft und gl\u00fccklich bin.", "tokens": ["Tu\u00b7gend\u00b7haft", "und", "gl\u00fcck\u00b7lich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sollt ich nicht \u2014 o dann erh\u00f6re", "tokens": ["Sollt", "ich", "nicht", "o", "dann", "er\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "$(", "FM", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein verdoppelt kindlich Flehn,", "tokens": ["Mein", "ver\u00b7dop\u00b7pelt", "kind\u00b7lich", "Flehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und la\u00df mich zu deiner Ehre", "tokens": ["Und", "la\u00df", "mich", "zu", "dei\u00b7ner", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Unschuldsvoll dein Antlitz seyn.", "tokens": ["Un\u00b7schulds\u00b7voll", "dein", "Ant\u00b7litz", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nimm mich fr\u00fch von dieser Erde,", "tokens": ["Nimm", "mich", "fr\u00fch", "von", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mir nicht dein Auge feind,", "tokens": ["Da\u00df", "mir", "nicht", "dein", "Au\u00b7ge", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wegen meiner S\u00fcnden werde,", "tokens": ["We\u00b7gen", "mei\u00b7ner", "S\u00fcn\u00b7den", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mein guter Engel weint.", "tokens": ["Wenn", "mein", "gu\u00b7ter", "En\u00b7gel", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}