{"textgrid.poem.37207": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine holde kenn' ich, Eine", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine holde kenn' ich, Eine", "tokens": ["Ei\u00b7ne", "hol\u00b7de", "kenn'", "ich", ",", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$,", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(weiter auf der ganzen Welt", "tokens": ["(", "wei\u00b7ter", "auf", "der", "gan\u00b7zen", "Welt"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebt es solcher Frauen keine!)", "tokens": ["Giebt", "es", "sol\u00b7cher", "Frau\u00b7en", "kei\u00b7ne", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PIAT", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gefangen nimmt und h\u00e4lt", "tokens": ["Die", "ge\u00b7fan\u00b7gen", "nimmt", "und", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was vor Augen ihr sich stellt.", "tokens": ["Was", "vor", "Au\u00b7gen", "ihr", "sich", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00e4nztest du in Silberhaaren,", "tokens": ["Gl\u00e4nz\u00b7test", "du", "in", "Sil\u00b7ber\u00b7haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gaukeltest in Knabenjahren,", "tokens": ["Gau\u00b7kel\u00b7test", "in", "Kna\u00b7ben\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4rst ein Weiser oder Held,", "tokens": ["W\u00e4rst", "ein", "Wei\u00b7ser", "o\u00b7der", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lebt in dir auch eine Welt:", "tokens": ["Lebt", "in", "dir", "auch", "ei\u00b7ne", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn du einen Blick empfingest,", "tokens": ["Wenn", "du", "ei\u00b7nen", "Blick", "emp\u00b7fin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie nur sie ihn blicken kann,", "tokens": ["Wie", "nur", "sie", "ihn", "bli\u00b7cken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Einmal ihr zur Seite gingest,", "tokens": ["Ein\u00b7mal", "ihr", "zur", "Sei\u00b7te", "gin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00f6rtest was ihr Geist ersann,", "tokens": ["H\u00f6r\u00b7test", "was", "ihr", "Geist", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "In die Kenntnis einmal dr\u00e4ngest,", "tokens": ["In", "die", "Kennt\u00b7nis", "ein\u00b7mal", "dr\u00e4n\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die sie spielend sich gewann,", "tokens": ["Die", "sie", "spie\u00b7lend", "sich", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Und zu H\u00f6hen, die ein Mann", "tokens": ["Und", "zu", "H\u00f6\u00b7hen", ",", "die", "ein", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Kaum erforscht, mit ihr dich schw\u00e4ngest,", "tokens": ["Kaum", "er\u00b7forscht", ",", "mit", "ihr", "dich", "schw\u00e4n\u00b7gest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "O mit Mund und Herzen dann", "tokens": ["O", "mit", "Mund", "und", "Her\u00b7zen", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Spr\u00e4chest du: Ich bet dich an!", "tokens": ["Spr\u00e4\u00b7chest", "du", ":", "Ich", "bet", "dich", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}