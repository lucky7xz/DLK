{"textgrid.poem.23941": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "An die Trauerweide", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trauerweide, erster Baum,", "tokens": ["Trau\u00b7er\u00b7wei\u00b7de", ",", "ers\u00b7ter", "Baum", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die gr\u00fcnen Wimpel schwingt,", "tokens": ["Der", "die", "gr\u00fc\u00b7nen", "Wim\u00b7pel", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem zuerst die Lebenslust", "tokens": ["Dem", "zu\u00b7erst", "die", "Le\u00b7bens\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frisch aus Ast und Zweige dringt, \u2013", "tokens": ["Frisch", "aus", "Ast", "und", "Zwei\u00b7ge", "dringt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum nennen sie dich so,", "tokens": ["Wa\u00b7rum", "nen\u00b7nen", "sie", "dich", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Den die Bl\u00e4tterf\u00fclle biegt,", "tokens": ["Den", "die", "Bl\u00e4t\u00b7ter\u00b7f\u00fcl\u00b7le", "biegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der zuerst im Fr\u00fchlingswind", "tokens": ["Der", "zu\u00b7erst", "im", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich im Fr\u00fchlingstanze wiegt?", "tokens": ["Sich", "im", "Fr\u00fch\u00b7lings\u00b7tan\u00b7ze", "wiegt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlecht verstehen sie die Kraft,", "tokens": ["Schlecht", "ver\u00b7ste\u00b7hen", "sie", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich spielend niederneigt,", "tokens": ["Die", "sich", "spie\u00b7lend", "nie\u00b7der\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Hand die Erde kost,", "tokens": ["Mit", "der", "Hand", "die", "Er\u00b7de", "kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Haupt den Himmel zeigt.", "tokens": ["Mit", "dem", "Haupt", "den", "Him\u00b7mel", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}