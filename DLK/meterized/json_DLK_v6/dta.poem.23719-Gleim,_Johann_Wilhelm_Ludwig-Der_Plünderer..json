{"dta.poem.23719": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Pl\u00fcnderer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da seufzete die Tochter:", "tokens": ["Da", "seuf\u00b7ze\u00b7te", "die", "Toch\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach, w\u00e4ren alle Feinde", "tokens": ["Ach", ",", "w\u00e4\u00b7ren", "al\u00b7le", "Fein\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So freundlich, so barmherzig;", "tokens": ["So", "freund\u00b7lich", ",", "so", "barm\u00b7her\u00b7zig", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So m\u00f6chten sie mich pl\u00fcndern!", "tokens": ["So", "m\u00f6ch\u00b7ten", "sie", "mich", "pl\u00fcn\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich that es, liebstes M\u00fchmchen,", "tokens": ["Ich", "that", "es", ",", "liebs\u00b7tes", "M\u00fchm\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich pl\u00fcnderte das M\u00e4dchen.", "tokens": ["Ich", "pl\u00fcn\u00b7der\u00b7te", "das", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}