["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NE", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nicht Silber/ Geld und Gold macht uns vom Tode frey.", "tokens": ["Nicht", "Sil\u00b7ber", "/", "Geld", "und", "Gold", "macht", "uns", "vom", "To\u00b7de", "frey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$(", "NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was Unfall/ Gl\u00fcck und Zeit mit ihrem Zahn verzehren/", "tokens": ["Was", "Un\u00b7fall", "/", "Gl\u00fcck", "und", "Zeit", "mit", "ih\u00b7rem", "Zahn", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Bejahet wol kein Mensch/ da\u00df es best\u00e4ndig sey.", "tokens": ["Be\u00b7ja\u00b7het", "wol", "kein", "Mensch", "/", "da\u00df", "es", "be\u00b7st\u00e4n\u00b7dig", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Tugend ists allein/ die keinen Wechsel kennet/", "tokens": ["Die", "Tu\u00b7gend", "ists", "al\u00b7lein", "/", "die", "kei\u00b7nen", "Wech\u00b7sel", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+