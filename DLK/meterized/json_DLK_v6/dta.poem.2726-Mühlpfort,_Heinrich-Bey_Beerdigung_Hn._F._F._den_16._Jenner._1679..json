{"dta.poem.2726": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Hn. F. F. den 16. Jenner.  \n 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wird deine B\u00f6rse denn nun eine Todten-Bahre?", "tokens": ["Wird", "dei\u00b7ne", "B\u00f6r\u00b7se", "denn", "nun", "ei\u00b7ne", "Tod\u00b7ten\u00b7Bah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schleust/ ", "tokens": ["Und", "schleust", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "F\u00e4ngst kein Journal mehr an in diesem Neuen Jahre?", "tokens": ["F\u00e4ngst", "kein", "Jour\u00b7nal", "mehr", "an", "in", "die\u00b7sem", "Neu\u00b7en", "Jah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "APPR", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "St\u00f6st eine gr\u00f6\u00dfre Reis\u2019 und Wechsel dir bevor:", "tokens": ["St\u00f6st", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "Reis'", "und", "Wech\u00b7sel", "dir", "be\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach ja/ du eilst hinweg Herr Oehmen zu empfangen/", "tokens": ["Ach", "ja", "/", "du", "eilst", "hin\u00b7weg", "Herr", "O\u00b7eh\u00b7men", "zu", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "PPER", "VVFIN", "ADV", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "----+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der jene Messe schon des Lebens-Marckt beschlo\u00df/", "tokens": ["Der", "je\u00b7ne", "Mes\u00b7se", "schon", "des", "Le\u00b7bens\u00b7Ma\u00b7rckt", "be\u00b7schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und in die Ewigkeit dir ist vorangegangen/", "tokens": ["Und", "in", "die", "E\u00b7wig\u00b7keit", "dir", "ist", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo er sein Capital macht unbeschreiblich gro\u00df.", "tokens": ["Wo", "er", "sein", "Ca\u00b7pi\u00b7tal", "macht", "un\u00b7be\u00b7schreib\u00b7lich", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da sol dein Hauptstam\u0303 stehn: Du magst der Welt nicht trauen/", "tokens": ["Da", "sol", "dein", "Haupt\u00b7stam\u0303", "stehn", ":", "Du", "magst", "der", "Welt", "nicht", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die nur Betr\u00fcgerey an statt der Zinsen gibt.", "tokens": ["Die", "nur", "Be\u00b7tr\u00fc\u00b7ge\u00b7rey", "an", "statt", "der", "Zin\u00b7sen", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du wilst im Himmel dort den Scontre-Platz dir bauen/", "tokens": ["Du", "wilst", "im", "Him\u00b7mel", "dort", "den", "Scon\u00b7tre\u00b7Platz", "dir", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADV", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo Treu und Glauben man f\u00fcr Tonnen Goldes liebt.", "tokens": ["Wo", "Treu", "und", "Glau\u00b7ben", "man", "f\u00fcr", "Ton\u00b7nen", "Gol\u00b7des", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PIS", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Tod ist dein Senzal/ und bringt dir di\u00df zu wege/", "tokens": ["Der", "Tod", "ist", "dein", "Sen\u00b7zal", "/", "und", "bringt", "dir", "di\u00df", "zu", "we\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "PDS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was keine Factorey der Menschen je verkehrt.", "tokens": ["Was", "kei\u00b7ne", "Fac\u00b7to\u00b7rey", "der", "Men\u00b7schen", "je", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du weist wie so genau er alles \u00fcberlege/", "tokens": ["Du", "weist", "wie", "so", "ge\u00b7nau", "er", "al\u00b7les", "\u00fc\u00b7berl\u00b7e\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "ADJD", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und da\u00df er nach dem Schlu\u00df die Ewigkeit gewehrt.", "tokens": ["Und", "da\u00df", "er", "nach", "dem", "Schlu\u00df", "die", "E\u00b7wig\u00b7keit", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ists m\u00f6glich/ da\u00df zwey Freund einander schleunig missen/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "da\u00df", "zwey", "Freund", "ein\u00b7an\u00b7der", "schleu\u00b7nig", "mis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "CARD", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und in vier Monat-Frist einander wieder sehn:", "tokens": ["Und", "in", "vier", "Mo\u00b7nat\u00b7Frist", "ein\u00b7an\u00b7der", "wie\u00b7der", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da ihr Verb\u00fcndn\u00fc\u00df wird auff nimmermehr zerrissen", "tokens": ["Da", "ihr", "Ver\u00b7b\u00fcnd\u00b7n\u00fc\u00df", "wird", "auff", "nim\u00b7mer\u00b7mehr", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und ihre Handelschafft kein Zufall kan verdrehn.", "tokens": ["Und", "ih\u00b7re", "Han\u00b7del\u00b7schafft", "kein", "Zu\u00b7fall", "kan", "ver\u00b7drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jhr habt ja beyde nun das h\u00f6chste Gut erhandelt/", "tokens": ["Ihr", "habt", "ja", "bey\u00b7de", "nun", "das", "h\u00f6chs\u00b7te", "Gut", "er\u00b7han\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Erlangt den Seelen-Schatz/ dem kein Gewin sich gleicht/", "tokens": ["Er\u00b7langt", "den", "See\u00b7len\u00b7Schatz", "/", "dem", "kein", "Ge\u00b7win", "sich", "gleicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "PIAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und vor der gantzen Welt schlecht und gerecht gewandelt/", "tokens": ["Und", "vor", "der", "gant\u00b7zen", "Welt", "schlecht", "und", "ge\u00b7recht", "ge\u00b7wan\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df auch der wahre Ruhm nicht von dem Grabe weicht.", "tokens": ["Da\u00df", "auch", "der", "wah\u00b7re", "Ruhm", "nicht", "von", "dem", "Gra\u00b7be", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Lasst was Vesputz durchkreutzt und Magellan entdecket/", "tokens": ["Lasst", "was", "Ves\u00b7putz", "durch\u00b7kreutzt", "und", "Ma\u00b7gel\u00b7lan", "ent\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "VVPP", "KON", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Und wo Columbus erst mit seinen Segeln lieff/", "tokens": ["Und", "wo", "Co\u00b7lum\u00b7bus", "erst", "mit", "sei\u00b7nen", "Se\u00b7geln", "lieff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Gold-ersoffnen Welt zum End-Ziel seyn gestecket/", "tokens": ["Der", "Gold\u00b7er\u00b7soff\u00b7nen", "Welt", "zum", "En\u00b7dZiel", "seyn", "ge\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "An einem sichrern Port l\u00e4ndt euer Glaubens-Schiff.", "tokens": ["An", "ei\u00b7nem", "sich\u00b7rern", "Port", "l\u00e4ndt", "eu\u00b7er", "Glau\u00b7bens\u00b7Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es mag das Alterthum von seinem Argo sagen/", "tokens": ["Es", "mag", "das", "Al\u00b7ter\u00b7thum", "von", "sei\u00b7nem", "Ar\u00b7go", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie es das g\u00f6ldne Flie\u00df von Colchos hergebracht:", "tokens": ["Wie", "es", "das", "g\u00f6ld\u00b7ne", "Flie\u00df", "von", "Col\u00b7chos", "her\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das Lamm/ das alle Schuld der Sterblichen getragen/", "tokens": ["Das", "Lamm", "/", "das", "al\u00b7le", "Schuld", "der", "Sterb\u00b7li\u00b7chen", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat an Verm\u00f6gen euch mehr reich und gro\u00df gemacht.", "tokens": ["Hat", "an", "Ver\u00b7m\u00f6\u00b7gen", "euch", "mehr", "reich", "und", "gro\u00df", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was sind die G\u00fcter doch/ die wir hier Reichthum nennen:", "to