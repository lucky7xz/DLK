{"dta.poem.23516": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00fcn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deutsches Volk, wie gut berathen!", "tokens": ["Deut\u00b7sches", "Volk", ",", "wie", "gut", "be\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffnung spriesst in deinen Gau'n:", "tokens": ["Hoff\u00b7nung", "spriesst", "in", "dei\u00b7nen", "Gau'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcn sind stets noch deine Saaten,", "tokens": ["Gr\u00fcn", "sind", "stets", "noch", "dei\u00b7ne", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine W\u00e4lder, deine Au'n.", "tokens": ["Dei\u00b7ne", "W\u00e4l\u00b7der", ",", "dei\u00b7ne", "Au'", "n."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der Hoffnung ruht dein Leben:", "tokens": ["In", "der", "Hoff\u00b7nung", "ruht", "dein", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt auch manche Hoffnung aus \u2014", "tokens": ["Bleibt", "auch", "man\u00b7che", "Hoff\u00b7nung", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steuern nehmen, Steuern geben,", "tokens": ["Steu\u00b7ern", "neh\u00b7men", ",", "Steu\u00b7ern", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Hoffnung stirbt nicht aus.", "tokens": ["Die\u00b7se", "Hoff\u00b7nung", "stirbt", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoffnung tilget deine Klagen,", "tokens": ["Hoff\u00b7nung", "til\u00b7get", "dei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6schet deines Zweifels Spur,", "tokens": ["L\u00f6\u00b7schet", "dei\u00b7nes", "Zwei\u00b7fels", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mit gr\u00fcnem Tuch beschlagen", "tokens": ["Denn", "mit", "gr\u00fc\u00b7nem", "Tuch", "be\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Sitzungstische nur.", "tokens": ["Sind", "die", "Sit\u00b7zungs\u00b7ti\u00b7sche", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Darum geh in diesen Tagen,", "tokens": ["Da\u00b7rum", "geh", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsches Volk, in Hoffnungstracht;", "tokens": ["Deut\u00b7sches", "Volk", ",", "in", "Hoff\u00b7nungs\u00b7tracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcne R\u00f6cke musst du tragen,", "tokens": ["Gr\u00fc\u00b7ne", "R\u00f6\u00b7cke", "musst", "du", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil man dir nur Hoffnung macht.", "tokens": ["Weil", "man", "dir", "nur", "Hoff\u00b7nung", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}