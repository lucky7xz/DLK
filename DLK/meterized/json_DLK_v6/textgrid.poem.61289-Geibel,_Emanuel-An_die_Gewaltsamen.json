{"textgrid.poem.61289": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An die Gewaltsamen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der heil'ge Geist ist Gottes freie Gabe,", "tokens": ["Der", "heil'\u00b7ge", "Geist", "ist", "Got\u00b7tes", "frei\u00b7e", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort ein Fels, ein ew'ger. Meint ihr gar,", "tokens": ["Das", "Wort", "ein", "Fels", ",", "ein", "ew'", "ger.", "Meint", "ihr", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df ihr ihn st\u00fctzen m\u00f6gt mit eurem Stabe?", "tokens": ["Da\u00df", "ihr", "ihn", "st\u00fct\u00b7zen", "m\u00f6gt", "mit", "eu\u00b7rem", "Sta\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und dessen Hand ihn hielt zweitausend Jahr,", "tokens": ["Und", "des\u00b7sen", "Hand", "ihn", "hielt", "zweit\u00b7au\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df auch kein K\u00f6rnchen durfte davon splittern,", "tokens": ["Da\u00df", "auch", "kein", "K\u00f6rn\u00b7chen", "durf\u00b7te", "da\u00b7von", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VMFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4hnt ihr, er schlafe, weil ihr tr\u00e4umt Gefahr?", "tokens": ["W\u00e4hnt", "ihr", ",", "er", "schla\u00b7fe", ",", "weil", "ihr", "tr\u00e4umt", "Ge\u00b7fahr", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kleingl\u00e4ubige, wie m\u00f6gt ihr also zittern!", "tokens": ["Klein\u00b7gl\u00e4u\u00b7bi\u00b7ge", ",", "wie", "m\u00f6gt", "ihr", "al\u00b7so", "zit\u00b7tern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein! La\u00dft die Geister wandeln ihre Bahn!", "tokens": ["Nein", "!", "La\u00dft", "die", "Geis\u00b7ter", "wan\u00b7deln", "ih\u00b7re", "Bahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVIMP", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Klar wird die Luft in Sturm und Ungewittern.", "tokens": ["Klar", "wird", "die", "Luft", "in", "Sturm", "und", "Un\u00b7ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und schw\u00f6lle berghoch die Verneinung an", "tokens": ["Und", "schw\u00f6l\u00b7le", "berg\u00b7hoch", "die", "Ver\u00b7nei\u00b7nung", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie eine neue S\u00fcndflut: mag sie schwellen!", "tokens": ["Wie", "ei\u00b7ne", "neu\u00b7e", "S\u00fcnd\u00b7flut", ":", "mag", "sie", "schwel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht eurem Machtspruch ist sie untertan.", "tokens": ["Nicht", "eu\u00b7rem", "Machts\u00b7pruch", "ist", "sie", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch glaubt, ob Menschensatzung mag zerschellen:", "tokens": ["Doch", "glaubt", ",", "ob", "Men\u00b7schen\u00b7sat\u00b7zung", "mag", "zer\u00b7schel\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wahren Kirche dreimal heilig Schiff", "tokens": ["Der", "wah\u00b7ren", "Kir\u00b7che", "drei\u00b7mal", "hei\u00b7lig", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Treibt gleich der Arche sicher auf den Wellen.", "tokens": ["Treibt", "gleich", "der", "Ar\u00b7che", "si\u00b7cher", "auf", "den", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Und wen die Sehnsucht nach dem Herrn ergriff:", "tokens": ["Und", "wen", "die", "Sehn\u00b7sucht", "nach", "dem", "Herrn", "er\u00b7griff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie immer auch gehei\u00dfen sei sein Glaube,", "tokens": ["Wie", "im\u00b7mer", "auch", "ge\u00b7hei\u00b7\u00dfen", "sei", "sein", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er mag sich bergen drin vor Flut und Riff.", "tokens": ["Er", "mag", "sich", "ber\u00b7gen", "drin", "vor", "Flut", "und", "Riff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und kommen wird der Tag, da bringt die Taube", "tokens": ["Und", "kom\u00b7men", "wird", "der", "Tag", ",", "da", "bringt", "die", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den \u00d6lzweig heim: es wurzelt im Gestein", "tokens": ["Den", "\u00d6l\u00b7zweig", "heim", ":", "es", "wur\u00b7zelt", "im", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Schiffes Kiel, nicht mehr der Flut zum Raube.", "tokens": ["Des", "Schif\u00b7fes", "Kiel", ",", "nicht", "mehr", "der", "Flut", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dann wird ", "tokens": ["Dann", "wird"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Verlaufen in der Tiefe sind die Wogen,", "tokens": ["Ver\u00b7lau\u00b7fen", "in", "der", "Tie\u00b7fe", "sind", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verweht vom Winde ist das letzte: Nein!", "tokens": ["Ver\u00b7weht", "vom", "Win\u00b7de", "ist", "das", "letz\u00b7te", ":", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VAFIN", "ART", "ADJA", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und auf den Wolken steht der Friedensbogen.", "tokens": ["Und", "auf", "den", "Wol\u00b7ken", "steht", "der", "Frie\u00b7dens\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}