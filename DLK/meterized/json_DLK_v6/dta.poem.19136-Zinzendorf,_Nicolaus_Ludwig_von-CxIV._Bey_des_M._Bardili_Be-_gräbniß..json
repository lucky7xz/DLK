{"dta.poem.19136": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxIV.  Bey des M. Bardili Be-  \n gr\u00e4bni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die Gnade ist gewi\u00df noch allzu unbekannt,", "tokens": ["Die", "Gna\u00b7de", "ist", "ge\u00b7wi\u00df", "noch", "all\u00b7zu", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gnade, eine Krafft, von Millionen Zungen", "tokens": ["Die", "Gna\u00b7de", ",", "ei\u00b7ne", "Krafft", ",", "von", "Mil\u00b7lion\u00b7en", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bald mit Beweglichkeit, bald nur mit Unverstand,", "tokens": ["Bald", "mit", "Be\u00b7weg\u00b7lich\u00b7keit", ",", "bald", "nur", "mit", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Daheim und offentlich, bezeuget und besungen.", "tokens": ["Da\u00b7heim", "und", "of\u00b7fent\u00b7lich", ",", "be\u00b7zeu\u00b7get", "und", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "ADJD", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was wird noch aus der Welt? Wie gro\u00df ist ihre Noth!", "tokens": ["Was", "wird", "noch", "aus", "der", "Welt", "?", "Wie", "gro\u00df", "ist", "ih\u00b7re", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ist das Leben selbst den Todten noch so todt!", "tokens": ["Wie", "ist", "das", "Le\u00b7ben", "selbst", "den", "Tod\u00b7ten", "noch", "so", "todt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kan der vom Gnaden-Stuhl zur\u00fcck geschreckte Geist,", "tokens": ["Kan", "der", "vom", "Gna\u00b7den\u00b7Stuhl", "zu\u00b7r\u00fcck", "ge\u00b7schreck\u00b7te", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "APPRART", "NN", "PTKVZ", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan er den Sinn des HErrn nicht g\u00e4ntzlich vorenthalten;", "tokens": ["Kan", "er", "den", "Sinn", "des", "Herrn", "nicht", "g\u00e4ntz\u00b7lich", "vor\u00b7ent\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So, da\u00df ein Mensch erf\u00e4hrt, was Gnade GOttes heist:", "tokens": ["So", ",", "da\u00df", "ein", "Mensch", "er\u00b7f\u00e4hrt", ",", "was", "Gna\u00b7de", "Got\u00b7tes", "heist", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "PWS", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bildet er das Wort in seltsame Gestalten;", "tokens": ["So", "bil\u00b7det", "er", "das", "Wort", "in", "selt\u00b7sa\u00b7me", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Bald gehts dem Glauben ", "tokens": ["Bald", "gehts", "dem", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bald macht er, da\u00df man ", "tokens": ["Bald", "macht", "er", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was ist doch eigentlich am Mi\u00dfverstande schuld?", "tokens": ["Was", "ist", "doch", "ei\u00b7gent\u00b7lich", "am", "Mi\u00df\u00b7ver\u00b7stan\u00b7de", "schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ists kein Mi\u00dfverstand, wer h\u00e4rtet das Beginnen", "tokens": ["Und", "ists", "kein", "Mi\u00df\u00b7ver\u00b7stand", ",", "wer", "h\u00e4r\u00b7tet", "das", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Volcks von frecher Stirn, von Diamantnen Sinnen,", "tokens": ["Des", "Volcks", "von", "fre\u00b7cher", "Stirn", ",", "von", "Di\u00b7a\u00b7mant\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von unbeschnittnem Ohr und halb-verwe\u00dften Geist,", "tokens": ["Von", "un\u00b7be\u00b7schnitt\u00b7nem", "Ohr", "und", "halb\u00b7ver\u00b7we\u00df\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dessen Sprichwort doch auch: ", "tokens": ["Und", "des\u00b7sen", "Sprich\u00b7wort", "doch", "auch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Nicht GOtt! Dann welcher Mensch ihn dieser Sache zeyht,", "tokens": ["Nicht", "Gott", "!", "Dann", "wel\u00b7cher", "Mensch", "ihn", "die\u00b7ser", "Sa\u00b7che", "zeyht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$.", "ADV", "PWAT", "NN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der macht den Sch\u00f6pffer selbst sehr deutlich zum Betr\u00fcger,", "tokens": ["Der", "macht", "den", "Sch\u00f6pf\u00b7fer", "selbst", "sehr", "deut\u00b7lich", "zum", "Be\u00b7tr\u00fc\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und damit wird zugleich das Hau\u00df der Menschlichkeit", "tokens": ["Und", "da\u00b7mit", "wird", "zu\u00b7gleich", "das", "Hau\u00df", "der", "Menschlich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer anders, als ein Geist der L\u00fcgen, hats erdacht,", "tokens": ["Wer", "an\u00b7ders", ",", "als", "ein", "Geist", "der", "L\u00fc\u00b7gen", ",", "hats", "er\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df GOtt uns anders ", "tokens": ["Da\u00df", "Gott", "uns", "an\u00b7ders"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}}}}