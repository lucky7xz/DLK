{"textgrid.poem.33612": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Andenken", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gekr\u00e4nkt ist, liebt zu hassen,", "tokens": ["Wer", "ge\u00b7kr\u00e4nkt", "ist", ",", "liebt", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$,", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ins Ungl\u00fcck ist verliebt,", "tokens": ["Und", "ins", "Un\u00b7gl\u00fcck", "ist", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer, vom holden Gl\u00fcck verlassen,", "tokens": ["Wer", ",", "vom", "hol\u00b7den", "Gl\u00fcck", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Trauer sich ergibt.", "tokens": ["Sei\u00b7ner", "Trau\u00b7er", "sich", "er\u00b7gibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du liebst in dem Heiligtume", "tokens": ["Du", "liebst", "in", "dem", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Deiner Treue, zart' Gem\u00fct,", "tokens": ["Dei\u00b7ner", "Treu\u00b7e", ",", "zart'", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends noch die welke Blume,", "tokens": ["A\u00b7bends", "noch", "die", "wel\u00b7ke", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die am Morgen dir erbl\u00fcht.", "tokens": ["Die", "am", "Mor\u00b7gen", "dir", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}