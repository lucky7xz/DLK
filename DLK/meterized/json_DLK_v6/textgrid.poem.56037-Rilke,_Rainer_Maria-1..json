{"textgrid.poem.56037": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unaufhaltsam heben sich die Parke", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "he\u00b7ben", "sich", "die", "Par\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus dem sanft zerfallenden Vergehn;", "tokens": ["aus", "dem", "sanft", "zer\u00b7fal\u00b7len\u00b7den", "Ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00fcberh\u00e4uft mit Himmeln, \u00fcberstarke", "tokens": ["\u00fc\u00b7berh\u00b7\u00e4uft", "mit", "Him\u00b7meln", ",", "\u00fc\u00b7bers\u00b7tar\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcberlieferte, die \u00fcberstehn,", "tokens": ["\u00dc\u00b7berl\u00b7ie\u00b7fer\u00b7te", ",", "die", "\u00fc\u00b7bers\u00b7tehn", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVINF", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "um sich auf den klaren Rasenpl\u00e4nen", "tokens": ["um", "sich", "auf", "den", "kla\u00b7ren", "Ra\u00b7sen\u00b7pl\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "auszubreiten und zur\u00fcckzuziehen,", "tokens": ["aus\u00b7zu\u00b7brei\u00b7ten", "und", "zu\u00b7r\u00fcck\u00b7zu\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "immer mit demselben souver\u00e4nen", "tokens": ["im\u00b7mer", "mit", "dem\u00b7sel\u00b7ben", "sou\u00b7ve\u00b7r\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aufwand, wie besch\u00fctzt durch ihn,", "tokens": ["Auf\u00b7wand", ",", "wie", "be\u00b7sch\u00fctzt", "durch", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVPP", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "und den unersch\u00f6pflichen Erl\u00f6s", "tokens": ["und", "den", "un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7chen", "Er\u00b7l\u00f6s"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "k\u00f6niglicher Gr\u00f6\u00dfe noch vermehrend,", "tokens": ["k\u00f6\u00b7nig\u00b7li\u00b7cher", "Gr\u00f6\u00b7\u00dfe", "noch", "ver\u00b7meh\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "aus sich steigend, in sich wiederkehrend:", "tokens": ["aus", "sich", "stei\u00b7gend", ",", "in", "sich", "wie\u00b7der\u00b7keh\u00b7rend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "$,", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "huldvoll, prunkend, purpurn und pomp\u00f6s.", "tokens": ["huld\u00b7voll", ",", "prun\u00b7kend", ",", "pur\u00b7purn", "und", "pom\u00b7p\u00f6s", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "NE", "KON", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}