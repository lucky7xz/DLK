{"dta.poem.14233": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsame .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagtest gar oft wie s\u00fc\u00df und rein", "tokens": ["Du", "sag\u00b7test", "gar", "oft", "wie", "s\u00fc\u00df", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KOKOM", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind Deine blauen Aeugelein!", "tokens": ["Sind", "Dei\u00b7ne", "blau\u00b7en", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzo m\u00fcssen sie immerfort weinen,", "tokens": ["Jet\u00b7zo", "m\u00fcs\u00b7sen", "sie", "im\u00b7mer\u00b7fort", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da sie nicht finden mehr, was sie meinen.", "tokens": ["Da", "sie", "nicht", "fin\u00b7den", "mehr", ",", "was", "sie", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wird auch der rothe Mund erblassen,", "tokens": ["Wird", "auch", "der", "ro\u00b7the", "Mund", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seit Du mich, s\u00fc\u00dfer Buhle, verlassen.", "tokens": ["Seit", "Du", "mich", ",", "s\u00fc\u00b7\u00dfer", "Buh\u00b7le", ",", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Eh Du wohl denkst, kann das Blatt sich wenden,", "tokens": ["Eh", "Du", "wohl", "denkst", ",", "kann", "das", "Blatt", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Geht alles gar bald zu seinem Ende.", "tokens": ["Geht", "al\u00b7les", "gar", "bald", "zu", "sei\u00b7nem", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}