{"textgrid.poem.41883": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehnend sitz' ich in der Ferne,", "tokens": ["Seh\u00b7nend", "sitz'", "ich", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00e4he wie aus dunkler Nacht", "tokens": ["Sp\u00e4\u00b7he", "wie", "aus", "dunk\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem holden Augensterne,", "tokens": ["Nach", "dem", "hol\u00b7den", "Au\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob er z\u00fcrnet, ob er lacht.", "tokens": ["Ob", "er", "z\u00fcr\u00b7net", ",", "ob", "er", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollt' ich mein Verlangen fragen,", "tokens": ["Wollt'", "ich", "mein", "Ver\u00b7lan\u00b7gen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, dann w\u00fc\u00dft' ich's leicht zu sagen;", "tokens": ["Ach", ",", "dann", "w\u00fc\u00dft'", "ich's", "leicht", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch wenn auch mit sel'gem Licht", "tokens": ["Doch", "wenn", "auch", "mit", "sel'\u00b7gem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Deine Blicke mich erfreuten;", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "mich", "er\u00b7freu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Es zu deuten,", "tokens": ["Es", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Wagt' ich nicht.", "tokens": ["Wagt'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Denn mein Herz ist fromm bescheiden,", "tokens": ["Denn", "mein", "Herz", "ist", "fromm", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wenn du nur fr\u00f6hlich bist,", "tokens": ["Und", "wenn", "du", "nur", "fr\u00f6h\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Will es gern dein Z\u00fcrnen leiden,", "tokens": ["Will", "es", "gern", "dein", "Z\u00fcr\u00b7nen", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sein h\u00f6chstes Leiden ist.", "tokens": ["Das", "sein", "h\u00f6chs\u00b7tes", "Lei\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber w\u00e4r' ihm auch vor Allen", "tokens": ["A\u00b7ber", "w\u00e4r'", "ihm", "auch", "vor", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein begl\u00fcckend Loos gefallen,", "tokens": ["Ein", "be\u00b7gl\u00fc\u00b7ckend", "Loos", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fc\u00dft' es sich von dir erh\u00f6rt,", "tokens": ["W\u00fc\u00dft'", "es", "sich", "von", "dir", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch w\u00fcrd' es sch\u00fcchtern fragen:", "tokens": ["Den\u00b7noch", "w\u00fcrd'", "es", "sch\u00fcch\u00b7tern", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Werd' ich's tragen?", "tokens": ["Werd'", "ich's", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Bin ich's werth?", "tokens": ["Bin", "ich's", "werth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}