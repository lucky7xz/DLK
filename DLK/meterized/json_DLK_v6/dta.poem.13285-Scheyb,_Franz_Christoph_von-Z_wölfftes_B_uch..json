{"dta.poem.13285": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evernimm! ich helffe dir: der Saal bracht an den Tag,", "tokens": ["\u201e", "ver\u00b7nimm", "!", "ich", "helf\u00b7fe", "dir", ":", "der", "Saal", "bracht", "an", "den", "Tag", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "PPER", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "30\u201dWas dieser K\u00f6niginn zum Ruhm gereichen mag.", "tokens": ["\"", "Was", "die\u00b7ser", "K\u00f6\u00b7ni\u00b7ginn", "zum", "Ruhm", "ge\u00b7rei\u00b7chen", "mag."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["$(", "PWS", "PDAT", "NN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201eder Tugenden Gespr\u00e4ch enthielt nichts als ein Streiten,", "tokens": ["\u201e", "der", "Tu\u00b7gen\u00b7den", "Ge\u00b7spr\u00e4ch", "ent\u00b7hielt", "nichts", "als", "ein", "Strei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eum so nachdr\u00fccklicher und kl\u00e4rer auszubreiten:", "tokens": ["\u201e", "um", "so", "nach\u00b7dr\u00fcck\u00b7li\u00b7cher", "und", "kl\u00e4\u00b7rer", "aus\u00b7zu\u00b7brei\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "ADV", "ADJD", "KON", "ADJD", "VVIZU", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "\u201ewie sich ", "tokens": ["\u201e", "wie", "sich"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PWAV", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "\u201edem sieben-fachen Schwert und dessen Wirbelstreich,", "tokens": ["\u201e", "dem", "sie\u00b7ben\u00b7fa\u00b7chen", "Schwert", "und", "des\u00b7sen", "Wir\u00b7bel\u00b7streich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "KON", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "35\u201dDer sich so f\u00fcrchterlich um ihre Krone schwunge,", "tokens": ["\"", "Der", "sich", "so", "f\u00fcrch\u00b7ter\u00b7lich", "um", "ih\u00b7re", "Kro\u00b7ne", "schwun\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df er dieselbige fast von der Scheitel drunge;", "tokens": ["\u201e", "da\u00df", "er", "die\u00b7sel\u00b7bi\u00b7ge", "fast", "von", "der", "Schei\u00b7tel", "drun\u00b7ge", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PDS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "\u201enur von dem Tugend-Schild und Heldenmuth beschirmt,", "tokens": ["\u201e", "nur", "von", "dem", "Tu\u00b7gen\u00b7dSchild", "und", "Hel\u00b7den\u00b7muth", "be\u00b7schirmt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201esonst Hilff- und Waffen-blo\u00df entgegen aufgeth\u00fcrmt;", "tokens": ["\u201e", "sonst", "Hilff", "und", "Waf\u00b7fen\u00b7blo\u00df", "ent\u00b7ge\u00b7gen", "auf\u00b7ge\u00b7th\u00fcrmt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "TRUNC", "KON", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewie sie die Tugenden im Kampf zu brauchen wu\u00dfte,", "tokens": ["\u201e", "wie", "sie", "die", "Tu\u00b7gen\u00b7den", "im", "Kampf", "zu", "brau\u00b7chen", "wu\u00df\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "40\u201dWodurch sie fast allein den Thron behaupten mu\u00dfte.", "tokens": ["\"", "Wo\u00b7durch", "sie", "fast", "al\u00b7lein", "den", "Thron", "be\u00b7haup\u00b7ten", "mu\u00df\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}