{"dta.poem.14940": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeitsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Kahn folgt' sie behende,", "tokens": ["Zum", "Kahn", "folgt'", "sie", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Setzt sich ganz vorne hin,", "tokens": ["Setzt", "sich", "ganz", "vor\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er setzt' sich an das Ende", "tokens": ["Er", "setzt'", "sich", "an", "das", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lie\u00df das Schifflein zieh'n.", "tokens": ["Und", "lie\u00df", "das", "Schif\u00b7flein", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}