{"dta.poem.9371": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Echo zeige mier mein Leben/", "tokens": ["E\u00b7cho", "zei\u00b7ge", "mier", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E. ja dier eben.", "tokens": ["E.", "ja", "dier", "e\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Weistu meine Liebste nicht?", "tokens": ["Weis\u00b7tu", "mei\u00b7ne", "Liebs\u00b7te", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "E. Die dein Licht?", "tokens": ["E.", "Die", "dein", "Licht", "?"], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ja Sie ist mein Licht und Sonne;", "tokens": ["Ja", "Sie", "ist", "mein", "Licht", "und", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "E. Freud\u2019 und wonne?", "tokens": ["E.", "Freud'", "und", "won\u00b7ne", "?"], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PWAV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ja sie ist mein Freuden-schein/", "tokens": ["Ja", "sie", "ist", "mein", "Freu\u00b7den\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "E. Sie ist dein.", "tokens": ["E.", "Sie", "ist", "dein", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Soll ich mich noch l\u00e4nger m\u00fchen?", "tokens": ["Soll", "ich", "mich", "noch", "l\u00e4n\u00b7ger", "m\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E. La\u00df verzihen/", "tokens": ["E.", "La\u00df", "ver\u00b7zi\u00b7hen", "/"], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "VVIMP", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ach! verzihen macht mir Leid/", "tokens": ["Ach", "!", "ver\u00b7zi\u00b7hen", "macht", "mir", "Leid", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVINF", "VVFIN", "PPER", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "E. nein/ die zeit.", "tokens": ["E.", "nein", "/", "die", "zeit", "."], "token_info": ["abbreviation", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Was kann lindern meine schmertzen?", "tokens": ["Was", "kann", "lin\u00b7dern", "mei\u00b7ne", "schmert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "E. Jungfern schertzen.", "tokens": ["E.", "Jung\u00b7fern", "schert\u00b7zen", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ist nun balde da die zeit?", "tokens": ["Ist", "nun", "bal\u00b7de", "da", "die", "zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "E. nicht mehr weit.", "tokens": ["E.", "nicht", "mehr", "weit", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wird mir bald das Leid benommen?", "tokens": ["Wird", "mir", "bald", "das", "Leid", "be\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E. Sie wird kommen.", "tokens": ["E.", "Sie", "wird", "kom\u00b7men", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Soll Sie kommen meine Zier?", "tokens": ["Soll", "Sie", "kom\u00b7men", "mei\u00b7ne", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "E. ja zu dier.", "tokens": ["E.", "ja", "zu", "dier", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PDAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ach der tag wil schon verflie\u00dfen/", "tokens": ["Ach", "der", "tag", "wil", "schon", "ver\u00b7flie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "E. la\u00df sie gr\u00fc\u00dfen/", "tokens": ["E.", "la\u00df", "sie", "gr\u00fc\u00b7\u00dfen", "/"], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ey so bring\u2019 jhr meinen gru\u00df/", "tokens": ["Ey", "so", "bring'", "jhr", "mei\u00b7nen", "gru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "E. wenn ich mu\u00df?", "tokens": ["E.", "wenn", "ich", "mu\u00df", "?"], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ach wo seyn die langen stunden?", "tokens": ["Ach", "wo", "seyn", "die", "lan\u00b7gen", "stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "PPOSAT", "ART", "ADJA", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E. gantz verschwunden.", "tokens": ["E.", "gantz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist dann schier der schmertz vorbey?", "tokens": ["Ist", "dann", "schier", "der", "schmertz", "vor\u00b7bey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "E. Du bist frey.", "tokens": ["E.", "Du", "bist", "frey", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ach wenn k\u00f6mmet mein Verlangen", "tokens": ["Ach", "wenn", "k\u00f6m\u00b7met", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "E. Sih die Wangen.", "tokens": ["E.", "Sih", "die", "Wan\u00b7gen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ist dann dis mein Wunder-Licht?", "tokens": ["Ist", "dann", "dis", "mein", "Wun\u00b7der\u00b7Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "E. Sihst es nicht?", "tokens": ["E.", "Sihst", "es", "nicht", "?"], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wohl! ich wil Sie auch empfangen/", "tokens": ["Wohl", "!", "ich", "wil", "Sie", "auch", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E. Dein verlangen?", "tokens": ["E.", "Dein", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wil sie k\u00fcssen unverwandt/", "tokens": ["Wil", "sie", "k\u00fcs\u00b7sen", "un\u00b7ver\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "E. Mund und hand.", "tokens": ["E.", "Mund", "und", "hand", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ey nun lieg ich in den armen/", "tokens": ["Ey", "nun", "lieg", "ich", "in", "den", "ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "E. zu erwarmen/", "tokens": ["E.", "zu", "er\u00b7war\u00b7men", "/"], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Bi\u00df der N\u00e4chte Licht verbleicht/", "tokens": ["Bi\u00df", "der", "N\u00e4ch\u00b7te", "Licht", "ver\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "E. und entweicht.", "tokens": ["E.", "und", "ent\u00b7weicht", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Nun wil ich die nacht verjagen/", "tokens": ["Nun", "wil", "ich", "die", "nacht", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "E. Schmertz und klagen.", "tokens": ["E.", "Schmertz", "und", "kla\u00b7gen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nun bring\u2019 ich in fr\u00f6ligkeit", "tokens": ["Nun", "bring'", "ich", "in", "fr\u00f6\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "E. zu die zeit.", "tokens": ["E.", "zu", "die", "zeit", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wil mich in dem s\u00fc\u00dfen Lieben", "tokens": ["Wil", "mich", "in", "dem", "s\u00fc\u00b7\u00dfen", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "E. stetig \u00fcben.", "tokens": ["E.", "ste\u00b7tig", "\u00fc\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dr\u00fcmb ade du schmertz und Leid/", "tokens": ["Dr\u00fcmb", "a\u00b7de", "du", "schmertz", "und", "Leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "KON", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "E. weiche weit.", "tokens": ["E.", "wei\u00b7che", "weit", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Hiemit wil ich dieses schlie\u00dfen/", "tokens": ["Hie\u00b7mit", "wil", "ich", "die\u00b7ses", "schlie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E. Dich zu gr\u00fc\u00dfen/", "tokens": ["E.", "Dich", "zu", "gr\u00fc\u00b7\u00dfen", "/"], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Edles Bild voll freundligkeit/", "tokens": ["Ed\u00b7les", "Bild", "voll", "freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "E. dieser zeit.", "tokens": ["E.", "die\u00b7ser", "zeit", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Echo leb\u2019 in gr\u00fcnen heyden/", "tokens": ["E\u00b7cho", "leb'", "in", "gr\u00fc\u00b7nen", "hey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "E. Jhr in freuden.", "tokens": ["E.", "Ihr", "in", "freu\u00b7den", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wir seyn nur auf Lust bedacht/", "tokens": ["Wir", "seyn", "nur", "auf", "Lust", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "E. diese Nacht.", "tokens": ["E.", "die\u00b7se", "Nacht", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}