{"dta.poem.19427": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "160.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beklagen sollt' ich dich? ich kann dich nur beneiden,", "tokens": ["Be\u00b7kla\u00b7gen", "sollt'", "ich", "dich", "?", "ich", "kann", "dich", "nur", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nicht jedwedem wird gegeben so zu scheiden,", "tokens": ["Denn", "nicht", "jed\u00b7we\u00b7dem", "wird", "ge\u00b7ge\u00b7ben", "so", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "VAFIN", "VVPP", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie du geschieden bist, mit Gott und Welt in Frieden,", "tokens": ["Wie", "du", "ge\u00b7schie\u00b7den", "bist", ",", "mit", "Gott", "und", "Welt", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ohne Schmerz und Weh von Weh und Schmerz geschieden.", "tokens": ["So", "oh\u00b7ne", "Schmerz", "und", "Weh", "von", "Weh", "und", "Schmerz", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Himmels Ruh verkl\u00e4rt dein Todtenangesicht;", "tokens": ["Des", "Him\u00b7mels", "Ruh", "ver\u00b7kl\u00e4rt", "dein", "Tod\u00b7ten\u00b7an\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00e4re sie gew\u00e4hrt dem sel'gen Geiste nicht?", "tokens": ["Und", "w\u00e4\u00b7re", "sie", "ge\u00b7w\u00e4hrt", "dem", "sel'\u00b7gen", "Geis\u00b7te", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es wird mir still zu Muth, ins Antlitz dir zu sehn,", "tokens": ["Es", "wird", "mir", "still", "zu", "Muth", ",", "ins", "Ant\u00b7litz", "dir", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und herzlich w\u00fcnsch' ich, m\u00f6g' auch mir einst so geschehn.", "tokens": ["Und", "herz\u00b7lich", "w\u00fcn\u00b7sch'", "ich", ",", "m\u00f6g'", "auch", "mir", "einst", "so", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "VMFIN", "ADV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}