{"dta.poem.20040": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Zwey S\u00e4rge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey S\u00e4rge einsam stehen", "tokens": ["Zwey", "S\u00e4r\u00b7ge", "ein\u00b7sam", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In des alten Domes Hut,", "tokens": ["In", "des", "al\u00b7ten", "Do\u00b7mes", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nig Ottmar liegt in dem einen,", "tokens": ["K\u00f6\u00b7nig", "Ott\u00b7mar", "liegt", "in", "dem", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "ART", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In dem andern der S\u00e4nger ruht.", "tokens": ["In", "dem", "an\u00b7dern", "der", "S\u00e4n\u00b7ger", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig sa\u00df einst m\u00e4chtig", "tokens": ["Der", "K\u00f6\u00b7nig", "sa\u00df", "einst", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch auf der V\u00e4ter Thron,", "tokens": ["Hoch", "auf", "der", "V\u00e4\u00b7ter", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm liegt das Schwerdt in der Rechten", "tokens": ["Ihm", "liegt", "das", "Schwerdt", "in", "der", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und auf dem Haupte die Kron'.", "tokens": ["Und", "auf", "dem", "Haup\u00b7te", "die", "Kron'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Doch neben dem stolzen K\u00f6nig", "tokens": ["Doch", "ne\u00b7ben", "dem", "stol\u00b7zen", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da liegt der S\u00e4nger traut,", "tokens": ["Da", "liegt", "der", "S\u00e4n\u00b7ger", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man noch in seinen H\u00e4nden", "tokens": ["Man", "noch", "in", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die fromme Harfe schaut.", "tokens": ["Die", "from\u00b7me", "Har\u00b7fe", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Burgen rings zerfallen,", "tokens": ["Die", "Bur\u00b7gen", "rings", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlachtruf t\u00f6nt durch das Land,", "tokens": ["Schlach\u00b7truf", "t\u00f6nt", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Schwerdt das regt sich nimmer", "tokens": ["Das", "Schwerdt", "das", "regt", "sich", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da in des K\u00f6nigs Hand.", "tokens": ["Da", "in", "des", "K\u00f6\u00b7nigs", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bl\u00fcthen und milde L\u00fcfte", "tokens": ["Bl\u00fc\u00b7then", "und", "mil\u00b7de", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wehen das Thal entlang \u2014", "tokens": ["We\u00b7hen", "das", "Thal", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPO", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des S\u00e4ngers Harfe t\u00f6net", "tokens": ["Des", "S\u00e4n\u00b7gers", "Har\u00b7fe", "t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ewigem Gesang.", "tokens": ["In", "e\u00b7wi\u00b7gem", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}