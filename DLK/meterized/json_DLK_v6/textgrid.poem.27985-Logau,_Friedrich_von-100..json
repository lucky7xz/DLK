{"textgrid.poem.27985": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kummet her und kauffet ein", "tokens": ["Kum\u00b7met", "her", "und", "kauf\u00b7fet", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar umsonsten Milch und Wein!", "tokens": ["Gar", "um\u00b7sons\u00b7ten", "Milch", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Pflegt ein Dorff-Pfarr stets zu sagen;", "tokens": ["Pflegt", "ein", "Dorff\u00b7Pfarr", "stets", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolte gleichwol sich beklagen,", "tokens": ["Wol\u00b7te", "gleich\u00b7wol", "sich", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann ihm nicht daf\u00fcr kam ein", "tokens": ["Wann", "ihm", "nicht", "da\u00b7f\u00fcr", "kam", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "PAV", "VVFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fette Milch und edler Wein.", "tokens": ["Fet\u00b7te", "Milch", "und", "ed\u00b7ler", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}