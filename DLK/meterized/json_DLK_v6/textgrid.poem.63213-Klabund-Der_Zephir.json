{"textgrid.poem.63213": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Zephir", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er gehet befl\u00fcgelt", "tokens": ["Er", "ge\u00b7het", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und l\u00e4utet am H\u00fcgel.", "tokens": ["Und", "l\u00e4u\u00b7tet", "am", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es streifen die Sohlen", "tokens": ["Es", "strei\u00b7fen", "die", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die frauliche Au.", "tokens": ["Die", "frau\u00b7li\u00b7che", "Au", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nun d\u00fcrfen wir schl\u00fcrfen", "tokens": ["Nun", "d\u00fcr\u00b7fen", "wir", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Auge den Abend", "tokens": ["Im", "Au\u00b7ge", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und Erde und Herde", "tokens": ["Und", "Er\u00b7de", "und", "Her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ertrinken im Tau.", "tokens": ["Er\u00b7trin\u00b7ken", "im", "Tau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Ich wende die H\u00e4nde", "tokens": ["Ich", "wen\u00b7de", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die feuchten ins Leuchten,", "tokens": ["Die", "feuch\u00b7ten", "ins", "Leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Aufs Herz mir gez\u00fcckt schon", "tokens": ["Aufs", "Herz", "mir", "ge\u00b7z\u00fcckt", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVPP", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Des Mondes Stilett.", "tokens": ["Des", "Mon\u00b7des", "Sti\u00b7lett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+---", "measure": "dactylic.init"}, "line.13": {"text": "Die z\u00e4rtlichen Winde", "tokens": ["Die", "z\u00e4rt\u00b7li\u00b7chen", "Win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Umfangen den Enkel.", "tokens": ["Um\u00b7fan\u00b7gen", "den", "En\u00b7kel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Er gleitet begl\u00fcckt schon.", "tokens": ["Er", "glei\u00b7tet", "be\u00b7gl\u00fcckt", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Sie f\u00fchren ihn linde", "tokens": ["Sie", "f\u00fch\u00b7ren", "ihn", "lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Ins ewige Bett.", "tokens": ["Ins", "e\u00b7wi\u00b7ge", "Bett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}