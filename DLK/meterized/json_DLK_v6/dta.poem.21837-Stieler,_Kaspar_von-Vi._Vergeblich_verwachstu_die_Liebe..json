rd", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kan es auch genugsam sein:", "tokens": ["kan", "es", "auch", "ge\u00b7nug\u00b7sam", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df sie mir nicht manches St\u00fcndchen/", "tokens": ["da\u00df", "sie", "mir", "nicht", "man\u00b7ches", "St\u00fcnd\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wenn/ du Gramhafft/ nicht bist dar/", "tokens": ["wenn", "/", "du", "Gram\u00b7hafft", "/", "nicht", "bist", "dar", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PPER", "NN", "$(", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "g\u00f6nn\u2019 ihr zartes Wollen m\u00fcndchen", "tokens": ["g\u00f6nn'", "ihr", "zar\u00b7tes", "Wol\u00b7len", "m\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nu nur hielt\u2019 ich sie umfangen/", "tokens": ["Nu", "nur", "hielt'", "ich", "sie", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mein Mund k\u00fc\u00dfet\u2019 ihren Mund/", "tokens": ["mein", "Mund", "k\u00fc\u00b7\u00dfet'", "ih\u00b7ren", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Jhre Wangen meine Wangen:", "tokens": ["Ih\u00b7re", "Wan\u00b7gen", "mei\u00b7ne", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weistu das auff diese Stund\u2019?", "tokens": ["Weis\u00b7tu", "das", "auff", "die\u00b7se", "Stund'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "PDAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ach! was woltestu doch rathen/", "tokens": ["Ach", "!", "was", "wol\u00b7tes\u00b7tu", "doch", "ra\u00b7then", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VMFIN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was auff einen Abend spat", "tokens": ["was", "auff", "ei\u00b7nen", "A\u00b7bend", "spat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "in geheim wir beyde tahten/", "tokens": ["in", "ge\u00b7heim", "wir", "bey\u00b7de", "tah\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "als sie mich/ wie folget baht.", "tokens": ["als", "sie", "mich", "/", "wie", "fol\u00b7get", "baht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$(", "PWAV", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herzgen/ wo ich so darff nennen", "tokens": ["Herz\u00b7gen", "/", "wo", "ich", "so", "darff", "nen\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "dich mein allertrautstes Kind/", "tokens": ["dich", "mein", "al\u00b7ler\u00b7trauts\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kanstu meine Brunst erkennen", "tokens": ["kans\u00b7tu", "mei\u00b7ne", "Brunst", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sich t\u00e4glich mehr entz\u00fcndt.", "tokens": ["die", "sich", "t\u00e4g\u00b7lich", "mehr", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey/ so linder meine Flammen", "tokens": ["Ey", "/", "so", "lin\u00b7der", "mei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "la\u00df mich deine Liebste sein.", "tokens": ["la\u00df", "mich", "dei\u00b7ne", "Liebs\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jezt sind wir allein beysammen", "tokens": ["Jezt", "sind", "wir", "al\u00b7lein", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "was du wilst