"auffs", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir versprachen sonder Ende", "tokens": ["Wir", "ver\u00b7spra\u00b7chen", "son\u00b7der", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "uns mit Schw\u00fcren stete Treu.", "tokens": ["uns", "mit", "Schw\u00fc\u00b7ren", "ste\u00b7te", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nu/ was denkstu/ wenn damahlen", "tokens": ["Nu", "/", "was", "denks\u00b7tu", "/", "wenn", "da\u00b7mah\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$(", "PWS", "VVFIN", "$(", "KOUS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ich ihr h\u00e4tte gut gemacht/", "tokens": ["ich", "ihr", "h\u00e4t\u00b7te", "gut", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "was Verliebte k\u00f6nnen zahlen/", "tokens": ["was", "Ver\u00b7lieb\u00b7te", "k\u00f6n\u00b7nen", "zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "h\u00e4ttestu sie auch bewacht?", "tokens": ["h\u00e4t\u00b7tes\u00b7tu", "sie", "auch", "be\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "daru