{"dta.poem.21837": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Vergeblich verwachstu die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Alter/ ich mu\u00df deiner lachen/", "tokens": ["Al\u00b7ter", "/", "ich", "mu\u00df", "dei\u00b7ner", "la\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VMFIN", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du wilt des Amors Gluht", "tokens": ["da\u00df", "du", "wilt", "des", "A\u00b7mors", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "durch dein Auffsehn k\u00e4lter machen/", "tokens": ["durch", "dein", "Auff\u00b7sehn", "k\u00e4l\u00b7ter", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe tuht doch/ wie sie tuht.", "tokens": ["Lie\u00b7be", "tuht", "doch", "/", "wie", "sie", "tuht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wacht man sie/ sie birgt ihr Feuer", "tokens": ["Wacht", "man", "sie", "/", "sie", "birgt", "ihr", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIS", "PPER", "$(", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Griffgen sind bey ihr nicht teuer.", "tokens": ["Griff\u00b7gen", "sind", "bey", "ihr", "nicht", "teu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Du verbeutst/ und weist nicht was.", "tokens": ["Du", "ver\u00b7beutst", "/", "und", "weist", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie wei\u00df mich doch wol zu halten", "tokens": ["Sie", "wei\u00df", "mich", "doch", "wol", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "meine sch\u00f6nst\u2019 Argine.", "tokens": ["mei\u00b7ne", "sch\u00f6nst'", "Ar\u00b7gi\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du machst wie du wilst verwalten", "tokens": ["Du", "machst", "wie", "du", "wilst", "ver\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "W\u00e4chters Amt. Uns thut nicht weh:", "tokens": ["W\u00e4ch\u00b7ters", "Amt", ".", "Uns", "thut", "nicht", "weh", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df du uns mit deinem schleiche