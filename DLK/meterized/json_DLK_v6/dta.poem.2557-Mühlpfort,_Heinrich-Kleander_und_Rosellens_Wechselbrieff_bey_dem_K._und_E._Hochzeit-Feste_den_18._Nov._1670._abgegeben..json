{"dta.poem.2557": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kleander und Rosellens Wechselbrieff bey  \n dem K. und E. Hochzeit-Feste den 18. Nov.  \n 1670. abgegeben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Licht/ ob gleich mein Brief mit Blumen nicht ge-\nschm\u00fccket/", "tokens": ["Mein", "Licht", "/", "ob", "gleich", "mein", "Brief", "mit", "Blu\u00b7men", "nicht", "ge", "schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "TRUNC", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schlechtes Wintergr\u00fcn zu seiner Kleidung hat/", "tokens": ["Und", "schlech\u00b7tes", "Win\u00b7ter\u00b7gr\u00fcn", "zu", "sei\u00b7ner", "Klei\u00b7dung", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kennstu doch die Glut/ die aus den Zeilen blicket", "tokens": ["So", "kenns\u00b7tu", "doch", "die", "Glut", "/", "die", "aus", "den", "Zei\u00b7len", "bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr roth als eine Nelck/ hell als ein Rosenblat.", "tokens": ["Mehr", "roth", "als", "ei\u00b7ne", "Nelck", "/", "hell", "als", "ein", "Ro\u00b7sen\u00b7blat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$(", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es hat die Flora zwar die Bilder ihrer Auen/", "tokens": ["Es", "hat", "die", "Flo\u00b7ra", "zwar", "die", "Bil\u00b7der", "ih\u00b7rer", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der G\u00e4rte Bluhmenwerck entzogen unsrer Lust/", "tokens": ["Der", "G\u00e4r\u00b7te", "Bluh\u00b7men\u00b7werck", "ent\u00b7zo\u00b7gen", "uns\u00b7rer", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch dennoch heist die Lieb ein Paradi\u00df aufbauen/", "tokens": ["Noch", "den\u00b7noch", "heist", "die", "Lieb", "ein", "Pa\u00b7ra\u00b7di\u00df", "auf\u00b7bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem die Ver\u00e4nderung der Zeiten nicht bewust.", "tokens": ["Dem", "die", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "der", "Zei\u00b7ten", "nicht", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So mu\u00df der Winter auch zu einem Fr\u00fchling werden/", "tokens": ["So", "mu\u00df", "der", "Win\u00b7ter", "auch", "zu", "ei\u00b7nem", "Fr\u00fch\u00b7ling", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und in dem Hertzen stets ein warmer Sommer seyn/", "tokens": ["Und", "in", "dem", "Hert\u00b7zen", "stets", "ein", "war\u00b7mer", "Som\u00b7mer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja deiner Augenstral/ und freundliche Geberden/", "tokens": ["Ja", "dei\u00b7ner", "Au\u00b7gen\u00b7stral", "/", "und", "freund\u00b7li\u00b7che", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sind mir mehr angenehm als aller Sonnenschein.", "tokens": ["Sind", "mir", "mehr", "an\u00b7ge\u00b7nehm", "als", "al\u00b7ler", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bald nur deine Hand wird meinen Brieff ber\u00fchren/", "tokens": ["So", "bald", "nur", "dei\u00b7ne", "Hand", "wird", "mei\u00b7nen", "Brieff", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ob gleich die strenge Lufft schon mit Gefr\u00f6ste dreut/", "tokens": ["Ob", "gleich", "die", "stren\u00b7ge", "Lufft", "schon", "mit", "Ge\u00b7fr\u00f6s\u00b7te", "dreut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wirstu gar gewi\u00df/ wie ich mu\u00df brennen/ sp\u00fcren/", "tokens": ["So", "wirs\u00b7tu", "gar", "ge\u00b7wi\u00df", "/", "wie", "ich", "mu\u00df", "bren\u00b7nen", "/", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$(", "PWAV", "PPER", "VMFIN", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie dem ", "tokens": ["Und", "wie", "dem"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Doch kanstu ohn Gefahr er\u00f6ffnen was geschrieben/", "tokens": ["Doch", "kans\u00b7tu", "ohn", "Ge\u00b7fahr", "er\u00b7\u00f6ff\u00b7nen", "was", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hier hemmt uns kein Verbot/ noch sch\u00e4les Angesicht/", "tokens": ["Hier", "hemmt", "uns", "kein", "Ver\u00b7bot", "/", "noch", "sch\u00e4\u00b7les", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$(", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der gantze Innhalt ist/ ich mu\u00df dich ewig lieben/", "tokens": ["Der", "gant\u00b7ze", "Inn\u00b7halt", "ist", "/", "ich", "mu\u00df", "dich", "e\u00b7wig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$(", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein Dencken und mein Thun ist blo\u00df auf dich gericht/", "tokens": ["Mein", "Den\u00b7cken", "und", "mein", "Thun", "ist", "blo\u00df", "auf", "dich", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und heute kommt der Tag der uns wird mehr verbinden/", "tokens": ["Und", "heu\u00b7te", "kommt", "der", "Tag", "der", "uns", "wird", "mehr", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und dieses B\u00fcndn\u00fcsses ein kr\u00e4fftig Zeuge seyn:", "tokens": ["Und", "die\u00b7ses", "B\u00fcnd\u00b7n\u00fcs\u00b7ses", "ein", "kr\u00e4ff\u00b7tig", "Zeu\u00b7ge", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jhr Buhler jener Zeit m\u00f6gt Eyd und Schwur erfinden/", "tokens": ["Ihr", "Buh\u00b7ler", "je\u00b7ner", "Zeit", "m\u00f6gt", "Eyd", "und", "Schwur", "er\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "VMFIN", "NN", "KON", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er trifft an Heiligkeit mit dem nicht \u00fcberein.", "tokens": ["Er", "trifft", "an", "Hei\u00b7lig\u00b7keit", "mit", "dem", "nicht", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PRELS", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.25": {"text": "Ist nicht von jener Brunst nur lauter Asche kommen?", "tokens": ["Ist", "nicht", "von", "je\u00b7ner", "Brunst", "nur", "lau\u00b7ter", "A\u00b7sche", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wird nicht der Dido Schwerd/ der Sappho Fel\u00df verlacht?", "tokens": ["Wird", "nicht", "der", "Di\u00b7do", "Schwerd", "/", "der", "Sap\u00b7pho", "Fel\u00df", "ver\u00b7lacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NE", "NE", "$(", "ART", "NE", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist nicht ihr Lebens-Tacht im h\u00f6chsten Schimpff verglommen?", "tokens": ["Ist", "nicht", "ihr", "Le\u00b7bens\u00b7\u00b7T\u00b7acht", "im", "h\u00f6chs\u00b7ten", "Schimpff", "ver\u00b7glom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Ertranck Leander nicht in Wellen bey der Nacht?", "tokens": ["Er\u00b7tranck", "Le\u00b7an\u00b7der", "nicht", "in", "Wel\u00b7len", "bey", "der", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nein/ gar ein ander Trieb regieret unsre Geister/", "tokens": ["Nein", "/", "gar", "ein", "an\u00b7der", "Trieb", "re\u00b7gie\u00b7ret", "uns\u00b7re", "Geis\u00b7ter", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es brennt ein st\u00e4rcker Arm die Hochzeit-Fackeln an.", "tokens": ["Es", "brennt", "ein", "st\u00e4r\u00b7cker", "Arm", "die", "Hoch\u00b7zeit\u00b7Fa\u00b7ckeln", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Menschen erst erschuff/ und auch der Menschen Meister", "tokens": ["Der", "Men\u00b7schen", "erst", "er\u00b7schuff", "/", "und", "auch", "der", "Men\u00b7schen", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$(", "KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat Oele reiner Zucht zu diesem Feur gethan.", "tokens": ["Hat", "O\u00b7e\u00b7le", "rei\u00b7ner", "Zucht", "zu", "die\u00b7sem", "Feur", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.33": {"text": "Das selbst de\u00df Priesters Mund wird heut and\u00e4chtig segnen/", "tokens": ["Das", "selbst", "de\u00df", "Pries\u00b7ters", "Mund", "wird", "heut", "an\u00b7d\u00e4ch\u00b7tig", "seg\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das nimmermehr verlescht und keinen Zufall kennt/", "tokens": ["Das", "nim\u00b7mer\u00b7mehr", "ver\u00b7lescht", "und", "kei\u00b7nen", "Zu\u00b7fall", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "In solchen Flammen wil ich dir mein Schatz begegnen/", "tokens": ["In", "sol\u00b7chen", "Flam\u00b7men", "wil", "ich", "dir", "mein", "Schatz", "be\u00b7geg\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bi\u00df da\u00df der blasse Tod die Lebens-Geister trennt.", "tokens": ["Bi\u00df", "da\u00df", "der", "blas\u00b7se", "Tod", "die", "Le\u00b7bens\u00b7Geis\u00b7ter", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jedoch du kennst mein Hertz/ was sol ichs mehr entdecken/", "tokens": ["Je\u00b7doch", "du", "kennst", "mein", "Hertz", "/", "was", "sol", "ichs", "mehr", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "PWS", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Du weist ja wie du bist versichert meiner Treu/", "tokens": ["Du", "weist", "ja", "wie", "du", "bist", "ver\u00b7si\u00b7chert", "mei\u00b7ner", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Lieb und Verlangen sinds so diesen Wunsch erwecken/", "tokens": ["Lieb", "und", "Ver\u00b7lan\u00b7gen", "sinds", "so", "die\u00b7sen", "Wunsch", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.40": {"text": "Da\u00df meine liebste Braut doch bald zu Bette sey.", "tokens": ["Da\u00df", "mei\u00b7ne", "liebs\u00b7te", "Braut", "doch", "bald", "zu", "Bet\u00b7te", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Music s\u00fcsser Klang/ der Safft von edlen Reben/", "tokens": ["Der", "Mu\u00b7sic", "s\u00fcs\u00b7ser", "Klang", "/", "der", "Safft", "von", "ed\u00b7len", "Re\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", 