{"dta.poem.18531": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines neuen Orbilii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wird nun endlich deine hand", "tokens": ["So", "wird", "nun", "end\u00b7lich", "dei\u00b7ne", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gescharret in den d\u00fcrren sand/", "tokens": ["Ge\u00b7schar\u00b7ret", "in", "den", "d\u00fcr\u00b7ren", "sand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die hier so oft und wohl gestrichen/", "tokens": ["Die", "hier", "so", "oft", "und", "wohl", "ge\u00b7stri\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es steht der kleinen knaben schaar", "tokens": ["Es", "steht", "der", "klei\u00b7nen", "kna\u00b7ben", "schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um deine schwartze todten-baar", "tokens": ["Um", "dei\u00b7ne", "schwart\u00b7ze", "tod\u00b7ten\u00b7baar"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und klagt: ihr lehrer sey verblichen.", "tokens": ["Und", "klagt", ":", "ihr", "leh\u00b7rer", "sey", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast manch nacktes angesicht", "tokens": ["Du", "hast", "manch", "nack\u00b7tes", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gestellet an das helle licht/", "tokens": ["Ge\u00b7stel\u00b7let", "an", "das", "hel\u00b7le", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und dessen bl\u00f6sse wohl besehen/", "tokens": ["Und", "des\u00b7sen", "bl\u00f6s\u00b7se", "wohl", "be\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du f\u00fchrtst die rute meisterlich/", "tokens": ["Du", "f\u00fchrtst", "die", "ru\u00b7te", "meis\u00b7ter\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Obgleich der knabe kr\u00fcmte sich/", "tokens": ["Ob\u00b7gleich", "der", "kna\u00b7be", "kr\u00fcm\u00b7te", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So halff doch kein erb\u00e4rmlich flehen.", "tokens": ["So", "halff", "doch", "kein", "er\u00b7b\u00e4rm\u00b7lich", "fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht einer/ den du hast gelehrt/", "tokens": ["Nicht", "ei\u00b7ner", "/", "den", "du", "hast", "ge\u00b7lehrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "$(", "ART", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der dir unflei\u00dfig zugeh\u00f6rt/", "tokens": ["Der", "dir", "un\u00b7flei\u00b7\u00dfig", "zu\u00b7ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Kan sagen/ da\u00df er sey entgangen:", "tokens": ["Kan", "sa\u00b7gen", "/", "da\u00df", "er", "sey", "ent\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$(", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Du hast niemahls den flei\u00df gespart/", "tokens": ["Du", "hast", "nie\u00b7mahls", "den", "flei\u00df", "ge\u00b7spart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Mit deiner seltnen wunder-art", "tokens": ["Mit", "dei\u00b7ner", "selt\u00b7nen", "wun\u00b7der\u00b7art"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Zu f\u00e4rben beyde hinter-wangen.", "tokens": ["Zu", "f\u00e4r\u00b7ben", "bey\u00b7de", "hin\u00b7ter\u00b7wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Bald muste dir ein knabe knien/", "tokens": ["Bald", "mus\u00b7te", "dir", "ein", "kna\u00b7be", "kni\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die hosen auf die erde ziehn/", "tokens": ["Die", "ho\u00b7sen", "auf", "die", "er\u00b7de", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df du den fu\u00df drein kontest setzen;", "tokens": ["Da\u00df", "du", "den", "fu\u00df", "drein", "kon\u00b7test", "set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bald stunden sie an einer reih/", "tokens": ["Bald", "stun\u00b7den", "sie", "an", "ei\u00b7ner", "reih", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bey iedem schmitz war ein geschrey/", "tokens": ["Bey", "ie\u00b7dem", "schmitz", "war", "ein", "ge\u00b7schrey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Das konte kr\u00e4fftig dich erg\u00f6tzen.", "tokens": ["Das", "kon\u00b7te", "kr\u00e4ff\u00b7tig", "dich", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Den steltest du auf eine banck/", "tokens": ["Den", "stel\u00b7test", "du", "auf", "ei\u00b7ne", "banck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und wenn die rute war geschwanck/", "tokens": ["Und", "wenn", "die", "ru\u00b7te", "war", "ge\u00b7schwanck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Umflochte sie fast gar die lenden/", "tokens": ["Um\u00b7floch\u00b7te", "sie", "fast", "gar", "die", "len\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Weh denen die aus trotzigkeit", "tokens": ["Weh", "de\u00b7nen", "die", "aus", "trot\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRELS", "ART", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Verkehrten irgend ihre seit", "tokens": ["Ver\u00b7kehr\u00b7ten", "ir\u00b7gend", "ih\u00b7re", "seit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und wolten dir den \u2012 \u2012 \u2012 \u2012 entwenden.", "tokens": ["Und", "wol\u00b7ten", "dir", "den", "\u2012", "\u2012", "\u2012", "\u2012", "ent\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "$(", "$(", "$(", "$(", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Nun/ werther lehrer/ schlafe wohl/", "tokens": ["Nun", "/", "wert\u00b7her", "leh\u00b7rer", "/", "schla\u00b7fe", "wohl", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die knaben wollen ein Mausol", "tokens": ["Die", "kna\u00b7ben", "wol\u00b7len", "ein", "Mau\u00b7sol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Auf deine grabes-stelle bauen/", "tokens": ["Auf", "dei\u00b7ne", "gra\u00b7bes\u00b7stel\u00b7le", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Entbl\u00f6st da einer sein gesicht/", "tokens": ["Ent\u00b7bl\u00f6st", "da", "ei\u00b7ner", "sein", "ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "So schwer ich du enth\u00e4ltst dich nicht/", "tokens": ["So", "schwer", "ich", "du", "ent\u00b7h\u00e4ltst", "dich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Du wirst ihn auch noch tod beschauen.", "tokens": ["Du", "wirst", "ihn", "auch", "noch", "tod", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}