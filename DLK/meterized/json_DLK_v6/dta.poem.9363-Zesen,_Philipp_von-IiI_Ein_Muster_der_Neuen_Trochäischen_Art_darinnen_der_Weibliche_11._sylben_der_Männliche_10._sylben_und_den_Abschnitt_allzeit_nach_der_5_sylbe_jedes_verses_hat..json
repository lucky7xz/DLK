{"dta.poem.9363": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "IiI  \n  Ein Muster der Neuen Troch\u00e4ischen  \n Art/ darinnen der Weibliche 11. sylben/ der M\u00e4nnliche  \n 10. sylben und den Abschnitt allzeit nach der  \n 5 sylbe jedes verses hat.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Held/ vor dessen macht Erd' und Himmel zittern/", "tokens": ["Held", "/", "vor", "des\u00b7sen", "macht", "Erd'", "und", "Him\u00b7mel", "zit\u00b7tern", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PDS", "VVFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und das wilde Meer seinen stoltz legt ein/", "tokens": ["und", "das", "wil\u00b7de", "Meer", "sei\u00b7nen", "stoltz", "legt", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "ART", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wenn sich regt dein fu\u00df/ mu\u00df die Erd\u2019 ersch\u00fcttern/", "tokens": ["wenn", "sich", "regt", "dein", "fu\u00df", "/", "mu\u00df", "die", "Erd'", "er\u00b7sch\u00fct\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PPOSAT", "PTKVZ", "$(", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und der Angel-stern bla\u00df und dunckel seyn.", "tokens": ["und", "der", "An\u00b7gel\u00b7stern", "bla\u00df", "und", "dun\u00b7ckel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ach! wie darffstu mier/ mier die gunst erzeigen/", "tokens": ["Ach", "!", "wie", "darffs\u00b7tu", "mier", "/", "mier", "die", "gunst", "er\u00b7zei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VMFIN", "PPER", "$(", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "solche gro\u00dfe gunst die zu hoch vor mich/", "tokens": ["sol\u00b7che", "gro\u00b7\u00dfe", "gunst", "die", "zu", "hoch", "vor", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "PTKA", "ADJD", "APPR", "PPER", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ehrestu dann den/ der sich dier sol beugen", "tokens": ["Eh\u00b7res\u00b7tu", "dann", "den", "/", "der", "sich", "dier", "sol", "beu\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "$(", "PRELS", "PRF", "PDS", "VMFIN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "und mit Demuth selbst hoch verehren dich?", "tokens": ["und", "mit", "De\u00b7muth", "selbst", "hoch", "ver\u00b7eh\u00b7ren", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Deine Sanfftmuth ists/ die der Himmel liebet/", "tokens": ["Dei\u00b7ne", "Sanfft\u00b7muth", "ists", "/", "die", "der", "Him\u00b7mel", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$(", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "deine Demuht selbst zieret dich so sch\u00f6n/", "tokens": ["dei\u00b7ne", "De\u00b7muht", "selbst", "zie\u00b7ret", "dich", "so", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "die der H\u00f6chste dier reichlich schenckt und giebet/", "tokens": ["die", "der", "H\u00f6chs\u00b7te", "dier", "reich\u00b7lich", "schenckt", "und", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "VVFIN", "KON", "VVFIN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "dar\u00fcmb soll dein Lob nimmer untergehn.", "tokens": ["da\u00b7r\u00fcmb", "soll", "dein", "Lob", "nim\u00b7mer", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}