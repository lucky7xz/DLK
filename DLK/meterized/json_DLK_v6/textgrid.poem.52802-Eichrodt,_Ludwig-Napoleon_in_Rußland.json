{"textgrid.poem.52802": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Napoleon in Ru\u00dfland", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Herrschaft auf der Erde", "tokens": ["Um", "die", "Herr\u00b7schaft", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hub mit seinem Siegerschwerdte", "tokens": ["Hub", "mit", "sei\u00b7nem", "Sie\u00b7ger\u00b7schwerd\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit f\u00fcrchterlichem Droh'n", "tokens": ["Und", "mit", "f\u00fcrch\u00b7ter\u00b7li\u00b7chem", "Droh'n"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich sich Napoleon:", "tokens": ["End\u00b7lich", "sich", "Na\u00b7po\u00b7le\u00b7on", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Sturmwind mit dem Meere", "tokens": ["Wie", "der", "Sturm\u00b7wind", "mit", "dem", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zog der ruhmgekr\u00f6nte Held", "tokens": ["Zog", "der", "ruhm\u00b7ge\u00b7kr\u00f6n\u00b7te", "Held"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem ungeheuren Heere", "tokens": ["Mit", "dem", "un\u00b7ge\u00b7heu\u00b7ren", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gegen Ru\u00dfland in das Feld.", "tokens": ["Ge\u00b7gen", "Ru\u00df\u00b7land", "in", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sie konnten sich nicht helfen", "tokens": ["Und", "sie", "konn\u00b7ten", "sich", "nicht", "hel\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Schafe vor den W\u00f6lfen", "tokens": ["Wie", "die", "Scha\u00b7fe", "vor", "den", "W\u00f6l\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Floh das tapfre Russenheer", "tokens": ["Floh", "das", "tapf\u00b7re", "Rus\u00b7sen\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Land und St\u00e4dte wurden leer.", "tokens": ["Land", "und", "St\u00e4d\u00b7te", "wur\u00b7den", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand sah man in den Stra\u00dfen,", "tokens": ["Nie\u00b7mand", "sah", "man", "in", "den", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schier ganz Moskau war entfloh'n,", "tokens": ["Schier", "ganz", "Mos\u00b7kau", "war", "ent\u00b7floh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwar erstaunt, doch sehr gelassen", "tokens": ["Zwar", "er\u00b7staunt", ",", "doch", "sehr", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schritt herein Napoleon.", "tokens": ["Schritt", "her\u00b7ein", "Na\u00b7po\u00b7le\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ney, des Heeres Allerbester,", "tokens": ["Ney", ",", "des", "Hee\u00b7res", "Al\u00b7ler\u00b7bes\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Orden trug er schier ein Sester,", "tokens": ["Or\u00b7den", "trug", "er", "schier", "ein", "Ses\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ritt an seiner Seite hin,", "tokens": ["Ritt", "an", "sei\u00b7ner", "Sei\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Kaiser liebte ihn;", "tokens": ["Denn", "der", "Kai\u00b7ser", "lieb\u00b7te", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es lie\u00df die Stimm' erschallen", "tokens": ["Und", "es", "lie\u00df", "die", "Stimm'", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jetzt zu Ney Napoleon,", "tokens": ["Jetzt", "zu", "Ney", "Na\u00b7po\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ney, es will mir nicht gefallen,", "tokens": ["Ney", ",", "es", "will", "mir", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinst Du nicht, es brenzelt schon?", "tokens": ["Meinst", "Du", "nicht", ",", "es", "bren\u00b7zelt", "schon", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch' ein \u00fcbertriebnes Feuer!", "tokens": ["Welch'", "ein", "\u00fc\u00b7bert\u00b7rieb\u00b7nes", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sind keine Freudenfeuer!", "tokens": ["Das", "sind", "kei\u00b7ne", "Freu\u00b7den\u00b7feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist Moskaus gro\u00dfer Brand,", "tokens": ["Das", "ist", "Mos\u00b7kaus", "gro\u00b7\u00dfer", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er in der Zeitung stand.", "tokens": ["Wie", "er", "in", "der", "Zei\u00b7tung", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht, in Millionen Flammen", "tokens": ["Seht", ",", "in", "Mil\u00b7lion\u00b7en", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schl\u00e4gt er \u00fcber jener Stadt", "tokens": ["Schl\u00e4gt", "er", "\u00fc\u00b7ber", "je\u00b7ner", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Napoleon zusammen,", "tokens": ["Und", "Na\u00b7po\u00b7le\u00b7on", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh' er sich's vermuthet hat.", "tokens": ["Eh'", "er", "sich's", "ver\u00b7mu\u00b7thet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zehnmalhunderttausend Reiser,", "tokens": ["Zehn\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Rei\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzen nicht, wie Moskaus H\u00e4user,", "tokens": ["St\u00fcr\u00b7zen", "nicht", ",", "wie", "Mos\u00b7kaus", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht mit solchem Krachen ein;", "tokens": ["Nicht", "mit", "sol\u00b7chem", "Kra\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feuer l\u00f6set das Gestein,", "tokens": ["Feu\u00b7er", "l\u00f6\u00b7set", "das", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feuer packt das Holz entsetzlich.", "tokens": ["Feu\u00b7er", "packt", "das", "Holz", "ent\u00b7setz\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Feuer ras't in jedem Haus,", "tokens": ["Feu\u00b7er", "ras't", "in", "je\u00b7dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Feuer jagt die Feinde pl\u00f6tzlich", "tokens": ["Feu\u00b7er", "jagt", "die", "Fein\u00b7de", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sammt Napoleon hinaus.", "tokens": ["Sammt", "Na\u00b7po\u00b7le\u00b7on", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dieser flieht und schaut zum Himmel:", "tokens": ["Die\u00b7ser", "flieht", "und", "schaut", "zum", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor ihm ist ein Schneegewimmel,", "tokens": ["Vor", "ihm", "ist", "ein", "Schnee\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter ihm die Feuersbrunst,", "tokens": ["Hin\u00b7ter", "ihm", "die", "Feu\u00b7ers\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies ersch\u00f6pfet seine Kunst.", "tokens": ["Dies", "er\u00b7sch\u00f6p\u00b7fet", "sei\u00b7ne", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solches hat er nie vernommen:", "tokens": ["Sol\u00b7ches", "hat", "er", "nie", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vorne hei\u00df und hinten kalt!", "tokens": ["Vor\u00b7ne", "hei\u00df", "und", "hin\u00b7ten", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ei, da mu\u00df zu Schanden kommen,", "tokens": ["Ei", ",", "da", "mu\u00df", "zu", "Schan\u00b7den", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch der Allerst\u00e4rkste halt.", "tokens": ["Auch", "der", "Al\u00b7ler\u00b7st\u00e4rks\u00b7te", "halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hunger, Durst mit weiten Nachen", "tokens": ["Hun\u00b7ger", ",", "Durst", "mit", "wei\u00b7ten", "Na\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzen wie ergrimmte Drachen,", "tokens": ["St\u00fcr\u00b7zen", "wie", "er\u00b7grimm\u00b7te", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Magen gro\u00df und leer,", "tokens": ["De\u00b7ren", "Ma\u00b7gen", "gro\u00df", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lechzend auf sein stolzes Heer;", "tokens": ["Lech\u00b7zend", "auf", "sein", "stol\u00b7zes", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Frost ergreift die Schaaren,", "tokens": ["Und", "ein", "Frost", "er\u00b7greift", "die", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schnee bedecket Alles wei\u00df,", "tokens": ["Schnee", "be\u00b7de\u00b7cket", "Al\u00b7les", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die am Abend Menschen waren,", "tokens": ["Die", "am", "A\u00b7bend", "Men\u00b7schen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Standen Morgens da als Eis.", "tokens": ["Stan\u00b7den", "Mor\u00b7gens", "da", "als", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die verfolgten Krieger weichen.", "tokens": ["Die", "ver\u00b7folg\u00b7ten", "Krie\u00b7ger", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmalhunderttausend Leichen", "tokens": ["Ein\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Lei\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Beresina Strand,", "tokens": ["An", "der", "Be\u00b7re\u00b7si\u00b7na", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der erschrockne Russe fand.", "tokens": ["Der", "er\u00b7schrock\u00b7ne", "Rus\u00b7se", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn sie bauten eine Br\u00fccke,", "tokens": ["Denn", "sie", "bau\u00b7ten", "ei\u00b7ne", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00e4ngten sich in wildem Lauf,", "tokens": ["Dr\u00e4ng\u00b7ten", "sich", "in", "wil\u00b7dem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Traten dreimal sie in St\u00fccke,", "tokens": ["Tra\u00b7ten", "drei\u00b7mal", "sie", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gingen so schier Alle drauf.", "tokens": ["Gin\u00b7gen", "so", "schier", "Al\u00b7le", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PIS", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Oberst M\u00fcller hat's gesehen,", "tokens": ["O\u00b7berst", "M\u00fcl\u00b7ler", "hat's", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er kann k\u00fchn als Zeuge stehen,", "tokens": ["Er", "kann", "k\u00fchn", "als", "Zeu\u00b7ge", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcr den f\u00fcrchterlichen Tag,", "tokens": ["F\u00fcr", "den", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er mit im Felde lag.", "tokens": ["Weil", "er", "mit", "im", "Fel\u00b7de", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So erfroren Frankreichs Krieger", "tokens": ["So", "er\u00b7fro\u00b7ren", "Fran\u00b7kreichs", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es floh Napoleon,", "tokens": ["Und", "es", "floh", "Na\u00b7po\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der bezwungne Weltbesieger,", "tokens": ["Der", "be\u00b7zwung\u00b7ne", "Welt\u00b7be\u00b7sie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Schaudernd kaum allein davon.", "tokens": ["Schau\u00b7dernd", "kaum", "al\u00b7lein", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}