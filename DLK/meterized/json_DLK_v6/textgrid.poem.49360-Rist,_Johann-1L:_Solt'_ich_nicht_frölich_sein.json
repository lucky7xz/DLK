{"textgrid.poem.49360": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Solt' ich nicht fr\u00f6lich sein", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solt' ich nicht fr\u00f6lich sein", "tokens": ["Solt'", "ich", "nicht", "fr\u00f6\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und danken dir allein,", "tokens": ["Und", "dan\u00b7ken", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Gott, da\u00df nun vergangen", "tokens": ["O", "Gott", ",", "da\u00df", "nun", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Zorn und das Verlangen", "tokens": ["Dein", "Zorn", "und", "das", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Armen ist erf\u00fcllet,", "tokens": ["Der", "Ar\u00b7men", "ist", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja da\u00df zu dieser Frist", "tokens": ["Ja", "da\u00df", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein Eifer ganz gestillet", "tokens": ["Dein", "Ei\u00b7fer", "ganz", "ge\u00b7stil\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und du vers\u00f6hnet bist?", "tokens": ["Und", "du", "ver\u00b7s\u00f6h\u00b7net", "bist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie wol ist mir geschehn!", "tokens": ["Wie", "wol", "ist", "mir", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nun kan ich H\u00fclfe sehn.", "tokens": ["Nun", "kan", "ich", "H\u00fcl\u00b7fe", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Solt' ich dir nicht vertrauen,", "tokens": ["Solt'", "ich", "dir", "nicht", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der du mich l\u00e4ssest schauen", "tokens": ["Der", "du", "mich", "l\u00e4s\u00b7sest", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Den Fried' in unsern Gr\u00e4nzen,", "tokens": ["Den", "Fried'", "in", "un\u00b7sern", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der uns durch deine Kraft", "tokens": ["Der", "uns", "durch", "dei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Wird alles das erg\u00e4nzen,", "tokens": ["Wird", "al\u00b7les", "das", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Was uns der Krieg gerafft.", "tokens": ["Was", "uns", "der", "Krieg", "ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Gott Lob! Das Kriegsgeschrei", "tokens": ["Gott", "Lob", "!", "Das", "Kriegs\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ist endlich nun vorbei,", "tokens": ["Ist", "end\u00b7lich", "nun", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "So werden nicht verlassen,", "tokens": ["So", "wer\u00b7den", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die Gott im Glauben fassen;", "tokens": ["Die", "Gott", "im", "Glau\u00b7ben", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Nun hat sich abgewendet", "tokens": ["Nun", "hat", "sich", "ab\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Sein Grimm, nachdem der Streit", "tokens": ["Sein", "Grimm", ",", "nach\u00b7dem", "der", "Streit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Der F\u00fcrsten sich geendet", "tokens": ["Der", "F\u00fcrs\u00b7ten", "sich", "ge\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Mit Lieb' und Freundlichkeit.", "tokens": ["Mit", "Lieb'", "und", "Freund\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Drauf rauschet nun hernach", "tokens": ["Drauf", "rau\u00b7schet", "nun", "her\u00b7nach"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Der Fried', als sonst ein Bach,", "tokens": ["Der", "Fried'", ",", "als", "sonst", "ein", "Bach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Der Feld und Wiesen netzet,", "tokens": ["Der", "Feld", "und", "Wie\u00b7sen", "net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Der edle Fried' ergetzet.", "tokens": ["Der", "ed\u00b7le", "Fried'", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Gleich wie der Thau den Acker,", "tokens": ["Gleich", "wie", "der", "Thau", "den", "A\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Den er gleich lechzend fand,", "tokens": ["Den", "er", "gleich", "lech\u00b7zend", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "So macht der Fried' jetzt wacker", "tokens": ["So", "macht", "der", "Fried'", "jetzt", "wa\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das werte Vaterland.", "tokens": ["Das", "wer\u00b7te", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Dir dank' ich Tag und Nacht,", "tokens": ["Dir", "dank'", "ich", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "O Gott, da\u00df du die Macht", "tokens": ["O", "Gott", ",", "da\u00df", "du", "die", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Des Feindes hast gebrochen", "tokens": ["Des", "Fein\u00b7des", "hast", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Und dich an ihm gerochen,", "tokens": ["Und", "dich", "an", "ihm", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.37": {"text": "L\u00e4\u00dft uns nun wieder kommen", "tokens": ["L\u00e4\u00dft", "uns", "nun", "wie\u00b7der", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Des Landes Obrigkeit,", "tokens": ["Des", "Lan\u00b7des", "Ob\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Zum Nutz und Trost der Frommen", "tokens": ["Zum", "Nutz", "und", "Trost", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "In dieser schweren Zeit.", "tokens": ["In", "die\u00b7ser", "schwe\u00b7ren", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Sch\u00fctz herlich ihren Stand", "tokens": ["Sch\u00fctz", "her\u00b7lich", "ih\u00b7ren", "Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Durch deine Wunderhand,", "tokens": ["Durch", "dei\u00b7ne", "Wun\u00b7der\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Da\u00df sie kein Feind betr\u00fcbe,", "tokens": ["Da\u00df", "sie", "kein", "Feind", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Noch das an uns ver\u00fcbe,", "tokens": ["Noch", "das", "an", "uns", "ver\u00b7\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.45": {"text": "Das abermal kan schaden", "tokens": ["Das", "a\u00b7ber\u00b7mal", "kan", "scha\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Des Landes Gl\u00fcck und Ruh';", "tokens": ["Des", "Lan\u00b7des", "Gl\u00fcck", "und", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Herr, deck uns doch mit Gnaden", "tokens": ["Herr", ",", "deck", "uns", "doch", "mit", "Gna\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Und sicherm Friede zu.", "tokens": ["Und", "si\u00b7cherm", "Frie\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.49": {"text": "Gott Lob, der Krieg ist fort!", "tokens": ["Gott", "Lob", ",", "der", "Krieg", "ist", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "Uns sol an diesem Ort", "tokens": ["Uns", "sol", "an", "die\u00b7sem", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Ein Feind nicht leicht erschrecken,", "tokens": ["Ein", "Feind", "nicht", "leicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Noch uns mit Angst aufwecken,", "tokens": ["Noch", "uns", "mit", "Angst", "auf\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Die Kreuzstund' ist vergangen,", "tokens": ["Die", "Kreuz\u00b7stund'", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Itz bricht der Trost herf\u00fcr.", "tokens": ["Itz", "bricht", "der", "Trost", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Wem sollte nicht verlangen,", "tokens": ["Wem", "soll\u00b7te", "nicht", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "O Fried', allein nach dir?", "tokens": ["O", "Fried'", ",", "al\u00b7lein", "nach", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Die Not ist abgethan,", "tokens": ["Die", "Not", "ist", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Die Lust tritt auf die Bahn;", "tokens": ["Die", "Lust", "tritt", "auf", "die", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Kein Krieg wird mehr gefunden,", "tokens": ["Kein", "Krieg", "wird", "mehr", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Der Fried' hat \u00fcberwunden.", "tokens": ["Der", "Fried'", "hat", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Wird der nun nimmer wanken,", "tokens": ["Wird", "der", "nun", "nim\u00b7mer", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Herr Gott, in dieser Zeit,", "tokens": ["Herr", "Gott", ",", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.63": {"text": "So wollen wir dir danken", "tokens": ["So", "wol\u00b7len", "wir", "dir", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Dort in der Ewigkeit.", "tokens": ["Dort", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}