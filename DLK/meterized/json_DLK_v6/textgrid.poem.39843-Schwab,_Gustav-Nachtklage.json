{"textgrid.poem.39843": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Nachtklage", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein holder J\u00fcngling, sagen uns die Alten,", "tokens": ["Ein", "hol\u00b7der", "J\u00fcng\u00b7ling", ",", "sa\u00b7gen", "uns", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint alln\u00e4chtlich an der Ruhest\u00e4tte,", "tokens": ["Er\u00b7scheint", "all\u00b7n\u00e4cht\u00b7lich", "an", "der", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er neigt sich sinnbeth\u00f6rend \u00fcber's Bette,", "tokens": ["Er", "neigt", "sich", "sinn\u00b7be\u00b7th\u00f6\u00b7rend", "\u00fc\u00b7ber's", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Still wei\u00df er mit des Mohnes Kraft zu walten.", "tokens": ["Still", "wei\u00df", "er", "mit", "des", "Moh\u00b7nes", "Kraft", "zu", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist der Schlaf, er gl\u00e4ttet alle Falten,", "tokens": ["Das", "ist", "der", "Schlaf", ",", "er", "gl\u00e4t\u00b7tet", "al\u00b7le", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerrei\u00dft des Lebens ew'ge Bilderkette,", "tokens": ["Zer\u00b7rei\u00dft", "des", "Le\u00b7bens", "ew'\u00b7ge", "Bil\u00b7der\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, da\u00df er von des Tags Getrieb' uns rette,", "tokens": ["Und", ",", "da\u00df", "er", "von", "des", "Tags", "Ge\u00b7trieb'", "uns", "ret\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchrt er den Reigen s\u00fc\u00dfer Traumgestalten.", "tokens": ["F\u00fchrt", "er", "den", "Rei\u00b7gen", "s\u00fc\u00b7\u00dfer", "Traum\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah ihn lange nicht, es naht statt seiner", "tokens": ["Ich", "sah", "ihn", "lan\u00b7ge", "nicht", ",", "es", "naht", "statt", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ander Bild mir schon seit vielen N\u00e4chten,", "tokens": ["Ein", "an\u00b7der", "Bild", "mir", "schon", "seit", "vie\u00b7len", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein holdes M\u00e4gdlein ist es anzusehen.", "tokens": ["Ein", "hol\u00b7des", "M\u00e4gd\u00b7lein", "ist", "es", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch nicht erbarmt es, wie der Schlaf, sich meiner,", "tokens": ["Doch", "nicht", "er\u00b7barmt", "es", ",", "wie", "der", "Schlaf", ",", "sich", "mei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PRF", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, l\u00e4chelt's gleich aus dunkeln Lockenflechten,", "tokens": ["Und", ",", "l\u00e4\u00b7chelt's", "gleich", "aus", "dun\u00b7keln", "Lo\u00b7cken\u00b7flech\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Angst und Liebesschmerz mu\u00df ich vergehen.", "tokens": ["In", "Angst", "und", "Lie\u00b7bes\u00b7schmerz", "mu\u00df", "ich", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}