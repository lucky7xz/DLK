{"dta.poem.3514": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von alten Liebesliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Spazieren wollt ich reiten,               ", "tokens": ["Spa\u00b7zie\u00b7ren", "wollt", "ich", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebsten vor die Th\u00fcr,", "tokens": ["Der", "Liebs\u00b7ten", "vor", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie blickt nach mir von weitem,", "tokens": ["Sie", "blickt", "nach", "mir", "von", "wei\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach mit gro\u00dfen Freuden:", "tokens": ["Und", "sprach", "mit", "gro\u00b7\u00dfen", "Freu\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eseht dort meines Herzens Zier,", "tokens": ["\u201e", "seht", "dort", "mei\u00b7nes", "Her\u00b7zens", "Zier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201ewie trabt er her zu mir.", "tokens": ["\u201e", "wie", "trabt", "er", "her", "zu", "mir", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201etrab R\u00f6\u00dflein trab,", "tokens": ["\u201e", "trab", "R\u00f6\u00df\u00b7lein", "trab", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "\u201etrab f\u00fcr und f\u00fcr.\u201c", "tokens": ["\u201e", "trab", "f\u00fcr", "und", "f\u00fcr", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "KON", "APPR", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Den Zaum, den lie\u00df ich schiessen,", "tokens": ["Den", "Zaum", ",", "den", "lie\u00df", "ich", "schies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sprengte hin zu ihr,", "tokens": ["Und", "spreng\u00b7te", "hin", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich th\u00e4t sie freundlich gr\u00fcssen,", "tokens": ["Ich", "th\u00e4t", "sie", "freund\u00b7lich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach mit Worten s\u00fc\u00df:", "tokens": ["Und", "sprach", "mit", "Wor\u00b7ten", "s\u00fc\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201emein Schatz, mein h\u00f6chste Zier,", "tokens": ["\u201e", "mein", "Schatz", ",", "mein", "h\u00f6chs\u00b7te", "Zier", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "\u201ewas macht ihr vor der Th\u00fcr?", "tokens": ["\u201e", "was", "macht", "ihr", "vor", "der", "Th\u00fcr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201etrab R\u00f6\u00dflein trab", "tokens": ["\u201e", "trab", "R\u00f6\u00df\u00b7lein", "trab"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "XY", "XY", "XY"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "\u201etrab her zu ihr.", "tokens": ["\u201e", "trab", "her", "zu", "ihr", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "APPR", "PPOSAT", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Vom R\u00f6\u00dflein mein ich sprange,", "tokens": ["Vom", "R\u00f6\u00df\u00b7lein", "mein", "ich", "spran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und band es an die Th\u00fcr,", "tokens": ["Und", "band", "es", "an", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Th\u00e4t freundlich sie umfangen,", "tokens": ["Th\u00e4t", "freund\u00b7lich", "sie", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zeit ward uns nicht lange,", "tokens": ["Die", "Zeit", "ward", "uns", "nicht", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Garten giengen wir", "tokens": ["In", "Gar\u00b7ten", "gien\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit liebender Begier;", "tokens": ["Mit", "lie\u00b7ben\u00b7der", "Be\u00b7gier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Trab R\u00f6\u00dflein trab,", "tokens": ["Trab", "R\u00f6\u00df\u00b7lein", "trab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Trab leis herf\u00fcr.", "tokens": ["Trab", "leis", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wir sezten uns da nieder", "tokens": ["Wir", "sez\u00b7ten", "uns", "da", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in das gr\u00fcne Gras,", "tokens": ["Wohl", "in", "das", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sangen hin und wieder", "tokens": ["Und", "san\u00b7gen", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alten Liebeslieder,", "tokens": ["Die", "al\u00b7ten", "Lie\u00b7bes\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis uns die Aeuglein na\u00df,", "tokens": ["Bis", "uns", "die", "A\u00b7e\u00b7u\u00b7glein", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wegen der Kl\u00e4ffer Ha\u00df.", "tokens": ["We\u00b7gen", "der", "Kl\u00e4f\u00b7fer", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Trab R\u00f6\u00dflein trab,", "tokens": ["Trab", "R\u00f6\u00df\u00b7lein", "trab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Trab, trab f\u00fcrbas.", "tokens": ["Trab", ",", "trab", "f\u00fcr\u00b7bas", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM.la", "FM.la", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}