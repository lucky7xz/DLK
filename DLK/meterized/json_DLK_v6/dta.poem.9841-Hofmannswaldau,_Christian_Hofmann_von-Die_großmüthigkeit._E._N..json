{"dta.poem.9841": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die gro\u00dfm\u00fcthigkeit.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unverzagt!", "tokens": ["Un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "So soll die losung heissen:", "tokens": ["So", "soll", "die", "lo\u00b7sung", "heis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00f6rdert rath und that.", "tokens": ["Sie", "f\u00f6r\u00b7dert", "rath", "und", "that", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer hertz im leibe hat/", "tokens": ["Wer", "hertz", "im", "lei\u00b7be", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPRART", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird sich darauff befleissen.", "tokens": ["Wird", "sich", "dar\u00b7auff", "be\u00b7fleis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein eisenfresser", "tokens": ["Ein", "ei\u00b7sen\u00b7fres\u00b7ser"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Frist drum die leute nicht.", "tokens": ["Frist", "drum", "die", "leu\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Und schneidet er gleich mit dem grossen messer/", "tokens": ["Und", "schnei\u00b7det", "er", "gleich", "mit", "dem", "gros\u00b7sen", "mes\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Gnug/ da\u00df es nicht in leib und leben sticht.", "tokens": ["Gnug", "/", "da\u00df", "es", "nicht", "in", "leib", "und", "le\u00b7ben", "sticht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wem stracks das hertz zun hosen f\u00e4llt/", "tokens": ["Wem", "stracks", "das", "hertz", "zun", "ho\u00b7sen", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der ist gar leicht zu schrecken/", "tokens": ["Der", "ist", "gar", "leicht", "zu", "schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und in den sack zu stecken.", "tokens": ["Und", "in", "den", "sack", "zu", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Doch welcher wiederh\u00e4lt/", "tokens": ["Doch", "wel\u00b7cher", "wie\u00b7der\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAT", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und seinen mann beweist/", "tokens": ["Und", "sei\u00b7nen", "mann", "be\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Der s", "tokens": ["Der", "s"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Ein garstger vetter heist.", "tokens": ["Ein", "garst\u00b7ger", "vet\u00b7ter", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ein redlich teutsches blut", "tokens": ["Ein", "red\u00b7lich", "teut\u00b7sches", "blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Zeigt immer seinen muth.", "tokens": ["Zeigt", "im\u00b7mer", "sei\u00b7nen", "muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Doch eine feige memme", "tokens": ["Doch", "ei\u00b7ne", "fei\u00b7ge", "mem\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ist/ was denn? blo\u00df ein zuckerhut.", "tokens": ["Ist", "/", "was", "denn", "?", "blo\u00df", "ein", "zu\u00b7cker\u00b7hut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PWS", "ADV", "$.", "ADV", "ART", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}