{"textgrid.poem.41762": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fenster hinter dichten Eisenst\u00e4ben,", "tokens": ["Ein", "Fens\u00b7ter", "hin\u00b7ter", "dich\u00b7ten", "Ei\u00b7sen\u00b7st\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das klein und schmal kaum einen Blick verstattet", "tokens": ["Das", "klein", "und", "schmal", "kaum", "ei\u00b7nen", "Blick", "ver\u00b7stat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "KON", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das nur ein wenig aufw\u00e4rts zu erheben,", "tokens": ["Das", "nur", "ein", "we\u00b7nig", "auf\u00b7w\u00e4rts", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geringelt Glas, darin das Licht ermattet.", "tokens": ["Ge\u00b7rin\u00b7gelt", "Glas", ",", "da\u00b7rin", "das", "Licht", "er\u00b7mat\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein enger Raum wie eine Klosterzelle,", "tokens": ["Ein", "en\u00b7ger", "Raum", "wie", "ei\u00b7ne", "Klos\u00b7ter\u00b7zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der W\u00e4nde Grau, die Farbe der Bedr\u00e4ngnis.", "tokens": ["Der", "W\u00e4n\u00b7de", "Grau", ",", "die", "Far\u00b7be", "der", "Be\u00b7dr\u00e4ng\u00b7nis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verscheucht schon fr\u00fch des kurzen Tages Helle,", "tokens": ["Ver\u00b7scheucht", "schon", "fr\u00fch", "des", "kur\u00b7zen", "Ta\u00b7ges", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verdunkelt noch das einsame Gef\u00e4ngnis.", "tokens": ["Ver\u00b7dun\u00b7kelt", "noch", "das", "ein\u00b7sa\u00b7me", "Ge\u00b7f\u00e4ng\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein bleicher Mann, versunken in Gedanken,", "tokens": ["Ein", "blei\u00b7cher", "Mann", ",", "ver\u00b7sun\u00b7ken", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lehnt an dem Fenster sucht des Himmels Bl\u00e4ue,", "tokens": ["Lehnt", "an", "dem", "Fens\u00b7ter", "sucht", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Denn auch in seines Kerkers enge Schranken", "tokens": ["Denn", "auch", "in", "sei\u00b7nes", "Ker\u00b7kers", "en\u00b7ge", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schaut noch dies Blau! \u2013 die Farbe ewger Treue!", "tokens": ["Schaut", "noch", "dies", "Blau", "!", "\u2013", "die", "Far\u00b7be", "ew\u00b7ger", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "NN", "$.", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und seines M\u00e4dchens, seiner Trauten Farbe!", "tokens": ["Und", "sei\u00b7nes", "M\u00e4d\u00b7chens", ",", "sei\u00b7ner", "Trau\u00b7ten", "Far\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er denkt an sie, die ihm die einzig Eine,", "tokens": ["Er", "denkt", "an", "sie", ",", "die", "ihm", "die", "ein\u00b7zig", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "ART", "ADJD", "ART", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und wie er leide, wie er duld und darbe,", "tokens": ["Und", "wie", "er", "lei\u00b7de", ",", "wie", "er", "duld", "und", "dar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchlt sich reich, denn sie bleibt doch die Seine!", "tokens": ["Er", "f\u00fchlt", "sich", "reich", ",", "denn", "sie", "bleibt", "doch", "die", "Sei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "KON", "PPER", "VVFIN", "ADV", "ART", "NE", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie denkt wie er, sie wei\u00df warum er leidet \u2013", "tokens": ["Sie", "denkt", "wie", "er", ",", "sie", "wei\u00df", "wa\u00b7rum", "er", "lei\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "$,", "PPER", "VVFIN", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor einer Welt hat stolz sie's ausgesprochen:", "tokens": ["Vor", "ei\u00b7ner", "Welt", "hat", "stolz", "sie's", "aus\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer f\u00fcr den Glauben seiner Seele streitet", "tokens": ["Wer", "f\u00fcr", "den", "Glau\u00b7ben", "sei\u00b7ner", "See\u00b7le", "strei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat nichts vor Gott, noch vor sich selbst verbrochen.", "tokens": ["Hat", "nichts", "vor", "Gott", ",", "noch", "vor", "sich", "selbst", "ver\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "$,", "ADV", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein Brieflein h\u00e4lt er zwischen seinen H\u00e4nden,", "tokens": ["Ein", "Brie\u00b7flein", "h\u00e4lt", "er", "zwi\u00b7schen", "sei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn nicht verbannt ist solches Liebeszeichen,", "tokens": ["Denn", "nicht", "ver\u00b7bannt", "ist", "sol\u00b7ches", "Lie\u00b7bes\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie d\u00fcrfen sich einander Gr\u00fc\u00dfe senden,", "tokens": ["Sie", "d\u00fcr\u00b7fen", "sich", "ein\u00b7an\u00b7der", "Gr\u00fc\u00b7\u00dfe", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn strenge Fristen auch dazwischen streichen.", "tokens": ["Wenn", "stren\u00b7ge", "Fris\u00b7ten", "auch", "da\u00b7zwi\u00b7schen", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Was kann sie andres ihm als Liebe schreiben,", "tokens": ["Was", "kann", "sie", "and\u00b7res", "ihm", "als", "Lie\u00b7be", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PIS", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der keinen Trost bedarf um nicht zu wanken?", "tokens": ["Der", "kei\u00b7nen", "Trost", "be\u00b7darf", "um", "nicht", "zu", "wan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie meldet ihm, da\u00df Myrt, und Lorber treiben", "tokens": ["Sie", "mel\u00b7det", "ihm", ",", "da\u00df", "Myrt", ",", "und", "Lor\u00b7ber", "trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und frisches Gr\u00fcn der Hoffnung Epheuranken!", "tokens": ["Und", "fri\u00b7sches", "Gr\u00fcn", "der", "Hoff\u00b7nung", "E\u00b7pheu\u00b7ran\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein Seufzer, dann ein L\u00e4cheln \u2013 und aufs neue", "tokens": ["Ein", "Seuf\u00b7zer", ",", "dann", "ein", "L\u00e4\u00b7cheln", "\u2013", "und", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$(", "KON", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fc\u00dft er den Brief, der Wonne ihn bereitet,", "tokens": ["K\u00fc\u00dft", "er", "den", "Brief", ",", "der", "Won\u00b7ne", "ihn", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Singt dazu leis' ein Lied von Lieb und Treue,", "tokens": ["Singt", "da\u00b7zu", "leis'", "ein", "Lied", "von", "Lieb", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Gottes Hand, die sie, wie ihn geleitet.", "tokens": ["Von", "Got\u00b7tes", "Hand", ",", "die", "sie", ",", "wie", "ihn", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PRELS", "PPER", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Zwei Eisengitter scheiden Dich von mir! \u2013", "tokens": ["Zwei", "Ei\u00b7sen\u00b7git\u00b7ter", "schei\u00b7den", "Dich", "von", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dazwischen schreitet auf und ab der W\u00e4chter \u2013", "tokens": ["Da\u00b7zwi\u00b7schen", "schrei\u00b7tet", "auf", "und", "ab", "der", "W\u00e4ch\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe schwingt ihr heiliges Panier,", "tokens": ["Die", "Lie\u00b7be", "schwingt", "ihr", "hei\u00b7li\u00b7ges", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Talisman f\u00fcr Dich und mich, ein echter!", "tokens": ["Ein", "Ta\u00b7lis\u00b7man", "f\u00fcr", "Dich", "und", "mich", ",", "ein", "ech\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "KON", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Kein H\u00e4ndedruck, kein Ku\u00df! \u2013 kein Gitter f\u00e4llt", "tokens": ["Kein", "H\u00e4n\u00b7de\u00b7druck", ",", "kein", "Ku\u00df", "!", "\u2013", "kein", "Git\u00b7ter", "f\u00e4llt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "$(", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und keine Hand kann durch die St\u00e4be langen,", "tokens": ["Und", "kei\u00b7ne", "Hand", "kann", "durch", "die", "St\u00e4\u00b7be", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst das Wort von Laurern rings umstellt,", "tokens": ["Und", "selbst", "das", "Wort", "von", "Lau\u00b7rern", "rings", "um\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es bleibt im Bann, es ist wie Du gefangen. \u2013", "tokens": ["Es", "bleibt", "im", "Bann", ",", "es", "ist", "wie", "Du", "ge\u00b7fan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VAFIN", "KOKOM", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Ihr Euch liebt in Freiheit hoch begl\u00fcckt:", "tokens": ["Die", "Ihr", "Euch", "liebt", "in", "Frei\u00b7heit", "hoch", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verm\u00f6gt Ihr wohl ein ", "tokens": ["Ver\u00b7m\u00f6gt", "Ihr", "wohl", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Euch auszumalen, Herz an Herz gedr\u00fcckt,", "tokens": ["Euch", "aus\u00b7zu\u00b7ma\u00b7len", ",", "Herz", "an", "Herz", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie m\u00f6chtet Ihr vor solchem Gitter stehen?", "tokens": ["Wie", "m\u00f6ch\u00b7tet", "Ihr", "vor", "sol\u00b7chem", "Git\u00b7ter", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wir standen so: Wir sahn uns Aug in Aug, \u2013", "tokens": ["Wir", "stan\u00b7den", "so", ":", "Wir", "sahn", "uns", "Aug", "in", "Aug", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iamb