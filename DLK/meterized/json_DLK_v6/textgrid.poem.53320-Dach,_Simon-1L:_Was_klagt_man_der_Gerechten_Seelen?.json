{"textgrid.poem.53320": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was klagt man der Gerechten Seelen?", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was klagt man der Gerechten Seelen?", "tokens": ["Was", "klagt", "man", "der", "Ge\u00b7rech\u00b7ten", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fahren aus des Leibes H\u00f6len", "tokens": ["Sie", "fah\u00b7ren", "aus", "des", "Lei\u00b7bes", "H\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hienauff in Gottes Hand:", "tokens": ["Hien\u00b7auff", "in", "Got\u00b7tes", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Angst noch Quahl wird sie ber\u00fchren,", "tokens": ["Nicht", "Angst", "noch", "Quahl", "wird", "sie", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wol jhnen, ewig wol! sie f\u00fchren", "tokens": ["Wol", "jh\u00b7nen", ",", "e\u00b7wig", "wol", "!", "sie", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADJD", "ADV", "$.", "PPER", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Den ausserwehlsten Frewden-Stand.", "tokens": ["Den", "aus\u00b7ser\u00b7wehls\u00b7ten", "Fre\u00b7wden\u00b7Stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man sieht sie an, als wenn sie st\u00fcrben", "tokens": ["Man", "sieht", "sie", "an", ",", "als", "wenn", "sie", "st\u00fcr\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd durch die Hinfahrt gantz verd\u00fcrben,", "tokens": ["Vnd", "durch", "die", "Hin\u00b7fahrt", "gantz", "ver\u00b7d\u00fcr\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wahnwitz halt sie todt;", "tokens": ["Der", "Wahn\u00b7witz", "halt", "sie", "todt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie aber sind bey GOTT in Frieden", "tokens": ["Sie", "a\u00b7ber", "sind", "bey", "GoTT", "in", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bald Ihr Geist ist abgeschieden", "tokens": ["So", "bald", "Ihr", "Geist", "ist", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd leben ausser aller Noht.", "tokens": ["Vnd", "le\u00b7ben", "aus\u00b7ser", "al\u00b7ler", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "GoTT steupt ein wenig hie auff Erden,", "tokens": ["GoTT", "steupt", "ein", "we\u00b7nig", "hie", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dafur doch jhnen dort sol werden", "tokens": ["Da\u00b7fur", "doch", "jh\u00b7nen", "dort", "sol", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PPER", "ADV", "VMFIN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Viel gutes, vnd viel Lieb' vnd Ehr'.", "tokens": ["Viel", "gu\u00b7tes", ",", "vnd", "viel", "Lieb'", "vnd", "Ehr'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "KON", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wol sie hie viel Leidens haben,", "tokens": ["Wie", "wol", "sie", "hie", "viel", "Lei\u00b7dens", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df sie die Hoffnung dennoch laben,", "tokens": ["Mu\u00df", "sie", "die", "Hoff\u00b7nung", "den\u00b7noch", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sterben nimmermehr.", "tokens": ["Sie", "ster\u00b7ben", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er hat in den Versuchungs Stunden", "tokens": ["Er", "hat", "in", "den", "Ver\u00b7su\u00b7chungs", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie seiner Liebe wehrt befunden,", "tokens": ["Sie", "sei\u00b7ner", "Lie\u00b7be", "wehrt", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben Ihm vertrawt,", "tokens": ["Sie", "ha\u00b7ben", "Ihm", "ver\u00b7trawt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Drumb sehn sie, da\u00df Er sey der Alte,", "tokens": ["Drumb", "sehn", "sie", ",", "da\u00df", "Er", "sey", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der ewig Bund vnd Glauben halte", "tokens": ["Der", "e\u00b7wig", "Bund", "vnd", "Glau\u00b7ben", "hal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem, der auff Ihn in Liebe bawt.", "tokens": ["Dem", ",", "der", "auff", "Ihn", "in", "Lie\u00b7be", "bawt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "GoTT l\u00e4sset Ihm doch die nicht nehmen", "tokens": ["GoTT", "l\u00e4s\u00b7set", "Ihm", "doch", "die", "nicht", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die trew sind vnd sich sein nicht sch\u00e4men.", "tokens": ["Die", "trew", "sind", "vnd", "sich", "sein", "nicht", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KON", "PRF", "PPOSAT", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib heilig iederzeit,", "tokens": ["Bleib", "hei\u00b7lig", "ie\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wird Er dich in Auffsicht fassen,", "tokens": ["So", "wird", "Er", "dich", "in", "Auff\u00b7sicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd weder ietzt noch ewig lassen", "tokens": ["Vnd", "we\u00b7der", "ietzt", "noch", "e\u00b7wig", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KON", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus seiner Gnad' vnd G\u00fctigkeit.", "tokens": ["Aus", "sei\u00b7ner", "Gnad'", "vnd", "G\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}