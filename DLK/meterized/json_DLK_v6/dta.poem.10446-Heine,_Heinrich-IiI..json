{"dta.poem.10446": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, mein Herz ist traurig,", "tokens": ["Mein", "Herz", ",", "mein", "Herz", "ist", "trau\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch lustig leuchtet der Mai;", "tokens": ["Doch", "lus\u00b7tig", "leuch\u00b7tet", "der", "Mai", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich stehe, gelehnt an der Linde,", "tokens": ["Ich", "ste\u00b7he", ",", "ge\u00b7lehnt", "an", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "APPR", "ART", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Hoch auf der alten Bastei.", "tokens": ["Hoch", "auf", "der", "al\u00b7ten", "Bas\u00b7tei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da drunten flie\u00dft der blaue", "tokens": ["Da", "drun\u00b7ten", "flie\u00dft", "der", "blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stadtgraben in stiller Ruh';", "tokens": ["Stadt\u00b7gra\u00b7ben", "in", "stil\u00b7ler", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Knabe f\u00e4hrt im Kahne,", "tokens": ["Ein", "Kna\u00b7be", "f\u00e4hrt", "im", "Kah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und angelt und pfeift dazu.", "tokens": ["Und", "an\u00b7gelt", "und", "pfeift", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KON", "VVFIN", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Jenseits erheben sich freundlich,", "tokens": ["Jen\u00b7seits", "er\u00b7he\u00b7ben", "sich", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In winziger, bunter Gestalt,", "tokens": ["In", "win\u00b7zi\u00b7ger", ",", "bun\u00b7ter", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Lusth\u00e4user, und G\u00e4rten, und Menschen,", "tokens": ["Lust\u00b7h\u00e4u\u00b7ser", ",", "und", "G\u00e4r\u00b7ten", ",", "und", "Men\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Ochsen, und Wiesen, und Wald.", "tokens": ["Und", "Och\u00b7sen", ",", "und", "Wie\u00b7sen", ",", "und", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die M\u00e4dchen bleichen W\u00e4sche,", "tokens": ["Die", "M\u00e4d\u00b7chen", "blei\u00b7chen", "W\u00e4\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und springen im Gras' herum;", "tokens": ["Und", "sprin\u00b7gen", "im", "Gras'", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das M\u00fchlrad st\u00e4ubt Diamanten,", "tokens": ["Das", "M\u00fchl\u00b7rad", "st\u00e4ubt", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich h\u00f6re sein fernes Gesumm'.", "tokens": ["Ich", "h\u00f6\u00b7re", "sein", "fer\u00b7nes", "Ge\u00b7summ'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Am alten grauen Thurme", "tokens": ["Am", "al\u00b7ten", "grau\u00b7en", "Thur\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schilderh\u00e4uschen steht;", "tokens": ["Ein", "Schil\u00b7der\u00b7h\u00e4usc\u00b7hen", "steht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein rothger\u00f6ckter Bursche", "tokens": ["Ein", "roth\u00b7ge\u00b7r\u00f6ck\u00b7ter", "Bur\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort auf und nieder geht.", "tokens": ["Dort", "auf", "und", "nie\u00b7der", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er spielt mit seiner Flinte,", "tokens": ["Er", "spielt", "mit", "sei\u00b7ner", "Flin\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die funkelt im Sonnenroth,", "tokens": ["Die", "fun\u00b7kelt", "im", "Son\u00b7nen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er pr\u00e4sentirt und schultert \u2014", "tokens": ["Er", "pr\u00e4\u00b7sen\u00b7tirt", "und", "schul\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wollt', er sch\u00f6sse mich todt.", "tokens": ["Ich", "wollt'", ",", "er", "sch\u00f6s\u00b7se", "mich", "todt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}