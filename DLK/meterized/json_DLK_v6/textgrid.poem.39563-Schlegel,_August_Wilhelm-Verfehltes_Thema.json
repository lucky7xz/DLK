{"textgrid.poem.39563": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Verfehltes Thema", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Leibeigenen schriebst du mit Recht; doch \u00fcber uns Freie", "tokens": ["Von", "Leib\u00b7ei\u00b7ge\u00b7nen", "schriebst", "du", "mit", "Recht", ";", "doch", "\u00fc\u00b7ber", "uns", "Frei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$.", "ADV", "APPR", "PPER", "NN"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Willst du nun schreiben durchaus: Bester, das ist dir zu hoch!", "tokens": ["Willst", "du", "nun", "schrei\u00b7ben", "durc\u00b7haus", ":", "Bes\u00b7ter", ",", "das", "ist", "dir", "zu", "hoch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$.", "NN", "$,", "PDS", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}