{"textgrid.poem.53468": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Fasching", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun spuckt sich der Berliner in die H\u00e4nde", "tokens": ["Nun", "spuckt", "sich", "der", "Ber\u00b7li\u00b7ner", "in", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und macht sich an das Werk der Fr\u00f6hlichkeit.", "tokens": ["und", "macht", "sich", "an", "das", "Werk", "der", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er schuftet sich von Anfang bis zu Ende", "tokens": ["Er", "schuf\u00b7tet", "sich", "von", "An\u00b7fang", "bis", "zu", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durch diese Faschingszeit.", "tokens": ["durch", "die\u00b7se", "Fa\u00b7schings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da h\u00f6rt man pl\u00f6tzlich von den h\u00f6chsten Stufen", "tokens": ["Da", "h\u00f6rt", "man", "pl\u00f6tz\u00b7lich", "von", "den", "h\u00f6chs\u00b7ten", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der eleganten Weltgesellschaft l\u00e4ngs", "tokens": ["der", "e\u00b7leg\u00b7an\u00b7ten", "Welt\u00b7ge\u00b7sell\u00b7schaft", "l\u00e4ngs"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Spree und den Kan\u00e4len lockend rufen:", "tokens": ["der", "Spree", "und", "den", "Ka\u00b7n\u00e4\u00b7len", "lo\u00b7ckend", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbrin in die Eskarpins!\u00ab", "tokens": ["\u00bb", "rin", "in", "die", "Es\u00b7kar\u00b7pins", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und diese Laune, diese Grazie, wei\u00dfte,", "tokens": ["Und", "die\u00b7se", "Lau\u00b7ne", ",", "die\u00b7se", "Gra\u00b7zie", ",", "wei\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PDAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die hat nat\u00fcrlich alle angesteckt;", "tokens": ["die", "hat", "na\u00b7t\u00fcr\u00b7lich", "al\u00b7le", "an\u00b7ge\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Hand, die tagshindurch Satin verschlei\u00dfte,", "tokens": ["die", "Hand", ",", "die", "tags\u00b7hin\u00b7durch", "Sa\u00b7tin", "ver\u00b7schlei\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "winkt ganz leschehr nach Sekt.", "tokens": ["winkt", "ganz", "le\u00b7schehr", "nach", "Sekt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Dame faschingt so auf ihre Weise:", "tokens": ["Die", "Da\u00b7me", "fa\u00b7schingt", "so", "auf", "ih\u00b7re", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gibt man ihr einmal schon im Jahr Lizenz,", "tokens": ["gibt", "man", "ihr", "ein\u00b7mal", "schon", "im", "Jahr", "Li\u00b7zenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "ADV", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dann knutscht sie sich in streng geschlossnem Kreise,", "tokens": ["dann", "knutscht", "sie", "sich", "in", "streng", "ge\u00b7schloss\u00b7nem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "fern jeder Konkurrenz.", "tokens": ["fern", "je\u00b7der", "Kon\u00b7kur\u00b7renz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und auch der Mittelstand f\u00fchlts im Gem\u00fcte:", "tokens": ["Und", "auch", "der", "Mit\u00b7tel\u00b7stand", "f\u00fchlts", "im", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "er macht den Bockbierfa\u00dfhahn nicht mehr zu,", "tokens": ["er", "macht", "den", "Bock\u00b7bier\u00b7fa\u00df\u00b7hahn", "nicht", "mehr", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "umspannt das Haupt mit einer bunten T\u00fcte", "tokens": ["um\u00b7spannt", "das", "Haupt", "mit", "ei\u00b7ner", "bun\u00b7ten", "T\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und rufet froh: \u00bbJuhu!\u00ab", "tokens": ["und", "ru\u00b7fet", "froh", ":", "\u00bb", "Ju\u00b7hu", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "ITJ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ja, selbst der Weise sch\u00e4tzt nicht nur die hehre", "tokens": ["Ja", ",", "selbst", "der", "Wei\u00b7se", "sch\u00e4tzt", "nicht", "nur", "die", "heh\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Philosophie: auch er bedarf des Weins!", "tokens": ["Phi\u00b7lo\u00b7so\u00b7phie", ":", "auch", "er", "be\u00b7darf", "des", "Weins", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leicht angef\u00fcllt geht er bei seine Claire,", "tokens": ["Leicht", "an\u00b7ge\u00b7f\u00fcllt", "geht", "er", "bei", "sei\u00b7ne", "Clai\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Berlin radaut, er l\u00e4chelt . . .", "tokens": ["Ber\u00b7lin", "ra\u00b7daut", ",", "er", "l\u00e4\u00b7chelt", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jeder seins.", "tokens": ["Je\u00b7der", "seins", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}