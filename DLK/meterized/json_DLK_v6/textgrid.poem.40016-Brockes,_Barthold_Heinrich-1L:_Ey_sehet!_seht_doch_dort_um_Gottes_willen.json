{"textgrid.poem.40016": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ey sehet! seht doch dort um Gottes willen", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ey sehet! seht doch dort um Gottes willen", "tokens": ["Ey", "se\u00b7het", "!", "seht", "doch", "dort", "um", "Got\u00b7tes", "wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "VVFIN", "ADV", "ADV", "APPR", "NN", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die g\u00fcld'ne Gluht, den Rosen-farb'nen Glantz,", "tokens": ["Die", "g\u00fcld'\u00b7ne", "Gluht", ",", "den", "Ro\u00b7sen\u00b7fa\u00b7rb'\u00b7nen", "Glantz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die dort des Waldes Nacht, und gr\u00fcne Schatten gantz,", "tokens": ["Die", "dort", "des", "Wal\u00b7des", "Nacht", ",", "und", "gr\u00fc\u00b7ne", "Schat\u00b7ten", "gantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "$,", "KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit einer himmlischen, nicht ird'schen Sch\u00f6nheit, f\u00fcllen!", "tokens": ["Mit", "ei\u00b7ner", "himm\u00b7li\u00b7schen", ",", "nicht", "ird'\u00b7schen", "Sch\u00f6n\u00b7heit", ",", "f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "PTKNEG", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat wohl ein menschlich Aug' ein holders Licht erblickt,", "tokens": ["Hat", "wohl", "ein", "menschlich", "Aug'", "ein", "hol\u00b7ders", "Licht", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Was sch\u00f6ners je gesehn? Es dringt mir in die Seele", "tokens": ["Was", "sch\u00f6\u00b7ners", "je", "ge\u00b7sehn", "?", "Es", "dringt", "mir", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df helle Freuden-Feu'r: sie wird fast als entz\u00fcckt,", "tokens": ["Die\u00df", "hel\u00b7le", "Freu\u00b7den\u00b7Feu'r", ":", "sie", "wird", "fast", "als", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und f\u00fchlet, wie mit ihr ein Etwas sich verm\u00e4hle,", "tokens": ["Und", "f\u00fch\u00b7let", ",", "wie", "mit", "ihr", "ein", "Et\u00b7was", "sich", "ver\u00b7m\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPR", "PPOSAT", "ART", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das \u00fcberirdisch ist. Sie senckt in diesen Schein,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rir\u00b7disch", "ist", ".", "Sie", "senckt", "in", "die\u00b7sen", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Urquell dieses Lichts, dem grossen All zu Ehren,", "tokens": ["Dem", "Ur\u00b7quell", "die\u00b7ses", "Lichts", ",", "dem", "gros\u00b7sen", "All", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich, als ein Opfer, selbst hinein.", "tokens": ["Sich", ",", "als", "ein", "Op\u00b7fer", ",", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "KOUS", "ART", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, m\u00f6chte diese reine Gluht", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "die\u00b7se", "rei\u00b7ne", "Gluht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das, was an ihr nicht gut,", "tokens": ["Das", ",", "was", "an", "ihr", "nicht", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Verbrennen und verzehren,", "tokens": ["Ver\u00b7bren\u00b7nen", "und", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Damit, wenn das, so b\u00f6s' an ihr, vergienge,", "tokens": ["Da\u00b7mit", ",", "wenn", "das", ",", "so", "b\u00f6s'", "an", "ihr", ",", "ver\u00b7gien\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PDS", "$,", "ADV", "ADJD", "APPR", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie dir, o Sch\u00f6pfer aller Dinge,", "tokens": ["Sie", "dir", ",", "o", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "FM", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "M\u00f6cht' ein gef\u00e4lligs Opfer seyn!", "tokens": ["M\u00f6cht'", "ein", "ge\u00b7f\u00e4l\u00b7ligs", "Op\u00b7fer", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}