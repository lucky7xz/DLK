{"dta.poem.2379": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ringelversche.  \n oder  \n Glukswunsch der gantzen H\u00fcrtengesel-  \n schafft/ an den Herrn Breutigam und seine  \n geliebte Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.85", "nl:0.14"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Lebet wol Herr Floridan! zu dem neuen Ehrenleben/", "tokens": ["Le\u00b7bet", "wol", "Herr", "Flo\u00b7ri\u00b7dan", "!", "zu", "dem", "neu\u00b7en", "Eh\u00b7ren\u00b7le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "NE", "$.", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Woll\u2019 euch der gerechte Gott Gl\u00fckk und Heyl und Sege\u0303", "tokens": ["Woll'", "euch", "der", "ge\u00b7rech\u00b7te", "Gott", "Gl\u00fckk", "und", "Heyl", "und", "Seg\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "geben;", "tokens": ["ge\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Treue Lieb\u2019 und Einigkent/ k\u00fcndigt euch der Himmel an/", "tokens": ["Treu\u00b7e", "Lieb'", "und", "Ei\u00b7nig\u00b7kent", "/", "k\u00fcn\u00b7digt", "euch", "der", "Him\u00b7mel", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und wie gleichsam selber rufft! Lebet wol Herr Floridan!", "tokens": ["Und", "wie", "gleich\u00b7sam", "sel\u00b7ber", "rufft", "!", "Le\u00b7bet", "wol", "Herr", "Flo\u00b7ri\u00b7dan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADV", "VVFIN", "$.", "VVFIN", "ADV", "NN", "NE", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Lebet so/ da\u00df mit der Zeit ihr mit eurer Basilenen", "tokens": ["Le\u00b7bet", "so", "/", "da\u00df", "mit", "der", "Zeit", "ihr", "mit", "eu\u00b7rer", "Ba\u00b7si\u00b7le\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "KOUS", "APPR", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.7": {"text": "Euren Disch besetzen k\u00f6n\u0304t und mit lieben Kindern Kr\u00f6hne\u0303.", "tokens": ["Eu\u00b7ren", "Disch", "be\u00b7set\u00b7zen", "k\u00f6n\u0304t", "und", "mit", "lie\u00b7ben", "Kin\u00b7dern", "Kr\u00f6hn\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "KON", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Kurtz/ wir w\u00fcnschen was ein Freund seinem Freunde", "tokens": ["Kurtz", "/", "wir", "w\u00fcn\u00b7schen", "was", "ein", "Freund", "sei\u00b7nem", "Freun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "PPER", "VVFIN", "PWS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "w\u00fcnschen kan/", "tokens": ["w\u00fcn\u00b7schen", "kan", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Jeder schreyet unter uns: Lebet wol Herr Floridan!", "tokens": ["Je\u00b7der", "schre\u00b7yet", "un\u00b7ter", "uns", ":", "Le\u00b7bet", "wol", "Herr", "Flo\u00b7ri\u00b7dan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$.", "VVFIN", "ADV", "NN", "NE", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}