{"textgrid.poem.35973": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mir im Auge", "tokens": ["Wie", "mir", "im", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Tr\u00e4nen stehn!", "tokens": ["Die", "Tr\u00e4\u00b7nen", "stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie ist mir heute", "tokens": ["Wie", "ist", "mir", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So weh geschehn.", "tokens": ["So", "weh", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Vor ihrem Fenster", "tokens": ["Vor", "ih\u00b7rem", "Fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Blumen mein", "tokens": ["Die", "Blu\u00b7men", "mein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hat all zerschlagen", "tokens": ["Hat", "all", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein gro\u00dfer Stein.", "tokens": ["Ein", "gro\u00b7\u00dfer", "Stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich sah ihn kommen", "tokens": ["Ich", "sah", "ihn", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den Berg hinab", "tokens": ["Den", "Berg", "hin\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und knicken schnelle", "tokens": ["Und", "kni\u00b7cken", "schnel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Blumen ab.", "tokens": ["Die", "Blu\u00b7men", "ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nun liegt er mitten", "tokens": ["Nun", "liegt", "er", "mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im G\u00e4rtchen dort,", "tokens": ["Im", "G\u00e4rt\u00b7chen", "dort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sieht aus, als wollt' er", "tokens": ["Sieht", "aus", ",", "als", "wollt'", "er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "VMFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nicht wieder fort.", "tokens": ["Nicht", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df nun wieder", "tokens": ["Ich", "mu\u00df", "nun", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hin pflanzen gehn.", "tokens": ["Hin", "pflan\u00b7zen", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie mir im Auge", "tokens": ["Wie", "mir", "im", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Tr\u00e4nen stehn.", "tokens": ["Die", "Tr\u00e4\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Was schlugst du nieder", "tokens": ["Was", "schlugst", "du", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Freude mein,", "tokens": ["Die", "Freu\u00b7de", "mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die zarten Blumen,", "tokens": ["Die", "zar\u00b7ten", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du grober Stein!", "tokens": ["Du", "gro\u00b7ber", "Stein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}