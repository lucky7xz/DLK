{"textgrid.poem.43209": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Abbazia", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewiges Meer, wie bist Du herrlich,", "tokens": ["E\u00b7wi\u00b7ges", "Meer", ",", "wie", "bist", "Du", "herr\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn der Sturm Dich wild bewegt,", "tokens": ["Wenn", "der", "Sturm", "Dich", "wild", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die schaumbedeckte Woge", "tokens": ["Wenn", "die", "schaum\u00b7be\u00b7deck\u00b7te", "Wo\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Felsenufer schl\u00e4gt.", "tokens": ["An", "die", "Fel\u00b7se\u00b7nu\u00b7fer", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die M\u00f6ve kreischend flattert,", "tokens": ["Wenn", "die", "M\u00f6\u00b7ve", "krei\u00b7schend", "flat\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hastig ihre Beute sucht,", "tokens": ["Has\u00b7tig", "ih\u00b7re", "Beu\u00b7te", "sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn des Schiffes Masten krachen", "tokens": ["Wenn", "des", "Schif\u00b7fes", "Mas\u00b7ten", "kra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der rauhe Seemann flucht.", "tokens": ["Und", "der", "rau\u00b7he", "See\u00b7mann", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Also hab' ich Dich bewundert", "tokens": ["Al\u00b7so", "hab'", "ich", "Dich", "be\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort an Abbazia's Strand,", "tokens": ["Dort", "an", "Ab\u00b7ba\u00b7zia's", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dort in jenem Tropengarten,", "tokens": ["Dort", "in", "je\u00b7nem", "Tro\u00b7pen\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ich M\u00e4rchenblumen fand.", "tokens": ["Wo", "ich", "M\u00e4r\u00b7chen\u00b7blu\u00b7men", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber meinem Haupt die Berge", "tokens": ["Ue\u00b7ber", "mei\u00b7nem", "Haupt", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Sonne letztem Gl\u00fchn,", "tokens": ["In", "der", "Son\u00b7ne", "letz\u00b7tem", "Gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir zu F\u00fc\u00dfen Brandungstoben,", "tokens": ["Mir", "zu", "F\u00fc\u00b7\u00dfen", "Bran\u00b7dungs\u00b7to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rund um mich ein duftend Bl\u00fchn.", "tokens": ["Rund", "um", "mich", "ein", "duf\u00b7tend", "Bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kampf und Friede, Licht und Schatten,", "tokens": ["Kampf", "und", "Frie\u00b7de", ",", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Land und Meer so seltsam-sch\u00f6n,", "tokens": ["Land", "und", "Meer", "so", "selt\u00b7sam\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wogenschaum und fremde Blumen,", "tokens": ["Wo\u00b7gen\u00b7schaum", "und", "frem\u00b7de", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vogellied und Sturmgedr\u00f6hn.", "tokens": ["Vo\u00b7gel\u00b7lied", "und", "Sturm\u00b7ge\u00b7dr\u00f6hn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Seele sang begeistert", "tokens": ["Mei\u00b7ne", "See\u00b7le", "sang", "be\u00b7geis\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit in dem Gigantenchor,", "tokens": ["Mit", "in", "dem", "Gi\u00b7gan\u00b7ten\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es rang f\u00fcr Land und Menschen", "tokens": ["Und", "es", "rang", "f\u00fcr", "Land", "und", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ein Segenswort empor.", "tokens": ["Sich", "ein", "Se\u00b7gens\u00b7wort", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}