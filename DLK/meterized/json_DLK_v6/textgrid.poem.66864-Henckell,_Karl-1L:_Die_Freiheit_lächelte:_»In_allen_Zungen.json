{"textgrid.poem.66864": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Freiheit l\u00e4chelte: \u00bbIn allen Zungen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit l\u00e4chelte: \u00bbIn allen Zungen", "tokens": ["Die", "Frei\u00b7heit", "l\u00e4\u00b7chel\u00b7te", ":", "\u00bb", "In", "al\u00b7len", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von wie viel Dichtern ward ich schon besungen!", "tokens": ["Von", "wie", "viel", "Dich\u00b7tern", "ward", "ich", "schon", "be\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie preist ihr unerm\u00fcdlich meinen Sinn,", "tokens": ["Wie", "preist", "ihr", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den unvollkommnen \u2013 einzig weil ich ", "tokens": ["Den", "un\u00b7voll\u00b7komm\u00b7nen", "\u2013", "ein\u00b7zig", "weil", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$(", "ADJD", "KOUS", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich durch dieses kampfumh\u00fcllte Leben", "tokens": ["Wenn", "ich", "durch", "die\u00b7ses", "kamp\u00b7fum\u00b7h\u00fcll\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Mantels Sonnenschleier lasse schweben,", "tokens": ["Des", "Man\u00b7tels", "Son\u00b7nen\u00b7schlei\u00b7er", "las\u00b7se", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Viel wei\u00dfe Rosen, rote Nelken streue", "tokens": ["Viel", "wei\u00b7\u00dfe", "Ro\u00b7sen", ",", "ro\u00b7te", "Nel\u00b7ken", "streu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Pfad zu Pfad, da\u00df sich die Jugend freue,", "tokens": ["Von", "Pfad", "zu", "Pfad", ",", "da\u00df", "sich", "die", "Ju\u00b7gend", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Menschheitsjugend, die das Haupt sich schm\u00fcckt,", "tokens": ["Die", "Menschheits\u00b7ju\u00b7gend", ",", "die", "das", "Haupt", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Von solchen Gaben, ach, so gern begl\u00fcckt ...", "tokens": ["Von", "sol\u00b7chen", "Ga\u00b7ben", ",", "ach", ",", "so", "gern", "be\u00b7gl\u00fcckt", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ITJ", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie schallen mir aus Lauben und aus Hallen", "tokens": ["Wie", "schal\u00b7len", "mir", "aus", "Lau\u00b7ben", "und", "aus", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ges\u00e4nge reich, mir liebend zu gefallen!", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "reich", ",", "mir", "lie\u00b7bend", "zu", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bald brausend tief, bald zart und strahlenfein,", "tokens": ["Bald", "brau\u00b7send", "tief", ",", "bald", "zart", "und", "strah\u00b7len\u00b7fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bald \u00fcberschwenglich wirr, bald sph\u00e4renrein.", "tokens": ["Bald", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "wirr", ",", "bald", "sph\u00e4\u00b7ren\u00b7rein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein Gru\u00df nur im Vor\u00fcbergehn \u2013 da schauen", "tokens": ["Ein", "Gru\u00df", "nur", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "\u2013", "da", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$(", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Voll seliger Sehnsucht mich die Menschen an,", "tokens": ["Voll", "se\u00b7li\u00b7ger", "Sehn\u00b7sucht", "mich", "die", "Men\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.17": {"text": "Sie m\u00f6chten hoffend mir ihr Gl\u00fcck vertrauen", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "hof\u00b7fend", "mir", "ihr", "Gl\u00fcck", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und f\u00fchlen, da\u00df ich ihnen helfen kann.", "tokens": ["Und", "f\u00fch\u00b7len", ",", "da\u00df", "ich", "ih\u00b7nen", "hel\u00b7fen", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Vielleicht ... ein wenig ... doch Geduld bescheidet", "tokens": ["Viel\u00b7leicht", "...", "ein", "we\u00b7nig", "...", "doch", "Ge\u00b7duld", "be\u00b7schei\u00b7det"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "ART", "PIS", "$(", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mir meine gar zu strenge Mutter Not,", "tokens": ["Mir", "mei\u00b7ne", "gar", "zu", "stren\u00b7ge", "Mut\u00b7ter", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich mu\u00df oft weinen, wie die Menschheit leidet,", "tokens": ["Ich", "mu\u00df", "oft", "wei\u00b7nen", ",", "wie", "die", "Menschheit", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Doch darf ich l\u00e4cheln, sprie\u00dft das Morgenrot ...\u00ab", "tokens": ["Doch", "darf", "ich", "l\u00e4\u00b7cheln", ",", "sprie\u00dft", "das", "Mor\u00b7gen\u00b7rot", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}