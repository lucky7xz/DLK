{"textgrid.poem.25558": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll ich der Flucht von meinen Tagen schelten?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich der Flucht von meinen Tagen schelten?", "tokens": ["Soll", "ich", "der", "Flucht", "von", "mei\u00b7nen", "Ta\u00b7gen", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll ich auf sie und ihre Tr\u00e4gheit schm\u00e4hn?", "tokens": ["Soll", "ich", "auf", "sie", "und", "ih\u00b7re", "Tr\u00e4g\u00b7heit", "schm\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll dieser Blick dort nach zufriednen Welten,", "tokens": ["Soll", "die\u00b7ser", "Blick", "dort", "nach", "zu\u00b7fried\u00b7nen", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie? oder noch ins Thal des Kummers sehn?", "tokens": ["Wie", "?", "o\u00b7der", "noch", "ins", "Thal", "des", "Kum\u00b7mers", "sehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was willst du thun? du Geist! der sonst so stille,", "tokens": ["Was", "willst", "du", "thun", "?", "du", "Geist", "!", "der", "sonst", "so", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "PPER", "NN", "$.", "ART", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der jung und fromm, der Scherze Meister war?", "tokens": ["Der", "jung", "und", "fromm", ",", "der", "Scher\u00b7ze", "Meis\u00b7ter", "war", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist Schmerz dein Wunsch? Ist Traurigkeit dein Wille?", "tokens": ["Ist", "Schmerz", "dein", "Wunsch", "?", "Ist", "Trau\u00b7rig\u00b7keit", "dein", "Wil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$.", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was willst du thun mit diesem langen Jahr'?", "tokens": ["Was", "willst", "du", "thun", "mit", "die\u00b7sem", "lan\u00b7gen", "Jahr'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der J\u00fcngling rennt, um Veilchen einzusammlen,", "tokens": ["Der", "J\u00fcng\u00b7ling", "rennt", ",", "um", "Veil\u00b7chen", "ein\u00b7zu\u00b7samm\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und sonnet sich auf kaum gebornem Klee;", "tokens": ["Und", "son\u00b7net", "sich", "auf", "kaum", "ge\u00b7bor\u00b7nem", "Klee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Das M\u00e4dchen sitzt, geheimen Wunsch zu stammlen,", "tokens": ["Das", "M\u00e4d\u00b7chen", "sitzt", ",", "ge\u00b7hei\u00b7men", "Wunsch", "zu", "stamm\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und spiegelt sich im aufgekl\u00e4rten See.", "tokens": ["Und", "spie\u00b7gelt", "sich", "im", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Allein nur ich, ich habe keine Freuden,", "tokens": ["Al\u00b7lein", "nur", "ich", ",", "ich", "ha\u00b7be", "kei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Todt ist f\u00fcr mich die Stadt, und todt die Flur;", "tokens": ["Todt", "ist", "f\u00fcr", "mich", "die", "Stadt", ",", "und", "todt", "die", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Den Fr\u00f6hlichen k\u00f6nnt' ich vielleicht beneiden,", "tokens": ["Den", "Fr\u00f6h\u00b7li\u00b7chen", "k\u00f6nnt'", "ich", "viel\u00b7leicht", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Den Traurigen, was hehl' ich's? lieb' ich nur.", "tokens": ["Den", "Trau\u00b7ri\u00b7gen", ",", "was", "hehl'", "ich's", "?", "lieb'", "ich", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "VVFIN", "PIS", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zwar fluch' ich nicht der Liebe, nicht den Scherzen,", "tokens": ["Zwar", "fluch'", "ich", "nicht", "der", "Lie\u00b7be", ",", "nicht", "den", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Denn ohne sie ist dieses Leben, Nacht.", "tokens": ["Denn", "oh\u00b7ne", "sie", "ist", "die\u00b7ses", "Le\u00b7ben", ",", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PDAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "O selig der, in dessen Engelherzen", "tokens": ["O", "se\u00b7lig", "der", ",", "in", "des\u00b7sen", "En\u00b7gel\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "ART", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "So Tag f\u00fcr Tag die Fr\u00fchlingssonne lacht!", "tokens": ["So", "Tag", "f\u00fcr", "Tag", "die", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch mu\u00df nicht ich mich selbst im Fr\u00fchling' gr\u00e4men?", "tokens": ["Doch", "mu\u00df", "nicht", "ich", "mich", "selbst", "im", "Fr\u00fch\u00b7ling'", "gr\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PPER", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wer tr\u00f6stet mich? ich selbst versteh' es nicht!", "tokens": ["Wer", "tr\u00f6s\u00b7tet", "mich", "?", "ich", "selbst", "ver\u00b7steh'", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Kommt, lehret nur mich meines Kummers sch\u00e4men,", "tokens": ["Kommt", ",", "leh\u00b7ret", "nur", "mich", "mei\u00b7nes", "Kum\u00b7mers", "sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So sehr mein Herz auch f\u00fcr den Kummer spricht.", "tokens": ["So", "sehr", "mein", "Herz", "auch", "f\u00fcr", "den", "Kum\u00b7mer", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wem sag' ich das? vier kalten tauben W\u00e4nden!", "tokens": ["Wem", "sag'", "ich", "das", "?", "vier", "kal\u00b7ten", "tau\u00b7ben", "W\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "$.", "CARD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch klagt' ich selbst vier hei\u00dfen Freunden vor,", "tokens": ["Doch", "klagt'", "ich", "selbst", "vier", "hei\u00b7\u00dfen", "Freun\u00b7den", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Sobald auch sie das M\u00e4dchen schuldig f\u00e4nden:", "tokens": ["So\u00b7bald", "auch", "sie", "das", "M\u00e4d\u00b7chen", "schul\u00b7dig", "f\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "W\u00e4r' aller Trost umsonst, und taub mein Ohr!", "tokens": ["W\u00e4r'", "al\u00b7ler", "Trost", "um\u00b7sonst", ",", "und", "taub", "mein", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Ach! wer sie sieht, mag keine Andre sehen,", "tokens": ["Ach", "!", "wer", "sie", "sieht", ",", "mag", "kei\u00b7ne", "And\u00b7re", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "PPER", "VVFIN", "$,", "VMFIN", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Dem, der sie h\u00f6rt, singt ", "tokens": ["Dem", ",", "der", "sie", "h\u00f6rt", ",", "singt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "++-++", "measure": "unknown.measure.tetra"}, "line.31": {"text": "Der, dem sie lacht, h\u00f6rt auf die Welt zu schm\u00e4hen,", "tokens": ["Der", ",", "dem", "sie", "lacht", ",", "h\u00f6rt", "auf", "die", "Welt", "zu", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dem, der sie k\u00fc\u00dft, wird sie zum Paradies.", "tokens": ["Dem", ",", "der", "sie", "k\u00fc\u00dft", ",", "wird", "sie", "zum", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Dem aber wird die Welt zum d\u00fcstern Kerker,", "tokens": ["Dem", "a\u00b7ber", "wird", "die", "Welt", "zum", "d\u00fcs\u00b7tern", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Das Sonnenlicht zum Lampenschein' darin,", "tokens": ["Das", "Son\u00b7nen\u00b7licht", "zum", "Lam\u00b7pen\u00b7schein'", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Wer sie verliert. Der Weis' ist dann nicht st\u00e4rker", "tokens": ["Wer", "sie", "ver\u00b7liert", ".", "Der", "Weis'", "ist", "dann", "nicht", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Als wie der Thor. Hin ist ihm alles, hin!", "tokens": ["Als", "wie", "der", "Thor", ".", "Hin", "ist", "ihm", "al\u00b7les", ",", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$.", "NN", "VAFIN", "PPER", "PIS", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Erlaubte gleich der Himmel, der zum Lachen", "tokens": ["Er\u00b7laub\u00b7te", "gleich", "der", "Him\u00b7mel", ",", "der", "zum", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Und Weinen mich gebildet hat, die Th\u00fcr", "tokens": ["Und", "Wei\u00b7nen", "mich", "ge\u00b7bil\u00b7det", "hat", ",", "die", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "PPER", "VVPP", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Des Lebens, selbst mir fr\u00fcher aufzumachen", "tokens": ["Des", "Le\u00b7bens", ",", "selbst", "mir", "fr\u00fc\u00b7her", "auf\u00b7zu\u00b7ma\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Und wegzugehn, ich bliebe dennoch hier.", "tokens": ["Und", "weg\u00b7zu\u00b7gehn", ",", "ich", "blie\u00b7be", "den\u00b7noch", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Ich bliebe hier, um nur an sie zu denken,", "tokens": ["Ich", "blie\u00b7be", "hier", ",", "um", "nur", "an", "sie", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUI", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "In meinem Gram' allm\u00e4hlig zu vergehn,", "tokens": ["In", "mei\u00b7nem", "Gram'", "all\u00b7m\u00e4h\u00b7lig", "zu", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und endlich noch Vergebung ihr zu schenken,", "tokens": ["Und", "end\u00b7lich", "noch", "Ver\u00b7ge\u00b7bung", "ihr", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Sie dann begl\u00fcckt, und mich beweint zu sehn.", "tokens": ["Sie", "dann", "be\u00b7gl\u00fcckt", ",", "und", "mich", "be\u00b7weint", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "KON", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Fort! la\u00dft mich hin zum Bach' der Riesenh\u00f6hle,", "tokens": ["Fort", "!", "la\u00dft", "mich", "hin", "zum", "Bach'", "der", "Rie\u00b7sen\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Wo sie das Herz mir einst mit Schw\u00fcren gab:", "tokens": ["Wo", "sie", "das", "Herz", "mir", "einst", "mit", "Schw\u00fc\u00b7ren", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Aushauchen will ich da die m\u00fcde Seele;", "tokens": ["Aus\u00b7hau\u00b7chen", "will", "ich", "da", "die", "m\u00fc\u00b7de", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Sie kommt vielleicht und weinet auf mein Grab.", "tokens": ["Sie", "kommt", "viel\u00b7leicht", "und", "wei\u00b7net", "auf", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}