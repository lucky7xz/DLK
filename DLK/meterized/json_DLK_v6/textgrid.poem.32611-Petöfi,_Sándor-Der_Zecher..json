{"textgrid.poem.32611": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Der Zecher.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei dem Sorgenbrecher Wein", "tokens": ["Bei", "dem", "Sor\u00b7gen\u00b7bre\u00b7cher", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiter mir das Leben lacht,", "tokens": ["Hei\u00b7ter", "mir", "das", "Le\u00b7ben", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei dem Sorgenbrecher Wein,", "tokens": ["Bei", "dem", "Sor\u00b7gen\u00b7bre\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schicksal, trotz' ich deiner Macht!", "tokens": ["Schick\u00b7sal", ",", "trotz'", "ich", "dei\u00b7ner", "Macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seid doch nicht erstaunt, wenn ich", "tokens": ["Seid", "doch", "nicht", "er\u00b7staunt", ",", "wenn", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAIMP", "ADV", "PTKNEG", "VVPP", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sage, da\u00df, als guter Christ,", "tokens": ["Sa\u00b7ge", ",", "da\u00df", ",", "als", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Gott des Weines mir", "tokens": ["Doch", "der", "Gott", "des", "Wei\u00b7nes", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets die liebste Zuflucht ist.", "tokens": ["Stets", "die", "liebs\u00b7te", "Zu\u00b7flucht", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin ich dieses Gottes voll,", "tokens": ["Bin", "ich", "die\u00b7ses", "Got\u00b7tes", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pfeif' ich auf die ganze Welt,", "tokens": ["Pfeif'", "ich", "auf", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der's mit Skorpionenwut", "tokens": ["Der's", "mit", "Skor\u00b7pi\u00b7o\u00b7nen\u00b7wut"], "token_info": ["word", "word", "word"], "pos": ["PIS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zu qu\u00e4len nur gef\u00e4llt.", "tokens": ["Mich", "zu", "qu\u00e4\u00b7len", "nur", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn der Wein hat mich gelehrt", "tokens": ["Denn", "der", "Wein", "hat", "mich", "ge\u00b7lehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche s\u00fc\u00dfe Melodein,", "tokens": ["Man\u00b7che", "s\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch vergessen mich gelehrt", "tokens": ["Auch", "ver\u00b7ges\u00b7sen", "mich", "ge\u00b7lehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch, ihr falschen M\u00e4gdelein!", "tokens": ["Euch", ",", "ihr", "fal\u00b7schen", "M\u00e4g\u00b7del\u00b7ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sitz' ich einst beim Weine just,", "tokens": ["Sitz'", "ich", "einst", "beim", "Wei\u00b7ne", "just", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Tod mich abberuft,", "tokens": ["Wenn", "der", "Tod", "mich", "ab\u00b7be\u00b7ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tu' ich einen letzten Schluck, \u2013", "tokens": ["Tu'", "ich", "ei\u00b7nen", "letz\u00b7ten", "Schluck", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachend fahr' ich in die Gruft!", "tokens": ["La\u00b7chend", "fahr'", "ich", "in", "die", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}