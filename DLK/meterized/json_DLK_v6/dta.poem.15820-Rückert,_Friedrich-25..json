{"dta.poem.15820": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als du mich kamst zu sehn, war ich zu Hause nicht,", "tokens": ["Als", "du", "mich", "kamst", "zu", "sehn", ",", "war", "ich", "zu", "Hau\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du verlorest mein Gespr\u00e4ch und mein Gesicht.", "tokens": ["Und", "du", "ver\u00b7lo\u00b7rest", "mein", "Ge\u00b7spr\u00e4ch", "und", "mein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An allen beiden hast du nicht zuviel verloren;", "tokens": ["An", "al\u00b7len", "bei\u00b7den", "hast", "du", "nicht", "zu\u00b7viel", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Sprechen bin ich nicht und nicht zum Sehn geboren.", "tokens": ["Zum", "Spre\u00b7chen", "bin", "ich", "nicht", "und", "nicht", "zum", "Sehn", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKNEG", "KON", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein denkendes Gef\u00fchl, ein innerlicher Sang,", "tokens": ["Ein", "den\u00b7ken\u00b7des", "Ge\u00b7f\u00fchl", ",", "ein", "in\u00b7ner\u00b7li\u00b7cher", "Sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist alles was ich bin, was mir zu seyn gelang.", "tokens": ["Ist", "al\u00b7les", "was", "ich", "bin", ",", "was", "mir", "zu", "seyn", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PWS", "PPER", "VAFIN", "$,", "PWS", "PPER", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und so, was an mir ist, send' ich zum Gru\u00df dir nieder,", "tokens": ["Und", "so", ",", "was", "an", "mir", "ist", ",", "send'", "ich", "zum", "Gru\u00df", "dir", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Echo meiner Brust, den Spiegel meiner Lieder.", "tokens": ["Das", "E\u00b7cho", "mei\u00b7ner", "Brust", ",", "den", "Spie\u00b7gel", "mei\u00b7ner", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}