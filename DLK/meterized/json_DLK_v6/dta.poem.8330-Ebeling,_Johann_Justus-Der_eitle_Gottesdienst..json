{"dta.poem.8330": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der eitle Gottesdienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch! wilt du GOttes Diener heissen,", "tokens": ["Mensch", "!", "wilt", "du", "Got\u00b7tes", "Die\u00b7ner", "heis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich bem\u00fchet ihn zu ehren;", "tokens": ["Der", "sich", "be\u00b7m\u00fc\u00b7het", "ihn", "zu", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So must du dich im Ernst befleissen,", "tokens": ["So", "must", "du", "dich", "im", "Ernst", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Herz zu ihm hinauf zu kehren.", "tokens": ["Dein", "Herz", "zu", "ihm", "hin\u00b7auf", "zu", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Lippen \u00e4userlich Bewegen,", "tokens": ["Der", "Lip\u00b7pen", "\u00e4u\u00b7ser\u00b7lich", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Zungen lallendes Geth\u00f6n,", "tokens": ["Der", "Zun\u00b7gen", "lal\u00b7len\u00b7des", "Ge\u00b7th\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gestimmet nach den Glokken-Schl\u00e4gen,", "tokens": ["Ge\u00b7stim\u00b7met", "nach", "den", "Glok\u00b7ken\u00b7Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00e4\u00dft nur die \u00e4usre Andacht sehn.", "tokens": ["L\u00e4\u00dft", "nur", "die", "\u00e4us\u00b7re", "An\u00b7dacht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wilt du ein rechtes Opfer bringen,", "tokens": ["Wilt", "du", "ein", "rech\u00b7tes", "Op\u00b7fer", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das durch die Andacht angegl\u00fcht,", "tokens": ["Das", "durch", "die", "An\u00b7dacht", "an\u00b7ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So sey dabei vor allen Dingen,", "tokens": ["So", "sey", "da\u00b7bei", "vor", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Um ein gereinigt Herz bem\u00fcht.", "tokens": ["Um", "ein", "ge\u00b7rei\u00b7nigt", "Herz", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "VVPP", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wo Weirauch des Gebetes glimmet,", "tokens": ["Wo", "Wei\u00b7rauch", "des", "Ge\u00b7be\u00b7tes", "glim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dabei das Herz vor K\u00e4lte friert,", "tokens": ["Da\u00b7bei", "das", "Herz", "vor", "K\u00e4l\u00b7te", "friert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und wo der Mund ein Lied anstimmet,", "tokens": ["Und", "wo", "der", "Mund", "ein", "Lied", "an\u00b7stim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das nicht des Glaubens Kraft gebiehrt,", "tokens": ["Das", "nicht", "des", "Glau\u00b7bens", "Kraft", "ge\u00b7biehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da kanst du deinen ", "tokens": ["Da", "kanst", "du", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Du s\u00fcndigest dadurch vielmehr", "tokens": ["Du", "s\u00fcn\u00b7di\u00b7gest", "da\u00b7durch", "viel\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und sch\u00e4ndest durch verstellte Mienen,", "tokens": ["Und", "sch\u00e4n\u00b7dest", "durch", "ver\u00b7stell\u00b7te", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Des grossen Sch\u00f6pfers heilge Ehr.", "tokens": ["Des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "heil\u00b7ge", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des H\u00f6chsten ewge Gnade suchen,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "ew\u00b7ge", "Gna\u00b7de", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Um seine Huld erb\u00e4rmlich schrein,", "tokens": ["Um", "sei\u00b7ne", "Huld", "er\u00b7b\u00e4rm\u00b7lich", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Den N\u00e4chsten wie ein Greul verfluchen,", "tokens": ["Den", "N\u00e4chs\u00b7ten", "wie", "ein", "Greul", "ver\u00b7flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Stimmt nicht mit dem Verlangen ein.", "tokens": ["Stimmt", "nicht", "mit", "dem", "Ver\u00b7lan\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Den H\u00f6chsten mit dem Lippen ehren,", "tokens": ["Den", "H\u00f6chs\u00b7ten", "mit", "dem", "Lip\u00b7pen", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wenn man in seinem Hause steht,", "tokens": ["Wenn", "man", "in", "sei\u00b7nem", "Hau\u00b7se", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und ihm hernach den R\u00fckken kehren,", "tokens": ["Und", "ihm", "her\u00b7nach", "den", "R\u00fck\u00b7ken", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.28": {"text": "Wenn man aus seinem Tempel geht,", "tokens": ["Wenn", "man", "aus", "sei\u00b7nem", "Tem\u00b7pel", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das heist ihn ehren mit den H\u00e4nden,", "tokens": ["Das", "heist", "ihn", "eh\u00b7ren", "mit", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Hingegen mit dem Herz, der That", "tokens": ["Hin\u00b7ge\u00b7gen", "mit", "dem", "Herz", ",", "der", "That"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.31": {"text": "In seiner eignen H\u00fctte sch\u00e4nden.", "tokens": ["In", "sei\u00b7ner", "eig\u00b7nen", "H\u00fct\u00b7te", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wer seinem ", "tokens": ["Wer", "sei\u00b7nem"], "token_info": ["word", "word"], "pos": ["PWS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Weil es die Mode mit sich bringet,", "tokens": ["Weil", "es", "die", "Mo\u00b7de", "mit", "sich", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und wer in seinem Heiligthum,", "tokens": ["Und", "wer", "in", "sei\u00b7nem", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Mit Seufzen blutge H\u00e4nde ringet,", "tokens": ["Mit", "Seuf\u00b7zen", "blut\u00b7ge", "H\u00e4n\u00b7de", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Verdienet auch sehr schlechten Ruhm.", "tokens": ["Ver\u00b7die\u00b7net", "auch", "sehr", "schlech\u00b7ten", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Die Einfalt meint ein blosses Lallen,", "tokens": ["Die", "Ein\u00b7falt", "meint", "ein", "blos\u00b7ses", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Das seufzend geht, und kl\u00e4glich klingt,", "tokens": ["Das", "seuf\u00b7zend", "geht", ",", "und", "kl\u00e4g\u00b7lich", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Das k\u00f6nn dem H\u00f6chsten wollgefallen,", "tokens": ["Das", "k\u00f6nn", "dem", "H\u00f6chs\u00b7ten", "woll\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Das sey ein Dienst der Seegen bringt.", "tokens": ["Das", "sey", "ein", "Dienst", "der", "See\u00b7gen", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sie glaubt wer in dem Tempel gehet,", "tokens": ["Sie", "glaubt", "wer", "in", "dem", "Tem\u00b7pel", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Jm heilgen Schlummer sich darstellt,", "tokens": ["Jm", "heil\u00b7gen", "Schlum\u00b7mer", "sich", "dar\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Vom Anfang bis zum Ende stehet,", "tokens": ["Vom", "An\u00b7fang", "bis", "zum", "En\u00b7de", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Das sey ein Dienst der ", "tokens": ["Das", "sey", "ein", "Dienst", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Betrogne Menschen! lernt erkennen,", "tokens": ["Be\u00b7trog\u00b7ne", "Men\u00b7schen", "!", "lernt", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Das alles sey nur eitler Tand,", "tokens": ["Das", "al\u00b7les", "sey", "nur", "eit\u00b7ler", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und noch kein Gottesdienst zu nennen,", "tokens": ["Und", "noch", "kein", "Got\u00b7tes\u00b7dienst", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Wenn man sizt an der heilgen Wand.", "tokens": ["Wenn", "man", "sizt", "an", "der", "heil\u00b7gen", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.49": {"text": "Wenn unbeweglich stille sizen,", "tokens": ["Wenn", "un\u00b7be\u00b7weg\u00b7lich", "stil\u00b7le", "si\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Ein Gottesdienst zu nennen w\u00e4r,", "tokens": ["Ein", "Got\u00b7tes\u00b7dienst", "zu", "nen\u00b7nen", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Wenns \u00e4usre ohne Herz k\u00f6nn n\u00fczen,", "tokens": ["Wenns", "\u00e4us\u00b7re", "oh\u00b7ne", "Herz", "k\u00f6nn", "n\u00fc\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und da\u00df des Allerh\u00f6chsten Ehr,", "tokens": ["Und", "da\u00df", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Bef\u00f6rdert w\u00fcrde, durch das Eilen", "tokens": ["Be\u00b7f\u00f6r\u00b7dert", "w\u00fcr\u00b7de", ",", "durch", "das", "Ei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Zu seines heilgen Nahmens Haus;", "tokens": ["Zu", "sei\u00b7nes", "heil\u00b7gen", "Nah\u00b7mens", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "So w\u00fcrden todte Bilders\u00e4ulen", "tokens": ["So", "w\u00fcr\u00b7den", "tod\u00b7te", "Bil\u00b7der\u00b7s\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "(denn diese kommen nie heraus)", "tokens": ["(", "denn", "die\u00b7se", "kom\u00b7men", "nie", "he\u00b7rau