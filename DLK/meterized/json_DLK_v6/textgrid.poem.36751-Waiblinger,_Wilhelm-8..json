{"textgrid.poem.36751": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' es m\u00f6glich, versucht' ich's zu r\u00fchmen, was huldreich der Genius", "tokens": ["W\u00e4r'", "es", "m\u00f6g\u00b7lich", ",", "ver\u00b7sucht'", "ich's", "zu", "r\u00fch\u00b7men", ",", "was", "huld\u00b7reich", "der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VVFIN", "PIS", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "ART", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Deinem Gedanken, was er all' deinem Mei\u00dfel vertraut,", "tokens": ["Dei\u00b7nem", "Ge\u00b7dan\u00b7ken", ",", "was", "er", "all'", "dei\u00b7nem", "Mei\u00b7\u00dfel", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "PPER", "PIAT", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dann auch w\u00fc\u00dft' ich, wie m\u00f6glich, da\u00df nun der Genius der Vorzeit", "tokens": ["Dann", "auch", "w\u00fc\u00dft'", "ich", ",", "wie", "m\u00f6g\u00b7lich", ",", "da\u00df", "nun", "der", "Ge\u00b7nius", "der", "Vor\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+---+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In der Mitwelt so reich, m\u00e4nnlich und th\u00e4tig sich zeigt.", "tokens": ["In", "der", "Mit\u00b7welt", "so", "reich", ",", "m\u00e4nn\u00b7lich", "und", "th\u00e4\u00b7tig", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "PRF", "VVFIN", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}}}}}