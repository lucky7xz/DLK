{"textgrid.poem.65259": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der st\u00fcrmische Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hat der Sturm zerrissen", "tokens": ["Wie", "hat", "der", "Sturm", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Himmels graues Kleid!", "tokens": ["Des", "Him\u00b7mels", "grau\u00b7es", "Kleid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wolkenfetzen flattern", "tokens": ["Die", "Wol\u00b7ken\u00b7fet\u00b7zen", "flat\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umher in mattem Streit.", "tokens": ["Um\u00b7her", "in", "mat\u00b7tem", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und rothe Feuerflammen", "tokens": ["Und", "ro\u00b7the", "Feu\u00b7er\u00b7flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ziehn zwischen ihnen hin.", "tokens": ["Ziehn", "zwi\u00b7schen", "ih\u00b7nen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das nenn' ich einen Morgen", "tokens": ["Das", "nenn'", "ich", "ei\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So recht nach meinem Sinn!", "tokens": ["So", "recht", "nach", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Herz sieht an dem Himmel", "tokens": ["Mein", "Herz", "sieht", "an", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gemalt sein eignes Bild \u2013", "tokens": ["Ge\u00b7malt", "sein", "eig\u00b7nes", "Bild", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist nichts als der Winter,", "tokens": ["Es", "ist", "nichts", "als", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Winter kalt und wild!", "tokens": ["Der", "Win\u00b7ter", "kalt", "und", "wild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}