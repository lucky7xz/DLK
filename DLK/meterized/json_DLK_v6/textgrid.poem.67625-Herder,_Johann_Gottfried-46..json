{"textgrid.poem.67625": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fuchs hatt' seinen Schwanz verloren.", "tokens": ["Der", "Fuchs", "hatt'", "sei\u00b7nen", "Schwanz", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbh\u00f6rt!\u00ab fing er an vor allen Ohren,", "tokens": ["\u00bb", "h\u00f6rt", "!", "\u00ab", "fing", "er", "an", "vor", "al\u00b7len", "Oh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "PPER", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbihr Br\u00fcder, h\u00f6rt, was soll der Schwanz?", "tokens": ["\u00bb", "ihr", "Br\u00fc\u00b7der", ",", "h\u00f6rt", ",", "was", "soll", "der", "Schwanz", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PWS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich ohn' ihn nicht ganz?", "tokens": ["Bin", "ich", "ohn'", "ihn", "nicht", "ganz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Seht mich an! sehet Ihr?\u00ab", "tokens": ["Seht", "mich", "an", "!", "se\u00b7het", "Ihr", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "\u00bbja, freilich sehen wir,", "tokens": ["\u00bb", "ja", ",", "frei\u00b7lich", "se\u00b7hen", "wir", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du kluges Thier,", "tokens": ["Du", "klu\u00b7ges", "Thier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Woran es Dir gebricht;", "tokens": ["Wo\u00b7ran", "es", "Dir", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sonst riethest Du's auch nicht!\u00ab", "tokens": ["Sonst", "riet\u00b7hest", "Du's", "auch", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geh\u00f6rt zur Fabel Poesie?", "tokens": ["Ge\u00b7h\u00f6rt", "zur", "Fa\u00b7bel", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbnein! denn mir fehlet die.\u00ab", "tokens": ["\u00bb", "nein", "!", "denn", "mir", "feh\u00b7let", "die", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$.", "KON", "PPER", "VVFIN", "ART", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh\u00f6rt zur Fabel Witz?", "tokens": ["Ge\u00b7h\u00f6rt", "zur", "Fa\u00b7bel", "Witz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbja! denn die mein' ist spitz.\u00ab", "tokens": ["\u00bb", "ja", "!", "denn", "die", "mein'", "ist", "spitz", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$.", "KON", "ART", "PPOSS", "VAFIN", "ADJD", "$.", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}}}}}