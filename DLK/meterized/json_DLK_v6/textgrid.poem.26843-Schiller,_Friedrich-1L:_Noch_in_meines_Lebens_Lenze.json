{"textgrid.poem.26843": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch in meines Lebens Lenze", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch in meines Lebens Lenze", "tokens": ["Noch", "in", "mei\u00b7nes", "Le\u00b7bens", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War ich, und ich wandert aus,", "tokens": ["War", "ich", ",", "und", "ich", "wan\u00b7dert", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Jugend frohe T\u00e4nze", "tokens": ["Und", "der", "Ju\u00b7gend", "fro\u00b7he", "T\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df ich in des Vaters Haus.", "tokens": ["Lie\u00df", "ich", "in", "des", "Va\u00b7ters", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "All mein Erbteil, meine Habe", "tokens": ["All", "mein", "Erb\u00b7teil", ",", "mei\u00b7ne", "Ha\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warf ich fr\u00f6hlich glaubend hin,", "tokens": ["Warf", "ich", "fr\u00f6h\u00b7lich", "glau\u00b7bend", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am leichten Pilgerstabe", "tokens": ["Und", "am", "leich\u00b7ten", "Pil\u00b7ger\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zog ich fort mit Kindersinn.", "tokens": ["Zog", "ich", "fort", "mit", "Kin\u00b7der\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn mich trieb ein m\u00e4chtig Hoffen", "tokens": ["Denn", "mich", "trieb", "ein", "m\u00e4ch\u00b7tig", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein dunkles Glaubenswort,", "tokens": ["Und", "ein", "dunk\u00b7les", "Glau\u00b7bens\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwandle\u00ab, riefs, \u00bbder Weg ist offen,", "tokens": ["\u00bb", "wand\u00b7le", "\u00ab", ",", "riefs", ",", "\u00bb", "der", "Weg", "ist", "of\u00b7fen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "$,", "VVFIN", "$,", "$(", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer nach dem Aufgang fort.", "tokens": ["Im\u00b7mer", "nach", "dem", "Auf\u00b7gang", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis zu einer goldnen Pforten", "tokens": ["Bis", "zu", "ei\u00b7ner", "gold\u00b7nen", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du gelangst, da gehst du ein,", "tokens": ["Du", "ge\u00b7langst", ",", "da", "gehst", "du", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn das Irdische wird dorten", "tokens": ["Denn", "das", "Ir\u00b7di\u00b7sche", "wird", "dor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmlisch unverg\u00e4nglich sein.\u00ab", "tokens": ["Himm\u00b7lisch", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Abend wards und wurde Morgen,", "tokens": ["A\u00b7bend", "wards", "und", "wur\u00b7de", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer, nimmer stand ich still,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "stand", "ich", "still", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber immer bliebs verborgen,", "tokens": ["A\u00b7ber", "im\u00b7mer", "bliebs", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich suche, was ich will.", "tokens": ["Was", "ich", "su\u00b7che", ",", "was", "ich", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Berge lagen mir im Wege,", "tokens": ["Ber\u00b7ge", "la\u00b7gen", "mir", "im", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6me hemmten meinen Fu\u00df,", "tokens": ["Str\u00f6\u00b7me", "hemm\u00b7ten", "mei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber Schl\u00fcnde baut ich Stege,", "tokens": ["\u00dc\u00b7ber", "Schl\u00fcn\u00b7de", "baut", "ich", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fccken durch den wilden Flu\u00df.", "tokens": ["Br\u00fc\u00b7cken", "durch", "den", "wil\u00b7den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und zu eines Stroms Gestaden", "tokens": ["Und", "zu", "ei\u00b7nes", "Stroms", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ich, der nach Morgen flo\u00df,", "tokens": ["Kam", "ich", ",", "der", "nach", "Mor\u00b7gen", "flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh vertrauend seinem Faden,", "tokens": ["Froh", "ver\u00b7trau\u00b7end", "sei\u00b7nem", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werf ich mich in seinen Scho\u00df.", "tokens": ["Werf", "ich", "mich", "in", "sei\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hin zu einem gro\u00dfen Meere", "tokens": ["Hin", "zu", "ei\u00b7nem", "gro\u00b7\u00dfen", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trieb mich seiner Wellen Spiel,", "tokens": ["Trieb", "mich", "sei\u00b7ner", "Wel\u00b7len", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor mir liegts in weiter Leere,", "tokens": ["Vor", "mir", "liegts", "in", "wei\u00b7ter", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4her bin ich nicht dem Ziel.", "tokens": ["N\u00e4\u00b7her", "bin", "ich", "nicht", "dem", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, kein Steg will dahin f\u00fchren,", "tokens": ["Ach", ",", "kein", "Steg", "will", "da\u00b7hin", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "VMFIN", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, der Himmel \u00fcber mir", "tokens": ["Ach", ",", "der", "Him\u00b7mel", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will die Erde nie ber\u00fchren,", "tokens": ["Will", "die", "Er\u00b7de", "nie", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Dort ist niemals Hier.", "tokens": ["Und", "das", "Dort", "ist", "nie\u00b7mals", "Hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}