{"dta.poem.21818": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Was Musen/ wo kein Geld ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Pakket euch/ ihr Pierinnen/", "tokens": ["Pak\u00b7ket", "euch", "/", "ihr", "Pie\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "wo ihr mir nicht helffen k\u00f6nnt!", "tokens": ["wo", "ihr", "mir", "nicht", "helf\u00b7fen", "k\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00f6bus/ Pallas/ weicht von hinnen/", "tokens": ["F\u00f6\u00b7bus", "/", "Pal\u00b7las", "/", "weicht", "von", "hin\u00b7nen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "VVFIN", "APPR", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht ein Blikk sey Euch verg\u00f6nnt!", "tokens": ["nicht", "ein", "Blikk", "sey", "Euch", "ver\u00b7g\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wo nicht euer Reimen-zwingen", "tokens": ["wo", "nicht", "eu\u00b7er", "Rei\u00b7men\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eitle Feder/ sey zerstossen/", "tokens": ["Eit\u00b7le", "Fe\u00b7der", "/", "sey", "zer\u00b7stos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nu mich Kloris au\u00dfgeschlossen/", "tokens": ["Nu", "mich", "Klo\u00b7ris", "au\u00df\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NE", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "n\u00fczzt mir keiner Verse Zier.", "tokens": ["n\u00fczzt", "mir", "kei\u00b7ner", "Ver\u00b7se", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nu der Geiz sie hat verblendet", "tokens": ["Nu", "der", "Geiz", "sie", "hat", "ver\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ist mein Dichterwerk gesch\u00e4ndet.", "tokens": ["ist", "mein", "Dich\u00b7ter\u00b7werk", "ge\u00b7sch\u00e4n\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darum hat mir euer Feuer", "tokens": ["Da\u00b7rum", "hat", "mir", "eu\u00b7er", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Brust nicht auffgeflammt/", "tokens": ["mei\u00b7ne", "Brust", "nicht", "auff\u00b7ge\u00b7flammt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "darum hab\u2019 ich zu der Leyer", "tokens": ["da\u00b7rum", "hab'", "ich", "zu", "der", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "meine Finger nicht verdammt", "tokens": ["mei\u00b7ne", "Fin\u00b7ger", "nicht", "ver\u00b7dammt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df ich wolte Mavors Helden/", "tokens": ["da\u00df", "ich", "wol\u00b7te", "Ma\u00b7vors", "Hel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "NE", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Krieges-Zucht und Schlachten melden.", "tokens": ["Krie\u00b7ge\u00b7sZucht", "und", "Schlach\u00b7ten", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich des Gestirnes L\u00e4uffe/", "tokens": ["Da\u00df", "ich", "des", "Ge\u00b7stir\u00b7nes", "L\u00e4uf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gr\u00f6\u00df\u2019 und Einflu\u00df schreiben solt\u2019/", "tokens": ["Gr\u00f6\u00df'", "und", "Ein\u00b7flu\u00df", "schrei\u00b7ben", "solt'", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner kleinen Hirten-Pfeiffe/", "tokens": ["Mei\u00b7ner", "klei\u00b7nen", "Hir\u00b7ten\u00b7Pfeif\u00b7fe", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ist die Kloris/ der nicht hold:", "tokens": ["ist", "die", "Klo\u00b7ris", "/", "der", "nicht", "hold", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "$(", "ART", "PTKNEG", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "wil ich sie in st\u00fckken schmeissen", "tokens": ["wil", "ich", "sie", "in", "st\u00fck\u00b7ken", "schmeis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und den Lorber-kranz zerreissen.", "tokens": ["und", "den", "Lor\u00b7ber\u00b7kranz", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jupiter/ schikk Blizz und Wetter", "tokens": ["Ju\u00b7pi\u00b7ter", "/", "schikk", "Blizz", "und", "Wet\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "in den schn\u00f6den Goldes-schacht/", "tokens": ["in", "den", "schn\u00f6\u00b7den", "Gol\u00b7des\u00b7schacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seng/ verbrenn/ zerrei\u00df/ zerschmetter", "tokens": ["Seng", "/", "ver\u00b7brenn", "/", "zer\u00b7rei\u00df", "/", "zer\u00b7schmet\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$(", "VVFIN", "$(", "VVFIN", "$(", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dehn/ der um die Ufer wacht", "tokens": ["dehn", "/", "der", "um", "die", "U\u00b7fer", "wacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$(", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "auff die Perlen und Gesteine/", "tokens": ["auff", "die", "Per\u00b7len", "und", "Ge\u00b7stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die uns Hirten ungemeine.", "tokens": ["die", "uns", "Hir\u00b7ten", "un\u00b7ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Daher ist die Hoffart kommen", "tokens": ["Da\u00b7her", "ist", "die", "Hof\u00b7fart", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "daher hat der grimme Neid", "tokens": ["da\u00b7her", "hat", "der", "grim\u00b7me", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "darum ward zur Abend-Zeit", "tokens": ["da\u00b7rum", "ward", "zur", "A\u00b7ben\u00b7dZeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "erst die harte T\u00fchr verschlossen", "tokens": ["erst", "die", "har\u00b7te", "T\u00fchr", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und ein Armer au\u00dfgestossen.", "tokens": ["und", "ein", "Ar\u00b7mer", "au\u00df\u00b7ge\u00b7stos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Daher wurd\u2019 ein Hund gehalten", "tokens": ["Da\u00b7her", "wurd'", "ein", "Hund", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "der doch Augenbliklich schweigt", "tokens": ["der", "doch", "Au\u00b7gen\u00b7blik\u00b7lich", "schweigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn ihm eine Hand der Alten", "tokens": ["wenn", "ihm", "ei\u00b7ne", "Hand", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Panken-tahler wird gezeigt.", "tokens": ["Pan\u00b7ken\u00b7tahler", "wird", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So hat Zevs mit Goldes-tonnen", "tokens": ["So", "hat", "Zevs", "mit", "Gol\u00b7des\u00b7ton\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Danens Jungfrauschafft gewonnen.", "tokens": ["Da\u00b7nens", "Jung\u00b7frausc\u00b7hafft", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Aber du/ der du mit Gaben", "tokens": ["A\u00b7ber", "du", "/", "der", "du", "mit", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mich Verliebten st\u00f6ssest au\u00df/", "tokens": ["mich", "Ver\u00b7lieb\u00b7ten", "st\u00f6s\u00b7sest", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuer/ Wind und Diebes-Raben", "tokens": ["Feu\u00b7er", "/", "Wind", "und", "Die\u00b7bes\u00b7Ra\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bi\u00df es m\u00f6ge gleich der Erden", "tokens": ["bi\u00df", "es", "m\u00f6\u00b7ge", "gleich", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und mit dir vertilget werden!", "tokens": ["und", "mit", "dir", "ver\u00b7til\u00b7get", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}