{"textgrid.poem.59257": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedstafel", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So r\u00fcckt denn in die Runde!", "tokens": ["So", "r\u00fcckt", "denn", "in", "die", "Run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es schleicht die Zeit im Dunkeln,", "tokens": ["Es", "schleicht", "die", "Zeit", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie soll uns r\u00fcstig finden", "tokens": ["Sie", "soll", "uns", "r\u00fcs\u00b7tig", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und heiter, stark und gut!", "tokens": ["Und", "hei\u00b7ter", ",", "stark", "und", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gar viel ist zu vollbringen,", "tokens": ["Gar", "viel", "ist", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gar vieles mu\u00df mi\u00dflingen.", "tokens": ["Gar", "vie\u00b7les", "mu\u00df", "mi\u00df\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So mag die letzte Stunde", "tokens": ["So", "mag", "die", "letz\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nachleuchten uns und funkeln!", "tokens": ["Nach\u00b7leuch\u00b7ten", "uns", "und", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wo unsre Pfad sich winden,", "tokens": ["Wo", "uns\u00b7re", "Pfad", "sich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wir sind in Gottes Hut.", "tokens": ["Wir", "sind", "in", "Got\u00b7tes", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Bruder meines Lebens,", "tokens": ["Dem", "Bru\u00b7der", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der, fern, mit mir zusammen,", "tokens": ["Der", ",", "fern", ",", "mit", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei denn aus Herzensgrunde", "tokens": ["Sei", "denn", "aus", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das erste Glas gebracht!", "tokens": ["Das", "ers\u00b7te", "Glas", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich brauch ihn nicht zu nennen,", "tokens": ["Ich", "brauch", "ihn", "nicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er aber wird mich kennen.", "tokens": ["Er", "a\u00b7ber", "wird", "mich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Viel Land trennt uns vergebens,", "tokens": ["Viel", "Land", "trennt", "uns", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm soll dies Wort, die Stunde,", "tokens": ["Ihm", "soll", "dies", "Wort", ",", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Durch alle Adern flammen,", "tokens": ["Durch", "al\u00b7le", "A\u00b7dern", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie ich an ihn gedacht!", "tokens": ["Wie", "ich", "an", "ihn", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu dir nun, heitre Sch\u00f6ne,", "tokens": ["Zu", "dir", "nun", ",", "heit\u00b7re", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wend ich mich voll Gedanken.", "tokens": ["Wend", "ich", "mich", "voll", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie sie zu dir sich wenden,", "tokens": ["Wie", "sie", "zu", "dir", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df ich so fr\u00f6hlich sein.", "tokens": ["Mu\u00df", "ich", "so", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So weit Poeten wohnen,", "tokens": ["So", "weit", "Po\u00b7et\u00b7en", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So weit der W\u00e4lder Kronen,", "tokens": ["So", "weit", "der", "W\u00e4l\u00b7der", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So weit kunstreiche T\u00f6ne", "tokens": ["So", "weit", "kuns\u00b7trei\u00b7che", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die heiteren Gedanken", "tokens": ["Die", "hei\u00b7te\u00b7ren", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und Himmelsgr\u00fc\u00dfe senden:", "tokens": ["Und", "Him\u00b7mels\u00b7gr\u00fc\u00b7\u00dfe", "sen\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ist alles mein und dein.", "tokens": ["Ist", "al\u00b7les", "mein", "und", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df nie die Schmach mich sehen,", "tokens": ["La\u00df", "nie", "die", "Schmach", "mich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df auch dein Herz, der L\u00fcge", "tokens": ["Da\u00df", "auch", "dein", "Herz", ",", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des andern Volks zum Raube,", "tokens": ["Des", "an\u00b7dern", "Volks", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereuend feig und hohl,", "tokens": ["Be\u00b7reu\u00b7end", "feig", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An Licht und Schmuck mag zagen!", "tokens": ["An", "Licht", "und", "Schmuck", "mag", "za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht wahr ist, was sie sagen:", "tokens": ["Nicht", "wahr", "ist", ",", "was", "sie", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Lieb und Lust vergehen,", "tokens": ["Da\u00df", "Lieb", "und", "Lust", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht wahr, da\u00df uns betr\u00fcge", "tokens": ["Nicht", "wahr", ",", "da\u00df", "uns", "be\u00b7tr\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der sch\u00f6ne, freud'ge Glaube,", "tokens": ["Der", "sch\u00f6\u00b7ne", ",", "freu\u00b7d'\u00b7ge", "Glau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und also lebe wohl!", "tokens": ["Und", "al\u00b7so", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr aber, klug Gesellen,", "tokens": ["Ihr", "a\u00b7ber", ",", "klug", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hier mit in dem Kreise,", "tokens": ["Die", "hier", "mit", "in", "dem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl qu\u00e4lt ihr mich seit Jahren", "tokens": ["Wohl", "qu\u00e4lt", "ihr", "mich", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit weisem Rat und Wort. \u2013", "tokens": ["Mit", "wei\u00b7sem", "Rat", "und", "Wort", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sto\u00dft an, es sei vergessen!", "tokens": ["Sto\u00dft", "an", ",", "es", "sei", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Meere, ungemessen,", "tokens": ["Im", "Mee\u00b7re", ",", "un\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sind viele tausend Wellen", "tokens": ["Sind", "vie\u00b7le", "tau\u00b7send", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und tausend Schiffe fahren,", "tokens": ["Und", "tau\u00b7send", "Schif\u00b7fe", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein jedes seine Reise,", "tokens": ["Ein", "je\u00b7des", "sei\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Komm jedes in seinen Port!", "tokens": ["Komm", "je\u00b7des", "in", "sei\u00b7nen", "Port", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Vom Berg hinabgewendet,", "tokens": ["Vom", "Berg", "hin\u00b7ab\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seh ich die Str\u00f6me, Zinnen,", "tokens": ["Seh", "ich", "die", "Str\u00f6\u00b7me", ",", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Liebsten Schlo\u00df darunter \u2013", "tokens": ["Der", "Liebs\u00b7ten", "Schlo\u00df", "da\u00b7run\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun, Morgenlohe, h\u00fclle", "tokens": ["Nun", ",", "Mor\u00b7gen\u00b7lo\u00b7he", ",", "h\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Glorie dein Reich!", "tokens": ["In", "Glo\u00b7rie", "dein", "Reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dir, tieflebend'ge F\u00fclle,", "tokens": ["Dir", ",", "tief\u00b7le\u00b7ben\u00b7d'\u00b7ge", "F\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Schleudr ich das Glas hinunter,", "tokens": ["Schleudr", "ich", "das", "Glas", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir schwindeln alle Sinnen,", "tokens": ["Mir", "schwin\u00b7deln", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So wend ich mich geblendet,", "tokens": ["So", "wend", "ich", "mich", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Gott segne dich und euch!", "tokens": ["Gott", "seg\u00b7ne", "dich", "und", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}