{"textgrid.poem.50778": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sie sich da drehn im Tanze,", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie sich da drehn im Tanze,", "tokens": ["Wie", "sie", "sich", "da", "drehn", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "CARD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Puppen aus geschnitztem Holz!", "tokens": ["Pup\u00b7pen", "aus", "ge\u00b7schnitz\u00b7tem", "Holz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eitles Volk im Kerzenglanze,", "tokens": ["Eit\u00b7les", "Volk", "im", "Ker\u00b7zen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben heuchelnd, steif und stolz!", "tokens": ["Le\u00b7ben", "heu\u00b7chelnd", ",", "steif", "und", "stolz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schl\u00fcsselbeine, Schulterbl\u00e4tter", "tokens": ["Schl\u00fcs\u00b7sel\u00b7bei\u00b7ne", ",", "Schul\u00b7ter\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sto\u00dfen schamlos hart mich an;", "tokens": ["Sto\u00b7\u00dfen", "scham\u00b7los", "hart", "mich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte Tanten, grau vom Wetter,", "tokens": ["Al\u00b7te", "Tan\u00b7ten", ",", "grau", "vom", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klatschen l\u00e4ngs der tollen Bahn.", "tokens": ["Klat\u00b7schen", "l\u00e4ngs", "der", "tol\u00b7len", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die dem Tode l\u00e4ngst verfallen,", "tokens": ["Die", "dem", "To\u00b7de", "l\u00e4ngst", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt der Wahnsinn hier im Kreis!", "tokens": ["Treibt", "der", "Wahn\u00b7sinn", "hier", "im", "Kreis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schleiche durch die Hallen,", "tokens": ["Und", "ich", "schlei\u00b7che", "durch", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam schl\u00e4gt mein Herz und leis.", "tokens": ["Ein\u00b7sam", "schl\u00e4gt", "mein", "Herz", "und", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein gedenkt es, zarte Bl\u00fcte!", "tokens": ["Dein", "ge\u00b7denkt", "es", ",", "zar\u00b7te", "Bl\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O mein rosiger Morgentraum!", "tokens": ["O", "mein", "ro\u00b7si\u00b7ger", "Mor\u00b7gen\u00b7traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da\u00df dich Gott mir treu beh\u00fcte", "tokens": ["Da\u00df", "dich", "Gott", "mir", "treu", "be\u00b7h\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern am gr\u00fcnen Wogensaum!", "tokens": ["Fern", "am", "gr\u00fc\u00b7nen", "Wo\u00b7gen\u00b7saum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fern am Wogensaum, im Grabe", "tokens": ["Fern", "am", "Wo\u00b7gen\u00b7saum", ",", "im", "Gra\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft, was Lust und Leben war! \u2013", "tokens": ["Schl\u00e4ft", ",", "was", "Lust", "und", "Le\u00b7ben", "war", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWS", "NN", "KON", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Bechers Feuergabe", "tokens": ["Die\u00b7ses", "Be\u00b7chers", "Feu\u00b7er\u00b7ga\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring der Schl\u00e4ferin ich dar!", "tokens": ["Bring", "der", "Schl\u00e4\u00b7fe\u00b7rin", "ich", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie ein Schild von frischen Rosen,", "tokens": ["Wie", "ein", "Schild", "von", "fri\u00b7schen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Schwert von Sonnenstrahl", "tokens": ["Wie", "ein", "Schwert", "von", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctzt dein Bild mich Freundeslosen", "tokens": ["Sch\u00fctzt", "dein", "Bild", "mich", "Freun\u00b7des\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier vor dieser \u00f6den Qual!", "tokens": ["Hier", "vor", "die\u00b7ser", "\u00f6\u00b7den", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jung geblieben ist mein Lieben,", "tokens": ["Jung", "ge\u00b7blie\u00b7ben", "ist", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und noch heute rosenrot;", "tokens": ["Und", "noch", "heu\u00b7te", "ro\u00b7sen\u00b7rot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Auch mein Liebchen jung geblieben:", "tokens": ["Auch", "mein", "Lieb\u00b7chen", "jung", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dank daf\u00fcr, du milder Tod!", "tokens": ["Dank", "da\u00b7f\u00fcr", ",", "du", "mil\u00b7der", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}