{"dta.poem.2306": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mittel die Franzosen zu schlagen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neulich sagt ich meiner Laute:", "tokens": ["Neu\u00b7lich", "sagt", "ich", "mei\u00b7ner", "Lau\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Carl besiegt die Franzen tapfer,", "tokens": ["Carl", "be\u00b7siegt", "die", "Fran\u00b7zen", "tap\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du ihn denn nicht besingen?", "tokens": ["Willst", "du", "ihn", "denn", "nicht", "be\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er verdient\u2019s, ich will dirs sagen,", "tokens": ["Er", "ver\u00b7dient's", ",", "ich", "will", "dirs", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er besiegt, dis mu\u00dft du wissen,", "tokens": ["Er", "be\u00b7siegt", ",", "dis", "mu\u00dft", "du", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutsche Laute, deine Feinde.", "tokens": ["Deut\u00b7sche", "Lau\u00b7te", ",", "dei\u00b7ne", "Fein\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wilst du sie nicht auch besiegen?", "tokens": ["Wilst", "du", "sie", "nicht", "auch", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lokke sie doch in ein Treffen;", "tokens": ["Lok\u00b7ke", "sie", "doch", "in", "ein", "Tref\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ich will singen, du sollt streiten;", "tokens": ["Ich", "will", "sin\u00b7gen", ",", "du", "sollt", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber nicht mit starken Waffen,", "tokens": ["A\u00b7ber", "nicht", "mit", "star\u00b7ken", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht mit t\u00f6dlichem Geschosse,", "tokens": ["Nicht", "mit", "t\u00f6d\u00b7li\u00b7chem", "Ge\u00b7schos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nein, mit sanften Liebest\u00f6nen.", "tokens": ["Nein", ",", "mit", "sanf\u00b7ten", "Lie\u00b7be\u00b7st\u00f6\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "La\u00df sie denn so z\u00e4rtlich klingen,", "tokens": ["La\u00df", "sie", "denn", "so", "z\u00e4rt\u00b7lich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "La\u00df dich so bezaubernd h\u00f6ren,", "tokens": ["La\u00df", "dich", "so", "be\u00b7zau\u00b7bernd", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df das ganze Heer der Franzen", "tokens": ["Da\u00df", "das", "gan\u00b7ze", "Heer", "der", "Fran\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sich den Augenblikk verliebe.", "tokens": ["Sich", "den", "Au\u00b7gen\u00b7blikk", "ver\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Dann soll Carl dazwischen kommen,", "tokens": ["Dann", "soll", "Carl", "da\u00b7zwi\u00b7schen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und zum Vorteil seiner Helden", "tokens": ["Und", "zum", "Vor\u00b7teil", "sei\u00b7ner", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ihnen alle M\u00e4dchens rauben,", "tokens": ["Ih\u00b7nen", "al\u00b7le", "M\u00e4d\u00b7chens", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und wenn er das beste k\u00fcsset,", "tokens": ["Und", "wenn", "er", "das", "bes\u00b7te", "k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Soll er sie noch sp\u00f6ttisch fragen:", "tokens": ["Soll", "er", "sie", "noch", "sp\u00f6t\u00b7tisch", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wie gef\u00e4llt euch unsre Beute?", "tokens": ["Wie", "ge\u00b7f\u00e4llt", "euch", "uns\u00b7re", "Beu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}