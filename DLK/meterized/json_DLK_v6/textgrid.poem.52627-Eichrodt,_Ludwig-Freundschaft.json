{"textgrid.poem.52627": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was aber h\u00e4tt ich von dieser Welt,", "tokens": ["Was", "a\u00b7ber", "h\u00e4tt", "ich", "von", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und h\u00e4tt ich, was ich w\u00fcnscht, im Nu,", "tokens": ["Und", "h\u00e4tt", "ich", ",", "was", "ich", "w\u00fcnscht", ",", "im", "Nu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Herz erw\u00e4rmt und Geist erhellt,", "tokens": ["Was", "Herz", "er\u00b7w\u00e4rmt", "und", "Geist", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4tt keinen Freund dazu?", "tokens": ["Und", "h\u00e4tt", "kei\u00b7nen", "Freund", "da\u00b7zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was h\u00e4tt ich von aller Liebe gar,", "tokens": ["Was", "h\u00e4tt", "ich", "von", "al\u00b7ler", "Lie\u00b7be", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was h\u00e4tt ich von dem funkelnden Wein,", "tokens": ["Was", "h\u00e4tt", "ich", "von", "dem", "fun\u00b7keln\u00b7den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wenn Alles, was s\u00fc\u00df mir ist und war,", "tokens": ["Wenn", "Al\u00b7les", ",", "was", "s\u00fc\u00df", "mir", "ist", "und", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWS", "VVFIN", "PPER", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur bl\u00fchte f\u00fcr mich allein?", "tokens": ["Nur", "bl\u00fch\u00b7te", "f\u00fcr", "mich", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was wollt ich mit der schwellenden Brust", "tokens": ["Was", "wollt", "ich", "mit", "der", "schwel\u00b7len\u00b7den", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und sch\u00fctte sie arglos nimmer aus?", "tokens": ["Und", "sch\u00fct\u00b7te", "sie", "arg\u00b7los", "nim\u00b7mer", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vergrabenes Leid, verschlossene Lust,", "tokens": ["Ver\u00b7gra\u00b7be\u00b7nes", "Leid", ",", "ver\u00b7schlos\u00b7se\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist der Seelengraus.", "tokens": ["Das", "ist", "der", "See\u00b7len\u00b7graus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Alles \u00fcberdauern mu\u00df,", "tokens": ["Der", "Al\u00b7les", "\u00fc\u00b7berd\u00b7au\u00b7ern", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn dir so manche Bl\u00fcthe geknickt,", "tokens": ["Wenn", "dir", "so", "man\u00b7che", "Bl\u00fc\u00b7the", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Das ist des Geistes kr\u00e4ftiger Genu\u00df,", "tokens": ["Das", "ist", "des", "Geis\u00b7tes", "kr\u00e4f\u00b7ti\u00b7ger", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ewig verj\u00fcngt, erquickt.", "tokens": ["Der", "e\u00b7wig", "ver\u00b7j\u00fcngt", ",", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es ist allein der liebende Freund,", "tokens": ["Es", "ist", "al\u00b7lein", "der", "lie\u00b7ben\u00b7de", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Einen ganz und gar versteht,", "tokens": ["Der", "Ei\u00b7nen", "ganz", "und", "gar", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mitgelacht und mitgeweint,", "tokens": ["Der", "mit\u00b7ge\u00b7lacht", "und", "mit\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ge\u00e4rntet, was mitges\u00e4t.", "tokens": ["Ge\u00b7\u00e4rn\u00b7tet", ",", "was", "mit\u00b7ge\u00b7s\u00e4t", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Dann erst, o dann, gesch\u00e4hs einmal,", "tokens": ["Dann", "erst", ",", "o", "dann", ",", "ge\u00b7sch\u00e4hs", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "FM", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da w\u00fcrd es einsam in dir und leer,", "tokens": ["Da", "w\u00fcrd", "es", "ein\u00b7sam", "in", "dir", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn deine Freunde wegst\u00fcrben all,", "tokens": ["Wenn", "dei\u00b7ne", "Freun\u00b7de", "weg\u00b7st\u00fcr\u00b7ben", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00fcrde dirs Leben schwer.", "tokens": ["W\u00fcr\u00b7de", "dirs", "Le\u00b7ben", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}