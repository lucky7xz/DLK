{"dta.poem.19930": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Stummseyn der Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl neigt nach goldner Sonne,", "tokens": ["Wohl", "neigt", "nach", "gold\u00b7ner", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich stumm die Blum' der Au,", "tokens": ["Sich", "stumm", "die", "Blum'", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch spricht von ihrer Wonne", "tokens": ["Doch", "spricht", "von", "ih\u00b7rer", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Kelch der helle Thau.", "tokens": ["Im", "Kelch", "der", "hel\u00b7le", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Halt' ich dich Lieb' umwunden,", "tokens": ["Halt'", "ich", "dich", "Lieb'", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedr\u00fcckt an's Herze ganz,", "tokens": ["Ge\u00b7dr\u00fcckt", "an's", "Her\u00b7ze", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schweigt Lippe fest gebunden,", "tokens": ["Schweigt", "Lip\u00b7pe", "fest", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spricht nur des Auges Glanz.", "tokens": ["Spricht", "nur", "des", "Au\u00b7ges", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Leidender entschlagen", "tokens": ["Ein", "Lei\u00b7den\u00b7der", "ent\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So pl\u00f6tzlich aller Pein,", "tokens": ["So", "pl\u00f6tz\u00b7lich", "al\u00b7ler", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Liebe! kann nichts sagen,", "tokens": ["O", "Lie\u00b7be", "!", "kann", "nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der kann nur stille seyn.", "tokens": ["Der", "kann", "nur", "stil\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}