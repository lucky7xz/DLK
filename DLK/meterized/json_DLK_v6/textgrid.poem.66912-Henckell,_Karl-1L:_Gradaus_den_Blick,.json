{"textgrid.poem.66912": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gradaus den Blick,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gradaus den Blick,", "tokens": ["Gra\u00b7daus", "den", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "K\u00fchneren Schrittes ins weltoffene Leben hinein:", "tokens": ["K\u00fch\u00b7ne\u00b7ren", "Schrit\u00b7tes", "ins", "welt\u00b7of\u00b7fe\u00b7ne", "Le\u00b7ben", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dich gr\u00fc\u00df ich, junges deutsches Geschlecht,", "tokens": ["Dich", "gr\u00fc\u00df", "ich", ",", "jun\u00b7ges", "deut\u00b7sches", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Garde der Zukunft, schimmernd im goldroten Fr\u00fchlichtschein.", "tokens": ["Gar\u00b7de", "der", "Zu\u00b7kunft", ",", "schim\u00b7mernd", "im", "gol\u00b7dro\u00b7ten", "Fr\u00fch\u00b7licht\u00b7schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Du bist die k\u00e4mpfende Truppe des neuen Volkes im Land,", "tokens": ["Du", "bist", "die", "k\u00e4mp\u00b7fen\u00b7de", "Trup\u00b7pe", "des", "neu\u00b7en", "Vol\u00b7kes", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Mutige Liebe zur Wahrheit das Schwert in deiner Hand,", "tokens": ["Mu\u00b7ti\u00b7ge", "Lie\u00b7be", "zur", "Wahr\u00b7heit", "das", "Schwert", "in", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Treue zum eigenen Wesen die Fahne, die du f\u00fchrst,", "tokens": ["Treu\u00b7e", "zum", "ei\u00b7ge\u00b7nen", "We\u00b7sen", "die", "Fah\u00b7ne", ",", "die", "du", "f\u00fchrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wille zur edlen Freiheit die Trommel, die du r\u00fchrst.", "tokens": ["Wil\u00b7le", "zur", "ed\u00b7len", "Frei\u00b7heit", "die", "Trom\u00b7mel", ",", "die", "du", "r\u00fchrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}}}}