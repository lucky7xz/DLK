{"textgrid.poem.40562": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gegen\u00fcber der Hofburg steht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gegen\u00fcber der Hofburg steht", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "der", "Hof\u00b7burg", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der Thurm der Kathedrale,", "tokens": ["Der", "Thurm", "der", "Ka\u00b7thed\u00b7ra\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drauf des Landes Banner weht", "tokens": ["Drauf", "des", "Lan\u00b7des", "Ban\u00b7ner", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Prunkhaft im Sonnenstrahle.", "tokens": ["Prunk\u00b7haft", "im", "Son\u00b7nen\u00b7strah\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Sein Nest an der Stange flicht", "tokens": ["Sein", "Nest", "an", "der", "Stan\u00b7ge", "flicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Vogel dort allj\u00e4hrlich:", "tokens": ["Ein", "Vo\u00b7gel", "dort", "all\u00b7j\u00e4hr\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ward ihr des Baues Gewicht,", "tokens": ["Ward", "ihr", "des", "Bau\u00b7es", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das Picken der Jungen gef\u00e4hrlich?", "tokens": ["Das", "Pi\u00b7cken", "der", "Jun\u00b7gen", "ge\u00b7f\u00e4hr\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Hat mitgeholfen der Wind,", "tokens": ["Hat", "mit\u00b7ge\u00b7hol\u00b7fen", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Zeit mit zermalmendem Zahne?", "tokens": ["Die", "Zeit", "mit", "zer\u00b7mal\u00b7men\u00b7dem", "Zah\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Eines Tages pfeilgeschwind", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "pfeil\u00b7ge\u00b7schwind"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Thurme st\u00fcrzte die Fahne.", "tokens": ["Vom", "Thur\u00b7me", "st\u00fcrz\u00b7te", "die", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der F\u00fcrst sieht vom Balkon", "tokens": ["Der", "F\u00fcrst", "sieht", "vom", "Bal\u00b7kon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Banners Sinken und Fallen:", "tokens": ["Des", "Ban\u00b7ners", "Sin\u00b7ken", "und", "Fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbverrath und Rebellion!", "tokens": ["\u00bb", "ver\u00b7rath", "und", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Herbei zum Kampf, ihr Vasallen!", "tokens": ["Her\u00b7bei", "zum", "Kampf", ",", "ihr", "Va\u00b7sal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Meuter erklommen den Thurm,", "tokens": ["Die", "Meu\u00b7ter", "er\u00b7klom\u00b7men", "den", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Zu l\u00e4uten des Aufstands Glocken!", "tokens": ["Zu", "l\u00e4u\u00b7ten", "des", "Auf\u00b7stands", "Glo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie st\u00fcrzten mein Banner im Sturm!\u00ab", "tokens": ["Sie", "st\u00fcrz\u00b7ten", "mein", "Ban\u00b7ner", "im", "Sturm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So rief der F\u00fcrst erschrocken.", "tokens": ["So", "rief", "der", "F\u00fcrst", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das ist durch Gang und Gemach", "tokens": ["Das", "ist", "durch", "Gang", "und", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein Rufen, Rennen und Schreien!", "tokens": ["Ein", "Ru\u00b7fen", ",", "Ren\u00b7nen", "und", "Schrei\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hofdamen fl\u00fcchten aufs Dach,", "tokens": ["Hof\u00b7da\u00b7men", "fl\u00fcch\u00b7ten", "aufs", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In den Keller die Lakaien.", "tokens": ["In", "den", "Kel\u00b7ler", "die", "La\u00b7kai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Es sprengen rechts und links", "tokens": ["Es", "spren\u00b7gen", "rechts", "und", "links"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ordonnanzen und Staffeten,", "tokens": ["Or\u00b7don\u00b7nan\u00b7zen", "und", "Staf\u00b7fe\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Und aus den Kasernen rings", "tokens": ["Und", "aus", "den", "Ka\u00b7ser\u00b7nen", "rings"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hallt's von Trommeln und Trompeten.", "tokens": ["Hallt's", "von", "Trom\u00b7meln", "und", "Trom\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Den friedlichen B\u00fcrger verschlingt", "tokens": ["Den", "fried\u00b7li\u00b7chen", "B\u00fcr\u00b7ger", "ver\u00b7schlingt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Des Marktes Dr\u00e4ngen und Tosen,", "tokens": ["Des", "Mark\u00b7tes", "Dr\u00e4n\u00b7gen", "und", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Staatsminister springt", "tokens": ["Der", "Staats\u00b7mi\u00b7nis\u00b7ter", "springt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verkehrt in die Galahosen.", "tokens": ["Ver\u00b7kehrt", "in", "die", "Ga\u00b7la\u00b7ho\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Von Bajonetten ein Strom", "tokens": ["Von", "Ba\u00b7jo\u00b7net\u00b7ten", "ein", "Strom"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Quillt blitzend hervor aus den Gassen,", "tokens": ["Quillt", "blit\u00b7zend", "her\u00b7vor", "aus", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es dr\u00f6hnen Palast und Dom", "tokens": ["Es", "dr\u00f6h\u00b7nen", "Pa\u00b7last", "und", "Dom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vom Trabe der Reitermassen.", "tokens": ["Vom", "Tra\u00b7be", "der", "Rei\u00b7ter\u00b7mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Zur Stadt im Fl\u00fcgelschritt", "tokens": ["Zur", "Stadt", "im", "Fl\u00fc\u00b7gel\u00b7schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zieht Landsturm aller Farben", "tokens": ["Zieht", "Land\u00b7sturm", "al\u00b7ler", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jammernde Bauern mit,", "tokens": ["Und", "jam\u00b7mern\u00b7de", "Bau\u00b7ern", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob der zertretenen Garben.", "tokens": ["Ob", "der", "zer\u00b7tre\u00b7te\u00b7nen", "Gar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Kanonen rasseln heran,", "tokens": ["Ka\u00b7no\u00b7nen", "ras\u00b7seln", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Lunte glimmt schlagfertig,", "tokens": ["Die", "Lun\u00b7te", "glimmt", "schlag\u00b7fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entrollt steht auf dem Plan", "tokens": ["Ent\u00b7rollt", "steht", "auf", "dem", "Plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Heer, des Kampfes gew\u00e4rtig.", "tokens": ["Das", "Heer", ",", "des", "Kamp\u00b7fes", "ge\u00b7w\u00e4r\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "In der L\u00fcfte sonnigen Strom,", "tokens": ["In", "der", "L\u00fcf\u00b7te", "son\u00b7ni\u00b7gen", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In der Wolken stummen Reigen", "tokens": ["In", "der", "Wol\u00b7ken", "stum\u00b7men", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ragt still und tief der Dom,", "tokens": ["Ragt", "still", "und", "tief", "der", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Thurm die Glocken schweigen.", "tokens": ["Am", "Thurm", "die", "Glo\u00b7cken", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wer hat in die\u00df Volk hinein", "tokens": ["Wer", "hat", "in", "die\u00df", "Volk", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PDS", "NN", "APZR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ges\u00e4't des Unheils Samen?", "tokens": ["Ge\u00b7s\u00e4't", "des", "Un\u00b7heils", "Sa\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein winziges V\u00f6gelein!", "tokens": ["Ein", "win\u00b7zi\u00b7ges", "V\u00f6\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wer nennt uns seinen Namen?", "tokens": ["Wer", "nennt", "uns", "sei\u00b7nen", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Den Namen kennt man kaum,", "tokens": ["Den", "Na\u00b7men", "kennt", "man", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er klingt fast wie Gewissen;", "tokens": ["Er", "klingt", "fast", "wie", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man macht aus des Vogels Flaum", "tokens": ["Man", "macht", "aus", "des", "Vo\u00b7gels", "Flaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Allerhand Ruhekissen.", "tokens": ["Al\u00b7ler\u00b7hand", "Ru\u00b7he\u00b7kis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}}}}