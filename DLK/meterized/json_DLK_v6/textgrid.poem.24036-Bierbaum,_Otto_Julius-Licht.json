{"textgrid.poem.24036": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Licht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lag in Tr\u00fcbsinns Klammer", "tokens": ["Ich", "lag", "in", "Tr\u00fcb\u00b7sinns", "Klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dicht verschloss'ner Kammer,", "tokens": ["In", "dicht", "ver\u00b7schloss'\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nacht war es um mich her.", "tokens": ["Nacht", "war", "es", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur auf der Fensterschwelle", "tokens": ["Nur", "auf", "der", "Fens\u00b7ter\u00b7schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lag breit ein Streifen Helle,", "tokens": ["Lag", "breit", "ein", "Strei\u00b7fen", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als w\u00e4r von Lichte drau\u00dfen gro\u00df ein Meer.", "tokens": ["Als", "w\u00e4r", "von", "Lich\u00b7te", "drau\u00b7\u00dfen", "gro\u00df", "ein", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}