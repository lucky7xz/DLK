{"dta.poem.9489": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solt' ich an Mavors statt jtzund Armeen f\u00fchren/", "tokens": ["Solt'", "ich", "an", "Ma\u00b7vors", "statt", "jt\u00b7zund", "Ar\u00b7me\u00b7en", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPR", "ADV", "NN", "VVINF", "$("], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So wolt ich aus staffieren", "tokens": ["So", "wolt", "ich", "aus", "staf\u00b7fie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das gantze Krieges-heer", "tokens": ["Das", "gant\u00b7ze", "Krie\u00b7ge\u00b7sheer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem solchen Volck/ das hold den B\u00fcchern wer;", "tokens": ["Mit", "ei\u00b7nem", "sol\u00b7chen", "Volck", "/", "das", "hold", "den", "B\u00fc\u00b7chern", "wer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$(", "ART", "ADJD", "ART", "NN", "PWS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Studenten m\u00fcsten seyn", "tokens": ["Die", "Stu\u00b7den\u00b7ten", "m\u00fcs\u00b7ten", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VAINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Meine beste Bursch und F\u00fchrer/", "tokens": ["Mei\u00b7ne", "bes\u00b7te", "Bursch", "und", "F\u00fch\u00b7rer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Gelehrten Officierer/", "tokens": ["Die", "Ge\u00b7lehr\u00b7ten", "Of\u00b7fi\u00b7cie\u00b7rer", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ph\u00f6bus-V\u00f6lcker in gemein", "tokens": ["Ph\u00f6\u00b7bus\u00b7V\u00f6l\u00b7cker", "in", "ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00fcsten die Feinde verjagen und d\u00e4mpfen/", "tokens": ["M\u00fcs\u00b7ten", "die", "Fein\u00b7de", "ver\u00b7ja\u00b7gen", "und", "d\u00e4mp\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "M\u00fcsten uns helfen und ritterlich k\u00e4mpfen.", "tokens": ["M\u00fcs\u00b7ten", "uns", "hel\u00b7fen", "und", "rit\u00b7ter\u00b7lich", "k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Constabel solten seyn die f\u00fc\u00dfen Musicanten/", "tokens": ["Cons\u00b7ta\u00b7bel", "sol\u00b7ten", "seyn", "die", "f\u00fc\u00b7\u00dfen", "Mu\u00b7si\u00b7can\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Helicons-verwanten/", "tokens": ["Die", "He\u00b7li\u00b7cons\u00b7ver\u00b7wan\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Orgeln freudenschall", "tokens": ["Der", "Or\u00b7geln", "freu\u00b7den\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Solt an Trompeten statt erklingen \u00fcberall/", "tokens": ["Solt", "an", "Trom\u00b7pe\u00b7ten", "statt", "er\u00b7klin\u00b7gen", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADJD", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bachus und sein Kammer", "tokens": ["Ba\u00b7chus", "und", "sein", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ceres solten uns wohl geben", "tokens": ["Ce\u00b7res", "sol\u00b7ten", "uns", "wohl", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Proviant und Wein zu leben/", "tokens": ["Pro\u00b7vi\u00b7ant", "und", "Wein", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "uns zu frischen fr\u00fch und spat/", "tokens": ["uns", "zu", "fri\u00b7schen", "fr\u00fch", "und", "spat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADJD", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Musen und Gratien m\u00fcsten mit k\u00e4mpfen/", "tokens": ["Mu\u00b7sen", "und", "Gra\u00b7ti\u00b7en", "m\u00fcs\u00b7ten", "mit", "k\u00e4mp\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VMFIN", "APPR", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "M\u00fcsten die Feinde verjagen und d\u00e4mpfen.", "tokens": ["M\u00fcs\u00b7ten", "die", "Fein\u00b7de", "ver\u00b7ja\u00b7gen", "und", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Feder solte mier an statt der Schwerter dienen/", "tokens": ["Die", "Fe\u00b7der", "sol\u00b7te", "mier", "an", "statt", "der", "Schwer\u00b7ter", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wolten uns erl\u00fchnen", "tokens": ["Wir", "wol\u00b7ten", "uns", "er\u00b7l\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In alle Welt zugehn.", "tokens": ["In", "al\u00b7le", "Welt", "zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich deucht ich w\u00f6lte wohl mit diesem Volck bestehn", "tokens": ["Mich", "deucht", "ich", "w\u00f6l\u00b7te", "wohl", "mit", "die\u00b7sem", "Volck", "be\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In gefahr und Krieges-noth;", "tokens": ["In", "ge\u00b7fahr", "und", "Krie\u00b7ge\u00b7snoth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schriffigelehrten und Juristen/", "tokens": ["Schrif\u00b7fi\u00b7ge\u00b7lehr\u00b7ten", "und", "Ju\u00b7ris\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "M\u00fcsten sich zu streiten r\u00fcsten;", "tokens": ["M\u00fcs\u00b7ten", "sich", "zu", "strei\u00b7ten", "r\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die/ vor denen fliht der Todt/", "tokens": ["Die", "/", "vor", "de\u00b7nen", "fliht", "der", "Todt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "APPR", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "M\u00fcsten uns helffen auch Riterlich k\u00e4mpfen/", "tokens": ["M\u00fcs\u00b7ten", "uns", "helf\u00b7fen", "auch", "Ri\u00b7ter\u00b7lich", "k\u00e4mp\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "M\u00fcsten die Feinde verjagen und d\u00e4mpfen.", "tokens": ["M\u00fcs\u00b7ten", "die", "Fein\u00b7de", "ver\u00b7ja\u00b7gen", "und", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Ade du w\u00fcster Mars/ ade mit deinen Kriegen/", "tokens": ["A\u00b7de", "du", "w\u00fcs\u00b7ter", "Mars", "/", "a\u00b7de", "mit", "dei\u00b7nen", "Krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$(", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich kann nun b\u00e4sser Siegen/", "tokens": ["Ich", "kann", "nun", "b\u00e4s\u00b7ser", "Sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ziel ist mier gesetzt/", "tokens": ["Ein", "Ziel", "ist", "mier", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das nicht nach solchen strebt/ was nur die welt erg\u00f6tzt;", "tokens": ["Das", "nicht", "nach", "sol\u00b7chen", "strebt", "/", "was", "nur", "die", "welt", "er\u00b7g\u00f6tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "PIAT", "VVFIN", "$(", "PWS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor den Mars wird nun gepreist", "tokens": ["Vor", "den", "Mars", "wird", "nun", "ge\u00b7preist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser Krieg und dieses Leben/", "tokens": ["Die\u00b7ser", "Krieg", "und", "die\u00b7ses", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das den K\u00fcnsten ist ergeben/", "tokens": ["Das", "den", "K\u00fcns\u00b7ten", "ist", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das uns hin zur Tugend weist.", "tokens": ["Das", "uns", "hin", "zur", "Tu\u00b7gend", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Packe dich Mavors und weiche von hinnen/", "tokens": ["Pa\u00b7cke", "dich", "Ma\u00b7vors", "und", "wei\u00b7che", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "KON", "ADJA", "APPR", "ADV", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "\u00fcbe bey andern dein tolles beginnen.", "tokens": ["\u00fc\u00b7be", "bey", "an\u00b7dern", "dein", "tol\u00b7les", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}