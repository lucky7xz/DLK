{"textgrid.poem.26234": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die deutschen Frauen 1915", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sparen sich das Brot vom Munde", "tokens": ["Sie", "spa\u00b7ren", "sich", "das", "Brot", "vom", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcgen gern sich in Geduld.", "tokens": ["Und", "f\u00fc\u00b7gen", "gern", "sich", "in", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4chst die Sorge jede Stunde,", "tokens": ["Und", "w\u00e4chst", "die", "Sor\u00b7ge", "je\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie \u00e4ndern nicht den Blick der Huld.", "tokens": ["Sie", "\u00e4n\u00b7dern", "nicht", "den", "Blick", "der", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie, die ihr Liebstes fern verloren,", "tokens": ["Sie", ",", "die", "ihr", "Liebs\u00b7tes", "fern", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie zeigen ihre Tr\u00e4nen nicht.", "tokens": ["Sie", "zei\u00b7gen", "ih\u00b7re", "Tr\u00e4\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wandeln nicht in schwarzen Floren,", "tokens": ["Sie", "wan\u00b7deln", "nicht", "in", "schwar\u00b7zen", "Flo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur blasser wird ihr ernst Gesicht.", "tokens": ["Nur", "blas\u00b7ser", "wird", "ihr", "ernst", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie stillen Blut der fremden Wunden", "tokens": ["Sie", "stil\u00b7len", "Blut", "der", "frem\u00b7den", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leugnen stumm die eignen fort.", "tokens": ["Und", "leug\u00b7nen", "stumm", "die", "eig\u00b7nen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stehn bei fremden Sterbestunden", "tokens": ["Sie", "stehn", "bei", "frem\u00b7den", "Ster\u00b7be\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00f6sten sanft mit Tat und Wort.", "tokens": ["Und", "tr\u00f6s\u00b7ten", "sanft", "mit", "Tat", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Herr, sieh die Heldinnen! Und kr\u00f6ne", "tokens": ["Herr", ",", "sieh", "die", "Hel\u00b7din\u00b7nen", "!", "Und", "kr\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "$.", "KON", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit Sieg mein Volk, dem solche Frauen,", "tokens": ["Mit", "Sieg", "mein", "Volk", ",", "dem", "sol\u00b7che", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stark, wie im Feld die braven S\u00f6hne,", "tokens": ["Stark", ",", "wie", "im", "Feld", "die", "bra\u00b7ven", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Mut und Zucht zur Zukunft schauen.", "tokens": ["Voll", "Mut", "und", "Zucht", "zur", "Zu\u00b7kunft", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}