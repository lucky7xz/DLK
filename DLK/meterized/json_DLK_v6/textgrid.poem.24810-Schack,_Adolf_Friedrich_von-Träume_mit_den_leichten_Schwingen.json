{"textgrid.poem.24810": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00e4ume mit den leichten Schwingen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ume mit den leichten Schwingen", "tokens": ["Tr\u00e4u\u00b7me", "mit", "den", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flattern zwischen ihr und mir,", "tokens": ["Flat\u00b7tern", "zwi\u00b7schen", "ihr", "und", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweben auf und schweben nieder,", "tokens": ["Schwe\u00b7ben", "auf", "und", "schwe\u00b7ben", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tragen kaum geborne Lieder", "tokens": ["Tra\u00b7gen", "kaum", "ge\u00b7bor\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fl\u00fcgelschnell ihr hin und bringen", "tokens": ["Fl\u00fc\u00b7gel\u00b7schnell", "ihr", "hin", "und", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir ein L\u00e4cheln heim von ihr;", "tokens": ["Mir", "ein", "L\u00e4\u00b7cheln", "heim", "von", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pfl\u00fccken Bl\u00fcten auf den Auen,", "tokens": ["Pfl\u00fc\u00b7cken", "Bl\u00fc\u00b7ten", "auf", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n, wie sie der Fr\u00fchling giebt,", "tokens": ["Sch\u00f6n", ",", "wie", "sie", "der", "Fr\u00fch\u00b7ling", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streuen auf ihr Ruhekissen", "tokens": ["Streu\u00b7en", "auf", "ihr", "Ru\u00b7he\u00b7kis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Maienglocken und Narzissen,", "tokens": ["Mai\u00b7en\u00b7glo\u00b7cken", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die in D\u00fcften ihr vertrauen,", "tokens": ["Die", "in", "D\u00fcf\u00b7ten", "ihr", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df mein Herz sie einzig liebt.", "tokens": ["Da\u00df", "mein", "Herz", "sie", "ein\u00b7zig", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihre Lippen regt sie leise,", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "regt", "sie", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie solche Gaben sieht;", "tokens": ["Wie", "sie", "sol\u00b7che", "Ga\u00b7ben", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Fl\u00fcstern, in dem Lallen", "tokens": ["In", "dem", "Fl\u00fcs\u00b7tern", ",", "in", "dem", "Lal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r' ich meinen Namen schallen,", "tokens": ["H\u00f6r'", "ich", "mei\u00b7nen", "Na\u00b7men", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wir reden wechselweise,", "tokens": ["Und", "wir", "re\u00b7den", "wech\u00b7sel\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bis der Schlummer von mir flieht.", "tokens": ["Bis", "der", "Schlum\u00b7mer", "von", "mir", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann im Dunklen aufgerichtet,", "tokens": ["Dann", "im", "Dunk\u00b7len", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau' ich, da\u00df ich einsam bin \u2013", "tokens": ["Schau'", "ich", ",", "da\u00df", "ich", "ein\u00b7sam", "bin", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, im Traum nur mocht' ich wagen,", "tokens": ["Ach", ",", "im", "Traum", "nur", "mocht'", "ich", "wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich f\u00fchle, ihr zu sagen,", "tokens": ["Was", "ich", "f\u00fch\u00b7le", ",", "ihr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Lied, f\u00fcr sie gedichtet,", "tokens": ["Und", "das", "Lied", ",", "f\u00fcr", "sie", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stirbt auf meiner Lippe hin.", "tokens": ["Stirbt", "auf", "mei\u00b7ner", "Lip\u00b7pe", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}