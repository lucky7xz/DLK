{"dta.poem.7236": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Der 1. Satz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr die Jhr den Jungen Jahren/", "tokens": ["Ihr", "die", "Ihr", "den", "Jun\u00b7gen", "Jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gar zu sehr ergeben seyd/", "tokens": ["gar", "zu", "sehr", "er\u00b7ge\u00b7ben", "seyd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "kommet/ schauet an di\u00df leid/", "tokens": ["kom\u00b7met", "/", "schau\u00b7et", "an", "di\u00df", "leid", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "APPR", "PDS", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "kommet/ sehet in der Bahren", "tokens": ["kom\u00b7met", "/", "se\u00b7het", "in", "der", "Bah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "lieget eine fr\u00fchlings blum;/", "tokens": ["lie\u00b7get", "ei\u00b7ne", "fr\u00fch\u00b7lings", "blum", ";", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "die der Pierinnen Ruhm;", "tokens": ["die", "der", "Pie\u00b7rin\u00b7nen", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den die Musen nicht gehasset/", "tokens": ["Den", "die", "Mu\u00b7sen", "nicht", "ge\u00b7has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "ist nun gantz vnd gar verblasset/", "tokens": ["ist", "nun", "gantz", "vnd", "gar", "ver\u00b7blas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KON", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "ach! beweinet dieses leid/", "tokens": ["ach", "!", "be\u00b7wei\u00b7net", "die\u00b7ses", "leid", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PDAT", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "lasset vollend \u00fcberlauffen", "tokens": ["las\u00b7set", "vol\u00b7lend", "\u00fc\u00b7berl\u00b7auf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Euren Augenbach mit hauffen/", "tokens": ["Eu\u00b7ren", "Au\u00b7gen\u00b7bach", "mit", "hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "wernur kan/ nun ist es zeit.", "tokens": ["wer\u00b7nur", "kan", "/", "nun", "ist", "es", "zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}