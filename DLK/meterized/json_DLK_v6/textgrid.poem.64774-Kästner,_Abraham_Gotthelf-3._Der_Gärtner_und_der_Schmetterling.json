{"textgrid.poem.64774": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "3. Der G\u00e4rtner und der Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach g\u00f6nne mir das Gl\u00fcck, mein Leben frey zu enden!", "tokens": ["Ach", "g\u00f6n\u00b7ne", "mir", "das", "Gl\u00fcck", ",", "mein", "Le\u00b7ben", "frey", "zu", "en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bat ein Schmetterling in seines F\u00e4ngers H\u00e4nden,", "tokens": ["So", "bat", "ein", "Schmet\u00b7ter\u00b7ling", "in", "sei\u00b7nes", "F\u00e4n\u00b7gers", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch wenig Tage sind zum Fliegen mir erlaubt,", "tokens": ["Noch", "we\u00b7nig", "Ta\u00b7ge", "sind", "zum", "Flie\u00b7gen", "mir", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was hilft die Grausamkeit, die mir auch diese raubt?", "tokens": ["Was", "hilft", "die", "Grau\u00b7sam\u00b7keit", ",", "die", "mir", "auch", "die\u00b7se", "raubt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wei\u00dft, der Blumen Schmuck wird nicht durch mich versehret,", "tokens": ["Du", "wei\u00dft", ",", "der", "Blu\u00b7men", "Schmuck", "wird", "nicht", "durch", "mich", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein unvermi\u00dfter Saft ist alles, was mich n\u00e4hret.", "tokens": ["Ein", "un\u00b7ver\u00b7mi\u00df\u00b7ter", "Saft", "ist", "al\u00b7les", ",", "was", "mich", "n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Flehen bringt mich nicht zu unbedachter Huld,", "tokens": ["Dein", "Fle\u00b7hen", "bringt", "mich", "nicht", "zu", "un\u00b7be\u00b7dach\u00b7ter", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagt ihm der G\u00e4rtner drauf, stirb jetzt f\u00fcr alte Schuld;", "tokens": ["Sagt", "ihm", "der", "G\u00e4rt\u00b7ner", "drauf", ",", "stirb", "jetzt", "f\u00fcr", "al\u00b7te", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "VVIMP", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wollt' ich der Raupe That dem Schmetterling vergeben,", "tokens": ["Wollt'", "ich", "der", "Rau\u00b7pe", "That", "dem", "Schmet\u00b7ter\u00b7ling", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird sie hundertfach in deinen Jungen leben.", "tokens": ["So", "wird", "sie", "hun\u00b7dert\u00b7fach", "in", "dei\u00b7nen", "Jun\u00b7gen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch bey der Bessrung Schein befiehlt des B\u00f6sen Tod", "tokens": ["Auch", "bey", "der", "Bess\u00b7rung", "Schein", "be\u00b7fiehlt", "des", "B\u00f6\u00b7sen", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Uebel, das er that, und mehr noch, das er droht.", "tokens": ["Das", "Ue\u00b7bel", ",", "das", "er", "that", ",", "und", "mehr", "noch", ",", "das", "er", "droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ADV", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}