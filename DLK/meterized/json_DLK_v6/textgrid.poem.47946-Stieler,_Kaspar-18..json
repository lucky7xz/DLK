{"textgrid.poem.47946": {"metadata": {"author": {"name": "Stieler, Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1669, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rstu nicht sch\u00f6n wie h\u00e4tt' ich dich geliebet?", "tokens": ["W\u00e4rs\u00b7tu", "nicht", "sch\u00f6n", "wie", "h\u00e4tt'", "ich", "dich", "ge\u00b7lie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "KOKOM", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "nu bistu sch\u00f6n so hasset mich der Neid/", "tokens": ["nu", "bis\u00b7tu", "sch\u00f6n", "so", "has\u00b7set", "mich", "der", "Neid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und g\u00f6nnet mir nicht deine Freundligkeit.", "tokens": ["und", "g\u00f6n\u00b7net", "mir", "nicht", "dei\u00b7ne", "Freund\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4rstu nicht sch\u00f6n/ so ha\u00dfte dich ein Ieder:", "tokens": ["W\u00e4rs\u00b7tu", "nicht", "sch\u00f6n", "/", "so", "ha\u00df\u00b7te", "dich", "ein", "Ie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$(", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "nu/ bistu sch\u00f6n/ so liebt dich jeder wieder.", "tokens": ["nu", "/", "bis\u00b7tu", "sch\u00f6n", "/", "so", "liebt", "dich", "je\u00b7der", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "ADJD", "$(", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach! m\u00f6chtestu doch mir nur sch\u00f6ne sein", "tokens": ["Ach", "!", "m\u00f6ch\u00b7tes\u00b7tu", "doch", "mir", "nur", "sch\u00f6\u00b7ne", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "ADV", "PPER", "ADV", "ADJA", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "so nennte sich kein ander deinen Knecht.", "tokens": ["so", "nenn\u00b7te", "sich", "kein", "an\u00b7der", "dei\u00b7nen", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "da\u00df du nu sch\u00f6ne bist/ ist recht und auch nicht recht.", "tokens": ["da\u00df", "du", "nu", "sch\u00f6\u00b7ne", "bist", "/", "ist", "recht", "und", "auch", "nicht", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VAFIN", "$(", "VAFIN", "ADJD", "KON", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}