{"textgrid.poem.35126": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "In tiefer Noth", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So, wie der Hirsch nach frischem Wasser schreit,", "tokens": ["So", ",", "wie", "der", "Hirsch", "nach", "fri\u00b7schem", "Was\u00b7ser", "schreit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So rufe ich, o Herr, nach deiner G\u00fcte.", "tokens": ["So", "ru\u00b7fe", "ich", ",", "o", "Herr", ",", "nach", "dei\u00b7ner", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich ging von dir hinweg so weit, so weit;", "tokens": ["Ich", "ging", "von", "dir", "hin\u00b7weg", "so", "weit", ",", "so", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O werde im Verzeihen nimmer, nimmer m\u00fcde!", "tokens": ["O", "wer\u00b7de", "im", "Ver\u00b7zei\u00b7hen", "nim\u00b7mer", ",", "nim\u00b7mer", "m\u00fc\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So, wie der Hirsch nach frischem Wasser schreit,", "tokens": ["So", ",", "wie", "der", "Hirsch", "nach", "fri\u00b7schem", "Was\u00b7ser", "schreit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So rufe ich, o Herr, nach deiner Gnade.", "tokens": ["So", "ru\u00b7fe", "ich", ",", "o", "Herr", ",", "nach", "dei\u00b7ner", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Send mir die Fluthen der Barmherzigkeit,", "tokens": ["Send", "mir", "die", "Flut\u00b7hen", "der", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "In denen ich mich rein von meinen S\u00fcnden bade!", "tokens": ["In", "de\u00b7nen", "ich", "mich", "rein", "von", "mei\u00b7nen", "S\u00fcn\u00b7den", "ba\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So, wie der Hirsch nach frischem Wasser schreit,", "tokens": ["So", ",", "wie", "der", "Hirsch", "nach", "fri\u00b7schem", "Was\u00b7ser", "schreit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So rufe ich, o Herr, nach deinem Segen.", "tokens": ["So", "ru\u00b7fe", "ich", ",", "o", "Herr", ",", "nach", "dei\u00b7nem", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erlaube mir, mit meinem Herzeleid", "tokens": ["Er\u00b7lau\u00b7be", "mir", ",", "mit", "mei\u00b7nem", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor deinem Throne tief mich in den Staub zu legen!", "tokens": ["Vor", "dei\u00b7nem", "Thro\u00b7ne", "tief", "mich", "in", "den", "Staub", "zu", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So, wie der Hirsch nach frischem Wasser schreit,", "tokens": ["So", ",", "wie", "der", "Hirsch", "nach", "fri\u00b7schem", "Was\u00b7ser", "schreit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So steh ich am Verschmachten, am Vergehen.", "tokens": ["So", "steh", "ich", "am", "Ver\u00b7schmach\u00b7ten", ",", "am", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist die h\u00f6chste, allerh\u00f6chste Zeit;", "tokens": ["Es", "ist", "die", "h\u00f6chs\u00b7te", ",", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O la\u00df dich, Herr und Vater, la\u00df dich doch erflehen!", "tokens": ["O", "la\u00df", "dich", ",", "Herr", "und", "Va\u00b7ter", ",", "la\u00df", "dich", "doch", "er\u00b7fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}