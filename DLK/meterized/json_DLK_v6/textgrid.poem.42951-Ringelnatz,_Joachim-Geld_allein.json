{"textgrid.poem.42951": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Geld allein", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gut, da\u00df alle einander nicht gleichen.", "tokens": ["Wie", "gut", ",", "da\u00df", "al\u00b7le", "ein\u00b7an\u00b7der", "nicht", "glei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PIS", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie recht, da\u00df manche es erreichen,", "tokens": ["Wie", "recht", ",", "da\u00df", "man\u00b7che", "es", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie eines Tages reich sind.", "tokens": ["Da\u00df", "sie", "ei\u00b7nes", "Ta\u00b7ges", "reich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie gut, da\u00df auch diese einander nicht gleich sind.", "tokens": ["Wie", "gut", ",", "da\u00df", "auch", "die\u00b7se", "ein\u00b7an\u00b7der", "nicht", "gleich", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ADV", "PDS", "ADV", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Schlechte Menschen ohne Geist, ohne Geschmack,", "tokens": ["Schlech\u00b7te", "Men\u00b7schen", "oh\u00b7ne", "Geist", 