{"dta.poem.11863": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschheit Geister sind zum H\u00f6chsten gleich berufen,", "tokens": ["Der", "Menschheit", "Geis\u00b7ter", "sind", "zum", "H\u00f6chs\u00b7ten", "gleich", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Doch J\u00fcngling, Greis und Kind steht nicht auf gleichen Stufen.", "tokens": ["Doch", "J\u00fcng\u00b7ling", ",", "Greis", "und", "Kind", "steht", "nicht", "auf", "glei\u00b7chen", "Stu\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alswie ein Vogel fliegt, indes ein andrer flattert;", "tokens": ["Als\u00b7wie", "ein", "Vo\u00b7gel", "fliegt", ",", "in\u00b7des", "ein", "an\u00b7drer", "flat\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie ein Vogel singt, indes ein andrer schnattert.", "tokens": ["Als\u00b7wie", "ein", "Vo\u00b7gel", "singt", ",", "in\u00b7des", "ein", "an\u00b7drer", "schnat\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "$,", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf, wenn du Schwingen hast, zu Gott dich aufzuschwingen!", "tokens": ["Auf", ",", "wenn", "du", "Schwin\u00b7gen", "hast", ",", "zu", "Gott", "dich", "auf\u00b7zu\u00b7schwin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf, wenn du singen kannst, in Gott dich auszusingen!", "tokens": ["Auf", ",", "wenn", "du", "sin\u00b7gen", "kannst", ",", "in", "Gott", "dich", "aus\u00b7zu\u00b7sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}