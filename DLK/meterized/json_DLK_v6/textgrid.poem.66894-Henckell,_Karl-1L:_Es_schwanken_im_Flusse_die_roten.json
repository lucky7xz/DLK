{"textgrid.poem.66894": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schwanken im Flusse die roten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schwanken im Flusse die roten", "tokens": ["Es", "schwan\u00b7ken", "im", "Flus\u00b7se", "die", "ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Lichter von kreuzenden Booten,", "tokens": ["Lich\u00b7ter", "von", "kreu\u00b7zen\u00b7den", "Boo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die zitternde Spiralen", "tokens": ["Die", "zit\u00b7tern\u00b7de", "Spi\u00b7ra\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In tiefschwarze Wasser malen,", "tokens": ["In", "tief\u00b7schwar\u00b7ze", "Was\u00b7ser", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit glimmenden Spuren die Ufer verbinden,", "tokens": ["Mit", "glim\u00b7men\u00b7den", "Spu\u00b7ren", "die", "U\u00b7fer", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Von Br\u00fccke zu Br\u00fccke hinhuschen und schwinden.", "tokens": ["Von", "Br\u00fc\u00b7cke", "zu", "Br\u00fc\u00b7cke", "hin\u00b7hu\u00b7schen", "und", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Durch hundert Br\u00fccken und Bogen", "tokens": ["Durch", "hun\u00b7dert", "Br\u00fc\u00b7cken", "und", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Geheimnisschauernd geflogen,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7schau\u00b7ernd", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo die Laute rauschend verschwimmen,", "tokens": ["Wo", "die", "Lau\u00b7te", "rau\u00b7schend", "ver\u00b7schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und von wirrphantastischen Stimmen", "tokens": ["Und", "von", "wirr\u00b7phan\u00b7tas\u00b7ti\u00b7schen", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hohldunkle W\u00f6lbungen widerhallen", "tokens": ["Hohl\u00b7dunk\u00b7le", "W\u00f6l\u00b7bun\u00b7gen", "wi\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie von Opfern, der schweigenden Tiefe verfallen.", "tokens": ["Wie", "von", "Op\u00b7fern", ",", "der", "schwei\u00b7gen\u00b7den", "Tie\u00b7fe", "ver\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Dumpf Murmeln, Fl\u00fcstern und Raunen", "tokens": ["Dumpf", "Mur\u00b7meln", ",", "Fl\u00fcs\u00b7tern", "und", "Rau\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Kronos rasenden Launen,", "tokens": ["Von", "Kro\u00b7nos", "ra\u00b7sen\u00b7den", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Glorias gl\u00fchendem Kosen", "tokens": ["Von", "Glo\u00b7ri\u00b7as", "gl\u00fc\u00b7hen\u00b7dem", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit bleichen, blutigen Rosen,", "tokens": ["Mit", "blei\u00b7chen", ",", "blu\u00b7ti\u00b7gen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von H\u00f6llentriumph, gotttrunkener Macht", "tokens": ["Von", "H\u00f6l\u00b7len\u00b7tri\u00b7umph", ",", "gott\u00b7trun\u00b7ke\u00b7ner", "Macht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Echo, hinsterbend in Schatten der Nacht ...", "tokens": ["Ein", "E\u00b7cho", ",", "hins\u00b7ter\u00b7bend", "in", "Schat\u00b7ten", "der", "Nacht", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}