{"textgrid.poem.49241": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich dir, Delia, ein Schreiben zugeschickt,", "tokens": ["Als", "ich", "dir", ",", "De\u00b7lia", ",", "ein", "Schrei\u00b7ben", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Daraus du meine Lieb' und gro\u00dfe Gunst erkennet,", "tokens": ["Da\u00b7raus", "du", "mei\u00b7ne", "Lieb'", "und", "gro\u00b7\u00dfe", "Gunst", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hast du es ohne Schuld ganz zornig angeblickt", "tokens": ["Hast", "du", "es", "oh\u00b7ne", "Schuld", "ganz", "zor\u00b7nig", "an\u00b7ge\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPR", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und, wie mir wird gesagt, aus Eifer bald verbrennet.", "tokens": ["Und", ",", "wie", "mir", "wird", "ge\u00b7sagt", ",", "aus", "Ei\u00b7fer", "bald", "ver\u00b7bren\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wunder' ich mich nicht, weil du mir feind gewesen,", "tokens": ["Doch", "wun\u00b7der'", "ich", "mich", "nicht", ",", "weil", "du", "mir", "feind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df diese meine Wort in dir den Grimm erweckt;", "tokens": ["Da\u00df", "die\u00b7se", "mei\u00b7ne", "Wort", "in", "dir", "den", "Grimm", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "APPR", "PPER", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df wundert mich viel mehr, weil du den Brief gelesen,", "tokens": ["Di\u00df", "wun\u00b7dert", "mich", "viel", "mehr", ",", "weil", "du", "den", "Brief", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df deiner Augen Glanz ihn nicht hat angesteckt.", "tokens": ["Da\u00df", "dei\u00b7ner", "Au\u00b7gen", "Glanz", "ihn", "nicht", "hat", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}