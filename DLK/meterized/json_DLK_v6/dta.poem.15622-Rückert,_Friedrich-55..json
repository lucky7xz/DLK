{"dta.poem.15622": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Herzen denkst du auch, nicht blos in deinem Haupt;", "tokens": ["Im", "Her\u00b7zen", "denkst", "du", "auch", ",", "nicht", "blos", "in", "dei\u00b7nem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von beiden Denken sei dem andern keins geraubt!", "tokens": ["Von", "bei\u00b7den", "Den\u00b7ken", "sei", "dem", "an\u00b7dern", "keins", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "ADJA", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was du im Herzen denkst, ist voll in sich gedrungen,", "tokens": ["Was", "du", "im", "Her\u00b7zen", "denkst", ",", "ist", "voll", "in", "sich", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du im Haupte denkst, kraus linienhaft geschlungen.", "tokens": ["Was", "du", "im", "Haup\u00b7te", "denkst", ",", "kraus", "li\u00b7ni\u00b7en\u00b7haft", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun will das Liniennetz die F\u00fcllen in sich fassen,", "tokens": ["Nun", "will", "das", "Li\u00b7ni\u00b7en\u00b7netz", "die", "F\u00fcl\u00b7len", "in", "sich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und diese wollen sich von ihm entfalten lassen.", "tokens": ["Und", "die\u00b7se", "wol\u00b7len", "sich", "von", "ihm", "ent\u00b7fal\u00b7ten", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo so die beiden sich umschlingen und durchdringen,", "tokens": ["Wo", "so", "die", "bei\u00b7den", "sich", "um\u00b7schlin\u00b7gen", "und", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "PIAT", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wird gehaltvoll ein Gestaltetes entspringen.", "tokens": ["Da", "wird", "ge\u00b7halt\u00b7voll", "ein", "Ge\u00b7stal\u00b7te\u00b7tes", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}