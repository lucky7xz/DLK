{"textgrid.poem.48088": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "4. Im Reiche des Todes", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nimmst du gleich die Welt zum Raub,", "tokens": ["Und", "nimmst", "du", "gleich", "die", "Welt", "zum", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch deine Reiche schreit ich;", "tokens": ["durch", "dei\u00b7ne", "Rei\u00b7che", "schreit", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00fcber das fallende Lindenlaub", "tokens": ["\u00fc\u00b7ber", "das", "fal\u00b7len\u00b7de", "Lin\u00b7den\u00b7laub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "und meiner Ahnen wehenden Staub", "tokens": ["und", "mei\u00b7ner", "Ah\u00b7nen", "we\u00b7hen\u00b7den", "Staub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "die segnenden H\u00e4nde breit ich!", "tokens": ["die", "seg\u00b7nen\u00b7den", "H\u00e4n\u00b7de", "breit", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bist der finstere Sieger nicht;", "tokens": ["Du", "bist", "der", "fins\u00b7te\u00b7re", "Sie\u00b7ger", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "du dienst dem lachenden Leben!", "tokens": ["du", "dienst", "dem", "la\u00b7chen\u00b7den", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus deinen Tiefen keimt und bricht", "tokens": ["Aus", "dei\u00b7nen", "Tie\u00b7fen", "keimt", "und", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und mu\u00df zum flammenden Sonnenlicht", "tokens": ["und", "mu\u00df", "zum", "flam\u00b7men\u00b7den", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "die junge Saat sich heben.", "tokens": ["die", "jun\u00b7ge", "Saat", "sich", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}