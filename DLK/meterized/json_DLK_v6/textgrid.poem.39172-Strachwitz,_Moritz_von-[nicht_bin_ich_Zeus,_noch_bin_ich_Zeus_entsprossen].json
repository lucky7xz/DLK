{"textgrid.poem.39172": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[nicht bin ich Zeus, noch bin ich Zeus entsprossen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht bin ich Zeus, noch bin ich Zeus entsprossen,", "tokens": ["Nicht", "bin", "ich", "Zeus", ",", "noch", "bin", "ich", "Zeus", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "NE", "$,", "ADV", "VAFIN", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der niederwettert in der Blitze Schweben,", "tokens": ["Der", "nie\u00b7der\u00b7wet\u00b7tert", "in", "der", "Blit\u00b7ze", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An dessen G\u00f6tterbrust die Braut von Theben,", "tokens": ["An", "des\u00b7sen", "G\u00f6t\u00b7ter\u00b7brust", "die", "Braut", "von", "The\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ird'scher Hauch, im Flammenku\u00df zerflossen.", "tokens": ["Als", "ird'\u00b7scher", "Hauch", ",", "im", "Flam\u00b7men\u00b7ku\u00df", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Liebe aber durch mein Blut gegossen,", "tokens": ["Die", "Lie\u00b7be", "a\u00b7ber", "durch", "mein", "Blut", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie stammt von Zeus und seiner Wetter Beben,", "tokens": ["Sie", "stammt", "von", "Zeus", "und", "sei\u00b7ner", "Wet\u00b7ter", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie irrt als Blitz in meinem tiefsten Leben", "tokens": ["Sie", "irrt", "als", "Blitz", "in", "mei\u00b7nem", "tiefs\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und braust daher wie Sturz von Bergkolossen.", "tokens": ["Und", "braust", "da\u00b7her", "wie", "Sturz", "von", "Berg\u00b7ko\u00b7los\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "KOKOM", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn du, Vesuv und Hekla, krachst zusammen", "tokens": ["Wenn", "du", ",", "Ve\u00b7suv", "und", "He\u00b7kla", ",", "krachst", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "KON", "NE", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mischest aller Sonnen Millionen,", "tokens": ["Und", "mi\u00b7schest", "al\u00b7ler", "Son\u00b7nen", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du k\u00f6nntest nicht mein Lieben \u00fcberflammen.", "tokens": ["Du", "k\u00f6nn\u00b7test", "nicht", "mein", "Lie\u00b7ben", "\u00fc\u00b7berf\u00b7lam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ihr, ihr wollt den Brand der Mittagszonen", "tokens": ["Und", "ihr", ",", "ihr", "wollt", "den", "Brand", "der", "Mit\u00b7tags\u00b7zo\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verl\u00f6schen, ihr Philister! und verdammen", "tokens": ["Ver\u00b7l\u00f6\u00b7schen", ",", "ihr", "Phi\u00b7lis\u00b7ter", "!", "und", "ver\u00b7dam\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die k\u00fchne Flamme der Uranionen?", "tokens": ["Die", "k\u00fch\u00b7ne", "Flam\u00b7me", "der", "U\u00b7ra\u00b7ni\u00b7o\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}