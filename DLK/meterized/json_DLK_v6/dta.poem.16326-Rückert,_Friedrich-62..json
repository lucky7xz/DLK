{"dta.poem.16326": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist lauter Dank, indem ich r\u00fcckwerts blicke,", "tokens": ["Mein", "Herz", "ist", "lau\u00b7ter", "Dank", ",", "in\u00b7dem", "ich", "r\u00fcck\u00b7werts", "bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus welcher Tr\u00fcbe sich gehellet mein Geschicke,", "tokens": ["Aus", "wel\u00b7cher", "Tr\u00fc\u00b7be", "sich", "ge\u00b7hel\u00b7let", "mein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie dumpfem Ringen sich entrang der lichte Schwung;", "tokens": ["Wie", "dum\u00b7pfem", "Rin\u00b7gen", "sich", "ent\u00b7rang", "der", "lich\u00b7te", "Schwung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jung war ich kummeralt, und alte freudenjung.", "tokens": ["Jung", "war", "ich", "kum\u00b7me\u00b7ralt", ",", "und", "al\u00b7te", "freu\u00b7den\u00b7jung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVFIN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measu