{"dta.poem.96": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Silvien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nimmt Silvia den Brief an mit geneigten H\u00e4nden?", "tokens": ["Nimmt", "Sil\u00b7via", "den", "Brief", "an", "mit", "ge\u00b7neig\u00b7ten", "H\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Darinn enthalten ist/ was meine Sinnen kr\u00e4nckt/", "tokens": ["Da\u00b7rinn", "ent\u00b7hal\u00b7ten", "ist", "/", "was", "mei\u00b7ne", "Sin\u00b7nen", "kr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und was dem Geist befiehlt sich zu ihr hinzuwenden/", "tokens": ["Und", "was", "dem", "Geist", "be\u00b7fiehlt", "sich", "zu", "ihr", "hin\u00b7zu\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wei\u00df ich/ da\u00df sie schon viel Guts von mir gedenckt.", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "sie", "schon", "viel", "Guts", "von", "mir", "ge\u00b7denckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar was hier heimlich ist/ sind ihr bekannte Sachen/", "tokens": ["Zwar", "was", "hier", "heim\u00b7lich", "ist", "/", "sind", "ihr", "be\u00b7kann\u00b7te", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "ADJD", "VAFIN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie wei\u00df/ da\u00df sie mein Hertz durch ihren Blick entz\u00fcndt/", "tokens": ["Sie", "wei\u00df", "/", "da\u00df", "sie", "mein", "Hertz", "durch", "ih\u00b7ren", "Blick", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr l\u00e4chelnd Auge will die Flammen gr\u00f6sser machen/", "tokens": ["Ihr", "l\u00e4\u00b7chelnd", "Au\u00b7ge", "will", "die", "Flam\u00b7men", "gr\u00f6s\u00b7ser", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die mehr denn allzugro\u00df/ ja unausl\u00f6sch lich sind.", "tokens": ["Die", "mehr", "denn", "all\u00b7zu\u00b7gro\u00df", "/", "ja", "un\u00b7aus\u00b7l\u00f6sch", "lich", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$(", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es kan mein schwacher Mund nicht alle Seuffzer z\u00e4hlen/", "tokens": ["Es", "kan", "mein", "schwa\u00b7cher", "Mund", "nicht", "al\u00b7le", "Seuff\u00b7zer", "z\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ihre Artigkeit in meiner Brust erregt;", "tokens": ["Die", "ih\u00b7re", "Ar\u00b7tig\u00b7keit", "in", "mei\u00b7ner", "Brust", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ob sie es schon wei\u00df/ will ich doch nicht verhehlen", "tokens": ["Und", "ob", "sie", "es", "schon", "wei\u00df", "/", "will", "ich", "doch", "nicht", "ver\u00b7heh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer in dem Hertzen erst das Feur hat angelegt.", "tokens": ["Wer", "in", "dem", "Hert\u00b7zen", "erst", "das", "Feur", "hat", "an\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der sch\u00f6nen Augen-Licht/ das gleich den Sternen blitzet/", "tokens": ["Der", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7Licht", "/", "das", "gleich", "den", "Ster\u00b7nen", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bringt mir die Liebes-Gluht/ ihr die Vollkommenheit;", "tokens": ["Bringt", "mir", "die", "Lie\u00b7bes\u00b7Gluht", "/", "ihr", "die", "Voll\u00b7kom\u00b7men\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vollkommen ist die Pracht/ die das Gesicht besitzet/", "tokens": ["Voll\u00b7kom\u00b7men", "ist", "die", "Pracht", "/", "die", "das", "Ge\u00b7sicht", "be\u00b7sit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$(", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer ist? der tadeln kan der Mienen Trefflichkeit.", "tokens": ["Wer", "ist", "?", "der", "ta\u00b7deln", "kan", "der", "Mie\u00b7nen", "Treff\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "ART", "VVINF", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit Schnee und Purpur sind die Wangen ausgezieret/", "tokens": ["Mit", "Schnee", "und", "Pur\u00b7pur", "sind", "die", "Wan\u00b7gen", "aus\u00b7ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da sieht man den ", "tokens": ["Da", "sieht", "man", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.19": {"text": "Die Stirne ist der Thron worauf den Scepter f\u00fchret", "tokens": ["Die", "Stir\u00b7ne", "ist", "der", "Thron", "wo\u00b7rauf", "den", "Scep\u00b7ter", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die holde Freundlichkeit geschm\u00fcckt mit tausend Sch\u00f6n.", "tokens": ["Die", "hol\u00b7de", "Freund\u00b7lich\u00b7keit", "ge\u00b7schm\u00fcckt", "mit", "tau\u00b7send", "Sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Lippen sind Rubin davon ein ", "tokens": ["Die", "Lip\u00b7pen", "sind", "Ru\u00b7bin", "da\u00b7von", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "PAV", "ART"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Der Athem \u00fcbersteigt des besten Balsams Krafft/", "tokens": ["Der", "A\u00b7them", "\u00fc\u00b7bers\u00b7teigt", "des", "bes\u00b7ten", "Bal\u00b7sams", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gl\u00fcckselig/ wer die Lust noch dermahleins geniesset", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "/", "wer", "die", "Lust", "noch", "der\u00b7mah\u00b7leins", "ge\u00b7nies\u00b7set"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "PWS", "ART", "NN", "ADV", "ADV", "VVPP"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Da\u00df ihm der s\u00fcsse Mund ein solches Labsahl schafft.", "tokens": ["Da\u00df", "ihm", "der", "s\u00fcs\u00b7se", "Mund", "ein", "sol\u00b7ches", "Lab\u00b7sahl", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Cedern glatte Hals wie aus Albast geschnitzet/", "tokens": ["Der", "Ce\u00b7dern", "glat\u00b7te", "Hals", "wie", "aus", "Al\u00b7bast", "ge\u00b7schnit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KOKOM", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der siegt den Liljen an/ die ", "tokens": ["Der", "siegt", "den", "Lil\u00b7jen", "an", "/", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Das Haar/ so als ein Wall den sch\u00f6nen Ort besch\u00fctzet/", "tokens": ["Das", "Haar", "/", "so", "als", "ein", "Wall", "den", "sch\u00f6\u00b7nen", "Ort", "be\u00b7sch\u00fct\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.29": {"text": "Der Br\u00fcste Perlen Pracht ist ein entz\u00fcckends Wesen/", "tokens": ["Der", "Br\u00fcs\u00b7te", "Per\u00b7len", "Pracht", "ist", "ein", "ent\u00b7z\u00fc\u00b7ckends", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Aus ihrer Rosen Spitz die holde Anmuth lacht/", "tokens": ["Aus", "ih\u00b7rer", "Ro\u00b7sen", "Spitz", "die", "hol\u00b7de", "An\u00b7muth", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Sch\u00f6nheit hat dabey den Wohn-Platz auserlesen/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "da\u00b7bey", "den", "Wohn\u00b7Platz", "au\u00b7ser\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und die Natur daran ihr Meister-St\u00fcck gemacht.", "tokens": ["Und", "die", "Na\u00b7tur", "da\u00b7ran", "ihr", "Meis\u00b7ter\u00b7St\u00fcck", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Den noch verdeckten Schatz und ander Kostbarkeiten/", "tokens": ["Den", "noch", "ver\u00b7deck\u00b7ten", "Schatz", "und", "an\u00b7der", "Kost\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die will ich als verdeckt mit Schweigen \u00fcbergehn.", "tokens": ["Die", "will", "ich", "als", "ver\u00b7deckt", "mit", "Schwei\u00b7gen", "\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "KOKOM", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer ist der gegen sie und ihre Pracht will streiten?", "tokens": ["Wer", "ist", "der", "ge\u00b7gen", "sie", "und", "ih\u00b7re", "Pracht", "will", "strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "APPR", "PPER", "KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der nicht durch ihre Macht sich mu\u00df bezwungen sehn.", "tokens": ["Der", "nicht", "durch", "ih\u00b7re", "Macht", "sich", "mu\u00df", "be\u00b7zwun\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPOSAT", "NN", "PRF", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mit Freuden h\u00f6re ich wie meine Fessel klingen/", "tokens": ["Mit", "Freu\u00b7den", "h\u00f6\u00b7re", "ich", "wie", "mei\u00b7ne", "Fes\u00b7sel", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich behte sie mein Licht als meine G\u00f6ttin an/", "tokens": ["Ich", "beh\u00b7te", "sie", "mein", "Licht", "als", "mei\u00b7ne", "G\u00f6t\u00b7tin", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jhr Auge/ so mein Hertz zur Liebe k\u00f6nnen zwingen/", "tokens": ["Ihr", "Au\u00b7ge", "/", "so", "mein", "Hertz", "zur", "Lie\u00b7be", "k\u00f6n\u00b7nen", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit einem sanfften Blick die Schmertzen lindern kan.", "tokens": ["Mit", "ei\u00b7nem", "sanff\u00b7ten", "Blick", "die", "Schmert\u00b7zen", "lin\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Erblickt ", "tokens": ["Er\u00b7blickt"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.42": {"text": "Den Brand/ den ihr erregt/ last euch gef\u00e4llig seyn/", "tokens": ["Den", "Brand", "/", "den", "ihr", "er\u00b7regt", "/", "last", "euch", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VVPP", "$(", "VVFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nehmt mein verirrtes Schiff in den gew\u00fcnschten Haven/", "tokens": ["Nehmt", "mein", "ver\u00b7irr\u00b7tes", "Schiff", "in", "den", "ge\u00b7w\u00