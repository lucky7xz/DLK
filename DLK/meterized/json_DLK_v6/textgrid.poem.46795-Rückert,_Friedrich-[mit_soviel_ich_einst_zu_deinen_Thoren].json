{"textgrid.poem.46795": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mit soviel ich einst zu deinen Thoren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit soviel ich einst zu deinen Thoren", "tokens": ["Mit", "so\u00b7viel", "ich", "einst", "zu", "dei\u00b7nen", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eingezogen bin,", "tokens": ["Ein\u00b7ge\u00b7zo\u00b7gen", "bin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df soviel mir noch sind unverloren,", "tokens": ["Da\u00df", "so\u00b7viel", "mir", "noch", "sind", "un\u00b7ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das ist mein Gewinn.", "tokens": ["Das", "ist", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Was in dir mir ward hinzugeboren,", "tokens": ["Was", "in", "dir", "mir", "ward", "hin\u00b7zu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Nahmst du wieder hin;", "tokens": ["Nahmst", "du", "wie\u00b7der", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "La\u00df mich nun hinaus zu deinen Thoren,", "tokens": ["La\u00df", "mich", "nun", "hin\u00b7aus", "zu", "dei\u00b7nen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APZR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie herein ich bin!", "tokens": ["Wie", "her\u00b7ein", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKVZ", "PPER", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}