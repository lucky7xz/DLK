{"dta.poem.1348": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Jesu/ meiner Seelen Ruh/", "tokens": ["Je\u00b7su", "/", "mei\u00b7ner", "See\u00b7len", "Ruh", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Wonn und Lust bist du!", "tokens": ["Mei\u00b7ne", "Wonn", "und", "Lust", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00f6st mir Creutz und Ungl\u00fcck f\u00fcr/", "tokens": ["St\u00f6st", "mir", "Creutz", "und", "Un\u00b7gl\u00fcck", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So besprech ich mich mit dir:", "tokens": ["So", "be\u00b7sprech", "ich", "mich", "mit", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du/ mein Trost und bester Rath/", "tokens": ["Du", "/", "mein", "Trost", "und", "bes\u00b7ter", "Rath", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der mich nie verlassen hat/", "tokens": ["Der", "mich", "nie", "ver\u00b7las\u00b7sen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df der offnen Wunden Scho\u00df", "tokens": ["La\u00df", "der", "off\u00b7nen", "Wun\u00b7den", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seyn mein sichres Ehren-Schlo\u00df/", "tokens": ["Seyn", "mein", "sich\u00b7res", "Eh\u00b7ren\u00b7Schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dein Creutz meinen Wander-Stab/", "tokens": ["Dein", "Creutz", "mei\u00b7nen", "Wan\u00b7der\u00b7Stab", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Deinen Arm mein Sorgen-Grab/", "tokens": ["Dei\u00b7nen", "Arm", "mein", "Sor\u00b7gen\u00b7Grab", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Meine Freude deine Noth", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "dei\u00b7ne", "Noth"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und mein Leben deinen Tod!", "tokens": ["Und", "mein", "Le\u00b7ben", "dei\u00b7nen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}