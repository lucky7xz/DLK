{"dta.poem.14450": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Versteinert schweigen mu\u00df der Wehmuth Welle,", "tokens": ["Ver\u00b7stei\u00b7nert", "schwei\u00b7gen", "mu\u00df", "der", "Weh\u00b7muth", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVINF", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie willig auch, schm\u00f6lz' ihn ein w\u00e4rmend Auge,", "tokens": ["Wie", "wil\u00b7lig", "auch", ",", "schm\u00f6lz'", "ihn", "ein", "w\u00e4r\u00b7mend", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "$,", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Krystall zerflie\u00dfen wollt' als Thr\u00e4nenquelle.", "tokens": ["Krys\u00b7tall", "zer\u00b7flie\u00b7\u00dfen", "wollt'", "als", "Thr\u00e4\u00b7nen\u00b7quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}