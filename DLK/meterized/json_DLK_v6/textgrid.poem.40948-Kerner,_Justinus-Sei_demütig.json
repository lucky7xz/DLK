{"textgrid.poem.40948": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sei dem\u00fctig", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchme dich auf dieser Welt,", "tokens": ["R\u00fch\u00b7me", "dich", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mensch! nicht deines eignen Lichts!", "tokens": ["Mensch", "!", "nicht", "dei\u00b7nes", "eig\u00b7nen", "Lichts", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnen sind ob dich gestellt,", "tokens": ["Son\u00b7nen", "sind", "ob", "dich", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen die dein Schein ein Nichts.", "tokens": ["Ge\u00b7gen", "die", "dein", "Schein", "ein", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ART", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst hier hoffen, glauben nur,", "tokens": ["Kannst", "hier", "hof\u00b7fen", ",", "glau\u00b7ben", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bitten, doch erzwingen nicht,", "tokens": ["Bit\u00b7ten", ",", "doch", "er\u00b7zwin\u00b7gen", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ver\u00e4ndert's die Natur,", "tokens": ["Nicht", "ver\u00b7\u00e4n\u00b7dert's", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ein Menschenherz zerbricht.", "tokens": ["Wenn", "ein", "Men\u00b7schen\u00b7herz", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoffe: da\u00df durch Todesnacht", "tokens": ["Hof\u00b7fe", ":", "da\u00df", "durch", "To\u00b7des\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "KOUS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott dich f\u00fchrt in Sonnen ein \u2013", "tokens": ["Gott", "dich", "f\u00fchrt", "in", "Son\u00b7nen", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was er immer mit dir macht,", "tokens": ["Was", "er", "im\u00b7mer", "mit", "dir", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du bist dein nicht, du bist ", "tokens": ["Du", "bist", "dein", "nicht", ",", "du", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "PTKNEG", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sei dem\u00fctig wie das Blatt,", "tokens": ["Sei", "de\u00b7m\u00fc\u00b7tig", "wie", "das", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das im Herbst vom Baume geht,", "tokens": ["Das", "im", "Herbst", "vom", "Bau\u00b7me", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemals das geklaget hat,", "tokens": ["Nie\u00b7mals", "das", "ge\u00b7kla\u00b7get", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es jetzt der Sturm verweht.", "tokens": ["Da\u00df", "es", "jetzt", "der", "Sturm", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}