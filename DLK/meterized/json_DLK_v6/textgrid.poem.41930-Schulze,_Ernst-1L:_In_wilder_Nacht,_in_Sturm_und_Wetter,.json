{"textgrid.poem.41930": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: In wilder Nacht, in Sturm und Wetter,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In wilder Nacht, in Sturm und Wetter,", "tokens": ["In", "wil\u00b7der", "Nacht", ",", "in", "Sturm", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sind' ich Kraft, da find' ich Lust;", "tokens": ["Da", "sind'", "ich", "Kraft", ",", "da", "find'", "ich", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Sturm erkenn' ich meine G\u00f6tter,", "tokens": ["Im", "Sturm", "er\u00b7kenn'", "ich", "mei\u00b7ne", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die stolzen Herrscher meiner Brust;", "tokens": ["Die", "stol\u00b7zen", "Herr\u00b7scher", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie sie nirgend friedlich hausen", "tokens": ["Und", "wie", "sie", "nir\u00b7gend", "fried\u00b7lich", "hau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ewig ziehn von Ort zu Ort,", "tokens": ["Und", "e\u00b7wig", "ziehn", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wandl' auch ich durch Nacht und Grausen", "tokens": ["So", "wandl'", "auch", "ich", "durch", "Nacht", "und", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf ihren dunklen Pfaden fort.", "tokens": ["Auf", "ih\u00b7ren", "dunk\u00b7len", "Pfa\u00b7den", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was soll ich zagend stehn und schwanken,", "tokens": ["Was", "soll", "ich", "za\u00b7gend", "stehn", "und", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Inde\u00df der W\u00fcrfel nie sich regt?", "tokens": ["In\u00b7de\u00df", "der", "W\u00fcr\u00b7fel", "nie", "sich", "regt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll das junge Herz erkranken,", "tokens": ["Was", "soll", "das", "jun\u00b7ge", "Herz", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lang es stark und muthig schl\u00e4gt?", "tokens": ["So", "lang", "es", "stark", "und", "mut\u00b7hig", "schl\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zieh' ich auch gekr\u00e4nkt, verlassen,", "tokens": ["Und", "zieh'", "ich", "auch", "ge\u00b7kr\u00e4nkt", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verbannt, ein heimathloser Mann,", "tokens": ["Ver\u00b7bannt", ",", "ein", "hei\u00b7math\u00b7lo\u00b7ser", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir gn\u00fcgt's, so lang ich bitter hassen", "tokens": ["Mir", "gn\u00fcgt's", ",", "so", "lang", "ich", "bit\u00b7ter", "has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "ADJD", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und tief von Herzen lieben kann.", "tokens": ["Und", "tief", "von", "Her\u00b7zen", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn wie die Wimpel lustig schweben,", "tokens": ["Denn", "wie", "die", "Wim\u00b7pel", "lus\u00b7tig", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn scheidend auch der Schiffer klagt,", "tokens": ["Wenn", "schei\u00b7dend", "auch", "der", "Schif\u00b7fer", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So weht im Schmerz ein keckes Leben,", "tokens": ["So", "weht", "im", "Schmerz", "ein", "ke\u00b7ckes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald er Gro\u00dfes tr\u00e4gt und wagt.", "tokens": ["So\u00b7bald", "er", "Gro\u00b7\u00dfes", "tr\u00e4gt", "und", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gilt es auch ein bittres Scheiden,", "tokens": ["Und", "gilt", "es", "auch", "ein", "bitt\u00b7res", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und gilt es Tod auf blut'gem Pfad;", "tokens": ["Und", "gilt", "es", "Tod", "auf", "blut'\u00b7gem", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im dunkeln Busen st\u00fcrmt das Leiden;", "tokens": ["Im", "dun\u00b7keln", "Bu\u00b7sen", "st\u00fcrmt", "das", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch hell im Auge lacht die That.", "tokens": ["Doch", "hell", "im", "Au\u00b7ge", "lacht", "die", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}