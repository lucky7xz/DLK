{"dta.poem.23361": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schnaderh\u00fcpfel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der F\u00fcrst und der Adel stehn immer im Bund,", "tokens": ["Der", "F\u00fcrst", "und", "der", "A\u00b7del", "stehn", "im\u00b7mer", "im", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der F\u00fcrst ist der J\u00e4ger, der Adel der Hund.", "tokens": ["Der", "F\u00fcrst", "ist", "der", "J\u00e4\u00b7ger", ",", "der", "A\u00b7del", "der", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der F\u00fcrst ist der J\u00e4ger, das Volk ist das Wild,", "tokens": ["Der", "F\u00fcrst", "ist", "der", "J\u00e4\u00b7ger", ",", "das", "Volk", "ist", "das", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Weil mehr das Regal als das Menschenrecht gilt.", "tokens": ["Weil", "mehr", "das", "Re\u00b7gal", "als", "das", "Men\u00b7schen\u00b7recht", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Und gehet der J\u00e4ger auf die Hasenjagd,", "tokens": ["Und", "ge\u00b7het", "der", "J\u00e4\u00b7ger", "auf", "die", "Ha\u00b7sen\u00b7jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat noch immer der Hund den Vermittler gemacht.", "tokens": ["Hat", "noch", "im\u00b7mer", "der", "Hund", "den", "Ver\u00b7mitt\u00b7ler", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Und wenn es sich handelt um Constitution,", "tokens": ["Und", "wenn", "es", "sich", "han\u00b7delt", "um", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vermittelt der Adel zwischen F\u00fcrst und Nation.", "tokens": ["Ver\u00b7mit\u00b7telt", "der", "A\u00b7del", "zwi\u00b7schen", "F\u00fcrst", "und", "Na\u00b7tion", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Bl\u00e4st J\u00e4ger und Hund und Has' in Ein Horn,", "tokens": ["Bl\u00e4st", "J\u00e4\u00b7ger", "und", "Hund", "und", "Has'", "in", "Ein", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "KON", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind wir alle vergn\u00fcgt von hinten und vorn.", "tokens": ["Sind", "wir", "al\u00b7le", "ver\u00b7gn\u00fcgt", "von", "hin\u00b7ten", "und", "vorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "APPR", "ADV", "KON", "ADV", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}}}}