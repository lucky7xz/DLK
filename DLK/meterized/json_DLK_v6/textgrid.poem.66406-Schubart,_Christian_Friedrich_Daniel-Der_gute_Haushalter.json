{"textgrid.poem.66406": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der gute Haushalter", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum reichen Dau\u00df sprach einst weiser Mann:", "tokens": ["Zum", "rei\u00b7chen", "Dau\u00df", "sprach", "einst", "wei\u00b7ser", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr Nachbar, sieh doch deine Kinder an.", "tokens": ["Herr", "Nach\u00b7bar", ",", "sieh", "doch", "dei\u00b7ne", "Kin\u00b7der", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVIMP", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein dummer Kerl, der selbst nichts kann,", "tokens": ["Ein", "dum\u00b7mer", "Kerl", ",", "der", "selbst", "nichts", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt ihnen Unterricht;", "tokens": ["Gibt", "ih\u00b7nen", "Un\u00b7ter\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ach! die Armen lernen nicht", "tokens": ["Und", "ach", "!", "die", "Ar\u00b7men", "ler\u00b7nen", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des B\u00fcrgers und des Christen Pflicht.", "tokens": ["Des", "B\u00fcr\u00b7gers", "und", "des", "Chris\u00b7ten", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kaltsinnig sprach der Nachbar Dau\u00df:", "tokens": ["Kalts\u00b7in\u00b7nig", "sprach", "der", "Nach\u00b7bar", "Dau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Das Ding \u2013 es gibt sich schon, Herr Bruder!", "tokens": ["Das", "Ding", "\u2013", "es", "gibt", "sich", "schon", ",", "Herr", "Bru\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PRF", "ADV", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo, Teufel, steckt der Knecht im Haus?", "tokens": ["Wo", ",", "Teu\u00b7fel", ",", "steckt", "der", "Knecht", "im", "Haus", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und gibt den Ochsen Futter,", "tokens": ["Und", "gibt", "den", "Och\u00b7sen", "Fut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Legt frische Streu und striegelt sie?", "tokens": ["Legt", "fri\u00b7sche", "Streu", "und", "strie\u00b7gelt", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein' Seel'! mich daurt das arme Vieh!", "tokens": ["Mein'", "Seel'", "!", "mich", "daurt", "das", "ar\u00b7me", "Vieh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}