{"dta.poem.23577": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Patrioten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df in einer alten Schenke,", "tokens": ["Ich", "sa\u00df", "in", "ei\u00b7ner", "al\u00b7ten", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verr\u00e4uchert waren Tisch' und B\u00e4nke,", "tokens": ["Ver\u00b7r\u00e4u\u00b7chert", "wa\u00b7ren", "Tisch'", "und", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum sah man Ohren, Aug' und Nase,", "tokens": ["Kaum", "sah", "man", "Oh\u00b7ren", ",", "Aug'", "und", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder sa\u00df vor seinem Glase.", "tokens": ["Ein", "je\u00b7der", "sa\u00df", "vor", "sei\u00b7nem", "Gla\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als sie so im Zwielichtscheine", "tokens": ["Und", "als", "sie", "so", "im", "Zwie\u00b7licht\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Still sa\u00dfen da bei ihrem Weine,", "tokens": ["Still", "sa\u00b7\u00dfen", "da", "bei", "ih\u00b7rem", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ward es Zwielicht auch in ihnen,", "tokens": ["Da", "ward", "es", "Zwie\u00b7licht", "auch", "in", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie sich selber hell erschienen.", "tokens": ["Da\u00df", "sie", "sich", "sel\u00b7ber", "hell", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Augen funkelten wie Blitze,", "tokens": ["Die", "Au\u00b7gen", "fun\u00b7kel\u00b7ten", "wie", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie r\u00fcckten schnell von ihrem Sitze,", "tokens": ["Sie", "r\u00fcck\u00b7ten", "schnell", "von", "ih\u00b7rem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wurden laut und immer lauter,", "tokens": ["Sie", "wur\u00b7den", "laut", "und", "im\u00b7mer", "lau\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vertrauter dann und noch vertrauter.", "tokens": ["Ver\u00b7trau\u00b7ter", "dann", "und", "noch", "ver\u00b7trau\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sie aus voller Kehle sangen!", "tokens": ["Wie", "sie", "aus", "vol\u00b7ler", "Keh\u00b7le", "san\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie die Gl\u00e4ser hell erklangen!", "tokens": ["Und", "wie", "die", "Gl\u00e4\u00b7ser", "hell", "er\u00b7klan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201egesegnet sei die gute Stunde!\u201c", "tokens": ["\u201e", "ge\u00b7seg\u00b7net", "sei", "die", "gu\u00b7te", "Stun\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So scholl es laut von jedem Munde.", "tokens": ["So", "scholl", "es", "laut", "von", "je\u00b7dem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201edem K\u00f6nig Heil! Heil seinen Fahnen!", "tokens": ["\u201e", "dem", "K\u00f6\u00b7nig", "Heil", "!", "Heil", "sei\u00b7nen", "Fah\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heil seinen guten Unterthanen!", "tokens": ["Heil", "sei\u00b7nen", "gu\u00b7ten", "Un\u00b7ter\u00b7tha\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heil seinen treuen braven Knechten,", "tokens": ["Heil", "sei\u00b7nen", "treu\u00b7en", "bra\u00b7ven", "Knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die f\u00fcr ihn sterben, f\u00fcr ihn fechten!\u201c", "tokens": ["Die", "f\u00fcr", "ihn", "ster\u00b7ben", ",", "f\u00fcr", "ihn", "fech\u00b7ten", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$,", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da gab es Witze, Scherz' und Schw\u00e4nke,", "tokens": ["Da", "gab", "es", "Wit\u00b7ze", ",", "Scher\u00b7z'", "und", "Schw\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lebendig ward die ganze Schenke,", "tokens": ["Le\u00b7ben\u00b7dig", "ward", "die", "gan\u00b7ze", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie wurden toll und immer toller,", "tokens": ["Sie", "wur\u00b7den", "toll", "und", "im\u00b7mer", "tol\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flaschen leer, die K\u00f6pfe voller.", "tokens": ["Die", "Fla\u00b7schen", "leer", ",", "die", "K\u00f6p\u00b7fe", "vol\u00b7ler", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der eine fiel, der andre schwankte,", "tokens": ["Der", "ei\u00b7ne", "fiel", ",", "der", "and\u00b7re", "schwank\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine sank, der andre wankte,", "tokens": ["Der", "ei\u00b7ne", "sank", ",", "der", "and\u00b7re", "wank\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hob sich einer auch mal wieder,", "tokens": ["Und", "hob", "sich", "ei\u00b7ner", "auch", "mal", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fiel er mit dem andern nieder.", "tokens": ["So", "fiel", "er", "mit", "dem", "an\u00b7dern", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und Wirth und G\u00e4ste, Tisch' und B\u00e4nke,", "tokens": ["Und", "Wirth", "und", "G\u00e4s\u00b7te", ",", "Tisch'", "und", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Flaschen, Gl\u00e4ser, Scherz' und Schw\u00e4nke,", "tokens": ["Und", "Fla\u00b7schen", ",", "Gl\u00e4\u00b7ser", ",", "Scher\u00b7z'", "und", "Schw\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie lags beisammen da so traulich,", "tokens": ["Wie", "lags", "bei\u00b7sam\u00b7men", "da", "so", "trau\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie gem\u00fcthlich und erbaulich!", "tokens": ["Und", "wie", "ge\u00b7m\u00fcth\u00b7lich", "und", "er\u00b7bau\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}