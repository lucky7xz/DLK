{"dta.poem.2597": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber den unverhofft j\u00e4mmerlichen  \n  Todes-Fall  \n Hn. C. L.   Studiosi   1658. 28. Jan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun tritt herzu du stoltze Welt/", "tokens": ["Nun", "tritt", "her\u00b7zu", "du", "stolt\u00b7ze", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lerne dich doch selbst erkennen/", "tokens": ["Und", "ler\u00b7ne", "dich", "doch", "selbst", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst dich einen Schauplatz nennen/", "tokens": ["Du", "wirst", "dich", "ei\u00b7nen", "Schau\u00b7platz", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da Tod und Leben f\u00fcrgestellt!", "tokens": ["Da", "Tod", "und", "Le\u00b7ben", "f\u00fcr\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da Jahr und Zeit das Urtheil sprechen/", "tokens": ["Da", "Jahr", "und", "Zeit", "das", "Ur\u00b7theil", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Kron/ und Thron/ und Scepter brechen", "tokens": ["Und", "Kron", "/", "und", "Thron", "/", "und", "Scep\u00b7ter", "bre\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$(", "KON", "NN", "$(", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit unerh\u00f6rter Grausamkeit:", "tokens": ["Mit", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Grau\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie gantze Reich\u2019 in Aschen k\u00f6nnen stehn/", "tokens": ["Wie", "gant\u00b7ze", "Reich'", "in", "A\u00b7schen", "k\u00f6n\u00b7nen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wie der Mensch ihr Herrscher soll vergehn.", "tokens": ["Und", "wie", "der", "Mensch", "ihr", "Herr\u00b7scher", "soll", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Di\u00df was viel K\u00fcnstler auffgebaut/", "tokens": ["Di\u00df", "was", "viel", "K\u00fcnst\u00b7ler", "auff\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Steinen/ Stahl und Ertz gebunden/", "tokens": ["Mit", "Stei\u00b7nen", "/", "Stahl", "und", "Ertz", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Schwindt und vergeht in wenig Stunden/", "tokens": ["Schwindt", "und", "ver\u00b7geht", "in", "we\u00b7nig", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Eh\u2019 als man fast den Zufall schaut.", "tokens": ["Eh'", "als", "man", "fast", "den", "Zu\u00b7fall", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der H\u00e4user Himmel-gleiche Spitzen;", "tokens": ["Der", "H\u00e4u\u00b7ser", "Him\u00b7mel\u00b7glei\u00b7che", "Spit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die m\u00fcssen offt in Flammen schwitzen/", "tokens": ["Die", "m\u00fcs\u00b7sen", "offt", "in", "Flam\u00b7men", "schwit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wenn der ergrimmte Donner tobt:", "tokens": ["Wenn", "der", "er\u00b7grimm\u00b7te", "Don\u00b7ner", "tobt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie offt zerreibt die allzustrenge Zeit/", "tokens": ["Wie", "offt", "zer\u00b7reibt", "die", "all\u00b7zu\u00b7stren\u00b7ge", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Durch eigne Krafft des Marmels Ewigkeit.", "tokens": ["Durch", "eig\u00b7ne", "Krafft", "des", "Mar\u00b7mels", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wo Troja stund/ da ist ein Feld/", "tokens": ["Wo", "Tro\u00b7ja", "stund", "/", "da", "ist", "ein", "Feld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$(", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und wo Athen vorhin gesieget/", "tokens": ["Und", "wo", "A\u00b7then", "vor\u00b7hin", "ge\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ADV", "VVPP", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Da wird geackert und gepfl\u00fcget/", "tokens": ["Da", "wird", "ge\u00b7a\u00b7ckert", "und", "ge\u00b7pfl\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wo bleibt die alte Forder-Welt?", "tokens": ["Wo", "bleibt", "die", "al\u00b7te", "For\u00b7der\u00b7Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Was Erde Mutter heist/ mu\u00df sterben/", "tokens": ["Was", "Er\u00b7de", "Mut\u00b7ter", "heist", "/", "mu\u00df", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VAFIN", "$(", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und auch in ihrer Scho\u00df verderben/", "tokens": ["Und", "auch", "in", "ih\u00b7rer", "Scho\u00df", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der Schlu\u00df der ist schon l\u00e4ngst gemacht:", "tokens": ["Der", "Schlu\u00df", "der", "ist", "schon", "l\u00e4ngst", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Kein Bauwerck hilfft/ noch Rhodis grosse Kunst/", "tokens": ["Kein", "Bau\u00b7werck", "hilfft", "/", "noch", "Rho\u00b7dis", "gros\u00b7se", "Kunst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$(", "ADV", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Sein Sonnenbild das w\u00e4re hier umbsonst.", "tokens": ["Sein", "Son\u00b7nen\u00b7bild", "das", "w\u00e4\u00b7re", "hier", "um\u00b7bsonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und solte Rom schon ewig seyn/", "tokens": ["Und", "sol\u00b7te", "Rom", "schon", "e\u00b7wig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Mit seinen Bergen und Pall\u00e4sten/", "tokens": ["Mit", "sei\u00b7nen", "Ber\u00b7gen", "und", "Pal\u00b7l\u00e4s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "So ward es doch von frembden G\u00e4sten", "tokens": ["So", "ward", "es", "doch", "von", "fremb\u00b7den", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Bi\u00df auf den Grund ge\u00e4schert ein.", "tokens": ["Bi\u00df", "auf", "den", "Grund", "ge\u00b7\u00e4s\u00b7chert", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die Stadt/ der nichts ist gleich gewesen/", "tokens": ["Die", "Stadt", "/", "der", "nichts", "ist", "gleich", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIS", "VAFIN", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die kan man kaum in B\u00fcchern lesen/", "tokens": ["Die", "kan", "man", "kaum", "in", "B\u00fc\u00b7chern", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Man findt ihr Ebenbild nicht mehr.", "tokens": ["Man", "findt", "ihr", "E\u00b7ben\u00b7bild", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Geh trotze nun/ o Mensch/ auff deine Macht/", "tokens": ["Geh", "trot\u00b7ze", "nun", "/", "o", "Mensch", "/", "auff", "dei\u00b7ne", "Macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "FM", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und suche Licht in dieser finstern Nacht.", "tokens": ["Und", "su\u00b7che", "Licht", "in", "die\u00b7ser", "fins\u00b7tern", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Betrachte deine Lebens-Zeit/", "tokens": ["Be\u00b7trach\u00b7te", "dei\u00b7ne", "Le\u00b7bens\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Die wie ein schneller Traum vergehet/", "tokens": ["Die", "wie", "ein", "schnel\u00b7ler", "Traum", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Und in dem Unbestand bestehet/", "tokens": ["Und", "in", "dem", "Un\u00b7be\u00b7stand", "be\u00b7ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Die voll von frecher Eitelkeit:", "tokens": ["Die", "voll", "von", "fre\u00b7cher", "Ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Was heute bl\u00fcht kan morgen welcken/", "tokens": ["Was", "heu\u00b7te", "bl\u00fcht", "kan", "mor\u00b7gen", "wel\u00b7