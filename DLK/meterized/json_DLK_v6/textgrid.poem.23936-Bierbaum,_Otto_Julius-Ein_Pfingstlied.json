{"textgrid.poem.23936": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ein Pfingstlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Maien f\u00fchr ich an meiner Hand,", "tokens": ["Den", "Mai\u00b7en", "f\u00fchr", "ich", "an", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Degen an der Seiten,", "tokens": ["Den", "De\u00b7gen", "an", "der", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pfingstjunker bin ich zubenannt", "tokens": ["Pfingst\u00b7jun\u00b7ker", "bin", "ich", "zu\u00b7be\u00b7nannt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will in das gelobte Land", "tokens": ["Und", "will", "in", "das", "ge\u00b7lob\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einem Schimmel reiten.", "tokens": ["Auf", "ei\u00b7nem", "Schim\u00b7mel", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf einem Schimmel bl\u00fchrieselwei\u00df", "tokens": ["Auf", "ei\u00b7nem", "Schim\u00b7mel", "bl\u00fch\u00b7rie\u00b7sel\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit seidenen Schabracken.", "tokens": ["Mit", "sei\u00b7de\u00b7nen", "Scha\u00b7bra\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Der Mai ihn wohl zu f\u00fchren wei\u00df", "tokens": ["Der", "Mai", "ihn", "wohl", "zu", "f\u00fch\u00b7ren", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Apfelbl\u00fctenrei\u00df.", "tokens": ["Mit", "ei\u00b7nem", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten\u00b7rei\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stolz tr\u00e4gt er seinen Nacken.", "tokens": ["Stolz", "tr\u00e4gt", "er", "sei\u00b7nen", "Na\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch nicht allein ich reiten mag,", "tokens": ["Doch", "nicht", "al\u00b7lein", "ich", "rei\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag nicht alleine reiten,", "tokens": ["Mag", "nicht", "al\u00b7lei\u00b7ne", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich soll durch Tag und Nacht und Tag,", "tokens": ["Mich", "soll", "durch", "Tag", "und", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich soll durch Feld und Wald und Hag", "tokens": ["Mich", "soll", "durch", "Feld", "und", "Wald", "und", "Hag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein M\u00e4del jung begleiten.", "tokens": ["Ein", "M\u00e4\u00b7del", "jung", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein M\u00e4del jung, das soll mit mir", "tokens": ["Ein", "M\u00e4\u00b7del", "jung", ",", "das", "soll", "mit", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PDS", "VMFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinem Schimmel schacken.", "tokens": ["Auf", "mei\u00b7nem", "Schim\u00b7mel", "scha\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hui da, du helle Maienzier!", "tokens": ["Hui", "da", ",", "du", "hel\u00b7le", "Mai\u00b7en\u00b7zier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Gr\u00fcne galoppieren wir,", "tokens": ["Durchs", "Gr\u00fc\u00b7ne", "ga\u00b7lop\u00b7pie\u00b7ren", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wind bl\u00e4ht die Schabracken.", "tokens": ["Der", "Wind", "bl\u00e4ht", "die", "Scha\u00b7bra\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun gehe, Mal, und klopfe an,", "tokens": ["Nun", "ge\u00b7he", ",", "Mal", ",", "und", "klop\u00b7fe", "an", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo liebe M\u00e4del hausen,", "tokens": ["Wo", "lie\u00b7be", "M\u00e4\u00b7del", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sag, ich bin ein rischer Mann,", "tokens": ["Und", "sag", ",", "ich", "bin", "ein", "ri\u00b7scher", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seinen Schimmel reiten kann", "tokens": ["Der", "sei\u00b7nen", "Schim\u00b7mel", "rei\u00b7ten", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und im Galoppe sausen.", "tokens": ["Und", "im", "Ga\u00b7lop\u00b7pe", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "F\u00fchr her zu mir an deiner Hand", "tokens": ["F\u00fchr", "her", "zu", "mir", "an", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APZR", "APPR", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieb mich will begleiten.", "tokens": ["Die", "lieb", "mich", "will", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schimmel scharrt schon in den Sand,", "tokens": ["Der", "Schim\u00b7mel", "scharrt", "schon", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00df in das gelobte Land", "tokens": ["Ich", "mu\u00df", "in", "das", "ge\u00b7lob\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit einer Holden reiten.", "tokens": ["Mit", "ei\u00b7ner", "Hol\u00b7den", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}