{"dta.poem.12916": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den von seiner Daphne ge-  \n liebten brunn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckseliges crystall, es ziert dich keine kunst,", "tokens": ["Gl\u00fcck\u00b7se\u00b7li\u00b7ges", "crys\u00b7tall", ",", "es", "ziert", "dich", "kei\u00b7ne", "kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und doch bestrahlet dich die sonne seltner gunst,", "tokens": ["Und", "doch", "be\u00b7strah\u00b7let", "dich", "die", "son\u00b7ne", "selt\u00b7ner", "gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil Daphne sich in dir den gantzen tag bespiegelt,", "tokens": ["Weil", "Daph\u00b7ne", "sich", "in", "dir", "den", "gant\u00b7zen", "tag", "be\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir aber weg und thor zu ihrer huld verriegelt.", "tokens": ["Mir", "a\u00b7ber", "weg", "und", "thor", "zu", "ih\u00b7rer", "huld", "ver\u00b7rie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein wasser ist so kalt, und hat gleichwol die krafft,", "tokens": ["Dein", "was\u00b7ser", "ist", "so", "kalt", ",", "und", "hat", "gleich\u00b7wol", "die", "krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "ADJD", "$,", "KON", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Daphne sich an dir, ich wei\u00df nicht wie, vergafft.", "tokens": ["Da\u00df", "Daph\u00b7ne", "sich", "an", "dir", ",", "ich", "wei\u00df", "nicht", "wie", ",", "ver\u00b7gafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hingegen steigt mein ach aus flammen-reichen lippen,", "tokens": ["Hin\u00b7ge\u00b7gen", "steigt", "mein", "ach", "aus", "flam\u00b7men\u00b7rei\u00b7chen", "lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Und findet dennoch nichts, als taube marmel-klippen.", "tokens": ["Und", "fin\u00b7det", "den\u00b7noch", "nichts", ",", "als", "tau\u00b7be", "mar\u00b7mel\u00b7klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn Daphne h\u00f6rt mich nicht, mein seuffzen wird verlacht,", "tokens": ["Denn", "Daph\u00b7ne", "h\u00f6rt", "mich", "nicht", ",", "mein", "seuff\u00b7zen", "wird", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr zartes ohre giebt blos auf dein lispeln acht,", "tokens": ["Ihr", "zar\u00b7tes", "oh\u00b7re", "giebt", "blos", "auf", "dein", "lis\u00b7peln", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und w\u00fcrd\u2019, ich wei\u00df es schon, dein ihm beliebtes rauschen", "tokens": ["Und", "w\u00fcrd'", ",", "ich", "wei\u00df", "es", "schon", ",", "dein", "ihm", "be\u00b7lieb\u00b7tes", "rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "PPER", "ADJA", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nicht um den lauten-klang des Ph\u00f6bus selbst vertauschen.", "tokens": ["Nicht", "um", "den", "lau\u00b7ten\u00b7klang", "des", "Ph\u00f6\u00b7bus", "selbst", "ver\u00b7tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vor ihren blicken hat mein hertze gute ruh,", "tokens": ["Vor", "ih\u00b7ren", "bli\u00b7cken", "hat", "mein", "hert\u00b7ze", "gu\u00b7te", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr holdes auge sieht nur deinen qvellen zu.", "tokens": ["Ihr", "hol\u00b7des", "au\u00b7ge", "sieht", "nur", "dei\u00b7nen", "qvel\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie spiegelt sich in dir, sie denckt nicht an narcissen,", "tokens": ["Sie", "spie\u00b7gelt", "sich", "in", "dir", ",", "sie", "denckt", "nicht", "an", "nar\u00b7cis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und da\u00df man andre nur, nicht aber sich kan k\u00fcssen.", "tokens": ["Und", "da\u00df", "man", "and\u00b7re", "nur", ",", "nicht", "a\u00b7ber", "sich", "kan", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PIS", "ADV", "$,", "PTKNEG", "ADV", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn sie die sonne sticht, so l\u00e4st sie deine flut", "tokens": ["Wenn", "sie", "die", "son\u00b7ne", "sticht", ",", "so", "l\u00e4st", "sie", "dei\u00b7ne", "flut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Um ihre glieder gehn, auf welchen milch und blut,", "tokens": ["Um", "ih\u00b7re", "glie\u00b7der", "gehn", ",", "auf", "wel\u00b7chen", "milch", "und", "blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$,", "APPR", "PWAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So wie in rosen spielt; ich aber mu\u00df verschmachten,", "tokens": ["So", "wie", "in", "ro\u00b7sen", "spielt", ";", "ich", "a\u00b7ber", "mu\u00df", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NE", "VVFIN", "$.", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und, leyder! gantz umsonst nach s\u00fc\u00dfer k\u00fchlung trachten.", "tokens": ["Und", ",", "ley\u00b7der", "!", "gantz", "um\u00b7sonst", "nach", "s\u00fc\u00b7\u00dfer", "k\u00fch\u00b7lung", "trach\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "$.", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allzubegl\u00fcckter brunn! sag\u2019, ob mein\u2019 eifersucht", "tokens": ["All\u00b7zu\u00b7be\u00b7gl\u00fcck\u00b7ter", "brunn", "!", "sag'", ",", "ob", "mein'", "ei\u00b7fer\u00b7sucht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nicht billig \u00fcber dir, als einem feinde, flucht?", "tokens": ["Nicht", "bil\u00b7lig", "\u00fc\u00b7ber", "dir", ",", "als", "ei\u00b7nem", "fein\u00b7de", ",", "flucht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn w\u00e4rest du nicht da, wer wei\u00df, ob nicht die bronnen,", "tokens": ["Denn", "w\u00e4\u00b7rest", "du", "nicht", "da", ",", "wer", "wei\u00df", ",", "ob", "nicht", "die", "bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PWS", "VVFIN", "$,", "KOUS", "PTKNEG", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So mein gesichte tr\u00e4gt, l\u00e4ngst Daphnens gunst gewonnen.", "tokens": ["So", "mein", "ge\u00b7sich\u00b7te", "tr\u00e4gt", ",", "l\u00e4ngst", "Daph\u00b7nens", "gunst", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "VVFIN", "$,", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein, mein eifer ist ein donner ohne krafft,", "tokens": ["Al\u00b7lein", ",", "mein", "ei\u00b7fer", "ist", "ein", "don\u00b7ner", "oh\u00b7ne", "krafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn wo der himmel mir nicht rath und h\u00fclffe schafft,", "tokens": ["Denn", "wo", "der", "him\u00b7mel", "mir", "nicht", "rath", "und", "h\u00fclf\u00b7fe", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "PTKNEG", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So wird mein hertze zwar ein brunnen der beschwerden,", "tokens": ["So", "wird", "mein", "hert\u00b7ze", "zwar", "ein", "brun\u00b7nen", "der", "be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch nicht ein aufenthalt der sch\u00f6nen Daphnen werden.", "tokens": ["Doch", "nicht", "ein", "auf\u00b7ent\u00b7halt", "der", "sch\u00f6\u00b7nen", "Daph\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}