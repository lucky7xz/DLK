{"dta.poem.10690": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Cvpido sodu bist nichts anders als ein Kindt/", "tokens": ["Cv\u00b7pi\u00b7do", "so\u00b7du", "bist", "nichts", "an\u00b7ders", "als", "ein", "Kindt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PIS", "ADV", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kompts da\u00df ich von dir so vilmahl sagen h\u00f6ren/", "tokens": ["Wie", "kompts", "da\u00df", "ich", "von", "dir", "so", "vil\u00b7mahl", "sa\u00b7gen", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df K\u00f6nig vnd gro\u00df Herrn bey Tag vnd Nacht dich ehren?", "tokens": ["Da\u00df", "K\u00f6\u00b7nig", "vnd", "gro\u00df", "Herrn", "bey", "Tag", "vnd", "Nacht", "dich", "eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "NN", "APPR", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie kompts da\u00df deine Handt so sehr viel vberwindt?", "tokens": ["Wie", "kompts", "da\u00df", "dei\u00b7ne", "Handt", "so", "sehr", "viel", "vber\u00b7windt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Cupido/ so du bist/ wie man dich mahlet/ blindt/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "so", "du", "bist", "/", "wie", "man", "dich", "mah\u00b7let", "/", "blindt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "ADV", "PPER", "VAFIN", "$(", "PWAV", "PIS", "PPER", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd die Poeten selbst/ dein eigne Diener/ lehren/", "tokens": ["Vnd", "die", "Po\u00b7et\u00b7en", "selbst", "/", "dein", "eig\u00b7ne", "Die\u00b7ner", "/", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$(", "PPOSAT", "ADJA", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie kanstu dann dein Reich so mechtiglich vermehren/", "tokens": ["Wie", "kans\u00b7tu", "dann", "dein", "Reich", "so", "mech\u00b7tig\u00b7lich", "ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie da\u00df dein Pfeil so just mir in mein Hertze findt?", "tokens": ["Wie", "da\u00df", "dein", "Pfeil", "so", "just", "mir", "in", "mein", "Hert\u00b7ze", "findt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein/ Nein du Hurensohn/ du kanst mit deinen Henden", "tokens": ["Nein", "/", "Nein", "du", "Hu\u00b7ren\u00b7sohn", "/", "du", "kanst", "mit", "dei\u00b7nen", "Hen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "PTKANT", "PPER", "NN", "$(", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Menschen st\u00e4rck vnd Krafft/ wie dirs geliebet/ wenden/", "tokens": ["Der", "Men\u00b7schen", "st\u00e4rck", "vnd", "Krafft", "/", "wie", "dirs", "ge\u00b7lie\u00b7bet", "/", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$(", "PWAV", "PIS", "VVPP", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nein/ nein du bist ein Gott dem alles ist bekandt.", "tokens": ["Nein", "/", "nein", "du", "bist", "ein", "Gott", "dem", "al\u00b7les", "ist", "be\u00b7kandt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "PPER", "VAFIN", "ART", "NN", "ART", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ey triff nur auch das Hertz der liebesten Freundinne/", "tokens": ["Ey", "triff", "nur", "auch", "das", "Hertz", "der", "lie\u00b7bes\u00b7ten", "Freun\u00b7din\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Da\u00df Sie mich/ wie ich sie/ auch wider lieb gewinne/", "tokens": ["Da\u00df", "Sie", "mich", "/", "wie", "ich", "sie", "/", "auch", "wi\u00b7der", "lieb", "ge\u00b7win\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$(", "PWAV", "PPER", "PPER", "$(", "ADV", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So sag ich/ du schie\u00dft recht mit Vrtheil vnd Verstandt.", "tokens": ["So", "sag", "ich", "/", "du", "schie\u00dft", "recht", "mit", "Vrtheil", "vnd", "Ver\u00b7standt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}