{"dta.poem.10211": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Der alte General a. D. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.85", "en:0.14"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Nun mu\u00df ich oft in\u2019s Thal hinunterlauschen", "tokens": ["Nun", "mu\u00df", "ich", "oft", "in's", "Thal", "hin\u00b7un\u00b7ter\u00b7lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom kahlen Berge der Verlassenheit.", "tokens": ["Vom", "kah\u00b7len", "Ber\u00b7ge", "der", "Ver\u00b7las\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es dringt zu mir herauf ein Singen, Rauschen,", "tokens": ["Es", "dringt", "zu", "mir", "her\u00b7auf", "ein", "Sin\u00b7gen", ",", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Musik und Trommeln bringen nun mir Leid.", "tokens": ["Mu\u00b7sik", "und", "Trom\u00b7meln", "brin\u00b7gen", "nun", "mir", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Rosse wiehern und die Fahnen bauschen,", "tokens": ["Die", "Ros\u00b7se", "wie\u00b7hern", "und", "die", "Fah\u00b7nen", "bau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Kanonendonner, matt und nebelweit.", "tokens": ["Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", ",", "matt", "und", "ne\u00b7bel\u00b7weit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O, jene Zeiten, k\u00f6nnte ich sie tauschen,", "tokens": ["O", ",", "je\u00b7ne", "Zei\u00b7ten", ",", "k\u00f6nn\u00b7te", "ich", "sie", "tau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDAT", "NN", "$,", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das alte Herz, die alte Fr\u00f6hlichkeit.", "tokens": ["Das", "al\u00b7te", "Herz", ",", "die", "al\u00b7te", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}