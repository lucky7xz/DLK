{"textgrid.poem.43468": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vereine von allen Sorten,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vereine von allen Sorten,", "tokens": ["Ver\u00b7ei\u00b7ne", "von", "al\u00b7len", "Sor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vereine kreuz und quer,", "tokens": ["Ver\u00b7ei\u00b7ne", "kreuz", "und", "quer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vereine an allen Orten", "tokens": ["Ver\u00b7ei\u00b7ne", "an", "al\u00b7len", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und immer mehr und mehr.", "tokens": ["Und", "im\u00b7mer", "mehr", "und", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum steht nicht mehr so alleine", "tokens": ["Drum", "steht", "nicht", "mehr", "so", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "ADV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und schlie\u00dft euch redlich an!", "tokens": ["Und", "schlie\u00dft", "euch", "red\u00b7lich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu irgend einem Vereine", "tokens": ["Zu", "ir\u00b7gend", "ei\u00b7nem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Geh\u00f6r' ein deutscher Mann!", "tokens": ["Ge\u00b7h\u00f6r'", "ein", "deut\u00b7scher", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vereine f\u00fcr Fasanen,", "tokens": ["Ver\u00b7ei\u00b7ne", "f\u00fcr", "Fa\u00b7sa\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Eisen, Zink und Galmei,", "tokens": ["F\u00fcr", "Ei\u00b7sen", ",", "Zink", "und", "Gal\u00b7mei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dampfkessel und Eisenbahnen,", "tokens": ["Dampf\u00b7kes\u00b7sel", "und", "Ei\u00b7sen\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Faschingsnarrethei;", "tokens": ["Und", "Fa\u00b7schings\u00b7nar\u00b7re\u00b7thei", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Urbarmachen der Heide,", "tokens": ["F\u00fcr", "Ur\u00b7bar\u00b7ma\u00b7chen", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "F\u00fcr Lerchen- und H\u00e4ringsfang,", "tokens": ["F\u00fcr", "Ler\u00b7chen", "und", "H\u00e4\u00b7rings\u00b7fang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "F\u00fcr R\u00fcbenzucker und Seide,", "tokens": ["F\u00fcr", "R\u00fc\u00b7ben\u00b7zu\u00b7cker", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "F\u00fcr Turnen, Tanz und Gesang.", "tokens": ["F\u00fcr", "Tur\u00b7nen", ",", "Tanz", "und", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Vereine f\u00fcr Judenbekehrung", "tokens": ["Ver\u00b7ei\u00b7ne", "f\u00fcr", "Ju\u00b7den\u00b7be\u00b7keh\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Emancipation,", "tokens": ["Und", "E\u00b7man\u00b7ci\u00b7pa\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcr fromme Volksbelehrung", "tokens": ["F\u00fcr", "from\u00b7me", "Volks\u00b7be\u00b7leh\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Adelsrestauration,", "tokens": ["Und", "A\u00b7dels\u00b7re\u00b7stau\u00b7ra\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "F\u00fcr Besserung der Verbrecher", "tokens": ["F\u00fcr", "Bes\u00b7se\u00b7rung", "der", "Ver\u00b7bre\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Schafveredelung,", "tokens": ["Und", "Schaf\u00b7ve\u00b7re\u00b7de\u00b7lung", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr M\u00e4\u00dfigkeit der Zecher", "tokens": ["F\u00fcr", "M\u00e4\u00b7\u00dfig\u00b7keit", "der", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Lebensversicherung.", "tokens": ["Und", "Le\u00b7bens\u00b7ver\u00b7si\u00b7che\u00b7rung", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Drum steht nicht mehr so alleine", "tokens": ["Drum", "steht", "nicht", "mehr", "so", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "ADV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und schlie\u00dft euch redlich an!", "tokens": ["Und", "schlie\u00dft", "euch", "red\u00b7lich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu irgend einem Vereine", "tokens": ["Zu", "ir\u00b7gend", "ei\u00b7nem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geh\u00f6r' ein deutscher Mann!", "tokens": ["Ge\u00b7h\u00f6r'", "ein", "deut\u00b7scher", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es g\u00f6nnen unsre Machthaber", "tokens": ["Es", "g\u00f6n\u00b7nen", "uns\u00b7re", "Macht\u00b7ha\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Uns solchen und manchen Verein,", "tokens": ["Uns", "sol\u00b7chen", "und", "man\u00b7chen", "Ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KON", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "F\u00fcr Deutschlands Einheit aber \u2013", "tokens": ["F\u00fcr", "Deutschlands", "Ein\u00b7heit", "a\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Das fall' euch nur nicht ein!", "tokens": ["Das", "fall'", "euch", "nur", "nicht", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}