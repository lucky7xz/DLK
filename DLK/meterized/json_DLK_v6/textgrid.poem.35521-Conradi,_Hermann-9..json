{"textgrid.poem.35521": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6stliche Stille der Einsamkeit!", "tokens": ["O", "k\u00f6st\u00b7li\u00b7che", "Stil\u00b7le", "der", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es schweigen N\u00e4he und Weite ...", "tokens": ["Es", "schwei\u00b7gen", "N\u00e4\u00b7he", "und", "Wei\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch in mir wogt es und braust es wie Sturm \u2013", "tokens": ["Doch", "in", "mir", "wogt", "es", "und", "braust", "es", "wie", "Sturm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "KOKOM", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Klingt es wie Glockengel\u00e4ute! ...", "tokens": ["Klingt", "es", "wie", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4u\u00b7te", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcckauf! Die gro\u00dfe, die herrliche Zeit", "tokens": ["Gl\u00fcc\u00b7kauf", "!", "Die", "gro\u00b7\u00dfe", ",", "die", "herr\u00b7li\u00b7che", "Zeit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Str\u00f6mender Fr\u00fchlingsgef\u00fchle:", "tokens": ["Str\u00f6\u00b7men\u00b7der", "Fr\u00fch\u00b7lings\u00b7ge\u00b7f\u00fch\u00b7le", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wieder bricht sie mit Macht herein \u2013", "tokens": ["Wie\u00b7der", "bricht", "sie", "mit", "Macht", "her\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "L\u00e4dt mich zum Waffenspiele ...", "tokens": ["L\u00e4dt", "mich", "zum", "Waf\u00b7fen\u00b7spie\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Zelter schn\u00fcr' ich \u2013 ich sch\u00e4rfe mein Schwert:", "tokens": ["Den", "Zel\u00b7ter", "schn\u00fcr'", "ich", "\u2013", "ich", "sch\u00e4r\u00b7fe", "mein", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch sp\u00fcr' ich Jugendgemutung!", "tokens": ["Noch", "sp\u00fcr'", "ich", "Ju\u00b7gend\u00b7ge\u00b7mu\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Winterklage sei abgetan \u2013", "tokens": ["Die", "Win\u00b7ter\u00b7kla\u00b7ge", "sei", "ab\u00b7ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sehnsucht nach stiller Verblutung!", "tokens": ["Die", "Sehn\u00b7sucht", "nach", "stil\u00b7ler", "Ver\u00b7blu\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Nicht sterben will ich im D\u00e4mmerasyl,", "tokens": ["Nicht", "ster\u00b7ben", "will", "ich", "im", "D\u00e4m\u00b7mer\u00b7a\u00b7syl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umkreuzt von Nebelphantasmen \u2013", "tokens": ["Um\u00b7kreuzt", "von", "Ne\u00b7bel\u00b7phan\u00b7tas\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Nicht sterben will ich verwelkt und zerm\u00fcrbt,", "tokens": ["Nicht", "ster\u00b7ben", "will", "ich", "ver\u00b7welkt", "und", "zer\u00b7m\u00fcrbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umd\u00fcnstet von Fiebermiasmen!", "tokens": ["Um\u00b7d\u00fcns\u00b7tet", "von", "Fie\u00b7ber\u00b7mi\u00b7as\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Wo aus feuchter Scholle des Fr\u00fchlings Blut", "tokens": ["Wo", "aus", "feuch\u00b7ter", "Schol\u00b7le", "des", "Fr\u00fch\u00b7lings", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Treibt lichtgr\u00fcne Ranken,", "tokens": ["Treibt", "licht\u00b7gr\u00fc\u00b7ne", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will ich mich betten und atmen tief \u2013", "tokens": ["Will", "ich", "mich", "bet\u00b7ten", "und", "at\u00b7men", "tief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "KON", "VVFIN", "ADJD", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Atmen des Fr\u00fchlings Gedanken! ...", "tokens": ["At\u00b7men", "des", "Fr\u00fch\u00b7lings", "Ge\u00b7dan\u00b7ken", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Will lauschen der Wiesenwasser Gesang \u2013", "tokens": ["Will", "lau\u00b7schen", "der", "Wie\u00b7sen\u00b7was\u00b7ser", "Ge\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Im Auge Flammen, den Muskel gestrafft \u2013", "tokens": ["Im", "Au\u00b7ge", "Flam\u00b7men", ",", "den", "Mus\u00b7kel", "ge\u00b7strafft", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Wei\u00dft du, versch\u00fcchterte Kreatur,", "tokens": ["Wei\u00dft", "du", ",", "ver\u00b7sch\u00fcch\u00b7ter\u00b7te", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Was Leben hei\u00dft und bedeutet?", "tokens": ["Was", "Le\u00b7ben", "hei\u00dft", "und", "be\u00b7deu\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In den bl\u00fchenden Fr\u00fchling tritt hinaus,", "tokens": ["In", "den", "bl\u00fc\u00b7hen\u00b7den", "Fr\u00fch\u00b7ling", "tritt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo die Welt dem Auge sich weitet! ...", "tokens": ["Wo", "die", "Welt", "dem", "Au\u00b7ge", "sich", "wei\u00b7tet", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da wird dir so gro\u00df, so siegreich ums Herz \u2013", "tokens": ["Da", "wird", "dir", "so", "gro\u00df", ",", "so", "sieg\u00b7reich", "ums", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPRART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da f\u00fchlst du ein k\u00f6stlich Erbeben \u2013", "tokens": ["Da", "f\u00fchlst", "du", "ein", "k\u00f6st\u00b7lich", "Er\u00b7be\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Hauch von der Gr\u00f6\u00dfe der Schw\u00e4rmerzeit \u2013", "tokens": ["Ein", "Hauch", "von", "der", "Gr\u00f6\u00b7\u00dfe", "der", "Schw\u00e4r\u00b7mer\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein einziges Schwellen und Leben! ...", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", "Schwel\u00b7len", "und", "Le\u00b7ben", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Dann sprengst du die Bande! Dann reckst du dich weit!", "tokens": ["Dann", "sprengst", "du", "die", "Ban\u00b7de", "!", "Dann", "reckst", "du", "dich", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann f\u00fchlst du es wogen und g\u00e4ren!", "tokens": ["Dann", "f\u00fchlst", "du", "es", "wo\u00b7gen", "und", "g\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann f\u00fchlst du, wie sich in wildem Drang", "tokens": ["Dann", "f\u00fchlst", "du", ",", "wie", "sich", "in", "wil\u00b7dem", "Drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine ", "tokens": ["Ei\u00b7ne"], "token_info": ["word"], "pos": ["ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Und jauchzend schreist du dein Dankgebet,", "tokens": ["Und", "jauch\u00b7zend", "schreist", "du", "dein", "Dank\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "All-eins mit den Weltengewalten:", "tokens": ["All\u00b7eins", "mit", "den", "Wel\u00b7ten\u00b7ge\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "F\u00fchlst du dich selig, f\u00fchlst du dich stark \u2013", "tokens": ["F\u00fchlst", "du", "dich", "se\u00b7lig", ",", "f\u00fchlst", "du", "dich", "stark", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sp\u00fcrst du die Kraft zum Gestalten! ...", "tokens": ["Sp\u00fcrst", "du", "die", "Kraft", "zum", "Ge\u00b7stal\u00b7ten", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Zur\u00fcck, ihr Schemen der Alltagswelt!", "tokens": ["Zu\u00b7r\u00fcck", ",", "ihr", "Sche\u00b7men", "der", "All\u00b7tags\u00b7welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zerflie\u00dft vor dem Fr\u00fchlingswunder! ...", "tokens": ["Zer\u00b7flie\u00dft", "vor", "dem", "Fr\u00fch\u00b7lings\u00b7wun\u00b7der", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was ich geschaut, ist Unsterblichkeit \u2013", "tokens": ["Was", "ich", "ge\u00b7schaut", ",", "ist", "U\u00b7nsterb\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihr aber seid nichtiger Plunder! ...", "tokens": ["Ihr", "a\u00b7ber", "seid", "nich\u00b7ti\u00b7ger", "Plun\u00b7der", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "O wonnige Stille der Einsamkeit \u2013", "tokens": ["O", "won\u00b7ni\u00b7ge", "Stil\u00b7le", "der", "Ein\u00b7sam\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es schweigen N\u00e4he und Weite ...", "tokens": ["Es", "schwei\u00b7gen", "N\u00e4\u00b7he", "und", "Wei\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch in mir \u2013 in mir klingt es wie \u00bbSieg!\u00ab \u2013", "tokens": ["Doch", "in", "mir", "\u2013", "in", "mir", "klingt", "es", "wie", "\u00bb", "Sieg", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "$(", "APPR", "PPER", "VVFIN", "PPER", "KOKOM", "$(", "NN", "$.", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "T\u00f6nt es wie ", "tokens": ["T\u00f6nt", "es", "wie"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}}}}}