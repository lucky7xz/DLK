{"textgrid.poem.40545": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eisblumen, starr, kristallen an den Scheiben,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eisblumen, starr, kristallen an den Scheiben,", "tokens": ["Eis\u00b7blu\u00b7men", ",", "starr", ",", "kris\u00b7tal\u00b7len", "an", "den", "Schei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein Gehege vor der Sturmnacht Tosen,", "tokens": ["Wie", "ein", "Ge\u00b7he\u00b7ge", "vor", "der", "Sturm\u00b7nacht", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie fl\u00fcstern mir, inde\u00df sie Flimmer st\u00e4uben:", "tokens": ["Sie", "fl\u00fcs\u00b7tern", "mir", ",", "in\u00b7de\u00df", "sie", "Flim\u00b7mer", "st\u00e4u\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir sind die Geister sch\u00f6ner Fr\u00fchlingsrosen!", "tokens": ["Wir", "sind", "die", "Geis\u00b7ter", "sch\u00f6\u00b7ner", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schneeflocken wirbeln hin mit wei\u00dfem Glanze!", "tokens": ["Schnee\u00b7flo\u00b7cken", "wir\u00b7beln", "hin", "mit", "wei\u00b7\u00dfem", "Glan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Es pochen leis' ans Fenster die verspr\u00fchten,", "tokens": ["Es", "po\u00b7chen", "leis'", "ans", "Fens\u00b7ter", "die", "ver\u00b7spr\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir lispelnd fl\u00fcchtig im Vor\u00fcbertanze:", "tokens": ["Mir", "lis\u00b7pelnd", "fl\u00fcch\u00b7tig", "im", "Vor\u00b7\u00fc\u00b7bert\u00b7an\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir sind die Geister duft'ger Fr\u00fchlingsbl\u00fcthen!", "tokens": ["Wir", "sind", "die", "Geis\u00b7ter", "duft'\u00b7ger", "Fr\u00fch\u00b7lings\u00b7bl\u00fc\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gef\u00fchle steigen auf in meiner Seele,", "tokens": ["Ge\u00b7f\u00fch\u00b7le", "stei\u00b7gen", "auf", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie beim Verklingen ferner Sterbeglocken,", "tokens": ["Wie", "beim", "Ver\u00b7klin\u00b7gen", "fer\u00b7ner", "Ster\u00b7be\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die bange Wehmuthseufzer meiner Kehle", "tokens": ["Die", "ban\u00b7ge", "Weh\u00b7mut\u00b7hseuf\u00b7zer", "mei\u00b7ner", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und reiche Thr\u00e4nen meinem Aug' entlocken;", "tokens": ["Und", "rei\u00b7che", "Thr\u00e4\u00b7nen", "mei\u00b7nem", "Aug'", "ent\u00b7lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie aber singen sanft mir ins Gem\u00fcthe:", "tokens": ["Sie", "a\u00b7ber", "sin\u00b7gen", "sanft", "mir", "ins", "Ge\u00b7m\u00fc\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sind die sel'gen Geister deiner Lieben,", "tokens": ["Wir", "sind", "die", "sel'\u00b7gen", "Geis\u00b7ter", "dei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit denen du durchwallt des Fr\u00fchlings Bl\u00fcthe,", "tokens": ["Mit", "de\u00b7nen", "du", "durch\u00b7wallt", "des", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf deren Grab nun diese Flocken stieben!", "tokens": ["Auf", "de\u00b7ren", "Grab", "nun", "die\u00b7se", "Flo\u00b7cken", "stie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}