{"textgrid.poem.55325": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Entoptische Farben", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dir von den Spiegeleien", "tokens": ["La\u00df", "dir", "von", "den", "Spie\u00b7ge\u00b7lei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Physiker erz\u00e4hlen,", "tokens": ["Uns\u00b7rer", "Phy\u00b7si\u00b7ker", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die am Ph\u00e4nomen sich freuen,", "tokens": ["Die", "am", "Ph\u00e4\u00b7no\u00b7men", "sich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mehr sich mit Gedanken qu\u00e4len.", "tokens": ["Mehr", "sich", "mit", "Ge\u00b7dan\u00b7ken", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spiegel h\u00fcben, Spiegel dr\u00fcben,", "tokens": ["Spie\u00b7gel", "h\u00fc\u00b7ben", ",", "Spie\u00b7gel", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doppelstellung, auserlesen;", "tokens": ["Dop\u00b7pel\u00b7stel\u00b7lung", ",", "au\u00b7ser\u00b7le\u00b7sen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dazwischen ruht im Tr\u00fcben", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "ruht", "im", "Tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Als Kristall das Erdewesen.", "tokens": ["Als", "Kris\u00b7tall", "das", "Er\u00b7de\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dieses zeigt, wenn jene blicken,", "tokens": ["Die\u00b7ses", "zeigt", ",", "wenn", "je\u00b7ne", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allersch\u00f6nste Farbenspiele;", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Far\u00b7ben\u00b7spie\u00b7le", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00e4mmerlicht, das beide schicken,", "tokens": ["D\u00e4m\u00b7mer\u00b7licht", ",", "das", "bei\u00b7de", "schi\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Offenbart sich dem Gef\u00fchle.", "tokens": ["Of\u00b7fen\u00b7bart", "sich", "dem", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwarz wie Kreuze wirst du sehen,", "tokens": ["Schwarz", "wie", "Kreu\u00b7ze", "wirst", "du", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pfauenaugen kann man finden;", "tokens": ["Pfau\u00b7en\u00b7au\u00b7gen", "kann", "man", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tag und Abendlicht vergehen,", "tokens": ["Tag", "und", "A\u00b7bend\u00b7licht", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zusammen beide schwinden.", "tokens": ["Bis", "zu\u00b7sam\u00b7men", "bei\u00b7de", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der Name wird ein Zeichen,", "tokens": ["Und", "der", "Na\u00b7me", "wird", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief ist der Kristall durchdrungen:", "tokens": ["Tief", "ist", "der", "Kris\u00b7tall", "durch\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Aug in Auge sieht dergleichen", "tokens": ["Aug", "in", "Au\u00b7ge", "sieht", "derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wundersame Spiegelungen.", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "Spie\u00b7ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df den Makrokosmus gelten,", "tokens": ["La\u00df", "den", "Mak\u00b7ro\u00b7kos\u00b7mus", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine spenstischen Gestalten!", "tokens": ["Sei\u00b7ne", "spens\u00b7ti\u00b7schen", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die lieben kleinen Welten", "tokens": ["Da", "die", "lie\u00b7ben", "klei\u00b7nen", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirklich Herrlichstes enthalten.", "tokens": ["Wirk\u00b7lich", "Herr\u00b7lichs\u00b7tes", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}