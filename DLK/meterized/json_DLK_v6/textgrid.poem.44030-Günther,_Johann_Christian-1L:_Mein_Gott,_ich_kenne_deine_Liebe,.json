{"textgrid.poem.44030": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gott, ich kenne deine Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott, ich kenne deine Liebe,", "tokens": ["Mein", "Gott", ",", "ich", "ken\u00b7ne", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du ziehst mich, doch ich folge nicht.", "tokens": ["Du", "ziehst", "mich", ",", "doch", "ich", "fol\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach hilf doch nur dem schwachen Triebe,", "tokens": ["Ach", "hilf", "doch", "nur", "dem", "schwa\u00b7chen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mir nur stets zuwider spricht,", "tokens": ["Der", "mir", "nur", "stets", "zu\u00b7wi\u00b7der", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mehre das getreue Sehnen,", "tokens": ["Und", "meh\u00b7re", "das", "ge\u00b7treu\u00b7e", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das dir mit Thr\u00e4nen", "tokens": ["Das", "dir", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Gewis dein Vaterherze bricht.", "tokens": ["Ge\u00b7wis", "dein", "Va\u00b7ter\u00b7her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin verdammt, ich bin ein S\u00fcnder;", "tokens": ["Ich", "bin", "ver\u00b7dammt", ",", "ich", "bin", "ein", "S\u00fcn\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies weis ich, doch erbarme dich!", "tokens": ["Dies", "weis", "ich", ",", "doch", "er\u00b7bar\u00b7me", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "PPER", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du rufest die verlaufnen Kinder,", "tokens": ["Du", "ru\u00b7fest", "die", "ver\u00b7lauf\u00b7nen", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du h\u00f6rest, komm und rette mich!", "tokens": ["Du", "h\u00f6\u00b7rest", ",", "komm", "und", "ret\u00b7te", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach rette die bedr\u00e4ngte Seele;", "tokens": ["Ach", "ret\u00b7te", "die", "be\u00b7dr\u00e4ng\u00b7te", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Marterh\u00f6hle", "tokens": ["Die", "Mar\u00b7ter\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mehrt Creuz und Jammer innerlich.", "tokens": ["Mehrt", "Creuz", "und", "Jam\u00b7mer", "in\u00b7ner\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist ein Arzt und hilfst dem Krancken:", "tokens": ["Du", "bist", "ein", "Arzt", "und", "hilfst", "dem", "Kran\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Aussaz greift schon ziemlich weit.", "tokens": ["Mein", "Aus\u00b7saz", "greift", "schon", "ziem\u00b7lich", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Glaube st\u00e4rcke die Gedancken,", "tokens": ["Der", "Glau\u00b7be", "st\u00e4r\u00b7cke", "die", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worein der Satan Zweifel streut.", "tokens": ["Wo\u00b7rein", "der", "Sa\u00b7tan", "Zwei\u00b7fel", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch will ich hier mit Glauben schweigen,", "tokens": ["Doch", "will", "ich", "hier", "mit", "Glau\u00b7ben", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und du wirst zeigen:", "tokens": ["Und", "du", "wirst", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Es w\u00fcrckt die Reu zur Seeligkeit.", "tokens": ["Es", "w\u00fcrckt", "die", "Reu", "zur", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Glaube w\u00fcrcket durch die Wercke,", "tokens": ["Der", "Glau\u00b7be", "w\u00fcr\u00b7cket", "durch", "die", "Wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein die Wercke helfen nichts:", "tokens": ["Al\u00b7lein", "die", "Wer\u00b7cke", "hel\u00b7fen", "nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Glaube kommt von deiner St\u00e4rcke,", "tokens": ["Mein", "Glau\u00b7be", "kommt", "von", "dei\u00b7ner", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist der Vater alles Lichts,", "tokens": ["Du", "bist", "der", "Va\u00b7ter", "al\u00b7les", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst mich auch bey finstern Zeiten", "tokens": ["Du", "wirst", "mich", "auch", "bey", "fins\u00b7tern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Tugend leiten;", "tokens": ["Zur", "Tu\u00b7gend", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Sprich nur ein Wort, ach, so geschichts.", "tokens": ["Sprich", "nur", "ein", "Wort", ",", "ach", ",", "so", "ge\u00b7schichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "ITJ", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, so geschichts, da\u00df ich erkenne,", "tokens": ["Ach", ",", "so", "ge\u00b7schichts", ",", "da\u00df", "ich", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was dein Gesez und Wort befiehlt;", "tokens": ["Was", "dein", "Ge\u00b7sez", "und", "Wort", "be\u00b7fiehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ob ich gleich vor Liebe brenne,", "tokens": ["Denn", "ob", "ich", "gleich", "vor", "Lie\u00b7be", "bren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist doch alles blind gezielt,", "tokens": ["So", "ist", "doch", "al\u00b7les", "blind", "ge\u00b7zielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nicht mein eu\u00dferstes Verlangen,", "tokens": ["Wenn", "nicht", "mein", "eu\u00b7\u00dfers\u00b7tes", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Dich zu umfangen,", "tokens": ["Dich", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Schon auf der Welt den Himmel f\u00fchlt.", "tokens": ["Schon", "auf", "der", "Welt", "den", "Him\u00b7mel", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich sterbe so, wie alle m\u00fc\u00dfen;", "tokens": ["Ich", "ster\u00b7be", "so", ",", "wie", "al\u00b7le", "m\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist der Tod der S\u00fcnden Sold.", "tokens": ["Es", "ist", "der", "Tod", "der", "S\u00fcn\u00b7den", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verklagt mich endlich mein Gewi\u00dfen,", "tokens": ["Ver\u00b7klagt", "mich", "end\u00b7lich", "mein", "Ge\u00b7wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Genug, mein Heiland ist mir hold.", "tokens": ["Ge\u00b7nug", ",", "mein", "Hei\u00b7land", "ist", "mir", "hold", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich w\u00e4lze mich in seinem Blute", "tokens": ["Ich", "w\u00e4l\u00b7ze", "mich", "in", "sei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit freyem Muthe;", "tokens": ["Mit", "frey\u00b7em", "Mu\u00b7the", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Schimpft, Welt und Satan, wie ihr wollt.", "tokens": ["Schimpft", ",", "Welt", "und", "Sa\u00b7tan", ",", "wie", "ihr", "wollt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es kommt ein Tag, der alles richtet;", "tokens": ["Es", "kommt", "ein", "Tag", ",", "der", "al\u00b7les", "rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Heiland richtet, klagt mich an!", "tokens": ["Mein", "Hei\u00b7land", "rich\u00b7tet", ",", "klagt", "mich", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Feind, so mich und alle sichtet,", "tokens": ["Der", "Feind", ",", "so", "mich", "und", "al\u00b7le", "sich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat doch mit allen nichts gethan,", "tokens": ["Hat", "doch", "mit", "al\u00b7len", "nichts", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit allen, welchen seine Liebe", "tokens": ["Mit", "al\u00b7len", ",", "wel\u00b7chen", "sei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "$,", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch heilge Triebe", "tokens": ["Durch", "heil\u00b7ge", "Trie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Eitelkeit verbittern kan.", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "ver\u00b7bit\u00b7tern", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich weis nicht, wenn und wo ich sterbe,", "tokens": ["Ich", "weis", "nicht", ",", "wenn", "und", "wo", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKNEG", "$,", "KOUS", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sterb ich allemahl vergn\u00fcgt,", "tokens": ["Doch", "sterb", "ich", "al\u00b7le\u00b7mahl", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sofern ich nur den Trost erwerbe,", "tokens": ["So\u00b7fern", "ich", "nur", "den", "Trost", "er\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dir mein Herz im Schoo\u00dfe liegt.", "tokens": ["Da\u00df", "dir", "mein", "Herz", "im", "Schoo\u00b7\u00dfe", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erdr\u00fccke mich in deinen Armen,", "tokens": ["Er\u00b7dr\u00fc\u00b7cke", "mich", "in", "dei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn dein Erbarmen", "tokens": ["Denn", "dein", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Macht, da\u00df mein Herz die Noth besiegt.", "tokens": ["Macht", ",", "da\u00df", "mein", "Herz", "die", "Noth", "be\u00b7siegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}