{"textgrid.poem.26155": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im gl\u00e4sernen Treibhaus stehn gro\u00df allein", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im gl\u00e4sernen Treibhaus stehn gro\u00df allein", "tokens": ["Im", "gl\u00e4\u00b7ser\u00b7nen", "Treib\u00b7haus", "stehn", "gro\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "ADV"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ein paar gipserne G\u00f6ttergestalten.", "tokens": ["Ein", "paar", "gip\u00b7ser\u00b7ne", "G\u00f6t\u00b7ter\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sie sehn in die sonnige Leere hinein,", "tokens": ["Sie", "sehn", "in", "die", "son\u00b7ni\u00b7ge", "Lee\u00b7re", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihre K\u00f6rper sind wei\u00df ohne Falten.", "tokens": ["Ih\u00b7re", "K\u00f6r\u00b7per", "sind", "wei\u00df", "oh\u00b7ne", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Die Palmen, die sonst in dem Winter hier hausen,", "tokens": ["Die", "Pal\u00b7men", ",", "die", "sonst", "in", "dem", "Win\u00b7ter", "hier", "hau\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die zischen wohlig im Parkgr\u00fcn drau\u00dfen.", "tokens": ["Die", "zi\u00b7schen", "woh\u00b7lig", "im", "Park\u00b7gr\u00fcn", "drau\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und totenstill von ihren steinernen Tischen,", "tokens": ["Und", "to\u00b7tens\u00b7till", "von", "ih\u00b7ren", "stei\u00b7ner\u00b7nen", "Ti\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wie vor Jahrtausend mit blendendem Leibe,", "tokens": ["Wie", "vor", "Jahr\u00b7tau\u00b7send", "mit", "blen\u00b7den\u00b7dem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Sehn die G\u00f6tter verliebt in die Sonnenscheibe.", "tokens": ["Sehn", "die", "G\u00f6t\u00b7ter", "ver\u00b7liebt", "in", "die", "Son\u00b7nen\u00b7schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}