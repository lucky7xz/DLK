{"textgrid.poem.30856": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werthes Paar, die Gott und Tugend selbst zusammen hat gepaart,", "tokens": ["Wert\u00b7hes", "Paar", ",", "die", "Gott", "und", "Tu\u00b7gend", "selbst", "zu\u00b7sam\u00b7men", "hat", "ge\u00b7paart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nemet hin durch lauter Segen zu der Ewigkeit die Fahrt;", "tokens": ["Ne\u00b7met", "hin", "durch", "lau\u00b7ter", "Se\u00b7gen", "zu", "der", "E\u00b7wig\u00b7keit", "die", "Fahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Lasset aber eure Tugend einem und dem andren hier,", "tokens": ["Las\u00b7set", "a\u00b7ber", "eu\u00b7re", "Tu\u00b7gend", "ei\u00b7nem", "und", "dem", "an\u00b7dren", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "KON", "ART", "ADJA", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das euch gleiche sey gestaltet und gesinnet so wie ihr!", "tokens": ["Das", "euch", "glei\u00b7che", "sey", "ge\u00b7stal\u00b7tet", "und", "ge\u00b7sin\u00b7net", "so", "wie", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJA", "VAFIN", "VVPP", "KON", "VVFIN", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+-+-+-+-+-++-", "measure": "unknown.measure.octa.plus"}}}}}