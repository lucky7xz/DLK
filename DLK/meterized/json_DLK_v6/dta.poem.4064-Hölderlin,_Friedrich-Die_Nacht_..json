{"dta.poem.4064": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Rings um ruhet die Stadt, still wird die erleuchtete", "tokens": ["Rings", "um", "ru\u00b7het", "die", "Stadt", ",", "still", "wird", "die", "er\u00b7leuch\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "VVFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Gasse,", "tokens": ["Gas\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und mit Fackeln geschm\u00fcckt rauschen die Wagen", "tokens": ["Und", "mit", "Fa\u00b7ckeln", "ge\u00b7schm\u00fcckt", "rau\u00b7schen", "die", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "hinweg.", "tokens": ["hin\u00b7weg", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Satt gehen heim, von Freuden des Tags zu ruhen,", "tokens": ["Satt", "ge\u00b7hen", "heim", ",", "von", "Freu\u00b7den", "des", "Tags", "zu", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "die Menschen,", "tokens": ["die", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und Gewinn und Verlust w\u00e4get ein sinniges Haupt", "tokens": ["Und", "Ge\u00b7winn", "und", "Ver\u00b7lust", "w\u00e4\u00b7get", "ein", "sin\u00b7ni\u00b7ges", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wohl zufrieden zu Haus; leer steht von Trauben", "tokens": ["Wohl", "zu\u00b7frie\u00b7den", "zu", "Haus", ";", "leer", "steht", "von", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "$.", "ADJD", "VVFIN", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "und Blumen,", "tokens": ["und", "Blu\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und von Werken der Hand ruht der gesch\u00e4ftige", "tokens": ["Und", "von", "Wer\u00b7ken", "der", "Hand", "ruht", "der", "ge\u00b7sch\u00e4f\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Markt.", "tokens": ["Markt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Aber das Saitenspiel t\u00f6nt fern aus G\u00e4rten; viel-", "tokens": ["A\u00b7ber", "das", "Sai\u00b7ten\u00b7spiel", "t\u00f6nt", "fern", "aus", "G\u00e4r\u00b7ten", ";", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$.", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "leicht, da\u00df", "tokens": ["leicht", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Dort ein Liebender spielt, oder ein einsamer Mann", "tokens": ["Dort", "ein", "Lie\u00b7ben\u00b7der", "spielt", ",", "o\u00b7der", "ein", "ein\u00b7sa\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Ferner Freunde gedenkt und der Jugendzeit; und", "tokens": ["Fer\u00b7ner", "Freun\u00b7de", "ge\u00b7denkt", "und", "der", "Ju\u00b7gend\u00b7zeit", ";", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVPP", "KON", "ART", "NN", "$.", "KON"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "die Brunnen,", "tokens": ["die", "Brun\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Immerquillend und frisch, rauschen an duftendem", "tokens": ["Im\u00b7mer\u00b7quil\u00b7lend", "und", "frisch", ",", "rau\u00b7schen", "an", "duf\u00b7ten\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "VVFIN", "APPR", "ADJA"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.18": {"text": "Beet.", "tokens": ["Beet", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Still in d\u00e4mmriger Luft ert\u00f6nen gel\u00e4utete Glocken,", "tokens": ["Still", "in", "d\u00e4mm\u00b7ri\u00b7ger", "Luft", "er\u00b7t\u00f6\u00b7nen", "ge\u00b7l\u00e4u\u00b7te\u00b7te", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Und der Stunden gedenk rufet ein W\u00e4chter die", "tokens": ["Und", "der", "Stun\u00b7den", "ge\u00b7denk", "ru\u00b7fet", "ein", "W\u00e4ch\u00b7ter", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Zahl.", "tokens": ["Zahl", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Jetzt auch kommet ein Wehn und regt die Gipfel", "tokens": ["Jetzt", "auch", "kom\u00b7met", "ein", "Wehn", "und", "regt", "die", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "des Hains auf,", "tokens": ["des", "Hains", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Sieh! und das Ebenbild unserer Erde, der Mond", "tokens": ["Sieh", "!", "und", "das", "E\u00b7ben\u00b7bild", "un\u00b7se\u00b7rer", "Er\u00b7de", ",", "der", "Mond"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "KON", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.25": {"text": "Kommet geheim nun auch; die Schw\u00e4rmerische, die", "tokens": ["Kom\u00b7met", "ge\u00b7heim", "nun", "auch", ";", "die", "Schw\u00e4r\u00b7me\u00b7ri\u00b7sche", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "$.", "ART", "NN", "$,", "PRELS"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Nacht kommt,", "tokens": ["Nacht", "kommt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "++", "measure": "spondeus"}, "line.27": {"text": "Voll mit Sternen und wohl wenig bek\u00fcmmert um", "tokens": ["Voll", "mit", "Ster\u00b7nen", "und", "wohl", "we\u00b7nig", "be\u00b7k\u00fcm\u00b7mert", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "ADV", "PIS", "VVFIN", "APPR"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "uns,", "tokens": ["uns", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.29": {"text": "Gl\u00e4nzt die Erstaunende dort, die Fremdlingin unter", "tokens": ["Gl\u00e4nzt", "die", "Er\u00b7stau\u00b7nen\u00b7de", "dort", ",", "die", "Fremd\u00b7lin\u00b7gin", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "APPR"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.30": {"text": "den Menschen,", "tokens": ["den", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Ueber Gebirgesh\u00f6hn traurig und pr\u00e4chtig herauf.", "tokens": ["Ue\u00b7ber", "Ge\u00b7bir\u00b7ges\u00b7h\u00f6hn", "trau\u00b7rig", "und", "pr\u00e4ch\u00b7tig", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}