{"textgrid.poem.39456": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Bradamante's Muth und Reiz und Feuer,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Bradamante's Muth und Reiz und Feuer,", "tokens": ["Mit", "Bra\u00b7da\u00b7man\u00b7te's", "Muth", "und", "Reiz", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf schlankem Ro\u00df, das sie behende z\u00fcgelt,", "tokens": ["Auf", "schlan\u00b7kem", "Ro\u00df", ",", "das", "sie", "be\u00b7hen\u00b7de", "z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom bunten Helmbusch ihre Stirn befl\u00fcgelt,", "tokens": ["Vom", "bun\u00b7ten", "Helm\u00b7busch", "ih\u00b7re", "Stirn", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zieht Ariosto's Mus' auf Abenteuer.", "tokens": ["Zieht", "A\u00b7ri\u00b7osto's", "Mus'", "auf", "A\u00b7bent\u00b7eu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "APPR", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie siegt und fliegt von dannen, keinem treuer;", "tokens": ["Sie", "siegt", "und", "fliegt", "von", "dan\u00b7nen", ",", "kei\u00b7nem", "treu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADV", "$,", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ebne Grund ist ihr zu eng umh\u00fcgelt,", "tokens": ["Der", "eb\u00b7ne", "Grund", "ist", "ihr", "zu", "eng", "um\u00b7h\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Luftrevier an ihrem Schild gespiegelt", "tokens": ["Im", "Luf\u00b7tre\u00b7vier", "an", "ih\u00b7rem", "Schild", "ge\u00b7spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erscheint die Welt ein sch\u00f6nes Ungeheuer.", "tokens": ["Er\u00b7scheint", "die", "Welt", "ein", "sch\u00f6\u00b7nes", "Un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Viel Wunder zwar nat\u00fcrlich drin geschehen:", "tokens": ["Viel", "Wun\u00b7der", "zwar", "na\u00b7t\u00fcr\u00b7lich", "drin", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschicktes Wagen gilt bei Lieb' und Waffen;", "tokens": ["Ge\u00b7schick\u00b7tes", "Wa\u00b7gen", "gilt", "bei", "Lieb'", "und", "Waf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tappt Roland zu, so pfl\u00fcckt Medor verstohlen.", "tokens": ["Tappt", "Ro\u00b7land", "zu", ",", "so", "pfl\u00fcckt", "Me\u00b7dor", "ver\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKVZ", "$,", "ADV", "VVFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Schalkheit ist die m\u00e4chtigste der Feen,", "tokens": ["Die", "Schalk\u00b7heit", "ist", "die", "m\u00e4ch\u00b7tigs\u00b7te", "der", "Feen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie l\u00e4\u00dft die Phantasie nur toll sich gaffen,", "tokens": ["Sie", "l\u00e4\u00dft", "die", "Phan\u00b7ta\u00b7sie", "nur", "toll", "sich", "gaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um aus dem Mond' ihr den Verstand zu holen.", "tokens": ["Um", "aus", "dem", "Mond'", "ihr", "den", "Ver\u00b7stand", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}