{"textgrid.poem.38576": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Hofnung", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Schimmer des Mondes", "tokens": ["Wie", "der", "Schim\u00b7mer", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch die Schatten der Haine blinkt,", "tokens": ["Durch", "die", "Schat\u00b7ten", "der", "Hai\u00b7ne", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Auf umnachteter Woge leuchtet:", "tokens": ["Auf", "um\u00b7nach\u00b7te\u00b7ter", "Wo\u00b7ge", "leuch\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Also gl\u00e4nzt, mit Sternenklarheit,", "tokens": ["Al\u00b7so", "gl\u00e4nzt", ",", "mit", "Ster\u00b7nen\u00b7klar\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch der Wehmuth Nebelschleier,", "tokens": ["Durch", "der", "Weh\u00b7muth", "Ne\u00b7bel\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch des Kummers Nacht dein L\u00e4cheln,", "tokens": ["Durch", "des", "Kum\u00b7mers", "Nacht", "dein", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freundin der Engel und Menschen, o Hofnung!", "tokens": ["Freun\u00b7din", "der", "En\u00b7gel", "und", "Men\u00b7schen", ",", "o", "Hof\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,", "FM", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der steigenden Sonne", "tokens": ["Wie", "der", "stei\u00b7gen\u00b7den", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die purpurne Fr\u00fche voranfleugt:", "tokens": ["Die", "pur\u00b7pur\u00b7ne", "Fr\u00fc\u00b7he", "vor\u00b7an\u00b7fleugt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Also fleugst du, auf stralenden Fl\u00fcgeln,", "tokens": ["Al\u00b7so", "fleugst", "du", ",", "auf", "stra\u00b7len\u00b7den", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dem Tage des Lohns am errungenen Ziele,", "tokens": ["Dem", "Ta\u00b7ge", "des", "Lohns", "am", "er\u00b7run\u00b7ge\u00b7nen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Dem Tage der ewigen Wonne voran,", "tokens": ["Dem", "Ta\u00b7ge", "der", "e\u00b7wi\u00b7gen", "Won\u00b7ne", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Tr\u00f6sterin aller Verla\u00dfnen, o Hofnung!", "tokens": ["Tr\u00f6s\u00b7te\u00b7rin", "al\u00b7ler", "Ver\u00b7la\u00df\u00b7nen", ",", "o", "Hof\u00b7nung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "FM", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie dem Ku\u00dfe der Lenzluft", "tokens": ["Wie", "dem", "Ku\u00b7\u00dfe", "der", "Lenz\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sich die Blume des Thals enth\u00fcllt,", "tokens": ["Sich", "die", "Blu\u00b7me", "des", "Thals", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sich die Knospe des Hains entfaltet:", "tokens": ["Sich", "die", "Knos\u00b7pe", "des", "Hains", "ent\u00b7fal\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Also schleu\u00dft dir meine Seele sich auf,", "tokens": ["Al\u00b7so", "schleu\u00dft", "dir", "mei\u00b7ne", "See\u00b7le", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Freundliche Tochter des Himmels, o Hofnung!", "tokens": ["Freund\u00b7li\u00b7che", "Toch\u00b7ter", "des", "Him\u00b7mels", ",", "o", "Hof\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Wann du mit holdem Engelgru\u00df,", "tokens": ["Wann", "du", "mit", "hol\u00b7dem", "En\u00b7gel\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf \u00f6den Felsenpfaden mir erscheinst:", "tokens": ["Auf", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7pfa\u00b7den", "mir", "er\u00b7scheinst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O dann vergoldet sich des Lebens Horizont", "tokens": ["O", "dann", "ver\u00b7gol\u00b7det", "sich", "des", "Le\u00b7bens", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit mildem Glanz aus bessern Welten;", "tokens": ["Mit", "mil\u00b7dem", "Glanz", "aus", "bes\u00b7sern", "Wel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "O dann schweben, heilverk\u00fcndend,", "tokens": ["O", "dann", "schwe\u00b7ben", ",", "heil\u00b7ver\u00b7k\u00fcn\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "ADV", "VVINF", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "L\u00e4chelnde Ahndungsgestalten,", "tokens": ["L\u00e4\u00b7cheln\u00b7de", "Ahn\u00b7dungs\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Gleich der Flammens\u00e4ule des erw\u00e4hlten Volks,", "tokens": ["Gleich", "der", "Flam\u00b7men\u00b7s\u00e4u\u00b7le", "des", "er\u00b7w\u00e4hl\u00b7ten", "Volks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "In lichten Schaaren vor mir her und streuen", "tokens": ["In", "lich\u00b7ten", "Schaa\u00b7ren", "vor", "mir", "her", "und", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Leitende Schimmer auf meine Bahn.", "tokens": ["Lei\u00b7ten\u00b7de", "Schim\u00b7mer", "auf", "mei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "O Hofnung! Hofnung! tr\u00f6stend wie Fr\u00fchlingshauch", "tokens": ["O", "Hof\u00b7nung", "!", "Hof\u00b7nung", "!", "tr\u00f6s\u00b7tend", "wie", "Fr\u00fch\u00b7lings\u00b7hauch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "$.", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach Winterst\u00fcrmen! freundlich wie Morgenroth!", "tokens": ["Nach", "Win\u00b7ter\u00b7st\u00fcr\u00b7men", "!", "freund\u00b7lich", "wie", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entz\u00fcckend wie die Sommermondnacht!", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "wie", "die", "Som\u00b7mer\u00b7mond\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lieblich wie auf M\u00e4dchenwangen", "tokens": ["Lieb\u00b7lich", "wie", "auf", "M\u00e4d\u00b7chen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Des ersten Ku\u00dfes keusche R\u00f6the:", "tokens": ["Des", "ers\u00b7ten", "Ku\u00b7\u00dfes", "keu\u00b7sche", "R\u00f6\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn alles um mich her verbl\u00fcht und stirbt,", "tokens": ["Wenn", "al\u00b7les", "um", "mich", "her", "ver\u00b7bl\u00fcht", "und", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PRF", "APZR", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn alles f\u00e4llt und sinkt und untergeht:", "tokens": ["Wenn", "al\u00b7les", "f\u00e4llt", "und", "sinkt", "und", "un\u00b7ter\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Hofnung! Hofnung! dann verla\u00df mich nicht!", "tokens": ["O", "Hof\u00b7nung", "!", "Hof\u00b7nung", "!", "dann", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "ADV", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Umstr\u00f6me ganz mit hoher Himmelsahndung,", "tokens": ["Um\u00b7str\u00f6\u00b7me", "ganz", "mit", "ho\u00b7her", "Him\u00b7mel\u00b7sahn\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit Vorempfindung der Unsterblichkeit,", "tokens": ["Mit", "Vor\u00b7emp\u00b7fin\u00b7dung", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Mit Freuden Gottes mir die m\u00fcde Seele,", "tokens": ["Mit", "Freu\u00b7den", "Got\u00b7tes", "mir", "die", "m\u00fc\u00b7de", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und hebe des Verla\u00dfnen Geist empor", "tokens": ["Und", "he\u00b7be", "des", "Ver\u00b7la\u00df\u00b7nen", "Geist", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zu lichten H\u00f6h'n und zeig in heil'ger Ferne", "tokens": ["Zu", "lich\u00b7ten", "H\u00f6h'n", "und", "zeig", "in", "heil'\u00b7ger", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ihm seiner Wallfahrt palmumkr\u00e4nztes Ziel,", "tokens": ["Ihm", "sei\u00b7ner", "Wall\u00b7fahrt", "pal\u00b7mum\u00b7kr\u00e4nz\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wo an des Urlichts unerschaffnem Quell", "tokens": ["Wo", "an", "des", "Ur\u00b7lichts", "un\u00b7er\u00b7schaff\u00b7nem", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das Halleluja der Vollendung t\u00f6nt;", "tokens": ["Das", "Hal\u00b7le\u00b7lu\u00b7ja", "der", "Vol\u00b7len\u00b7dung", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Ach! wo die bessern, gleichgeschaffnen Seelen", "tokens": ["Ach", "!", "wo", "die", "bes\u00b7sern", ",", "gleich\u00b7ge\u00b7schaff\u00b7nen", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Des Wiederfindens unaussprechliches Entz\u00fccken,", "tokens": ["Des", "Wie\u00b7der\u00b7fin\u00b7dens", "un\u00b7aus\u00b7sprech\u00b7li\u00b7ches", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Des ewigen Vereinens sanftre Wonne", "tokens": ["Des", "e\u00b7wi\u00b7gen", "Ver\u00b7ei\u00b7nens", "sanft\u00b7re", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In Str\u00f6men trinken, unter Engelch\u00f6ren", "tokens": ["In", "Str\u00f6\u00b7men", "trin\u00b7ken", ",", "un\u00b7ter", "En\u00b7gelc\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dem Thron des Allvollkommnen n\u00e4her wallen,", "tokens": ["Dem", "Thron", "des", "All\u00b7voll\u00b7komm\u00b7nen", "n\u00e4\u00b7her", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mit s\u00fcsser Sehnsucht ihrer Zukunft Loos", "tokens": ["Mit", "s\u00fcs\u00b7ser", "Sehn\u00b7sucht", "ih\u00b7rer", "Zu\u00b7kunft", "Loos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Im Seraph ahnden und es ganz empfinden,", "tokens": ["Im", "Se\u00b7raph", "ahn\u00b7den", "und", "es", "ganz", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Da\u00df Lieb' auf Erden tr\u00fcbe D\u00e4mmrung nur,", "tokens": ["Da\u00df", "Lieb'", "auf", "Er\u00b7den", "tr\u00fc\u00b7be", "D\u00e4mm\u00b7rung", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da\u00df Lieb' im Himmel Sonnenaufgang ist!", "tokens": ["Da\u00df", "Lieb'", "im", "Him\u00b7mel", "Son\u00b7nen\u00b7auf\u00b7gang", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}