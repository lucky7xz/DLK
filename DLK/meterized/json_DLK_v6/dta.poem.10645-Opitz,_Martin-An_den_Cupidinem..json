{"dta.poem.10645": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An den Cupidinem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Odu Gott der s\u00fcssen Schmertzen/", "tokens": ["O\u00b7du", "Gott", "der", "s\u00fcs\u00b7sen", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warumb da\u00df man dich so blindt", "tokens": ["Wa\u00b7rumb", "da\u00df", "man", "dich", "so", "blindt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KOUS", "PIS", "PRF", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vberal gemahlet findt?", "tokens": ["Vbe\u00b7ral", "ge\u00b7mah\u00b7let", "findt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Es geht schwer ein meinem Hertzen/", "tokens": ["Es", "geht", "schwer", "ein", "mei\u00b7nem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun du seyest ohn Gesicht/", "tokens": ["Nun", "du", "se\u00b7yest", "ohn", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ich kans aber glauben nicht.", "tokens": ["Ich", "kans", "a\u00b7ber", "glau\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sichstu nicht/ wie kanstu wissen/", "tokens": ["Sichs\u00b7tu", "nicht", "/", "wie", "kans\u00b7tu", "wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$(", "PWAV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo dein Pfeil hinfliegen soll?", "tokens": ["Wo", "dein", "Pfeil", "hin\u00b7flie\u00b7gen", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Blinde sehen sonst nicht wohl/", "tokens": ["Blin\u00b7de", "se\u00b7hen", "sonst", "nicht", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Du kanst fein gerade schiessen/ Nun du seyest/ rc.", "tokens": ["Du", "kanst", "fein", "ge\u00b7ra\u00b7de", "schies\u00b7sen", "/", "Nun", "du", "se\u00b7yest", "/", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "VVINF", "$(", "ADV", "PPER", "VAFIN", "$(", "NE", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Die in P\u00fcschen vmbher ziehen/", "tokens": ["Die", "in", "P\u00fc\u00b7schen", "vm\u00b7bher", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die in w\u00fcsten W\u00e4ldern sein/", "tokens": ["Die", "in", "w\u00fcs\u00b7ten", "W\u00e4l\u00b7dern", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00f6nnen doch der Liebes-Pein/", "tokens": ["K\u00f6n\u00b7nen", "doch", "der", "Lie\u00b7bes\u00b7Pein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Vnd den Pfeilen nit entfliehen/ Nun du seyest/ rc.", "tokens": ["Vnd", "den", "Pfei\u00b7len", "nit", "ent\u00b7flie\u00b7hen", "/", "Nun", "du", "se\u00b7yest", "/", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$(", "ADV", "PPER", "VAFIN", "$(", "NE", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.15": {"text": "Die das weite Meer durchjagen/", "tokens": ["Die", "das", "wei\u00b7te", "Meer", "durch\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "M\u00fcssen f\u00fchlen deine st\u00e4rck/", "tokens": ["M\u00fcs\u00b7sen", "f\u00fch\u00b7len", "dei\u00b7ne", "st\u00e4rck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ist das blinder Leuthe Werck?", "tokens": ["Ist", "das", "blin\u00b7der", "Leu\u00b7the", "Werck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Soll ich recht die Warheit sagen; Nu du/ rc.", "tokens": ["Soll", "ich", "recht", "die", "War\u00b7heit", "sa\u00b7gen", ";", "Nu", "du", "/", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "ADV", "PPER", "$(", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Gingstu nicht die enge Strassen", "tokens": ["Gings\u00b7tu", "nicht", "die", "en\u00b7ge", "Stras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "In das Himmlische Gebew/", "tokens": ["In", "das", "Himm\u00b7li\u00b7sche", "Ge\u00b7bew", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Gantz allein ohn alle schew/", "tokens": ["Gantz", "al\u00b7lein", "ohn", "al\u00b7le", "schew", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIS", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "D\u00f6rfftest Jovem auch anfassen? Nun du/ rc.", "tokens": ["D\u00f6rff\u00b7test", "Jo\u00b7vem", "auch", "an\u00b7fas\u00b7sen", "?", "Nun", "du", "/", "rc", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVINF", "$.", "ADV", "NE", "$(", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "Kontest du nicht Pluto finden", "tokens": ["Kon\u00b7test", "du", "nicht", "Plu\u00b7to", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "In der tieffen H\u00f6llen Schlundt/", "tokens": ["In", "der", "tief\u00b7fen", "H\u00f6l\u00b7len", "Schlundt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "In dem finsteren Abgrundt/", "tokens": ["In", "dem", "fins\u00b7te\u00b7ren", "Ab\u00b7grundt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Ihn zuschiessen dich erwinden? Nun du seyest/ rc.", "tokens": ["Ihn", "zu\u00b7schies\u00b7sen", "dich", "er\u00b7win\u00b7den", "?", "Nun", "du", "se\u00b7yest", "/", "rc", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$.", "ADV", "PPER", "VAFIN", "$(", "NE", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.27": {"text": "Du wilt keine Klage wissen/", "tokens": ["Du", "wilt", "kei\u00b7ne", "Kla\u00b7ge", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Auch von denen/ die durch dich", "tokens": ["Auch", "von", "de\u00b7nen", "/", "die", "durch", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PRELS", "$(", "ART", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Seind verwundet juniglich/", "tokens": ["Seind", "ver\u00b7wun\u00b7det", "ju\u00b7nig\u00b7lich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Thust all jhre Klag au\u00dfschlissen/", "tokens": ["Thust", "all", "jhre", "Klag", "au\u00df\u00b7schlis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Blindt bistu wol nicht: Ich glaub", "tokens": ["Blindt", "bis\u00b7tu", "wol", "nicht", ":", "Ich", "glaub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Da\u00df du seyst gewaltig taub.", "tokens": ["Da\u00df", "du", "seyst", "ge\u00b7wal\u00b7tig", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}