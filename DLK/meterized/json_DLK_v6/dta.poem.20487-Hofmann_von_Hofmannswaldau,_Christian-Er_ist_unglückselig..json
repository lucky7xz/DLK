{"dta.poem.20487": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Er ist ungl\u00fcckselig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ungl\u00fcckseliger! warum bin ich gebohren?", "tokens": ["Ich", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ger", "!", "wa\u00b7rum", "bin", "ich", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll ich ein gauckel-spiel der falschen sterne seyn?", "tokens": ["Soll", "ich", "ein", "gau\u00b7ckel\u00b7spiel", "der", "fal\u00b7schen", "ster\u00b7ne", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat das verh\u00e4ngni\u00df denn zum balle mich erkohren?", "tokens": ["Hat", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "denn", "zum", "bal\u00b7le", "mich", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schau ich f\u00fcr sonnen-licht plitz und cometen-schein?", "tokens": ["Schau", "ich", "f\u00fcr", "son\u00b7nen\u00b7licht", "plitz", "und", "co\u00b7me\u00b7ten\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NE", "NE", "KON", "NE", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wei\u00df ich von freuden nichts zu sagen?", "tokens": ["Wei\u00df", "ich", "von", "freu\u00b7den", "nichts", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Macht mich der himmel nur zur wahlstatt vieler plagen?", "tokens": ["Macht", "mich", "der", "him\u00b7mel", "nur", "zur", "wahl\u00b7statt", "vie\u00b7ler", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich mu\u00df mit kummer-brodt die matte seele speisen;", "tokens": ["Ich", "mu\u00df", "mit", "kum\u00b7mer\u00b7brodt", "die", "mat\u00b7te", "see\u00b7le", "spei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das thr\u00e4nen-wasser ist mein muscateller-most.", "tokens": ["Das", "thr\u00e4\u00b7nen\u00b7was\u00b7ser", "ist", "mein", "mus\u00b7ca\u00b7tel\u00b7ler\u00b7most", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich mu\u00df best\u00e4ndiglich durch scharffe disteln reisen;", "tokens": ["Ich", "mu\u00df", "be\u00b7st\u00e4n\u00b7dig\u00b7lich", "durch", "scharf\u00b7fe", "dis\u00b7teln", "rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die schmertzen sind mein tranck/ das ungl\u00fcck meine kost.", "tokens": ["Die", "schmert\u00b7zen", "sind", "mein", "tranck", "/", "das", "un\u00b7gl\u00fcck", "mei\u00b7ne", "kost", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mu\u00df auff folter-b\u00e4ncke sitzen/", "tokens": ["Ich", "mu\u00df", "auff", "fol\u00b7ter\u00b7b\u00e4n\u00b7cke", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und auff den schweren stab des jammers mich nur st\u00fctzen.", "tokens": ["Und", "auff", "den", "schwe\u00b7ren", "stab", "des", "jam\u00b7mers", "mich", "nur", "st\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "APPR", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein leib ist ein spital/ darinn der geist mu\u00df krancken;", "tokens": ["Mein", "leib", "ist", "ein", "spi\u00b7tal", "/", "da\u00b7rinn", "der", "geist", "mu\u00df", "kran\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "$(", "PAV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin ein ebenbild der bleichen traurigkeit;", "tokens": ["Ich", "bin", "ein", "e\u00b7ben\u00b7bild", "der", "blei\u00b7chen", "trau\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schlage mich mit nichts als sorglichen gedancken/", "tokens": ["Ich", "schla\u00b7ge", "mich", "mit", "nichts", "als", "sorg\u00b7li\u00b7chen", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "KOKOM", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit ach und weh verk\u00fcrtz ich meine lebens-zeit;", "tokens": ["Mit", "ach", "und", "weh", "ver\u00b7k\u00fcrtz", "ich", "mei\u00b7ne", "le\u00b7bens\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "XY", "KON", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich werde j\u00e4mmerlich geqv\u00e4let/", "tokens": ["Ich", "wer\u00b7de", "j\u00e4m\u00b7mer\u00b7lich", "ge\u00b7qv\u00e4\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil dem gem\u00fcthe ruh/ dem blute k\u00fchlung fehlet.", "tokens": ["Weil", "dem", "ge\u00b7m\u00fc\u00b7the", "ruh", "/", "dem", "blu\u00b7te", "k\u00fch\u00b7lung", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie vielmahl sitz ich doch bet\u00e4ubt an allen sinnen!", "tokens": ["Wie", "viel\u00b7mahl", "sitz", "ich", "doch", "be\u00b7t\u00e4ubt", "an", "al\u00b7len", "sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVIMP", "PPER", "ADV", "VVPP", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr schmertzen seh ich offt mit offnen augen nicht.", "tokens": ["F\u00fcr", "schmert\u00b7zen", "seh", "ich", "offt", "mit", "off\u00b7nen", "au\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verstand und witz ist weg/ ich wei\u00df nichts zu beginnen/", "tokens": ["Ver\u00b7stand", "und", "witz", "ist", "weg", "/", "ich", "wei\u00df", "nichts", "zu", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df da\u00df ein seuffzer mir den dicken nebel bricht.", "tokens": ["Bi\u00df", "da\u00df", "ein", "seuff\u00b7zer", "mir", "den", "di\u00b7cken", "ne\u00b7bel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dennoch kan ich weder meinen sachen/", "tokens": ["Und", "den\u00b7noch", "kan", "ich", "we\u00b7der", "mei\u00b7nen", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Noch meiner thr\u00e4nen lauffein tr\u00f6stlich ende machen.", "tokens": ["Noch", "mei\u00b7ner", "thr\u00e4\u00b7nen", "lauf\u00b7fein", "tr\u00f6st\u00b7lich", "en\u00b7de", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach da\u00df ich einem nur mein leiden k\u00f6nt entdecken!", "tokens": ["Ach", "da\u00df", "ich", "ei\u00b7nem", "nur", "mein", "lei\u00b7den", "k\u00f6nt", "ent\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ART", "ADV", "PPOSAT", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielleicht w\u00fcrd dieses noch ein pflaster f\u00fcr mich seyn/", "tokens": ["Viel\u00b7leicht", "w\u00fcrd", "die\u00b7ses", "noch", "ein", "pflas\u00b7ter", "f\u00fcr", "mich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "ART", "NN", "APPR", "PPER", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in den wunden mir was linderung erwecken.", "tokens": ["Und", "in", "den", "wun\u00b7den", "mir", "was", "lin\u00b7de\u00b7rung", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch nein! es wei\u00df kein freund mehr rath f\u00fcr meine pein;", "tokens": ["Doch", "nein", "!", "es", "wei\u00df", "kein", "freund", "mehr", "rath", "f\u00fcr", "mei\u00b7ne", "pein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PIAT", "NN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum soll kein mensch von mir erfahren/", "tokens": ["Drum", "soll", "kein", "mensch", "von", "mir", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was f\u00fcr gefehrten sich des ungl\u00fccks mit mir paaren.", "tokens": ["Was", "f\u00fcr", "ge\u00b7fehr\u00b7ten", "sich", "des", "un\u00b7gl\u00fccks", "mit", "mir", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "PRF", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich will hinf\u00fcro nicht mit meinen f\u00e4sseln klingen.", "tokens": ["Ich", "will", "hin\u00b7f\u00fc\u00b7ro", "nicht", "mit", "mei\u00b7nen", "f\u00e4s\u00b7seln", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn welcher sclave r\u00fchrt ohn schmertzen doch sein joch?", "tokens": ["Denn", "wel\u00b7cher", "scla\u00b7ve", "r\u00fchrt", "ohn", "schmert\u00b7zen", "doch", "sein", "joch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ADJA", "VVFIN", "APPR", "VVINF", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein stummer seuffzer soll nur nach dem himmel dringen/", "tokens": ["Ein", "stum\u00b7mer", "seuff\u00b7zer", "soll", "nur", "nach", "dem", "him\u00b7mel", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielleicht erbarmet der sich meiner wunden noch.", "tokens": ["Viel\u00b7leicht", "er\u00b7bar\u00b7met", "der", "sich", "mei\u00b7ner", "wun\u00b7den", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PRF", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zum himmel sollen meine z\u00e4hren", "tokens": ["Zum", "him\u00b7mel", "sol\u00b7len", "mei\u00b7ne", "z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich ferner hin zwar still/ doch unabl\u00e4\u00dfig kehren.", "tokens": ["Sich", "fer\u00b7ner", "hin", "zwar", "still", "/", "doch", "un\u00b7ab\u00b7l\u00e4\u00b7\u00dfig", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADV", "ADJD", "$(", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es kan doch nirgends hin ein wasser freyer fliessen/", "tokens": ["Es", "kan", "doch", "nir\u00b7gends", "hin", "ein", "was\u00b7ser", "frey\u00b7er", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als an denselben ort/ wovon es anfangs kam.", "tokens": ["Als", "an", "den\u00b7sel\u00b7ben", "ort", "/", "wo\u00b7von", "es", "an\u00b7fangs", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "$(", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der himmel martert mich; Drum darff die welt nicht wissen/", "tokens": ["Der", "him\u00b7mel", "mar\u00b7tert", "mich", ";", "Drum", "darff", "die", "welt", "nicht", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PAV", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was eigentlich mein leid und meines hertzens-gram.", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "mein", "leid", "und", "mei\u00b7nes", "hert\u00b7zens\u00b7gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJD", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch will mich iemand noch beklagen/", "tokens": ["Doch", "will", "mich", "ie\u00b7mand", "noch", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So schreib er auff mein grab: Hier ruht ein ziel der plagen.", "tokens": ["So", "schreib", "er", "auff", "mein", "grab", ":", "Hier", "ruht", "ein", "ziel", "der", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}