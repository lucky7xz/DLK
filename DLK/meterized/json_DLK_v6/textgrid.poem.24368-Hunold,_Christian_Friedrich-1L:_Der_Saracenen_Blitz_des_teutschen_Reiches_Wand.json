{"textgrid.poem.24368": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Saracenen Blitz/ des teutschen Reiches Wand/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Saracenen Blitz/ des teutschen Reiches Wand/", "tokens": ["Der", "Sa\u00b7ra\u00b7ce\u00b7nen", "Blitz", "/", "des", "teut\u00b7schen", "Rei\u00b7ches", "Wand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dar\u00fcber Franckreich nie mit Vortheil ist gerannt/", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "Fran\u00b7ck\u00b7reich", "nie", "mit", "Vor\u00b7theil", "ist", "ge\u00b7rannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Im \u00fcberlegen klug/ bedachtsam was zu wagen/", "tokens": ["Im", "\u00fc\u00b7berl\u00b7e\u00b7gen", "klug", "/", "be\u00b7dacht\u00b7sam", "was", "zu", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "$(", "ADJD", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So durch Verstand begl\u00fcckt/ als tapfer in dem Schlagen.", "tokens": ["So", "durch", "Ver\u00b7stand", "be\u00b7gl\u00fcckt", "/", "als", "tap\u00b7fer", "in", "dem", "Schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$(", "KOUS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}