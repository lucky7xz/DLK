{"dta.poem.20519": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Floretten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein augen-plitz prei\u00df-w\u00fcrdigste Florette/", "tokens": ["Dein", "au\u00b7gen\u00b7plitz", "prei\u00df\u00b7w\u00fcr\u00b7digs\u00b7te", "Flo\u00b7ret\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat auch mein hertz durch stille glut ger\u00fchrt.", "tokens": ["Hat", "auch", "mein", "hertz", "durch", "stil\u00b7le", "glut", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchle schon die schwere liebes-kette/", "tokens": ["Ich", "f\u00fch\u00b7le", "schon", "die", "schwe\u00b7re", "lie\u00b7bes\u00b7ket\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die deine pracht um meine glieder f\u00fchrt.", "tokens": ["Die", "dei\u00b7ne", "pracht", "um", "mei\u00b7ne", "glie\u00b7der", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein wesen hat gesiegt/", "tokens": ["Dein", "we\u00b7sen", "hat", "ge\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich aber bin gefangen/", "tokens": ["Ich", "a\u00b7ber", "bin", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und trage doch verlangen", "tokens": ["Und", "tra\u00b7ge", "doch", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach dem/ was mich und meine lust bekriegt.", "tokens": ["Nach", "dem", "/", "was", "mich", "und", "mei\u00b7ne", "lust", "be\u00b7kriegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWS", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verzeihe nur/ du tugend-volle seele/", "tokens": ["Ver\u00b7zei\u00b7he", "nur", "/", "du", "tu\u00b7gen\u00b7dvol\u00b7le", "see\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich mein hertz dir so vor augen stellt/", "tokens": ["Da\u00df", "sich", "mein", "hertz", "dir", "so", "vor", "au\u00b7gen", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df ich nicht nach meiner art verh\u00f6le/", "tokens": ["Und", "da\u00df", "ich", "nicht", "nach", "mei\u00b7ner", "art", "ver\u00b7h\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was meine brust sonst gar verborgen h\u00e4lt;", "tokens": ["Was", "mei\u00b7ne", "brust", "sonst", "gar", "ver\u00b7bor\u00b7gen", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das leiden ist zugro\u00df/", "tokens": ["Das", "lei\u00b7den", "ist", "zu\u00b7gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Warum ich mich betr\u00fcbe:", "tokens": ["Wa\u00b7rum", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum rei\u00df durch deine liebe", "tokens": ["Drum", "rei\u00df", "durch", "dei\u00b7ne", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir meine last und f\u00e4ssel lo\u00df.", "tokens": ["Mir", "mei\u00b7ne", "last", "und", "f\u00e4s\u00b7sel", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "VVFIN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df es zwar; Du wirst mir widersprechen/", "tokens": ["Ich", "wei\u00df", "es", "zwar", ";", "Du", "wirst", "mir", "wi\u00b7der\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sagen: Ach! es ist nur schatten-spiel.", "tokens": ["Und", "sa\u00b7gen", ":", "Ach", "!", "es", "ist", "nur", "schat\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "ITJ", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich die welt mit worten l\u00e4st bestechen/", "tokens": ["Wer", "sich", "die", "welt", "mit", "wor\u00b7ten", "l\u00e4st", "be\u00b7ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "APPR", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der f\u00e4llt/ wenn er am besten steigen will.", "tokens": ["Der", "f\u00e4llt", "/", "wenn", "er", "am", "bes\u00b7ten", "stei\u00b7gen", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPER", "PTKA", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach! aber meine pein", "tokens": ["Ach", "!", "a\u00b7ber", "mei\u00b7ne", "pein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist anders weit beschaffen/", "tokens": ["Ist", "an\u00b7ders", "weit", "be\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als mancher liebes-affen/", "tokens": ["Als", "man\u00b7cher", "lie\u00b7bes\u00b7af\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die voller list und complimente seyn.", "tokens": ["Die", "vol\u00b7ler", "list", "und", "com\u00b7pli\u00b7men\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein liebes-feur besteht nicht in dem munde/", "tokens": ["Mein", "lie\u00b7bes\u00b7feur", "be\u00b7steht", "nicht", "in", "dem", "mun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die falschheit ist mein \u00e4rgster seelen-feind.", "tokens": ["Die", "falschheit", "ist", "mein", "\u00e4rgs\u00b7ter", "see\u00b7len\u00b7feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich liebe dich aus meines hertzens grunde/", "tokens": ["Ich", "lie\u00b7be", "dich", "aus", "mei\u00b7nes", "hert\u00b7zens", "grun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Di\u00df ist genug. Bistu nun wieder freund/", "tokens": ["Di\u00df", "ist", "ge\u00b7nug", ".", "Bis\u00b7tu", "nun", "wie\u00b7der", "freund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$.", "NE", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So solstu eher sehn", "tokens": ["So", "sols\u00b7tu", "e\u00b7her", "sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das leben mich verdammen/", "tokens": ["Das", "le\u00b7ben", "mich", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als meine liebes-flammen", "tokens": ["Als", "mei\u00b7ne", "lie\u00b7bes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor mattigkeit und ohnmacht untergehn.", "tokens": ["Vor", "mat\u00b7tig\u00b7keit", "und", "ohn\u00b7macht", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}