{"textgrid.poem.52956": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vater Aller, alle Erdenkreise,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater Aller, alle Erdenkreise,", "tokens": ["Va\u00b7ter", "Al\u00b7ler", ",", "al\u00b7le", "Er\u00b7den\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle Zeiten ehren dein Geboth;", "tokens": ["Al\u00b7le", "Zei\u00b7ten", "eh\u00b7ren", "dein", "Ge\u00b7both", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hordenwilde, Heilige und Weise", "tokens": ["Hor\u00b7den\u00b7wil\u00b7de", ",", "Hei\u00b7li\u00b7ge", "und", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nennen Zevs dich, Jovah oder Gott.", "tokens": ["Nen\u00b7nen", "Zevs", "dich", ",", "Jo\u00b7vah", "o\u00b7der", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$,", "NE", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gro\u00dfer Urquell, den ich nie ergr\u00fcnde,", "tokens": ["Gro\u00b7\u00dfer", "Ur\u00b7quell", ",", "den", "ich", "nie", "er\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dahin nur beschr\u00e4nkst du meinen Sinn,", "tokens": ["Da\u00b7hin", "nur", "be\u00b7schr\u00e4nkst", "du", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df ich immer deine G\u00fcte finde,", "tokens": ["Da\u00df", "ich", "im\u00b7mer", "dei\u00b7ne", "G\u00fc\u00b7te", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nur seh, da\u00df ich ein Blinder bin.", "tokens": ["Und", "nur", "seh", ",", "da\u00df", "ich", "ein", "Blin\u00b7der", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch du gabst mir in dem finstern Stande", "tokens": ["Doch", "du", "gabst", "mir", "in", "dem", "fins\u00b7tern", "Stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das Gef\u00fchl, was Gut und B\u00f6se sey;", "tokens": ["Das", "Ge\u00b7f\u00fchl", ",", "was", "Gut", "und", "B\u00f6\u00b7se", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Legtest die Natur in ihre Bande,", "tokens": ["Leg\u00b7test", "die", "Na\u00b7tur", "in", "ih\u00b7re", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber lie\u00dfest meinen Willen frey.", "tokens": ["A\u00b7ber", "lie\u00b7\u00dfest", "mei\u00b7nen", "Wil\u00b7len", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wo Gewissensregungen mich ziehen,", "tokens": ["Wo", "Ge\u00b7wis\u00b7sens\u00b7re\u00b7gun\u00b7gen", "mich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Oder wo der Warner mir verbeut,", "tokens": ["O\u00b7der", "wo", "der", "War\u00b7ner", "mir", "ver\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df mich dieses mehr als H\u00f6lle fliehen,", "tokens": ["La\u00df", "mich", "die\u00b7ses", "mehr", "als", "H\u00f6l\u00b7le", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "PIAT", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jenes suchen mehr als Seligkeit.", "tokens": ["Je\u00b7nes", "su\u00b7chen", "mehr", "als", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Segnet deine Milde mein Verlangen,", "tokens": ["Seg\u00b7net", "dei\u00b7ne", "Mil\u00b7de", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df mich deinen Segen nicht entweihen;", "tokens": ["La\u00df", "mich", "dei\u00b7nen", "Se\u00b7gen", "nicht", "ent\u00b7wei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Menschen zahlen Dir nur durch Empfangen,", "tokens": ["Men\u00b7schen", "zah\u00b7len", "Dir", "nur", "durch", "Emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und genie\u00dfen hei\u00dft gehorsam seyn.", "tokens": ["Und", "ge\u00b7nie\u00b7\u00dfen", "hei\u00dft", "ge\u00b7hor\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber nicht in unsre enge Scene", "tokens": ["A\u00b7ber", "nicht", "in", "uns\u00b7re", "en\u00b7ge", "Sce\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sey mir deine Vaterhuld begr\u00e4nzt,", "tokens": ["Sey", "mir", "dei\u00b7ne", "Va\u00b7ter\u00b7huld", "be\u00b7gr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mehr als Herr der armen Erdens\u00f6hne,", "tokens": ["Mehr", "als", "Herr", "der", "ar\u00b7men", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da ein Weltenwirbel um uns gl\u00e4nzt.", "tokens": ["Da", "ein", "Wel\u00b7ten\u00b7wir\u00b7bel", "um", "uns", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Nie la\u00df meine schwache Hand es wagen,", "tokens": ["Nie", "la\u00df", "mei\u00b7ne", "schwa\u00b7che", "Hand", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deinen Blitz zu schleudern auf den Feind,", "tokens": ["Dei\u00b7nen", "Blitz", "zu", "schleu\u00b7dern", "auf", "den", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Noch Verdammni\u00df rund umher zu tragen", "tokens": ["Noch", "Ver\u00b7damm\u00b7ni\u00df", "rund", "um\u00b7her", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJD", "PTKVZ", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jedem, der dein Widersacher scheint.", "tokens": ["Je\u00b7dem", ",", "der", "dein", "Wi\u00b7der\u00b7sa\u00b7cher", "scheint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Wenn ich richtig wandle, Vater, schenke", "tokens": ["Wenn", "ich", "rich\u00b7tig", "wand\u00b7le", ",", "Va\u00b7ter", ",", "schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir die Gnade, richtig fort zu gehn;", "tokens": ["Mir", "die", "Gna\u00b7de", ",", "rich\u00b7tig", "fort", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich aber irre, Vater, lenke", "tokens": ["Wenn", "ich", "a\u00b7ber", "ir\u00b7re", ",", "Va\u00b7ter", ",", "len\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Du mein Herz, den bessern Weg zu sehn.", "tokens": ["Du", "mein", "Herz", ",", "den", "bes\u00b7sern", "Weg", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Sch\u00fctze mich vor Stolz, der Thoren blendet,", "tokens": ["Sch\u00fct\u00b7ze", "mich", "vor", "Stolz", ",", "der", "Tho\u00b7ren", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Frevler Unzufriedenheit,", "tokens": ["Und", "der", "Frev\u00b7ler", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn mir die\u00df nicht deine Weisheit spendet,", "tokens": ["Wenn", "mir", "die\u00df", "nicht", "dei\u00b7ne", "Weis\u00b7heit", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder die\u00df mir deine Huld verleiht.", "tokens": ["O\u00b7der", "die\u00df", "mir", "dei\u00b7ne", "Huld", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "La\u00df mich meiner Br\u00fcder Schmerz empfinden,", "tokens": ["La\u00df", "mich", "mei\u00b7ner", "Br\u00fc\u00b7der", "Schmerz", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und den Fehler decken neben mir;", "tokens": ["Und", "den", "Feh\u00b7ler", "de\u00b7cken", "ne\u00b7ben", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Erbarmung, die bey mir sie finden,", "tokens": ["Die", "Er\u00b7bar\u00b7mung", ",", "die", "bey", "mir", "sie", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "PPER", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die Erbarmung sind' ich dann bey dir.", "tokens": ["Die", "Er\u00b7bar\u00b7mung", "sind'", "ich", "dann", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Niedrig bin ich, doch nicht ganz von Erde,", "tokens": ["Nied\u00b7rig", "bin", "ich", ",", "doch", "nicht", "ganz", "von", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADV", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da dein Hauch zu leben mir geboth;", "tokens": ["Da", "dein", "Hauch", "zu", "le\u00b7ben", "mir", "ge\u00b7both", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fchre du, wohin ich gehen werde,", "tokens": ["F\u00fch\u00b7re", "du", ",", "wo\u00b7hin", "ich", "ge\u00b7hen", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heute mich durch Leben oder Tod.", "tokens": ["Heu\u00b7te", "mich", "durch", "Le\u00b7ben", "o\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Gib zum Loos mir heute Brot und Frieden;", "tokens": ["Gib", "zum", "Loos", "mir", "heu\u00b7te", "Brot", "und", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jede andre Gabe der Natur", "tokens": ["Je\u00b7de", "and\u00b7re", "Ga\u00b7be", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird mir, ist es gut, von dir beschieden;", "tokens": ["Wird", "mir", ",", "ist", "es", "gut", ",", "von", "dir", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dein Wille, Gott, geschehe nur.", "tokens": ["Und", "dein", "Wil\u00b7le", ",", "Gott", ",", "ge\u00b7sche\u00b7he", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Gott, dein Tempel ist der Himmel Sph\u00e4re,", "tokens": ["Gott", ",", "dein", "Tem\u00b7pel", "ist", "der", "Him\u00b7mel", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erde, Meer und Luft dein Opferhain!", "tokens": ["Er\u00b7de", ",", "Meer", "und", "Luft", "dein", "Op\u00b7fer\u00b7hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jauchzt, was lebt, im Chor zu seiner Ehre,", "tokens": ["Jauchzt", ",", "was", "lebt", ",", "im", "Chor", "zu", "sei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "$,", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und das Weltall m\u00fcsse Weihrauch streun.", "tokens": ["Und", "das", "Wel\u00b7tall", "m\u00fcs\u00b7se", "Weih\u00b7rauch", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}