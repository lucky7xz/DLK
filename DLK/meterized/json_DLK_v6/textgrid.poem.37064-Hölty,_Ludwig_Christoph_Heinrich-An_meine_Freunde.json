{"textgrid.poem.37064": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An meine Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schande ladet der Mann auf sich,", "tokens": ["Schan\u00b7de", "la\u00b7det", "der", "Mann", "auf", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PRF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Auf sein sklavisches Volk, welcher den Meucheldolch", "tokens": ["Auf", "sein", "skla\u00b7vi\u00b7sches", "Volk", ",", "wel\u00b7cher", "den", "Meu\u00b7chel\u00b7dolch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "In den Busen der Unschuld senkt,", "tokens": ["In", "den", "Bu\u00b7sen", "der", "Un\u00b7schuld", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und in Engelgewand h\u00fcllet den Otternleib", "tokens": ["Und", "in", "En\u00b7gel\u00b7ge\u00b7wand", "h\u00fcl\u00b7let", "den", "Ot\u00b7tern\u00b7leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Seiner G\u00f6ttin der Buhlerey.", "tokens": ["Sei\u00b7ner", "G\u00f6t\u00b7tin", "der", "Buh\u00b7le\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Edle schw\u00f6ren ihm Ha\u00df! Streuet ein Schmeichelsklav", "tokens": ["Ed\u00b7le", "schw\u00f6\u00b7ren", "ihm", "Ha\u00df", "!", "Streu\u00b7et", "ein", "Schmei\u00b7chels\u00b7klav"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "PPER", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Kron' auf Kronen in seinen Schoos,", "tokens": ["Kron'", "auf", "Kro\u00b7nen", "in", "sei\u00b7nen", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "O er jauchze nur nicht! Prahler dein Lorbeer welkt,", "tokens": ["O", "er", "jauch\u00b7ze", "nur", "nicht", "!", "Prah\u00b7ler", "dein", "Lor\u00b7beer", "welkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ADV", "PTKNEG", "$.", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Eh dein Leben verflogen ist,", "tokens": ["Eh", "dein", "Le\u00b7ben", "ver\u00b7flo\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Welkt, und sch\u00e4ndet dein Haupt! Gr\u00fcnt er Aeonenlang,", "tokens": ["Welkt", ",", "und", "sch\u00e4n\u00b7det", "dein", "Haupt", "!", "Gr\u00fcnt", "er", "A\u00b7e\u00b7o\u00b7nen\u00b7lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.11": {"text": "Keiner neidete solchen Kranz,", "tokens": ["Kei\u00b7ner", "nei\u00b7de\u00b7te", "sol\u00b7chen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Welcher tr\u00e4ufelt von Tod! Reue der Seelenwurm", "tokens": ["Wel\u00b7cher", "tr\u00e4u\u00b7felt", "von", "Tod", "!", "Reu\u00b7e", "der", "See\u00b7len\u00b7wurm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "VVFIN", "APPR", "NN", "$.", "NN", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.13": {"text": "Rei\u00dft vom Schlummer sich j\u00e4hlings auf,", "tokens": ["Rei\u00dft", "vom", "Schlum\u00b7mer", "sich", "j\u00e4h\u00b7lings", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Und zerw\u00fchlet sein Herz! T\u00f6chter des Sch\u00f6pferhauchs,", "tokens": ["Und", "zer\u00b7w\u00fch\u00b7let", "sein", "Herz", "!", "T\u00f6ch\u00b7ter", "des", "Sch\u00f6p\u00b7fer\u00b7hauchs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "M\u00e4dchenseelen, durch ihn verf\u00fchrt,", "tokens": ["M\u00e4d\u00b7chen\u00b7see\u00b7len", ",", "durch", "ihn", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Wimmern gegen ihn hin, wimmern und starren Fluch,", "tokens": ["Wim\u00b7mern", "ge\u00b7gen", "ihn", "hin", ",", "wim\u00b7mern", "und", "star\u00b7ren", "Fluch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Kommt sein eiserner Todestag.", "tokens": ["Kommt", "sein", "ei\u00b7ser\u00b7ner", "To\u00b7des\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Jedes Buhlergelust, das er entz\u00fcndete,", "tokens": ["Je\u00b7des", "Buh\u00b7ler\u00b7ge\u00b7lust", ",", "das", "er", "ent\u00b7z\u00fcn\u00b7de\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Wird zum Teufel, und gei\u00dfelt ihn", "tokens": ["Wird", "zum", "Teu\u00b7fel", ",", "und", "gei\u00b7\u00dfelt", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Auf dem Lager der Rast, bis er den Geist verhaucht.", "tokens": ["Auf", "dem", "La\u00b7ger", "der", "Rast", ",", "bis", "er", "den", "Geist", "ver\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "S\u00fcnder, st\u00fcrzeten Str\u00f6me Bluts", "tokens": ["S\u00fcn\u00b7der", ",", "st\u00fcr\u00b7ze\u00b7ten", "Str\u00f6\u00b7me", "Bluts"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Deine Wangen herab, st\u00fcrzeten, bis des Tods", "tokens": ["Dei\u00b7ne", "Wan\u00b7gen", "her\u00b7ab", ",", "st\u00fcr\u00b7ze\u00b7ten", ",", "bis", "des", "Tods"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.23": {"text": "Eisenklauigte W\u00fcrgerfaust", "tokens": ["Ei\u00b7sen\u00b7klau\u00b7ig\u00b7te", "W\u00fcr\u00b7ger\u00b7faust"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.24": {"text": "Dr\u00e4uend \u00fcber dir schwebt, S\u00fcnder, sie weinten nicht", "tokens": ["Dr\u00e4u\u00b7end", "\u00fc\u00b7ber", "dir", "schwebt", ",", "S\u00fcn\u00b7der", ",", "sie", "wein\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "PTKNEG"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Deiner Frevel den kleinsten aus!", "tokens": ["Dei\u00b7ner", "Fre\u00b7vel", "den", "kleins\u00b7ten", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "Freunde beuget das Knie, danket zu Gott hinauf,", "tokens": ["Freun\u00b7de", "beu\u00b7get", "das", "Knie", ",", "dan\u00b7ket", "zu", "Gott", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.27": {"text": "Da\u00df kein Tropfen des Seelengifts", "tokens": ["Da\u00df", "kein", "Trop\u00b7fen", "des", "See\u00b7len\u00b7gifts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Rann aus euren Gesang, danket zu Gott hinauf!", "tokens": ["Rann", "aus", "eu\u00b7ren", "Ge\u00b7sang", ",", "dan\u00b7ket", "zu", "Gott", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}}}}}