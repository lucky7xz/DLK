{"textgrid.poem.32529": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "X. Der L\u00f6we und die M\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Held vom muntern Heere,", "tokens": ["Ein", "jun\u00b7ger", "Held", "vom", "mun\u00b7tern", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das nur der Sonnenschein belebt,", "tokens": ["Das", "nur", "der", "Son\u00b7nen\u00b7schein", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und das mit saugendem Gewehre", "tokens": ["Und", "das", "mit", "sau\u00b7gen\u00b7dem", "Ge\u00b7weh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Ruhm gestochner Beulen strebt,", "tokens": ["Nach", "Ruhm", "ge\u00b7stoch\u00b7ner", "Beu\u00b7len", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch die man noch zum gro\u00dfen Gl\u00fccke,", "tokens": ["Doch", "die", "man", "noch", "zum", "gro\u00b7\u00dfen", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch zwei Paar Str\u00fcmpfe hindern kann,", "tokens": ["Durch", "zwei", "Paar", "Str\u00fcmp\u00b7fe", "hin\u00b7dern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der junge Held war eine M\u00fccke.", "tokens": ["Der", "jun\u00b7ge", "Held", "war", "ei\u00b7ne", "M\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00f6rt meines Helden Taten an!", "tokens": ["H\u00f6rt", "mei\u00b7nes", "Hel\u00b7den", "Ta\u00b7ten", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf ihren Kreuz- und Ritterz\u00fcgen", "tokens": ["Auf", "ih\u00b7ren", "Kreuz", "und", "Rit\u00b7ter\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fand sie, entfernt von ihrer Schar,", "tokens": ["Fand", "sie", ",", "ent\u00b7fernt", "von", "ih\u00b7rer", "Schar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Schlummer einen L\u00f6wen liegen,", "tokens": ["Im", "Schlum\u00b7mer", "ei\u00b7nen", "L\u00f6\u00b7wen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der von der Jagd entkr\u00e4ftet war.", "tokens": ["Der", "von", "der", "Jagd", "ent\u00b7kr\u00e4f\u00b7tet", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seht, Schwestern, dort den L\u00f6wen schlafen,", "tokens": ["Seht", ",", "Schwes\u00b7tern", ",", "dort", "den", "L\u00f6\u00b7wen", "schla\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schrie sie die Schwestern gaukelnd an.", "tokens": ["Schrie", "sie", "die", "Schwes\u00b7tern", "gau\u00b7kelnd", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt will ich hin, und will ihn strafen.", "tokens": ["Jetzt", "will", "ich", "hin", ",", "und", "will", "ihn", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er soll mir bluten, der Tyrann!", "tokens": ["Er", "soll", "mir", "blu\u00b7ten", ",", "der", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie eilt, und mit verwegnem Sprunge", "tokens": ["Sie", "eilt", ",", "und", "mit", "ver\u00b7weg\u00b7nem", "Sprun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Setzt sie sich auf des K\u00f6nigs Schwanz.", "tokens": ["Setzt", "sie", "sich", "auf", "des", "K\u00f6\u00b7nigs", "Schwanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sticht, und flieht mit schnellem Schwunge,", "tokens": ["Sie", "sticht", ",", "und", "flieht", "mit", "schnel\u00b7lem", "Schwun\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz auf den sauern Lorbeerkranz.", "tokens": ["Stolz", "auf", "den", "sau\u00b7ern", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der L\u00f6we will sich nicht bewegen?", "tokens": ["Der", "L\u00f6\u00b7we", "will", "sich", "nicht", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie? ist er tot? Das hei\u00df ich Wut!", "tokens": ["Wie", "?", "ist", "er", "tot", "?", "Das", "hei\u00df", "ich", "Wut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "ADJD", "$.", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu m\u00f6rdrisch war der M\u00fccke Degen:", "tokens": ["Zu", "m\u00f6rd\u00b7risch", "war", "der", "M\u00fc\u00b7cke", "De\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch sagt, ob er nicht Wunder tut?", "tokens": ["Doch", "sagt", ",", "ob", "er", "nicht", "Wun\u00b7der", "tut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bblch bin es, die den Wald befreiet,", "tokens": ["\u00bb", "lch", "bin", "es", ",", "die", "den", "Wald", "be\u00b7frei\u00b7et", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo seine Mordsucht sonst getobt.", "tokens": ["Wo", "sei\u00b7ne", "Mord\u00b7sucht", "sonst", "ge\u00b7tobt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht, Schwestern, den der Tiger scheuet,", "tokens": ["Seht", ",", "Schwes\u00b7tern", ",", "den", "der", "Ti\u00b7ger", "scheu\u00b7et", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der stirbt! Mein Stachel sei gelobt!\u00ab", "tokens": ["Der", "stirbt", "!", "Mein", "Sta\u00b7chel", "sei", "ge\u00b7lobt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schwestern jauchzen, voll Vergn\u00fcgen,", "tokens": ["Die", "Schwes\u00b7tern", "jauch\u00b7zen", ",", "voll", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um ihre laute Siegerin.", "tokens": ["Um", "ih\u00b7re", "lau\u00b7te", "Sie\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie? L\u00f6wen, L\u00f6wen zu besiegen!", "tokens": ["Wie", "?", "L\u00f6\u00b7wen", ",", "L\u00f6\u00b7wen", "zu", "be\u00b7sie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie, Schwester, kam dir das in Sinn?", "tokens": ["Wie", ",", "Schwes\u00b7ter", ",", "kam", "dir", "das", "in", "Sinn", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbja, Schwestern, wagen mu\u00df man! wagen!", "tokens": ["\u00bb", "ja", ",", "Schwes\u00b7tern", ",", "wa\u00b7gen", "mu\u00df", "man", "!", "wa\u00b7gen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "$,", "VVINF", "VMFIN", "PIS", "$.", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00e4tt' es selber nicht gedacht.", "tokens": ["Ich", "h\u00e4tt'", "es", "sel\u00b7ber", "nicht", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! lasset uns mehr Feinde schlagen.", "tokens": ["Auf", "!", "las\u00b7set", "uns", "mehr", "Fein\u00b7de", "schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Anfang ist zu sch\u00f6n gemacht.\u00ab", "tokens": ["Der", "An\u00b7fang", "ist", "zu", "sch\u00f6n", "ge\u00b7macht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch unter diesen Siegesliedern,", "tokens": ["Doch", "un\u00b7ter", "die\u00b7sen", "Sie\u00b7ges\u00b7lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da jede von Triumphen sprach,", "tokens": ["Da", "je\u00b7de", "von", "Tri\u00b7um\u00b7phen", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erwacht der matte L\u00f6we wieder,", "tokens": ["Er\u00b7wacht", "der", "mat\u00b7te", "L\u00f6\u00b7we", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und eilt erquickt dem Raube nach.", "tokens": ["Und", "eilt", "er\u00b7quickt", "dem", "Rau\u00b7be", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}