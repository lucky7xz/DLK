{"textgrid.poem.33930": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfmutter", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Ehrfurcht stand ich einst vor dir,", "tokens": ["Mit", "Ehr\u00b7furcht", "stand", "ich", "einst", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einer ernsten Stunde,", "tokens": ["In", "ei\u00b7ner", "erns\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Segen, fromm, erbat ich mir", "tokens": ["Den", "Se\u00b7gen", ",", "fromm", ",", "er\u00b7bat", "ich", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinem heil'gen Munde.", "tokens": ["Von", "dei\u00b7nem", "heil'\u00b7gen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du sahst nicht mehr, du h\u00f6rtest kaum,", "tokens": ["Du", "sahst", "nicht", "mehr", ",", "du", "h\u00f6r\u00b7test", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kalt waren deine H\u00e4nde,", "tokens": ["Kalt", "wa\u00b7ren", "dei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, sprachst du, war's, als ob im Traum", "tokens": ["Und", ",", "sprachst", "du", ",", "wa\u00b7r's", ",", "als", "ob", "im", "Traum"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "VAFIN", "$,", "KOKOM", "KOUS", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein Todter Worte f\u00e4nde.", "tokens": ["Ein", "Tod\u00b7ter", "Wor\u00b7te", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du strichst die Locken mir zur\u00fcck,", "tokens": ["Du", "strichst", "die", "Lo\u00b7cken", "mir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann frugst du manche Sachen", "tokens": ["Dann", "frugst", "du", "man\u00b7che", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und batest mich, dein letztes Gl\u00fcck", "tokens": ["Und", "ba\u00b7test", "mich", ",", "dein", "letz\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Alter noch zu machen.", "tokens": ["Im", "Al\u00b7ter", "noch", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbsie sagten mir, du w\u00e4rest todt!\u00ab", "tokens": ["\u00bb", "sie", "sag\u00b7ten", "mir", ",", "du", "w\u00e4\u00b7rest", "todt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dumpf riefst du's aus und weintest;", "tokens": ["Dumpf", "riefst", "du's", "aus", "und", "wein\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da ward mir klar in deiner Noth,", "tokens": ["Da", "ward", "mir", "klar", "in", "dei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df du den Vater meintest.", "tokens": ["Da\u00df", "du", "den", "Va\u00b7ter", "mein\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von seinem Leben sprachst du nun,", "tokens": ["Von", "sei\u00b7nem", "Le\u00b7ben", "sprachst", "du", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r's mein eig'nes Leben;", "tokens": ["Als", "w\u00e4r's", "mein", "eig'\u00b7nes", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah ihn in der Wiege ruh'n,", "tokens": ["Ich", "sah", "ihn", "in", "der", "Wie\u00b7ge", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wonne dich darneben;", "tokens": ["Mit", "Won\u00b7ne", "dich", "dar\u00b7ne\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PAV", "$."], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Ich gab durch manches sch\u00f6ne Jahr", "tokens": ["Ich", "gab", "durch", "man\u00b7ches", "sch\u00f6\u00b7ne", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ger\u00fchrt ihm das Geleite;", "tokens": ["Ge\u00b7r\u00fchrt", "ihm", "das", "Ge\u00b7lei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich sah ihn endlich am Altar,", "tokens": ["Ich", "sah", "ihn", "end\u00b7lich", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An meiner Mutter Seite.", "tokens": ["An", "mei\u00b7ner", "Mut\u00b7ter", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Manch schlichtes Gl\u00fcck erfreute ihn,", "tokens": ["Manch", "schlich\u00b7tes", "Gl\u00fcck", "er\u00b7freu\u00b7te", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wurde ihm geboren;", "tokens": ["Ich", "wur\u00b7de", "ihm", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Bruder dann; jetzt aber schien", "tokens": ["Mein", "Bru\u00b7der", "dann", ";", "jetzt", "a\u00b7ber", "schien"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Faden dir verloren.", "tokens": ["Der", "Fa\u00b7den", "dir", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du stocktest pl\u00f6tzlich, brachest ab", "tokens": ["Du", "stock\u00b7test", "pl\u00f6tz\u00b7lich", ",", "bra\u00b7chest", "ab"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und frugst, was nun gekommen,", "tokens": ["Und", "frugst", ",", "was", "nun", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich dachte an sein fr\u00fches Grab,", "tokens": ["Ich", "dach\u00b7te", "an", "sein", "fr\u00fc\u00b7hes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch schwieg ich, tief beklommen.", "tokens": ["Doch", "schwieg", "ich", ",", "tief", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du schluchztest, aufgethaut und weich,", "tokens": ["Du", "schluchz\u00b7test", ",", "auf\u00b7ge\u00b7thaut", "und", "weich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als h\u00e4tt'st du Nichts vergessen,", "tokens": ["Als", "h\u00e4tt'st", "du", "Nichts", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch begannest du zugleich,", "tokens": ["Und", "doch", "be\u00b7gan\u00b7nest", "du", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einer Frucht zu essen.", "tokens": ["Von", "ei\u00b7ner", "Frucht", "zu", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Stuhl zum Ofen schobst du dann,", "tokens": ["Den", "Stuhl", "zum", "O\u00b7fen", "schobst", "du", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich wieder einsam w\u00e4hnend,", "tokens": ["Dich", "wie\u00b7der", "ein\u00b7sam", "w\u00e4h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und fingest laut zu beten an,", "tokens": ["Und", "fin\u00b7gest", "laut", "zu", "be\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Haupt vor\u00fcber lehnend.", "tokens": ["Dein", "Haupt", "vor\u00b7\u00fc\u00b7ber", "leh\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich aber sah von fern die Zeit", "tokens": ["Ich", "a\u00b7ber", "sah", "von", "fern", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch mein schon dunkel harren,", "tokens": ["Auch", "mein", "schon", "dun\u00b7kel", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo mir die Welt Nichts weiter beut,", "tokens": ["Wo", "mir", "die", "Welt", "Nichts", "wei\u00b7ter", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Gr\u00e4ber aufzuscharren,", "tokens": ["Als", "Gr\u00e4\u00b7ber", "auf\u00b7zu\u00b7schar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und, weil dem schlotternden Gebein", "tokens": ["Und", ",", "weil", "dem", "schlot\u00b7tern\u00b7den", "Ge\u00b7bein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich noch versagt das Bette,", "tokens": ["Sich", "noch", "ver\u00b7sagt", "das", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich, selbst vergl\u00fcht, in Gottes Sein", "tokens": ["Ich", ",", "selbst", "ver\u00b7gl\u00fcht", ",", "in", "Got\u00b7tes", "Sein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ADV", "VVPP", "$,", "APPR", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich still hin\u00fcber rette.", "tokens": ["Mich", "still", "hin\u00b7\u00fc\u00b7ber", "ret\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}