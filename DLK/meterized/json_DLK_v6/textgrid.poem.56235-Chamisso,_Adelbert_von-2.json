{"textgrid.poem.56235": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen str\u00f6mt, die Sonne scheint,", "tokens": ["Der", "Re\u00b7gen", "str\u00f6mt", ",", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht bergauf, es geht bergab, \u2013", "tokens": ["Es", "geht", "ber\u00b7gauf", ",", "es", "geht", "berg\u00b7ab", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich denke sie, die mich nur meint,", "tokens": ["Ich", "den\u00b7ke", "sie", ",", "die", "mich", "nur", "meint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie, die mir ihre Treue gab.", "tokens": ["Sie", ",", "die", "mir", "ih\u00b7re", "Treu\u00b7e", "gab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was gehst du suchend durch das Land,", "tokens": ["Was", "gehst", "du", "su\u00b7chend", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du M\u00fcder mit ergrautem Bart? \u2013", "tokens": ["Du", "M\u00fc\u00b7der", "mit", "er\u00b7grau\u00b7tem", "Bart", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich suche nicht, was ich schon fand,", "tokens": ["Ich", "su\u00b7che", "nicht", ",", "was", "ich", "schon", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich suche nicht, was mir schon ward.", "tokens": ["Ich", "su\u00b7che", "nicht", ",", "was", "mir", "schon", "ward", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin noch frisch, ich bin noch jung,", "tokens": ["Ich", "bin", "noch", "frisch", ",", "ich", "bin", "noch", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist kalt und ohne Lust,", "tokens": ["Die", "Welt", "ist", "kalt", "und", "oh\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab daheim der Freude genung,", "tokens": ["Ich", "hab", "da\u00b7heim", "der", "Freu\u00b7de", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird mir warm an ihrer Brust.", "tokens": ["Es", "wird", "mir", "warm", "an", "ih\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}