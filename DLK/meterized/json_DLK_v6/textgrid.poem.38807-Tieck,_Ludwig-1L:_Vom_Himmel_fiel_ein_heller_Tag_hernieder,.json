{"textgrid.poem.38807": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Himmel fiel ein heller Tag hernieder,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Himmel fiel ein heller Tag hernieder,", "tokens": ["Vom", "Him\u00b7mel", "fiel", "ein", "hel\u00b7ler", "Tag", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinem Blumenschmucke angethan", "tokens": ["Mit", "sei\u00b7nem", "Blu\u00b7men\u00b7schmu\u00b7cke", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War schon der gr\u00fcne Wiesenplan,", "tokens": ["War", "schon", "der", "gr\u00fc\u00b7ne", "Wie\u00b7sen\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Nachtigall sang ihre Sommerlieder,", "tokens": ["Und", "Nach\u00b7ti\u00b7gall", "sang", "ih\u00b7re", "Som\u00b7mer\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da trat ein holdes Kind in diese Welt,", "tokens": ["Da", "trat", "ein", "hol\u00b7des", "Kind", "in", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00e4chelte und freute sich im bunten Fr\u00fchlingszelt.", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "und", "freu\u00b7te", "sich", "im", "bun\u00b7ten", "Fr\u00fch\u00b7lings\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Die zarten Geister, die in den Kristallen", "tokens": ["Die", "zar\u00b7ten", "Geis\u00b7ter", ",", "die", "in", "den", "Kris\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Fl\u00fcsse wohnen, die in Luft,", "tokens": ["Der", "Fl\u00fcs\u00b7se", "woh\u00b7nen", ",", "die", "in", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Sommerwind, in Bl\u00fcthenduft,", "tokens": ["In", "Som\u00b7mer\u00b7wind", ",", "in", "Bl\u00fc\u00b7then\u00b7duft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In k\u00fchlen Lauben, gr\u00fcnen Bl\u00e4ttern lallen,", "tokens": ["In", "k\u00fch\u00b7len", "Lau\u00b7ben", ",", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7tern", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie kamen alle spielend hergezogen,", "tokens": ["Sie", "ka\u00b7men", "al\u00b7le", "spie\u00b7lend", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sangen s\u00fc\u00dfen Gru\u00df: \u00bbwir sind dem sch\u00f6nen Kind gewogen.", "tokens": ["Und", "san\u00b7gen", "s\u00fc\u00b7\u00dfen", "Gru\u00df", ":", "\u00bb", "wir", "sind", "dem", "sch\u00f6\u00b7nen", "Kind", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$.", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Sieh, wie sich dir entgegen neigt gelinde", "tokens": ["Sieh", ",", "wie", "sich", "dir", "ent\u00b7ge\u00b7gen", "neigt", "ge\u00b7lin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PRF", "PPER", "PTKVZ", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hellen Lilien weisse Pracht,", "tokens": ["Der", "hel\u00b7len", "Li\u00b7li\u00b7en", "weis\u00b7se", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie dir entgegen Rose lacht,", "tokens": ["Wie", "dir", "ent\u00b7ge\u00b7gen", "Ro\u00b7se", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Veilchen blau, das liebliche Gesinde", "tokens": ["Das", "Veil\u00b7chen", "blau", ",", "das", "lieb\u00b7li\u00b7che", "Ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Fr\u00fchling mit sich bringt zu seinen Scherzen,", "tokens": ["Das", "Fr\u00fch\u00b7ling", "mit", "sich", "bringt", "zu", "sei\u00b7nen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Aeuglein s\u00fc\u00df, die Lippen roth, die brennend goldnen Kerzen", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "s\u00fc\u00df", ",", "die", "Lip\u00b7pen", "roth", ",", "die", "bren\u00b7nend", "gold\u00b7nen", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Sie alle dienen dir, die bunten Strahlen,", "tokens": ["Sie", "al\u00b7le", "die\u00b7nen", "dir", ",", "die", "bun\u00b7ten", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die still und z\u00e4rtlich nach dir blicken,", "tokens": ["Die", "still", "und", "z\u00e4rt\u00b7lich", "nach", "dir", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich mit lieben Duft erquicken,", "tokens": ["Und", "dich", "mit", "lie\u00b7ben", "Duft", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie alle wollen nur dein Bildni\u00df mahlen;", "tokens": ["Sie", "al\u00b7le", "wol\u00b7len", "nur", "dein", "Bild\u00b7ni\u00df", "mah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch Anmuth und den Reiz die dich umschweben,", "tokens": ["Doch", "An\u00b7muth", "und", "den", "Reiz", "die", "dich", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ART", "NN", "ART", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ermangeln sie der Mahlerkunst, drum stirbt ihr fl\u00fcchtig Leben.", "tokens": ["Er\u00b7man\u00b7geln", "sie", "der", "Mah\u00b7ler\u00b7kunst", ",", "drum", "stirbt", "ihr", "fl\u00fcch\u00b7tig", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PAV", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Wir schenken dir der Nachtigallen Kl\u00e4nge,", "tokens": ["Wir", "schen\u00b7ken", "dir", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du mit zartem Lautenspiele,", "tokens": ["Da\u00df", "du", "mit", "zar\u00b7tem", "Lau\u00b7ten\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kannst singen deiner Brust Gef\u00fchle,", "tokens": ["Kannst", "sin\u00b7gen", "dei\u00b7ner", "Brust", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gern vernimmst der Dichtenden Ges\u00e4nge.", "tokens": ["Und", "gern", "ver\u00b7nimmst", "der", "Dich\u00b7ten\u00b7den", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum horche deines Dichters Weisen,", "tokens": ["Drum", "hor\u00b7che", "dei\u00b7nes", "Dich\u00b7ters", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird er des Liebesgotts Gewalt und deine Sch\u00f6nheit preisen.\u00ab", "tokens": ["Wird", "er", "des", "Lie\u00b7bes\u00b7gotts", "Ge\u00b7walt", "und", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "prei\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}