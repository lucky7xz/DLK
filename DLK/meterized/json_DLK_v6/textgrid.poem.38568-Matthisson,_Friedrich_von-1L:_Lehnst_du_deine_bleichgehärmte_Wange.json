{"textgrid.poem.38568": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lehnst du deine bleichgeh\u00e4rmte Wange", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lehnst du deine bleichgeh\u00e4rmte Wange", "tokens": ["Lehnst", "du", "dei\u00b7ne", "bleich\u00b7ge\u00b7h\u00e4rm\u00b7te", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Immer noch an diesen Aschenkrug?", "tokens": ["Im\u00b7mer", "noch", "an", "die\u00b7sen", "A\u00b7schen\u00b7krug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und beweinst den Todten, den schon lange", "tokens": ["Und", "be\u00b7weinst", "den", "Tod\u00b7ten", ",", "den", "schon", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zu der Seraphim Triumphgesange", "tokens": ["Zu", "der", "Se\u00b7ra\u00b7phim", "Tri\u00b7umph\u00b7ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der Vollendung Fl\u00fcgel trug?", "tokens": ["Der", "Vol\u00b7len\u00b7dung", "Fl\u00fc\u00b7gel", "trug", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Siehst du Gottes Sternenschrift dort flimmern,", "tokens": ["Siehst", "du", "Got\u00b7tes", "Ster\u00b7nen\u00b7schrift", "dort", "flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die der bangen Schwermuth Trost verhei\u00dft?", "tokens": ["Die", "der", "ban\u00b7gen", "Schwer\u00b7muth", "Trost", "ver\u00b7hei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heller wird der Glaube nun dir schimmern,", "tokens": ["Hel\u00b7ler", "wird", "der", "Glau\u00b7be", "nun", "dir", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df hoch \u00fcber seiner H\u00fclle Tr\u00fcmmern", "tokens": ["Da\u00df", "hoch", "\u00fc\u00b7ber", "sei\u00b7ner", "H\u00fcl\u00b7le", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Walle des Geliebten Geist!", "tokens": ["Wal\u00b7le", "des", "Ge\u00b7lieb\u00b7ten", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seelen, die den Kelch des Glaubens tranken", "tokens": ["See\u00b7len", ",", "die", "den", "Kelch", "des", "Glau\u00b7bens", "tran\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann ihr Pfad in Dunkel sich verlor,", "tokens": ["Wann", "ihr", "Pfad", "in", "Dun\u00b7kel", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Steigen aus der Schwermuth finstern Schranken,", "tokens": ["Stei\u00b7gen", "aus", "der", "Schwer\u00b7muth", "fins\u00b7tern", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie auf Adlersfl\u00fcgeln, zum Gedanken", "tokens": ["Wie", "auf", "Ad\u00b7ler\u00b7sfl\u00fc\u00b7geln", ",", "zum", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "APPRART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Der Unsterblichkeit empor!", "tokens": ["Der", "U\u00b7nsterb\u00b7lich\u00b7keit", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wohl, o wohl dem liebenden Gef\u00e4hrten", "tokens": ["Wohl", ",", "o", "wohl", "dem", "lie\u00b7ben\u00b7den", "Ge\u00b7f\u00e4hr\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Sehnsucht, er ist ewig dein!", "tokens": ["Dei\u00b7ner", "Sehn\u00b7sucht", ",", "er", "ist", "e\u00b7wig", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wiedersehn, im Lande der Verkl\u00e4rten,", "tokens": ["Wie\u00b7der\u00b7sehn", ",", "im", "Lan\u00b7de", "der", "Ver\u00b7kl\u00e4r\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wirst du, Dulderin, den Langentbehrten", "tokens": ["Wirst", "du", ",", "Dul\u00b7de\u00b7rin", ",", "den", "Lan\u00b7gent\u00b7behr\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und wie er unsterblich seyn!", "tokens": ["Und", "wie", "er", "uns\u00b7terb\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}