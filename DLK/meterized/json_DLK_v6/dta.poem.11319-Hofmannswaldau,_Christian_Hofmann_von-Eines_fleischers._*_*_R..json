{"dta.poem.11319": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines fleischers.  \n * *  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schaf', ochsen, kalb und schwein, die haben offt geschmeckt,", "tokens": ["Schaf'", ",", "och\u00b7sen", ",", "kalb", "und", "schwein", ",", "die", "ha\u00b7ben", "offt", "ge\u00b7schmeckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "PRELS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie t\u00f6dlich meine faust im leben sey gewesen.", "tokens": ["Wie", "t\u00f6d\u00b7lich", "mei\u00b7ne", "faust", "im", "le\u00b7ben", "sey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "APPRART", "VVINF", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du, leser! gehe fort, so bald du di\u00df gelesen,", "tokens": ["Du", ",", "le\u00b7ser", "!", "ge\u00b7he", "fort", ",", "so", "bald", "du", "di\u00df", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$.", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh sich noch meine hand nach deiner gurgel streckt.", "tokens": ["Eh", "sich", "noch", "mei\u00b7ne", "hand", "nach", "dei\u00b7ner", "gur\u00b7gel", "streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}