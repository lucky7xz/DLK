{"dta.poem.14173": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Verlorene Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Lieder schweigen jetzt und Klagen,", "tokens": ["Lie\u00b7der", "schwei\u00b7gen", "jetzt", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun will ich erst fr\u00f6hlich sein,", "tokens": ["Nun", "will", "ich", "erst", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "All mein Leid will ich zerschlagen", "tokens": ["All", "mein", "Leid", "will", "ich", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Erinnern \u2014 gebt mir Wein!", "tokens": ["Und", "E\u00b7rin\u00b7nern", "gebt", "mir", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "VVFIN", "PPER", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie er mir verlockend spiegelt", "tokens": ["Wie", "er", "mir", "ver\u00b7lo\u00b7ckend", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterne und der Erde Lust,", "tokens": ["Ster\u00b7ne", "und", "der", "Er\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stillgesch\u00e4ftig dann entriegelt", "tokens": ["Still\u00b7ge\u00b7sch\u00e4f\u00b7tig", "dann", "ent\u00b7rie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "All' die Teufel in der Brust,", "tokens": ["All'", "die", "Teu\u00b7fel", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Erst der Knecht und dann der Meister", "tokens": ["Erst", "der", "Knecht", "und", "dann", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bricht er durch die Nacht herein,", "tokens": ["Bricht", "er", "durch", "die", "Nacht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wildester der L\u00fcgengeister,", "tokens": ["Wil\u00b7des\u00b7ter", "der", "L\u00fc\u00b7gen\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Ring' mit mir, ich lache dein!", "tokens": ["Ring'", "mit", "mir", ",", "ich", "la\u00b7che", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und den Becher voll Entsetzen", "tokens": ["Und", "den", "Be\u00b7cher", "voll", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Werf' ich in des Stromes Grund,", "tokens": ["Wer\u00b7f'", "ich", "in", "des", "Stro\u00b7mes", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df sich nimmer dran soll letzen", "tokens": ["Da\u00df", "sich", "nim\u00b7mer", "dran", "soll", "let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "PAV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wer noch fr\u00f6hlich und gesund!", "tokens": ["Wer", "noch", "fr\u00f6h\u00b7lich", "und", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}