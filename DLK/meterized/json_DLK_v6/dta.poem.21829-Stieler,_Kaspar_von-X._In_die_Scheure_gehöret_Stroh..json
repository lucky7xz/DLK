{"dta.poem.21829": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  In die Scheure geh\u00f6ret Stroh.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ne\u00e4re h\u00e4lt sich wakker/\nsie schm\u00fckt und schminket sich.", "tokens": ["Ne\u00e4\u00b7re", "h\u00e4lt", "sich", "wak\u00b7ker", "/", "sie", "schm\u00fckt", "und", "schmin\u00b7ket", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADJD", "$(", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du alter Knochen-knakker/", "tokens": ["Du", "al\u00b7ter", "Kno\u00b7chen\u00b7knak\u00b7ker", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "meinstu/ es sey vor dich?", "tokens": ["meins\u00b7tu", "/", "es", "sey", "vor", "dich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nein/ Chremes/ la\u00df dich lehren:", "tokens": ["Nein", "/", "Chre\u00b7mes", "/", "la\u00df", "dich", "leh\u00b7ren", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "NE", "$(", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man pfleget also nicht", "tokens": ["Man", "pfle\u00b7get", "al\u00b7so", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "den Husten zu verehren.", "tokens": ["den", "Hus\u00b7ten", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was anders vor die Gicht.", "tokens": ["Was", "an\u00b7ders", "vor", "die", "Gicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und w\u00e4re sie voll Runzeln/", "tokens": ["Und", "w\u00e4\u00b7re", "sie", "voll", "Run\u00b7zeln", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "von schwarz-begeelter Haut", "tokens": ["von", "schwa\u00b7rz\u00b7be\u00b7ge\u00b7el\u00b7ter", "Haut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "beliebt/ als Schweine schmunzeln", "tokens": ["be\u00b7liebt", "/", "als", "Schwei\u00b7ne", "schmun\u00b7zeln"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$(", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und w\u00e4re dir getraut;", "tokens": ["und", "w\u00e4\u00b7re", "dir", "ge\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "dir dennoch viel zu sch\u00f6n.", "tokens": ["dir", "den\u00b7noch", "viel", "zu", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dem Teufel m\u00f6cht\u2019 auch grauen/", "tokens": ["Dem", "Teu\u00b7fel", "m\u00f6cht'", "auch", "grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Holz/ mit dir umzugehn.", "tokens": ["Holz", "/", "mit", "dir", "um\u00b7zu\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PPER", "VVIZU", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Was soll der Kuh Muskaten/", "tokens": ["Was", "soll", "der", "Kuh", "Mus\u00b7ka\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kaneel/ Konfekt dem Schwein\u2019/", "tokens": ["Ka\u00b7ne\u00b7el", "/", "Kon\u00b7fekt", "dem", "Schwein'", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und Hunden Hirschen-braten?", "tokens": ["und", "Hun\u00b7den", "Hirschen\u00b7bra\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "ein abgefleischtes Bein", "tokens": ["ein", "ab\u00b7ge\u00b7fleischtes", "Bein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "ist gut f\u00fcr ihren Hunger.", "tokens": ["ist", "gut", "f\u00fcr", "ih\u00b7ren", "Hun\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das gleiche findet sich.", "tokens": ["Das", "glei\u00b7che", "fin\u00b7det", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die deine liebt ein Junger.", "tokens": ["Die", "dei\u00b7ne", "liebt", "ein", "Jun\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Vettel ist f\u00fcr dich.", "tokens": ["Die", "Vet\u00b7tel", "ist", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du sizzest auff der Schleuder", "tokens": ["Du", "siz\u00b7zest", "auff", "der", "Schleu\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und bist ein guter Mann.", "tokens": ["und", "bist", "ein", "gu\u00b7ter", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jhr schaffstu Sammet-Kleider/", "tokens": ["Ihr", "schaffs\u00b7tu", "Sam\u00b7met\u00b7Klei\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dir stehn die Federn an.", "tokens": ["dir", "stehn", "die", "Fe\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gib her die alten Tahler:", "tokens": ["Gib", "her", "die", "al\u00b7ten", "Tah\u00b7ler", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "auch ich bed\u00fcrff iezt Geld.", "tokens": ["auch", "ich", "be\u00b7d\u00fcrff", "iezt", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein grauer W\u00f6rter-Prahler", "tokens": ["Ein", "grau\u00b7er", "W\u00f6r\u00b7ter\u00b7Prah\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "dient nicht ins Feder-Feld.", "tokens": ["dient", "nicht", "ins", "Fe\u00b7der\u00b7Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Chremes/ sey geduldig/", "tokens": ["Mein", "Chre\u00b7mes", "/", "sey", "ge\u00b7dul\u00b7dig", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "es findt sich einer wol/", "tokens": ["es", "findt", "sich", "ei\u00b7ner", "wol", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der/ was du Jhr bist schuldig/", "tokens": ["der", "/", "was", "du", "Ihr", "bist", "schul\u00b7dig", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der Frauen zahlen soll.", "tokens": ["der", "Frau\u00b7en", "zah\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du kanst es leicht gedenken/", "tokens": ["Du", "kanst", "es", "leicht", "ge\u00b7den\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "es machs ein gelbes Haar/", "tokens": ["es", "machs", "ein", "gel\u00b7bes", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "da\u00df sie sich wil behenken", "tokens": ["da\u00df", "sie", "sich", "wil", "be\u00b7hen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "mit Gold und Seiden-wahr\u2019.", "tokens": ["mit", "Gold", "und", "Sei\u00b7den\u00b7wahr'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jhr habt ia sonst der Sparren/", "tokens": ["Ihr", "habt", "i\u00b7a", "sonst", "der", "Spar\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ihr Alten/ allzuviel", "tokens": ["ihr", "Al\u00b7ten", "/", "all\u00b7zu\u00b7viel"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$(", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wie seid ihr denn so Narren", "tokens": ["wie", "seid", "ihr", "denn", "so", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und sucht der Liebe Spiel", "tokens": ["und", "sucht", "der", "Lie\u00b7be", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "bey einem frischem Feinde/", "tokens": ["bey", "ei\u00b7nem", "fri\u00b7schem", "Fein\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die Jhr entkr\u00e4fftet seid?", "tokens": ["die", "Ihr", "ent\u00b7kr\u00e4ff\u00b7tet", "seid", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "doch was? Jhr suchet Freunde", "tokens": ["doch", "was", "?", "Ihr", "su\u00b7chet", "Freun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "in Eurer Winters Zeit.", "tokens": ["in", "Eu\u00b7rer", "Win\u00b7ters", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}