{"dta.poem.20747": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Habsucht der K\u00f6nige .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vater alles Zanks ist das verha\u00dfte Mein,                 ", "tokens": ["Der", "Va\u00b7ter", "al\u00b7les", "Zanks", "ist", "das", "ver\u00b7ha\u00df\u00b7te", "Mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gepaaret mit dem bittern Dein!", "tokens": ["Ge\u00b7paa\u00b7ret", "mit", "dem", "bit\u00b7tern", "Dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An einer halben Welt hat Geizes voll ihr K\u00f6nig", "tokens": ["An", "ei\u00b7ner", "hal\u00b7ben", "Welt", "hat", "Gei\u00b7zes", "voll", "ihr", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch seiner Erde Raum zu wenig.", "tokens": ["Noch", "sei\u00b7ner", "Er\u00b7de", "Raum", "zu", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er fragt nach G\u00f6ttern nichts, auch nicht nach V\u00f6lker-", "tokens": ["Er", "fragt", "nach", "G\u00f6t\u00b7tern", "nichts", ",", "auch", "nicht", "nach", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIS", "$,", "ADV", "PTKNEG", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Recht,", "tokens": ["Recht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Aus Ehrgeiz schont er nicht das menschliche Geschlecht.", "tokens": ["Aus", "Ehr\u00b7geiz", "schont", "er", "nicht", "das", "menschli\u00b7che", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Er siehet hundert tausend Leben", "tokens": ["Er", "sie\u00b7het", "hun\u00b7dert", "tau\u00b7send", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In einem Treffen aufgegeben,", "tokens": ["In", "ei\u00b7nem", "Tref\u00b7fen", "auf\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und sieht die Mordthat nicht. Der Himmel sieht sie", "tokens": ["Und", "sieht", "die", "Mord\u00b7that", "nicht", ".", "Der", "Him\u00b7mel", "sieht", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "scharf.", "tokens": ["scharf", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Heil sey dem K\u00f6nig, der nicht mit ihm rechten darf!", "tokens": ["Heil", "sey", "dem", "K\u00f6\u00b7nig", ",", "der", "nicht", "mit", "ihm", "rech\u00b7ten", "darf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}