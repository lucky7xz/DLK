{"textgrid.poem.41908": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die Th\u00e4ler und \u00fcber die H\u00f6hn", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Th\u00e4ler und \u00fcber die H\u00f6hn", "tokens": ["Durch", "die", "Th\u00e4\u00b7ler", "und", "\u00fc\u00b7ber", "die", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Flieh' ich so leicht wie des Windes Wehn;", "tokens": ["Flieh'", "ich", "so", "leicht", "wie", "des", "Win\u00b7des", "Wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Von der Wiese, von Busch und Baum", "tokens": ["Von", "der", "Wie\u00b7se", ",", "von", "Busch", "und", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Streif' ich die thauigen Tropfen kaum.", "tokens": ["Streif'", "ich", "die", "thau\u00b7i\u00b7gen", "Trop\u00b7fen", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Duftige Bl\u00e4tter und schwellendes Gr\u00fcn", "tokens": ["Duf\u00b7ti\u00b7ge", "Bl\u00e4t\u00b7ter", "und", "schwel\u00b7len\u00b7des", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Pfl\u00fcck' ich mir ab im Vor\u00fcberfliehn,", "tokens": ["Pfl\u00fcck", "ich", "mir", "ab", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Hole vom Bach mir den k\u00fchligen Trank,", "tokens": ["Ho\u00b7le", "vom", "Bach", "mir", "den", "k\u00fch\u00b7li\u00b7gen", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Bade die Glieder, so glatt und schlank.", "tokens": ["Ba\u00b7de", "die", "Glie\u00b7der", ",", "so", "glatt", "und", "schlank", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Quelle, wie rieselst du rasch im Hain!", "tokens": ["Quel\u00b7le", ",", "wie", "rie\u00b7selst", "du", "rasch", "im", "Hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hole das fl\u00fcchtige Reh doch ein!", "tokens": ["Ho\u00b7le", "das", "fl\u00fcch\u00b7ti\u00b7ge", "Reh", "doch", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Quelle, wie blitzest du licht und klar!", "tokens": ["Quel\u00b7le", ",", "wie", "blit\u00b7zest", "du", "licht", "und", "klar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "NN", "KON", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lichter noch blitzet mein Augenpaar!", "tokens": ["Lich\u00b7ter", "noch", "blit\u00b7zet", "mein", "Au\u00b7gen\u00b7paar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Fr\u00fchlingss\u00e4useln und Morgenstrahl", "tokens": ["Fr\u00fch\u00b7lings\u00b7s\u00e4u\u00b7seln", "und", "Mor\u00b7gen\u00b7strahl"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Spielen so lustig in Wald und Thal;", "tokens": ["Spie\u00b7len", "so", "lus\u00b7tig", "in", "Wald", "und", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie sie spielen, so spiel' ich auch", "tokens": ["Wie", "sie", "spie\u00b7len", ",", "so", "spiel'", "ich", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit den Gesellen durch Busch und Strauch.", "tokens": ["Mit", "den", "Ge\u00b7sel\u00b7len", "durch", "Busch", "und", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Lieber J\u00e4ger, o la\u00df uns gehn,", "tokens": ["Lie\u00b7ber", "J\u00e4\u00b7ger", ",", "o", "la\u00df", "uns", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "FM", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "M\u00f6chten gern mehr von der Welt noch sehn,", "tokens": ["M\u00f6ch\u00b7ten", "gern", "mehr", "von", "der", "Welt", "noch", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lebten noch gar zu kurze Zeit,", "tokens": ["Leb\u00b7ten", "noch", "gar", "zu", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Thaten ja Keinem noch was zu Leid.", "tokens": ["Tha\u00b7ten", "ja", "Kei\u00b7nem", "noch", "was", "zu", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "ADV", "PIS", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Hast du ein Liebchen, so bring's herbey,", "tokens": ["Hast", "du", "ein", "Lieb\u00b7chen", ",", "so", "bring's", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo wir spielen so frisch und frey,", "tokens": ["Wo", "wir", "spie\u00b7len", "so", "frisch", "und", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df dich das bl\u00fchende Liebchen k\u00fc\u00dft,", "tokens": ["Da\u00df", "dich", "das", "bl\u00fc\u00b7hen\u00b7de", "Lieb\u00b7chen", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freuet sich, da\u00df du ein J\u00e4ger bist.", "tokens": ["Freu\u00b7et", "sich", ",", "da\u00df", "du", "ein", "J\u00e4\u00b7ger", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}