{"dta.poem.14963": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die andern streckten die Schwingen", "tokens": ["Die", "an\u00b7dern", "streck\u00b7ten", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In den Morgenglanz hinaus,", "tokens": ["In", "den", "Mor\u00b7gen\u00b7glanz", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und h\u00f6rten die Engel singen,", "tokens": ["Und", "h\u00f6r\u00b7ten", "die", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und flogen jauchzend nach Haus!", "tokens": ["Und", "flo\u00b7gen", "jauch\u00b7zend", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}