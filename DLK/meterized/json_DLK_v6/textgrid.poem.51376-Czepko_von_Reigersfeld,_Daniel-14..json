{"textgrid.poem.51376": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttin, du bist Stall und Stein,", "tokens": ["G\u00f6t\u00b7tin", ",", "du", "bist", "Stall", "und", "Stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sol ich meine Lieb und Pein", "tokens": ["Sol", "ich", "mei\u00b7ne", "Lieb", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dein hartes Hertze graben,", "tokens": ["In", "dein", "har\u00b7tes", "Hert\u00b7ze", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So mu\u00df ich was h\u00e4rters haben.", "tokens": ["So", "mu\u00df", "ich", "was", "h\u00e4r\u00b7ters", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist h\u00e4rteres als du:", "tokens": ["Was", "ist", "h\u00e4r\u00b7te\u00b7res", "als", "du", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mich reitzt so starck dazu.", "tokens": ["Das", "mich", "reitzt", "so", "starck", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es sind die standhafften Flammen,", "tokens": ["Es", "sind", "die", "stand\u00b7haff\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die aus Witz und Tugend stammen.", "tokens": ["Die", "aus", "Witz", "und", "Tu\u00b7gend", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es bringt einen Azoth mir", "tokens": ["Es", "bringt", "ei\u00b7nen", "A\u00b7zoth", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Best\u00e4ndigkeit herf\u00fcr:", "tokens": ["Die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der wird dich voll Lieb und Lachen", "tokens": ["Der", "wird", "dich", "voll", "Lieb", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Rauer Felsen m\u00fcrbe machen.", "tokens": ["Rau\u00b7er", "Fel\u00b7sen", "m\u00fcr\u00b7be", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so m\u00fcrbe, da\u00df man mich", "tokens": ["Und", "so", "m\u00fcr\u00b7be", ",", "da\u00df", "man", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "$,", "KOUS", "PIS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6ttin, oder selber dich,", "tokens": ["G\u00f6t\u00b7tin", ",", "o\u00b7der", "sel\u00b7ber", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dir w\u00fcrd abdrucken k\u00f6nnen,", "tokens": ["In", "dir", "w\u00fcrd", "ab\u00b7dru\u00b7cken", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch du must mich lieb gewinnen.", "tokens": ["Doch", "du", "must", "mich", "lieb", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}