{"textgrid.poem.56841": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flockendichte Winternacht ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flockendichte Winternacht ...", "tokens": ["Flo\u00b7cken\u00b7dich\u00b7te", "Win\u00b7ter\u00b7nacht", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimkehr von der Schenke ...", "tokens": ["Heim\u00b7kehr", "von", "der", "Schen\u00b7ke", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Stilles Einsamwandern macht,", "tokens": ["Stil\u00b7les", "Ein\u00b7sam\u00b7wan\u00b7dern", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df ich deiner denke.", "tokens": ["da\u00df", "ich", "dei\u00b7ner", "den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schau dich fern im dunklen Raum", "tokens": ["Schau", "dich", "fern", "im", "dunk\u00b7len", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ruhn in bleichen Linnen ...", "tokens": ["ruhn", "in", "blei\u00b7chen", "Lin\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leb ich wohl in deinem Traum", "tokens": ["Leb", "ich", "wohl", "in", "dei\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ganz geheim tiefinnen? ...", "tokens": ["ganz", "ge\u00b7heim", "tie\u00b7fin\u00b7nen", "?", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Stilles Einsamwandern macht,", "tokens": ["Stil\u00b7les", "Ein\u00b7sam\u00b7wan\u00b7dern", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ich nach dir leide ...", "tokens": ["da\u00df", "ich", "nach", "dir", "lei\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "fl\u00fcstert um uns beide ...", "tokens": ["fl\u00fcs\u00b7tert", "um", "uns", "bei\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PIS", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}