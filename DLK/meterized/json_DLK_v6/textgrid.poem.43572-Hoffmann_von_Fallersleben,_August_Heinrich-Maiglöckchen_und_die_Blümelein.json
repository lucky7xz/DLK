{"textgrid.poem.43572": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maigl\u00f6ckchen und die Bl\u00fcmelein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maigl\u00f6ckchen l\u00e4utet in dem Thal,", "tokens": ["Mai\u00b7gl\u00f6ck\u00b7chen", "l\u00e4u\u00b7tet", "in", "dem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das klingt so hell und fein:", "tokens": ["Das", "klingt", "so", "hell", "und", "fein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So kommt zum Reigen allzumal,", "tokens": ["So", "kommt", "zum", "Rei\u00b7gen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr lieben Bl\u00fcmelein!", "tokens": ["Ihr", "lie\u00b7ben", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00fcmchen blau und gelb und wei\u00df,", "tokens": ["Die", "Bl\u00fcm\u00b7chen", "blau", "und", "gelb", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kommen all' herbei;", "tokens": ["Die", "kom\u00b7men", "all'", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00dfmeinnicht und Ehrenpreis,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "Eh\u00b7ren\u00b7preis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zeitlos' und Akelei.", "tokens": ["Zeit\u00b7los'", "und", "A\u00b7kel\u00b7ei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Maigl\u00f6ckchen spielt zum Tanz im Nu", "tokens": ["Mai\u00b7gl\u00f6ck\u00b7chen", "spielt", "zum", "Tanz", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Alle tanzen dann,", "tokens": ["Und", "Al\u00b7le", "tan\u00b7zen", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mond sieht ihnen freundlich zu,", "tokens": ["Der", "Mond", "sieht", "ih\u00b7nen", "freund\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat seine Freude dran.", "tokens": ["Hat", "sei\u00b7ne", "Freu\u00b7de", "dran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Den Junker Reif verdro\u00df das sehr,", "tokens": ["Den", "Jun\u00b7ker", "Reif", "ver\u00b7dro\u00df", "das", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PDS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kommt ins Thal hinein:", "tokens": ["Er", "kommt", "ins", "Thal", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maigl\u00f6ckchen spielt zum Tanz nicht mehr,", "tokens": ["Mai\u00b7gl\u00f6ck\u00b7chen", "spielt", "zum", "Tanz", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Fort sind die Bl\u00fcmelein.", "tokens": ["Fort", "sind", "die", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Doch kaum der Reif das Thal verl\u00e4\u00dft,", "tokens": ["Doch", "kaum", "der", "Reif", "das", "Thal", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da rufet wiederum", "tokens": ["Da", "ru\u00b7fet", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maigl\u00f6ckchen zu dem Fr\u00fchlingsfest", "tokens": ["Mai\u00b7gl\u00f6ck\u00b7chen", "zu", "dem", "Fr\u00fch\u00b7lings\u00b7fest"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4utet bim bam bum.", "tokens": ["Und", "l\u00e4u\u00b7tet", "bim", "bam", "bum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun h\u00e4lt's auch mich nicht mehr zu Haus,", "tokens": ["Nun", "h\u00e4lt's", "auch", "mich", "nicht", "mehr", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Maigl\u00f6ckchen ruft auch mich:", "tokens": ["Mai\u00b7gl\u00f6ck\u00b7chen", "ruft", "auch", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Bl\u00fcmchen geh'n zum Tanz hinaus,", "tokens": ["Die", "Bl\u00fcm\u00b7chen", "geh'n", "zum", "Tanz", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Tanze geh' auch ich.", "tokens": ["Zum", "Tan\u00b7ze", "geh'", "auch", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}