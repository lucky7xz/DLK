{"textgrid.poem.40934": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Des rheinl\u00e4ndischen Hausfreundes Danksagung an Herrn Pfarrer J\u00e4ck in Triberg", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbzeig wie, Her Peter! Wenn der's Gl\u00e4sli schmeckt,", "tokens": ["\u00bb", "zeig", "wie", ",", "Her", "Pe\u00b7ter", "!", "Wenn", "der's", "Gl\u00e4s\u00b7li", "schmeckt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "KOKOM", "$,", "NN", "NE", "$.", "KOUS", "PIS", "NE", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "voll Chirsiwasser, und der Chueche dri,", "tokens": ["voll", "Chir\u00b7si\u00b7was\u00b7ser", ",", "und", "der", "Chu\u00b7e\u00b7che", "dri", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "und 's Lied vo Triberg vom Her Pfarer J\u00e4ck,", "tokens": ["und", "'s", "Lied", "vo", "Tri\u00b7berg", "vom", "Her", "Pfa\u00b7rer", "J\u00e4ck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "NE", "NE", "APPRART", "NN", "NN", "NN", "$,"], "meter": "--+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "weisch nit, was sch\u00f6n isch? Git men eim nit d'Hand,", "tokens": ["weisch", "nit", ",", "was", "sch\u00f6n", "isch", "?", "Git", "men", "eim", "nit", "d'\u00b7Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "$,", "PRELS", "ADJD", "ADJD", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "zieht's Ch\u00e4ppli ab und seit: Vergelt's ich Gott!", "tokens": ["zieht's", "Ch\u00e4pp\u00b7li", "ab", "und", "seit", ":", "Ver\u00b7gelt's", "ich", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "KON", "NN", "$.", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du nit? Und trinksch, as wenn di eigne Baum", "tokens": ["Du", "nit", "?", "Und", "trink\u00b7sch", ",", "as", "wenn", "di", "eig\u00b7ne", "Baum"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "$.", "KON", "ADJD", "$,", "NE", "KOUS", "NE", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "die Chirsi treit h\u00e4tt? Und de hesch doch kein.\u00ab", "tokens": ["die", "Chir\u00b7si", "treit", "h\u00e4tt", "?", "Und", "de", "hesch", "doch", "kein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$.", "KON", "NE", "ADJD", "ADV", "PIAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "'s isch wohr, Her J\u00e4ck, i ha kei eigne Baum,", "tokens": ["'s", "isch", "wohr", ",", "Her", "J\u00e4ck", ",", "i", "ha", "kei", "eig\u00b7ne", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,", "NN", "NN", "$,", "NE", "NE", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "i ha kei Hus, i ha kei Schof im Stall,", "tokens": ["i", "ha", "kei", "Hus", ",", "i", "ha", "kei", "Schof", "im", "Stall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "NN", "$,", "NE", "NE", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "kei Pflueg im Feld, kei Immestand im Hof,", "tokens": ["kei", "Pflu\u00b7eg", "im", "Feld", ",", "kei", "Im\u00b7me\u00b7stand", "im", "Hof", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "kei Chatz, kei H\u00fcenli, mengmol au kei Geld.", "tokens": ["kei", "Chatz", ",", "kei", "H\u00fcen\u00b7li", ",", "meng\u00b7mol", "au", "kei", "Geld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "NE", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "'s macht n\u00fct. 's isch doch im ganze Dorf kei Buur", "tokens": ["'s", "macht", "n\u00fct", ".", "'s", "isch", "doch", "im", "gan\u00b7ze", "Dorf", "kei", "Buur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "ADJD", "ADV", "APPRART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "so rich as ich. Der w\u00fc\u00dfet, wie me's macht.", "tokens": ["so", "rich", "as", "ich", ".", "Der", "w\u00fc\u00b7\u00dfet", ",", "wie", "me's", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$.", "ART", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Me meint, me heig's. So meini au, i heig's", "tokens": ["Me", "meint", ",", "me", "heig'", "s.", "So", "mei\u00b7ni", "au", ",", "i", "heig's"], "token_info": ["word", "word", "punct", "word", "word", "abbreviation", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "im s\u00fce\u00dfe Wahn, und wo ne B\u00e4umli bl\u00fceiht,", "tokens": ["im", "s\u00fce\u00b7\u00dfe", "Wahn", ",", "und", "wo", "ne", "B\u00e4um\u00b7li", "bl\u00fce\u00b7iht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "PWAV", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "'s isch mi, und wo ne Feld voll \u00c4hri schwankt,", "tokens": ["'s", "isch", "mi", ",", "und", "wo", "ne", "Feld", "voll", "\u00c4h\u00b7ri", "schwankt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "$,", "KON", "PWAV", "NE", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "'s isch au mi; wo ne S\u00e4uli Eichle fri\u00dft,", "tokens": ["'s", "isch", "au", "mi", ";", "wo", "ne", "S\u00e4u\u00b7li", "Eich\u00b7le", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "NE", "$.", "PWAV", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "es fri\u00dft sie us mim Wald.", "tokens": ["es", "fri\u00dft", "sie", "us", "mim", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "So bini rich. Doch richer bini no", "tokens": ["So", "bi\u00b7ni", "rich", ".", "Doch", "ri\u00b7cher", "bi\u00b7ni", "no"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "$.", "KON", "FM", "FM", "FM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "im Heuet, in der Ernt, im frohe Herbst.", "tokens": ["im", "Heu\u00b7et", ",", "in", "der", "Ernt", ",", "im", "fro\u00b7he", "Herbst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "I sag: \u00bbJez ch\u00f6mmet L\u00fct, wer will und mag,", "tokens": ["I", "sag", ":", "\u00bb", "Jez", "ch\u00f6m\u00b7met", "L\u00fct", ",", "wer", "will", "und", "mag", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "$.", "$(", "NE", "NE", "NE", "$,", "PWS", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "und heuet, schnidet, hauet Tr\u00fcbli ab!", "tokens": ["und", "heu\u00b7et", ",", "schni\u00b7det", ",", "hau\u00b7et", "Tr\u00fcb\u00b7li", "ab", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "I ha mi Freud an allem gha, mi Herz", "tokens": ["I", "ha", "mi", "Freud", "an", "al\u00b7lem", "gha", ",", "mi", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "NN", "APPR", "PIS", "NE", "$,", "NE", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.24": {"text": "an alle D\u00fcften, aller Sch\u00f6ni g'labt.", "tokens": ["an", "al\u00b7le", "D\u00fcf\u00b7ten", ",", "al\u00b7ler", "Sch\u00f6\u00b7ni", "g'\u00b7labt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.25": {"text": "Was \u00fcbrig isch, isch euer. Traget's heim.\u00ab", "tokens": ["Was", "\u00fcb\u00b7rig", "isch", ",", "isch", "eu\u00b7er", ".", "Tra\u00b7get's", "heim", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "ADJD", "ADJD", "$,", "FM.la", "FM.la", "$.", "NE", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Her J\u00e4ck, mir isch, der sch\u00fcttlet eue Chopf,", "tokens": ["Her", "J\u00e4ck", ",", "mir", "isch", ",", "der", "sch\u00fctt\u00b7let", "eu\u00b7e", "Chopf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADJD", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "und saget f\u00fcr ich selber: \u00bbGuete Fr\u00fcnd,", "tokens": ["und", "sa\u00b7get", "f\u00fcr", "ich", "sel\u00b7ber", ":", "\u00bb", "Gue\u00b7te", "Fr\u00fcnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$.", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "so lebt men im Schlaraffeland.\u00ab He jo,", "tokens": ["so", "lebt", "men", "im", "Schla\u00b7raf\u00b7fe\u00b7land", ".", "\u00ab", "He", "jo", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$.", "$(", "NE", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "so lebi im Schlaraffland, 's isch wohr.", "tokens": ["so", "le\u00b7bi", "im", "Schla\u00b7raf\u00b7fland", ",", "'s", "isch", "wohr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "PPER", "ADJD", "ADJD", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Treit nit meng Immli s\u00fce\u00dfe Hunig heim", "tokens": ["Treit", "nit", "meng", "I\u00b7mmli", "s\u00fce\u00b7\u00dfe", "Hu\u00b7nig", "heim"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "NE", "NE", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "um Triberg? Hangt nit menge Chirsibaum", "tokens": ["um", "Tri\u00b7berg", "?", "Hangt", "nit", "men\u00b7ge", "Chir\u00b7si\u00b7baum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$.", "NE", "PTKNEG", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "voll schwarze Chinder? Mir do niede fliegt", "tokens": ["voll", "schwar\u00b7ze", "Chin\u00b7der", "?", "Mir", "do", "nie\u00b7de", "fliegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$.", "PPER", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "der Chuechen und der Chirsiwasserchrueg", "tokens": ["der", "Chu\u00b7e\u00b7chen", "und", "der", "Chir\u00b7si\u00b7was\u00b7ser\u00b7chru\u00b7eg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "und drei f\u00fcr ein zum Fenster i. \u00bbDo trink!\u00ab", "tokens": ["und", "drei", "f\u00fcr", "ein", "zum", "Fens\u00b7ter", "i.", "\u00bb", "Do", "trink", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "CARD", "APPR", "ART", "APPRART", "NN", "NE", "$(", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.35": {"text": "Und lueg, do fliegt e Blatt, 's isch schwarz uf wii\u00df.", "tokens": ["Und", "lu\u00b7eg", ",", "do", "fliegt", "e", "Blatt", ",", "'s", "isch", "schwarz", "uf", "wii\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "VVFIN", "NE", "NN", "$,", "PPER", "ADJD", "ADJD", "APPR", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Her J\u00e4ck, viel S\u00fce\u00dfi wohnt im Bluemechelch,", "tokens": ["Her", "J\u00e4ck", ",", "viel", "S\u00fce\u00b7\u00dfi", "wohnt", "im", "Blu\u00b7e\u00b7me\u00b7chelch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "viel Gw\u00fcrz im brune Chirsichern, 's isch wohr.", "tokens": ["viel", "Gw\u00fcrz", "im", "bru\u00b7ne", "Chir\u00b7si\u00b7chern", ",", "'s", "isch", "wohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$,", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Doch was im frumme Menscheherz ersprie\u00dft,", "tokens": ["Doch", "was", "im", "frum\u00b7me", "Men\u00b7sche\u00b7herz", "er\u00b7sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "und ufgoht, und in sch\u00f6ne Liedere bl\u00fceiht,", "tokens": ["und", "uf\u00b7goht", ",", "und", "in", "sch\u00f6\u00b7ne", "Lie\u00b7de\u00b7re", "bl\u00fce\u00b7iht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "wie euer Lied, goht \u00fcbers Zuckerbrot", "tokens": ["wie", "eu\u00b7er", "Lied", ",", "goht", "\u00fc\u00b7bers", "Zu\u00b7cker\u00b7brot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "und Zimmetgeist. Das treit ke Immli heim.", "tokens": ["und", "Zim\u00b7met\u00b7geist", ".", "Das", "treit", "ke", "I\u00b7mmli", "heim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "ART", "NN", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Das distellirt der Summer an keim Baum.", "tokens": ["Das", "dis\u00b7tel\u00b7lirt", "der", "Sum\u00b7mer", "an", "keim", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Drum dank ich Gott f\u00fcr alles Liebs und Guets.", "tokens": ["Drum", "dank", "ich", "Gott", "f\u00fcr", "al\u00b7les", "Liebs", "und", "Guets", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "APPR", "PIAT", 