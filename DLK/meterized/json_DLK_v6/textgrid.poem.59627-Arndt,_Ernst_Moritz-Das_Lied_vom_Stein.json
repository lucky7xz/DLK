{"textgrid.poem.59627": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Stein", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo zu des Rheines heil'gen Wogen", "tokens": ["Wo", "zu", "des", "Rhei\u00b7nes", "heil'\u00b7gen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lahn in bunten Ufern rauscht,", "tokens": ["Die", "Lahn", "in", "bun\u00b7ten", "U\u00b7fern", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist ein Adler aufgeflogen,", "tokens": ["Da", "ist", "ein", "Ad\u00b7ler", "auf\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der fr\u00fch dem Sph\u00e4renklang gelauscht,", "tokens": ["Der", "fr\u00fch", "dem", "Sph\u00e4\u00b7ren\u00b7klang", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der fr\u00fche in des Lichtes Wonne", "tokens": ["Der", "fr\u00fc\u00b7he", "in", "des", "Lich\u00b7tes", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die junge Seele eingetaucht,", "tokens": ["Die", "jun\u00b7ge", "See\u00b7le", "ein\u00b7ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den fr\u00fch der goldne Reiz der Sonne", "tokens": ["Den", "fr\u00fch", "der", "gold\u00b7ne", "Reiz", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit stolzer Sehnsucht angehaucht.", "tokens": ["Mit", "stol\u00b7zer", "Sehn\u00b7sucht", "an\u00b7ge\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sa\u00df er in dem Felsenneste,", "tokens": ["Da", "sa\u00df", "er", "in", "dem", "Fel\u00b7sen\u00b7nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das seine V\u00e4ter einst gebaut,", "tokens": ["Das", "sei\u00b7ne", "V\u00e4\u00b7ter", "einst", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da klang ihm auf der hohen Feste", "tokens": ["Da", "klang", "ihm", "auf", "der", "ho\u00b7hen", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der grauen Vorzeit Wunderlaut:", "tokens": ["Der", "grau\u00b7en", "Vor\u00b7zeit", "Wun\u00b7der\u00b7laut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hei! Wie dem J\u00fcngling von dem Klingen", "tokens": ["Hei", "!", "Wie", "dem", "J\u00fcng\u00b7ling", "von", "dem", "Klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die Brust erschwoll im s\u00fc\u00dfen Wahn!", "tokens": ["Die", "Brust", "er\u00b7schwoll", "im", "s\u00fc\u00b7\u00dfen", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hei! Wie er oft geregt die Schwingen,", "tokens": ["Hei", "!", "Wie", "er", "oft", "ge\u00b7regt", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Als m\u00e4\u00df' er schon die Sonnenbahn!", "tokens": ["Als", "m\u00e4\u00df'", "er", "schon", "die", "Son\u00b7nen\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf in das Leben ausgeflogen", "tokens": ["Drauf", "in", "das", "Le\u00b7ben", "aus\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie find't er alles anders gar!", "tokens": ["Wie", "find't", "er", "al\u00b7les", "an\u00b7ders", "gar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfinstert hat den Himmelsbogen", "tokens": ["Ver\u00b7fins\u00b7tert", "hat", "den", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein w\u00fcster Schwarm dem Sonnenaar,", "tokens": ["Ein", "w\u00fcs\u00b7ter", "Schwarm", "dem", "Son\u00b7ne\u00b7naar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kr\u00e4hen und die Dohlen haben", "tokens": ["Die", "Kr\u00e4\u00b7hen", "und", "die", "Doh\u00b7len", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verh\u00fcllt des Lichtes goldnen Schein,", "tokens": ["Ver\u00b7h\u00fcllt", "des", "Lich\u00b7tes", "gold\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Eulen wollen gar und Raben", "tokens": ["Und", "Eu\u00b7len", "wol\u00b7len", "gar", "und", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herolde und Propheten sein.", "tokens": ["He\u00b7rol\u00b7de", "und", "Pro\u00b7phe\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch mitten in den Truggestalten", "tokens": ["Doch", "mit\u00b7ten", "in", "den", "Trug\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schirmt ihn des Herzens fromme Scheu,", "tokens": ["Schirmt", "ihn", "des", "Her\u00b7zens", "from\u00b7me", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bleibt den himmlischen Gewalten", "tokens": ["Er", "bleibt", "den", "himm\u00b7li\u00b7schen", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jugendwahnes redlich treu,", "tokens": ["Des", "Ju\u00b7gend\u00b7wah\u00b7nes", "red\u00b7lich", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er winkt hinauf zur h\u00f6chsten Ferne,", "tokens": ["Er", "winkt", "hin\u00b7auf", "zur", "h\u00f6chs\u00b7ten", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hinab zum tiefsten Geisterort", "tokens": ["Hin\u00b7ab", "zum", "tiefs\u00b7ten", "Geis\u00b7ter\u00b7ort"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und spricht: \u00bbDie G\u00f6tter und die Sterne", "tokens": ["Und", "spricht", ":", "\u00bb", "Die", "G\u00f6t\u00b7ter", "und", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie halten ewig fest ihr Wort.", "tokens": ["Sie", "hal\u00b7ten", "e\u00b7wig", "fest", "ihr", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist gleich der Sonnenpfad der V\u00e4ter", "tokens": ["Ist", "gleich", "der", "Son\u00b7nen\u00b7pfad", "der", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom schwarzen P\u00f6belschwarm verh\u00fcllt,", "tokens": ["Vom", "schwar\u00b7zen", "P\u00f6\u00b7bel\u00b7schwarm", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So brennt mir doch vom lichten \u00c4ther", "tokens": ["So", "brennt", "mir", "doch", "vom", "lich\u00b7ten", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In tiefster Brust ein Flammenbild;", "tokens": ["In", "tiefs\u00b7ter", "Brust", "ein", "Flam\u00b7men\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df ewige Nacht das All bedecken,", "tokens": ["La\u00df", "e\u00b7wi\u00b7ge", "Nacht", "das", "All", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Himmel tun den H\u00f6llenfall,", "tokens": ["Den", "Him\u00b7mel", "tun", "den", "H\u00f6l\u00b7len\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seele zittert keinen Schrecken,", "tokens": ["Die", "See\u00b7le", "zit\u00b7tert", "kei\u00b7nen", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie tr\u00e4gt das All, sie ist das All.\u00ab", "tokens": ["Sie", "tr\u00e4gt", "das", "All", ",", "sie", "ist", "das", "All", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Heil dir, du Sohn vom Felsenneste!", "tokens": ["Heil", "dir", ",", "du", "Sohn", "vom", "Fel\u00b7sen\u00b7nes\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heil dir, du mutig Sonnenkind!", "tokens": ["Heil", "dir", ",", "du", "mu\u00b7tig", "Son\u00b7nen\u00b7kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hohe Walter ob der Feste,", "tokens": ["Der", "ho\u00b7he", "Wal\u00b7ter", "ob", "der", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat gesandt den Sausewind:", "tokens": ["Er", "hat", "ge\u00b7sandt", "den", "Sau\u00b7se\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die schwachen Fl\u00fcgel sind zerbrochen,", "tokens": ["Die", "schwa\u00b7chen", "Fl\u00fc\u00b7gel", "sind", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Adler sind die L\u00fcfte rein,", "tokens": ["Dem", "Ad\u00b7ler", "sind", "die", "L\u00fcf\u00b7te", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Nichts ist in sein Nichts gekrochen,", "tokens": ["Das", "Nichts", "ist", "in", "sein", "Nichts", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tugend soll das Zepter sein!", "tokens": ["Der", "Tu\u00b7gend", "soll", "das", "Zep\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Heil, fester Stein von festem Steine!", "tokens": ["Heil", ",", "fes\u00b7ter", "Stein", "von", "fes\u00b7tem", "Stei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heil, stolzer, freier, deutscher Mann!", "tokens": ["Heil", ",", "stol\u00b7zer", ",", "frei\u00b7er", ",", "deut\u00b7scher", "Mann", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der in des Ruhmes Sonnenscheine", "tokens": ["Der", "in", "des", "Ruh\u00b7mes", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor aller Welt nun leuchten kann!", "tokens": ["Vor", "al\u00b7ler", "Welt", "nun", "leuch\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zerschmettert liegt die P\u00f6belrotte,", "tokens": ["Zer\u00b7schmet\u00b7tert", "liegt", "die", "P\u00f6\u00b7bel\u00b7rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zerflogen ist der Knechte Wahn,", "tokens": ["Zer\u00b7flo\u00b7gen", "ist", "der", "Knech\u00b7te", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit dem alten deutschen Gotte", "tokens": ["Und", "mit", "dem", "al\u00b7ten", "deut\u00b7schen", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geht Ehre auf der Ehrenbahn.", "tokens": ["Geht", "Eh\u00b7re", "auf", "der", "Eh\u00b7ren\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Heil, fester Stein von festem Steine!", "tokens": ["Heil", ",", "fes\u00b7ter", "Stein", "von", "fes\u00b7tem", "Stei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heil Freiheit, Vaterland und Recht!", "tokens": ["Heil", "Frei\u00b7heit", ",", "Va\u00b7ter\u00b7land", "und", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh lange noch am deutschen Rheine", "tokens": ["Sieh", "lan\u00b7ge", "noch", "am", "deut\u00b7schen", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPRART", "ADJA", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Freuden bl\u00fchen Teuts Geschlecht!", "tokens": ["In", "Freu\u00b7den", "bl\u00fc\u00b7hen", "Teuts", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh lange noch vom Sitz der Ahnen", "tokens": ["Sieh", "lan\u00b7ge", "noch", "vom", "Sitz", "der", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im sch\u00f6nsten Lebensabendschein", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Le\u00b7bens\u00b7a\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word"],