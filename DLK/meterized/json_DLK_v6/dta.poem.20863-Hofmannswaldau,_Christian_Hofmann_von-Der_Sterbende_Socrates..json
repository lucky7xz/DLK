{"dta.poem.20863": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der reine Strom/ so aus den Felsen springet/", "tokens": ["Der", "rei\u00b7ne", "Strom", "/", "so", "aus", "den", "Fel\u00b7sen", "sprin\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in den Bach mit schnellen Fluten dringet/", "tokens": ["Und", "in", "den", "Bach", "mit", "schnel\u00b7len", "Flu\u00b7ten", "drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird durch den Schlam offt tr\u00fcb und schwartz gemacht/", "tokens": ["Wird", "durch", "den", "Schlam", "offt", "tr\u00fcb", "und", "schwartz", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und unser Geist/ wie rein er ist gebohren/", "tokens": ["Und", "un\u00b7ser", "Geist", "/", "wie", "rein", "er", "ist", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verleuret bey den Sinnen seine Pracht/", "tokens": ["Ver\u00b7leu\u00b7ret", "bey", "den", "Sin\u00b7nen", "sei\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In dem er ihm den faulen Weg erkohren.", "tokens": ["In", "dem", "er", "ihm", "den", "fau\u00b7len", "Weg", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wann nun der Geist allein auf sich besteht/", "tokens": ["Wann", "nun", "der", "Geist", "al\u00b7lein", "auf", "sich", "be\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Und ihm kein Sinn nicht an der Seiten geht/", "tokens": ["Und", "ihm", "kein", "Sinn", "nicht", "an", "der", "Sei\u00b7ten", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja gantz allein auf seine Rechnung trauet/", "tokens": ["Ja", "gantz", "al\u00b7lein", "auf", "sei\u00b7ne", "Rech\u00b7nung", "trau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wandert er von allem Schlamm befreyt/", "tokens": ["So", "wan\u00b7dert", "er", "von", "al\u00b7lem", "Schlamm", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch einen Weg darauf die Warheit bauet/", "tokens": ["Durch", "ei\u00b7nen", "Weg", "da\u00b7rauf", "die", "War\u00b7heit", "bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gantz ungeirrt hin auf die Ewigkeit.", "tokens": ["Gantz", "un\u00b7ge\u00b7irrt", "hin", "auf", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der reine Trieb kan solche Wirckung machen/", "tokens": ["Der", "rei\u00b7ne", "Trieb", "kan", "sol\u00b7che", "Wir\u00b7ckung", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er erlangt die Eigenschafft der Sachen/", "tokens": ["Da\u00df", "er", "er\u00b7langt", "die", "Ei\u00b7gen\u00b7schafft", "der", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die weder Zeit noch Zufall \u00e4ndern kan/", "tokens": ["Die", "we\u00b7der", "Zeit", "noch", "Zu\u00b7fall", "\u00e4n\u00b7dern", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist das Thun/ dem sich der Geist ergiebet/", "tokens": ["Das", "ist", "das", "Thun", "/", "dem", "sich", "der", "Geist", "er\u00b7gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "PRELS", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So nimmermehr dem Wesen unterthan/", "tokens": ["So", "nim\u00b7mer\u00b7mehr", "dem", "We\u00b7sen", "un\u00b7ter\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem von Natur Verwandelung beliebet.", "tokens": ["Dem", "von", "Na\u00b7tur", "Ver\u00b7wan\u00b7de\u00b7lung", "be\u00b7lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}