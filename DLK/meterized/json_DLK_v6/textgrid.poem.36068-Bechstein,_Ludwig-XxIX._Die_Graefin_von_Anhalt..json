{"textgrid.poem.36068": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxIX. Die Graefin von Anhalt.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebt im Menschenherzen ein wunderbarer Drang,", "tokens": ["Es", "lebt", "im", "Men\u00b7schen\u00b7her\u00b7zen", "ein", "wun\u00b7der\u00b7ba\u00b7rer", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der es zur Freude hinlockt wie magischer Gesang.", "tokens": ["Der", "es", "zur", "Freu\u00b7de", "hin\u00b7lockt", "wie", "ma\u00b7gi\u00b7scher", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was seinem W\u00fcnschen schmeichelt, erfasst der Erdensohn,", "tokens": ["Was", "sei\u00b7nem", "W\u00fcn\u00b7schen", "schmei\u00b7chelt", ",", "er\u00b7fasst", "der", "Er\u00b7den\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sei's Bl\u00fcthe der Empfindung, sei's Farbe, Duft und Ton.", "tokens": ["Sei's", "Bl\u00fc\u00b7the", "der", "Emp\u00b7fin\u00b7dung", ",", "sei's", "Far\u00b7be", ",", "Duft", "und", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nach Lust verlangt das Kind schon, das noch die Amme tr\u00e4gt,", "tokens": ["Nach", "Lust", "ver\u00b7langt", "das", "Kind", "schon", ",", "das", "noch", "die", "Am\u00b7me", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nach Lust verlangt der J\u00fcngling, dess volle Kraft sich regt;", "tokens": ["Nach", "Lust", "ver\u00b7langt", "der", "J\u00fcng\u00b7ling", ",", "dess", "vol\u00b7le", "Kraft", "sich", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der Lust begehrt das Alter bis an den letzten Tag,", "tokens": ["Der", "Lust", "be\u00b7gehrt", "das", "Al\u00b7ter", "bis", "an", "den", "letz\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was immer lebt, es strebet der holden G\u00f6ttin nach.", "tokens": ["Was", "im\u00b7mer", "lebt", ",", "es", "stre\u00b7bet", "der", "hol\u00b7den", "G\u00f6t\u00b7tin", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Noch wunderbarer regt sich oft seltsames Gel\u00fcst,", "tokens": ["Noch", "wun\u00b7der\u00b7ba\u00b7rer", "regt", "sich", "oft", "selt\u00b7sa\u00b7mes", "Ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "PRF", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Dess unerkl\u00e4rter Ursprung ein tiefes R\u00e4thsel ist;", "tokens": ["Dess", "un\u00b7er\u00b7kl\u00e4r\u00b7ter", "Ur\u00b7sprung", "ein", "tie\u00b7fes", "R\u00e4th\u00b7sel", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein R\u00e4thsel und ein Schleier gewoben wundervoll,", "tokens": ["Ein", "R\u00e4th\u00b7sel", "und", "ein", "Schlei\u00b7er", "ge\u00b7wo\u00b7ben", "wun\u00b7der\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der nicht von M\u00e4nnerh\u00e4nden gehoben werden soll.", "tokens": ["Der", "nicht", "von", "M\u00e4n\u00b7ner\u00b7h\u00e4n\u00b7den", "ge\u00b7ho\u00b7ben", "wer\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wem ein Gott verlieben uns zu erfreun die Kunst,", "tokens": ["Und", "wem", "ein", "Gott", "ver\u00b7lie\u00b7ben", "uns", "zu", "er\u00b7freun", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dem sprudelt dankbarlohnreich der G\u00f6tterwein der Gunst.", "tokens": ["Dem", "spru\u00b7delt", "dank\u00b7bar\u00b7lohn\u00b7reich", "der", "G\u00f6t\u00b7ter\u00b7wein", "der", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vom Hesperidenbaume des Gl\u00fccks er Fr\u00fcchte bricht,", "tokens": ["Vom", "Hes\u00b7pe\u00b7ri\u00b7den\u00b7bau\u00b7me", "des", "Gl\u00fccks", "er", "Fr\u00fcch\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und seine hellen Bl\u00fcthen sich um die Schl\u00e4fe flicht.", "tokens": ["Und", "sei\u00b7ne", "hel\u00b7len", "Bl\u00fc\u00b7then", "sich", "um", "die", "Schl\u00e4\u00b7fe", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch Faustus hascht begierig nach jener Gl\u00fccksfrucht Lohn:", "tokens": ["Auch", "Faus\u00b7tus", "hascht", "be\u00b7gie\u00b7rig", "nach", "je\u00b7ner", "Gl\u00fccks\u00b7frucht", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er steht mit seiner Vollmacht vor manchem F\u00fcrstenthron,", "tokens": ["Er", "steht", "mit", "sei\u00b7ner", "Voll\u00b7macht", "vor", "man\u00b7chem", "F\u00fcrs\u00b7ten\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er, dem die Geister dienen, wenn sie sein Zauber rief,", "tokens": ["Er", ",", "dem", "die", "Geis\u00b7ter", "die\u00b7nen", ",", "wenn", "sie", "sein", "Zau\u00b7ber", "rief", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er ist ein Hofmann worden, neigt sich vor Schranzen tief.", "tokens": ["Er", "ist", "ein", "Hof\u00b7mann", "wor\u00b7den", ",", "neigt", "sich", "vor", "Schran\u00b7zen", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$,", "VVFIN", "PRF", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wohl bleibt es immer herrlich, in deren Gunst zu stehn,", "tokens": ["Wohl", "bleibt", "es", "im\u00b7mer", "herr\u00b7lich", ",", "in", "de\u00b7ren", "Gunst", "zu", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "APPR", "PRELAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Gl\u00fcck vor Allen hochstellt auf Lebenssonnenh\u00f6hn.", "tokens": ["Die", "Gl\u00fcck", "vor", "Al\u00b7len", "hoch\u00b7stellt", "auf", "Le\u00b7bens\u00b7son\u00b7nen\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch anders ist's, ob einer in's heilge Fr\u00fchroth sieht,", "tokens": ["Doch", "an\u00b7ders", "ist's", ",", "ob", "ei\u00b7ner", "in's", "heil\u00b7ge", "Fr\u00fch\u00b7roth", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "$,", "KOUS", "ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und anders ob er knechtisch im Staub vor G\u00f6tzen kniet. \u2013", "tokens": ["Und", "an\u00b7ders", "ob", "er", "knech\u00b7tisch", "im", "Staub", "vor", "G\u00f6t\u00b7zen", "kniet", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADJD", "APPRART", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Als noch auf Bergeszinnen das Schloss von Anhalt stand,", "tokens": ["Als", "noch", "auf", "Ber\u00b7ges\u00b7zin\u00b7nen", "das", "Schloss", "von", "An\u00b7halt", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Flog mancher Blick hinunter in ein begl\u00fccktes Land,", "tokens": ["Flog", "man\u00b7cher", "Blick", "hin\u00b7un\u00b7ter", "in", "ein", "be\u00b7gl\u00fcck\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPO", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sah W\u00e4lder, gr\u00fcne Wiesen, sah D\u00f6rfer, gr\u00fcnumlaubt,", "tokens": ["Sah", "W\u00e4l\u00b7der", ",", "gr\u00fc\u00b7ne", "Wie\u00b7sen", ",", "sah", "D\u00f6r\u00b7fer", ",", "gr\u00fcn\u00b7um\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Harzwald Bergesriesen, des Brocken blaues Haupt.", "tokens": ["Des", "Harz\u00b7wald", "Ber\u00b7ges\u00b7rie\u00b7sen", ",", "des", "Bro\u00b7cken", "blau\u00b7es", "Haupt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Dort weilt der Graf von Anhalt mit seinem Ehgemahl;", "tokens": ["Dort", "weilt", "der", "Graf", "von", "An\u00b7halt", "mit", "sei\u00b7nem", "Eh\u00b7ge\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie schauen von der Bergh\u00f6h herab ins Selkethal.", "tokens": ["Sie", "schau\u00b7en", "von", "der", "Berg\u00b7h\u00f6h", "her\u00b7ab", "ins", "Sel\u00b7ke\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbsieh Theure, dort hinunter! Wer mag der Reiter sein?", "tokens": ["\u00bb", "sieh", "Theu\u00b7re", ",", "dort", "hin\u00b7un\u00b7ter", "!", "Wer", "mag", "der", "Rei\u00b7ter", "sein", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "ADV", "PTKVZ", "$.", "PWS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der stattlich Wohlgeschm\u00fcckte, zwei Diener hinterdrein.\u00ab", "tokens": ["Der", "statt\u00b7lich", "Wohl\u00b7ge\u00b7schm\u00fcck\u00b7te", ",", "zwei", "Die\u00b7ner", "hin\u00b7ter\u00b7drein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "CARD", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbihn tr\u00e4gt so stolz sein Schwarzross, wie er sein Prunkgewand;", "tokens": ["\u00bb", "ihn", "tr\u00e4gt", "so", "stolz", "sein", "Schwarz\u00b7ross", ",", "wie", "er", "sein", "Prunk\u00b7ge\u00b7wand", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der eine der Begleiter scheint von gelehrtem Stand.", "tokens": ["Der", "ei\u00b7ne", "der", "Be\u00b7glei\u00b7ter", "scheint", "von", "ge\u00b7lehr\u00b7tem", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der zweite, dem vom Hute die rothe Feder weht,", "tokens": ["Der", "zwei\u00b7te", ",", "dem", "vom", "Hu\u00b7te", "die", "ro\u00b7the", "Fe\u00b7der", "weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mag wohl ein lustger Rath sein, der guten Scherz versteht.\u00ab", "tokens": ["Mag", "wohl", "ein", "lust\u00b7ger", "Rath", "sein", ",", "der", "gu\u00b7ten", "Scherz", "ver\u00b7steht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$,", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbsie ziehn herauf. \u2013 O Wunder! Und sieht mein Auge recht?", "tokens": ["\u00bb", "sie", "ziehn", "her\u00b7auf", ".", "\u2013", "O", "Wun\u00b7der", "!", "Und", "sieht", "mein", "Au\u00b7ge", "recht", "?"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$.", "$(", "NE", "NN", "$.", "KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ist \u2013 weit auf das Burgthor! \u2013 Er ist ", "tokens": ["Das", "ist", "\u2013", "weit", "auf", "das", "Burg\u00b7thor", "!", "\u2013", "Er", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "$(", "ADJD", "APPR", "ART", "NN", "$.", "$(", "PPER", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und doch so grossen Namens, dem keiner gleichen kann,", "tokens": ["Und", "doch", "so", "gros\u00b7sen", "Na\u00b7mens", ",", "dem", "kei\u00b7ner", "glei\u00b7chen", "kann", ","], "token_info": ["word", "word", 