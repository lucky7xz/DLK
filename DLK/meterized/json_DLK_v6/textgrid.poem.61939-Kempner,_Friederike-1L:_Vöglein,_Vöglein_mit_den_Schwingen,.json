{"textgrid.poem.61939": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: V\u00f6glein, V\u00f6glein mit den Schwingen,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein, V\u00f6glein mit den Schwingen,", "tokens": ["V\u00f6\u00b7glein", ",", "V\u00f6\u00b7glein", "mit", "den", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den \u00c4uglein schwarz und klein,", "tokens": ["Mit", "den", "\u00c4ug\u00b7lein", "schwarz", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df uns mit einander singen,", "tokens": ["La\u00df", "uns", "mit", "ein\u00b7an\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df uns liebe Freunde sein!", "tokens": ["La\u00df", "uns", "lie\u00b7be", "Freun\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "V\u00f6glein h\u00fcpfte auf den B\u00e4umen,", "tokens": ["V\u00f6\u00b7glein", "h\u00fcpf\u00b7te", "auf", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich es mit Sang begann:", "tokens": ["End\u00b7lich", "es", "mit", "Sang", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du kannst nur von Freiheit tr\u00e4umen,", "tokens": ["Du", "kannst", "nur", "von", "Frei\u00b7heit", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich seh' ich als Fremdling an!", "tokens": ["Dich", "seh'", "ich", "als", "Fremd\u00b7ling", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mensch, auch Du hast Deine Schwingen,", "tokens": ["Mensch", ",", "auch", "Du", "hast", "Dei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00c4uglein klar und hell und rein,", "tokens": ["\u00c4ug\u00b7lein", "klar", "und", "hell", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nntest Freiheit dir erringen,", "tokens": ["K\u00f6nn\u00b7test", "Frei\u00b7heit", "dir", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann erst la\u00df uns Freunde sein!", "tokens": ["Dann", "erst", "la\u00df", "uns", "Freun\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}