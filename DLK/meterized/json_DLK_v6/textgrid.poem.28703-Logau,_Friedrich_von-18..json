{"textgrid.poem.28703": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiber-H\u00e4ndel, die, wie br\u00e4uchlich, unter ihnen stets entstehn,", "tokens": ["Wei\u00b7ber\u00b7H\u00e4n\u00b7del", ",", "die", ",", "wie", "br\u00e4uch\u00b7lich", ",", "un\u00b7ter", "ih\u00b7nen", "stets", "ent\u00b7stehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "$,", "PWAV", "ADJD", "$,", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Pflegen endlich auff ein sagen und auff nichts mehr au\u00dfzugehn.", "tokens": ["Pfle\u00b7gen", "end\u00b7lich", "auff", "ein", "sa\u00b7gen", "und", "auff", "nichts", "mehr", "au\u00df\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "VVINF", "KON", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jene sagte dieses neulich, und es sagte jenes die;", "tokens": ["Je\u00b7ne", "sag\u00b7te", "die\u00b7ses", "neu\u00b7lich", ",", "und", "es", "sag\u00b7te", "je\u00b7nes", "die", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "ADV", "$,", "KON", "PPER", "VVFIN", "PDS", "ART", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dieses hat sie nicht gesaget; jene sagte solches nie.", "tokens": ["Die\u00b7ses", "hat", "sie", "nicht", "ge\u00b7sa\u00b7get", ";", "je\u00b7ne", "sag\u00b7te", "sol\u00b7ches", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$.", "PDS", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Eine sagte, das da sagte diese: jene sagte das;", "tokens": ["Ei\u00b7ne", "sag\u00b7te", ",", "das", "da", "sag\u00b7te", "die\u00b7se", ":", "je\u00b7ne", "sag\u00b7te", "das", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "PDS", "$.", "PDS", "VVFIN", "PDS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Nein! sie sagte, da\u00df sie sagte dieses nicht, nur sonsten was.", "tokens": ["Nein", "!", "sie", "sag\u00b7te", ",", "da\u00df", "sie", "sag\u00b7te", "die\u00b7ses", "nicht", ",", "nur", "sons\u00b7ten", "was", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "PDS", "PTKNEG", "$,", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "O, ich wei\u00df wol, was sie sagte; wil sie, sagt ihr, sagen nicht,", "tokens": ["O", ",", "ich", "wei\u00df", "wol", ",", "was", "sie", "sag\u00b7te", ";", "wil", "sie", ",", "sagt", "ihr", ",", "sa\u00b7gen", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$.", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Was sie sagte, wil ich sagen, was sie sagte, frey ans Licht;", "tokens": ["Was", "sie", "sag\u00b7te", ",", "wil", "ich", "sa\u00b7gen", ",", "was", "sie", "sag\u00b7te", ",", "frey", "ans", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ey, sie sage, was ich sagte; eh ich sagte, sagt sie vor;", "tokens": ["Ey", ",", "sie", "sa\u00b7ge", ",", "was", "ich", "sag\u00b7te", ";", "eh", "ich", "sag\u00b7te", ",", "sagt", "sie", "vor", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Sagt nur, da\u00df sie solle sagen, was sie mir sagt in ein Ohr.", "tokens": ["Sagt", "nur", ",", "da\u00df", "sie", "sol\u00b7le", "sa\u00b7gen", ",", "was", "sie", "mir", "sagt", "in", "ein", "Ohr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,", "PRELS", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Dieses sagen wil nun wehren, weil das Leder wehrt ums Maul;", "tokens": ["Die\u00b7ses", "sa\u00b7gen", "wil", "nun", "weh\u00b7ren", ",", "weil", "das", "Le\u00b7der", "wehrt", "ums", "Maul", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "ADV", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Dann zum sagen und zum plaudern sind die Weiber selten faul.", "tokens": ["Dann", "zum", "sa\u00b7gen", "und", "zum", "plau\u00b7dern", "sind", "die", "Wei\u00b7ber", "sel\u00b7ten", "faul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "VVINF", "KON", "APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}