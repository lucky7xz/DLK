{"dta.poem.6630": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es m\u00f6gen immerhin die Thier-Patronen dencken", "tokens": ["Es", "m\u00f6\u00b7gen", "im\u00b7mer\u00b7hin", "die", "Thier\u00b7Pa\u00b7tro\u00b7nen", "den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob seh\u2019 ein Frosch im Sumpf, wie wir, den Himmel lencken:", "tokens": ["Ob", "seh'", "ein", "Frosch", "im", "Sumpf", ",", "wie", "wir", ",", "den", "Him\u00b7mel", "len\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob w\u00e4r\u2019 ihm nichts als Anmuth und als Lust", "tokens": ["Ob", "w\u00e4r'", "ihm", "nichts", "als", "An\u00b7muth", "und", "als", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PIS", "KOKOM", "NN", "KON", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf frischem Gras, im schwancken Rohr bewust:", "tokens": ["Auf", "fri\u00b7schem", "Gras", ",", "im", "schwan\u00b7cken", "Rohr", "be\u00b7wust", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Regens Uberflu\u00df, des heitern Tages Schein", "tokens": ["Des", "Re\u00b7gens", "U\u00b7ber\u00b7flu\u00df", ",", "des", "hei\u00b7tern", "Ta\u00b7ges", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erg\u00f6sse sich und strahl\u2019 \u00fcm und f\u00fcr ihm allein.", "tokens": ["Er\u00b7g\u00f6s\u00b7se", "sich", "und", "strahl'", "\u00fcm", "und", "f\u00fcr", "ihm", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "ADJD", "KON", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}