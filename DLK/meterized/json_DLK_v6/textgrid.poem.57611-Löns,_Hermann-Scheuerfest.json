{"textgrid.poem.57611": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Scheuerfest", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustig, los, zum Scheuerfest,", "tokens": ["Lus\u00b7tig", ",", "los", ",", "zum", "Scheu\u00b7er\u00b7fest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht mir rein das alte Nest,", "tokens": ["Macht", "mir", "rein", "das", "al\u00b7te", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns fegen, scheuern, kehren,", "tokens": ["La\u00dft", "uns", "fe\u00b7gen", ",", "scheu\u00b7ern", ",", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aberglauben, Ammenm\u00e4ren,", "tokens": ["A\u00b7berg\u00b7lau\u00b7ben", ",", "Am\u00b7men\u00b7m\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorurteile: \u2013 Spinneweben,", "tokens": ["Vor\u00b7ur\u00b7tei\u00b7le", ":", "\u2013", "Spin\u00b7ne\u00b7we\u00b7ben", ","], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die in allen Ecken kleben,", "tokens": ["Die", "in", "al\u00b7len", "E\u00b7cken", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Voller Staub das alte Haus,", "tokens": ["Vol\u00b7ler", "Staub", "das", "al\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heute mu\u00df der Staub heraus.", "tokens": ["Heu\u00b7te", "mu\u00df", "der", "Staub", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schlagt die Butzenscheiben ein!", "tokens": ["Schlagt", "die", "But\u00b7zen\u00b7schei\u00b7ben", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei soll unsre Aussicht sein.", "tokens": ["Frei", "soll", "uns\u00b7re", "Aus\u00b7sicht", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte Bilder \u2013 schnell verbrannt,", "tokens": ["Al\u00b7te", "Bil\u00b7der", "\u2013", "schnell", "ver\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fort mit all dem Ahnentand,", "tokens": ["Fort", "mit", "all", "dem", "Ah\u00b7nen\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reine Zimmer, klare Fenster", "tokens": ["Rei\u00b7ne", "Zim\u00b7mer", ",", "kla\u00b7re", "Fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind kein Heim f\u00fcr Nachtgespenster,", "tokens": ["Sind", "kein", "Heim", "f\u00fcr", "Nacht\u00b7ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf die T\u00fcren! \u2013 scharfer Zug", "tokens": ["Auf", "die", "T\u00fc\u00b7ren", "!", "\u2013", "schar\u00b7fer", "Zug"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "$(", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weht hinaus den Zauberspuk.", "tokens": ["Weht", "hin\u00b7aus", "den", "Zau\u00b7bers\u00b7puk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier in diesem alten Schrank", "tokens": ["Hier", "in", "die\u00b7sem", "al\u00b7ten", "Schrank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ordenszeichen, blink und blank,", "tokens": ["Or\u00b7dens\u00b7zei\u00b7chen", ",", "blink", "und", "blank", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tressen, Degen, Adelsschreiben \u2013", "tokens": ["Tres\u00b7sen", ",", "De\u00b7gen", ",", "A\u00b7dels\u00b7schrei\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll das Zeug im Hause bleiben?", "tokens": ["Soll", "das", "Zeug", "im", "Hau\u00b7se", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schmei\u00dft den Tr\u00f6del in die Gosse,", "tokens": ["Schmei\u00dft", "den", "Tr\u00f6\u00b7del", "in", "die", "Gos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlu\u00df macht mit der Narrensposse \u2013", "tokens": ["Schlu\u00df", "macht", "mit", "der", "Nar\u00b7rens\u00b7pos\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie das glitzert, glei\u00dft und blinkt", "tokens": ["Wie", "das", "glit\u00b7zert", ",", "glei\u00dft", "und", "blinkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nach Rost und Gr\u00fcnspan stinkt!", "tokens": ["Und", "nach", "Rost", "und", "Gr\u00fcn\u00b7span", "stinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}