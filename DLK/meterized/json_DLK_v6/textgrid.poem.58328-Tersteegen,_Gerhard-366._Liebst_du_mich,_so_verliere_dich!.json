{"textgrid.poem.58328": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "366. Liebst du mich, so verliere dich!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer seine Seele liebt, bewahret sie mit Flei\u00df,", "tokens": ["Wer", "sei\u00b7ne", "See\u00b7le", "liebt", ",", "be\u00b7wah\u00b7ret", "sie", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer Gott reine liebt, mu\u00df seine Seel' verlieren;", "tokens": ["Und", "wer", "Gott", "rei\u00b7ne", "liebt", ",", "mu\u00df", "sei\u00b7ne", "Seel'", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "ADJA", "VVFIN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie selig ist sie doch verlor'n auf solche Weis',", "tokens": ["Wie", "se\u00b7lig", "ist", "sie", "doch", "ver\u00b7lor'n", "auf", "sol\u00b7che", "Weis'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Feind sie ewig darf in Gottes Hand ber\u00fchren!", "tokens": ["Kein", "Feind", "sie", "e\u00b7wig", "darf", "in", "Got\u00b7tes", "Hand", "be\u00b7r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "VMFIN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}