{"textgrid.poem.59404": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft wollt mich die Welt erm\u00fcden,", "tokens": ["Wie", "oft", "wollt", "mich", "die", "Welt", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich beugt aufs Schwert mein Angesicht", "tokens": ["Ich", "beugt", "aufs", "Schwert", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bat dich frevelhaft um Frieden \u2013", "tokens": ["Und", "bat", "dich", "fre\u00b7vel\u00b7haft", "um", "Frie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wu\u00dftest's besser, gabst ihn nicht.", "tokens": ["Du", "wu\u00df\u00b7test's", "bes\u00b7ser", ",", "gabst", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sah in Nacht das Land vergehen,", "tokens": ["Ich", "sah", "in", "Nacht", "das", "Land", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Blitzen du die Wetter brachst,", "tokens": ["In", "Blit\u00b7zen", "du", "die", "Wet\u00b7ter", "brachst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da konnt ich schauernd erst verstehen,", "tokens": ["Da", "konnt", "ich", "schau\u00b7ernd", "erst", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was du zu mir Erschrocknem sprachst:", "tokens": ["Was", "du", "zu", "mir", "Er\u00b7schrock\u00b7nem", "sprachst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmeine Lieder sind nicht deine Lieder", "tokens": ["\u00bb", "mei\u00b7ne", "Lie\u00b7der", "sind", "nicht", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leg ab den falschen Schmuck der Zeit", "tokens": ["Leg", "ab", "den", "fal\u00b7schen", "Schmuck", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimm das Kreuz, dann komme wieder", "tokens": ["Und", "nimm", "das", "Kreuz", ",", "dann", "kom\u00b7me", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deines Herzens Einsamkeit.\u00ab", "tokens": ["In", "dei\u00b7nes", "Her\u00b7zens", "Ein\u00b7sam\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und alle Bilder ferne treten,", "tokens": ["Und", "al\u00b7le", "Bil\u00b7der", "fer\u00b7ne", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tief noch rauschet kaum die Rund \u2013", "tokens": ["Und", "tief", "noch", "rau\u00b7schet", "kaum", "die", "Rund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie geht ein wunderbares Beten", "tokens": ["Wie", "geht", "ein", "wun\u00b7der\u00b7ba\u00b7res", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir leuchtend durch der Seele Grund!", "tokens": ["Mir", "leuch\u00b7tend", "durch", "der", "See\u00b7le", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}