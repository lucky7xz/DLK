{"textgrid.poem.53793": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Was w\u00e4re, wenn . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": ". . . Dein harter Lebensfaden liegt zerschnitten.", "tokens": [".", ".", ".", "Dein", "har\u00b7ter", "Le\u00b7bens\u00b7fa\u00b7den", "liegt", "zer\u00b7schnit\u00b7ten", "."], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du stiller Dulder hast nun ausgelitten,", "tokens": ["Du", "stil\u00b7ler", "Dul\u00b7der", "hast", "nun", "aus\u00b7ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du, der du stets die V\u00f6lker \u00fcberragt,", "tokens": ["Du", ",", "der", "du", "stets", "die", "V\u00f6l\u00b7ker", "\u00fc\u00b7berr\u00b7agt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Zahn der Zeit nur hat dich hohlgenagt.", "tokens": ["Der", "Zahn", "der", "Zeit", "nur", "hat", "dich", "hohl\u00b7ge\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In schwarzen Schleifen Trauerreiser,", "tokens": ["In", "schwar\u00b7zen", "Schlei\u00b7fen", "Trau\u00b7er\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies bringt dein Volk dir! \u2013 Deutscher Kaiser!", "tokens": ["Dies", "bringt", "dein", "Volk", "dir", "!", "\u2013", "Deut\u00b7scher", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PPER", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00e4mtliche Minister nehmen am Trauerzuge teil!", "tokens": ["S\u00e4mt\u00b7li\u00b7che", "Mi\u00b7nis\u00b7ter", "neh\u00b7men", "am", "Trau\u00b7er\u00b7zu\u00b7ge", "teil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Kronprinz im Trauerzug. Rechts oben: Der Kronprinz bei den Ruderregatten in Gr\u00fcnau.", "tokens": ["Der", "Kron\u00b7prinz", "im", "Trau\u00b7er\u00b7zug", ".", "Rechts", "o\u00b7ben", ":", "Der", "Kron\u00b7prinz", "bei", "den", "Ru\u00b7der\u00b7re\u00b7gat\u00b7ten", "in", "Gr\u00fc\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "ADV", "ADV", "$.", "ART", "NN", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-+--+-+-+--+--++", "measure": "iambic.octa.plus.relaxed"}}}}}