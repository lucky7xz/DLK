{"textgrid.poem.46239": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede an seine Liebste", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dicht, ich sag, ich sing: ach nein! ich seufz, schrei, klag", "tokens": ["Ich", "dicht", ",", "ich", "sag", ",", "ich", "sing", ":", "ach", "nein", "!", "ich", "seufz", ",", "schrei", ",", "klag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "ADJD", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "ADV", "PTKANT", "$.", "PPER", "ADJD", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die lieb, das leid, damit mein junges herz gestritten,", "tokens": ["die", "lieb", ",", "das", "leid", ",", "da\u00b7mit", "mein", "jun\u00b7ges", "herz", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "verlierend allen trost und hofnung mit dem tag,", "tokens": ["ver\u00b7lie\u00b7rend", "al\u00b7len", "trost", "und", "hof\u00b7nung", "mit", "dem", "tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verwundet durch und durch endlich den tod erlitten.", "tokens": ["ver\u00b7wun\u00b7det", "durch", "und", "durch", "end\u00b7lich", "den", "tod", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "KON", "APPR", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kein soldat in der schlacht und gr\u00f6sten niderlag", "tokens": ["Kein", "sol\u00b7dat", "in", "der", "schlacht", "und", "gr\u00f6s\u00b7ten", "ni\u00b7der\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "war jemals, als mein herz, zerhacket und zerschnitten,", "tokens": ["war", "je\u00b7mals", ",", "als", "mein", "herz", ",", "zer\u00b7ha\u00b7cket", "und", "zer\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und bittend um quartier kont ich weder vertrag,", "tokens": ["und", "bit\u00b7tend", "um", "quar\u00b7tier", "kont", "ich", "we\u00b7der", "ver\u00b7trag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "VMFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "noch meiner feindin gnad erbeuten noch erbitten.", "tokens": ["noch", "mei\u00b7ner", "fein\u00b7din", "gnad", "er\u00b7beu\u00b7ten", "noch", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O greuliche sch\u00f6nheit, die mit ernst oder scherz,", "tokens": ["O", "greu\u00b7li\u00b7che", "sch\u00f6n\u00b7heit", ",", "die", "mit", "ernst", "o\u00b7der", "scherz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "nach euerm eignen lust, den seelen widerstrebet,", "tokens": ["nach", "eu\u00b7erm", "eig\u00b7nen", "lust", ",", "den", "see\u00b7len", "wi\u00b7der\u00b7stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "erkennet doch wie gro\u00df euer stolz und mein schmerz!", "tokens": ["er\u00b7ken\u00b7net", "doch", "wie", "gro\u00df", "eu\u00b7er", "stolz", "und", "mein", "schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ADJD", "PPOSAT", "ADJD", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "O die ihr, wan ihr wolt, den tod, das leben gebet,", "tokens": ["O", "die", "ihr", ",", "wan", "ihr", "wolt", ",", "den", "tod", ",", "das", "le\u00b7ben", "ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "$,", "PDS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "verleihet, da\u00df durch euch, weil ja durch euch mein herz", "tokens": ["ver\u00b7lei\u00b7het", ",", "da\u00df", "durch", "euch", ",", "weil", "ja", "durch", "euch", "mein", "herz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPER", "$,", "KOUS", "ADV", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "get\u00f6tet, mein gesang hingegen werd belebet.", "tokens": ["ge\u00b7t\u00f6\u00b7tet", ",", "mein", "ge\u00b7sang", "hin\u00b7ge\u00b7gen", "werd", "be\u00b7le\u00b7bet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "ADJD", "ADV", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}