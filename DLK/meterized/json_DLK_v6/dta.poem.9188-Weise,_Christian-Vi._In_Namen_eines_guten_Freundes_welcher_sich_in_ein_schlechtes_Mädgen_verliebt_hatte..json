{"dta.poem.9188": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n In Namen eines guten Freundes/ welcher sich in  \n ein schlechtes M\u00e4dgen verliebt hatte.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "da:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebstes kind/ mein ander leben/", "tokens": ["Ljebs\u00b7tes", "kind", "/", "mein", "an\u00b7der", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehrtes seelgen wilst du nicht/", "tokens": ["Wehr\u00b7tes", "seel\u00b7gen", "wilst", "du", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen diener glauben geben/", "tokens": ["Dei\u00b7nen", "die\u00b7ner", "glau\u00b7ben", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann er dir so viel verspricht/", "tokens": ["Wann", "er", "dir", "so", "viel", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wer solche worte gibt/", "tokens": ["Ach", "wer", "sol\u00b7che", "wor\u00b7te", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist nichts anders als verliebt.", "tokens": ["Ist", "nichts", "an\u00b7ders", "als", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Welche mit der liebe schertzen/", "tokens": ["Wel\u00b7che", "mit", "der", "lie\u00b7be", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geben sich bi\u00dfweilen blo\u00df/", "tokens": ["Ge\u00b7ben", "sich", "bi\u00df\u00b7wei\u00b7len", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber doch in meinem hertzen", "tokens": ["A\u00b7ber", "doch", "in", "mei\u00b7nem", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die sehnsucht viel zu gro\u00df/", "tokens": ["Ist", "die", "sehn\u00b7sucht", "viel", "zu", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "ADV", "PTKA", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich dir du hertzgen du/", "tokens": ["Da\u00df", "ich", "dir", "du", "hertz\u00b7gen", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keinen tritt zu leide thu.", "tokens": ["Kei\u00b7nen", "tritt", "zu", "lei\u00b7de", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ach sprichst du es sind personen/", "tokens": ["Ach", "sprichst", "du", "es", "sind", "per\u00b7so\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPER", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich mehr verpflichtet bin/", "tokens": ["Den", "ich", "mehr", "ver\u00b7pflich\u00b7tet", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in andern gassen wohnen/", "tokens": ["Die", "in", "an\u00b7dern", "gas\u00b7sen", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da gedenck ich lieber hin/", "tokens": ["Da", "ge\u00b7denck", "ich", "lie\u00b7ber", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andern armen leuten sey", "tokens": ["An\u00b7dern", "ar\u00b7men", "leu\u00b7ten", "sey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich zum blossen possen treu.", "tokens": ["Ich", "zum", "blos\u00b7sen", "pos\u00b7sen", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Schweig und lern es besser treffen", "tokens": ["Schweig", "und", "lern", "es", "bes\u00b7ser", "tref\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses war noch weit gefehlt/", "tokens": ["Die\u00b7ses", "war", "noch", "weit", "ge\u00b7fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer kan euch mit worten \u00e4ffen/", "tokens": ["Wer", "kan", "euch", "mit", "wor\u00b7ten", "\u00e4f\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wann ihr uns das hertze stehlt/", "tokens": ["Wann", "ihr", "uns", "das", "hert\u00b7ze", "stehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr/ ihr m\u00e4dgen seyd uns hold/", "tokens": ["Ihr", "/", "ihr", "m\u00e4d\u00b7gen", "seyd", "uns", "hold", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "VVINF", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann ihr uns vexiren wolt.", "tokens": ["Wann", "ihr", "uns", "ve\u00b7xi\u00b7ren", "wolt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Und darzu was sols bedeuten/", "tokens": ["Und", "dar\u00b7zu", "was", "sols", "be\u00b7deu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PWS", "VMFIN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df du nicht so vornehm bist?", "tokens": ["Da\u00df", "du", "nicht", "so", "vor\u00b7nehm", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann nur bey den armen leuten", "tokens": ["Wann", "nur", "bey", "den", "ar\u00b7men", "leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tugend lieb und sch\u00f6nheit ist/", "tokens": ["Tu\u00b7gend", "lieb", "und", "sch\u00f6n\u00b7heit", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so trifft man alles an/", "tokens": ["Ach", "so", "trifft", "man", "al\u00b7les", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PIS", "PIS", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die liebe w\u00fcnschen kan.", "tokens": ["Was", "die", "lie\u00b7be", "w\u00fcn\u00b7schen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Alles was ich bey den reichen", "tokens": ["Al\u00b7les", "was", "ich", "bey", "den", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PWS", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Forn und hinten suchen wil/", "tokens": ["Forn", "und", "hin\u00b7ten", "su\u00b7chen", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das kan ich mit dir vergleichen:", "tokens": ["Das", "kan", "ich", "mit", "dir", "ver\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann du hast doch gleich so viel/", "tokens": ["Dann", "du", "hast", "doch", "gleich", "so", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bein und adern/ fleisch und blut/", "tokens": ["Bein", "und", "a\u00b7dern", "/", "fleisch", "und", "blut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$(", "ADJD", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hast du eben auch so gut.", "tokens": ["Hast", "du", "e\u00b7ben", "auch", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Wann sich andre m\u00e4dgen schmincken/", "tokens": ["Wann", "sich", "and\u00b7re", "m\u00e4d\u00b7gen", "schmin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du von natur so sch\u00f6n/", "tokens": ["Bist", "du", "von", "na\u00b7tur", "so", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann sie nach ziebethe stincken/", "tokens": ["Wann", "sie", "nach", "zie\u00b7be\u00b7the", "stin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "VVFIN", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kanst du ohne di\u00df bestehn/", "tokens": ["Kanst", "du", "oh\u00b7ne", "di\u00df", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDS", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche tauschte gern mit dir/", "tokens": ["Man\u00b7che", "tauschte", "gern", "mit", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und geb alles geld daf\u00fcr.", "tokens": ["Und", "geb", "al\u00b7les", "geld", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "PAV", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "8. Drum so la\u00df dich noch gewinnen/", "tokens": ["Drum", "so", "la\u00df", "dich", "noch", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVIMP", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich anders bitten kan/", "tokens": ["Wo", "ich", "an\u00b7ders", "bit\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaue mit geneigten sinnen", "tokens": ["Schau\u00b7e", "mit", "ge\u00b7neig\u00b7ten", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines hertzens unschuld an/", "tokens": ["Mei\u00b7nes", "hert\u00b7zens", "un\u00b7schuld", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebstes seelgen ich will dein", "tokens": ["Liebs\u00b7tes", "seel\u00b7gen", "ich", "will", "dein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VMFIN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und du solst mein liebgen seyn.", "tokens": ["Und", "du", "solst", "mein", "lieb\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Hast du nicht viel edelsteine/", "tokens": ["Hast", "du", "nicht", "viel", "e\u00b7del\u00b7stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gst du keine perle nicht;", "tokens": ["Tr\u00e4gst", "du", "kei\u00b7ne", "per\u00b7le", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach so gl\u00e4ntzt mit s\u00fcsserm scheine/", "tokens": ["Ach", "so", "gl\u00e4ntzt", "mit", "s\u00fcs\u00b7serm", "schei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner zarten-augen-liecht.", "tokens": ["Dei\u00b7ner", "zar\u00b7ten\u00b7au\u00b7gen\u00b7liecht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast du gleich kein hau\u00df voll gold/", "tokens": ["Hast", "du", "gleich", "kein", "hau\u00df", "voll", "gold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich deinen lippen hold.", "tokens": ["Bin", "ich", "dei\u00b7nen", "lip\u00b7pen", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "10. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Deine br\u00fcstgen und was mehr/", "tokens": ["Dei\u00b7ne", "br\u00fcst\u00b7gen", "und", "was", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PWS", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reitzen mich und mein verlangen/", "tokens": ["Reit\u00b7zen", "mich", "und", "mein", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PPOSAT", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich als wann es silber w\u00e4r/", "tokens": ["Gleich", "als", "wann", "es", "sil\u00b7ber", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PWAV", "PPER", "ADV", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also kan dein fleisch allein", "tokens": ["Al\u00b7so", "kan", "dein", "fleisch", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein vergn\u00fcgter reichthum seyn.", "tokens": ["Mein", "ver\u00b7gn\u00fcg\u00b7ter", "reicht\u00b7hum", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}