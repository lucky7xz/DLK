{"textgrid.poem.45688": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "**Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser neue Walther von der Vogelweide", "tokens": ["Die\u00b7ser", "neu\u00b7e", "Walt\u00b7her", "von", "der", "Vo\u00b7gel\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Machte mir im Lesen wenig Freude.", "tokens": ["Mach\u00b7te", "mir", "im", "Le\u00b7sen", "we\u00b7nig", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Derlei schlichtes, biedres deutsches Wesen", "tokens": ["Der\u00b7lei", "schlich\u00b7tes", ",", "bie\u00b7dres", "deut\u00b7sches", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "$,", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gefiele mir im Handeln besser als im Lesen.", "tokens": ["Ge\u00b7fie\u00b7le", "mir", "im", "Han\u00b7deln", "bes\u00b7ser", "als", "im", "Le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADJD", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}