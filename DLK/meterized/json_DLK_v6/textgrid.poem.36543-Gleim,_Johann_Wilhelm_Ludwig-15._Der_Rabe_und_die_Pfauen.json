{"textgrid.poem.36543": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "15. Der Rabe und die Pfauen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf eines F\u00fcrsten Hof ging eine Herde Pfauen;", "tokens": ["Auf", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Hof", "ging", "ei\u00b7ne", "Her\u00b7de", "Pfau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Aufzug, welchen anzuschauen", "tokens": ["Ein", "Auf\u00b7zug", ",", "wel\u00b7chen", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Auge m\u00fcde ward; denn jeder trug sein Rad,", "tokens": ["Kein", "Au\u00b7ge", "m\u00fc\u00b7de", "ward", ";", "denn", "je\u00b7der", "trug", "sein", "Rad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAFIN", "$.", "KON", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Farben, wie sie nur der Regenbogen hat,", "tokens": ["Mit", "Far\u00b7ben", ",", "wie", "sie", "nur", "der", "Re\u00b7gen\u00b7bo\u00b7gen", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gebreitet hinter sich, ein wenig stolz darauf!", "tokens": ["Ge\u00b7brei\u00b7tet", "hin\u00b7ter", "sich", ",", "ein", "we\u00b7nig", "stolz", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PRF", "$,", "ART", "PIS", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus den empor gestiegnen R\u00e4dern", "tokens": ["Aus", "den", "em\u00b7por", "ge\u00b7stieg\u00b7nen", "R\u00e4\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PTKVZ", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entfielen wundersch\u00f6ne Federn,", "tokens": ["Ent\u00b7fie\u00b7len", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Fe\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht umsonst! Ein Rabe las sie auf", "tokens": ["Und", "nicht", "um\u00b7sonst", "!", "Ein", "Ra\u00b7be", "las", "sie", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schm\u00fcckte heimlich sich damit,", "tokens": ["Und", "schm\u00fcck\u00b7te", "heim\u00b7lich", "sich", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ging, mit abgeme\u00dfnem Schritt,", "tokens": ["Und", "ging", ",", "mit", "ab\u00b7ge\u00b7me\u00df\u00b7nem", "Schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In die Versammlung rechter Pfauen;", "tokens": ["In", "die", "Ver\u00b7samm\u00b7lung", "rech\u00b7ter", "Pfau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "War auch ein wenig stolz, und lie\u00df sich auch beschauen.", "tokens": ["War", "auch", "ein", "we\u00b7nig", "stolz", ",", "und", "lie\u00df", "sich", "auch", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PIS", "ADJD", "$,", "KON", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein man kannt' ihn allsobald;", "tokens": ["Al\u00b7lein", "man", "kannt'", "ihn", "all\u00b7so\u00b7bald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nahm ihm den fremden Zierrat ab,", "tokens": ["Nahm", "ihm", "den", "frem\u00b7den", "Zier\u00b7rat", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bi\u00df ihn gelinde, gab", "tokens": ["Bi\u00df", "ihn", "ge\u00b7lin\u00b7de", ",", "gab"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dem armen Dieb die vorige Gestalt.", "tokens": ["Dem", "ar\u00b7men", "Dieb", "die", "vo\u00b7ri\u00b7ge", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So leicht gestraft, ging er mit gro\u00dfen Freuden wieder", "tokens": ["So", "leicht", "ge\u00b7straft", ",", "ging", "er", "mit", "gro\u00b7\u00dfen", "Freu\u00b7den", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In die Gesellschaft seiner Br\u00fcder.", "tokens": ["In", "die", "Ge\u00b7sell\u00b7schaft", "sei\u00b7ner", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dieser aber kam er noch weit \u00fcbler an:", "tokens": ["In", "die\u00b7ser", "a\u00b7ber", "kam", "er", "noch", "weit", "\u00fcb\u00b7ler", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn sein Vergehen war den Raben kund gethan.", "tokens": ["Denn", "sein", "Ver\u00b7ge\u00b7hen", "war", "den", "Ra\u00b7ben", "kund", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie stehn umher um ihn, sie lachen, spotten, schrein:", "tokens": ["Sie", "stehn", "um\u00b7her", "um", "ihn", ",", "sie", "la\u00b7chen", ",", "spot\u00b7ten", ",", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPER", "$,", "PPER", "VVINF", "$,", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr Pfau! Herr Pfau! Herr Pfau! sie hauen auf ihn ein,", "tokens": ["Herr", "Pfau", "!", "Herr", "Pfau", "!", "Herr", "Pfau", "!", "sie", "hau\u00b7en", "auf", "ihn", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "NE", "$.", "NN", "NE", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und raufen ihm, einm\u00fctig, mit Gewalt,", "tokens": ["Und", "rau\u00b7fen", "ihm", ",", "ein\u00b7m\u00fc\u00b7tig", ",", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die eignen und die fremden Federn aus.", "tokens": ["Die", "eig\u00b7nen", "und", "die", "frem\u00b7den", "Fe\u00b7dern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der arme Schelm entflieht in eines Dichters Haus,", "tokens": ["Der", "ar\u00b7me", "Schelm", "ent\u00b7flieht", "in", "ei\u00b7nes", "Dich\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und rettet sich, allein in kl\u00e4glicher Gestalt!", "tokens": ["Und", "ret\u00b7tet", "sich", ",", "al\u00b7lein", "in", "kl\u00e4g\u00b7li\u00b7cher", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}