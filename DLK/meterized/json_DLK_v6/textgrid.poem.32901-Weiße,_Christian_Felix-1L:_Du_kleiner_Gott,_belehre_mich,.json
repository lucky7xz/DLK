{"textgrid.poem.32901": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleiner Gott, belehre mich,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleiner Gott, belehre mich,", "tokens": ["Du", "klei\u00b7ner", "Gott", ",", "be\u00b7leh\u00b7re", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich, wenn ich vom Lieb entbrenne,", "tokens": ["Wie", "ich", ",", "wenn", "ich", "vom", "Lieb", "ent\u00b7bren\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem M\u00e4dchen mich erkl\u00e4ren k\u00f6nne:", "tokens": ["Dem", "M\u00e4d\u00b7chen", "mich", "er\u00b7kl\u00e4\u00b7ren", "k\u00f6n\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Red ich von Flammen, Pfeil und Stich,", "tokens": ["Red", "ich", "von", "Flam\u00b7men", ",", "Pfeil", "und", "Stich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Wunden und vom Tode,", "tokens": ["Von", "Wun\u00b7den", "und", "vom", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die\u00df ist ja aus der Mode?", "tokens": ["Die\u00df", "ist", "ja", "aus", "der", "Mo\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "\u00bbsey erst verliebt, sprach er, und dann", "tokens": ["\u00bb", "sey", "erst", "ver\u00b7liebt", ",", "sprach", "er", ",", "und", "dann"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "ADV", "VVPP", "$,", "VVFIN", "PPER", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kannst du zum M\u00e4dchen gehen:", "tokens": ["Kannst", "du", "zum", "M\u00e4d\u00b7chen", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gieb acht! sie wird dich bald verstehen.", "tokens": ["Gieb", "acht", "!", "sie", "wird", "dich", "bald", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es bricht das Licht der Sonnen an:", "tokens": ["Es", "bricht", "das", "Licht", "der", "Son\u00b7nen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Brauchst du der Welt zu sagen:", "tokens": ["Brauchst", "du", "der", "Welt", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt fieng es an zu tagen!\u00ab", "tokens": ["Jetzt", "fi\u00b7eng", "es", "an", "zu", "ta\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}