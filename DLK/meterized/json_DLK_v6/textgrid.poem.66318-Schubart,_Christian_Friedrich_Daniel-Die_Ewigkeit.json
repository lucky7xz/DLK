{"textgrid.poem.66318": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Ewigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut hebet meine Seele sich", "tokens": ["Heut", "he\u00b7bet", "mei\u00b7ne", "See\u00b7le", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihren engen Schranken,", "tokens": ["Aus", "ih\u00b7ren", "en\u00b7gen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wagt mit tiefstem Ernste dich,", "tokens": ["Und", "wagt", "mit", "tiefs\u00b7tem", "Erns\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Furchtbarster der Gedanken!", "tokens": ["Furcht\u00b7bars\u00b7ter", "der", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich, schauervolle Ewigkeit!", "tokens": ["Dich", ",", "schau\u00b7er\u00b7vol\u00b7le", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich, Urquell der vergangnen Zeit", "tokens": ["Dich", ",", "Ur\u00b7quell", "der", "ver\u00b7gang\u00b7nen", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und aller K\u00fcnftigkeiten.", "tokens": ["Und", "al\u00b7ler", "K\u00fcnf\u00b7tig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich Sonnenstaub von gestern her", "tokens": ["Ich", "Son\u00b7nen\u00b7staub", "von", "ge\u00b7stern", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ADV", "APZR"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Soll mich an's Ufer wagen,", "tokens": ["Soll", "mich", "an's", "U\u00b7fer", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohin vom unbeschifften Meer", "tokens": ["Wo\u00b7hin", "vom", "un\u00b7be\u00b7schiff\u00b7ten", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schwarzen Wogen schlagen?", "tokens": ["Die", "schwar\u00b7zen", "Wo\u00b7gen", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An ungeheure Tiefen, wo", "tokens": ["An", "un\u00b7ge\u00b7heu\u00b7re", "Tie\u00b7fen", ",", "wo"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zahllose Welten, leicht wie Stroh", "tokens": ["Zahl\u00b7lo\u00b7se", "Wel\u00b7ten", ",", "leicht", "wie", "Stroh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "KOKOM", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Auf Meereswogen schwimmen?", "tokens": ["Auf", "Mee\u00b7res\u00b7wo\u00b7gen", "schwim\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wagt es meine Seele gleich,", "tokens": ["Und", "wagt", "es", "mei\u00b7ne", "See\u00b7le", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit eines Cherubs Schwingen", "tokens": ["Mit", "ei\u00b7nes", "Che\u00b7rubs", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In dieses unbeflogne Thal", "tokens": ["In", "die\u00b7ses", "un\u00b7be\u00b7flog\u00b7ne", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ewigkeit zu dringen:", "tokens": ["Der", "E\u00b7wig\u00b7keit", "zu", "drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach tausend Jahren steh' ich doch,", "tokens": ["Nach", "tau\u00b7send", "Jah\u00b7ren", "steh'", "ich", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach nach Aeonen, immer noch,", "tokens": ["Ach", "nach", "A\u00b7e\u00b7o\u00b7nen", ",", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie heute an dem Ufer.", "tokens": ["Wie", "heu\u00b7te", "an", "dem", "U\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bald l\u00e4\u00dft der Geist ihn weit zur\u00fcck", "tokens": ["Bald", "l\u00e4\u00dft", "der", "Geist", "ihn", "weit", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Anfang aller Zeiten;", "tokens": ["Den", "An\u00b7fang", "al\u00b7ler", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald wagt er einen scharfen Blick", "tokens": ["Bald", "wagt", "er", "ei\u00b7nen", "schar\u00b7fen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tausend K\u00fcnftigkeiten;", "tokens": ["In", "tau\u00b7send", "K\u00fcnf\u00b7tig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch dort und da ist Ewigkeit,", "tokens": ["Doch", "dort", "und", "da", "ist", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcberall ist Ewigkeit,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ist", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und meine Fl\u00fcgel sinken.", "tokens": ["Und", "mei\u00b7ne", "Fl\u00fc\u00b7gel", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schaut Sterne, Sonnen, Welten an,", "tokens": ["Schaut", "Ster\u00b7ne", ",", "Son\u00b7nen", ",", "Wel\u00b7ten", "an", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und z\u00e4hlet ihre Heere;", "tokens": ["Und", "z\u00e4h\u00b7let", "ih\u00b7re", "Hee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ersch\u00f6pft den ganzen Ocean,", "tokens": ["Er\u00b7sch\u00f6pft", "den", "gan\u00b7zen", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Und w\u00e4gt den Sand am Meere;", "tokens": ["Und", "w\u00e4gt", "den", "Sand", "am", "Mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vermehrt die ungeheure Zahl", "tokens": ["Ver\u00b7mehrt", "die", "un\u00b7ge\u00b7heu\u00b7re", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tausendmillionenmal,", "tokens": ["Mit", "tau\u00b7send\u00b7mil\u00b7li\u00b7o\u00b7nen\u00b7mal", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alle Zahl verschwindet.", "tokens": ["Und", "al\u00b7le", "Zahl", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und hier! im grenzenlosen Reich", "tokens": ["Und", "hier", "!", "im", "gren\u00b7zen\u00b7lo\u00b7sen", "Reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Namloser Ewigkeiten", "tokens": ["Nam\u00b7lo\u00b7ser", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind \u2013 Seelen bebet! \u2013 sind vor euch", "tokens": ["Sind", "\u2013", "See\u00b7len", "be\u00b7bet", "!", "\u2013", "sind", "vor", "euch"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VAFIN", "$(", "NN", "VVFIN", "$.", "$(", "VAFIN", "APPR", "PPER"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Nur Qualen oder Freuden.", "tokens": ["Nur", "Qua\u00b7len", "o\u00b7der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Qual, die ohne Ende dr\u00e4ut!", "tokens": ["Die", "Qual", ",", "die", "oh\u00b7ne", "En\u00b7de", "dr\u00e4ut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freude einer Ewigkeit!", "tokens": ["Die", "Freu\u00b7de", "ei\u00b7ner", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich denke sie, und bete:", "tokens": ["Ich", "den\u00b7ke", "sie", ",", "und", "be\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gott, der da ", "tokens": ["Gott", ",", "der", "da"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ohn' Anfang, ohne Ende!", "tokens": ["Ohn'", "An\u00b7fang", ",", "oh\u00b7ne", "En\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Staube jammert hier ein Christ", "tokens": ["Im", "Stau\u00b7be", "jam\u00b7mert", "hier", "ein", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und faltet seine H\u00e4nde.", "tokens": ["Und", "fal\u00b7tet", "sei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mich durch deines Sohnes Pein", "tokens": ["La\u00df", "mich", "durch", "dei\u00b7nes", "Soh\u00b7nes", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht ewiglich verloren sein,", "tokens": ["Nicht", "e\u00b7wig\u00b7lich", "ver\u00b7lo\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du Vater des Erbarmens!", "tokens": ["Du", "Va\u00b7ter", "des", "Er\u00b7bar\u00b7mens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Herr Jesu! will des Richters Glut", "tokens": ["Herr", "Je\u00b7su", "!", "will", "des", "Rich\u00b7ters", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich schon von ferne t\u00f6dten,", "tokens": ["Mich", "schon", "von", "fer\u00b7ne", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00df dein Blut, dein theures Blut,", "tokens": ["So", "la\u00df", "dein", "Blut", ",", "dein", "theu\u00b7res", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meiner Rettung reden.", "tokens": ["Zu", "mei\u00b7ner", "Ret\u00b7tung", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geist Gottes, f\u00fchre aus der Zeit", "tokens": ["Geist", "Got\u00b7tes", ",", "f\u00fch\u00b7re", "aus", "der", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich in die frohe Ewigkeit!", "tokens": ["Mich", "in", "die", "fro\u00b7he", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann jauchz' ich durch den Himmel:", "tokens": ["Dann", "jauch\u00b7z'", "ich", "durch", "den", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Unsterblich Lob sei Gott! Durch seines Sohnes Wunden", "tokens": ["U\u00b7nsterb\u00b7lich", "Lob", "sei", "Gott", "!", "Durch", "sei\u00b7nes", "Soh\u00b7nes", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VAFIN", "NN", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Hab' ich den Todeskampf gek\u00e4mpft und \u00fcberwunden.", "tokens": ["Hab'", "ich", "den", "To\u00b7des\u00b7kampf", "ge\u00b7k\u00e4mpft", "und", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "H\u00f6r' mich, o Ewigkeit! Jauchzt, Himmel, jauchzt, wie ich!", "tokens": ["H\u00f6r'", "mich", ",", "o", "E\u00b7wig\u00b7keit", "!", "Jauchzt", ",", "Him\u00b7mel", ",", "jauchzt", ",", "wie", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$.", "NN", "$,", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor Wonne bin ich stumm \u2013 Ihr Engel, singt f\u00fcr mich!", "tokens": ["Vor", "Won\u00b7ne", "bin", "ich", "stumm", "\u2013", "Ihr", "En\u00b7gel", ",", "singt", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$(", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}