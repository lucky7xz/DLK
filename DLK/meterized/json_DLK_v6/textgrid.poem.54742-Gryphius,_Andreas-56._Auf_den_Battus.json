{"textgrid.poem.54742": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "56. Auf den Battus", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und wo man t\u00f6richt/ mischt er lauter Wei\u00dfheit ein.", "tokens": ["Und", "wo", "man", "t\u00f6\u00b7richt", "/", "mischt", "er", "lau\u00b7ter", "Wei\u00df\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "$(", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fragt man was ich und der und jener von ihm halte:", "tokens": ["Fragt", "man", "was", "ich", "und", "der", "und", "je\u00b7ner", "von", "ihm", "hal\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "PPER", "KON", "ART", "KON", "PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er ein Ehenbild verkehrter Zeitt verwalte.", "tokens": ["Da\u00df", "er", "ein", "E\u00b7hen\u00b7bild", "ver\u00b7kehr\u00b7ter", "Zeitt", "ver\u00b7wal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}