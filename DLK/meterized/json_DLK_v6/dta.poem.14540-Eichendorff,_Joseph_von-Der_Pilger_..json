{"dta.poem.14540": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Wille, Herr, geschehe!", "tokens": ["Dein", "Wil\u00b7le", ",", "Herr", ",", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verdunkelt schweigt das Land,", "tokens": ["Ver\u00b7dun\u00b7kelt", "schweigt", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Zug der Wetter sehe", "tokens": ["Im", "Zug", "der", "Wet\u00b7ter", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich schauernd Deine Hand.", "tokens": ["Ich", "schau\u00b7ernd", "Dei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O mit uns S\u00fcndern gehe", "tokens": ["O", "mit", "uns", "S\u00fcn\u00b7dern", "ge\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Erbarmend in's Gericht!", "tokens": ["Er\u00b7bar\u00b7mend", "in's", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich beug' im tiefsten Wehe", "tokens": ["Ich", "beug'", "im", "tiefs\u00b7ten", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Staub mein Angesicht,", "tokens": ["Zum", "Staub", "mein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dein Wille, Herr, geschehe!", "tokens": ["Dein", "Wil\u00b7le", ",", "Herr", ",", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}