{"textgrid.poem.39975": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3. Wie die Schwabenst\u00e4dte Abt Kuno Hilfe senden", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandrer m\u00f6gen gerne sp\u00e4hen", "tokens": ["Wand\u00b7rer", "m\u00f6\u00b7gen", "ger\u00b7ne", "sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem V\u00f6gliseck in's Land,", "tokens": ["Von", "dem", "V\u00f6g\u00b7li\u00b7seck", "in's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich den blauen See besehen", "tokens": ["Sich", "den", "blau\u00b7en", "See", "be\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die St\u00e4dte l\u00e4ngs dem Strand:", "tokens": ["Und", "die", "St\u00e4d\u00b7te", "l\u00e4ngs", "dem", "Strand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bregenz unter d\u00fcstern Fichten,", "tokens": ["Bre\u00b7genz", "un\u00b7ter", "d\u00fcs\u00b7tern", "Fich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Helles Lindau, Inselstadt,", "tokens": ["Hel\u00b7les", "Lin\u00b7dau", ",", "In\u00b7sel\u00b7stadt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6rsburg zwischen Wein und Fr\u00fcchten,", "tokens": ["M\u00f6rs\u00b7burg", "zwi\u00b7schen", "Wein", "und", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kostnitz, das den Rheinstrom hat;", "tokens": ["Kost\u00b7nitz", ",", "das", "den", "Rhein\u00b7strom", "hat", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber das ist nicht, was heute", "tokens": ["A\u00b7ber", "das", "ist", "nicht", ",", "was", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PTKNEG", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht der Appenzeller Hirt,", "tokens": ["Sieht", "der", "Ap\u00b7pen\u00b7zel\u00b7ler", "Hirt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen Blick die offne Weite,", "tokens": ["Des\u00b7sen", "Blick", "die", "off\u00b7ne", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Finstrer Sorgen voll, durchirrt:", "tokens": ["Finst\u00b7rer", "Sor\u00b7gen", "voll", ",", "durch\u00b7irrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er z\u00e4hlt nur die M\u00e4nnerscharen,", "tokens": ["Er", "z\u00e4hlt", "nur", "die", "M\u00e4n\u00b7ner\u00b7scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die aus Schwabens St\u00e4dten ziehn,", "tokens": ["Die", "aus", "Schwa\u00b7bens", "St\u00e4d\u00b7ten", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er sieht nur die Schiffe fahren,", "tokens": ["Er", "sieht", "nur", "die", "Schif\u00b7fe", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Alle ", "tokens": ["Al\u00b7le"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Wie von giftigen Gew\u00fcrmen", "tokens": ["Wie", "von", "gif\u00b7ti\u00b7gen", "Ge\u00b7w\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wimmelt das Gestade schon,", "tokens": ["Wim\u00b7melt", "das", "Ge\u00b7sta\u00b7de", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00f6hlich von Sankt Gallens Th\u00fcrmen", "tokens": ["Fr\u00f6h\u00b7lich", "von", "Sankt", "Gal\u00b7lens", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "VVFIN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4dt sie ein der Glocken Ton.", "tokens": ["L\u00e4dt", "sie", "ein", "der", "Glo\u00b7cken", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Wiehern steigt von Pferden", "tokens": ["Und", "ein", "Wie\u00b7hern", "steigt", "von", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem tiefen Thal herauf;", "tokens": ["Aus", "dem", "tie\u00b7fen", "Thal", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach der Heimat mit den Heerden", "tokens": ["Nach", "der", "Hei\u00b7mat", "mit", "den", "Heer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eilt der Hirt in schnellem Lauf.", "tokens": ["Eilt", "der", "Hirt", "in", "schnel\u00b7lem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drunten meldet er die Kunde;", "tokens": ["Drun\u00b7ten", "mel\u00b7det", "er", "die", "Kun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, die Panzer angethan,", "tokens": ["Und", ",", "die", "Pan\u00b7zer", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4ngt in seinem Wiesengrunde", "tokens": ["F\u00e4ngt", "in", "sei\u00b7nem", "Wie\u00b7sen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Appenzell zu tagen an.", "tokens": ["Ap\u00b7pen\u00b7zell", "zu", "ta\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wer soll dir Kundschaft bringen", "tokens": ["Doch", "wer", "soll", "dir", "Kund\u00b7schaft", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus der feindevollen Stadt,", "tokens": ["Aus", "der", "fein\u00b7de\u00b7vol\u00b7len", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "V\u00f6lklein, das zu solchen Dingen", "tokens": ["V\u00f6l\u00b7klein", ",", "das", "zu", "sol\u00b7chen", "Din\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenig Witz und Gabe hat?", "tokens": ["We\u00b7nig", "Witz", "und", "Ga\u00b7be", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Greif' nur mutig zu den Wehren,", "tokens": ["Greif'", "nur", "mu\u00b7tig", "zu", "den", "Weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcre deinen Landshauptmann;", "tokens": ["K\u00fc\u00b7re", "dei\u00b7nen", "Lands\u00b7haupt\u00b7mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst du doch die Welt bald lehren,", "tokens": ["Wirst", "du", "doch", "die", "Welt", "bald", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die kluge Unschuld kann:", "tokens": ["Was", "die", "klu\u00b7ge", "Un\u00b7schuld", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine T\u00f6chter werden Boten,", "tokens": ["Dei\u00b7ne", "T\u00f6ch\u00b7ter", "wer\u00b7den", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ziehen zu dem Feind mit Lust;", "tokens": ["Zie\u00b7hen", "zu", "dem", "Feind", "mit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den Miedern bebt, den roten,", "tokens": ["In", "den", "Mie\u00b7dern", "bebt", ",", "den", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mutig eine treue Brust!", "tokens": ["Mu\u00b7tig", "ei\u00b7ne", "treu\u00b7e", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Durch die Thore von Sankt Gallen,", "tokens": ["Durch", "die", "Tho\u00b7re", "von", "Sankt", "Gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der W\u00e4chter stehn genug,", "tokens": ["Wo", "der", "W\u00e4ch\u00b7ter", "stehn", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft man doch die M\u00e4gde wallen", "tokens": ["L\u00e4\u00dft", "man", "doch", "die", "M\u00e4g\u00b7de", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Milch im schmucken Krug.", "tokens": ["Mit", "der", "Milch", "im", "schmu\u00b7cken", "Krug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die St\u00e4dter in dem Saale", "tokens": ["Denn", "die", "St\u00e4d\u00b7ter", "in", "dem", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit des See's bejahrtem Most", "tokens": ["Mit", "des", "See's", "be\u00b7jahr\u00b7tem", "Most"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00e4nkt der Abt, doch zu dem Mahle", "tokens": ["Tr\u00e4nkt", "der", "Abt", ",", "doch", "zu", "dem", "Mah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Taugt der Alpen fette Kost.", "tokens": ["Taugt", "der", "Al\u00b7pen", "fet\u00b7te", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die Jungfraun stehen drinnen", "tokens": ["Und", "die", "Jung\u00b7fraun", "ste\u00b7hen", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zierlich in des Klosters Flur,", "tokens": ["Zier\u00b7lich", "in", "des", "Klos\u00b7ters", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00e4hn mit klugen Weibersinnen,", "tokens": ["Sp\u00e4hn", "mit", "klu\u00b7gen", "Wei\u00b7ber\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommen vielem auf die Spur:", "tokens": ["Kom\u00b7men", "vie\u00b7lem", "auf", "die", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo Herr Kuno mit den Schwaben", "tokens": ["Wo", "Herr", "Ku\u00b7no", "mit", "den", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "H\u00e4lt beim Becher lauten Rat;", "tokens": ["H\u00e4lt", "beim", "Be\u00b7cher", "lau\u00b7ten", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn sie g'nug erlauschet haben,", "tokens": ["Wenn", "sie", "g'\u00b7nug", "er\u00b7lau\u00b7schet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gehn sie heim auf steilem Pfad. \u2013", "tokens": ["Gehn", "sie", "heim", "auf", "stei\u00b7lem", "Pfad", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jene tagten auf der Wiese,", "tokens": ["Je\u00b7ne", "tag\u00b7ten", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die Schar der T\u00f6chter kam,", "tokens": ["Bis", "die", "Schar", "der", "T\u00f6ch\u00b7ter", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Vater eilet diese,", "tokens": ["Und", "zum", "Va\u00b7ter", "ei\u00b7let", "die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die zum r\u00fcst'gen Br\u00e4utigam:", "tokens": ["Die", "zum", "r\u00fcst'\u00b7gen", "Br\u00e4u\u00b7ti\u00b7gam", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbm\u00e4nner! weiter nicht ges\u00e4umet,", "tokens": ["\u00bb", "m\u00e4n\u00b7ner", "!", "wei\u00b7ter", "nicht", "ge\u00b7s\u00e4u\u00b7met", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf, gen Speicher diese Nacht!", "tokens": ["Auf", ",", "gen", "Spei\u00b7cher", "die\u00b7se", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn sie meinen, da\u00df ihr tr\u00e4umet,", "tokens": ["Wenn", "sie", "mei\u00b7nen", ",", "da\u00df", "ihr", "tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haltet vor dem Lande Wacht!\u00ab", "tokens": ["Hal\u00b7tet", "vor", "dem", "Lan\u00b7de", "Wacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und zweihundert sind ger\u00fcstet,", "tokens": ["Und", "zwei\u00b7hun\u00b7dert", "sind", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Eh' der Mond am Himmel scheint,", "tokens": ["Eh'", "der", "Mond", "am", "Him\u00b7mel", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nach k\u00fchnem Kampf gel\u00fcstet", "tokens": ["Die", "nach", "k\u00fch\u00b7nem", "Kampf", "ge\u00b7l\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen zehnmal st\u00e4rkern Feind.", "tokens": ["Ge\u00b7gen", "zehn\u00b7mal", "st\u00e4r\u00b7kern", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen klugen Scharenmeister", "tokens": ["Ei\u00b7nen", "klu\u00b7gen", "Scha\u00b7ren\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat das treue Schwyz gesandt;", "tokens": ["Hat", "das", "treu\u00b7e", "Schwyz", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stille ziehen sie wie Geister,", "tokens": ["Stil\u00b7le", "zie\u00b7hen", "sie", "wie", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chtlich auf des Berges Rand.", "tokens": ["N\u00e4cht\u00b7lich", "auf", "des", "Ber\u00b7ges", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ueber ihren H\u00e4uptern gehet", "tokens": ["Ue\u00b7ber", "ih\u00b7ren", "H\u00e4up\u00b7tern", "ge\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00fcb und rot ein seltner Stern,", "tokens": ["Tr\u00fcb", "und", "rot", "ein", "selt\u00b7ner", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie den Scheitel Haar umwehet,", "tokens": ["Wie", "den", "Schei\u00b7tel", "Haar", "um\u00b7we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallt ein Schweif um seinen Kern.", "tokens": ["Wallt", "ein", "Schweif", "um", "sei\u00b7nen", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl ist er ein finster Zeichen,", "tokens": ["Wohl", "ist", "er", "ein", "fins\u00b7ter", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wo er scheint, da flie\u00dfet Blut;", "tokens": ["Wo", "er", "scheint", ",", "da", "flie\u00b7\u00dfet", "Blut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flie\u00df' es denn von unsern Streichen!", "tokens": ["Flie\u00df'", "es", "denn", "von", "un\u