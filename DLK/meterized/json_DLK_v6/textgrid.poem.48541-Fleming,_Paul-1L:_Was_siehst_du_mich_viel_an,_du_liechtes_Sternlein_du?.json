{"textgrid.poem.48541": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was siehst du mich viel an, du liechtes Sternlein du?", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was siehst du mich viel an, du liechtes Sternlein du?", "tokens": ["Was", "siehst", "du", "mich", "viel", "an", ",", "du", "liech\u00b7tes", "Stern\u00b7lein", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach zeuch nur wieder heim und tu dein Antlitz zu!", "tokens": ["Ach", "zeuch", "nur", "wie\u00b7der", "heim", "und", "tu", "dein", "Ant\u00b7litz", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ADV", "ADV", "PTKVZ", "KON", "NE", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da bist du viel zu schlecht, zu sein ein Widerschein", "tokens": ["Da", "bist", "du", "viel", "zu", "schlecht", ",", "zu", "sein", "ein", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,", "APPR", "PPOSAT", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Augen, die mir mehr als Mon und Sonne sein.", "tokens": ["der", "Au\u00b7gen", ",", "die", "mir", "mehr", "als", "Mon", "und", "Son\u00b7ne", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN