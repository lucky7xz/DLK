{"dta.poem.21867": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dank\u2019 es dir allein/ du Weyser von Sta-", "tokens": ["Ich", "dank'", "es", "dir", "al\u00b7lein", "/", "du", "Wey\u00b7ser", "von", "Sta"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "$(", "PPER", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "da\u00df ich durch deinen Wizz zu solchem Wiz-", "tokens": ["da\u00df", "ich", "durch", "dei\u00b7nen", "Wizz", "zu", "sol\u00b7chem", "Wi\u00b7z"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der einen Zweiffel mir/ den du nicht weist/", "tokens": ["der", "ei\u00b7nen", "Zweif\u00b7fel", "mir", "/", "den", "du", "nicht", "weist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "$(", "ART", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nu wiltu kl\u00fcger sein/ kom\u0303 lerne was von mir.", "tokens": ["Nu", "wil\u00b7tu", "kl\u00fc\u00b7ger", "sein", "/", "kom\u0303", "ler\u00b7ne", "was", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "VAINF", "$(", "VVIMP", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}