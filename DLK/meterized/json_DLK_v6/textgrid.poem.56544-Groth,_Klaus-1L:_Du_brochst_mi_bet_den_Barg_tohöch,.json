{"textgrid.poem.56544": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du brochst mi bet den Barg toh\u00f6ch,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du brochst mi bet den Barg toh\u00f6ch,", "tokens": ["Du", "brochst", "mi", "bet", "den", "Barg", "to\u00b7h\u00f6ch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De S\u00fcnn de sack hendal:", "tokens": ["De", "S\u00fcnn", "de", "sack", "hen\u00b7dal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Do s\u00e4st du sachen, dat war Tid,", "tokens": ["Do", "s\u00e4st", "du", "sa\u00b7chen", ",", "dat", "war", "Tid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "PAV", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un wennst di mit enmal.", "tokens": ["Un", "wennst", "di", "mit", "en\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Do stunn ik dar un seeg opt Holt", "tokens": ["Do", "stunn", "ik", "dar", "un", "seeg", "opt", "Holt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gr\u00f6n inne Abends\u00fcnn,", "tokens": ["Gr\u00f6n", "in\u00b7ne", "A\u00b7bend\u00b7s\u00fcnn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn seeg ik langs den smallen Weg,", "tokens": ["Denn", "seeg", "ik", "langs", "den", "smal\u00b7len", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar gungst du ruhi hin.", "tokens": ["Dar", "gungst", "du", "ru\u00b7hi", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Do weerst du weg, doch weer de Thorn", "tokens": ["Do", "weerst", "du", "weg", ",", "doch", "weer", "de", "Thorn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch smuck un blank to sehn;", "tokens": ["Noch", "smuck", "un", "blank", "to", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ik gung de anner Sid hendal:", "tokens": ["Ik", "gung", "de", "an\u00b7ner", "Sid", "hen\u00b7dal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar weer ik ganz alleen. \u2013", "tokens": ["Dar", "weer", "ik", "ganz", "al\u00b7le\u00b7en", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "N\u00f6s heff ik \u00f6fter Afsched nam' \u2013", "tokens": ["N\u00f6s", "heff", "ik", "\u00f6f\u00b7ter", "Af\u00b7sched", "nam'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott weet, wa mennimal!", "tokens": ["Gott", "weet", ",", "wa", "men\u00b7ni\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Min Hart dat is dar baben bl\u0119bn,", "tokens": ["Min", "Hart", "dat", "is", "dar", "ba\u00b7ben", "bl\u0119bn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00fcht vun den Barg hendal.", "tokens": ["S\u00fcht", "vun", "den", "Barg", "hen\u00b7dal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}