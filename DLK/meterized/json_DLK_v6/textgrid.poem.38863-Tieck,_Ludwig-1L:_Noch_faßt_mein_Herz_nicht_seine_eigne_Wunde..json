{"textgrid.poem.38863": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch fa\u00dft mein Herz nicht seine eigne Wunde.", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch fa\u00dft mein Herz nicht seine eigne Wunde.", "tokens": ["Noch", "fa\u00dft", "mein", "Herz", "nicht", "sei\u00b7ne", "eig\u00b7ne", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als alle die dich kannten und dich liebten,", "tokens": ["Als", "al\u00b7le", "die", "dich", "kann\u00b7ten", "und", "dich", "lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ungewohntem Kummer sich betr\u00fcbten,", "tokens": ["Mit", "un\u00b7ge\u00b7wohn\u00b7tem", "Kum\u00b7mer", "sich", "be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ging mir vorbei der Kelch der bittern Stunde.", "tokens": ["Ging", "mir", "vor\u00b7bei", "der", "Kelch", "der", "bit\u00b7tern", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin noch so wie sonst mit dir im Bunde,", "tokens": ["Ich", "bin", "noch", "so", "wie", "sonst", "mit", "dir", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KOKOM", "ADV", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ist da\u00df wir wie ehedem uns \u00fcbten,", "tokens": ["Mir", "ist", "da\u00df", "wir", "wie", "e\u00b7he\u00b7dem", "uns", "\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "PPER", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An edlen Dichtern freun, den vielgeliebten,", "tokens": ["An", "ed\u00b7len", "Dich\u00b7tern", "freun", ",", "den", "viel\u00b7ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als br\u00e4cht' ein Brief von dir mir frohe Kunde.", "tokens": ["Als", "br\u00e4cht'", "ein", "Brief", "von", "dir", "mir", "fro\u00b7he", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPR", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon sonst bin ich von dir entfernt gewesen,", "tokens": ["Schon", "sonst", "bin", "ich", "von", "dir", "ent\u00b7fernt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und du und deine Liebe schien ein Tr\u00e4umen,", "tokens": ["Und", "du", "und", "dei\u00b7ne", "Lie\u00b7be", "schien", "ein", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich besa\u00df dich nur durch meinen Glauben:", "tokens": ["Und", "ich", "be\u00b7sa\u00df", "dich", "nur", "durch", "mei\u00b7nen", "Glau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So kann ich nun in Blumen, Sternen lesen", "tokens": ["So", "kann", "ich", "nun", "in", "Blu\u00b7men", ",", "Ster\u00b7nen", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dir, mein Freund, entfernt in gro\u00dfern R\u00e4umen.", "tokens": ["Von", "dir", ",", "mein", "Freund", ",", "ent\u00b7fernt", "in", "gro\u00b7\u00dfern", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Zeit, nicht Tod kann dich mir jemals rauben.", "tokens": ["Nicht", "Zeit", ",", "nicht", "Tod", "kann", "dich", "mir", "je\u00b7mals", "rau\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}