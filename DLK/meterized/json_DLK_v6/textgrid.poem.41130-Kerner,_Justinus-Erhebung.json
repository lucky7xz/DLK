{"textgrid.poem.41130": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Erhebung", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das Kamel sich aufhebt klagelos", "tokens": ["Wie", "das", "Ka\u00b7mel", "sich", "auf\u00b7hebt", "kla\u00b7ge\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "ADJD"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit Lasten, die der F\u00fchrer ihm aufband,", "tokens": ["Mit", "Las\u00b7ten", ",", "die", "der", "F\u00fch\u00b7rer", "ihm", "auf\u00b7band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und trauend ihm, da\u00df sie nicht allzu gro\u00df,", "tokens": ["Und", "trau\u00b7end", "ihm", ",", "da\u00df", "sie", "nicht", "all\u00b7zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinschreitet mutig durch der W\u00fcste Sand:", "tokens": ["Hin\u00b7schrei\u00b7tet", "mu\u00b7tig", "durch", "der", "W\u00fcs\u00b7te", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Also erheb dich ohne Klagewort", "tokens": ["Al\u00b7so", "er\u00b7heb", "dich", "oh\u00b7ne", "Kla\u00b7ge\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Mit jenen Lasten, die in deinen Scho\u00df,", "tokens": ["Mit", "je\u00b7nen", "Las\u00b7ten", ",", "die", "in", "dei\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl abgewogen legte Gottes Hand;", "tokens": ["Wohl", "ab\u00b7ge\u00b7wo\u00b7gen", "leg\u00b7te", "Got\u00b7tes", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Trag still sie durch des Lebens W\u00fcste fort.", "tokens": ["Trag", "still", "sie", "durch", "des", "Le\u00b7bens", "W\u00fcs\u00b7te", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}