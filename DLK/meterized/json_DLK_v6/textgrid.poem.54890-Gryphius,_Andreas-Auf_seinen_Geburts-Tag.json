{"textgrid.poem.54890": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Auf seinen Geburts-Tag", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich die Zeiten \u00fcberlege", "tokens": ["Wenn", "ich", "die", "Zei\u00b7ten", "\u00fc\u00b7berl\u00b7e\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich des H\u00f6chsten Gunst erwege", "tokens": ["Wenn", "ich", "des", "H\u00f6chs\u00b7ten", "Gunst", "er\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die in der Zeit er mir erweist", "tokens": ["Die", "in", "der", "Zeit", "er", "mir", "er\u00b7weist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sinck ich ehrerbietig nieder", "tokens": ["So", "sinck", "ich", "ehr\u00b7er\u00b7bie\u00b7tig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und opffre Danck- und Freuden-Lieder", "tokens": ["Und", "opff\u00b7re", "Dan\u00b7ck", "und", "Freu\u00b7den\u00b7Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem/ der Lufft/ Erd/ und Himmel preist.", "tokens": ["Dem", "/", "der", "Lufft", "/", "Erd", "/", "und", "Him\u00b7mel", "preist", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "NN", "$(", "NN", "$(", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hat mich frisch und unverletzet", "tokens": ["Er", "hat", "mich", "frisch", "und", "un\u00b7ver\u00b7let\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Mutterleib ins Licht versetzet;", "tokens": ["Aus", "Mut\u00b7ter\u00b7leib", "ins", "Licht", "ver\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Licht/ das mit hochheiterm Schein", "tokens": ["Ins", "Licht", "/", "das", "mit", "hoch\u00b7hei\u00b7term", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seine Kirche strahlt und brennet/", "tokens": ["Durch", "sei\u00b7ne", "Kir\u00b7che", "strahlt", "und", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er hat mich vor sein Kind erkennet/", "tokens": ["Er", "hat", "mich", "vor", "sein", "Kind", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schrieb ins Lebens-Buch mich ein.", "tokens": ["Und", "schrieb", "ins", "Le\u00b7bens\u00b7Buch", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er hat/ als alles mir entsuncken/", "tokens": ["Er", "hat", "/", "als", "al\u00b7les", "mir", "ent\u00b7sun\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "PIS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich in Ach und Angst ertruncken/", "tokens": ["Als", "ich", "in", "Ach", "und", "Angst", "er\u00b7trun\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geboten H\u00fclff und treue Hand.", "tokens": ["Ge\u00b7bo\u00b7ten", "H\u00fclff", "und", "treu\u00b7e", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich um Freund und Eltern kommen.", "tokens": ["Als", "ich", "um", "Freund", "und", "El\u00b7tern", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat Er mich in die Scho\u00df genommen:", "tokens": ["Hat", "Er", "mich", "in", "die", "Scho\u00df", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er gab mir selbst sein Hertz zu Pfand.", "tokens": ["Er", "gab", "mir", "selbst", "sein", "Hertz", "zu", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich auf so viel glatten Wegen", "tokens": ["Da\u00df", "ich", "auf", "so", "viel", "glat\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Umringt mit Blitz und Donnerschl\u00e4gen", "tokens": ["Um\u00b7ringt", "mit", "Blitz", "und", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch unversehrt geh nach dem Ziel", "tokens": ["Noch", "un\u00b7ver\u00b7sehrt", "geh", "nach", "dem", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Zweck und Ende meiner Reise", "tokens": ["Dem", "Zweck", "und", "En\u00b7de", "mei\u00b7ner", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Danck ich nur Ihm/ der Stra\u00df und Weise", "tokens": ["Danck", "ich", "nur", "Ihm", "/", "der", "Stra\u00df", "und", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "PPER", "$(", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst zeigt und selbst mich f\u00fchren wil.", "tokens": ["Selbst", "zeigt", "und", "selbst", "mich", "f\u00fch\u00b7ren", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier bin ich Herr! beut deinem Knechte", "tokens": ["Hier", "bin", "ich", "Herr", "!", "beut", "dei\u00b7nem", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein ewig starck und treue Rechte", "tokens": ["Dein", "e\u00b7wig", "starck", "und", "treu\u00b7e", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir schwindelt auf der steilen Bahn.", "tokens": ["Mir", "schwin\u00b7delt", "auf", "der", "stei\u00b7len", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo du nicht h\u00e4ltest ists geschehen", "tokens": ["Wo", "du", "nicht", "h\u00e4l\u00b7test", "ists", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O du mein Leitstern la\u00df dich sehen/", "tokens": ["O", "du", "mein", "Leits\u00b7tern", "la\u00df", "dich", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "VVIMP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst leider ists um mich gethan!", "tokens": ["Sonst", "lei\u00b7der", "ists", "um", "mich", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du wirst mich ja durch Feind und Schrecken", "tokens": ["Du", "wirst", "mich", "ja", "durch", "Feind", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Furcht auf Furchten sich erwecken/", "tokens": ["Wo", "Furcht", "auf", "Furch\u00b7ten", "sich", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo keine Wehmuth ist zu gro\u00df/", "tokens": ["Wo", "kei\u00b7ne", "Weh\u00b7muth", "ist", "zu", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich di\u00df Elend \u00fcberstanden", "tokens": ["Wenn", "ich", "di\u00df", "E\u00b7lend", "\u00fc\u00b7bers\u00b7tan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entfreit von Kercker Ach und Banden", "tokens": ["Ent\u00b7freit", "von", "Ker\u00b7cker", "Ach", "und", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Heimf\u00fchren in dein Ehren-Schlo\u00df.", "tokens": ["Heim\u00b7f\u00fch\u00b7ren", "in", "dein", "Eh\u00b7ren\u00b7Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}