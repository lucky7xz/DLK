{"dta.poem.15716": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im goldnen Alter, da ein Paradies hienieden,", "tokens": ["Im", "gold\u00b7nen", "Al\u00b7ter", ",", "da", "ein", "Pa\u00b7ra\u00b7dies", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ew'ger Fr\u00fchling war darin und ew'ger Frieden \u2014", "tokens": ["Und", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "war", "da\u00b7rin", "und", "ew'\u00b7ger", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PAV", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die junge Knospe ward genagt von keinem Wurm,", "tokens": ["Die", "jun\u00b7ge", "Knos\u00b7pe", "ward", "ge\u00b7nagt", "von", "kei\u00b7nem", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihre volle Kron' entbl\u00e4tterte kein Sturm.", "tokens": ["Und", "ih\u00b7re", "vol\u00b7le", "Kron'", "ent\u00b7bl\u00e4t\u00b7ter\u00b7te", "kein", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Noch nicht gebunden war an Arbeit der Genu\u00df,", "tokens": ["Noch", "nicht", "ge\u00b7bun\u00b7den", "war", "an", "Ar\u00b7beit", "der", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht der Freude nach schlich heimlich Ueberdru\u00df.", "tokens": ["Und", "nicht", "der", "Freu\u00b7de", "nach", "schlich", "heim\u00b7lich", "Ue\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPR", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In Trauben war kein Rausch, noch an der Lieb' ein Dorn,", "tokens": ["In", "Trau\u00b7ben", "war", "kein", "Rausch", ",", "noch", "an", "der", "Lieb'", "ein", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Auge keine Thr\u00e4n', und in der Brust kein Zorn.", "tokens": ["Im", "Au\u00b7ge", "kei\u00b7ne", "Thr\u00e4n'", ",", "und", "in", "der", "Brust", "kein", "Zorn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "KON", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da hatte Echo's Ohr noch keine Klag' empfangen,", "tokens": ["Da", "hat\u00b7te", "E\u00b7cho's", "Ohr", "noch", "kei\u00b7ne", "Klag'", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und spiegeln sah der See nur L\u00e4cheln auf den Wangen.", "tokens": ["Und", "spie\u00b7geln", "sah", "der", "See", "nur", "L\u00e4\u00b7cheln", "auf", "den", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ART", "NN", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Am Himmel Sonn' und Thau, nicht Wolk' und Ungewitter,", "tokens": ["Am", "Him\u00b7mel", "Sonn'", "und", "Thau", ",", "nicht", "Wol\u00b7k'", "und", "Un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht giftig war die Schlang' und Wermuth noch nicht bitter.", "tokens": ["Nicht", "gif\u00b7tig", "war", "die", "Schlang'", "und", "Wer\u00b7muth", "noch", "nicht", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "KON", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Da mu\u00dften V\u00f6gel noch nicht wandern um zu br\u00fcten,", "tokens": ["Da", "mu\u00df\u00b7ten", "V\u00f6\u00b7gel", "noch", "nicht", "wan\u00b7dern", "um", "zu", "br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADV", "PTKNEG", "VVFIN", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und B\u00e4ume f\u00fcr die Frucht nicht opfern ihre Bl\u00fcten.", "tokens": ["Und", "B\u00e4u\u00b7me", "f\u00fcr", "die", "Frucht", "nicht", "op\u00b7fern", "ih\u00b7re", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "PTKNEG", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nicht Neid noch Eifersucht, nicht Ha\u00df noch Zwietracht fand", "tokens": ["Nicht", "Neid", "noch", "Ei\u00b7fer\u00b7sucht", ",", "nicht", "Ha\u00df", "noch", "Zwiet\u00b7racht", "fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "NN", "$,", "PTKNEG", "NN", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem Herzen Raum, das sich voll Gl\u00fcck empfand.", "tokens": ["In", "ei\u00b7nem", "Her\u00b7zen", "Raum", ",", "das", "sich", "voll", "Gl\u00fcck", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "PRF", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sie waren alle gleich, und sahn mit Wohlgefallen", "tokens": ["Sie", "wa\u00b7ren", "al\u00b7le", "gleich", ",", "und", "sahn", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In fremdem Gl\u00fcck ihr Bild, und liebten sich in allen.", "tokens": ["In", "frem\u00b7dem", "Gl\u00fcck", "ihr", "Bild", ",", "und", "lieb\u00b7ten", "sich", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Um G\u00fcter war kein Streit, sie waren allgemein,", "tokens": ["Um", "G\u00fc\u00b7ter", "war", "kein", "Streit", ",", "sie", "wa\u00b7ren", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Ich und Du entzweit, und gleichviel Mein und Dein.", "tokens": ["Nicht", "Ich", "und", "Du", "ent\u00b7zweit", ",", "und", "gleich\u00b7viel", "Mein", "und", "Dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "KON", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "In sich verst\u00e4ndlich klar, empfunden, nicht erdacht,", "tokens": ["In", "sich", "ver\u00b7st\u00e4nd\u00b7lich", "klar", ",", "emp\u00b7fun\u00b7den", ",", "nicht", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "ADJD", "$,", "VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Liebestausch von Welt und Sinn hervorgebracht,", "tokens": ["Im", "Lie\u00b7be\u00b7stausch", "von", "Welt", "und", "Sinn", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Verst\u00e4ndnis ihrer selbst, Verst\u00e4ndnis der Natur,", "tokens": ["Ver\u00b7st\u00e4nd\u00b7nis", "ih\u00b7rer", "selbst", ",", "Ver\u00b7st\u00e4nd\u00b7nis", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War ihrer Sprache Schall, ein Loblied Gottes nur.", "tokens": ["War", "ih\u00b7rer", "Spra\u00b7che", "Schall", ",", "ein", "Lob\u00b7lied", "Got\u00b7tes", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "So war ihr Leben, doch ihr Tod war sch\u00f6ner noch,", "tokens": ["So", "war", "ihr", "Le\u00b7ben", ",", "doch", "ihr", "Tod", "war", "sch\u00f6\u00b7ner", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch den die Bl\u00fct' am Baum des Lebens aufgieng hoch.", "tokens": ["Durch", "den", "die", "Bl\u00fct'", "am", "Baum", "des", "Le\u00b7bens", "auf\u00b7gieng", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Von h\u00f6herm Daseyn nicht ein mattes dunkles Ahnen,", "tokens": ["Von", "h\u00f6\u00b7herm", "Da\u00b7seyn", "nicht", "ein", "mat\u00b7tes", "dunk\u00b7les", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es waren klar geschaut lichtaufgethane Bahnen;", "tokens": ["Es", "wa\u00b7ren", "klar", "ge\u00b7schaut", "lich\u00b7tauf\u00b7ge\u00b7tha\u00b7ne", "Bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Kein Schweben zwischen Furcht und Glauben, Wahn und Hoffen,", "tokens": ["Kein", "Schwe\u00b7ben", "zwi\u00b7schen", "Furcht", "und", "Glau\u00b7ben", ",", "Wahn", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sel'ge Zukunft lag dem Geist zum Eintritt offen.", "tokens": ["Die", "sel'\u00b7ge", "Zu\u00b7kunft", "lag", "dem", "Geist", "zum", "Ein\u00b7tritt", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}