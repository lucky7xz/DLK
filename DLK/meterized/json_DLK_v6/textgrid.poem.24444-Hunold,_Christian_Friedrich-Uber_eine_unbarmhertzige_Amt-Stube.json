{"textgrid.poem.24444": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber eine unbarmhertzige Amt-Stube", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr m\u00fcsset alle rein/", "tokens": ["Ihr", "m\u00fcs\u00b7set", "al\u00b7le", "rein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es sey gro\u00df oder klein.", "tokens": ["Es", "sey", "gro\u00df", "o\u00b7der", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Bader will allhier nun zu Gerichte sitzen/", "tokens": ["Ein", "Ba\u00b7der", "will", "all\u00b7hier", "nun", "zu", "Ge\u00b7rich\u00b7te", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer ja sein Blut nicht l\u00e4st/ mu\u00df dennoch wichtig schwitzen.", "tokens": ["Wer", "ja", "sein", "Blut", "nicht", "l\u00e4st", "/", "mu\u00df", "den\u00b7noch", "wich\u00b7tig", "schwit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$(", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}