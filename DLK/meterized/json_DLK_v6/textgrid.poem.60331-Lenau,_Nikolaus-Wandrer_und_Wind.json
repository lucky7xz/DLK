{"textgrid.poem.60331": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Wandrer und Wind", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbstwind, o sei willkommen!", "tokens": ["Herbst\u00b7wind", ",", "o", "sei", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VAFIN", "ADJD", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcnf Tage lag das Meer", "tokens": ["F\u00fcnf", "Ta\u00b7ge", "lag", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So still, so bang beklommen,", "tokens": ["So", "still", ",", "so", "bang", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein L\u00fcftchen zog daher.", "tokens": ["Kein", "L\u00fcft\u00b7chen", "zog", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Wind, nach deinem Rauschen", "tokens": ["O", "Wind", ",", "nach", "dei\u00b7nem", "Rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sehnt ich mich auf der See,", "tokens": ["Sehnt", "ich", "mich", "auf", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie einst mein J\u00e4gerlauschen", "tokens": ["Wie", "einst", "mein", "J\u00e4\u00b7ger\u00b7lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Wald nach Hirsch und Reh,", "tokens": ["Im", "Wald", "nach", "Hirsch", "und", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie geht es meinen W\u00e4ldern", "tokens": ["Wie", "geht", "es", "mei\u00b7nen", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am frischen Neckarflu\u00df?", "tokens": ["Am", "fri\u00b7schen", "Ne\u00b7ckar\u00b7flu\u00df", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den heimatlichen Feldern?", "tokens": ["Den", "hei\u00b7mat\u00b7li\u00b7chen", "Fel\u00b7dern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bringst du mir keinen Gru\u00df?", "tokens": ["Bringst", "du", "mir", "kei\u00b7nen", "Gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u203aentlaubt hab ich die W\u00e4lder", "tokens": ["\u203a", "ent\u00b7laubt", "hab", "ich", "die", "W\u00e4l\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im raschen Wanderzug,", "tokens": ["Im", "ra\u00b7schen", "Wan\u00b7der\u00b7zug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nahm durch die Stoppelfelder", "tokens": ["Nahm", "durch", "die", "Stop\u00b7pel\u00b7fel\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ungehemmten Flug.", "tokens": ["Den", "un\u00b7ge\u00b7hemm\u00b7ten", "Flug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun ich durch Feld und Auen", "tokens": ["Nun", "ich", "durch", "Feld", "und", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Wanderliedlein pfiff,", "tokens": ["Mein", "Wan\u00b7der\u00b7lied\u00b7lein", "pfiff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm ich nach euch zu schauen", "tokens": ["Komm", "ich", "nach", "euch", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Emigrantenschiff.", "tokens": ["Im", "E\u00b7mi\u00b7gran\u00b7ten\u00b7schiff", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weil alter Liebesbande", "tokens": ["Weil", "al\u00b7ter", "Lie\u00b7bes\u00b7ban\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Schifflein m\u00fcd und matt,", "tokens": ["Das", "Schif\u00b7flein", "m\u00fcd", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jag ichs vom Mutterstrande", "tokens": ["Jag", "ichs", "vom", "Mut\u00b7ter\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dahin, ein welkes Blatt!\u2039", "tokens": ["Da\u00b7hin", ",", "ein", "wel\u00b7kes", "Blatt", "!", "\u2039"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}