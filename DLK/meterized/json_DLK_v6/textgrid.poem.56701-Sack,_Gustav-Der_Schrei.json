{"textgrid.poem.56701": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Schrei", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dieser steingewordenen Not,", "tokens": ["Aus", "die\u00b7ser", "stein\u00b7ge\u00b7wor\u00b7de\u00b7nen", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "aus dieser Wut nach Brunst und Brot,", "tokens": ["aus", "die\u00b7ser", "Wut", "nach", "Brunst", "und", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "aus dieser lauten Totenstadt,", "tokens": ["aus", "die\u00b7ser", "lau\u00b7ten", "To\u00b7ten\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die sich mir aufgelagert hat", "tokens": ["die", "sich", "mir", "auf\u00b7ge\u00b7la\u00b7gert", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "h\u00e4rter als Erz, schwerer als Blei,", "tokens": ["h\u00e4r\u00b7ter", "als", "Erz", ",", "schwe\u00b7rer", "als", "Blei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "steigt meine Sehnsucht wie ein Schrei", "tokens": ["steigt", "mei\u00b7ne", "Sehn\u00b7sucht", "wie", "ein", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "quellend empor nach Meeren und Weiten", "tokens": ["quel\u00b7lend", "em\u00b7por", "nach", "Mee\u00b7ren", "und", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "und ungeheuren Einsamkeiten,", "tokens": ["und", "un\u00b7ge\u00b7heu\u00b7ren", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "aus all dem Staub und Schmutz und Gewimmel", "tokens": ["aus", "all", "dem", "Staub", "und", "Schmutz", "und", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "nach einem grenzenlosen Himmel.", "tokens": ["nach", "ei\u00b7nem", "gren\u00b7zen\u00b7lo\u00b7sen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}