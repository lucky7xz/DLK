{"dta.poem.3190": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir haben, spricht der Herr, der Erde Schmuck bereitet,", "tokens": ["Wir", "ha\u00b7ben", ",", "spricht", "der", "Herr", ",", "der", "Er\u00b7de", "Schmuck", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit daran gepr\u00fcft sei, wer durch's Leben schreitet.", "tokens": ["Da\u00b7mit", "da\u00b7ran", "ge\u00b7pr\u00fcft", "sei", ",", "wer", "durch's", "Le\u00b7ben", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PAV", "VVPP", "VAFIN", "$,", "PWS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer nach dem Schmucke hascht und sich darin verf\u00e4ngt,", "tokens": ["Wer", "nach", "dem", "Schmu\u00b7cke", "hascht", "und", "sich", "da\u00b7rin", "ver\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gelangt zum Himmel nicht, weil er am Ird'schen h\u00e4ngt.", "tokens": ["Ge\u00b7langt", "zum", "Him\u00b7mel", "nicht", ",", "weil", "er", "am", "Ird'\u00b7schen", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wer mit F\u00fc\u00dfen tritt den Schmuck und ihn verachtet,", "tokens": ["Doch", "wer", "mit", "F\u00fc\u00b7\u00dfen", "tritt", "den", "Schmuck", "und", "ihn", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat h\u00f6hern Sinn, nur da\u00df er zu gewaltsam trachtet.", "tokens": ["Hat", "h\u00f6\u00b7hern", "Sinn", ",", "nur", "da\u00df", "er", "zu", "ge\u00b7walt\u00b7sam", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADV", "KOUS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer mit dem Schmuck sich schm\u00fcckt, und, wie er Blumen pfl\u00fcckt,", "tokens": ["Wer", "mit", "dem", "Schmuck", "sich", "schm\u00fcckt", ",", "und", ",", "wie", "er", "Blu\u00b7men", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,", "KON", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Ziel h\u00e4lt unverr\u00fcckt, nur der ist ganz begl\u00fcckt.", "tokens": ["Sein", "Ziel", "h\u00e4lt", "un\u00b7ver\u00b7r\u00fcckt", ",", "nur", "der", "ist", "ganz", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "ADV", "ART", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O freue dich, da\u00df, wo du gehst, an deinen Pfaden", "tokens": ["O", "freu\u00b7e", "dich", ",", "da\u00df", ",", "wo", "du", "gehst", ",", "an", "dei\u00b7nen", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die k\u00fchlen Schatten stehn, die dich zur Ruh einladen.", "tokens": ["Die", "k\u00fch\u00b7len", "Schat\u00b7ten", "stehn", ",", "die", "dich", "zur", "Ruh", "ein\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erquick' und st\u00e4rke dich, doch nicht in tr\u00e4ger Rast", "tokens": ["Er\u00b7quick'", "und", "st\u00e4r\u00b7ke", "dich", ",", "doch", "nicht", "in", "tr\u00e4\u00b7ger", "Rast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,", "ADV", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergi\u00df des Weges, den du noch zu machen hast.", "tokens": ["Ver\u00b7gi\u00df", "des", "We\u00b7ges", ",", "den", "du", "noch", "zu", "ma\u00b7chen", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}