{"dta.poem.20677": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Danklied  \n am  \n  drey und sechzigsten Geburtstage  \n nach langwieriger Krankheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pfer, mein Erhalter,                 ", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "mein", "Er\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du kanntest, eh\u2019 du Menschen schufst,", "tokens": ["Du", "kann\u00b7test", ",", "eh'", "du", "Men\u00b7schen", "schufst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich und mein sinkend Alter,", "tokens": ["Mich", "und", "mein", "sin\u00b7kend", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hilfst meiner Schwachheit auf und rufst", "tokens": ["Hilfst", "mei\u00b7ner", "Schwach\u00b7heit", "auf", "und", "rufst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allm\u00e4chtig mich vom Grabe,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "mich", "vom", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als ob ich wieder neu", "tokens": ["Als", "ob", "ich", "wie\u00b7der", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit deiner Lebensgabe", "tokens": ["Mit", "dei\u00b7ner", "Le\u00b7bens\u00b7ga\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ans Licht geboren sey.", "tokens": ["Ans", "Licht", "ge\u00b7bo\u00b7ren", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du giebst zum Ewigheile", "tokens": ["Du", "giebst", "zum", "E\u00b7wig\u00b7hei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Ewigwohlseyn giebst du mir", "tokens": ["Zum", "E\u00b7wig\u00b7wohl\u00b7seyn", "giebst", "du", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch l\u00e4ngre Lebensweile,", "tokens": ["Noch", "l\u00e4ng\u00b7re", "Le\u00b7bens\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df meine Seele sich zu dir", "tokens": ["Da\u00df", "mei\u00b7ne", "See\u00b7le", "sich", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erheben soll hiernieden", "tokens": ["Er\u00b7he\u00b7ben", "soll", "hier\u00b7nie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo sie am Staube hing,", "tokens": ["Wo", "sie", "am", "Stau\u00b7be", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ganz von dir geschieden", "tokens": ["Und", "ganz", "von", "dir", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verworrne Wege ging.", "tokens": ["Ver\u00b7worr\u00b7ne", "We\u00b7ge", "ging", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie grub zum Freudetrinken", "tokens": ["Sie", "grub", "zum", "Freu\u00b7de\u00b7trin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich selber Br\u00fcnnlein s\u00fc\u00df und lieb,", "tokens": ["Sich", "sel\u00b7ber", "Br\u00fcnn\u00b7lein", "s\u00fc\u00df", "und", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Rechten und zur Linken,", "tokens": ["Zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verga\u00df dich Urquell, trank und blieb", "tokens": ["Ver\u00b7ga\u00df", "dich", "Ur\u00b7quell", ",", "trank", "und", "blieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stets durstig, und erkennet", "tokens": ["Stets", "durs\u00b7tig", ",", "und", "er\u00b7ken\u00b7net"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit ungel\u00f6schter Gier,", "tokens": ["Mit", "un\u00b7ge\u00b7l\u00f6schter", "Gier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df sie nur sehnt und brennet,", "tokens": ["Da\u00df", "sie", "nur", "sehnt", "und", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du Lebensborn nach dir.", "tokens": ["Du", "Le\u00b7bens\u00b7born", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie kehrt voll Schaam und Reue", "tokens": ["Sie", "kehrt", "voll", "Schaam", "und", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom langen Irrsaal um, und dankt", "tokens": ["Vom", "lan\u00b7gen", "Irr\u00b7saal", "um", ",", "und", "dankt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir deine Vatertreue,", "tokens": ["Dir", "dei\u00b7ne", "Va\u00b7ter\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nimmer m\u00fcdet, nimmer wankt;", "tokens": ["Die", "nim\u00b7mer", "m\u00fc\u00b7det", ",", "nim\u00b7mer", "wankt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich prei\u00dft sie, der dem S\u00fcnder", "tokens": ["Dich", "prei\u00dft", "sie", ",", "der", "dem", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zeit umzukehren giebt,", "tokens": ["Zeit", "um\u00b7zu\u00b7keh\u00b7ren", "giebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, weil er irrt, nicht minder", "tokens": ["Und", ",", "weil", "er", "irrt", ",", "nicht", "min\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihn in der Irre liebt.", "tokens": ["Ihn", "in", "der", "Ir\u00b7re", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du liebtest meine Seele,", "tokens": ["Du", "lieb\u00b7test", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh\u2019 du der Erde Grund gelegt;", "tokens": ["Eh'", "du", "der", "Er\u00b7de", "Grund", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du giebst sie nicht der H\u00f6hle,", "tokens": ["Du", "giebst", "sie", "nicht", "der", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worein man ihre H\u00fclle tr\u00e4gt;", "tokens": ["Wo\u00b7rein", "man", "ih\u00b7re", "H\u00fcl\u00b7le", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Odem deines Mundes", "tokens": ["Der", "O\u00b7dem", "dei\u00b7nes", "Mun\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Belebte dies Gebein,", "tokens": ["Be\u00b7leb\u00b7te", "dies", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kraft meines Hoffnunggrundes", "tokens": ["Kraft", "mei\u00b7nes", "Hoff\u00b7nung\u00b7grun\u00b7des"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "K\u00f6mmt wieder Leben drein.", "tokens": ["K\u00f6mmt", "wie\u00b7der", "Le\u00b7ben", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Aus meiner Asche f\u00e4hret", "tokens": ["Aus", "mei\u00b7ner", "A\u00b7sche", "f\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein K\u00f6rper, leicht wie reine Luft,", "tokens": ["Ein", "K\u00f6r\u00b7per", ",", "leicht", "wie", "rei\u00b7ne", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir empor gekehret,", "tokens": ["Zu", "dir", "em\u00b7por", "ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn deine Stimme Todten ruft.", "tokens": ["Wenn", "dei\u00b7ne", "Stim\u00b7me", "Tod\u00b7ten", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich kleidest du mit Schimmer,", "tokens": ["Mich", "klei\u00b7dest", "du", "mit", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich selber werd es seyn,", "tokens": ["Ich", "sel\u00b7ber", "werd", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will Dich sehn und immer", "tokens": ["Ich", "will", "Dich", "sehn", "und", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich deiner Liebe frenn!", "tokens": ["Mich", "dei\u00b7ner", "Lie\u00b7be", "frenn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}