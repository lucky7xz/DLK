{"textgrid.poem.65927": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wesen", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Sonne ist mein Wesen hergesponnen:", "tokens": ["Aus", "Son\u00b7ne", "ist", "mein", "We\u00b7sen", "her\u00b7ge\u00b7spon\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfohlen ward ein Leib in seine Hut,", "tokens": ["Emp\u00b7foh\u00b7len", "ward", "ein", "Leib", "in", "sei\u00b7ne", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und er gedeiht, denn eifrig quillt das Blut,", "tokens": ["Und", "er", "ge\u00b7deiht", ",", "denn", "eif\u00b7rig", "quillt", "das", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Auge nun hat fromm sein Gl\u00fcck begonnen.", "tokens": ["Mein", "Au\u00b7ge", "nun", "hat", "fromm", "sein", "Gl\u00fcck", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Sonne zu, bin ich aus Haft entronnen,", "tokens": ["Der", "Son\u00b7ne", "zu", ",", "bin", "ich", "aus", "Haft", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In der umwurzelt Gott auf uns beruht;", "tokens": ["In", "der", "um\u00b7wur\u00b7zelt", "Gott", "auf", "uns", "be\u00b7ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt seh ich Ihn: sein Blick bewahrt mich gut,", "tokens": ["Jetzt", "seh", "ich", "Ihn", ":", "sein", "Blick", "be\u00b7wahrt", "mich", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drum Welt, dir hab ich goldnen Preis gewonnen!", "tokens": ["Drum", "Welt", ",", "dir", "hab", "ich", "gold\u00b7nen", "Preis", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dem Baum verwandt, mit seinen Fr\u00fchjahrstrieben,", "tokens": ["Dem", "Baum", "ver\u00b7wandt", ",", "mit", "sei\u00b7nen", "Fr\u00fch\u00b7jahr\u00b7strie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Mensch, bin Sonne ich und dann ihr Regen;", "tokens": ["O", "Mensch", ",", "bin", "Son\u00b7ne", "ich", "und", "dann", "ihr", "Re\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "NN", "PPER", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Heimatwind wird unsre Wipfel lieben.", "tokens": ["Ein", "Hei\u00b7mat\u00b7wind", "wird", "uns\u00b7re", "Wip\u00b7fel", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie Schatten mag sich Demut um dich legen:", "tokens": ["Wie", "Schat\u00b7ten", "mag", "sich", "De\u00b7mut", "um", "dich", "le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VMFIN", "PRF", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Sonnenk\u00fcnder, der so k\u00fchn geblieben,", "tokens": ["Doch", "Son\u00b7nen\u00b7k\u00fcn\u00b7der", ",", "der", "so", "k\u00fchn", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lebendiger, besorg der S\u00f6hne Segen!", "tokens": ["Le\u00b7ben\u00b7di\u00b7ger", ",", "be\u00b7sorg", "der", "S\u00f6h\u00b7ne", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}