{"dta.poem.15986": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du verstehest, reizt dich wenig; was du nicht", "tokens": ["Was", "du", "ver\u00b7ste\u00b7hest", ",", "reizt", "dich", "we\u00b7nig", ";", "was", "du", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$.", "PWS", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verstehst, spricht dich nicht an; was willst du vom Gedicht?", "tokens": ["Ver\u00b7stehst", ",", "spricht", "dich", "nicht", "an", ";", "was", "willst", "du", "vom", "Ge\u00b7dicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "PWS", "VMFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du willst mit Recht, es sei verst\u00e4ndlich-unverst\u00e4ndlich,", "tokens": ["Du", "willst", "mit", "Recht", ",", "es", "sei", "ver\u00b7st\u00e4nd\u00b7lich\u00b7un\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollendet an Gestalt, doch an Gehalt unendlich.", "tokens": ["Voll\u00b7en\u00b7det", "an", "Ge\u00b7stalt", ",", "doch", "an", "Ge\u00b7halt", "un\u00b7end\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word