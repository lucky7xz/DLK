{"dta.poem.8898": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "So d\u00fcrfte Keiner sich verwundert zeigen,", "tokens": ["So", "d\u00fcrf\u00b7te", "Kei\u00b7ner", "sich", "ver\u00b7wun\u00b7dert", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich nicht ganz verm\u00f6chte zu verschweigen,", "tokens": ["Wenn", "ich", "nicht", "ganz", "ver\u00b7m\u00f6ch\u00b7te", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie deine Liebe mir die Seele spaltet.", "tokens": ["Wie", "dei\u00b7ne", "Lie\u00b7be", "mir", "die", "See\u00b7le", "spal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, da\u00df nie mir dies Gef\u00fchl veraltet,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "nie", "mir", "dies", "Ge\u00b7f\u00fchl", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn mit Venedig wird sich's eng verzweigen:", "tokens": ["Denn", "mit", "Ve\u00b7ne\u00b7dig", "wird", "sich's", "eng", "ver\u00b7zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VAFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Stets wird ein Seufzer meiner Brust entsteigen", "tokens": ["Stets", "wird", "ein", "Seuf\u00b7zer", "mei\u00b7ner", "Brust", "ent\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach einem Lenz, der sich nur halb entfaltet.", "tokens": ["Nach", "ei\u00b7nem", "Lenz", ",", "der", "sich", "nur", "halb", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie soll der Fremdling eine Gunst dir danken,", "tokens": ["Wie", "soll", "der", "Fremd\u00b7ling", "ei\u00b7ne", "Gunst", "dir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst wenn dein Herz ihn zu begl\u00fccken d\u00e4chte,", "tokens": ["Selbst", "wenn", "dein", "Herz", "ihn", "zu", "be\u00b7gl\u00fc\u00b7cken", "d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begegnend ihm in z\u00e4rtlichen Gedanken?", "tokens": ["Be\u00b7ge\u00b7gnend", "ihm", "in", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Kein Mittel giebt's, das mich dir n\u00e4her br\u00e4chte,", "tokens": ["Kein", "Mit\u00b7tel", "giebt's", ",", "das", "mich", "dir", "n\u00e4\u00b7her", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und einsam siehst du meine Tritte wanken", "tokens": ["Und", "ein\u00b7sam", "siehst", "du", "mei\u00b7ne", "Trit\u00b7te", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Markus auf und nieder alle N\u00e4chte.", "tokens": ["Den", "Mar\u00b7kus", "auf", "und", "nie\u00b7der", "al\u00b7le", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}