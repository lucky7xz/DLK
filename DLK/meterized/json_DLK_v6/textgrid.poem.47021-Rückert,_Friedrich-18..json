{"textgrid.poem.47021": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und will's so ganz und gar denn nicht vom Platze,", "tokens": ["Und", "will's", "so", "ganz", "und", "gar", "denn", "nicht", "vom", "Plat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "KON", "ADV", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Herz, mit deinem Flehen, Seufzen, Lallen;", "tokens": ["O", "Herz", ",", "mit", "dei\u00b7nem", "Fle\u00b7hen", ",", "Seuf\u00b7zen", ",", "Lal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieh, ob das Spiel ihr besser wird gefallen,", "tokens": ["Sieh", ",", "ob", "das", "Spiel", "ihr", "bes\u00b7ser", "wird", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn du's versuchest aus dem Gegensatze.", "tokens": ["Wenn", "du's", "ver\u00b7su\u00b7chest", "aus", "dem", "Ge\u00b7gen\u00b7sat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf, sei aus Inbrunst z\u00e4nkisch gleich dem Spatze,", "tokens": ["Auf", ",", "sei", "aus", "In\u00b7brunst", "z\u00e4n\u00b7kisch", "gleich", "dem", "Spat\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VAFIN", "APPR", "NN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es half nicht z\u00e4rtlich sein gleich Nachtigallen;", "tokens": ["Es", "half", "nicht", "z\u00e4rt\u00b7lich", "sein", "gleich", "Nach\u00b7ti\u00b7gal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "VAINF", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es frommt nicht frommen L\u00e4mmern gleich zu wallen,", "tokens": ["Es", "frommt", "nicht", "from\u00b7men", "L\u00e4m\u00b7mern", "gleich", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versuch es denn mit scharfer Tatz' und kratze.", "tokens": ["Ver\u00b7such", "es", "denn", "mit", "schar\u00b7fer", "Tatz'", "und", "krat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sei ganz an Art und Laun' und List ein K\u00e4tzchen,", "tokens": ["Sei", "ganz", "an", "Art", "und", "Laun'", "und", "List", "ein", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dr\u00e4ng' dich an sie mit H\u00e4keln und mit Schmeicheln,", "tokens": ["Dr\u00e4ng'", "dich", "an", "sie", "mit", "H\u00e4\u00b7keln", "und", "mit", "Schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Lern' art'ge Ungezogenheiten treiben.", "tokens": ["Lern'", "art'\u00b7ge", "Un\u00b7ge\u00b7zo\u00b7gen\u00b7hei\u00b7ten", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Katzenfreundin g\u00f6nnt dir dann ein Pl\u00e4tzchen", "tokens": ["Die", "Kat\u00b7zen\u00b7freun\u00b7din", "g\u00f6nnt", "dir", "dann", "ein", "Pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihrem Scho\u00df, um, wo nicht dich zu streicheln,", "tokens": ["Auf", "ih\u00b7rem", "Scho\u00df", ",", "um", ",", "wo", "nicht", "dich", "zu", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "$,", "PWAV", "PTKNEG", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch mindestens mit Bosheit dich zu reiben.", "tokens": ["Doch", "min\u00b7des\u00b7tens", "mit", "Bos\u00b7heit", "dich", "zu", "rei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}