{"textgrid.poem.24796": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckliches Haus", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem offenen Altane sangen", "tokens": ["Auf", "ei\u00b7nem", "of\u00b7fe\u00b7nen", "Al\u00b7ta\u00b7ne", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Greise orgelspielend gegen Himmel,", "tokens": ["Grei\u00b7se", "or\u00b7gel\u00b7spie\u00b7lend", "ge\u00b7gen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Indes auf einer Tenne, ihm zu F\u00fc\u00dfen,", "tokens": ["In\u00b7des", "auf", "ei\u00b7ner", "Ten\u00b7ne", ",", "ihm", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schlanke mit dem b\u00e4rtigen Enkel focht,", "tokens": ["Der", "Schlan\u00b7ke", "mit", "dem", "b\u00e4r\u00b7ti\u00b7gen", "En\u00b7kel", "focht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df durch den reinen Schaft des Oleanders", "tokens": ["Da\u00df", "durch", "den", "rei\u00b7nen", "Schaft", "des", "O\u00b7lean\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Zittern aufw\u00e4rts lief; allein ein Vogel", "tokens": ["Ein", "Zit\u00b7tern", "auf\u00b7w\u00e4rts", "lief", ";", "al\u00b7lein", "ein", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Still in der Krone bl\u00fctevollem Schein", "tokens": ["Still", "in", "der", "Kro\u00b7ne", "bl\u00fc\u00b7te\u00b7vol\u00b7lem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Floh nicht und \u00e4ugte klugen Blicks herab.", "tokens": ["Floh", "nicht", "und", "\u00e4ug\u00b7te", "klu\u00b7gen", "Blicks", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf dem behauenen Rand des Brunnens aber", "tokens": ["Auf", "dem", "be\u00b7hau\u00b7e\u00b7nen", "Rand", "des", "Brun\u00b7nens", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die junge Frau gab ihrem Kind die Brust.", "tokens": ["Die", "jun\u00b7ge", "Frau", "gab", "ih\u00b7rem", "Kind", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein der Wandrer, dem die Stra\u00dfe sich", "tokens": ["Al\u00b7lein", "der", "Wand\u00b7rer", ",", "dem", "die", "Stra\u00b7\u00dfe", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "PRF"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Entlang der Tenne ums Gem\u00e4uer bog,", "tokens": ["Ent\u00b7lang", "der", "Ten\u00b7ne", "ums", "Ge\u00b7m\u00e4u\u00b7er", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warf hinter sich den einen Blick des Fremden", "tokens": ["Warf", "hin\u00b7ter", "sich", "den", "ei\u00b7nen", "Blick", "des", "Frem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PRF", "ART", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trug in sich \u2013 gleich jener Abendwolke", "tokens": ["Und", "trug", "in", "sich", "\u2013", "gleich", "je\u00b7ner", "Ab\u00b7end\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$(", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entschwebend \u00fcber stillem Flu\u00df und Wald \u2013", "tokens": ["Ent\u00b7schwe\u00b7bend", "\u00fc\u00b7ber", "stil\u00b7lem", "Flu\u00df", "und", "Wald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das wundervolle Bild des Friedens fort.", "tokens": ["Das", "wun\u00b7der\u00b7vol\u00b7le", "Bild", "des", "Frie\u00b7dens", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}