{"textgrid.poem.59255": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Unmut", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herbst! betr\u00fcbt verh\u00fcllst du", "tokens": ["O", "Herbst", "!", "be\u00b7tr\u00fcbt", "ver\u00b7h\u00fcllst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVPP", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Strom, Wald und Blumenlust,", "tokens": ["Strom", ",", "Wald", "und", "Blu\u00b7men\u00b7lust", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erbleichte Flor, wie f\u00fcllst du", "tokens": ["Er\u00b7bleich\u00b7te", "Flor", ",", "wie", "f\u00fcllst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Sehnsucht nun die Brust!", "tokens": ["Mit", "Sehn\u00b7sucht", "nun", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Weit hinter diesen H\u00f6hen", "tokens": ["Weit", "hin\u00b7ter", "die\u00b7sen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hier mich eng umstellt,", "tokens": ["Die", "hier", "mich", "eng", "um\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6r ich eratmend gehen", "tokens": ["H\u00f6r", "ich", "e\u00b7rat\u00b7mend", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den gro\u00dfen Strom der Welt.", "tokens": ["Den", "gro\u00b7\u00dfen", "Strom", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In lichtem Glanze wandelt", "tokens": ["In", "lich\u00b7tem", "Glan\u00b7ze", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Helden heil'ger Mut,", "tokens": ["Der", "Hel\u00b7den", "heil'\u00b7ger", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es steigt das Land verwandelt", "tokens": ["Es", "steigt", "das", "Land", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus seiner S\u00f6hne Blut.", "tokens": ["Aus", "sei\u00b7ner", "S\u00f6h\u00b7ne", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch mich f\u00fcllt' m\u00e4nnlich Trauern,", "tokens": ["Auch", "mich", "f\u00fcllt'", "m\u00e4nn\u00b7lich", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie euch, bei Deutschlands Wehn \u2013", "tokens": ["Wie", "euch", ",", "bei", "Deutschlands", "Wehn", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NE", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mu\u00df in Sehnsuchtsschauern", "tokens": ["Und", "mu\u00df", "in", "Sehn\u00b7suchts\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier ruhmlos untergehn!", "tokens": ["Hier", "ruh\u00b7mlos", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}