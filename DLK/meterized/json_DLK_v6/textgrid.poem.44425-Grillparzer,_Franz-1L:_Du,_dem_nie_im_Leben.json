{"textgrid.poem.44425": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, dem nie im Leben", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dem nie im Leben", "tokens": ["Du", ",", "dem", "nie", "im", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ruhstatt war, noch Haus;", "tokens": ["Ruh\u00b7statt", "war", ",", "noch", "Haus", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ruhe nun, du M\u00fcder,", "tokens": ["Ru\u00b7he", "nun", ",", "du", "M\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ruh im Tod noch aus.", "tokens": ["Ruh", "im", "Tod", "noch", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und reicht Freundestr\u00e4ne", "tokens": ["Und", "reicht", "Freun\u00b7des\u00b7tr\u00e4\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u00dcbers Grab hinaus,", "tokens": ["\u00dc\u00b7bers", "Grab", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "APZR", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6r die eignen T\u00f6ne", "tokens": ["H\u00f6r", "die", "eig\u00b7nen", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tief im stillen Haus.", "tokens": ["Tief", "im", "stil\u00b7len", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}