{"textgrid.poem.29243": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werden wo nicht meine Reime wol in fremden Ohren klingen,", "tokens": ["Wer\u00b7den", "wo", "nicht", "mei\u00b7ne", "Rei\u00b7me", "wol", "in", "frem\u00b7den", "Oh\u00b7ren", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "PTKNEG", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "M\u00fcssen fremde nur gedencken, es gescheh auch ihren Dingen;", "tokens": ["M\u00fcs\u00b7sen", "frem\u00b7de", "nur", "ge\u00b7den\u00b7cken", ",", "es", "ge\u00b7scheh", "auch", "ih\u00b7ren", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADV", "NN", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Weil die Worte wie die Menschen haben auch ihr Vaterland,", "tokens": ["Weil", "die", "Wor\u00b7te", "wie", "die", "Men\u00b7schen", "ha\u00b7ben", "auch", "ihr", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KOKOM", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gelten sie nur da am meisten, wo sie lang und wol bekant.", "tokens": ["Gel\u00b7ten", "sie", "nur", "da", "am", "meis\u00b7ten", ",", "wo", "sie", "lang", "und", "wol", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKA", "PIS", "$,", "PWAV", "PPER", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}