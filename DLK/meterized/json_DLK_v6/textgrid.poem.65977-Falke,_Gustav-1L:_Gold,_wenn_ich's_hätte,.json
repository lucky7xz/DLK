{"textgrid.poem.65977": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gold, wenn ich's h\u00e4tte,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gold, wenn ich's h\u00e4tte,", "tokens": ["Gold", ",", "wenn", "ich's", "h\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "VAFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das gro\u00dfe Los!", "tokens": ["Das", "gro\u00b7\u00dfe", "Los", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ob ich mir ein Reitpferd hielte?", "tokens": ["Ob", "ich", "mir", "ein", "Reit\u00b7pferd", "hiel\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Einen Viererzug?", "tokens": ["Ei\u00b7nen", "Vier\u00b7er\u00b7zug", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ob ich mir ein Rittergut kaufte?", "tokens": ["Ob", "ich", "mir", "ein", "Rit\u00b7ter\u00b7gut", "kauf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Vielleicht gr\u00fcndete ich ein Asyl", "tokens": ["Viel\u00b7leicht", "gr\u00fcn\u00b7de\u00b7te", "ich", "ein", "A\u00b7syl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "F\u00fcr verarmte B\u00f6rsianer", "tokens": ["F\u00fcr", "ver\u00b7arm\u00b7te", "B\u00f6r\u00b7si\u00b7a\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder invalide Rennpferde,", "tokens": ["O\u00b7der", "in\u00b7va\u00b7li\u00b7de", "Renn\u00b7pfer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Vielleicht kaufte ich Schopenhauers", "tokens": ["Viel\u00b7leicht", "kauf\u00b7te", "ich", "Scho\u00b7pen\u00b7hau\u00b7ers"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gesammelte Werke.", "tokens": ["Ge\u00b7sam\u00b7mel\u00b7te", "Wer\u00b7ke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ich th\u00e4te noch viel mehr,", "tokens": ["Ich", "th\u00e4\u00b7te", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Sch\u00f6neres, Edleres:", "tokens": ["Sch\u00f6\u00b7ne\u00b7res", ",", "Ed\u00b7le\u00b7res", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Ich rauchte eine bessere Cigarre,", "tokens": ["Ich", "rauch\u00b7te", "ei\u00b7ne", "bes\u00b7se\u00b7re", "Ci\u00b7gar\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und g\u00e4be meiner Frau", "tokens": ["Und", "g\u00e4\u00b7be", "mei\u00b7ner", "Frau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Hundert Mark,", "tokens": ["Hun\u00b7dert", "Mark", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Tausend Mark Wochengeld.", "tokens": ["Tau\u00b7send", "Mark", "Wo\u00b7chen\u00b7geld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Vielleicht auch hielt' ich eine zweite Frau,", "tokens": ["Viel\u00b7leicht", "auch", "hielt'", "ich", "ei\u00b7ne", "zwei\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein kleiner Pascha,", "tokens": ["Ein", "klei\u00b7ner", "Pa\u00b7scha", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "In jedem Stadtviertel eine.", "tokens": ["In", "je\u00b7dem", "Stadt\u00b7vier\u00b7tel", "ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Vor allem aber", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "W\u00fcrde fromm ich, sehr fromm,", "tokens": ["W\u00fcr\u00b7de", "fromm", "ich", ",", "sehr", "fromm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Und lie\u00dfe f\u00fcr Sankt Marien", "tokens": ["Und", "lie\u00b7\u00dfe", "f\u00fcr", "Sankt", "Ma\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "VVFIN", "NE"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Ein Altarbild malen:", "tokens": ["Ein", "Al\u00b7tar\u00b7bild", "ma\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Christus,", "tokens": ["Chris\u00b7tus", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Die Sch\u00e4cher zum Tempel hinausjagend.", "tokens": ["Die", "Sch\u00e4\u00b7cher", "zum", "Tem\u00b7pel", "hin\u00b7aus\u00b7ja\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Aber ein Realist sollt' es malen,", "tokens": ["A\u00b7ber", "ein", "Re\u00b7a\u00b7list", "sollt'", "es", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.27": {"text": "So einer mit gro\u00dfen, wahren Augen,", "tokens": ["So", "ei\u00b7ner", "mit", "gro\u00b7\u00dfen", ",", "wah\u00b7ren", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Der die Dinge sieht, wie sie sind,", "tokens": ["Der", "die", "Din\u00b7ge", "sieht", ",", "wie", "sie", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Ohne Heiligenschein.", "tokens": ["Oh\u00b7ne", "Hei\u00b7li\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "Christus,", "tokens": ["Chris\u00b7tus", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Mit dem heiligen Feuer des Zornes,", "tokens": ["Mit", "dem", "hei\u00b7li\u00b7gen", "Feu\u00b7er", "des", "Zor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Verachtung im edlen Antlitz,", "tokens": ["Ver\u00b7ach\u00b7tung", "im", "ed\u00b7len", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Das derbe Tau in der strafenden Hand,", "tokens": ["Das", "der\u00b7be", "Tau", "in", "der", "stra\u00b7fen\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.34": {"text": "Und vor ihm geduckt,", "tokens": ["Und", "vor", "ihm", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Zitternd, stolpernd, fluchend, greinend,", "tokens": ["Zit\u00b7ternd", ",", "stol\u00b7pernd", ",", "flu\u00b7chend", ",", "grei\u00b7nend", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "In Kaftan und Frack,", "tokens": ["In", "Kaf\u00b7tan", "und", "Frack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Schmierig au\u00dfen und innen,", "tokens": ["Schmie\u00b7rig", "au\u00b7\u00dfen", "und", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.38": {"text": "Oder nur innen,", "tokens": ["O\u00b7der", "nur", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Und au\u00dfen parf\u00fcmiert und geschniegelt,", "tokens": ["Und", "au\u00b7\u00dfen", "par\u00b7f\u00fc\u00b7miert", "und", "ge\u00b7schnie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Alle die edlen Seelen,", "tokens": ["Al\u00b7le", "die", "ed\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.41": {"text": "Die hundert Prozent nehmen;", "tokens": ["Die", "hun\u00b7dert", "Pro\u00b7zent", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Die Kaffeeschwindler mit scheinehrlichem Gesicht;", "tokens": ["Die", "Kaf\u00b7fee\u00b7schwind\u00b7ler", "mit", "schei\u00b7nehr\u00b7li\u00b7chem", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die Buttermanscher mit den angesehenen B\u00e4uchen;", "tokens": ["Die", "But\u00b7ter\u00b7man\u00b7scher", "mit", "den", "an\u00b7ge\u00b7se\u00b7he\u00b7nen", "B\u00e4u\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Die Gotteswortf\u00e4lscher", "tokens": ["Die", "Got\u00b7tes\u00b7wort\u00b7f\u00e4l\u00b7scher"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Mit den gleichfalls angesehenen B\u00e4uchen,", "tokens": ["Mit", "den", "gleich\u00b7falls", "an\u00b7ge\u00b7se\u00b7he\u00b7nen", "B\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.46": {"text": "Und noch viele andere.", "tokens": ["Und", "noch", "vie\u00b7le", "an\u00b7de\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Und einige Leute,", "tokens": ["Und", "ei\u00b7ni\u00b7ge", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Die ich besonders hasse,", "tokens": ["Die", "ich", "be\u00b7son\u00b7ders", "has\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Die sollten mir ganz vorne abkonterfeit werden,", "tokens": ["Die", "soll\u00b7ten", "mir", "ganz", "vor\u00b7ne", "ab\u00b7kon\u00b7ter\u00b7feit", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Ganz so ehrlich, tugendhaft,", "tokens": ["Ganz", "so", "ehr\u00b7lich", ",", "tu\u00b7gend\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Mit Pharis\u00e4erl\u00e4cheln,", "tokens": ["Mit", "Pha\u00b7ri\u00b7s\u00e4\u00b7er\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Wie ich t\u00e4glich sie sehe.", "tokens": ["Wie", "ich", "t\u00e4g\u00b7lich", "sie", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.53": {"text": "Aber das Genie meines Realisten", "tokens": ["A\u00b7ber", "das", "Ge\u00b7nie", "mei\u00b7nes", "Re\u00b7a\u00b7lis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.54": {"text": "Ereilte sie mit heiliger Vergeltung,", "tokens": ["Er\u00b7eil\u00b7te", "sie", "mit", "hei\u00b7li\u00b7ger", "Ver\u00b7gel\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "Und durch Farbe und Lack,", "tokens": ["Und", "durch", "Far\u00b7be", "und", "Lack", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.56": {"text": "Durch D\u00fcnkel und L\u00e4cheln", "tokens": ["Durch", "D\u00fcn\u00b7kel", "und", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.57": {"text": "Grinste ihr hohles Nichts,", "tokens": ["Grins\u00b7te", "ihr", "hoh\u00b7les", "Nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.58": {"text": "Deutlich,", "tokens": ["Deut\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.59": {"text": "Man k\u00f6nnte es mit H\u00e4nden greifen.", "tokens": ["Man", "k\u00f6nn\u00b7te", "es", "mit", "H\u00e4n\u00b7den", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Gold wenn ich's h\u00e4tte,", "tokens": ["Gold", "wenn", "ich's", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "VAFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.61": {"text": "Das gro\u00dfe Los.", "tokens": ["Das", "gro\u00b7\u00dfe", "Los", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Kein Reitpferd, keine Maitresse.", "tokens": ["Kein", "Reit\u00b7pferd", ",", "kei\u00b7ne", "Mai\u00b7tres\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Kein Asyl", "tokens": ["Kein"