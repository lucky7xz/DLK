{"dta.poem.23365": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auch ein M\u00e4dchen aus der Fremde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4dchen aus der Fremde kam", "tokens": ["Ein", "M\u00e4d\u00b7chen", "aus", "der", "Frem\u00b7de", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollte sich verm\u00e4hlen,", "tokens": ["Und", "woll\u00b7te", "sich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wollte sie den Br\u00e4utigam", "tokens": ["Doch", "woll\u00b7te", "sie", "den", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selber nur erw\u00e4hlen.", "tokens": ["Sich", "sel\u00b7ber", "nur", "er\u00b7w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Willkommen, junge fremde Fee", "tokens": ["Will\u00b7kom\u00b7men", ",", "jun\u00b7ge", "frem\u00b7de", "Fee"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Anmuth, Mild' und G\u00fcte,", "tokens": ["Voll", "An\u00b7muth", ",", "Mild'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So rein wie frisch gefallner Schnee,", "tokens": ["So", "rein", "wie", "frisch", "ge\u00b7fall\u00b7ner", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n wie Maienbl\u00fcthe!", "tokens": ["So", "sch\u00f6n", "wie", "Mai\u00b7en\u00b7bl\u00fct\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohin sie kam, da schien sogleich", "tokens": ["Wo\u00b7hin", "sie", "kam", ",", "da", "schien", "sog\u00b7leich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Alles froh zu regen,", "tokens": ["Sich", "Al\u00b7les", "froh", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo sie weilte, stand das Reich", "tokens": ["Und", "wo", "sie", "weil\u00b7te", ",", "stand", "das", "Reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Kraft und Macht und Segen.", "tokens": ["In", "Kraft", "und", "Macht", "und", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Willkommen! schollen hell und laut", "tokens": ["Will\u00b7kom\u00b7men", "!", "schol\u00b7len", "hell", "und", "laut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volkes frohe Lieder:", "tokens": ["Des", "Vol\u00b7kes", "fro\u00b7he", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O lie\u00dfe sich die holde Braut", "tokens": ["O", "lie\u00b7\u00dfe", "sich", "die", "hol\u00b7de", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei uns doch heimisch nieder!", "tokens": ["Bei", "uns", "doch", "hei\u00b7misch", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie aber bot ihr H\u00e4ndelein", "tokens": ["Sie", "a\u00b7ber", "bot", "ihr", "H\u00e4n\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur einem K\u00f6nigssohne:", "tokens": ["Nur", "ei\u00b7nem", "K\u00f6\u00b7nigs\u00b7soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich will mit ihm verm\u00e4hlet sein,", "tokens": ["Ich", "will", "mit", "ihm", "ver\u00b7m\u00e4h\u00b7let", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm und seinem Throne!", "tokens": ["Mit", "ihm", "und", "sei\u00b7nem", "Thro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er n\u00e4hme sie auch gern zur Eh',", "tokens": ["Er", "n\u00e4h\u00b7me", "sie", "auch", "gern", "zur", "Eh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn's ginge morganatisch,", "tokens": ["Wenn's", "gin\u00b7ge", "mor\u00b7ga\u00b7na\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das aber war der lieben Fee", "tokens": ["Das", "a\u00b7ber", "war", "der", "lie\u00b7ben", "Fee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch gar zu problematisch.", "tokens": ["Doch", "gar", "zu", "prob\u00b7le\u00b7ma\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sogleich war ohne alle Spur", "tokens": ["Sog\u00b7leich", "war", "oh\u00b7ne", "al\u00b7le", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fee wie weggetrieben,", "tokens": ["Die", "Fee", "wie", "weg\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns aber ist ihr Name nur:", "tokens": ["Uns", "a\u00b7ber", "ist", "ihr", "Na\u00b7me", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}