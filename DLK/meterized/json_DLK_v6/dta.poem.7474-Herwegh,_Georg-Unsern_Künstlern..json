{"dta.poem.7474": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Unsern K\u00fcnstlern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben hat am Ende doch gewonnen,", "tokens": ["Das", "Le\u00b7ben", "hat", "am", "En\u00b7de", "doch", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und all die \u00fcberhimmlischen Gestalten,", "tokens": ["Und", "all", "die", "\u00fc\u00b7ber\u00b7himm\u00b7li\u00b7schen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkl\u00e4rten Leiber und verkl\u00e4rten Falten,", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "Lei\u00b7ber", "und", "ver\u00b7kl\u00e4r\u00b7ten", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die schattenhaft durchsichtigen Madonnen,", "tokens": ["Die", "schat\u00b7ten\u00b7haft", "durch\u00b7sich\u00b7ti\u00b7gen", "Ma\u00b7don\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus Aetherduft und Veilchenblau gesponnen,", "tokens": ["Aus", "A\u00b7e\u00b7ther\u00b7duft", "und", "Veil\u00b7chen\u00b7blau", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "VVPP", "$,"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die nur auf Rosen und auf Lilien wallten, \u2014", "tokens": ["Die", "nur", "auf", "Ro\u00b7sen", "und", "auf", "Li\u00b7li\u00b7en", "wall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "APPR", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie konnten sich nicht mehr zusammenhalten,", "tokens": ["Sie", "konn\u00b7ten", "sich", "nicht", "mehr", "zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sind in Andacht gottvollst nun zerronnen.", "tokens": ["Und", "sind", "in", "An\u00b7dacht", "gott\u00b7vollst", "nun", "zer\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, liebe K\u00fcnstler, drum kein Klaggest\u00f6hn!", "tokens": ["Doch", ",", "lie\u00b7be", "K\u00fcnst\u00b7ler", ",", "drum", "kein", "Klag\u00b7ge\u00b7st\u00f6hn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde mag noch viel des Guten treiben,", "tokens": ["Die", "Er\u00b7de", "mag", "noch", "viel", "des", "Gu\u00b7ten", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlasset nur die schroffen, k\u00fchlen H\u00f6h'n;", "tokens": ["Ver\u00b7las\u00b7set", "nur", "die", "schrof\u00b7fen", ",", "k\u00fch\u00b7len", "H\u00f6h'n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sucht wieder Gott der Welt einzuverleiben!", "tokens": ["Sucht", "wie\u00b7der", "Gott", "der", "Welt", "ein\u00b7zu\u00b7ver\u00b7lei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Heilige gelingt so selten sch\u00f6n,", "tokens": ["Das", "Hei\u00b7li\u00b7ge", "ge\u00b7lingt", "so", "sel\u00b7ten", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Sch\u00f6ne nur wird ewig heilig bleiben.", "tokens": ["Das", "Sch\u00f6\u00b7ne", "nur", "wird", "e\u00b7wig", "hei\u00b7lig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}