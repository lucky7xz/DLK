{"dta.poem.12477": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.   Wohlgeboren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hab' ich es nach langen Jahren", "tokens": ["So", "hab'", "ich", "es", "nach", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu diesem Posten noch gebracht,", "tokens": ["Zu", "die\u00b7sem", "Pos\u00b7ten", "noch", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leider nur zu oft erfahren", "tokens": ["Und", "lei\u00b7der", "nur", "zu", "oft", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PTKA", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer hier im Land das Wetter macht.", "tokens": ["Wer", "hier", "im", "Land", "das", "Wet\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sollst, verdammte Freiheit! mir", "tokens": ["Du", "sollst", ",", "ver\u00b7damm\u00b7te", "Frei\u00b7heit", "!", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "$,", "ADJA", "NN", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ruhe f\u00fcrder nicht gef\u00e4hrden;", "tokens": ["Die", "Ru\u00b7he", "f\u00fcr\u00b7der", "nicht", "ge\u00b7f\u00e4hr\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lisette, noch ein Gl\u00e4schen Bier!", "tokens": ["Li\u00b7set\u00b7te", ",", "noch", "ein", "Gl\u00e4s\u00b7chen", "Bier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will ein guter B\u00fcrger werden.", "tokens": ["Ich", "will", "ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch ich sprach einst vom Vaterland", "tokens": ["Auch", "ich", "sprach", "einst", "vom", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und solchen sonderbaren Dingen,", "tokens": ["Und", "sol\u00b7chen", "son\u00b7der\u00b7ba\u00b7ren", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trug mein schwarzrothgolden Band", "tokens": ["Ich", "trug", "mein", "schwarz\u00b7roth\u00b7gol\u00b7den", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df die Sporen furchtbar klingen:", "tokens": ["Und", "lie\u00df", "die", "Spo\u00b7ren", "furcht\u00b7bar", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, selig wer im Gleise geht", "tokens": ["Doch", ",", "se\u00b7lig", "wer", "im", "Glei\u00b7se", "geht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "PWS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und still im Joche zieht auf Erden \u2014", "tokens": ["Und", "still", "im", "Jo\u00b7che", "zieht", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was hilft die Genialit\u00e4t?", "tokens": ["Was", "hilft", "die", "Ge\u00b7ni\u00b7a\u00b7li\u00b7t\u00e4t", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will ein guter B\u00fcrger werden.", "tokens": ["Ich", "will", "ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Diogenes vor seiner Tonne \u2014", "tokens": ["Dio\u00b7ge\u00b7nes", "vor", "sei\u00b7ner", "Ton\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Vortrefflich, wie beneid' ich ihn!", "tokens": ["Vor\u00b7treff\u00b7lich", ",", "wie", "be\u00b7neid'", "ich", "ihn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war noch keine Julisonne,", "tokens": ["Es", "war", "noch", "kei\u00b7ne", "Ju\u00b7li\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die jenen Gl\u00fccklichen beschien.", "tokens": ["Die", "je\u00b7nen", "Gl\u00fcck\u00b7li\u00b7chen", "be\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Monarchie? was Republik?", "tokens": ["Was", "Mon\u00b7ar\u00b7chie", "?", "was", "Re\u00b7pub\u00b7lik", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "PWS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sich die Leute toll geberden!", "tokens": ["Wie", "sich", "die", "Leu\u00b7te", "toll", "ge\u00b7ber\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Teufel mit der Politik!", "tokens": ["Zum", "Teu\u00b7fel", "mit", "der", "Po\u00b7li\u00b7tik", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will ein guter B\u00fcrger werden.", "tokens": ["Ich", "will", "ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gewi\u00df, man tobt sich Einmal aus \u2014", "tokens": ["Ge\u00b7wi\u00df", ",", "man", "tobt", "sich", "Ein\u00b7mal", "aus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "PRF", "ADV", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es w\u00e4r' ja um die Jugend schade \u2014", "tokens": ["Es", "w\u00e4r'", "ja", "um", "die", "Ju\u00b7gend", "scha\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, f\u00fchrt man erst sein eignes Haus,", "tokens": ["Doch", ",", "f\u00fchrt", "man", "erst", "sein", "eig\u00b7nes", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werden F\u00fcnfe pl\u00f6tzlich grade.", "tokens": ["So", "wer\u00b7den", "F\u00fcn\u00b7fe", "pl\u00f6tz\u00b7lich", "gra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In welcher M\u00fchle man uns mahlt,", "tokens": ["In", "wel\u00b7cher", "M\u00fch\u00b7le", "man", "uns", "mahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das macht uns nimmer viel Beschwerden;", "tokens": ["Das", "macht", "uns", "nim\u00b7mer", "viel", "Be\u00b7schwer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der ist mein Herr, der mich bezahlt \u2014,", "tokens": ["Der", "ist", "mein", "Herr", ",", "der", "mich", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will ein guter B\u00fcrger werden.", "tokens": ["Ich", "will", "ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jedwedem Umtrieb bleib' ich fern,", "tokens": ["Jed\u00b7we\u00b7dem", "Um\u00b7trieb", "bleib'", "ich", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Henker mag das Volk begl\u00fccken!", "tokens": ["Der", "Hen\u00b7ker", "mag", "das", "Volk", "be\u00b7gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Orden ist ein eigner Stern,", "tokens": ["Ein", "Or\u00b7den", "ist", "ein", "eig\u00b7ner", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer einen hat, der soll sich b\u00fccken.", "tokens": ["Wer", "ei\u00b7nen", "hat", ",", "der", "soll", "sich", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "PRELS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "B\u00fcck' dich, mein Herz! bald fahren wir", "tokens": ["B\u00fcck", "dich", ",", "mein", "Herz", "!", "bald", "fah\u00b7ren", "wir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Residenz mit ", "tokens": ["Zur", "Re\u00b7si\u00b7denz", "mit"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Lisette, noch ein Gl\u00e4schen Bier!", "tokens": ["Li\u00b7set\u00b7te", ",", "noch", "ein", "Gl\u00e4s\u00b7chen", "Bier", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will ein guter B\u00fcrger werden.", "tokens": ["Ich", "will", "ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}