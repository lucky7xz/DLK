{"textgrid.poem.59519": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Epistel an Elisa", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df so stumm, wie in dem schwarzen", "tokens": ["Ich", "sa\u00df", "so", "stumm", ",", "wie", "in", "dem", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gericht des Orkus Rhadamanth,", "tokens": ["Ge\u00b7richt", "des", "Or\u00b7kus", "Rha\u00b7da\u00b7man\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und wog auf schwerer Schicksalshand", "tokens": ["Und", "wog", "auf", "schwe\u00b7rer", "Schick\u00b7sals\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schuld und Unschuld mit den Parzen,", "tokens": ["Die", "Schuld", "und", "Un\u00b7schuld", "mit", "den", "Par\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Las in der blutigen Schrift der Zeit", "tokens": ["Las", "in", "der", "blu\u00b7ti\u00b7gen", "Schrift", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Thronen Sturz und K\u00f6nigsmorde", "tokens": ["Der", "Thro\u00b7nen", "Sturz", "und", "K\u00f6\u00b7nigs\u00b7mor\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und siegreich die Banditenhorde,", "tokens": ["Und", "sieg\u00b7reich", "die", "Ban\u00b7di\u00b7ten\u00b7hor\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die einer Welt mit Knechtschaft dr\u00e4ut. \u2013", "tokens": ["Die", "ei\u00b7ner", "Welt", "mit", "Knecht\u00b7schaft", "dr\u00e4ut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da dacht' ich an die gro\u00dfen Seelen,", "tokens": ["Da", "dacht'", "ich", "an", "die", "gro\u00b7\u00dfen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die, keinem Schicksal untertan,", "tokens": ["Die", ",", "kei\u00b7nem", "Schick\u00b7sal", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den Weg zum stillen Ozean", "tokens": ["Den", "Weg", "zum", "stil\u00b7len", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich durch das freie Eisen w\u00e4hlen:", "tokens": ["Sich", "durch", "das", "frei\u00b7e", "Ei\u00b7sen", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Ist jedem Erdensohn gegeben;", "tokens": ["Ist", "je\u00b7dem", "Er\u00b7den\u00b7sohn", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die erste Majest\u00e4t im Leben,", "tokens": ["Die", "ers\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die h\u00f6chste, hei\u00dft Gesetz und Recht,", "tokens": ["Die", "h\u00f6chs\u00b7te", ",", "hei\u00dft", "Ge\u00b7setz", "und", "Recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und wenn nur Knechte und Despoten", "tokens": ["Und", "wenn", "nur", "Knech\u00b7te", "und", "Des\u00b7po\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auf Erden grasen matt und dumm,", "tokens": ["Auf", "Er\u00b7den", "gra\u00b7sen", "matt", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dann kehrt die alte Welt sich um,", "tokens": ["Dann", "kehrt", "die", "al\u00b7te", "Welt", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und Leben bl\u00fchet aus dem Toten", "tokens": ["Und", "Le\u00b7ben", "bl\u00fc\u00b7het", "aus", "dem", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Still in der Zeiten Wechsellauf,", "tokens": ["Still", "in", "der", "Zei\u00b7ten", "Wech\u00b7sel\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df edlere Geschlechter werden;", "tokens": ["Da\u00df", "ed\u00b7le\u00b7re", "Ge\u00b7schlech\u00b7ter", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und spr\u00e4ngen sie aus Steinen und Erden", "tokens": ["Und", "spr\u00e4n\u00b7gen", "sie", "aus", "Stei\u00b7nen", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Durch Kadmen und durch Pyrrhen auf.", "tokens": ["Durch", "Kad\u00b7men", "und", "durch", "Pyrr\u00b7hen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So sa\u00df ich, hielt die letzten Enden", "tokens": ["So", "sa\u00df", "ich", ",", "hielt", "die", "letz\u00b7ten", "En\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dinge w\u00e4gend in der Hand", "tokens": ["Der", "Din\u00b7ge", "w\u00e4\u00b7gend", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schaute stumm ins dunkle Land,", "tokens": ["Und", "schau\u00b7te", "stumm", "ins", "dunk\u00b7le", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von wannen nimmer Wandrer wenden;", "tokens": ["Von", "wan\u00b7nen", "nim\u00b7mer", "Wand\u00b7rer", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da rief mich deine Stimme s\u00fc\u00df,", "tokens": ["Da", "rief", "mich", "dei\u00b7ne", "Stim\u00b7me", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Freundin, in das frische Leben", "tokens": ["O", "Freun\u00b7din", ",", "in", "das", "fri\u00b7sche", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Blumenlust und Wolkenschweben,", "tokens": ["Mit", "Blu\u00b7men\u00b7lust", "und", "Wol\u00b7ken\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ins volle Fr\u00fchlingsparadies.", "tokens": ["Ins", "vol\u00b7le", "Fr\u00fch\u00b7lings\u00b7pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Freundschaft, holde Wundersaite,", "tokens": ["O", "Freund\u00b7schaft", ",", "hol\u00b7de", "Wun\u00b7der\u00b7sai\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieblich durch den Busen klingt", "tokens": ["Die", "lieb\u00b7lich", "durch", "den", "Bu\u00b7sen", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles Sch\u00f6ne wiederbringt,", "tokens": ["Und", "al\u00b7les", "Sch\u00f6\u00b7ne", "wie\u00b7der\u00b7bringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kindheit Traum, der Jugend Weite,", "tokens": ["Der", "Kind\u00b7heit", "Traum", ",", "der", "Ju\u00b7gend", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du M\u00e4nnerstahl und Frauenschutz,", "tokens": ["Du", "M\u00e4n\u00b7ner\u00b7stahl", "und", "Frau\u00b7en\u00b7schutz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herz zur Tugend zu ermannen,", "tokens": ["Das", "Herz", "zur", "Tu\u00b7gend", "zu", "er\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu gro\u00df f\u00fcr Sklaven und Tyrannen,", "tokens": ["Zu", "gro\u00df", "f\u00fcr", "Skla\u00b7ven", "und", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beutst du gemeinen G\u00fctern Trutz", "tokens": ["Beutst", "du", "ge\u00b7mei\u00b7nen", "G\u00fc\u00b7tern", "Trutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schwingst unsterblich durch das Leben,", "tokens": ["Und", "schwingst", "uns\u00b7terb\u00b7lich", "durch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie ein Gestirn den Feuerglanz,", "tokens": ["Wie", "ein", "Ge\u00b7stirn", "den", "Feu\u00b7er\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von Sph\u00e4rentanz zu Sph\u00e4rentanz", "tokens": ["Von", "Sph\u00e4\u00b7ren\u00b7tanz", "zu", "Sph\u00e4\u00b7ren\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Uns aufw\u00e4rts, wo die G\u00f6tter schweben.", "tokens": ["Uns", "auf\u00b7w\u00e4rts", ",", "wo", "die", "G\u00f6t\u00b7ter", "schwe\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich h\u00f6re deinen Zauberklang,", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7nen", "Zau\u00b7ber\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Gram entflieht ins \u00f6de Dunkel,", "tokens": ["Der", "Gram", "ent\u00b7flieht", "ins", "\u00f6\u00b7de", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der Himmel leuchtet, ein Karfunkel;", "tokens": ["Der", "Him\u00b7mel", "leuch\u00b7tet", ",", "ein", "Kar\u00b7fun\u00b7kel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die ganze Erde wird Gesang;", "tokens": ["Die", "gan\u00b7ze", "Er\u00b7de", "wird", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und Guillotinen und Banditen,", "tokens": ["Und", "Guil\u00b7lo\u00b7ti\u00b7nen", "und", "Ban\u00b7di\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Tyrannenseelen gro\u00df und klein", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7see\u00b7len", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Versinken aus des Lichtes Schein", "tokens": ["Ver\u00b7sin\u00b7ken", "aus", "des", "Lich\u00b7tes", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Tief, wo die Teufel H\u00f6llen h\u00fcten;", "tokens": ["Tief", ",", "wo", "die", "Teu\u00b7fel", "H\u00f6l\u00b7len", "h\u00fc\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und in der Freude freierm Schlag", "tokens": ["Und", "in", "der", "Freu\u00b7de", "frei\u00b7erm", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Hebt sich die Brust dem Licht entgegen,", "tokens": ["Hebt", "sich", "die", "Brust", "dem", "Licht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und jedes Unheil wird ein Segen,", "tokens": ["Und", "je\u00b7des", "Un\u00b7heil", "wird", "ein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ein Wonneruf wird jedes Ach.", "tokens": ["Ein", "Won\u00b7ne\u00b7ruf", "wird", "je\u00b7des", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So lieb und wunderbar getroffen", "tokens": ["So", "lieb", "und", "wun\u0