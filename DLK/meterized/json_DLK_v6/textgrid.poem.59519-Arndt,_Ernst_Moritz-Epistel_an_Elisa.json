u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PRELS", "ADJD", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Empor ins Sonnenreich des Lichts.", "tokens": ["Em\u00b7por", "ins", "Son\u00b7nen\u00b7reich", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, Freundin, welche ferne Lande", "tokens": ["Ja", ",", "Freun\u00b7din", ",", "wel\u00b7che", "fer\u00b7ne", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Fu\u00df auch noch durchwandern mu\u00df,", "tokens": ["Mein", "Fu\u00df", "auch", "noch", "durch\u00b7wan\u00b7dern", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' ich den letzten Obolus", "tokens": ["Eh'", "ich", "den", "letz\u00b7ten", "O\u00b7bo\u00b7lus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bezahle an dem stygischen Strande,", "tokens": ["Be\u00b7zah\u00b7le", "an", "dem", "sty\u00b7gi\u00b7schen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich schw\u00f6r' es dir und jener Glut,", "tokens": ["Ich", "schw\u00f6r'", "es", "dir", "und", "je\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word