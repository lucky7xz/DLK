{"textgrid.poem.60594": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Patriot", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen Helden, die der Welt", "tokens": ["Von", "al\u00b7len", "Hel\u00b7den", ",", "die", "der", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ewige Gestirne gl\u00e4nzen,", "tokens": ["Als", "e\u00b7wi\u00b7ge", "Ge\u00b7stir\u00b7ne", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch alle Gegenden bis an der Erde Gr\u00e4nzen,", "tokens": ["Durch", "al\u00b7le", "Ge\u00b7gen\u00b7den", "bis", "an", "der", "Er\u00b7de", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Patriot, bist du mein Held:", "tokens": ["O", "Pat\u00b7ri\u00b7ot", ",", "bist", "du", "mein", "Held", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der du, von Menschen oft verkannt,", "tokens": ["Der", "du", ",", "von", "Men\u00b7schen", "oft", "ver\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich ganz dem Vaterlande schenkest,", "tokens": ["Dich", "ganz", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "schen\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur seine Leiden f\u00fchlst, nur seine Gr\u00f6\u00dfe denkest,", "tokens": ["Nur", "sei\u00b7ne", "Lei\u00b7den", "f\u00fchlst", ",", "nur", "sei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "den\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und lebst und stirbst f\u00fcrs Vaterland!", "tokens": ["Und", "lebst", "und", "stirbst", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umsonst sucht von der Tugend Bahn", "tokens": ["Um\u00b7sonst", "sucht", "von", "der", "Tu\u00b7gend", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eigennutz dich zu verdr\u00e4ngen,", "tokens": ["Der", "Ei\u00b7gen\u00b7nutz", "dich", "zu", "ver\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchret wider dich, mit Jauchzen und Ges\u00e4ngen,", "tokens": ["Und", "f\u00fch\u00b7ret", "wi\u00b7der", "dich", ",", "mit", "Jauch\u00b7zen", "und", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die lockende Verf\u00fchrung an;", "tokens": ["Die", "lo\u00b7cken\u00b7de", "Ver\u00b7f\u00fch\u00b7rung", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ihr Gefolg, die g\u00fcldne Pracht,", "tokens": ["Und", "ihr", "Ge\u00b7folg", ",", "die", "g\u00fcld\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den stolzen Reichthum, mit der Ehre,", "tokens": ["Den", "stol\u00b7zen", "Reicht\u00b7hum", ",", "mit", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Pfauenfl\u00fcgel schwingt, und einem Freudenheere,", "tokens": ["Die", "Pfau\u00b7en\u00b7fl\u00fc\u00b7gel", "schwingt", ",", "und", "ei\u00b7nem", "Freu\u00b7den\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das um die s\u00fcsse Wollust lacht.", "tokens": ["Das", "um", "die", "s\u00fcs\u00b7se", "Wol\u00b7lust", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Siegprangender, als C\u00e4sar war,", "tokens": ["Sieg\u00b7pran\u00b7gen\u00b7der", ",", "als", "C\u00e4\u00b7sar", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt sich durch diesen furchtbarn Haufen", "tokens": ["Schl\u00e4gt", "sich", "durch", "die\u00b7sen", "furcht\u00b7barn", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gro\u00dfe Seele durch, mit Gold nicht zu erkaufen,", "tokens": ["Die", "gro\u00b7\u00dfe", "See\u00b7le", "durch", ",", "mit", "Gold", "nicht", "zu", "er\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$,", "APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht zu ersch\u00fcttern durch Gefahr.", "tokens": ["Nicht", "zu", "er\u00b7sch\u00fct\u00b7tern", "durch", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn wie ein Fels, der unbewegt,", "tokens": ["Denn", "wie", "ein", "Fels", ",", "der", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann Wogen sich auf Wogen th\u00fcrmen,", "tokens": ["Wann", "Wo\u00b7gen", "sich", "auf", "Wo\u00b7gen", "th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Oceane steht, und, ruhig in den St\u00fcrmen,", "tokens": ["Im", "O\u00b7cea\u00b7ne", "steht", ",", "und", ",", "ru\u00b7hig", "in", "den", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Den ganzen Zorn des Himmels tr\u00e4gt:", "tokens": ["Den", "gan\u00b7zen", "Zorn", "des", "Him\u00b7mels", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So stehest du mit festem Muth,", "tokens": ["So", "ste\u00b7hest", "du", "mit", "fes\u00b7tem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trotzest, ohne Freund, verlassen,", "tokens": ["Und", "trot\u00b7zest", ",", "oh\u00b7ne", "Freund", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Grimm der M\u00e4chtigen, der B\u00f6sen, die dich hassen,", "tokens": ["Dem", "Grimm", "der", "M\u00e4ch\u00b7ti\u00b7gen", ",", "der", "B\u00f6\u00b7sen", ",", "die", "dich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihrer ungerechten Wuth.", "tokens": ["Und", "ih\u00b7rer", "un\u00b7ge\u00b7rech\u00b7ten", "Wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Vaterland begl\u00fcckt zu sehn,", "tokens": ["Das", "Va\u00b7ter\u00b7land", "be\u00b7gl\u00fcckt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist dir die g\u00f6ttlichste der Freuden,", "tokens": ["Ist", "dir", "die", "g\u00f6tt\u00b7lichs\u00b7te", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist dir Ambrosia, selbst in dem h\u00e4rtsten Leiden,", "tokens": ["Ist", "dir", "A\u00b7mbro\u00b7sia", ",", "selbst", "in", "dem", "h\u00e4rts\u00b7ten", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wann B\u00fcrger dich undankbar schm\u00e4hn:", "tokens": ["Wann", "B\u00fcr\u00b7ger", "dich", "un\u00b7dank\u00b7bar", "schm\u00e4hn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Bis dich der Himmel wieder ruft,", "tokens": ["Bis", "dich", "der", "Him\u00b7mel", "wie\u00b7der", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lichte Wohnung wahrer Helden,", "tokens": ["Die", "lich\u00b7te", "Woh\u00b7nung", "wah\u00b7rer", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wer du warest, einst des Volkes Thr\u00e4nen melden,", "tokens": ["Und", ",", "wer", "du", "wa\u00b7rest", ",", "einst", "des", "Vol\u00b7kes", "Thr\u00e4\u00b7nen", "mel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VAFIN", "$,", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verstr\u00f6mt um deine stille Gruft.", "tokens": ["Ver\u00b7str\u00f6mt", "um", "dei\u00b7ne", "stil\u00b7le", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Unr\u00fchmlich, unbeweint im Tod,", "tokens": ["Un\u00b7r\u00fchm\u00b7lich", ",", "un\u00b7be\u00b7weint", "im", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermodern in verge\u00dfnen H\u00f6hlen", "tokens": ["Ver\u00b7mo\u00b7dern", "in", "ver\u00b7ge\u00df\u00b7nen", "H\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die B\u00fcrger schlimmer Art, in derer kleinen Seelen", "tokens": ["Die", "B\u00fcr\u00b7ger", "schlim\u00b7mer", "Art", ",", "in", "de\u00b7rer", "klei\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur niedrer Eigennutz geboth.", "tokens": ["Nur", "nie\u00b7drer", "Ei\u00b7gen\u00b7nutz", "ge\u00b7both", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die Sch\u00e4ndlichen! Das Vaterland,", "tokens": ["Die", "Sch\u00e4nd\u00b7li\u00b7chen", "!", "Das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ihnen, was sie hatten, Leben,", "tokens": ["Das", "ih\u00b7nen", ",", "was", "sie", "hat\u00b7ten", ",", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ruh, Ehr und Ueberflu\u00df und sichre Lust gegeben,", "tokens": ["Ruh", ",", "Ehr", "und", "Ue\u00b7berf\u00b7lu\u00df", "und", "sich\u00b7re", "Lust", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bat h\u00fclflos mit erhabner Hand.", "tokens": ["Bat", "h\u00fcl\u00b7flos", "mit", "er\u00b7hab\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie aber wichen scheu zur\u00fcck,", "tokens": ["Sie", "a\u00b7ber", "wi\u00b7chen", "scheu", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und n\u00fctzten den erz\u00fcrnten Himmel", "tokens": ["Und", "n\u00fctz\u00b7ten", "den", "er\u00b7z\u00fcrn\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu h\u00e4\u00dflichem Gewinn, und dachten im Get\u00fcmmel", "tokens": ["Zu", "h\u00e4\u00df\u00b7li\u00b7chem", "Ge\u00b7winn", ",", "und", "dach\u00b7ten", "im", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur sich und ihres Hauses Gl\u00fcck.", "tokens": ["Nur", "sich", "und", "ih\u00b7res", "Hau\u00b7ses", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ihr Haus entflieht der Rache nicht,", "tokens": ["Ihr", "Haus", "ent\u00b7flieht", "der", "Ra\u00b7che", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die endlich den Verbrecher findet:", "tokens": ["Die", "end\u00b7lich", "den", "Ver\u00b7bre\u00b7cher", "fin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "m