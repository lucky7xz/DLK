{"textgrid.poem.47552": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[im Begriff, zu reisen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Begriff, zu reisen,", "tokens": ["Im", "Be\u00b7griff", ",", "zu", "rei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Thu' ich ein Gel\u00fcbde,", "tokens": ["Thu'", "ich", "ein", "Ge\u00b7l\u00fcb\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Werd' es sicher halten:", "tokens": ["Werd'", "es", "si\u00b7cher", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wird die Huld des Himmels", "tokens": ["Wird", "die", "Huld", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihre Macht beweisen,", "tokens": ["Ih\u00b7re", "Macht", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Werd' ich wohlbehalten", "tokens": ["Werd'", "ich", "wohl\u00b7be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Der geliebten Heimath", "tokens": ["Der", "ge\u00b7lieb\u00b7ten", "Hei\u00b7math"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wonne wiederseh'n \u2013", "tokens": ["Won\u00b7ne", "wie\u00b7der\u00b7seh'n", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Auf der Stelle werd' ich,", "tokens": ["Auf", "der", "Stel\u00b7le", "werd'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Meinen Gott zu preisen,", "tokens": ["Mei\u00b7nen", "Gott", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "In die Schenke geh'n.", "tokens": ["In", "die", "Schen\u00b7ke", "geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}