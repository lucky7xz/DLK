{"textgrid.poem.60329": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sturm mit seinen Donnerschl\u00e4gen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sturm mit seinen Donnerschl\u00e4gen", "tokens": ["Sturm", "mit", "sei\u00b7nen", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann mir nicht wie du", "tokens": ["Kann", "mir", "nicht", "wie", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PWAV", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "So das tiefste Herz bewegen,", "tokens": ["So", "das", "tiefs\u00b7te", "Herz", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefe Meeresruh!", "tokens": ["Tie\u00b7fe", "Mee\u00b7res\u00b7ruh", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Du allein nur konntest lehren", "tokens": ["Du", "al\u00b7lein", "nur", "konn\u00b7test", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns den sch\u00f6nen Wahn", "tokens": ["Uns", "den", "sch\u00f6\u00b7nen", "Wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seliger Musik der Sph\u00e4ren,", "tokens": ["Se\u00b7li\u00b7ger", "Mu\u00b7sik", "der", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Stiller Ozean!", "tokens": ["Stil\u00b7ler", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "N\u00e4chtlich Meer, nun ist dein Schweigen", "tokens": ["N\u00e4cht\u00b7lich", "Meer", ",", "nun", "ist", "dein", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So tief ungest\u00f6rt,", "tokens": ["So", "tief", "un\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da\u00df die Seele wohl ihr eigen", "tokens": ["Da\u00df", "die", "See\u00b7le", "wohl", "ihr", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umen klingen h\u00f6rt;", "tokens": ["Tr\u00e4u\u00b7men", "klin\u00b7gen", "h\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df im Schutz geschlo\u00dfnen Mundes", "tokens": ["Da\u00df", "im", "Schutz", "ge\u00b7schlo\u00df\u00b7nen", "Mun\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch mein Herz erschrickt,", "tokens": ["Doch", "mein", "Herz", "er\u00b7schrickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das Geheimnis heilgen Bundes", "tokens": ["Das", "Ge\u00b7heim\u00b7nis", "heil\u00b7gen", "Bun\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fester an sich dr\u00fcckt.", "tokens": ["Fes\u00b7ter", "an", "sich", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}