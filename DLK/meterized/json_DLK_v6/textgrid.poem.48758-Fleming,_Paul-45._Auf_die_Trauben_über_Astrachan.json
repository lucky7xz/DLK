{"textgrid.poem.48758": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "45. Auf die Trauben \u00fcber Astrachan", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilt, unsre gute Post, ihr Aphrodiser Knaben,", "tokens": ["Eilt", ",", "uns\u00b7re", "gu\u00b7te", "Post", ",", "ihr", "A\u00b7phro\u00b7di\u00b7ser", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "eilt, tolgt, wohin euch vor die treue ", "tokens": ["eilt", ",", "tolgt", ",", "wo\u00b7hin", "euch", "vor", "die", "treu\u00b7e"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "folgt, tretet an das Land, da sie euch hin bestimmt,", "tokens": ["folgt", ",", "tre\u00b7tet", "an", "das", "Land", ",", "da", "sie", "euch", "hin", "be\u00b7stimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wo Evan und sein Volk den s\u00fc\u00dfen Nectar graben.", "tokens": ["wo", "E\u00b7van", "und", "sein", "Volk", "den", "s\u00fc\u00b7\u00dfen", "Nec\u00b7tar", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Geht, sagt ihm hohen Dank f\u00fcr diese seine Gaben,", "tokens": ["Geht", ",", "sagt", "ihm", "ho\u00b7hen", "Dank", "f\u00fcr", "die\u00b7se", "sei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "APPR", "PDAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit welchen er uns schon allhier entgegen nimt", "tokens": ["mit", "wel\u00b7chen", "er", "uns", "schon", "all\u00b7hier", "ent\u00b7ge\u00b7gen", "nimt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "zum Zeugn\u00fc\u00df seiner Gunst. Sprecht, da\u00df er selbsten k\u00f6mmt,", "tokens": ["zum", "Zeug\u00b7n\u00fc\u00df", "sei\u00b7ner", "Gunst", ".", "Sprecht", ",", "da\u00df", "er", "selbs\u00b7ten", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "an seiner edlen Kost mit uns sich hier zu laben.", "tokens": ["an", "sei\u00b7ner", "ed\u00b7len", "Kost", "mit", "uns", "sich", "hier", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Fall' er aber s\u00e4umt, so gebt ihm di\u00df Konfect,", "tokens": ["Im", "Fall'", "er", "a\u00b7ber", "s\u00e4umt", ",", "so", "gebt", "ihm", "di\u00df", "Kon\u00b7fect", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und fragt ihr, ob auch ihm, wie uns, was Fremdes schmeckt.", "tokens": ["und", "fragt", "ihr", ",", "ob", "auch", "ihm", ",", "wie", "uns", ",", "was", "Frem\u00b7des", "schmeckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "$,", "PWAV", "PPER", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht, eilt und bleibt allda! Man leichtet schon den Draggen.", "tokens": ["Geht", ",", "eilt", "und", "bleibt", "all\u00b7da", "!", "Man", "leich\u00b7tet", "schon", "den", "Drag\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$.", "PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Greift frisch die Remen an, bringt alle Segel bei!", "tokens": ["Greift", "frisch", "die", "Re\u00b7men", "an", ",", "bringt", "al\u00b7le", "Se\u00b7gel", "bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der fromme Norden jagt die aufgesteckten Flaggen,", "tokens": ["Der", "from\u00b7me", "Nor\u00b7den", "jagt", "die", "auf\u00b7ge\u00b7steck\u00b7ten", "Flag\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df er noch unser Gast auf diesen Abend sei.", "tokens": ["da\u00df", "er", "noch", "un\u00b7ser", "Gast", "auf", "die\u00b7sen", "A\u00b7bend", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}