{"textgrid.poem.65271": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Liebesaufruf", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist dein kleines Fensterlein", "tokens": ["Nun", "ist", "dein", "klei\u00b7nes", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl wieder aufgethaut?", "tokens": ["Wohl", "wie\u00b7der", "auf\u00b7ge\u00b7thaut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lieb Dirnel, hab' so manches Mal", "tokens": ["Lieb", "Dir\u00b7nel", ",", "hab'", "so", "man\u00b7ches", "Mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Winter 'nach geschaut.", "tokens": ["Im", "Win\u00b7ter", "'nach", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "War'n dicke wei\u00dfe Blumen vor,", "tokens": ["Wa\u00b7r'n", "di\u00b7cke", "wei\u00b7\u00dfe", "Blu\u00b7men", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich konnte dich nicht sehn;", "tokens": ["Ich", "konn\u00b7te", "dich", "nicht", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00dft' ich \u00fcber Eis und Schnee", "tokens": ["So", "mu\u00dft'", "ich", "\u00fc\u00b7ber", "Eis", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00fcbt nach Hause gehn.", "tokens": ["Be\u00b7tr\u00fcbt", "nach", "Hau\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da hab' ich auf dem kalten Weg", "tokens": ["Da", "hab'", "ich", "auf", "dem", "kal\u00b7ten", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dich recht warm gedacht,", "tokens": ["An", "dich", "recht", "warm", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Hab' deinen lieben Namen laut", "tokens": ["Hab'", "dei\u00b7nen", "lie\u00b7ben", "Na\u00b7men", "laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genannt bei Tag und Nacht.", "tokens": ["Ge\u00b7nannt", "bei", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn ich so oft gebetet h\u00e4tt'", "tokens": ["Wenn", "ich", "so", "oft", "ge\u00b7be\u00b7tet", "h\u00e4tt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Winterzeit,", "tokens": ["Die", "gan\u00b7ze", "Win\u00b7ter\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als dein gedacht in einem Tag,", "tokens": ["Als", "dein", "ge\u00b7dacht", "in", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00e4re benedeit.", "tokens": ["Ich", "w\u00e4\u00b7re", "be\u00b7ne\u00b7deit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ob's Lieben wohl was B\u00f6ses ist?", "tokens": ["Ob's", "Lie\u00b7ben", "wohl", "was", "B\u00f6\u00b7ses", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADV", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6glein thun's uns vor,", "tokens": ["Die", "V\u00f6\u00b7glein", "thun's", "uns", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schwingen doch mit Sang und Klang", "tokens": ["Und", "schwin\u00b7gen", "doch", "mit", "Sang", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Himmel sich empor.", "tokens": ["Zum", "Him\u00b7mel", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So zieh' ich aus zur Maienzeit", "tokens": ["So", "zieh'", "ich", "aus", "zur", "Mai\u00b7en\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf gr\u00fcne Liebeslust!", "tokens": ["Auf", "gr\u00fc\u00b7ne", "Lie\u00b7bes\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist's Fensterlein erst aufgethaut,", "tokens": ["Ist's", "Fens\u00b7ter\u00b7lein", "erst", "auf\u00b7ge\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird's warm auch um die Brust.", "tokens": ["Wird's", "warm", "auch", "um", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}