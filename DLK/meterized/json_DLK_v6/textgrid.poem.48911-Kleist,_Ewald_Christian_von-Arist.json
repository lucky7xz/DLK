{"textgrid.poem.48911": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Arist", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einer langen Reis' Arists, war stets", "tokens": ["Auf", "ei\u00b7ner", "lan\u00b7gen", "Reis'", "A\u00b7rists", ",", "war", "stets"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonn in Dunst versteckt. Oft heulte Sturm", "tokens": ["Die", "Sonn", "in", "Dunst", "ver\u00b7steckt", ".", "Oft", "heul\u00b7te", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In der durchw\u00fchlten Luft, oft, wenn er schwieg,", "tokens": ["In", "der", "durch\u00b7w\u00fchl\u00b7ten", "Luft", ",", "oft", ",", "wenn", "er", "schwieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ergo\u00df der Wolken-Last gleich einer See", "tokens": ["Er\u00b7go\u00df", "der", "Wol\u00b7ken\u00b7Last", "gleich", "ei\u00b7ner", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich \u00fcber Berg und Thal. Die Seel Arists", "tokens": ["Sich", "\u00fc\u00b7ber", "Berg", "und", "Thal", ".", "Die", "Seel", "A\u00b7rists"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "$.", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War finster, wie die Luft. Er hoft umsonst", "tokens": ["War", "fins\u00b7ter", ",", "wie", "die", "Luft", ".", "Er", "hoft", "um\u00b7sonst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Sonne wiederum am Firmament", "tokens": ["Die", "Son\u00b7ne", "wie\u00b7de\u00b7rum", "am", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu sehen, die daraus verschwunden schien,", "tokens": ["Zu", "se\u00b7hen", ",", "die", "da\u00b7raus", "ver\u00b7schwun\u00b7den", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PAV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und klagt voll Ungeduld den Himmel an,", "tokens": ["Und", "klagt", "voll", "Un\u00b7ge\u00b7duld", "den", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der bald die Welt verbrennt und bald ers\u00e4uft. \u2013", "tokens": ["Der", "bald", "die", "Welt", "ver\u00b7brennt", "und", "bald", "er\u00b7s\u00e4uft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "KON", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Schnell fuhr ein Pfeil vor ihm ins Erdreich \u2013 Thor!", "tokens": ["Schnell", "fuhr", "ein", "Pfeil", "vor", "ihm", "ins", "Er\u00b7dreich", "\u2013", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Um was beschwerst du dich? rief eine Stimm", "tokens": ["Um", "was", "be\u00b7schwerst", "du", "dich", "?", "rief", "ei\u00b7ne", "Stimm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PWS", "VVFIN", "PPER", "PRF", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.13": {"text": "Vom Himmel. Dieser Pfeil h\u00e4tt dich erreicht,", "tokens": ["Vom", "Him\u00b7mel", ".", "Die\u00b7ser", "Pfeil", "h\u00e4tt", "dich", "er\u00b7reicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "W\u00e4r nicht die Sehne durch den Regen schlaff", "tokens": ["W\u00e4r", "nicht", "die", "Seh\u00b7ne", "durch", "den", "Re\u00b7gen", "schlaff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Geworden. Tadle nicht, so k\u00fchn als schwach,", "tokens": ["Ge\u00b7wor\u00b7den", ".", "Tad\u00b7le", "nicht", ",", "so", "k\u00fchn", "als", "schwach", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PTKNEG", "$,", "ADV", "ADJD", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Einrichtung der Welt! Was willst du doch", "tokens": ["Die", "Ein\u00b7rich\u00b7tung", "der", "Welt", "!", "Was", "willst", "du", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Mit Maulwurfs Augen durch den Himmel sehn!", "tokens": ["Mit", "Maul\u00b7wurfs", "Au\u00b7gen", "durch", "den", "Him\u00b7mel", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Den du in St\u00fcrmen h\u00f6rst, und \u00fcber dir", "tokens": ["Den", "du", "in", "St\u00fcr\u00b7men", "h\u00f6rst", ",", "und", "\u00fc\u00b7ber", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "KON", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "In Blitz geh\u00fcllet siehst, der sorgt f\u00fcr dich!", "tokens": ["In", "Blitz", "ge\u00b7h\u00fcl\u00b7let", "siehst", ",", "der", "sorgt", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}