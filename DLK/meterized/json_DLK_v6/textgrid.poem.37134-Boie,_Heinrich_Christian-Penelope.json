{"textgrid.poem.37134": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Penelope", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die List Penelopens des frommen Weibchens lebe!", "tokens": ["Die", "List", "Pe\u00b7ne\u00b7lo\u00b7pens", "des", "from\u00b7men", "Weib\u00b7chens", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihre Tugend her zog sie ein Schutzgewebe;", "tokens": ["Um", "ih\u00b7re", "Tu\u00b7gend", "her", "zog", "sie", "ein", "Schutz\u00b7ge\u00b7we\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APZR", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch das was sie bei Tage gut gemacht,", "tokens": ["Doch", "das", "was", "sie", "bei", "Ta\u00b7ge", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "PPER", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verdarb sie wieder bei der Nacht.", "tokens": ["Ver\u00b7darb", "sie", "wie\u00b7der", "bei", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}