{"textgrid.poem.34023": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Todtenopfer", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber den Kirchhof ging ich und pfl\u00fcckte von jedem der Gr\u00e4ber", "tokens": ["Ue\u00b7ber", "den", "Kirch\u00b7hof", "ging", "ich", "und", "pfl\u00fcck\u00b7te", "von", "je\u00b7dem", "der", "Gr\u00e4\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PIS", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Eine Blume mir ab, bis sie mir schwollen zum Strau\u00df.", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", "mir", "ab", ",", "bis", "sie", "mir", "schwol\u00b7len", "zum", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NE", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber, was soll er mir doch \u2013 so rief ich, pl\u00f6tzlich erschaudernd, \u2013", "tokens": ["A\u00b7ber", ",", "was", "soll", "er", "mir", "doch", "\u2013", "so", "rief", "ich", ",", "pl\u00f6tz\u00b7lich", "er\u00b7schau\u00b7dernd", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "PWS", "VMFIN", "PPER", "PPER", "ADV", "$(", "ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "$("], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Borgt das Leben sich denn Zierde und Zeichen vom Tod?", "tokens": ["Borgt", "das", "Le\u00b7ben", "sich", "denn", "Zier\u00b7de", "und", "Zei\u00b7chen", "vom", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Doch, da traf ich ein Grab, verwildert liegend und einsam,", "tokens": ["Doch", ",", "da", "traf", "ich", "ein", "Grab", ",", "ver\u00b7wil\u00b7dert", "lie\u00b7gend", "und", "ein\u00b7sam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Welchem jeglicher Kranz fehlte, der welke sogar.", "tokens": ["Wel\u00b7chem", "jeg\u00b7li\u00b7cher", "Kranz", "fehl\u00b7te", ",", "der", "wel\u00b7ke", "so\u00b7gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Nimm sie, sprach ich und streute die Blumen, die schlummernden Nachbarn", "tokens": ["Nimm", "sie", ",", "sprach", "ich", "und", "streu\u00b7te", "die", "Blu\u00b7men", ",", "die", "schlum\u00b7mern\u00b7den", "Nach\u00b7barn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Senden das Opfer durch mich, da es die Liebe vers\u00e4umt!", "tokens": ["Sen\u00b7den", "das", "Op\u00b7fer", "durch", "mich", ",", "da", "es", "die", "Lie\u00b7be", "ver\u00b7s\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}