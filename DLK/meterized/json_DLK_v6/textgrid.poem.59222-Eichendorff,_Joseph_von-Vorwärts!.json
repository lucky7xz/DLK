{"textgrid.poem.59222": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vorw\u00e4rts!", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Strom sich schwingt", "tokens": ["Wie", "der", "Strom", "sich", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Aus den Wolken, die ihn tr\u00e4nken,", "tokens": ["Aus", "den", "Wol\u00b7ken", ",", "die", "ihn", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle B\u00e4che verschlingt,", "tokens": ["Al\u00b7le", "B\u00e4\u00b7che", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie ins Meer zu lenken \u2013", "tokens": ["Sie", "ins", "Meer", "zu", "len\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Drein m\u00f6cht ich versenken", "tokens": ["Drein", "m\u00f6cht", "ich", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Was in mir ringt!", "tokens": ["Was", "in", "mir", "ringt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Tritt nur mit in mein Schiff!", "tokens": ["Tritt", "nur", "mit", "in", "mein", "Schiff", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo wir landen oder stranden,", "tokens": ["Wo", "wir", "lan\u00b7den", "o\u00b7der", "stran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erklinget das Riff,", "tokens": ["Er\u00b7klin\u00b7get", "das", "Riff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bricht der Lenz aus dem Sande,", "tokens": ["Bricht", "der", "Lenz", "aus", "dem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Hinter uns dann ins Branden", "tokens": ["Hin\u00b7ter", "uns", "dann", "ins", "Bran\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Versenk ich das Schiff!", "tokens": ["Ver\u00b7senk", "ich", "das", "Schiff", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}