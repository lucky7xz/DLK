{"textgrid.poem.35677": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Wald voll Cedern Gottes zittert!", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wald voll Cedern Gottes zittert!", "tokens": ["Der", "Wald", "voll", "Ce\u00b7dern", "Got\u00b7tes", "zit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es braust des Jordans hohle Flut!", "tokens": ["Es", "braust", "des", "Jor\u00b7dans", "hoh\u00b7le", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sturm zerrei\u00dft die Wolken und ersch\u00fcttert", "tokens": ["Der", "Sturm", "zer\u00b7rei\u00dft", "die", "Wol\u00b7ken", "und", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jerusalem mit j\u00e4her Wut! \u2013", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "mit", "j\u00e4\u00b7her", "Wut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Willkommen, seligster der Tage,", "tokens": ["Will\u00b7kom\u00b7men", ",", "se\u00b7ligs\u00b7ter", "der", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verhei\u00dfen von Jehovens Sohn", "tokens": ["Ver\u00b7hei\u00b7\u00dfen", "von", "Je\u00b7ho\u00b7vens", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den J\u00fcngern, da\u00df nicht ihre Seele zage,", "tokens": ["Den", "J\u00fcn\u00b7gern", ",", "da\u00df", "nicht", "ih\u00b7re", "See\u00b7le", "za\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ihr des Todes Schrecken drohn!", "tokens": ["Wenn", "ihr", "des", "To\u00b7des", "Schre\u00b7cken", "drohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Geist wird auf sie ausgegossen,", "tokens": ["Der", "Geist", "wird", "auf", "sie", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre Bl\u00f6digkeit entflieht;", "tokens": ["Und", "ih\u00b7re", "Bl\u00f6\u00b7dig\u00b7keit", "ent\u00b7flieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lippe, von des Geistes Glanz umflossen,", "tokens": ["Die", "Lip\u00b7pe", ",", "von", "des", "Geis\u00b7tes", "Glanz", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "T\u00f6nt Gottes Preis: die Zunge gl\u00fcht.", "tokens": ["T\u00f6nt", "Got\u00b7tes", "Preis", ":", "die", "Zun\u00b7ge", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es h\u00f6rt die Sprache seines Landes", "tokens": ["Es", "h\u00f6rt", "die", "Spra\u00b7che", "sei\u00b7nes", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erstaunt der Kreter, Araber,", "tokens": ["Er\u00b7staunt", "der", "Kre\u00b7ter", ",", "A\u00b7ra\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der braune B\u00fcrger des Cyrenersandes,", "tokens": ["Der", "brau\u00b7ne", "B\u00fcr\u00b7ger", "des", "Cy\u00b7re\u00b7ner\u00b7san\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Grajer und Ausonier.", "tokens": ["Der", "Gra\u00b7jer", "und", "Au\u00b7so\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Bald fliegt ihr Name zu den Sternen,", "tokens": ["Bald", "fliegt", "ihr", "Na\u00b7me", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihrer Stimme, f\u00fcrchterlich", "tokens": ["Wenn", "ih\u00b7rer", "Stim\u00b7me", ",", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Gotteskraft, sich Ottern schnell entfernen,", "tokens": ["Durch", "Got\u00b7tes\u00b7kraft", ",", "sich", "Ot\u00b7tern", "schnell", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRF", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Satans schwarze Scharen sich,", "tokens": ["Und", "Sa\u00b7tans", "schwar\u00b7ze", "Scha\u00b7ren", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und Seuchen in Gehennas Kl\u00fcfte", "tokens": ["Und", "Seu\u00b7chen", "in", "Ge\u00b7hen\u00b7nas", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sich st\u00fcrzen; wenn, durch Gottes Schutz", "tokens": ["Sich", "st\u00fcr\u00b7zen", ";", "wenn", ",", "durch", "Got\u00b7tes", "Schutz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "$.", "KOUS", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gest\u00e4rkt, sie Becher voll von Pontus Gifte", "tokens": ["Ge\u00b7st\u00e4rkt", ",", "sie", "Be\u00b7cher", "voll", "von", "Pon\u00b7tus", "Gif\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PPER", "NN", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00f6hnen, und des W\u00fctrichs Trutz! \u2013", "tokens": ["Ver\u00b7h\u00f6h\u00b7nen", ",", "und", "des", "W\u00fct\u00b7richs", "Trutz", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Gott, bin ich in deines Sohnes", "tokens": ["O", "Gott", ",", "bin", "ich", "in", "dei\u00b7nes", "Soh\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimnisvolles Blut getaucht,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Blut", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ewig an den Stufen deines Thrones", "tokens": ["Das", "e\u00b7wig", "an", "den", "Stu\u00b7fen", "dei\u00b7nes", "Thro\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im schauerhaften Dunkel raucht:", "tokens": ["Im", "schau\u00b7er\u00b7haf\u00b7ten", "Dun\u00b7kel", "raucht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So h\u00f6re itzt mein gl\u00e4ubig Flehen!", "tokens": ["So", "h\u00f6\u00b7re", "itzt", "mein", "gl\u00e4u\u00b7big", "Fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Feuer taufe meinen Geist,", "tokens": ["Mit", "Feu\u00b7er", "tau\u00b7fe", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ihn im Sturm zu jenen lichten H\u00f6hen", "tokens": ["Das", "ihn", "im", "Sturm", "zu", "je\u00b7nen", "lich\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPRART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Greuel dieses Staubes rei\u00dft!", "tokens": ["Vom", "Greu\u00b7el", "die\u00b7ses", "Stau\u00b7bes", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df er mit R\u00fcstungen des \u00c4thers", "tokens": ["Da\u00df", "er", "mit", "R\u00fcs\u00b7tun\u00b7gen", "des", "\u00c4\u00b7thers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bewehrt, gest\u00e4rkt durch deine Macht,", "tokens": ["Be\u00b7wehrt", ",", "ge\u00b7st\u00e4rkt", "durch", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den ganzen Trupp des h\u00f6llischen Verr\u00e4ters", "tokens": ["Den", "gan\u00b7zen", "Trupp", "des", "h\u00f6l\u00b7li\u00b7schen", "Ver\u00b7r\u00e4\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur\u00fcckschreck' in die alte Nacht;", "tokens": ["Zu\u00b7r\u00fcck\u00b7schreck'", "in", "die", "al\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und frei vom Kummer, der hienieden", "tokens": ["Und", "frei", "vom", "Kum\u00b7mer", ",", "der", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn oft in dunkle Schatten h\u00fcllt,", "tokens": ["Ihn", "oft", "in", "dunk\u00b7le", "Schat\u00b7ten", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die lautre Quelle trinke, die dem M\u00fcden", "tokens": ["Die", "laut\u00b7re", "Quel\u00b7le", "trin\u00b7ke", ",", "die", "dem", "M\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Edens Palmen Labsal quillt!", "tokens": ["In", "E\u00b7dens", "Pal\u00b7men", "Lab\u00b7sal", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}