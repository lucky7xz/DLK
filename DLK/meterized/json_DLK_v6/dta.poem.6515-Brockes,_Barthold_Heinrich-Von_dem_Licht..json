{"dta.poem.6515": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun die Materie, die lebt, und sich stets r\u00fchrt,", "tokens": ["Nun", "die", "Ma\u00b7te\u00b7rie", ",", "die", "lebt", ",", "und", "sich", "stets", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "KON", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die in sich selbst begr\u00e4ntzet und bezircket,", "tokens": ["Die", "in", "sich", "selbst", "be\u00b7gr\u00e4nt\u00b7zet", "und", "be\u00b7zir\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eben dadurch noch viel st\u00e4rcker w\u00fcrcket,", "tokens": ["Und", "e\u00b7ben", "da\u00b7durch", "noch", "viel", "st\u00e4r\u00b7cker", "w\u00fcr\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird um den Mittelpunct sehr schnell herum gef\u00fchrt,", "tokens": ["Wird", "um", "den", "Mit\u00b7tel\u00b7punct", "sehr", "schnell", "he\u00b7rum", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie spannet um sich her dasselbige Bewegen,", "tokens": ["Sie", "span\u00b7net", "um", "sich", "her", "das\u00b7sel\u00b7bi\u00b7ge", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APZR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wird ein ", "tokens": ["Und", "wird", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "nennt.", "tokens": ["nennt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Der zarten Kugeln Heer, das in den L\u00fcfften rennt,", "tokens": ["Der", "zar\u00b7ten", "Ku\u00b7geln", "Heer", ",", "das", "in", "den", "L\u00fcff\u00b7ten", "rennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die alle nach uns zu und zwar gerade fallen,", "tokens": ["Die", "al\u00b7le", "nach", "uns", "zu", "und", "zwar", "ge\u00b7ra\u00b7de", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "PTKVZ", "KON", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Werckzeug unsers Aug\u2019s, in die Gesichts-Crystallen,", "tokens": ["In", "Wer\u00b7ck\u00b7zeug", "un\u00b7sers", "Aug's", ",", "in", "die", "Ge\u00b7sichts\u00b7Cry\u00b7stal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Kan man ein ", "tokens": ["Kan", "man", "ein"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Mit allem Rechte nennen,", "tokens": ["Mit", "al\u00b7lem", "Rech\u00b7te", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Das ist es, so uns r\u00fchrt, und das wir f\u00fchlen k\u00f6nnen.", "tokens": ["Das", "ist", "es", ",", "so", "uns", "r\u00fchrt", ",", "und", "das", "wir", "f\u00fch\u00b7len", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "ADV", "PPER", "VVFIN", "$,", "KON", "PRELS", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}