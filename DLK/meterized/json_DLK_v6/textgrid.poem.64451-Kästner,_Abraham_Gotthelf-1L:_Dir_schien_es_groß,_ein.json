{"textgrid.poem.64451": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir schien es gro\u00df, ein ", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir schien es gro\u00df, ein ", "tokens": ["Dir", "schien", "es", "gro\u00df", ",", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ART"], "meter": "-+---", "measure": "dactylic.init"}, "line.2": {"text": "Was that er? Einen Mord; Rom konnt' er nicht befreyn.", "tokens": ["Was", "that", "er", "?", "Ei\u00b7nen", "Mord", ";", "Rom", "konnt'", "er", "nicht", "be\u00b7freyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "$.", "NE", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie? Da\u00df dir ", "tokens": ["Wie", "?", "Da\u00df", "dir"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Der f\u00fcr sein Volk als Sieger fiel!", "tokens": ["Der", "f\u00fcr", "sein", "Volk", "als", "Sie\u00b7ger", "fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, mehr zu seyn, als ", "tokens": ["Und", ",", "mehr", "zu", "seyn", ",", "als"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADV", "PTKZU", "VAINF", "$,", "KOUS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Weswegen warest du nicht mein und dein ", "tokens": ["Wes\u00b7we\u00b7gen", "wa\u00b7rest", "du", "nicht", "mein", "und", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "KON", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}