{"textgrid.poem.48211": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3. Der Sommer- und Winter-Geheimrat", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Sommerszeit sind sie wie andre Menschen", "tokens": ["Um", "die", "Som\u00b7mers\u00b7zeit", "sind", "sie", "wie", "and\u00b7re", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPER", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Aus Schwiebus, Reppen oder Bentschen.", "tokens": ["Aus", "Schwie\u00b7bus", ",", "Rep\u00b7pen", "o\u00b7der", "Bent\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zumal in B\u00e4dern, in Ostseefrischen", "tokens": ["Zu\u00b7mal", "in", "B\u00e4\u00b7dern", ",", "in", "Ost\u00b7see\u00b7fri\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sitzt man mit ihnen an selben Tischen,", "tokens": ["Sitzt", "man", "mit", "ih\u00b7nen", "an", "sel\u00b7ben", "Ti\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und sind auch verschieden der Menschheit Lose,", "tokens": ["Und", "sind", "auch", "ver\u00b7schie\u00b7den", "der", "Menschheit", "Lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gleichmacherisch wirkt die Badehose,", "tokens": ["Gleich\u00b7ma\u00b7che\u00b7risch", "wirkt", "die", "Ba\u00b7de\u00b7ho\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der alte Adam mit seinen Gebrechen", "tokens": ["Der", "al\u00b7te", "A\u00b7dam", "mit", "sei\u00b7nen", "Ge\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+----+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "L\u00e4\u00dft manches schweigen und manches sprechen.", "tokens": ["L\u00e4\u00dft", "man\u00b7ches", "schwei\u00b7gen", "und", "man\u00b7ches", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "KON", "PIS", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Am Spill wurde gestern ein Seehund geschossen,", "tokens": ["Am", "Spill", "wur\u00b7de", "ge\u00b7stern", "ein", "See\u00b7hund", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu dr\u00e4ngen sich alle Strandgenossen;", "tokens": ["Zu", "dr\u00e4n\u00b7gen", "sich", "al\u00b7le", "Strand\u00b7ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Man will ein Kinderhospiz errichten,", "tokens": ["Man", "will", "ein", "Kin\u00b7der\u00b7ho\u00b7spiz", "er\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u00bbsie k\u00f6nnten einen Prolog uns dichten.\u00ab", "tokens": ["\u00bb", "sie", "k\u00f6nn\u00b7ten", "ei\u00b7nen", "Pro\u00b7log", "uns", "dich\u00b7ten", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "PPER", "ADJA", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Allgemeines heitres sich Anbequemen,", "tokens": ["All\u00b7ge\u00b7mei\u00b7nes", "heit\u00b7res", "sich", "An\u00b7be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "PRF", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.12": {"text": "Ein Unterschied ist nicht wahrzunehmen.", "tokens": ["Ein", "Un\u00b7ter\u00b7schied", "ist", "nicht", "wahr\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So der Sommer; er hat sein Bestes getan,", "tokens": ["So", "der", "Som\u00b7mer", ";", "er", "hat", "sein", "Bes\u00b7tes", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aber nun bricht der Winter an.", "tokens": ["A\u00b7ber", "nun", "bricht", "der", "Win\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Beim Botschafter S. ist Gala-Fete,", "tokens": ["Beim", "Bot\u00b7schaf\u00b7ter", "S.", "ist", "Ga\u00b7la\u00b7Fe\u00b7te", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VAFIN", "NE", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dein Spill-Freund ist mit an der Tete,", "tokens": ["Dein", "Spill\u00b7Freund", "ist", "mit", "an", "der", "Te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch sch\u00fctzt dich die bergende Fensternische,", "tokens": ["Noch", "sch\u00fctzt", "dich", "die", "ber\u00b7gen\u00b7de", "Fens\u00b7ter\u00b7ni\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt aber gilt es, jetzt geht es zu Tische,", "tokens": ["Jetzt", "a\u00b7ber", "gilt", "es", ",", "jetzt", "geht", "es", "zu", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du sitzt vis-\u00e0-vis ihm, es trifft dich sein Gru\u00df,", "tokens": ["Du", "sitzt", "vis\u00b7\u00e0\u00b7vis", "ihm", ",", "es", "trifft", "dich", "sein", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Davor dein Herz ersteinen mu\u00df.", "tokens": ["Da\u00b7vor", "dein", "Herz", "er\u00b7stei\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es wundert sein Chef sich, sein Kollege,", "tokens": ["Es", "wun\u00b7dert", "sein", "Chef", "sich", ",", "sein", "Kol\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die Badebekanntschaft ist pl\u00f6tzlich im Wege,", "tokens": ["Die", "Ba\u00b7de\u00b7be\u00b7kannt\u00b7schaft", "ist", "pl\u00f6tz\u00b7lich", "im", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Von dem, mit dem du den Seehund umstanden,", "tokens": ["Von", "dem", ",", "mit", "dem", "du", "den", "See\u00b7hund", "um\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Von dem \u00bbsommerlichen\u00ab ist nichts mehr vorhanden,", "tokens": ["Von", "dem", "\u00bb", "som\u00b7mer\u00b7li\u00b7chen", "\u00ab", "ist", "nichts", "mehr", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "VVINF", "$(", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Statt seiner der \u00bbwinterliche\u00ab ... Du frierst.", "tokens": ["Statt", "sei\u00b7ner", "der", "\u00bb", "win\u00b7ter\u00b7li\u00b7che", "\u00ab", "...", "Du", "frierst", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ART", "$(", "VVFIN", "$(", "$(", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Suche, da\u00df du dich rasch verlierst.", "tokens": ["Su\u00b7che", ",", "da\u00df", "du", "dich", "rasch", "ver\u00b7lierst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}