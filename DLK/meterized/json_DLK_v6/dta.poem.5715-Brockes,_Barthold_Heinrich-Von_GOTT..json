{"dta.poem.5715": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Meinung tr\u00e4umt nun Epicur entgegen;", "tokens": ["Der", "Mei\u00b7nung", "tr\u00e4umt", "nun", "E\u00b7pi\u00b7cur", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob ein grosses weites Leer", "tokens": ["Als", "ob", "ein", "gros\u00b7ses", "wei\u00b7tes", "Leer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von G\u00f6ttern dort bewohnet w\u00e4r,", "tokens": ["Von", "G\u00f6t\u00b7tern", "dort", "be\u00b7woh\u00b7net", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nicht einmal, in ihrer Ruh, das Regen", "tokens": ["Die", "nicht", "ein\u00b7mal", ",", "in", "ih\u00b7rer", "Ruh", ",", "das", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel w\u00fcrdigen zu h\u00f6ren,", "tokens": ["Der", "Him\u00b7mel", "w\u00fcr\u00b7di\u00b7gen", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die an dem Regiment der Welt sich gar nicht kehren,", "tokens": ["Die", "an", "dem", "Re\u00b7gi\u00b7ment", "der", "Welt", "sich", "gar", "nicht", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und die ihr h\u00f6chstes Gl\u00fcck, da sie unsterblich seyn,", "tokens": ["Und", "die", "ihr", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ",", "da", "sie", "uns\u00b7terb\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm tieffsten M\u00fcssiggange setzen.", "tokens": ["Jm", "tieffs\u00b7ten", "M\u00fcs\u00b7sig\u00b7gan\u00b7ge", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie \u00fcberlassen uns dem ", "tokens": ["Sie", "\u00fc\u00b7ber\u00b7las\u00b7sen", "uns", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein Schlu\u00df, den wir mit Recht der GOTTHEIT schimpf-", "tokens": ["Ein", "Schlu\u00df", ",", "den", "wir", "mit", "Recht", "der", "GoT\u00b7T\u00b7HEIT", "schimpf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "lich sch\u00e4tzen!", "tokens": ["lich", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}