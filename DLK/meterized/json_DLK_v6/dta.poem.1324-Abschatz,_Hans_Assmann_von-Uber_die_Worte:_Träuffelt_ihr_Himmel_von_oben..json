, "tokens": ["Gott", "von", "Gott", ":", "Ich", "h\u00f6\u00b7re", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Weynacht-Engel singt:", "tokens": ["Wie", "der", "Wey\u00b7nacht\u00b7En\u00b7gel", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Volck soll freudig werden/", "tokens": ["Al\u00b7les", "Volck", "soll", "freu\u00b7dig", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie in hoher Lufft erklingt:", "tokens": ["Wie", "in", "ho\u00b7her", "Lufft", "er\u00b7klingt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott die Ehre/ Fried auff Erden.", "tokens": ["Gott", "die", "Eh\u00b7re", "/", "Fried", "auff", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mache dich zum Wette-Lauff/", "tokens": ["Ma\u00b7che", "dich", "zum", "Wet\u00b7te\u00b7Lauff", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seele/ mit den Hirten auff/", "tokens": ["See\u00b7le", "/", "mit", "den", "Hir\u00b7ten", "auff", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "ART", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo das grosse Wunder-Kind", "tokens": ["Wo", "das", "gros\u00b7se", "Wun\u00b7der\u00b7Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird in harter Krippe funden/", "tokens": ["Wird", "in", "har\u00b7ter", "Krip\u00b7pe", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bey solcher Wiege sind", "tokens": ["Und", "bey", "sol\u00b7cher", "Wie\u00b7ge", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ochs und Esel angebunden.", "tokens": ["Ochs", "und", "E\u00b7sel", "an\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Binde die Vernunfft hier an/", "tokens": ["Bin\u00b7de", "die", "Ver\u00b7nunfft", "hier", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche nicht erreichen kan/", "tokens": ["Wel\u00b7che", "nicht", "er\u00b7rei\u00b7chen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Erde Niedrigkeit", "tokens": ["Wie", "der", "Er\u00b7de", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die hohen Himmels-H\u00f6hen", "tokens": ["Und", "die", "ho\u00b7hen", "Him\u00b7mels\u00b7H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig seyn/ und in der Zeit", "tokens": ["E\u00b7wig", "seyn", "/", "und", "in", "der", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAINF", "$(", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mensch und GOtt beysammen stehen.", "tokens": ["Mensch", "und", "Gott", "bey\u00b7sam\u00b7men", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gl\u00e4ube/ was GOtt l\u00e4ngst verhie\u00df/", "tokens": ["Gl\u00e4u\u00b7be", "/", "was", "Gott", "l\u00e4ngst", "ver\u00b7hie\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Engel macht gewi\u00df/", "tokens": ["Und", "der", "En\u00b7gel", "macht", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4ube/ da\u00df di\u00df Freuden-Kind", "tokens": ["Gl\u00e4u\u00b7be", "/", "da\u00df", "di\u00df", "Freu\u00b7den\u00b7Kind"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir auch sey zum Heyl gebohren/", "tokens": ["Dir", "auch", "sey", "zum", "Heyl", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bey ihm das Leben findt", "tokens": ["Und", "bey", "ihm", "das", "Le\u00b7ben", "findt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Was im Tode war verlohren.", "tokens": ["Was", "im", "To\u00b7de", "war", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lege Tasch und Hirten-Stab", "tokens": ["Le\u00b7ge", "Tasch", "und", "Hir\u00b7ten\u00b7Stab"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Eitelkeiten ab/", "tokens": ["Dei\u00b7ner", "Ei\u00b7tel\u00b7kei\u00b7ten", "ab", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier ist deiner Seelen Hirt", "tokens": ["Hier", "ist", "dei\u00b7ner", "See\u00b7len", "Hirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der rechte F\u00fcrst des Lebens;", "tokens": ["Und", "der", "rech\u00b7te", "F\u00fcrst", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer auff andern Trifften irrt/", "tokens": ["Wer", "auff", "an\u00b7dern", "Triff\u00b7ten", "irrt", "/"], "token_info": ["word", "wor