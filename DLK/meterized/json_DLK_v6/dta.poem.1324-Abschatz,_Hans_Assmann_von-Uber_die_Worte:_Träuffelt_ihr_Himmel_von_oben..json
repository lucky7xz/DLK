{"dta.poem.1324": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Worte:  \n Tr\u00e4uffelt ihr Himmel von oben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Himmel/ ob uns ausgespannt/", "tokens": ["Him\u00b7mel", "/", "ob", "uns", "aus\u00b7ge\u00b7spannt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4uffelt auff das d\u00fcrre Land!", "tokens": ["Tr\u00e4uf\u00b7felt", "auff", "das", "d\u00fcr\u00b7re", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df auff Zions matte Stadt", "tokens": ["La\u00df", "auff", "Zi\u00b7ons", "mat\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die ausgebrennten Auen/", "tokens": ["Und", "die", "aus\u00b7ge\u00b7brenn\u00b7ten", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Fluch gebraten hat/", "tokens": ["Die", "der", "Fluch", "ge\u00b7bra\u00b7ten", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jacobs edlen Segen thauen!", "tokens": ["Ja\u00b7cobs", "ed\u00b7len", "Se\u00b7gen", "thau\u00b7en