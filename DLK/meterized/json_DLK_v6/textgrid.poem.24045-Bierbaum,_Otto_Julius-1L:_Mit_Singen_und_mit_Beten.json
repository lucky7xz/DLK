{"textgrid.poem.24045": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Singen und mit Beten", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Singen und mit Beten", "tokens": ["Mit", "Sin\u00b7gen", "und", "mit", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hin zum Altar getreten", "tokens": ["Hin", "zum", "Al\u00b7tar", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVPP"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Qualfreudige Asketen.", "tokens": ["Qual\u00b7freu\u00b7di\u00b7ge", "As\u00b7ke\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir stehn vor Grabesthoren,", "tokens": ["Wir", "stehn", "vor", "Gra\u00b7be\u00b7stho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir sind dem Tod geboren,", "tokens": ["Wir", "sind", "dem", "Tod", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Leben ist verloren.", "tokens": ["Das", "Le\u00b7ben", "ist", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt, wie die Heiden singen!", "tokens": ["H\u00f6rt", ",", "wie", "die", "Hei\u00b7den", "sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ihre Becken klingen!", "tokens": ["Wie", "ih\u00b7re", "Be\u00b7cken", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seht, wie die Nackten springen!", "tokens": ["Seht", ",", "wie", "die", "Nack\u00b7ten", "sprin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie tanzen in der Sonnen", "tokens": ["Sie", "tan\u00b7zen", "in", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um einen gr\u00fcnen Bronnen,", "tokens": ["Um", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Draus kommt rot Wein geronnen.", "tokens": ["Draus", "kommt", "rot", "Wein", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der rote Wein vergossen,", "tokens": ["Der", "ro\u00b7te", "Wein", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die rote Lust genossen,", "tokens": ["Die", "ro\u00b7te", "Lust", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit St\u00f6hnen wild beschlossen.", "tokens": ["Mit", "St\u00f6h\u00b7nen", "wild", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Blut str\u00f6mt am Himmelsrande,", "tokens": ["Blut", "str\u00f6mt", "am", "Him\u00b7mels\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie liegen tot im Sande,", "tokens": ["Sie", "lie\u00b7gen", "tot", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Leichentuch die Schande.", "tokens": ["Ihr", "Lei\u00b7chen\u00b7tuch", "die", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir schreiten auf den Leichen,", "tokens": ["Wir", "schrei\u00b7ten", "auf", "den", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir Seligen, wir Reichen,", "tokens": ["Wir", "Se\u00b7li\u00b7gen", ",", "wir", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blut str\u00f6mt von unsern Streichen.", "tokens": ["Blut", "str\u00f6mt", "von", "un\u00b7sern", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}