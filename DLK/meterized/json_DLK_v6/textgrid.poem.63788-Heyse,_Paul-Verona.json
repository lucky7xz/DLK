{"textgrid.poem.63788": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Verona", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so entl\u00e4\u00dft dich, wie sie dich empfangen,", "tokens": ["Und", "so", "ent\u00b7l\u00e4\u00dft", "dich", ",", "wie", "sie", "dich", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Italiens sch\u00f6ne Tochter an der Schwelle,", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "sch\u00f6\u00b7ne", "Toch\u00b7ter", "an", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf da\u00df nach ihrer Mutter Sonnenhelle", "tokens": ["Auf", "da\u00df", "nach", "ih\u00b7rer", "Mut\u00b7ter", "Son\u00b7nen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sehnlich immer m\u00fcssest heimverlangen.", "tokens": ["Du", "sehn\u00b7lich", "im\u00b7mer", "m\u00fcs\u00b7sest", "heim\u00b7ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "All ihre Lieblichkeit und stolzes Prangen", "tokens": ["All", "ih\u00b7re", "Lieb\u00b7lich\u00b7keit", "und", "stol\u00b7zes", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gr\u00fc\u00dft dich noch einmal aus des Stromes Welle;", "tokens": ["Gr\u00fc\u00dft", "dich", "noch", "ein\u00b7mal", "aus", "des", "Stro\u00b7mes", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was dir der S\u00fcden bot, an dieser Stelle", "tokens": ["Was", "dir", "der", "S\u00fc\u00b7den", "bot", ",", "an", "die\u00b7ser", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist's wie im Auszug dir vorbeigegangen.", "tokens": ["Ist's", "wie", "im", "Aus\u00b7zug", "dir", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Amphitheater, Dom, Arkaden, Pl\u00e4tze", "tokens": ["Am\u00b7phit\u00b7he\u00b7a\u00b7ter", ",", "Dom", ",", "Ar\u00b7ka\u00b7den", ",", "Pl\u00e4t\u00b7ze"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Marktgew\u00fchls und ausgela\u00dfner Schreier,", "tokens": ["Voll", "Markt\u00b7ge\u00b7w\u00fchls", "und", "aus\u00b7ge\u00b7la\u00df\u00b7ner", "Schrei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja ein Triumphtor selbst ward nicht vergessen;", "tokens": ["Ja", "ein", "Tri\u00b7umph\u00b7tor", "selbst", "ward", "nicht", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Mal- und Bildkunst unersch\u00f6pfte Sch\u00e4tze,", "tokens": ["Der", "Ma\u00b7l", "und", "Bild\u00b7kunst", "un\u00b7er\u00b7sch\u00f6pf\u00b7te", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Glutaugen, leuchtend unter schwarzem Schleier,", "tokens": ["Glu\u00b7tau\u00b7gen", ",", "leuch\u00b7tend", "un\u00b7ter", "schwar\u00b7zem", "Schlei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und jenes Giusti-Gartens Prachtzypressen.", "tokens": ["Und", "je\u00b7nes", "Gi\u00b7us\u00b7ti\u00b7Gar\u00b7tens", "Pracht\u00b7zy\u00b7pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}