{"textgrid.poem.62400": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr wundersch\u00f6nen Augenblicke,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wundersch\u00f6nen Augenblicke,", "tokens": ["Ihr", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lieblichste der ganzen Welt", "tokens": ["Die", "Lieb\u00b7lichs\u00b7te", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat euch mit ihrem ew'gen Gl\u00fccke,", "tokens": ["Hat", "euch", "mit", "ih\u00b7rem", "ew'\u00b7gen", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrem s\u00fc\u00dfen Licht erhellt.", "tokens": ["Mit", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Licht", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Stellen, ihr geweihten Pl\u00e4tze,", "tokens": ["Ihr", "Stel\u00b7len", ",", "ihr", "ge\u00b7weih\u00b7ten", "Pl\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr trugt ja das geliebte Bild,", "tokens": ["Ihr", "trugt", "ja", "das", "ge\u00b7lieb\u00b7te", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Wunder habt ihr, was f\u00fcr Sch\u00e4tze", "tokens": ["Was", "Wun\u00b7der", "habt", "ihr", ",", "was", "f\u00fcr", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor meinen Augen dort enth\u00fcllt!", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", "dort", "ent\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr G\u00e4rten, all ihr gr\u00fcnen Haine,", "tokens": ["Ihr", "G\u00e4r\u00b7ten", ",", "all", "ihr", "gr\u00fc\u00b7nen", "Hai\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Weinberg in der s\u00fc\u00dfen Zier,", "tokens": ["Du", "Wein\u00b7berg", "in", "der", "s\u00fc\u00b7\u00dfen", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es nahte sich die Hehre, Reine", "tokens": ["Es", "nah\u00b7te", "sich", "die", "Heh\u00b7re", ",", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Z\u00fcchten gar zu freundlich mir.", "tokens": ["In", "Z\u00fcch\u00b7ten", "gar", "zu", "freund\u00b7lich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKA", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr Worte, die sie da gesprochen,", "tokens": ["Ihr", "Wor\u00b7te", ",", "die", "sie", "da", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sch\u00f6nstes, halbverhauchtes Wort,", "tokens": ["Du", "sch\u00f6ns\u00b7tes", ",", "halb\u00b7ver\u00b7hauch\u00b7tes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Zauberbann wird nie gebrochen,", "tokens": ["Dein", "Zau\u00b7ber\u00b7bann", "wird", "nie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du klingst und wirkest fort und fort.", "tokens": ["Du", "klingst", "und", "wir\u00b7kest", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr wundersch\u00f6nen Augenblicke,", "tokens": ["Ihr", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr lacht und lockt in ew'gem Reiz!", "tokens": ["Ihr", "lacht", "und", "lockt", "in", "ew'\u00b7gem", "Reiz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schaue sehnsuchtsvoll zur\u00fccke", "tokens": ["Ich", "schau\u00b7e", "sehn\u00b7suchts\u00b7voll", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Schmerz und Lust und Liebesgeiz.", "tokens": ["Voll", "Schmerz", "und", "Lust", "und", "Lie\u00b7bes\u00b7geiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}