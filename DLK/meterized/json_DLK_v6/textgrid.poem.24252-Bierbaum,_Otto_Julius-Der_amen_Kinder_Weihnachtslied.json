{"textgrid.poem.24252": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der amen Kinder Weihnachtslied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, sch\u00f6ne Herrn und Frauen,", "tokens": ["H\u00f6rt", ",", "sch\u00f6\u00b7ne", "Herrn", "und", "Frau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr im Lichte seid:", "tokens": ["Die", "ihr", "im", "Lich\u00b7te", "seid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir kommen aus dem Grauen,", "tokens": ["Wir", "kom\u00b7men", "aus", "dem", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Lande Not und Leid;", "tokens": ["Dem", "Lan\u00b7de", "Not", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weh tun uns unsre F\u00fc\u00dfe", "tokens": ["Weh", "tun", "uns", "uns\u00b7re", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und unsre Herzen weh,", "tokens": ["Und", "uns\u00b7re", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch kam uns eine s\u00fc\u00dfe", "tokens": ["Doch", "kam", "uns", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Botschaft aus Eis und Schnee.", "tokens": ["Bot\u00b7schaft", "aus", "Eis", "und", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Es ist ein Licht erglommen,", "tokens": ["Es", "ist", "ein", "Licht", "er\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und uns auch gilt sein Schein.", "tokens": ["Und", "uns", "auch", "gilt", "sein", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wir habens wohl vernommen:", "tokens": ["Wir", "ha\u00b7bens", "wohl", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das Christkind ist gekommen", "tokens": ["Das", "Christ\u00b7kind", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und soll auch uns gekommen sein.", "tokens": ["Und", "soll", "auch", "uns", "ge\u00b7kom\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum gehn wir zu den Orten,", "tokens": ["Drum", "gehn", "wir", "zu", "den", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hell erleuchtet sind,", "tokens": ["Die", "hell", "er\u00b7leuch\u00b7tet", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und klopfen an die Pforten:", "tokens": ["Und", "klop\u00b7fen", "an", "die", "Pfor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist hier das Christuskind?", "tokens": ["Ist", "hier", "das", "Chris\u00b7tus\u00b7kind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hat wohl nicht gefunden", "tokens": ["Es", "hat", "wohl", "nicht", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Weg in unsre Nacht,", "tokens": ["Den", "Weg", "in", "uns\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum haben wir mit wunden", "tokens": ["Drum", "ha\u00b7ben", "wir", "mit", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fc\u00dfen uns aufgemacht,", "tokens": ["F\u00fc\u00b7\u00dfen", "uns", "auf\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Da\u00df wir ihm unsre frommen", "tokens": ["Da\u00df", "wir", "ihm", "uns\u00b7re", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Herzen und Bitten weihn.", "tokens": ["Her\u00b7zen", "und", "Bit\u00b7ten", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Wir habens wohl vernommen:", "tokens": ["Wir", "ha\u00b7bens", "wohl", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das Christkind ist gekommen", "tokens": ["Das", "Christ\u00b7kind", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und soll auch uns gekommen sein.", "tokens": ["Und", "soll", "auch", "uns", "ge\u00b7kom\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So la\u00dft es uns erschauen,", "tokens": ["So", "la\u00dft", "es", "uns", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr im Lichte seid!", "tokens": ["Die", "ihr", "im", "Lich\u00b7te", "seid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir kommen aus dem Grauen,", "tokens": ["Wir", "kom\u00b7men", "aus", "dem", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Lande Not und Leid;", "tokens": ["Dem", "Lan\u00b7de", "Not", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir kommen mit wunden F\u00fc\u00dfen,", "tokens": ["Wir", "kom\u00b7men", "mit", "wun\u00b7den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch sind wir trostgemut:", "tokens": ["Doch", "sind", "wir", "trost\u00b7ge\u00b7mut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn wir das Christkind gr\u00fc\u00dfen,", "tokens": ["Wenn", "wir", "das", "Christ\u00b7kind", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird alles, alles gut.", "tokens": ["Wird", "al\u00b7les", ",", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der Stern, der heut erglommen,", "tokens": ["Der", "Stern", ",", "der", "heut", "er\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Gibt allen seinen Schein:", "tokens": ["Gibt", "al\u00b7len", "sei\u00b7nen", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das Christkind ist gekommen! \u2013", "tokens": ["Das", "Christ\u00b7kind", "ist", "ge\u00b7kom\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die ihr es aufgenommen,", "tokens": ["Die", "ihr", "es", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O, la\u00dft auch uns zu Gaste sein!", "tokens": ["O", ",", "la\u00dft", "auch", "uns", "zu", "Gas\u00b7te", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ADV", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}