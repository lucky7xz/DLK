{"textgrid.poem.58998": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "217. Das verschloss'ne K\u00e4mmerlein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein K\u00e4mmerlein hei\u00dft Gottes Wille,", "tokens": ["Mein", "K\u00e4m\u00b7mer\u00b7lein", "hei\u00dft", "Got\u00b7tes", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drin ich verschlossen leb' und stille;", "tokens": ["Drin", "ich", "ver\u00b7schlos\u00b7sen", "leb'", "und", "stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt Freude, sie mag drau\u00dfen stehn,", "tokens": ["Kommt", "Freu\u00b7de", ",", "sie", "mag", "drau\u00b7\u00dfen", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt Kreuz, ich la\u00df mich doch nicht sehn.", "tokens": ["Kommt", "Kreuz", ",", "ich", "la\u00df", "mich", "doch", "nicht", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PPER", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}