{"textgrid.poem.26298": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Bin wie ein Kranker", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blaue Himmel und der Sonnenschein,", "tokens": ["Der", "blau\u00b7e", "Him\u00b7mel", "und", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie stiegen nicht mit mir ins Schiff hinein.", "tokens": ["Sie", "stie\u00b7gen", "nicht", "mit", "mir", "ins", "Schiff", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beim Regen bin ich einsam hier gelandet.", "tokens": ["Beim", "Re\u00b7gen", "bin", "ich", "ein\u00b7sam", "hier", "ge\u00b7lan\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Meere schlug mein Herz noch weit \u2013 jetzt liegt es hier versandet.", "tokens": ["Im", "Mee\u00b7re", "schlug", "mein", "Herz", "noch", "weit", "\u2013", "jetzt", "liegt", "es", "hier", "ver\u00b7san\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$(", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Der Regen f\u00e4llt und f\u00fcllt mit Tr\u00fcbnis die Gedanken,", "tokens": ["Der", "Re\u00b7gen", "f\u00e4llt", "und", "f\u00fcllt", "mit", "Tr\u00fcb\u00b7nis", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schwimmen grau vorbei wie tote H\u00fcllen.", "tokens": ["Sie", "schwim\u00b7men", "grau", "vor\u00b7bei", "wie", "to\u00b7te", "H\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Mut starb hin, da sie in Gram versanken.", "tokens": ["Ihr", "Mut", "starb", "hin", ",", "da", "sie", "in", "Gram", "ver\u00b7san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die W\u00fcnsche t\u00f6ten, die sich nicht erf\u00fcllen.", "tokens": ["Die", "W\u00fcn\u00b7sche", "t\u00f6\u00b7ten", ",", "die", "sich", "nicht", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bin wie ein Kranker, den die Nacht bedr\u00e4ngt,", "tokens": ["Bin", "wie", "ein", "Kran\u00b7ker", ",", "den", "die", "Nacht", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bin wie der Mond, der bla\u00df im Raume h\u00e4ngt.", "tokens": ["Bin", "wie", "der", "Mond", ",", "der", "bla\u00df", "im", "Rau\u00b7me", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darf ich mein Blut nicht bald an Liebe stillen", "tokens": ["Darf", "ich", "mein", "Blut", "nicht", "bald", "an", "Lie\u00b7be", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sterb ich an meiner W\u00fcnsche letztem Willen.", "tokens": ["Sterb", "ich", "an", "mei\u00b7ner", "W\u00fcn\u00b7sche", "letz\u00b7tem", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}