{"dta.poem.8492": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Gem\u00fcth .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gr&#252;n ist die Flur, der Himmel blau,", "tokens": ["Gr", "&#252;", "n", "ist", "die", "Flur", ",", "der", "Him\u00b7mel", "blau", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch tausend Farben spielt der Thau;", "tokens": ["Doch", "tau\u00b7send", "Far\u00b7ben", "spielt", "der", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hofft die Erde bis zum Grabe,", "tokens": ["Es", "hofft", "die", "Er\u00b7de", "bis", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gew\u00e4hrung fiel dem Himmel zu;", "tokens": ["Ge\u00b7w\u00e4h\u00b7rung", "fiel", "dem", "Him\u00b7mel", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sprich, was ist denn deine Gabe,", "tokens": ["Und", "sprich", ",", "was", "ist", "denn", "dei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gem\u00fcth, der Seele Iris, Du?", "tokens": ["Ge\u00b7m\u00fcth", ",", "der", "See\u00b7le", "I\u00b7ris", ",", "Du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NE", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Tropfen Wolkenthau, der sich", "tokens": ["Du", "Trop\u00b7fen", "Wol\u00b7kent\u00b7hau", ",", "der", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "NE", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Scholle Poren schlich,", "tokens": ["In", "uns\u00b7rer", "Schol\u00b7le", "Po\u00b7ren", "schlich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er dem Himmel sie gew\u00f6hne", "tokens": ["Da\u00df", "er", "dem", "Him\u00b7mel", "sie", "ge\u00b7w\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem lieblichsten Gedicht,", "tokens": ["An", "sei\u00b7nem", "lieb\u00b7lichs\u00b7ten", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Du irdisch heilig wie die Thr\u00e4ne,", "tokens": ["Du", "ir\u00b7disch", "hei\u00b7lig", "wie", "die", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und himmlisch heilig wie das Licht.", "tokens": ["Und", "himm\u00b7lisch", "hei\u00b7lig", "wie", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Tropfen nur, ein Wiederschein,", "tokens": ["Ein", "Trop\u00b7fen", "nur", ",", "ein", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch alle Wunder saugend ein,", "tokens": ["Doch", "al\u00b7le", "Wun\u00b7der", "sau\u00b7gend", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Perle, dich am Blatte wiegend", "tokens": ["Ob", "Per\u00b7le", ",", "dich", "am", "Blat\u00b7te", "wie\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PRF", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und spielend um der Wiege Fu\u00df,", "tokens": ["Und", "spie\u00b7lend", "um", "der", "Wie\u00b7ge", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob s\u00fc\u00dfer Traum, im Grase liegend", "tokens": ["Ob", "s\u00fc\u00b7\u00dfer", "Traum", ",", "im", "Gra\u00b7se", "lie\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4chelnd bei des Halmes Gru\u00df.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "bei", "des", "Hal\u00b7mes", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Erd\u2019 und Himmel l\u00e4chlen auch,", "tokens": ["O", "Erd'", "und", "Him\u00b7mel", "l\u00e4ch\u00b7len", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du, geweckt vom Morgenhauch,", "tokens": ["Wenn", "du", ",", "ge\u00b7weckt", "vom", "Mor\u00b7gen\u00b7hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich einem Kinde hebst den weichen", "tokens": ["Gleich", "ei\u00b7nem", "Kin\u00b7de", "hebst", "den", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Versch\u00e4mten Mondesblick zum Tag,", "tokens": ["Ver\u00b7sch\u00e4m\u00b7ten", "Mon\u00b7des\u00b7blick", "zum", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erharrend, was die Hand des Reichen", "tokens": ["Er\u00b7har\u00b7rend", ",", "was", "die", "Hand", "des", "Rei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Glanz und Duft dir geben mag.", "tokens": ["Von", "Glanz", "und", "Duft", "dir", "ge\u00b7ben", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "L\u00e4chle nur, l\u00e4chle f\u00fcr und f\u00fcr,", "tokens": ["L\u00e4ch\u00b7le", "nur", ",", "l\u00e4ch\u00b7le", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des Kindes Reichthum wird auch dir;", "tokens": ["Des", "Kin\u00b7des", "Reicht\u00b7hum", "wird", "auch", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir wird des Zweiges Blatt zur Halle,", "tokens": ["Dir", "wird", "des", "Zwei\u00b7ges", "Blatt", "zur", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Sammet dir des Mooses Vlie\u00df,", "tokens": ["Zum", "Sam\u00b7met", "dir", "des", "Moo\u00b7ses", "Vlie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Opale, funkelnde Metalle", "tokens": ["O\u00b7pa\u00b7le", ",", "fun\u00b7keln\u00b7de", "Me\u00b7tal\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4scht Muschelscherbe dir und Kies.", "tokens": ["W\u00e4scht", "Mu\u00b7schel\u00b7scher\u00b7be", "dir", "und", "Kies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "KON", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Des kranken Blattes r\u00f6thlich Gr\u00fcn", "tokens": ["Des", "kran\u00b7ken", "Blat\u00b7tes", "r\u00f6th\u00b7lich", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fcckt auf die Stirn dir den Rubin,", "tokens": ["Dr\u00fcckt", "auf", "die", "Stirn", "dir", "den", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Mit Chrisolithes goldnen Flittern", "tokens": ["Mit", "Chri\u00b7so\u00b7li\u00b7thes", "gold\u00b7nen", "Flit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schm\u00fcckt deinen Spiegel Kraut und Gras,", "tokens": ["Schm\u00fcckt", "dei\u00b7nen", "Spie\u00b7gel", "Kraut", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und selbst des d\u00fcrren Laubes Zittern", "tokens": ["Und", "selbst", "des", "d\u00fcr\u00b7ren", "Lau\u00b7bes", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schenkt dir den br\u00e4unlichen Topas.", "tokens": ["Schenkt", "dir", "den", "br\u00e4un\u00b7li\u00b7chen", "To\u00b7pas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Und gar, wenn losch das Sonnenlicht,", "tokens": ["Und", "gar", ",", "wenn", "losch", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und um dein eigenstes Gedicht", "tokens": ["Und", "um", "dein", "ei\u00b7gens\u00b7tes", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Morgana deines Seees gaukelt,", "tokens": ["Mor\u00b7ga\u00b7na", "dei\u00b7nes", "See\u00b7es", "gau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein Traum von Licht um deinen Ball", "tokens": ["Ein", "Traum", "von", "Licht", "um", "dei\u00b7nen", "Ball"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zarte Schattenbilder schaukelt,", "tokens": ["Und", "zar\u00b7te", "Schat\u00b7ten\u00b7bil\u00b7der", "schau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gefangne Geister im Kristall:", "tokens": ["Ge\u00b7fang\u00b7ne", "Geis\u00b7ter", "im", "Kris\u00b7tall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Dann schl\u00e4fst du, schl\u00e4fst in eigner Haft,", "tokens": ["Dann", "schl\u00e4fst", "du", ",", "schl\u00e4fst", "in", "eig\u00b7ner", "Haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft walten die verborgne Kraft,", "tokens": ["L\u00e4\u00dft", "wal\u00b7ten", "die", "ver\u00b7borg\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was nicht dem Himmel, nicht der Erden,", "tokens": ["Was", "nicht", "dem", "Him\u00b7mel", ",", "nicht", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Was nie gewesen, nie wird werden,", "tokens": ["Was", "nie", "ge\u00b7we\u00b7sen", ",", "nie", "wird", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAPP", "$,", "ADV", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Embryone deiner Brust.", "tokens": ["Die", "Emb\u00b7ry\u00b7o\u00b7ne", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O l\u00e4chle, tr\u00e4ume immer zu,", "tokens": ["O", "l\u00e4ch\u00b7le", ",", "tr\u00e4u\u00b7me", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Iris der Seele, Tropfen du!", "tokens": ["I\u00b7ris", "der", "See\u00b7le", ",", "Trop\u00b7fen", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "NN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Den Wald la\u00df rauschen, im Gewimmel", "tokens": ["Den", "Wald", "la\u00df", "rau\u00b7schen", ",", "im", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entfunkeln la\u00df der Sterne Reih\u2019n;", "tokens": ["Ent\u00b7fun\u00b7keln", "la\u00df", "der", "Ster\u00b7ne", "Reih'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast die Erde, hast den Himmel,", "tokens": ["Du", "hast", "die", "Er\u00b7de", ",", "hast", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und deine Geister obendrein.", "tokens": ["Und", "dei\u00b7ne", "Geis\u00b7ter", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}