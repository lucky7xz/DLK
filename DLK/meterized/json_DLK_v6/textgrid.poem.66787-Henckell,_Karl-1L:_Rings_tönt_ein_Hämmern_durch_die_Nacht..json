{"textgrid.poem.66787": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rings t\u00f6nt ein H\u00e4mmern durch die Nacht.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings t\u00f6nt ein H\u00e4mmern durch die Nacht.", "tokens": ["Rings", "t\u00f6nt", "ein", "H\u00e4m\u00b7mern", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wo bin ich? Tief im Erdenschacht.", "tokens": ["Wo", "bin", "ich", "?", "Tief", "im", "Er\u00b7den\u00b7schacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Grubenlampen flimmern tr\u00fcb,", "tokens": ["Die", "Gru\u00b7ben\u00b7lam\u00b7pen", "flim\u00b7mern", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gib acht, so dir dein Leben lieb!", "tokens": ["Gib", "acht", ",", "so", "dir", "dein", "Le\u00b7ben", "lieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "ADV", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Links droht ein Abgrund, rechts ein Spalt,", "tokens": ["Links", "droht", "ein", "Ab\u00b7grund", ",", "rechts", "ein", "Spalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tritt sicher, wahre deinen Halt! \u2013", "tokens": ["Tritt", "si\u00b7cher", ",", "wah\u00b7re", "dei\u00b7nen", "Halt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "He, Bergmann, kehr dein Licht mir zu!", "tokens": ["He", ",", "Berg\u00b7mann", ",", "kehr", "dein", "Licht", "mir", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was klopfst du ohne Rast und Ruh?", "tokens": ["Was", "klopfst", "du", "oh\u00b7ne", "Rast", "und", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbich h\u00e4mmre fr\u00fch und h\u00e4mmre sp\u00e4t,", "tokens": ["\u00bb", "ich", "h\u00e4mm\u00b7re", "fr\u00fch", "und", "h\u00e4mm\u00b7re", "sp\u00e4t", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df dieser Fels in St\u00fccke geht.\u00ab", "tokens": ["Da\u00df", "die\u00b7ser", "Fels", "in", "St\u00fc\u00b7cke", "geht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und welche Ader zu befrein,", "tokens": ["Und", "wel\u00b7che", "A\u00b7der", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zerschmetterst du den morschen Stein?", "tokens": ["Zer\u00b7schmet\u00b7terst", "du", "den", "mor\u00b7schen", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbich suche keiner Ader Licht,", "tokens": ["\u00bb", "ich", "su\u00b7che", "kei\u00b7ner", "A\u00b7der", "Licht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ich will nur, da\u00df Zerfressnes bricht.\u00ab", "tokens": ["Ich", "will", "nur", ",", "da\u00df", "Zer\u00b7fress\u00b7nes", "bricht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und doch, um Gott, schau dort hinauf!", "tokens": ["Und", "doch", ",", "um", "Gott", ",", "schau", "dort", "hin\u00b7auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "NN", "$,", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Auf deinem Block, welch wirrer Hauf?", "tokens": ["Auf", "dei\u00b7nem", "Block", ",", "welch", "wir\u00b7rer", "Hauf", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sie klammern sich mit Angst und Pein", "tokens": ["Sie", "klam\u00b7mern", "sich", "mit", "Angst", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "An den von Grund zerh\u00f6hlten Stein.", "tokens": ["An", "den", "von", "Grund", "zer\u00b7h\u00f6hl\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "St\u00fcrzt pl\u00f6tzlich er gel\u00f6st herab,", "tokens": ["St\u00fcrzt", "pl\u00f6tz\u00b7lich", "er", "ge\u00b7l\u00f6st", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sie finden all ein schaurig Grab.", "tokens": ["Sie", "fin\u00b7den", "all", "ein", "schau\u00b7rig", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Schon gleiten im Verzweiflungswahn", "tokens": ["Schon", "glei\u00b7ten", "im", "Ver\u00b7zwei\u00b7flungs\u00b7wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Hier Mann, dort Weib die Ungl\u00fccksbahn.", "tokens": ["Hier", "Mann", ",", "dort", "Weib", "die", "Un\u00b7gl\u00fccks\u00b7bahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sieh jener Krone blinden Glanz", "tokens": ["Sieh", "je\u00b7ner", "Kro\u00b7ne", "blin\u00b7den", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und rings den unterw\u00fcrfigen Kranz!", "tokens": ["Und", "rings", "den", "un\u00b7ter\u00b7w\u00fcr\u00b7fi\u00b7gen", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Sieh dort das Kreuz, das wankend steht,", "tokens": ["Sieh", "dort", "das", "Kreuz", ",", "das", "wan\u00b7kend", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Von einer kleinen Schar umfleht!", "tokens": ["Von", "ei\u00b7ner", "klei\u00b7nen", "Schar", "um\u00b7fleht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sieh den Greis, bepackt mit Gold,", "tokens": ["Und", "sieh", "den", "Greis", ",", "be\u00b7packt", "mit", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Dem St\u00fcck um St\u00fcck zur Tiefe rollt!", "tokens": ["Dem", "St\u00fcck", "um", "St\u00fcck", "zur", "Tie\u00b7fe", "rollt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das Kreuz zerbirst, die Krone sinkt,", "tokens": ["Das", "Kreuz", "zer\u00b7birst", ",", "die", "Kro\u00b7ne", "sinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wenn dein Zerst\u00f6rungswerk gelingt,", "tokens": ["Wenn", "dein", "Zer\u00b7st\u00f6\u00b7rungs\u00b7werk", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und deiner schrankenlosen Wut", "tokens": ["Und", "dei\u00b7ner", "schran\u00b7ken\u00b7lo\u00b7sen", "Wut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Verf\u00e4llt das alterworbne Gut.", "tokens": ["Ver\u00b7f\u00e4llt", "das", "al\u00b7ter\u00b7worb\u00b7ne", "Gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbo, ich wei\u00df alles, was Ihr sagt,", "tokens": ["\u00bb", "o", ",", "ich", "wei\u00df", "al\u00b7les", ",", "was", "Ihr", "sagt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "PPER", "VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und habe lang und schwer geklagt.", "tokens": ["Und", "ha\u00b7be", "lang", "und", "schwer", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Was gehst du, rief ich, ins Gericht?", "tokens": ["Was", "gehst", "du", ",", "rief", "ich", ",", "ins", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "La\u00df sein, bis alles selbst zerbricht!", "tokens": ["La\u00df", "sein", ",", "bis", "al\u00b7les", "selbst", "zer\u00b7bricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VAINF", "$,", "KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Der Hammer sank, die Z\u00e4hre flo\u00df,", "tokens": ["Der", "Ham\u00b7mer", "sank", ",", "die", "Z\u00e4h\u00b7re", "flo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df ich die blinden Augen schlo\u00df.", "tokens": ["Da\u00df", "ich", "die", "blin\u00b7den", "Au\u00b7gen", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Es mu\u00df geschehn, sprach dann ich dumpf", "tokens": ["Es", "mu\u00df", "ge\u00b7schehn", ",", "sprach", "dann", "ich", "dumpf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und traf mit Macht den Felsenstumpf.", "tokens": ["Und", "traf", "mit", "Macht", "den", "Fel\u00b7sen\u00b7stumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Was morsch, mu\u00df fallen, sei's mit Graun,", "tokens": ["Was", "morsch", ",", "mu\u00df", "fal\u00b7len", ",", "sei's", "mit", "Graun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VMFIN", "VVINF", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Dort hinten magst du Bessres schaun.", "tokens": ["Dort", "hin\u00b7ten", "magst", "du", "Bess\u00b7res", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wer hier das Ende nahen sieht,", "tokens": ["Wer", "hier", "das", "En\u00b7de", "na\u00b7hen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Tut wohl, da\u00df dorthin er entflieht.", "tokens": ["Tut", "wohl", ",", "da\u00df", "dor\u00b7thin", "er", "ent\u00b7flieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Ich h\u00e4mmre fr\u00fch und h\u00e4mmre sp\u00e4t,", "tokens": ["Ich", "h\u00e4mm\u00b7re", "fr\u00fch", "und", "h\u00e4mm\u00b7re", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df dieser Fels in St\u00fccke geht.\u00ab", "tokens": ["Da\u00df", "die\u00b7ser", "Fels", "in", "St\u00fc\u00b7cke", "geht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Gestalten huschen sp\u00e4rlich fort", "tokens": ["Ge\u00b7stal\u00b7ten", "hu\u00b7schen", "sp\u00e4r\u00b7lich", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Nach dem verhei\u00dfungsvollen Ort.", "tokens": ["Nach", "dem", "ver\u00b7hei\u00b7\u00dfungs\u00b7vol\u00b7len", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ah, hell und heller flammt der Schein,", "tokens": ["Ah", ",", "hell", "und", "hel\u00b7ler", "flammt", "der", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Entstrahlt aus seltsam neuem Stein,", "tokens": ["Ent\u00b7strahlt", "aus", "selt\u00b7sam", "neu\u00b7em", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Den, wo er br\u00fcchig noch und rauh,", "tokens": ["Den", ",", "wo", "er", "br\u00fc\u00b7chig", "noch", "und", "rauh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Viel Mei\u00dfler gl\u00e4tten kunstgenau,", "tokens": ["Viel", "Mei\u00df\u00b7ler", "gl\u00e4t\u00b7ten", "kunst\u00b7ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Bis, was vordem ei