{"textgrid.poem.38641": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der unbew\u00f6lkten Luna Silberschein", "tokens": ["Der", "un\u00b7be\u00b7w\u00f6lk\u00b7ten", "Lu\u00b7na", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wallt lieblich durch der Kirchhofb\u00e4ume Laub,", "tokens": ["Wallt", "lieb\u00b7lich", "durch", "der", "Kirch\u00b7hof\u00b7b\u00e4u\u00b7me", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Bl\u00fcthen, wie zum Todtenopfer, streun,", "tokens": ["Und", "Bl\u00fc\u00b7then", ",", "wie", "zum", "Tod\u00b7ten\u00b7op\u00b7fer", ",", "streun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "C\u00e4cilia, die Wind' auf deinen Staub!", "tokens": ["C\u00e4\u00b7ci\u00b7lia", ",", "die", "Wind'", "auf", "dei\u00b7nen", "Staub", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dir lacht kein Mai, dir gl\u00e4nzt vom Sternenraum", "tokens": ["Dir", "lacht", "kein", "Mai", ",", "dir", "gl\u00e4nzt", "vom", "Ster\u00b7nen\u00b7raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In lauer Sommernacht kein Vollmond mehr:", "tokens": ["In", "lau\u00b7er", "Som\u00b7mer\u00b7nacht", "kein", "Voll\u00b7mond", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wohl, Befreite, wohl dir, ach! dein Traum", "tokens": ["Doch", ",", "wohl", ",", "Be\u00b7frei\u00b7te", ",", "wohl", "dir", ",", "ach", "!", "dein", "Traum"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "$,", "NN", "$,", "ADV", "PPER", "$,", "ITJ", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Lande der Entsagung war so schwer!", "tokens": ["Im", "Lan\u00b7de", "der", "Ent\u00b7sa\u00b7gung", "war", "so", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Wahrheit Sonnenschimmer starben hier,", "tokens": ["Der", "Wahr\u00b7heit", "Son\u00b7nen\u00b7schim\u00b7mer", "star\u00b7ben", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Flamm' in Gr\u00fcften matt sich senkt;", "tokens": ["Wie", "ei\u00b7ne", "Flamm'", "in", "Gr\u00fcf\u00b7ten", "matt", "sich", "senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf Heiligenlegenden und Brevier", "tokens": ["Auf", "Hei\u00b7li\u00b7gen\u00b7le\u00b7gen\u00b7den", "und", "Bre\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+++", "measure": "zehnsilber"}, "line.4": {"text": "Blieb deiner Kenntni\u00df enger Kreis beschr\u00e4nkt.", "tokens": ["Blieb", "dei\u00b7ner", "Kennt\u00b7ni\u00df", "en\u00b7ger", "Kreis", "be\u00b7schr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir hat die Z\u00e4hmerin des Mi\u00dfgeschicks,", "tokens": ["Dir", "hat", "die", "Z\u00e4h\u00b7me\u00b7rin", "des", "Mi\u00df\u00b7ge\u00b7schicks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tonkunst, ihre Zauber nicht enth\u00fcllt;", "tokens": ["Die", "Ton\u00b7kunst", ",", "ih\u00b7re", "Zau\u00b7ber", "nicht", "ent\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein ganzer Hausrath war ein Kruzifix,", "tokens": ["Dein", "gan\u00b7zer", "Haus\u00b7rath", "war", "ein", "Kru\u00b7zi\u00b7fix", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Todtenkopf und ein Madonnenbild.", "tokens": ["Ein", "Tod\u00b7ten\u00b7kopf", "und", "ein", "Ma\u00b7don\u00b7nen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Am Fenster, welches Rebengr\u00fcn umzog,", "tokens": ["Am", "Fens\u00b7ter", ",", "wel\u00b7ches", "Re\u00b7ben\u00b7gr\u00fcn", "um\u00b7zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verlor sich oft ins weite Meer dein Blick,", "tokens": ["Ver\u00b7lor", "sich", "oft", "ins", "wei\u00b7te", "Meer", "dein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bebte, wann ein Schiff vor\u00fcberflog,", "tokens": ["Und", "beb\u00b7te", ",", "wann", "ein", "Schiff", "vor\u00b7\u00fc\u00b7berf\u00b7log", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bethr\u00e4nter in des Kerkers Graun zur\u00fcck.", "tokens": ["Be\u00b7thr\u00e4n\u00b7ter", "in", "des", "Ker\u00b7kers", "Graun", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bei Filomelens Abendlied umflo\u00df", "tokens": ["Bei", "Fi\u00b7lo\u00b7me\u00b7lens", "A\u00b7ben\u00b7dlied", "um\u00b7flo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schwermuth Wolke dunkler dein Gesicht;", "tokens": ["Der", "Schwer\u00b7muth", "Wol\u00b7ke", "dunk\u00b7ler", "dein", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur mit dem Hall der Sterbeglocken go\u00df", "tokens": ["Nur", "mit", "dem", "Hall", "der", "Ster\u00b7be\u00b7glo\u00b7cken", "go\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "In deines Daseyns Nacht sich Morgenlicht.", "tokens": ["In", "dei\u00b7nes", "Da\u00b7seyns", "Nacht", "sich", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr Himmelsboten, die ihr unsichtbar", "tokens": ["Ihr", "Him\u00b7mels\u00b7bo\u00b7ten", ",", "die", "ihr", "un\u00b7sicht\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Menschheit hingesunkne Blumen hebt,", "tokens": ["Der", "Menschheit", "hin\u00b7ge\u00b7sunk\u00b7ne", "Blu\u00b7men", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und um des Aberglaubens Weihaltar", "tokens": ["Und", "um", "des", "A\u00b7berg\u00b7lau\u00b7bens", "Wei\u00b7hal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im S\u00e4useln hoher Friedensahnung schwebt:", "tokens": ["Im", "S\u00e4u\u00b7seln", "ho\u00b7her", "Frie\u00b7den\u00b7sah\u00b7nung", "schwebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ihr h\u00f6rtet an des offnen Grabes Rand'", "tokens": ["Ihr", "h\u00f6r\u00b7tet", "an", "des", "off\u00b7nen", "Gra\u00b7bes", "Rand'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihrer Brust den ersten Wonnelaut;", "tokens": ["Aus", "ih\u00b7rer", "Brust", "den", "ers\u00b7ten", "Won\u00b7ne\u00b7laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr saht, wie auf des Todes kalte Hand", "tokens": ["Ihr", "saht", ",", "wie", "auf", "des", "To\u00b7des", "kal\u00b7te", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie Thr\u00e4nen, freudig schaudernd, hingethaut.", "tokens": ["Sie", "Thr\u00e4\u00b7nen", ",", "freu\u00b7dig", "schau\u00b7dernd", ",", "hin\u00b7ge\u00b7thaut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie schlummert in der Espen D\u00e4mmrung dort,", "tokens": ["Sie", "schlum\u00b7mert", "in", "der", "Es\u00b7pen", "D\u00e4mm\u00b7rung", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo fromm den Wandrer, der betrachtend steht,", "tokens": ["Wo", "fromm", "den", "Wand\u00b7rer", ",", "der", "be\u00b7trach\u00b7tend", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kreuz mit Namen, Jahr und Heimatsort,", "tokens": ["Ein", "Kreuz", "mit", "Na\u00b7men", ",", "Jahr", "und", "Hei\u00b7mat\u00b7sort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um ein Gebet und eine Z\u00e4hre fleht.", "tokens": ["Um", "ein", "Ge\u00b7bet", "und", "ei\u00b7ne", "Z\u00e4h\u00b7re", "fleht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}