{"dta.poem.20653": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  einen jungen Freund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tugend Freund! der Wahrheit Redner, Du &#8212;                 ", "tokens": ["Der", "Tu\u00b7gend", "Freund", "!", "der", "Wahr\u00b7heit", "Red\u00b7ner", ",", "Du", "&#8212;"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "XML_entity"], "pos": ["ART", "NN", "NN", "$.", "ART", "NN", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lobst mein Talent, schreibst der Natur es zu.", "tokens": ["Lobst", "mein", "Ta\u00b7lent", ",", "schreibst", "der", "Na\u00b7tur", "es", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie ist es werth, und ihr geb\u00fchrt die Ehre,", "tokens": ["Sie", "ist", "es", "werth", ",", "und", "ihr", "ge\u00b7b\u00fchrt", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr dank ich Einfall, Ausdruck, Geist und Schwung;", "tokens": ["Ihr", "dank", "ich", "Ein\u00b7fall", ",", "Aus\u00b7druck", ",", "Geist", "und", "Schwung", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir gab die Kunst niemals Bereicherung,", "tokens": ["Mir", "gab", "die", "Kunst", "nie\u00b7mals", "Be\u00b7rei\u00b7che\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und nie nahm ich von einem Meister Lehre.", "tokens": ["Und", "nie", "nahm", "ich", "von", "ei\u00b7nem", "Meis\u00b7ter", "Leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Vater, der nicht Geld in Kasten wog,", "tokens": ["Mein", "Va\u00b7ter", ",", "der", "nicht", "Geld", "in", "Kas\u00b7ten", "wog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PTKNEG", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der war nicht gro\u00df, und wo man mich erzog,", "tokens": ["Der", "war", "nicht", "gro\u00df", ",", "und", "wo", "man", "mich", "er\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "KON", "PWAV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War keiner, der das Unterweisen kannte.", "tokens": ["War", "kei\u00b7ner", ",", "der", "das", "Un\u00b7ter\u00b7wei\u00b7sen", "kann\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich spielte l\u00e4ndlich, baute mir im Sand", "tokens": ["Ich", "spiel\u00b7te", "l\u00e4nd\u00b7lich", ",", "bau\u00b7te", "mir", "im", "Sand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Oft einen Thurm, er war der Gegenstand", "tokens": ["Oft", "ei\u00b7nen", "Thurm", ",", "er", "war", "der", "Ge\u00b7gen\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von einer Wuth, mit der ich ihn berannte.", "tokens": ["Von", "ei\u00b7ner", "Wuth", ",", "mit", "der", "ich", "ihn", "be\u00b7rann\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So kriegerisch, und doch ein M\u00e4dchen seyn?", "tokens": ["So", "krie\u00b7ge\u00b7risch", ",", "und", "doch", "ein", "M\u00e4d\u00b7chen", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja! doch ich lud oft Sch\u00e4ferinnen ein", "tokens": ["Ja", "!", "doch", "ich", "lud", "oft", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KON", "PPER", "VVFIN", "ADV", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur frischen Milch, zu Tanz und Spielen.", "tokens": ["Zur", "fri\u00b7schen", "Milch", ",", "zu", "Tanz", "und", "Spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ungef\u00e4hr fand ich als Kind ein Blatt,", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4hr", "fand", "ich", "als", "Kind", "ein", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Franken, der ehmals gedichtet hat,", "tokens": ["Von", "Fran\u00b7ken", ",", "der", "eh\u00b7mals", "ge\u00b7dich\u00b7tet", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und las den Vers, und lernte f\u00fchlen.", "tokens": ["Und", "las", "den", "Vers", ",", "und", "lern\u00b7te", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O, die Geschichte meiner Jugend ist", "tokens": ["O", ",", "die", "Ge\u00b7schich\u00b7te", "mei\u00b7ner", "Ju\u00b7gend", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zu sonderbar; und weil Du g\u00fctig bist,", "tokens": ["Zu", "son\u00b7der\u00b7bar", ";", "und", "weil", "Du", "g\u00fc\u00b7tig", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$.", "KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So darf ich Dich zu meinem Freunde w\u00e4hlen:", "tokens": ["So", "darf", "ich", "Dich", "zu", "mei\u00b7nem", "Freun\u00b7de", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann werd\u2019 ich Dir, Du sch\u00f6ner Dichter! nur", "tokens": ["Dann", "werd'", "ich", "Dir", ",", "Du", "sch\u00f6\u00b7ner", "Dich\u00b7ter", "!", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Empfindungsvoll zur Ehre der Natur,", "tokens": ["Emp\u00b7fin\u00b7dungs\u00b7voll", "zur", "Eh\u00b7re", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie sie mich ausgebildet hat, erz\u00e4hlen.", "tokens": ["Wie", "sie", "mich", "aus\u00b7ge\u00b7bil\u00b7det", "hat", ",", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}