{"textgrid.poem.41666": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bists, Andromache! Dies Fl\u00fc\u00dfchen, das ein grauer", "tokens": ["Du", "bists", ",", "And\u00b7ro\u00b7ma\u00b7che", "!", "Dies", "Fl\u00fc\u00df\u00b7chen", ",", "das", "ein", "grau\u00b7er"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "$.", "PDS", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und armer Spiegel ist, wo einstmals hoheitsvoll", "tokens": ["Und", "ar\u00b7mer", "Spie\u00b7gel", "ist", ",", "wo", "einst\u00b7mals", "ho\u00b7heits\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gegl\u00e4nzt die Majest\u00e4t all deiner Witwentrauer,", "tokens": ["Ge\u00b7gl\u00e4nzt", "die", "Ma\u00b7jes\u00b7t\u00e4t", "all", "dei\u00b7ner", "Wit\u00b7went\u00b7rau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der falsche Simois, der durch dein Weinen schwoll,", "tokens": ["Der", "fal\u00b7sche", "Si\u00b7mois", ",", "der", "durch", "dein", "Wei\u00b7nen", "schwoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Scheint mir, als ob belebt er mein Ged\u00e4chtnis h\u00e4tte,", "tokens": ["Scheint", "mir", ",", "als", "ob", "be\u00b7lebt", "er", "mein", "Ge\u00b7d\u00e4cht\u00b7nis", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "KOUS", "VVFIN", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich hin\u00fcberschritt das neue Karussell.", "tokens": ["Als", "ich", "hin\u00b7\u00fc\u00b7bersc\u00b7hritt", "das", "neu\u00b7e", "Ka\u00b7rus\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u2013 Das einstige Paris ist hin, die Form der St\u00e4dte", "tokens": ["\u2013", "Das", "eins\u00b7ti\u00b7ge", "Pa\u00b7ris", "ist", "hin", ",", "die", "Form", "der", "St\u00e4d\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NE", "VAFIN", "ADV", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwischt sich, nicht einmal die Liebe stirbt so schnell.", "tokens": ["Ver\u00b7wischt", "sich", ",", "nicht", "ein\u00b7mal", "die", "Lie\u00b7be", "stirbt", "so", "schnell", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Geiste schau ich nur das Feld von H\u00fctten wimmelnd,", "tokens": ["Im", "Geis\u00b7te", "schau", "ich", "nur", "das", "Feld", "von", "H\u00fct\u00b7ten", "wim\u00b7melnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kapitale und der Sch\u00e4fte wirren Wust,", "tokens": ["Der", "Ka\u00b7pi\u00b7ta\u00b7le", "und", "der", "Sch\u00e4f\u00b7te", "wir\u00b7ren", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Gras, die Bl\u00f6cke rings, in feuchtem Moose schimmelnd,", "tokens": ["Das", "Gras", ",", "die", "Bl\u00f6\u00b7cke", "rings", ",", "in", "feuch\u00b7tem", "Moo\u00b7se", "schim\u00b7melnd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch der Fenster Schein den Tr\u00f6delkram und Grust.", "tokens": ["Und", "durch", "der", "Fens\u00b7ter", "Schein", "den", "Tr\u00f6\u00b7del\u00b7kram", "und", "Grust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Schaubuden standen dort. Da sah ich in der Fr\u00fche,", "tokens": ["Schau\u00b7bu\u00b7den", "stan\u00b7den", "dort", ".", "Da", "sah", "ich", "in", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Zur Zeit, da fr\u00f6stelnd sich in klarem Morgenduft", "tokens": ["Zur", "Zeit", ",", "da", "fr\u00f6s\u00b7telnd", "sich", "in", "kla\u00b7rem", "Mor\u00b7gen\u00b7duft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADJD", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Arbeit neu erhebt und es uns deucht, als spr\u00fche", "tokens": ["Die", "Ar\u00b7beit", "neu", "er\u00b7hebt", "und", "es", "uns", "deucht", ",", "als", "spr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "KON", "PPER", "PPER", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Besen wie ein Sturm Staub in die stille Luft,", "tokens": ["Vom", "Be\u00b7sen", "wie", "ein", "Sturm", "Staub", "in", "die", "stil\u00b7le", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Wie sich ein Schwan, der, dem Verlie\u00df entkommen, schweifte,", "tokens": ["Wie", "sich", "ein", "Schwan", ",", "der", ",", "dem", "Ver\u00b7lie\u00df", "ent\u00b7kom\u00b7men", ",", "schweif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "$,", "PRELS", "$,", "ART", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit breitem Flossenfu\u00df am staubgen Pflaster rieb,", "tokens": ["Mit", "brei\u00b7tem", "Flos\u00b7sen\u00b7fu\u00df", "am", "staub\u00b7gen", "Pflas\u00b7ter", "rieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wei\u00dfen Schwingen auf dem rauhen Boden schleifte,", "tokens": ["Die", "wei\u00b7\u00dfen", "Schwin\u00b7gen", "auf", "dem", "rau\u00b7hen", "Bo\u00b7den", "schleif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Schnabel \u00f6ffnend vor der Gosse stehen blieb.", "tokens": ["Den", "Schna\u00b7bel", "\u00f6ff\u00b7nend", "vor", "der", "Gos\u00b7se", "ste\u00b7hen", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erzitternd badete im Staub er sein Gefieder", "tokens": ["Er\u00b7zit\u00b7ternd", "ba\u00b7de\u00b7te", "im", "Staub", "er", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach, das Herz erf\u00fcllt vom blauen Heimatssee:", "tokens": ["Und", "sprach", ",", "das", "Herz", "er\u00b7f\u00fcllt", "vom", "blau\u00b7en", "Hei\u00b7mats\u00b7see", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann, Wolke, regnest du? Wann f\u00e4llst, o Blitz, du nieder?", "tokens": ["Wann", ",", "Wol\u00b7ke", ",", "reg\u00b7nest", "du", "?", "Wann", "f\u00e4llst", ",", "o", "Blitz", ",", "du", "nie\u00b7der", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "$,", "FM", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sah des Armen fremd und sagenhaftes Weh.", "tokens": ["Ich", "sah", "des", "Ar\u00b7men", "fremd", "und", "sa\u00b7gen\u00b7haf\u00b7tes", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zum Himmel reckte er, wie es Ovid gedichtet,", "tokens": ["Zum", "Him\u00b7mel", "reck\u00b7te", "er", ",", "wie", "es", "O\u00b7vid", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Himmel, blau und hart wie grausam bittrer Spott,", "tokens": ["Zum", "Him\u00b7mel", ",", "blau", "und", "hart", "wie", "grau\u00b7sam", "bit\u00b7trer", "Spott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "KOKOM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf seinem schwanken Hals sein durstig Haupt, als richtet'", "tokens": ["Auf", "sei\u00b7nem", "schwan\u00b7ken", "Hals", "sein", "durs\u00b7tig", "Haupt", ",", "als", "rich\u00b7tet'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "In seiner herben Qual Vorw\u00fcrfe er an Gott!", "tokens": ["In", "sei\u00b7ner", "her\u00b7ben", "Qual", "Vor\u00b7w\u00fcr\u00b7fe", "er", "an", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}