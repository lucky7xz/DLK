{"textgrid.poem.56895": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach Klostersitte flo\u00df dein wollen Kleid", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Klostersitte flo\u00df dein wollen Kleid", "tokens": ["Nach", "Klos\u00b7ter\u00b7sit\u00b7te", "flo\u00df", "dein", "wol\u00b7len", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in grauer Strenge faltenlos zum Fu\u00df,", "tokens": ["in", "grau\u00b7er", "Stren\u00b7ge", "fal\u00b7ten\u00b7los", "zum", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch dr\u00fcber hin, gel\u00f6st und quellend reich,", "tokens": ["doch", "dr\u00fc\u00b7ber", "hin", ",", "ge\u00b7l\u00f6st", "und", "quel\u00b7lend", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKVZ", "$,", "VVPP", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "des sanftesten Marienkopfs Gelock.", "tokens": ["des", "sanf\u00b7tes\u00b7ten", "Ma\u00b7ri\u00b7en\u00b7kopfs", "Ge\u00b7lock", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Braunaugen, wie von stiller Gluten Wehn", "tokens": ["Braun\u00b7au\u00b7gen", ",", "wie", "von", "stil\u00b7ler", "Glu\u00b7ten", "Wehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "erschimmernd, sich verschleiernd \u2013 strahlt ihr noch? ...", "tokens": ["er\u00b7schim\u00b7mernd", ",", "sich", "ver\u00b7schlei\u00b7ernd", "\u2013", "strahlt", "ihr", "noch", "?", "..."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "PRF", "VVPP", "$(", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich war wohl acht, du dreizehn Jahre alt.", "tokens": ["Ich", "war", "wohl", "acht", ",", "du", "drei\u00b7zehn", "Jah\u00b7re", "alt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "$,", "PPER", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was wars, das unsre Lippen j\u00e4h verband \u2013", "tokens": ["Was", "wars", ",", "das", "uns\u00b7re", "Lip\u00b7pen", "j\u00e4h", "ver\u00b7band", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "ach ", "tokens": ["ach"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "wie erster unaussprechlich s\u00fc\u00dfer Durst", "tokens": ["wie", "ers\u00b7ter", "un\u00b7aus\u00b7sprech\u00b7lich", "s\u00fc\u00b7\u00dfer", "Durst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "von Mann zu Weib \u2013 in weltverge\u00dfnem Ku\u00df \u2013", "tokens": ["von", "Mann", "zu", "Weib", "\u2013", "in", "welt\u00b7ver\u00b7ge\u00df\u00b7nem", "Ku\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$(", "APPR", "PWAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "dem sch\u00f6nsten Kusse, den ich je gek\u00fc\u00dft ...? ...", "tokens": ["dem", "sch\u00f6ns\u00b7ten", "Kus\u00b7se", ",", "den", "ich", "je", "ge\u00b7k\u00fc\u00dft", "...", "?", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$(", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wo weilst du, Liebe, \u2013 nun wohl Mutter l\u00e4ngst,", "tokens": ["Wo", "weilst", "du", ",", "Lie\u00b7be", ",", "\u2013", "nun", "wohl", "Mut\u00b7ter", "l\u00e4ngst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "$(", "ADV", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "doch ewig junge Beatrice mir \u2013?", "tokens": ["doch", "e\u00b7wig", "jun\u00b7ge", "Be\u00b7at\u00b7ri\u00b7ce", "mir", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "PPER", "$(", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Gemahnt auch dich noch Hauch versunkner Zeit \u2013", "tokens": ["Ge\u00b7mahnt", "auch", "dich", "noch", "Hauch", "ver\u00b7sun\u00b7kner", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PPER", "ADV", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und gabst auch Du dein Herz nie s\u00fc\u00dfer hin?", "tokens": ["und", "gabst", "auch", "Du", "dein", "Herz", "nie", "s\u00fc\u00b7\u00dfer", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}