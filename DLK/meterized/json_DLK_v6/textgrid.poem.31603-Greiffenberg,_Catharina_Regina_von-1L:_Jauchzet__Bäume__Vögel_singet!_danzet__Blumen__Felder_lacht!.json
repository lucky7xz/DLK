{"textgrid.poem.31603": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jauchzet / B\u00e4ume / V\u00f6gel singet! danzet / Blumen / Felder lacht!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jauchzet / B\u00e4ume / V\u00f6gel singet! danzet / Blumen / Felder lacht!", "tokens": ["Jauch\u00b7zet", "/", "B\u00e4u\u00b7me", "/", "V\u00f6\u00b7gel", "sin\u00b7get", "!", "dan\u00b7zet", "/", "Blu\u00b7men", "/", "Fel\u00b7der", "lacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "VVFIN", "$.", "VVFIN", "$(", "NN", "$(", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "springt / ihr Br\u00fcnnlein! B\u00e4chlein rauscht! spielet ihr gelinden Winde!", "tokens": ["springt", "/", "ihr", "Br\u00fcnn\u00b7lein", "!", "B\u00e4ch\u00b7lein", "rauscht", "!", "spie\u00b7let", "ihr", "ge\u00b7lin\u00b7den", "Win\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPOSAT", "NN", "$.", "NN", "VVFIN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "walle / Lust-bewegtes Tr\u00e4id! s\u00fcsse Fl\u00fcsse fliest geschwinde!", "tokens": ["wal\u00b7le", "/", "Lust\u00b7be\u00b7weg\u00b7tes", "Tr\u00e4id", "!", "s\u00fcs\u00b7se", "Fl\u00fcs\u00b7se", "fliest", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$.", "ADJA", "NN", "VVFIN", "ADJA", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "opffert Lob-Geruch dem Sch\u00f6pffer / der euch frisch und neu gemacht!", "tokens": ["opf\u00b7fert", "Lob\u00b7Ge\u00b7ruch", "dem", "Sch\u00f6pf\u00b7fer", "/", "der", "euch", "frisch", "und", "neu", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$(", "PRELS", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "jedes Bl\u00fchlein sey ein Schale / drauff Lob-Opffer ihm gebracht /", "tokens": ["je\u00b7des", "Bl\u00fch\u00b7lein", "sey", "ein", "Scha\u00b7le", "/", "drauff", "Lob\u00b7Opf\u00b7fer", "ihm", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$(", "PAV", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "jedes Gr\u00e4slein eine Seul / da sein Namens-Ehr man finde.", "tokens": ["je\u00b7des", "Gr\u00e4s\u00b7lein", "ei\u00b7ne", "Seul", "/", "da", "sein", "Na\u00b7mens\u00b7Ehr", "man", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$(", "KOUS", "PPOSAT", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "an die neu-belaubten Aestlein / Gottes Gnaden-Ruhm man binde!", "tokens": ["an", "die", "neu\u00b7be\u00b7laub\u00b7ten", "A\u00b7est\u00b7lein", "/", "Got\u00b7tes", "Gna\u00b7den\u00b7Ruhm", "man", "bin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "NN", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "da\u00df / so weit sein G\u00fct sich strecket / werd' auch seiner Ehr gedacht.", "tokens": ["da\u00df", "/", "so", "weit", "sein", "G\u00fct", "sich", "stre\u00b7cket", "/", "werd'", "auch", "sei\u00b7ner", "Ehr", "ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ADV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$(", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Du vor alles / Menschen Volck / seiner G\u00fcte Einflu\u00df Ziele!", "tokens": ["Du", "vor", "al\u00b7les", "/", "Men\u00b7schen", "Volck", "/", "sei\u00b7ner", "G\u00fc\u00b7te", "Ein\u00b7flu\u00df", "Zie\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "$(", "NN", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "aller Lieblichkeit Genie\u00dfer Abgrund / wo der Wunderflu\u00df", "tokens": ["al\u00b7ler", "Lieb\u00b7lich\u00b7keit", "Ge\u00b7nie\u00b7\u00dfer", "Ab\u00b7grund", "/", "wo", "der", "Wun\u00b7der\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN", "$(", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "endet und zu gut verwendet seinen Lieb-vergulten Gu\u00df.", "tokens": ["en\u00b7det", "und", "zu", "gut", "ver\u00b7wen\u00b7det", "sei\u00b7nen", "Lie\u00b7bver\u00b7gul\u00b7ten", "Gu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PTKA", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Gott mit Herz / Hand / Sinn und Stimm / lobe / prei\u00dfe / dicht' und spiele.", "tokens": ["Gott", "mit", "Herz", "/", "Hand", "/", "Sinn", "und", "Stimm", "/", "lo\u00b7be", "/", "prei\u00b7\u00dfe", "/", "dicht'", "und", "spie\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "La\u00df / vor Lieb' und Lobes-Gier / Muht und Blut zu Kohlen werden /", "tokens": ["La\u00df", "/", "vor", "Lieb'", "und", "Lo\u00b7bes\u00b7Gier", "/", "Muht", "und", "Blut", "zu", "Koh\u00b7len", "wer\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "APPR", "NN", "KON", "NN", "$(", "NN", "KON", "NN", "APPR", "NN", "VAINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "lege Lob und Dank darauff: Gott zum s\u00fcssen Rauch auf Erden.", "tokens": ["le\u00b7ge", "Lob", "und", "Dank", "dar\u00b7auff", ":", "Gott", "zum", "s\u00fcs\u00b7sen", "Rauch", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PAV", "$.", "NN", "APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}