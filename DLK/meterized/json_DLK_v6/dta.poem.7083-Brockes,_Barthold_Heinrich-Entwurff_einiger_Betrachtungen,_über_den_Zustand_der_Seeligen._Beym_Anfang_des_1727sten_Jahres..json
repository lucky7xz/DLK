{"dta.poem.7083": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOTT! wenn ich nur dies allein,", "tokens": ["Ach", "GoTT", "!", "wenn", "ich", "nur", "dies", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "KOUS", "PPER", "ADV", "PDS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Andacht \u00fcberleg\u2019; erblick ich schon hienieden,", "tokens": ["Mit", "An\u00b7dacht", "\u00fc\u00b7ber\u00b7leg'", ";", "er\u00b7blick", "ich", "schon", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$.", "VVIMP", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Licht, das mich begl\u00fcckt:", "tokens": ["Ein", "Licht", ",", "das", "mich", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich darinn, je mehr ich es ergr\u00fcnde,", "tokens": ["Weil", "ich", "da\u00b7rinn", ",", "je", "mehr", "ich", "es", "er\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein etwas, welches uns nicht unbegreifflich, finde.", "tokens": ["Ein", "et\u00b7was", ",", "wel\u00b7ches", "uns", "nicht", "un\u00b7be\u00b7greif\u00b7flich", ",", "fin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich werde gleichsam, als entz\u00fcckt!", "tokens": ["Ich", "wer\u00b7de", "gleich\u00b7sam", ",", "als", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich \u00fcbersch\u00fcttet Freud und Wonne,", "tokens": ["Mich", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", "Freud", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn aus der Pracht von unsrer Welt,", "tokens": ["Wenn", "aus", "der", "Pracht", "von", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und aus der Sch\u00f6nheit einer Sonne,", "tokens": ["Und", "aus", "der", "Sch\u00f6n\u00b7heit", "ei\u00b7ner", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Geist sich f\u00fcrgestellt:", "tokens": ["Mein", "Geist", "sich", "f\u00fcr\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wie tausend Millionen Erden,", "tokens": ["Wie", "tau\u00b7send", "Mil\u00b7lion\u00b7en", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Von unterschiedner Pracht und Gr\u00f6sse,", "tokens": ["Von", "un\u00b7ter\u00b7schied\u00b7ner", "Pracht", "und", "Gr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mir so viel herrliche Gef\u00e4sse,", "tokens": ["Mir", "so", "viel", "herr\u00b7li\u00b7che", "Ge\u00b7f\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Voll Majest\u00e4t des Sch\u00f6pffers, werden.", "tokens": ["Voll", "Ma\u00b7jes\u00b7t\u00e4t", "des", "Sch\u00f6pf\u00b7fers", ",", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}