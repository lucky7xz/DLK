{"textgrid.poem.24952": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrsnacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnee, weithin Schnee! Und immer noch streut", "tokens": ["Schnee", ",", "weit\u00b7hin", "Schnee", "!", "Und", "im\u00b7mer", "noch", "streut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "NN", "$.", "KON", "ADV", "ADV", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Winter st\u00e4ubende Flocken;", "tokens": ["Der", "Win\u00b7ter", "st\u00e4u\u00b7ben\u00b7de", "Flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vom Turme t\u00f6nt es wie Sterbegel\u00e4ut,", "tokens": ["Vom", "Tur\u00b7me", "t\u00f6nt", "es", "wie", "Ster\u00b7be\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mir bebt das Herz mit den Glocken.", "tokens": ["Und", "mir", "bebt", "das", "Herz", "mit", "den", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Todblasse Gestalten durchschweben die Luft,", "tokens": ["Tod\u00b7blas\u00b7se", "Ge\u00b7stal\u00b7ten", "durch\u00b7schwe\u00b7ben", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die jammernd die H\u00e4nde ringen,", "tokens": ["Die", "jam\u00b7mernd", "die", "H\u00e4n\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fernher dazwischen in Nebelduft", "tokens": ["Fern\u00b7her", "da\u00b7zwi\u00b7schen", "in", "Ne\u00b7bel\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPR", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der Mitternachtmette Singen.", "tokens": ["Der", "Mit\u00b7ter\u00b7nacht\u00b7met\u00b7te", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Klagt, Glocken, klagt, da\u00df mit jedem Jahr", "tokens": ["Klagt", ",", "Glo\u00b7cken", ",", "klagt", ",", "da\u00df", "mit", "je\u00b7dem", "Jahr"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir \u00e4rmer an Glauben und Lieben!", "tokens": ["Wir", "\u00e4r\u00b7mer", "an", "Glau\u00b7ben", "und", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "KON", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Klagt um das Viele, das unser war,", "tokens": ["Klagt", "um", "das", "Vie\u00b7le", ",", "das", "un\u00b7ser", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und das Wenige, das uns geblieben.", "tokens": ["Und", "das", "We\u00b7ni\u00b7ge", ",", "das", "uns", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dahin, was nimmer, o nimmer kehrt,", "tokens": ["Da\u00b7hin", ",", "was", "nim\u00b7mer", ",", "o", "nim\u00b7mer", "kehrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "ADV", "$,", "FM", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Tau auf des Lebens Bl\u00fcte;", "tokens": ["Der", "Tau", "auf", "des", "Le\u00b7bens", "Bl\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erloschen der heilige Flammenherd,", "tokens": ["Er\u00b7lo\u00b7schen", "der", "hei\u00b7li\u00b7ge", "Flam\u00b7men\u00b7herd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Glut in die Seele spr\u00fchte!", "tokens": ["Der", "Glut", "in", "die", "See\u00b7le", "spr\u00fch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Klagt, Glocken, klagt um des Menschen Geschick,", "tokens": ["Klagt", ",", "Glo\u00b7cken", ",", "klagt", "um", "des", "Men\u00b7schen", "Ge\u00b7schick", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der ewig Verlornes bejammert", "tokens": ["Der", "e\u00b7wig", "Ver\u00b7lor\u00b7nes", "be\u00b7jam\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und umsonst den schwindenden Augenblick", "tokens": ["Und", "um\u00b7sonst", "den", "schwin\u00b7den\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Schiffbruch des Lebens umklammert.", "tokens": ["Im", "Schiff\u00b7bruch", "des", "Le\u00b7bens", "um\u00b7klam\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Er sp\u00e4ht und sp\u00e4ht nach dem rettenden Port", "tokens": ["Er", "sp\u00e4ht", "und", "sp\u00e4ht", "nach", "dem", "ret\u00b7ten\u00b7den", "Port"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Meere, dem uferlosen,", "tokens": ["Im", "Mee\u00b7re", ",", "dem", "u\u00b7fer\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch fort, von Klippe zu Klippe fort,", "tokens": ["Doch", "fort", ",", "von", "Klip\u00b7pe", "zu", "Klip\u00b7pe", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "APPR", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rei\u00dft ihn der Wellen Tosen.", "tokens": ["Rei\u00dft", "ihn", "der", "Wel\u00b7len", "To\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zw\u00f6lf Schl\u00e4ge vom Turm! Gestorben das Jahr;", "tokens": ["Zw\u00f6lf", "Schl\u00e4\u00b7ge", "vom", "Turm", "!", "Ge\u00b7stor\u00b7ben", "das", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bleich d\u00e4mmert durch Nebel und Flocken", "tokens": ["Bleich", "d\u00e4m\u00b7mert", "durch", "Ne\u00b7bel", "und", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das neue herauf mit dem kalten Januar;", "tokens": ["Das", "neu\u00b7e", "her\u00b7auf", "mit", "dem", "kal\u00b7ten", "Ja\u00b7nu\u00b7ar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Im Winde verhallen die Glocken.", "tokens": ["Im", "Win\u00b7de", "ver\u00b7hal\u00b7len", "die", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}