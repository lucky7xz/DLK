{"textgrid.poem.36890": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Demut", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! lehre mich, wenn ich der Tugend diene,", "tokens": ["Herr", "!", "leh\u00b7re", "mich", ",", "wenn", "ich", "der", "Tu\u00b7gend", "die\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nicht mein Herz des Stolzes sich erk\u00fchne,", "tokens": ["Da\u00df", "nicht", "mein", "Herz", "des", "Stol\u00b7zes", "sich", "er\u00b7k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht auf sie vermessen sei.", "tokens": ["Und", "nicht", "auf", "sie", "ver\u00b7mes\u00b7sen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr! lehre mich, wie oft ich fehle, merken.", "tokens": ["Herr", "!", "leh\u00b7re", "mich", ",", "wie", "oft", "ich", "feh\u00b7le", ",", "mer\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was ist der Mensch bei seinen besten Werken?", "tokens": ["Was", "ist", "der", "Mensch", "bei", "sei\u00b7nen", "bes\u00b7ten", "Wer\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sind sie von Gebrechen frei?", "tokens": ["Wenn", "sind", "sie", "von", "Ge\u00b7bre\u00b7chen", "frei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie oft fehlt mir zum Guten selbst der Wille!", "tokens": ["Wie", "oft", "fehlt", "mir", "zum", "Gu\u00b7ten", "selbst", "der", "Wil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wie oft, wenn ich auch dein Gebot erf\u00fclle,", "tokens": ["Wie", "oft", ",", "wenn", "ich", "auch", "dein", "Ge\u00b7bot", "er\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erf\u00fcll ich's minder, als ich soll!", "tokens": ["Er\u00b7f\u00fcll", "ich's", "min\u00b7der", ",", "als", "ich", "soll", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Lieb und Furcht stets die Bewegungsgr\u00fcnde", "tokens": ["Sind", "Lieb", "und", "Furcht", "stets", "die", "Be\u00b7we\u00b7gungs\u00b7gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der guten Tat, der unterla\u00dfnen S\u00fcnde?", "tokens": ["Der", "gu\u00b7ten", "Tat", ",", "der", "un\u00b7ter\u00b7la\u00df\u00b7nen", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ist mein Herz des Eifers voll?", "tokens": ["Und", "ist", "mein", "Herz", "des", "Ei\u00b7fers", "voll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gedenke nicht der S\u00fcnden meiner Jugend,", "tokens": ["Ge\u00b7den\u00b7ke", "nicht", "der", "S\u00fcn\u00b7den", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedenke nicht der unvollkommnen Tugend", "tokens": ["Ge\u00b7den\u00b7ke", "nicht", "der", "un\u00b7voll\u00b7komm\u00b7nen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der reifern Jahre meiner Zeit.", "tokens": ["Der", "rei\u00b7fern", "Jah\u00b7re", "mei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich noch oft aus Stolz nach Tugend strebe,", "tokens": ["Wenn", "ich", "noch", "oft", "aus", "Stolz", "nach", "Tu\u00b7gend", "stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aus Menschenfurcht mich Lastern nicht ergebe;", "tokens": ["Aus", "Men\u00b7schen\u00b7furcht", "mich", "Las\u00b7tern", "nicht", "er\u00b7ge\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ist denn meine Fr\u00f6mmigkeit?", "tokens": ["Was", "ist", "denn", "mei\u00b7ne", "Fr\u00f6m\u00b7mig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ich den Geiz aus Furcht der Schande fliehe,", "tokens": ["Wenn", "ich", "den", "Geiz", "aus", "Furcht", "der", "Schan\u00b7de", "flie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Weichlichkeit mich wohlzutun bem\u00fche,", "tokens": ["Aus", "Weich\u00b7lich\u00b7keit", "mich", "wohl\u00b7zu\u00b7tun", "be\u00b7m\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00e4\u00dfig bin, gesund zu sein;", "tokens": ["Und", "m\u00e4\u00b7\u00dfig", "bin", ",", "ge\u00b7sund", "zu", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich die Rach aus Eigennutze hasse,", "tokens": ["Wenn", "ich", "die", "Rach", "aus", "Ei\u00b7gen\u00b7nut\u00b7ze", "has\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Ehrsucht Pfad aus Tr\u00e4gheit nur verlasse;", "tokens": ["Der", "Ehr\u00b7sucht", "Pfad", "aus", "Tr\u00e4g\u00b7heit", "nur", "ver\u00b7las\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ist an dieser Tugend mein?", "tokens": ["Was", "ist", "an", "die\u00b7ser", "Tu\u00b7gend", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Gott, wie oft sind unsre besten Triebe", "tokens": ["Und", "Gott", ",", "wie", "oft", "sind", "uns\u00b7re", "bes\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Fr\u00f6mmigkeit, nicht Fr\u00fcchte deiner Liebe,", "tokens": ["Nicht", "Fr\u00f6m\u00b7mig\u00b7keit", ",", "nicht", "Fr\u00fcch\u00b7te", "dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Fr\u00fcchte der Natur und Zeit!", "tokens": ["Nur", "Fr\u00fcch\u00b7te", "der", "Na\u00b7tur", "und", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn f\u00fchlen wir der Tugend ganze W\u00fcrde?", "tokens": ["Wenn", "f\u00fch\u00b7len", "wir", "der", "Tu\u00b7gend", "gan\u00b7ze", "W\u00fcr\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ist dein Joch uns eine leichte B\u00fcrde,", "tokens": ["Wenn", "ist", "dein", "Joch", "uns", "ei\u00b7ne", "leich\u00b7te", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dein Gebot Zufriedenheit?", "tokens": ["Und", "dein", "Ge\u00b7bot", "Zu\u00b7frie\u00b7den\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch, Herr, mein Gott! wenn auch zu deiner Ehre", "tokens": ["Doch", ",", "Herr", ",", "mein", "Gott", "!", "wenn", "auch", "zu", "dei\u00b7ner", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herze rein, rein meine Tugend w\u00e4re;", "tokens": ["Mein", "Her\u00b7ze", "rein", ",", "rein", "mei\u00b7ne", "Tu\u00b7gend", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJD", "$,", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wes ist denn dieses Eigentum?", "tokens": ["Wes", "ist", "denn", "die\u00b7ses", "Ei\u00b7gen\u00b7tum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer lie\u00df mich fr\u00fch zur Tugend unterrichten,", "tokens": ["Wer", "lie\u00df", "mich", "fr\u00fch", "zur", "Tu\u00b7gend", "un\u00b7ter\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Gl\u00fcck mich sehn in meines Lebens Pflichten,", "tokens": ["Mein", "Gl\u00fcck", "mich", "sehn", "in", "mei\u00b7nes", "Le\u00b7bens", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und im Gehorsam meinen Ruhm?", "tokens": ["Und", "im", "Ge\u00b7hor\u00b7sam", "mei\u00b7nen", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer gab mir Mut, Herr, dein Gebot zu lieben?", "tokens": ["Wer", "gab", "mir", "Mut", ",", "Herr", ",", "dein", "Ge\u00b7bot", "zu", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer gab mir Kraft, es freudig auszu\u00fcben,", "tokens": ["Wer", "gab", "mir", "Kraft", ",", "es", "freu\u00b7dig", "aus\u00b7zu\u00b7\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in Versuchung Schild und Sieg?", "tokens": ["Und", "in", "Ver\u00b7su\u00b7chung", "Schild", "und", "Sieg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wes ist der Quell, der mich mit Weisheit tr\u00e4nkte?", "tokens": ["Wes", "ist", "der", "Quell", ",", "der", "mich", "mit", "Weis\u00b7heit", "tr\u00e4nk\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Und wes der Freund, der mich zum Guten lenkte,", "tokens": ["Und", "wes", "der", "Freund", ",", "der", "mich", "zum", "Gu\u00b7ten", "lenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mir den Fehler nicht verschwieg?", "tokens": ["Und", "mir", "den", "Feh\u00b7ler", "nicht", "ver\u00b7schwieg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du triebst mich an, da\u00df ich das Gute w\u00e4hlte,", "tokens": ["Du", "triebst", "mich", "an", ",", "da\u00df", "ich", "das", "Gu\u00b7te", "w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und riefst mich oft, wenn ich des Wegs verfehlte,", "tokens": ["Und", "riefst", "mich", "oft", ",", "wenn", "ich", "des", "Wegs", "ver\u00b7fehl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Stimmen deines Geists zur\u00fcck;", "tokens": ["Durch", "Stim\u00b7men", "dei\u00b7nes", "Geists", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zogst mich durch Kreuz, durch Wohltat auch, von S\u00fcnden,", "tokens": ["Zogst", "mich", "durch", "Kreuz", ",", "durch", "Wohl\u00b7tat", "auch", ",", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "APPR", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Lie\u00dft, wenn ich rief, mich wieder Gnade finden,", "tokens": ["Lie\u00dft", ",", "wenn", "ich", "rief", ",", "mich", "wie\u00b7der", "Gna\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "ADV", "NN", "VVINF", 