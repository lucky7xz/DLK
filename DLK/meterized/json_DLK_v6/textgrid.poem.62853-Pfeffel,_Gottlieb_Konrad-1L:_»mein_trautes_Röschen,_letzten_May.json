{"textgrid.poem.62853": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbmein trautes R\u00f6schen, letzten May", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein trautes R\u00f6schen, letzten May", "tokens": ["\u00bb", "mein", "trau\u00b7tes", "R\u00f6\u00b7schen", ",", "letz\u00b7ten", "May"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschied Graf Woldemar;", "tokens": ["Ver\u00b7schied", "Graf", "Wol\u00b7de\u00b7mar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schenkte mir f\u00fcr meine Treu", "tokens": ["Er", "schenk\u00b7te", "mir", "f\u00fcr", "mei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zweyhundert Gulden baar.\u00ab", "tokens": ["Zwey\u00b7hun\u00b7dert", "Gul\u00b7den", "baar", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbnun, Liebchen, hab ich f\u00fcr uns Brod;", "tokens": ["\u00bb", "nun", ",", "Lieb\u00b7chen", ",", "hab", "ich", "f\u00fcr", "uns", "Brod", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum komm, o komm doch bald.", "tokens": ["Drum", "komm", ",", "o", "komm", "doch", "bald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leb wohl! ich bin bis in den Tod", "tokens": ["Leb", "wohl", "!", "ich", "bin", "bis", "in", "den", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein treuer Theobald.\u00ab", "tokens": ["Dein", "treu\u00b7er", "Theo\u00b7bald", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Als R\u00f6schen diesen Brief bekam", "tokens": ["Als", "R\u00f6\u00b7schen", "die\u00b7sen", "Brief", "be\u00b7kam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Mons in Hennegau,", "tokens": ["Zu", "Mons", "in", "Hen\u00b7ne\u00b7gau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch selben Tag sie Abschied nahm", "tokens": ["Noch", "sel\u00b7ben", "Tag", "sie", "Ab\u00b7schied", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrer gn\u00e4dgen Frau.", "tokens": ["Von", "ih\u00b7rer", "gn\u00e4d\u00b7gen", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie zog nach dem Ardennerwald", "tokens": ["Sie", "zog", "nach", "dem", "Ar\u00b7den\u00b7ner\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Zur Gr\u00e4fin Adelgund,", "tokens": ["Zur", "Gr\u00e4\u00b7fin", "A\u00b7del\u00b7gund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey der ihr lieber Theobald", "tokens": ["Bey", "der", "ihr", "lie\u00b7ber", "Theo\u00b7bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Noch itzt als J\u00e4ger stund.", "tokens": ["Noch", "itzt", "als", "J\u00e4\u00b7ger", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Frisch wallt das M\u00e4gdlein seine Bahn", "tokens": ["Frisch", "wallt", "das", "M\u00e4gd\u00b7lein", "sei\u00b7ne", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und langt am sechsten Tag", "tokens": ["Und", "langt", "am", "sechs\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sp\u00e4t auf der Gr\u00e4fin Herrschaft an,", "tokens": ["Sp\u00e4t", "auf", "der", "Gr\u00e4\u00b7fin", "Herr\u00b7schaft", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die tief im Walde lag.", "tokens": ["Die", "tief", "im", "Wal\u00b7de", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch eine Meile; doch der Flor", "tokens": ["Noch", "ei\u00b7ne", "Mei\u00b7le", ";", "doch", "der", "Flor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nacht umh\u00fcllt das Land.", "tokens": ["Der", "Nacht", "um\u00b7h\u00fcllt", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Sumpf und B\u00fcsche drang sie vor,", "tokens": ["Durch", "Sumpf", "und", "B\u00fc\u00b7sche", "drang", "sie", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sumpf und Busch verschwand.", "tokens": ["Und", "Sumpf", "und", "Busch", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die V\u00f6gel schweigen, nur der Ost", "tokens": ["Die", "V\u00f6\u00b7gel", "schwei\u00b7gen", ",", "nur", "der", "Ost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch alte Buchen schwirrt,", "tokens": ["Durch", "al\u00b7te", "Bu\u00b7chen", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf deren einer ohne Trost", "tokens": ["Auf", "de\u00b7ren", "ei\u00b7ner", "oh\u00b7ne", "Trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "PIS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Turtelt\u00e4ubchen girrt.", "tokens": ["Ein", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "girrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In stille Schwermuth aufgel\u00f6st", "tokens": ["In", "stil\u00b7le", "Schwer\u00b7muth", "auf\u00b7ge\u00b7l\u00f6st"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Horcht R\u00f6schen, bis ihr Fu\u00df", "tokens": ["Horcht", "R\u00f6\u00b7schen", ",", "bis", "ihr", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An einen Erdenh\u00fcgel st\u00f6\u00dft,", "tokens": ["An", "ei\u00b7nen", "Er\u00b7den\u00b7h\u00fc\u00b7gel", "st\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den sie fallen mu\u00df.", "tokens": ["Auf", "den", "sie", "fal\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gott, ruft sie, soll ich sterben hier", "tokens": ["Gott", ",", "ruft", "sie", ",", "soll", "ich", "ster\u00b7ben", "hier"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem wilden Wald?", "tokens": ["In", "ei\u00b7nem", "wil\u00b7den", "Wald", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich Arme! w\u00e4rst du doch bey mir,", "tokens": ["Ich", "Ar\u00b7me", "!", "w\u00e4rst", "du", "doch", "bey", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebter Theobald!", "tokens": ["Ge\u00b7lieb\u00b7ter", "Theo\u00b7bald", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Es blitzt; der Erdenh\u00fcgel bebt;", "tokens": ["Es", "blitzt", ";", "der", "Er\u00b7den\u00b7h\u00fc\u00b7gel", "bebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es steigt ein Geist empor;", "tokens": ["Es", "steigt", "ein", "Geist", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Kleid, an welchem Blut noch klebt,", "tokens": ["Sein", "Kleid", ",", "an", "wel\u00b7chem", "Blut", "noch", "klebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PWAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist wei\u00df, wie Silbermoor.", "tokens": ["Ist", "wei\u00df", ",", "wie", "Sil\u00b7ber\u00b7moor", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da bin ich, sprach ein dumpfer Laut;", "tokens": ["Da", "bin", "ich", ",", "sprach", "ein", "dum\u00b7pfer", "Laut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wilddieb gab mir hier", "tokens": ["Ein", "Wild\u00b7dieb", "gab", "mir", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Tod; doch freu dich s\u00fc\u00dfe Braut,", "tokens": ["Den", "Tod", ";", "doch", "freu", "dich", "s\u00fc\u00b7\u00dfe", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "ADJD", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In kurzem folgst du mir.", "tokens": ["In", "kur\u00b7zem", "folgst", "du", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er l\u00e4chelt. Ha, mein Theobald!", "tokens": ["Er", "l\u00e4\u00b7chelt", ".", "Ha", ",", "mein", "Theo\u00b7bald", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ruft sie mit wildem Harm,", "tokens": ["Ruft", "sie", "mit", "wil\u00b7dem", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00fcrzt der luftigen Gestalt", "tokens": ["Und", "st\u00fcrzt", "der", "luf\u00b7ti\u00b7gen", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Todt in den kalten Arm.", "tokens": ["Todt", "in", "den", "kal\u00b7ten", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Nun sieht man Hand in Hand das Paar", "tokens": ["Nun", "sieht", "man", "Hand", "in", "Hand", "das", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Nacht den Hayn durchziehn,", "tokens": ["Zu", "Nacht", "den", "Hayn", "durch\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf dem Grabe jedes Jahr", "tokens": ["Und", "auf", "dem", "Gra\u00b7be", "je\u00b7des", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zwo weisse Rosen bl\u00fchn.", "tokens": ["Zwo", "weis\u00b7se", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}