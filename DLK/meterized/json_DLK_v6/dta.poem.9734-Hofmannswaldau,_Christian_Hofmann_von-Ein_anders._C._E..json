{"dta.poem.9734": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Venus ohngefehr auff die gedancken kam/", "tokens": ["Als", "Ve\u00b7nus", "ohn\u00b7ge\u00b7fehr", "auff", "die", "ge\u00b7dan\u00b7cken", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach ihrem tag ", "tokens": ["Nach", "ih\u00b7rem", "tag"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wei\u00df ich nicht/ wie es sich zu deinem vortheil schickte/", "tokens": ["Wei\u00df", "ich", "nicht", "/", "wie", "es", "sich", "zu", "dei\u00b7nem", "vor\u00b7theil", "schick\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie gantz unverhofft dein nahmens-fest erblickte.", "tokens": ["Da\u00df", "sie", "gantz", "un\u00b7ver\u00b7hofft", "dein", "nah\u00b7mens\u00b7fest", "er\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Berintho/ sprach sie/ der mir so viel ehr erwei\u00dft/", "tokens": ["Be\u00b7rin\u00b7tho", "/", "sprach", "sie", "/", "der", "mir", "so", "viel", "ehr", "er\u00b7wei\u00dft", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "$(", "PRELS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der mich und meinen sohn nach h\u00f6chsten w\u00fcrden prei\u00dft/", "tokens": ["Der", "mich", "und", "mei\u00b7nen", "sohn", "nach", "h\u00f6chs\u00b7ten", "w\u00fcr\u00b7den", "prei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN", "APPR", "ADJA", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den ich/ dieweil er lebt/ getren und werth erfunden/", "tokens": ["Den", "ich", "/", "die\u00b7weil", "er", "lebt", "/", "ge\u00b7tren", "und", "werth", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$(", "VVPP", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie? solte dieser nicht von mir seyn angebunden?", "tokens": ["Wie", "?", "sol\u00b7te", "die\u00b7ser", "nicht", "von", "mir", "seyn", "an\u00b7ge\u00b7bun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PDS", "PTKNEG", "APPR", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hierauff verkn\u00fcpffte sie hertz/ seele/ muth und hand/", "tokens": ["Hier\u00b7auff", "ver\u00b7kn\u00fcpff\u00b7te", "sie", "hertz", "/", "see\u00b7le", "/", "muth", "und", "hand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$(", "VVFIN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und deine freyheit selbst mit einem perlen-band. ", "tokens": ["Und", "dei\u00b7ne", "frey\u00b7heit", "selbst", "mit", "ei\u00b7nem", "per\u00b7len\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie artig/ rieff sie/ wird es dem Berintho stehen/", "tokens": ["Wie", "ar\u00b7tig", "/", "rieff", "sie", "/", "wird", "es", "dem", "Be\u00b7rin\u00b7tho", "ste\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$(", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn er ins k\u00fcnfftige wird so gebunden gehen.", "tokens": ["Wenn", "er", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "wird", "so", "ge\u00b7bun\u00b7den", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}