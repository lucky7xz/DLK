{"textgrid.poem.42812": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur an der Gurgel soll man Schurken fassen.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur an der Gurgel soll man Schurken fassen.", "tokens": ["Nur", "an", "der", "Gur\u00b7gel", "soll", "man", "Schur\u00b7ken", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man soll Getier einander schlucken lassen.", "tokens": ["Man", "soll", "Ge\u00b7tier", "ein\u00b7an\u00b7der", "schlu\u00b7cken", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man soll \u2013 was wei\u00df ich, was man soll!", "tokens": ["Man", "soll", "\u2013", "was", "wei\u00df", "ich", ",", "was", "man", "soll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$(", "PWS", "VVFIN", "PPER", "$,", "PRELS", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wird ein Seepferd je ein Heupferd hassen?", "tokens": ["Doch", "wird", "ein", "See\u00b7pferd", "je", "ein", "Heu\u00b7pferd", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich pfeife auf den Gott Apoll.", "tokens": ["Ich", "pfei\u00b7fe", "auf", "den", "Gott", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}