{"textgrid.poem.41853": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Im October 1813", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vaterland, du prangst mit heil'gen Siegen", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "du", "prangst", "mit", "heil'\u00b7gen", "Sie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wandelst k\u00fchn des Ruhmes ew'gen Pfad;", "tokens": ["Und", "wan\u00b7delst", "k\u00fchn", "des", "Ruh\u00b7mes", "ew'\u00b7gen", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf steiler Bahn bist du emporgestiegen,", "tokens": ["Auf", "stei\u00b7ler", "Bahn", "bist", "du", "em\u00b7por\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Freiheit keimt und Fried' aus blut'ger Saat;", "tokens": ["Und", "Frei\u00b7heit", "keimt", "und", "Fried'", "aus", "blut'\u00b7ger", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch sch\u00fcchtern hat der S\u00e4nger dir geschwiegen,", "tokens": ["Doch", "sch\u00fcch\u00b7tern", "hat", "der", "S\u00e4n\u00b7ger", "dir", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und zagend wich das Wort der gr\u00f6\u00dfern That.", "tokens": ["Und", "za\u00b7gend", "wich", "das", "Wort", "der", "gr\u00f6\u00b7\u00dfern", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mag Schwachheit auch auf stolzen Wahn vertrauen;", "tokens": ["Mag", "Schwach\u00b7heit", "auch", "auf", "stol\u00b7zen", "Wahn", "ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Adler nur darf auf zur Sonne schauen.", "tokens": ["Der", "Ad\u00b7ler", "nur", "darf", "auf", "zur", "Son\u00b7ne", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch jetzt ist mir ein starker Muth entglommen,", "tokens": ["Doch", "jetzt", "ist", "mir", "ein", "star\u00b7ker", "Muth", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ernst ermahnt mich eine theure Pflicht;", "tokens": ["Und", "ernst", "er\u00b7mahnt", "mich", "ei\u00b7ne", "theu\u00b7re", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Himmelsh\u00f6hn ist mir die Kraft gekommen,", "tokens": ["Von", "Him\u00b7mels\u00b7h\u00f6hn", "ist", "mir", "die", "Kraft", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Gluth der Brust, dem Geiste klares Licht.", "tokens": ["Und", "Gluth", "der", "Brust", ",", "dem", "Geis\u00b7te", "kla\u00b7res", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Engelslippen hab' ich ihn vernommen,", "tokens": ["Von", "En\u00b7gels\u00b7lip\u00b7pen", "hab'", "ich", "ihn", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den heil'gen Ruf, drum zag' ich f\u00fcrder nicht.", "tokens": ["Den", "heil'\u00b7gen", "Ruf", ",", "drum", "zag'", "ich", "f\u00fcr\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wen Lieb' und Gott zur Bahn des Kampfes leiten,", "tokens": ["Wen", "Lieb'", "und", "Gott", "zur", "Bahn", "des", "Kamp\u00b7fes", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der zweifle nicht; er wird den Sieg erstreiten.", "tokens": ["Der", "zweif\u00b7le", "nicht", ";", "er", "wird", "den", "Sieg", "er\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn Sie, die still, als noch die Schand' uns dr\u00fcckte,", "tokens": ["Denn", "Sie", ",", "die", "still", ",", "als", "noch", "die", "Schand'", "uns", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein deutsches Herz im freyen Busen trug,", "tokens": ["Ein", "deut\u00b7sches", "Herz", "im", "frey\u00b7en", "Bu\u00b7sen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die stolz hinab auf fremden Schimmer blickte,", "tokens": ["Die", "stolz", "hin\u00b7ab", "auf", "frem\u00b7den", "Schim\u00b7mer", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit strengem Spott den Sklaven niederschlug,", "tokens": ["Mit", "stren\u00b7gem", "Spott", "den", "Skla\u00b7ven", "nie\u00b7der\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die fromm und zart die rauhe Welt uns schm\u00fcckte,", "tokens": ["Die", "fromm", "und", "zart", "die", "rau\u00b7he", "Welt", "uns", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein segnend Licht in finstrer Zeiten Fluch,", "tokens": ["Ein", "seg\u00b7nend", "Licht", "in", "finst\u00b7rer", "Zei\u00b7ten", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Gott schon fr\u00fch zu seinem Thron erhoben,", "tokens": ["Die", "Gott", "schon", "fr\u00fch", "zu", "sei\u00b7nem", "Thron", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um herrlicher sein sch\u00f6nstes Werk zu loben;", "tokens": ["Um", "herr\u00b7li\u00b7cher", "sein", "sch\u00f6ns\u00b7tes", "Werk", "zu", "lo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie nahte mir von ihren lichten H\u00f6hen,", "tokens": ["Sie", "nah\u00b7te", "mir", "von", "ih\u00b7ren", "lich\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Spiel des Traums ein ernstes Heil'genbild;", "tokens": ["Im", "Spiel", "des", "Traums", "ein", "erns\u00b7tes", "Heil'\u00b7gen\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Auge war wie Fr\u00fchlicht anzusehen,", "tokens": ["Ihr", "Au\u00b7ge", "war", "wie", "Fr\u00fch\u00b7licht", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Morgenroth die helle Wang' umh\u00fcllt;", "tokens": ["Von", "Mor\u00b7gen\u00b7roth", "die", "hel\u00b7le", "Wang'", "um\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um ihren Kranz entflo\u00df ein g\u00f6ttlich Wehen,", "tokens": ["Um", "ih\u00b7ren", "Kranz", "ent\u00b7flo\u00df", "ein", "g\u00f6tt\u00b7lich", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie durch den Thau der Bl\u00fcthe Duft entquillt,", "tokens": ["Wie", "durch", "den", "Thau", "der", "Bl\u00fc\u00b7the", "Duft", "ent\u00b7quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und gleich dem Klang verkl\u00e4rter Harfenlieder", "tokens": ["Und", "gleich", "dem", "Klang", "ver\u00b7kl\u00e4r\u00b7ter", "Har\u00b7fen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kam so ihr Wort zu meinem Geist hernieder:", "tokens": ["Kam", "so", "ihr", "Wort", "zu", "mei\u00b7nem", "Geist", "her\u00b7nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was feyerst du und schweigst in d\u00fcstern Klagen,", "tokens": ["Was", "fe\u00b7yerst", "du", "und", "schweigst", "in", "d\u00fcs\u00b7tern", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Nachtgew\u00f6lk im hellen Morgenroth,", "tokens": ["Ein", "Nacht\u00b7ge\u00b7w\u00f6lk", "im", "hel\u00b7len", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und weinst, da Gl\u00fcck und Ruhm f\u00fcr Alle tagen,", "tokens": ["Und", "weinst", ",", "da", "Gl\u00fcck", "und", "Ruhm", "f\u00fcr", "Al\u00b7le", "ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit feigem Schmerz um deines Gl\u00fcckes Tod?", "tokens": ["Mit", "fei\u00b7gem", "Schmerz", "um", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer mich geliebt, der mu\u00df das Gro\u00dfe wagen,", "tokens": ["Wer", "mich", "ge\u00b7liebt", ",", "der", "mu\u00df", "das", "Gro\u00b7\u00dfe", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ART", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Ruf der Kraft, er ist auch mein Gebot;", "tokens": ["Der", "Ruf", "der", "Kraft", ",", "er", "ist", "auch", "mein", "Ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was ich empfand, das sollst auch du empfinden,", "tokens": ["Was", "ich", "emp\u00b7fand", ",", "das", "sollst", "auch", "du", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und meinen Werth durch deinen Werth verk\u00fcnden.", "tokens": ["Und", "mei\u00b7nen", "Werth", "durch", "dei\u00b7nen", "Werth", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hab' ich nicht oft mit stillgeweinten Thr\u00e4nen", "tokens": ["Hab'", "ich", "nicht", "oft", "mit", "still\u00b7ge\u00b7wein\u00b7ten", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In stummem Gram mich um mein Volk verzehrt,", "tokens": ["In", "stum\u00b7mem", "Gram", "mich", "um", "mein", "Volk", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht oft von Gott mit hei\u00dfem Flehn und Sehnen", "tokens": ["Nicht", "oft", "von", "Gott", "mit", "hei\u00b7\u00dfem", "Flehn", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Frevels Sturz, der Freyheit Sieg begehrt?", "tokens": ["Des", "Fre\u00b7vels", "Sturz", ",", "der", "Frey\u00b7heit", "Sieg", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hab' ich den Kranz des Guten und des Sch\u00f6nen", "tokens": ["Hab'", "ich", "den", "Kranz", "des", "Gu\u00b7ten", "und", "des", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht hoffnungsvoll in finstrer Zeit gen\u00e4hrt?", "tokens": ["Nicht", "hoff\u00b7nungs\u00b7voll", "in", "finst\u00b7rer", "Zeit", "ge\u00b7n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ADJA", "NN", 