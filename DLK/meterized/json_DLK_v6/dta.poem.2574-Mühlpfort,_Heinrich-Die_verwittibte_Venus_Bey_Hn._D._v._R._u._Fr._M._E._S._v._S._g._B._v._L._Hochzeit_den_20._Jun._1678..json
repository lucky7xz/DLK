{"dta.poem.2574": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die verwittibte Venus  \n  Bey Hn. D. v. R. u. Fr. M. E. S. v. S. g.  \n B. v. L. Hochzeit den 20. Jun. 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein \u00f6d und w\u00fcster Ort/ wo nie kein Stern erscheint/", "tokens": ["Ein", "\u00f6d", "und", "w\u00fcs\u00b7ter", "Ort", "/", "wo", "nie", "kein", "Stern", "er\u00b7scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$(", "PWAV", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den nicht das Morgen-Roth mit seinem Purpur zieret/", "tokens": ["Den", "nicht", "das", "Mor\u00b7gen\u00b7Roth", "mit", "sei\u00b7nem", "Pur\u00b7pur", "zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umb den nie einen Tantz die g\u00f6ldne Sonne f\u00fchret/", "tokens": ["Umb", "den", "nie", "ei\u00b7nen", "Tantz", "die", "g\u00f6ld\u00b7ne", "Son\u00b7ne", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wo nichts als Schatten sind und stets der Himmel weint/", "tokens": ["Wo", "nichts", "als", "Schat\u00b7ten", "sind", "und", "stets", "der", "Him\u00b7mel", "weint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "NN", "VAFIN", "KON", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die L\u00fcffte seufftzen nach/ die B\u00e4che giessen Z\u00e4hren/", "tokens": ["Die", "L\u00fcff\u00b7te", "seufft\u00b7zen", "nach", "/", "die", "B\u00e4\u00b7che", "gies\u00b7sen", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und lauter Weh\u2019 und Ach die Echo wil gewehren.", "tokens": ["Und", "lau\u00b7ter", "Weh'", "und", "Ach", "die", "E\u00b7cho", "wil", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der war zum Auffenthalt der K\u00f6nigin bestimmt/", "tokens": ["Der", "war", "zum", "Auf\u00b7fent\u00b7halt", "der", "K\u00f6\u00b7ni\u00b7gin", "be\u00b7stimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So die gevierdte Welt mit ihrem Scepter dr\u00fccket/", "tokens": ["So", "die", "ge\u00b7vierd\u00b7te", "Welt", "mit", "ih\u00b7rem", "Scep\u00b7ter", "dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und in der Menschen Brust die Liebes-Flammen schicket/", "tokens": ["Und", "in", "der", "Men\u00b7schen", "Brust", "die", "Lie\u00b7bes\u00b7Flam\u00b7men", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sonst das s\u00fcsse Wort der Mutter an sich nimmt/", "tokens": ["Die", "sonst", "das", "s\u00fcs\u00b7se", "Wort", "der", "Mut\u00b7ter", "an", "sich", "nimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Lust und Wonne schenckt und voller Anmuth lachet/", "tokens": ["Und", "Lust", "und", "Won\u00b7ne", "schenckt", "und", "vol\u00b7ler", "An\u00b7muth", "la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die hatte sich alldar zum Trauer-Bild gemachet.", "tokens": ["Die", "hat\u00b7te", "sich", "all\u00b7dar", "zum", "Trau\u00b7e\u00b7rBild", "ge\u00b7ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Die gantze Hofestadt bestand in Einsamkeit/", "tokens": ["Die", "gant\u00b7ze", "Ho\u00b7fe\u00b7stadt", "be\u00b7stand", "in", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ein bemoster Stein must\u2019 ihr zum Stuhle dienen/", "tokens": ["Und", "ein", "be\u00b7mos\u00b7ter", "Stein", "must'", "ihr", "zum", "Stuh\u00b7le", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Leib/ der Sternen gleich in Perl und Gold geschienen/", "tokens": ["Der", "Leib", "/", "der", "Ster\u00b7nen", "gleich", "in", "Perl", "und", "Gold", "ge\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADV", "APPR", "NE", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "War ohne Pracht und Glantz. Kein Opffer stand bereit/", "tokens": ["War", "oh\u00b7ne", "Pracht", "und", "Glantz", ".", "Kein", "Opf\u00b7fer", "stand", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "$.", "PIAT", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Weyrauch angez\u00fcndt die G\u00f6ttin zu vers\u00f6hnen/", "tokens": ["Kein", "Wey\u00b7rauch", "an\u00b7ge\u00b7z\u00fcndt", "die", "G\u00f6t\u00b7tin", "zu", "ver\u00b7s\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es kam kein eintzig Mensch ihr Altar zu bekr\u00f6nen.", "tokens": ["Es", "kam", "kein", "eint\u00b7zig", "Mensch", "ihr", "Al\u00b7tar", "zu", "be\u00b7kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Man sah die Schwanen nicht in frischen Rosen gehn/", "tokens": ["Man", "sah", "die", "Schwa\u00b7nen", "nicht", "in", "fri\u00b7schen", "Ro\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es schwieg der liebe Mund der schwesterlichen Tauben/", "tokens": ["Es", "schwieg", "der", "lie\u00b7be", "Mund", "der", "schwes\u00b7ter\u00b7li\u00b7chen", "Tau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Die F\u00fcrstin selber sa\u00df verkappt in Flor und Hauben;", "tokens": ["Die", "F\u00fcrs\u00b7tin", "sel\u00b7ber", "sa\u00df", "ver\u00b7kappt", "in", "Flor", "und", "Hau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Kein Liebes-Engel wolt\u2019 ihr da zu Dienste stehn/", "tokens": ["Kein", "Lie\u00b7bes\u00b7En\u00b7gel", "wolt'", "ihr", "da", "zu", "Diens\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die K\u00f6cher hingen leer ohn ein\u2019ge Pflitz und Pfeile/", "tokens": ["Die", "K\u00f6\u00b7cher", "hin\u00b7gen", "leer", "ohn", "ein'\u00b7ge", "Pflitz", "und", "Pfei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Jhr Bogen und Gescho\u00df ward andern nur zu Theile.", "tokens": ["Ihr", "Bo\u00b7gen", "und", "Ge\u00b7scho\u00df", "ward", "an\u00b7dern", "nur", "zu", "Thei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jhr Mund/ der vorhin nichts als Amber von sich blie\u00df/", "tokens": ["Ihr", "Mund", "/", "der", "vor\u00b7hin", "nichts", "als", "Am\u00b7ber", "von", "sich", "blie\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADV", "PIS", "KOKOM", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Rieff mit gebrochner Stimm: Verwittibte Dione", "tokens": ["Rieff", "mit", "ge\u00b7broch\u00b7ner", "Stimm", ":", "Ver\u00b7wit\u00b7tib\u00b7te", "Dio\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$.", "VVFIN", "NE"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Wo ist dein m\u00e4chtig Arm? Wo ist dein Reich und Krone?", "tokens": ["Wo", "ist", "dein", "m\u00e4ch\u00b7tig", "Arm", "?", "Wo", "ist", "dein", "Reich", "und", "Kro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJD", "NN", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo der/ umb den ich auch das Schlo\u00df des Himmels lie\u00df/", "tokens": ["Wo", "der", "/", "umb", "den", "ich", "auch", "das", "Schlo\u00df", "des", "Him\u00b7mels", "lie\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$(", "APPR", "ART", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein sch\u00f6nester Adon/ der Au\u00dfzug meiner Seele/", "tokens": ["Mein", "sch\u00f6\u00b7nes\u00b7ter", "A\u00b7don", "/", "der", "Au\u00df\u00b7zug", "mei\u00b7ner", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Schleust meine Hoheit ein die ungeheure H\u00f6le?", "tokens": ["Schleust", "mei\u00b7ne", "Ho\u00b7heit", "ein", "die", "un\u00b7ge\u00b7heu\u00b7re", "H\u00f6\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Recht/ diese W\u00fcsteney hab\u2019 ich mir selbst erwehlt/", "tokens": ["Recht", "/", "die\u00b7se", "W\u00fcs\u00b7te\u00b7ney", "hab'", "ich", "mir", "selbst", "er\u00b7wehlt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDAT", "NN", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Umb mein unendlich Leid und Klagen auszusch\u00fctten/", "tokens": ["Umb", "mein", "un\u00b7end\u00b7lich", "Leid", "und", "Kla\u00b7gen", "aus\u00b7zu\u00b7sch\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJD", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Welt durch ein Gebot das Lieben zu verbitten;", "tokens": ["Der", "Welt", "durch", "ein", "Ge\u00b7bot", "das", "Lie\u00b7ben", "zu", "ver\u00b7bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und ich/ nach dem Adon den Sternen zugezehlt/", "tokens": ["Und", "ich", "/", "nach", "dem", "A\u00b7don", "den", "Ster\u00b7nen", "zu\u00b7ge\u00b7zehlt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "F\u00fcg allem Frauen-Volck als K\u00f6nigin zu wissen/", "tokens": ["F\u00fcg", "al\u00b7lem", "Frau\u00b7en\u00b7Volck", "als", "K\u00f6\u00b7ni\u00b7gin", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "NN", "KOUS", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn sie verwittibt sind/ die Werckstadt zuzuschliessen.", "tokens": ["Wenn", "sie", "ver\u00b7wit\u00b7tibt", "sind", "/", "die", "Werck\u00b7stadt", "zu\u00b7zu\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$(", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hier sollen sie den Rest vollbringen ihrer Zeit/", "tokens": ["Hier", "sol\u00b7len", "sie", "den", "Rest", "voll\u00b7brin\u00b7gen", "ih\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Orden den ich ietzt mit ihnen angefangen", "tokens": ["Der", "Or\u00b7den", "den", "ich", "ietzt", "mit", "ih\u00b7nen", "an\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Setzt diesen Denckspruch bey: ", "tokens": ["Setzt", "die\u00b7sen", "Denck\u00b7spruch", "bey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Und iede welche mir als Priesterin geweiht/", "tokens": ["Und", "ie\u00b7de", "wel\u00b7che", "mir", "als", "Pries\u00b7te\u00b7rin", "ge\u00b7weiht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PRELS", "PPER", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sol sich Vestalen gleich einkerckern und verbauen/", "tokens": ["Sol", "sich", "Ves\u00b7ta\u00b7len", "gleich", "ein\u00b7ker\u00b7ckern", "und", "ver\u00b7bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NN", 