{"textgrid.poem.66679": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Waffenknechtschaft \u2013 Milit\u00e4r gehei\u00dfen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Waffenknechtschaft \u2013 Milit\u00e4r gehei\u00dfen \u2013", "tokens": ["Die", "Waf\u00b7fen\u00b7knecht\u00b7schaft", "\u2013", "Mi\u00b7li\u00b7t\u00e4r", "ge\u00b7hei\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist solch ein andres scheu\u00dfliches Insekt,", "tokens": ["Ist", "solch", "ein", "and\u00b7res", "scheu\u00df\u00b7li\u00b7ches", "In\u00b7sekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Blut saugt und mit niedertr\u00e4chtigem Bei\u00dfen", "tokens": ["Das", "Blut", "saugt", "und", "mit", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7gem", "Bei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Menschen von dem eignen Lager schreckt,", "tokens": ["Die", "Men\u00b7schen", "von", "dem", "eig\u00b7nen", "La\u00b7ger", "schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das sie zu Massen macht zusammenschwei\u00dfen,", "tokens": ["Das", "sie", "zu", "Mas\u00b7sen", "macht", "zu\u00b7sam\u00b7men\u00b7schwei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die lappenbunt die Uniform befleckt.", "tokens": ["Die", "lap\u00b7pen\u00b7bunt", "die", "U\u00b7ni\u00b7form", "be\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr Puppenheim sind riesige Kasernen,", "tokens": ["Ihr", "Pup\u00b7pen\u00b7heim", "sind", "rie\u00b7si\u00b7ge", "Ka\u00b7ser\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Drin l\u00e4\u00dft sich sogenannte Mannszucht lernen.", "tokens": ["Drin", "l\u00e4\u00dft", "sich", "so\u00b7ge\u00b7nann\u00b7te", "Manns\u00b7zucht", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hurra, die Mannszucht! Dieses ist die Spitze", "tokens": ["Hur\u00b7ra", ",", "die", "Manns\u00b7zucht", "!", "Die\u00b7ses", "ist", "die", "Spit\u00b7ze"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Selbsterziehung freigebornen Manns.", "tokens": ["Der", "Selbs\u00b7ter\u00b7zie\u00b7hung", "frei\u00b7ge\u00b7bor\u00b7nen", "Manns", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fragt nur die K\u00f6ckeritz und Itzenplitze", "tokens": ["Fragt", "nur", "die", "K\u00f6\u00b7cke\u00b7ritz", "und", "It\u00b7zen\u00b7plit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und s\u00e4mtliche Reserveleutenants!", "tokens": ["Und", "s\u00e4mt\u00b7li\u00b7che", "Re\u00b7ser\u00b7ve\u00b7leu\u00b7ten\u00b7ants", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sch\u00f6n ist der Helm und blank sei seine Spitze,", "tokens": ["Sch\u00f6n", "ist", "der", "Helm", "und", "blank", "sei", "sei\u00b7ne", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und blind sei dein Gehorsam, braver Hans!", "tokens": ["Und", "blind", "sei", "dein", "Ge\u00b7hor\u00b7sam", ",", "bra\u00b7ver", "Hans", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr deinen Unteroffizier und Kaiser", "tokens": ["F\u00fcr", "dei\u00b7nen", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", "und", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gehst du durchs Feuer, schreist die Kehle heiser.", "tokens": ["Gehst", "du", "durchs", "Feu\u00b7er", ",", "schreist", "die", "Keh\u00b7le", "hei\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die heiligen Instrumente lernst du brauchen,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Inst\u00b7ru\u00b7men\u00b7te", "lernst", "du", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die du zur Ehre Gottes t\u00e4glich putzst,", "tokens": ["Die", "du", "zur", "Eh\u00b7re", "Got\u00b7tes", "t\u00e4g\u00b7lich", "putz\u00b7st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df du sie, den Mitmensch zu enthauchen,", "tokens": ["Auf", "da\u00df", "du", "sie", ",", "den", "Mit\u00b7mensch", "zu", "ent\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Ernstfall ohne jeden Fehl benutzst.", "tokens": ["Im", "Ernst\u00b7fall", "oh\u00b7ne", "je\u00b7den", "Fehl", "be\u00b7nutz\u00b7st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(gottlob, das Pulver sieht man nicht mehr rauchen.)", "tokens": ["(", "gott\u00b7lob", ",", "das", "Pul\u00b7ver", "sieht", "man", "nicht", "mehr", "rau\u00b7chen", ".", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "ART", "NN", "VVFIN", "PIS", "PTKNEG", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du lernst, da\u00df du mit Mord dich nicht beschmutzst,", "tokens": ["Du", "lernst", ",", "da\u00df", "du", "mit", "Mord", "dich", "nicht", "be\u00b7schmutz\u00b7st", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Gegenteil, dir holst das Ehrenzeichen,", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "dir", "holst", "das", "Eh\u00b7ren\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geht es um Massen nur und \u00bbFeindes\u00ableichen.", "tokens": ["Geht", "es", "um", "Mas\u00b7sen", "nur", "und", "\u00bb", "Fein\u00b7des", "\u00ab", "lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "KON", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Namen Gottes drillt man deine Seele", "tokens": ["Im", "Na\u00b7men", "Got\u00b7tes", "drillt", "man", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur S\u00e4ule wahrer Zivilisation,", "tokens": ["Zur", "S\u00e4u\u00b7le", "wah\u00b7rer", "Zi\u00b7vi\u00b7li\u00b7sa\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das hei\u00dft: stumm zu gehorchen dem Befehle", "tokens": ["Das", "hei\u00dft", ":", "stumm", "zu", "ge\u00b7hor\u00b7chen", "dem", "Be\u00b7feh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "ADJD", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aufzugehn in Subordination.", "tokens": ["Und", "auf\u00b7zu\u00b7gehn", "in", "Sub\u00b7or\u00b7di\u00b7na\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Denn du bist Null, da\u00df ich dir nichts verhehle,", "tokens": ["Denn", "du", "bist", "Null", ",", "da\u00df", "ich", "dir", "nichts", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NE", "$,", "KOUS", "PPER", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Eins davor machst du die Million,", "tokens": ["Mit", "Eins", "da\u00b7vor", "machst", "du", "die", "Mil\u00b7li\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und mit so einigen Prachtsmillionen", "tokens": ["Und", "mit", "so", "ei\u00b7ni\u00b7gen", "Prachts\u00b7mil\u00b7li\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kann man selbst Chinas Kaiserin entthronen.", "tokens": ["Kann", "man", "selbst", "Chi\u00b7nas", "Kai\u00b7se\u00b7rin", "ent\u00b7thro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das war ja Christi Wunsch im gro\u00dfen ganzen,", "tokens": ["Das", "war", "ja", "Chris\u00b7ti", "Wunsch", "im", "gro\u00b7\u00dfen", "gan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NE", "NN", "APPRART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Worin er mit Lao-tse sich ber\u00fchrt,", "tokens": ["Wo\u00b7rin", "er", "mit", "Lao\u00b7\u00b7tse", "sich", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Germania m\u00f6ge China recht kuranzen,", "tokens": ["Ger\u00b7ma\u00b7nia", "m\u00f6\u00b7ge", "Chi\u00b7na", "recht", "ku\u00b7ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es die Heilstat der Kultur versp\u00fcrt,", "tokens": ["Da\u00df", "es", "die", "Heil\u00b7stat", "der", "Kul\u00b7tur", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und gottgeweihte Fahnen m\u00f6g' es pflanzen", "tokens": ["Und", "gott\u00b7ge\u00b7weih\u00b7te", "Fah\u00b7nen", "m\u00f6g'", "es", "pflan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf Pekings Zinnen rachegeistgesch\u00fcrt,", "tokens": ["Auf", "Pe\u00b7kings", "Zin\u00b7nen", "ra\u00b7che\u00b7geist\u00b7ge\u00b7sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Spricht doch der Heiland: Liebe, die dich hassen!", "tokens": ["Spricht", "doch", "der", "Hei\u00b7land", ":", "Lie\u00b7be", ",", "die", "dich", "has\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Lao-tse: Sei frei vom Zwang der Massen!", "tokens": ["Und", "Lao\u00b7\u00b7tse", ":", "Sei", "frei", "vom", "Zwang", "der", "Mas\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "VAIMP", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "O sch\u00f6nste Pflicht, die Erde zu begl\u00fccken", "tokens": ["O", "sch\u00f6ns\u00b7te", "Pflicht", ",", "die", "Er\u00b7de", "zu", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit unserer Kasernenhofkultur,", "tokens": ["Mit", "un\u00b7se\u00b7rer", "Ka\u00b7ser\u00b7nen\u00b7hof\u00b7kul\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vor hohen Vorgesetzten sich zu b\u00fccken", "tokens": ["Vor", "ho\u00b7hen", "Vor\u00b7ge\u00b7setz\u00b7ten", "sich", "zu", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und subaltern zu gehn auf \u00bbh\u00f6herer\u00ab Spur.", "tokens": ["Und", "sub\u00b7al\u00b7tern", "zu", "gehn", "auf", "\u00bb", "h\u00f6\u00b7he\u00b7rer", "\u00ab", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "PTKZU", "VVINF", "APPR", "$(", "ADJA", "$(", "NN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nach oben kriechen und nach unten dr\u00fccken,", "tokens": ["Nach", "o\u00b7ben", "krie\u00b7chen", "und", "nach", "un\u00b7ten", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "KON", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur einem Gott sich weihn mit h\u00f6chstem Schwur:", "tokens": ["Nur", "ei\u00b7nem", "Gott", "sich", "weihn", "mit", "h\u00f6chs\u00b7tem", "Schwur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem wehgen\u00e4hrten, blutgetr\u00e4nkten Gotte", "tokens": ["Dem", "weh\u00b7ge\u00b7n\u00e4hr\u00b7ten", ",", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Got\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der mit Hurra fanatisierten Rotte ...", "tokens": ["Der", "mit", "Hur\u00b7ra", "fa\u00b7na\u00b7ti\u00b7sier\u00b7ten", "Rot\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "NN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}