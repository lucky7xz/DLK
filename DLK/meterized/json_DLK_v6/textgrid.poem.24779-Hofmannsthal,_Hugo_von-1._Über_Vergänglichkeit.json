{"textgrid.poem.24779": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1. \u00dcber Verg\u00e4nglichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch sp\u00fcr ich ihren Atem auf den Wangen:", "tokens": ["Noch", "sp\u00fcr", "ich", "ih\u00b7ren", "A\u00b7tem", "auf", "den", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie kann das sein, da\u00df diese nahen Tage", "tokens": ["Wie", "kann", "das", "sein", ",", "da\u00df", "die\u00b7se", "na\u00b7hen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PDS", "VAINF", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fort sind, f\u00fcr immer fort, und ganz vergangen?", "tokens": ["Fort", "sind", ",", "f\u00fcr", "im\u00b7mer", "fort", ",", "und", "ganz", "ver\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPR", "ADV", "PTKVZ", "$,", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dies ist ein Ding, das keiner voll aussinnt,", "tokens": ["Dies", "ist", "ein", "Ding", ",", "das", "kei\u00b7ner", "voll", "aus\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und viel zu grauenvoll, als da\u00df man klage:", "tokens": ["Und", "viel", "zu", "grau\u00b7en\u00b7voll", ",", "als", "da\u00df", "man", "kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "$,", "KOKOM", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df alles gleitet und vor\u00fcberrinnt", "tokens": ["Da\u00df", "al\u00b7les", "glei\u00b7tet", "und", "vor\u00b7\u00fc\u00b7berr\u00b7innt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und da\u00df mein eignes Ich, durch nichts gehemmt,", "tokens": ["Und", "da\u00df", "mein", "eig\u00b7nes", "Ich", ",", "durch", "nichts", "ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "PPER", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Her\u00fcberglitt aus einem kleinen Kind", "tokens": ["Her\u00b7\u00fc\u00b7berg\u00b7litt", "aus", "ei\u00b7nem", "klei\u00b7nen", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir wie ein Hund unheimlich stumm und fremd.", "tokens": ["Mir", "wie", "ein", "Hund", "un\u00b7heim\u00b7lich", "stumm", "und", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann: da\u00df ich auch vor hundert Jahren war", "tokens": ["Dann", ":", "da\u00df", "ich", "auch", "vor", "hun\u00b7dert", "Jah\u00b7ren", "war"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "PPER", "ADV", "APPR", "CARD", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Ahnen, die im Totenhemd,", "tokens": ["Und", "mei\u00b7ne", "Ah\u00b7nen", ",", "die", "im", "To\u00b7ten\u00b7hemd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit mir verwandt sind wie mein eignes Haar,", "tokens": ["Mit", "mir", "ver\u00b7wandt", "sind", "wie", "mein", "eig\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So eins mit mir als wie mein eignes Haar.", "tokens": ["So", "eins", "mit", "mir", "als", "wie", "mein", "eig\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PPER", "KOUS", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}