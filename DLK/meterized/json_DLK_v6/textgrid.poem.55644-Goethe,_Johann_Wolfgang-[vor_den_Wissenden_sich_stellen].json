{"textgrid.poem.55644": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[vor den Wissenden sich stellen]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor den Wissenden sich stellen,", "tokens": ["Vor", "den", "Wis\u00b7sen\u00b7den", "sich", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sicher ist's in allen F\u00e4llen!", "tokens": ["Si\u00b7cher", "ist's", "in", "al\u00b7len", "F\u00e4l\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du lange dich gequ\u00e4let,", "tokens": ["Wenn", "du", "lan\u00b7ge", "dich", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df er gleich, wo dir es fehlet;", "tokens": ["Wei\u00df", "er", "gleich", ",", "wo", "dir", "es", "feh\u00b7let", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch auf Beifall darfst du hoffen,", "tokens": ["Auch", "auf", "Bei\u00b7fall", "darfst", "du", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn er wei\u00df, wo du's getroffen.", "tokens": ["Denn", "er", "wei\u00df", ",", "wo", "du's", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}