{"textgrid.poem.55070": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Musen und Grazien", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie ist die Stadt so wenig;", "tokens": ["O", "wie", "ist", "die", "Stadt", "so", "we\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ADV", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft die Maurer k\u00fcnftig ruhn!", "tokens": ["La\u00dft", "die", "Mau\u00b7rer", "k\u00fcnf\u00b7tig", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre B\u00fcrger, unser K\u00f6nig", "tokens": ["Uns\u00b7re", "B\u00fcr\u00b7ger", ",", "un\u00b7ser", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnten wohl was Bessers tun.", "tokens": ["K\u00f6nn\u00b7ten", "wohl", "was", "Bes\u00b7sers", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PWS", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ball und Oper wird uns t\u00f6ten;", "tokens": ["Ball", "und", "O\u00b7per", "wird", "uns", "t\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebchen, komm auf meine Flur,", "tokens": ["Lieb\u00b7chen", ",", "komm", "auf", "mei\u00b7ne", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn besonders die Poeten,", "tokens": ["Denn", "be\u00b7son\u00b7ders", "die", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die verderben die Natur.", "tokens": ["Die", "ver\u00b7der\u00b7ben", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie freut es mich, mein Liebchen,", "tokens": ["O", "wie", "freut", "es", "mich", ",", "mein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du so nat\u00fcrlich bist;", "tokens": ["Da\u00df", "du", "so", "na\u00b7t\u00fcr\u00b7lich", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre M\u00e4dchen, unsre B\u00fcbchen", "tokens": ["Uns\u00b7re", "M\u00e4d\u00b7chen", ",", "uns\u00b7re", "B\u00fcb\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielen k\u00fcnftig auf dem Mist!", "tokens": ["Spie\u00b7len", "k\u00fcnf\u00b7tig", "auf", "dem", "Mist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auf unsern Promenaden", "tokens": ["Und", "auf", "un\u00b7sern", "Pro\u00b7me\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt sich erst die Neigung stark.", "tokens": ["Zeigt", "sich", "erst", "die", "Nei\u00b7gung", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebes M\u00e4dchen! la\u00df uns waten,", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", "!", "la\u00df", "uns", "wa\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Waten noch durch diesen Quark.", "tokens": ["Wa\u00b7ten", "noch", "durch", "die\u00b7sen", "Quark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann im Sand uns zu verlieren,", "tokens": ["Dann", "im", "Sand", "uns", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der uns keinen Weg versperrt!", "tokens": ["Der", "uns", "kei\u00b7nen", "Weg", "ver\u00b7sperrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich den Anger hin zu f\u00fchren,", "tokens": ["Dich", "den", "An\u00b7ger", "hin", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Dorn das R\u00f6ckchen zerrt!", "tokens": ["Wo", "der", "Dorn", "das", "R\u00f6ck\u00b7chen", "zerrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Zu dem D\u00f6rfchen la\u00df uns schleichen", "tokens": ["Zu", "dem", "D\u00f6rf\u00b7chen", "la\u00df", "uns", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem spitzen Turme hier;", "tokens": ["Mit", "dem", "spit\u00b7zen", "Tur\u00b7me", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welch ein Wirtshaus sondergleichen!", "tokens": ["Welch", "ein", "Wirts\u00b7haus", "son\u00b7derg\u00b7lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trocknes Brot und saures Bier!", "tokens": ["Trock\u00b7nes", "Brot", "und", "sau\u00b7res", "Bier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sagt mir nichts von gutem Boden,", "tokens": ["Sagt", "mir", "nichts", "von", "gu\u00b7tem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts vom Magdeburger Land!", "tokens": ["Nichts", "vom", "Mag\u00b7de\u00b7bur\u00b7ger", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Samen, unsre Toten", "tokens": ["Uns\u00b7re", "Sa\u00b7men", ",", "uns\u00b7re", "To\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhen in dem leichten Sand.", "tokens": ["Ru\u00b7hen", "in", "dem", "leich\u00b7ten", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst die Wissenschaft verlieret", "tokens": ["Selbst", "die", "Wis\u00b7sen\u00b7schaft", "ver\u00b7lie\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts an ihrem raschen Lauf,", "tokens": ["Nichts", "an", "ih\u00b7rem", "ra\u00b7schen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn bei uns, was vegetieret,", "tokens": ["Denn", "bei", "uns", ",", "was", "ve\u00b7ge\u00b7tie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles keimt getrocknet auf.", "tokens": ["Al\u00b7les", "keimt", "ge\u00b7trock\u00b7net", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Geht es nicht in unserm Hofe", "tokens": ["Geht", "es", "nicht", "in", "un\u00b7serm", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Paradiese zu?", "tokens": ["Wie", "im", "Pa\u00b7ra\u00b7die\u00b7se", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Statt der Dame, statt der Zofe", "tokens": ["Statt", "der", "Da\u00b7me", ",", "statt", "der", "Zo\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht die Henne glu! glu! glu!", "tokens": ["Macht", "die", "Hen\u00b7ne", "glu", "!", "glu", "!", "glu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$.", "NE", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns besch\u00e4ftigt nicht der Pfauen,", "tokens": ["Uns", "be\u00b7sch\u00e4f\u00b7tigt", "nicht", "der", "Pfau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur der G\u00e4nse Lebenslauf;", "tokens": ["Nur", "der", "G\u00e4n\u00b7se", "Le\u00b7bens\u00b7lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Mutter zieht die grauen,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "zieht", "die", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Frau die wei\u00dfen auf.", "tokens": ["Mei\u00b7ne", "Frau", "die", "wei\u00b7\u00dfen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df den Witzling uns besticheln!", "tokens": ["La\u00df", "den", "Witz\u00b7ling", "uns", "be\u00b7sti\u00b7cheln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fccklich, wenn ein deutscher Mann", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wenn", "ein", "deut\u00b7scher", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinem Freunde Vetter Micheln", "tokens": ["Sei\u00b7nem", "Freun\u00b7de", "Vet\u00b7ter", "Mi\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Guten Abend bieten kann.", "tokens": ["Gu\u00b7ten", "A\u00b7bend", "bie\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ist der Gedanke labend:", "tokens": ["Wie", "ist", "der", "Ge\u00b7dan\u00b7ke", "la\u00b7bend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Solch ein Edler bleibt uns nah!", "tokens": ["Solch", "ein", "Ed\u00b7ler", "bleibt", "uns", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer sagt man: Gestern abend", "tokens": ["Im\u00b7mer", "sagt", "man", ":", "Ge\u00b7stern", "a\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$.", "NN", "VVPP"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "War doch Vetter Michel da!", "tokens": ["War", "doch", "Vet\u00b7ter", "Mi\u00b7chel", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und in unsern Liedern keimet", "tokens": ["Und", "in", "un\u00b7sern", "Lie\u00b7dern", "kei\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Silb aus Silbe, Wort aus Wort.", "tokens": ["Silb", "aus", "Sil\u00b7be", ",", "Wort", "aus", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sich gleich auf Deutsch nichts reimet,", "tokens": ["Ob", "sich", "gleich", "auf", "Deutsch", "nichts", "rei\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "APPR", "NE", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reimt der Deutsche dennoch fort.", "tokens": ["Reimt", "der", "Deut\u00b7sche", "den\u00b7noch", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob es kr\u00e4ftig oder zierlich,", "tokens": ["Ob", "es", "kr\u00e4f\u00b7tig", "o\u00b7der", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht uns so genau nicht an;", "tokens": ["Geht", "uns", "so", "ge\u00b7nau", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir sind bieder und nat\u00fcrlich,", "tokens": ["Wir", "sind", "bie\u00b7der", "und", "na\u00b7t\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und das ist genug getan.", "tokens": ["Und", "das", "ist", "ge\u00b7nug", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}