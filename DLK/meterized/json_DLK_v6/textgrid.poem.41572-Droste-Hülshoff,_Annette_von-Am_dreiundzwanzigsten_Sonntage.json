{"textgrid.poem.41572": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dreiundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn oft in kranken Stunden", "tokens": ["Wenn", "oft", "in", "kran\u00b7ken", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich auf mein Schuldbuch schl\u00e4gt,", "tokens": ["Sich", "auf", "mein", "Schuld\u00b7buch", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Skorpion an Wunden", "tokens": ["Der", "Skor\u00b7pi\u00b7on", "an", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat stechend sich gelegt:", "tokens": ["Hat", "ste\u00b7chend", "sich", "ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wei\u00df ich dann noch", "tokens": ["Wei\u00df", "ich", "dann", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Was zu beginnen?", "tokens": ["Was", "zu", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Leib ein modernd Joch", "tokens": ["Der", "Leib", "ein", "mo\u00b7dernd", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und ein Gespenst was drinnen!", "tokens": ["Und", "ein", "Ge\u00b7spenst", "was", "drin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PWS", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hab' ich so viel begangen", "tokens": ["Hab'", "ich", "so", "viel", "be\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn in so kurzer Zeit,", "tokens": ["Denn", "in", "so", "kur\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was wohl zur Schmach gelangen", "tokens": ["Was", "wohl", "zur", "Schmach", "ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "M\u00f6cht' einer Ewigkeit?", "tokens": ["M\u00f6cht'", "ei\u00b7ner", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin zerst\u00f6rt,", "tokens": ["Ich", "bin", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich bin vernichtet,", "tokens": ["Ich", "bin", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und langsam abgekehrt", "tokens": ["Und", "lang\u00b7sam", "ab\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ins Nichts mein Blick sich richtet.", "tokens": ["Ins", "Nichts", "mein", "Blick", "sich", "rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In solchen Augenblicken", "tokens": ["In", "sol\u00b7chen", "Au\u00b7gen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht meine Seele still,", "tokens": ["Steht", "mei\u00b7ne", "See\u00b7le", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darf nicht Gedanke r\u00fccken,", "tokens": ["Darf", "nicht", "Ge\u00b7dan\u00b7ke", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gefesselt liegt der Will'.", "tokens": ["Ge\u00b7fes\u00b7selt", "liegt", "der", "Will'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Schlafes Macht", "tokens": ["Und", "Schla\u00b7fes", "Macht"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mu\u00df ich beschw\u00f6ren", "tokens": ["Mu\u00df", "ich", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die angsterf\u00fcllte Nacht", "tokens": ["Die", "angs\u00b7ter\u00b7f\u00fcll\u00b7te", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In Tr\u00e4ume zu verkehren.", "tokens": ["In", "Tr\u00e4u\u00b7me", "zu", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch jetzt, wo klar die Sinnen,", "tokens": ["Doch", "jetzt", ",", "wo", "klar", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo mein Gedanke frei,", "tokens": ["Wo", "mein", "Ge\u00b7dan\u00b7ke", "frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt darf mein Flehn beginnen,", "tokens": ["Jetzt", "darf", "mein", "Flehn", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allgn\u00e4d'ger, steh mir bei!", "tokens": ["All\u00b7gn\u00e4d'\u00b7ger", ",", "steh", "mir", "bei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu solcher Zeit", "tokens": ["Zu", "sol\u00b7cher", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ohn' Trost und Beten,", "tokens": ["Ohn'", "Trost", "und", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "O dann an meine Seit'", "tokens": ["O", "dann", "an", "mei\u00b7ne", "Seit'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "La\u00df deinen Engel treten,", "tokens": ["La\u00df", "dei\u00b7nen", "En\u00b7gel", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df ich im Kampf bestehen", "tokens": ["Da\u00df", "ich", "im", "Kampf", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dunkle Stunde kann,", "tokens": ["Die", "dunk\u00b7le", "Stun\u00b7de", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nicht verloren gehen", "tokens": ["Und", "nicht", "ver\u00b7lo\u00b7ren", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meiner \u00c4ngsten Bann.", "tokens": ["In", "mei\u00b7ner", "\u00c4ngs\u00b7ten", "Bann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herr, nicht wirst du", "tokens": ["Herr", ",", "nicht", "wirst", "du"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PTKNEG", "VAFIN", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Umsonst mich qu\u00e4len,", "tokens": ["Um\u00b7sonst", "mich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Hast wohl ein Ziel der Ruh'", "tokens": ["Hast", "wohl", "ein", "Ziel", "der", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr mattgehetzte Seelen!", "tokens": ["F\u00fcr", "matt\u00b7ge\u00b7hetz\u00b7te", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "W\u00fc\u00dft' ich aus mir zu tragen", "tokens": ["W\u00fc\u00dft'", "ich", "aus", "mir", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Balsam in den Gift,", "tokens": ["Den", "Bal\u00b7sam", "in", "den", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat mich so geschlagen", "tokens": ["Wer", "hat", "mich", "so", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie deine heil'ge Schrift:", "tokens": ["Wie", "dei\u00b7ne", "heil'\u00b7ge", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem, der vergibt,", "tokens": ["Dem", ",", "der", "ver\u00b7gibt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wird Heil und Leben!", "tokens": ["Wird", "Heil", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wie mich es, Herr, betr\u00fcbt,", "tokens": ["Wie", "mich", "es", ",", "Herr", ",", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df nichts ich zu vergeben:", "tokens": ["Da\u00df", "nichts", "ich", "zu", "ver\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vielleicht ein Mi\u00dfbehagen,", "tokens": ["Viel\u00b7leicht", "ein", "Mi\u00df\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein armes F\u00fcnkchen Neid;", "tokens": ["Ein", "ar\u00b7mes", "F\u00fcnk\u00b7chen", "Neid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es tat ja meinen Tagen", "tokens": ["Es", "tat", "ja", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch keiner rechtes Leid,", "tokens": ["Noch", "kei\u00b7ner", "rech\u00b7tes", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und unverdient", "tokens": ["Und", "un\u00b7ver\u00b7di\u00b7ent"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "War nur das Lieben.", "tokens": ["War", "nur", "das", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So ist was, ach, dich s\u00fchnt,", "tokens": ["So", "ist", "was", ",", "ach", ",", "dich", "s\u00fchnt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "ITJ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kein Opfer mir geblieben.", "tokens": ["Kein", "Op\u00b7fer", "mir", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch weil du es geboten,", "tokens": ["Doch", "weil", "du", "es", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spricht aus des Herzens Grund", "tokens": ["Spricht", "aus", "des", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So Lebenden als Toten", "tokens": ["So", "Le\u00b7ben\u00b7den", "als", "To\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergebung aus mein Mund.", "tokens": ["Ver\u00b7ge\u00b7bung", "aus", "mein", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und was noch mag", "tokens": ["Und", "was", "noch", "mag"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mir sein beschieden", "tokens": ["Mir", "sein", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA"], "meter": "++-+-", "measure": "iambic.di"}, "line.7": {"text": "An Kr\u00e4nkung oder Schmach,", "tokens": ["An", "Kr\u00e4n\u00b7kung", "o\u00b7der", "Schmach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was noch vielleicht hienieden", "tokens": ["Was", "noch", "viel\u00b7leicht", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In meiner Zukunft Buch", "tokens": ["In", "mei\u00b7ner", "Zu\u00b7kunft", "Buch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hast gn\u00e4dig angeschrieben,", "tokens": ["Hast", "gn\u00e4\u00b7dig", "an\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann es nicht genug", "tokens": ["Ich", "kann", "es", "nicht", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ersehnen, sch\u00e4tzen, lieben,", "tokens": ["Er\u00b7seh\u00b7nen", ",", "sch\u00e4t\u00b7zen", ",", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": 