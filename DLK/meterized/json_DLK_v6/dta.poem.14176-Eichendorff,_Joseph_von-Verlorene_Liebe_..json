{"dta.poem.14176": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Verlorene Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4t von dem verlor'nen Kinde", "tokens": ["Sp\u00e4t", "von", "dem", "ver\u00b7lor'\u00b7nen", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ich durch die Nacht daher,", "tokens": ["Kam", "ich", "durch", "die", "Nacht", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahnen drehten sich im Winde,", "tokens": ["Fah\u00b7nen", "dreh\u00b7ten", "sich", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Gassen waren leer.", "tokens": ["Al\u00b7le", "Gas\u00b7sen", "wa\u00b7ren", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oben lag noch meine Laute", "tokens": ["O\u00b7ben", "lag", "noch", "mei\u00b7ne", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Fenster stand noch auf,", "tokens": ["Und", "mein", "Fens\u00b7ter", "stand", "noch", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus dem stillen Grunde graute", "tokens": ["Aus", "dem", "stil\u00b7len", "Grun\u00b7de", "grau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wunderbar die Stadt herauf.", "tokens": ["Wun\u00b7der\u00b7bar", "die", "Stadt", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Drau\u00dfen aber blitzt's von weiten,", "tokens": ["Drau\u00b7\u00dfen", "a\u00b7ber", "blitzt's", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alter Zeiten ich gedacht',", "tokens": ["Al\u00b7ter", "Zei\u00b7ten", "ich", "ge\u00b7dacht'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Schauernd rei\u00df' ich in den Saiten", "tokens": ["Schau\u00b7ernd", "rei\u00df'", "ich", "in", "den", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und ich sing' die halbe Nacht.", "tokens": ["Und", "ich", "sing'", "die", "hal\u00b7be", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Die verschlaf'nen Nachbarn sprechen,", "tokens": ["Die", "ver\u00b7schlaf'\u00b7nen", "Nach\u00b7barn", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df ich n\u00e4chtlich trunken sei \u2014", "tokens": ["Da\u00df", "ich", "n\u00e4cht\u00b7lich", "trun\u00b7ken", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "O du mein Gott! und mir brechen", "tokens": ["O", "du", "mein", "Gott", "!", "und", "mir", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.16": {"text": "Herz und Saitenspiel entzwei!", "tokens": ["Herz", "und", "Sai\u00b7ten\u00b7spiel", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}