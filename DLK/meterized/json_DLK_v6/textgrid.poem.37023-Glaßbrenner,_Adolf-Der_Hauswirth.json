{"textgrid.poem.37023": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Der Hauswirth", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Hauswirth, der p\u00fcnktlich nach jedem Quartal", "tokens": ["Mein", "Haus\u00b7wirth", ",", "der", "p\u00fcnkt\u00b7lich", "nach", "je\u00b7dem", "Quar\u00b7tal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+--+--+-++", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mir bezahlte die schuldige Miethe,", "tokens": ["Mir", "be\u00b7zahl\u00b7te", "die", "schul\u00b7di\u00b7ge", "Mie\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Anschnauzte mich heut, da\u00df zu wenig Skandal", "tokens": ["An\u00b7schnauz\u00b7te", "mich", "heut", ",", "da\u00df", "zu", "we\u00b7nig", "Skan\u00b7dal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Spektakel mein Hiersein ihm biete.", "tokens": ["Und", "Spek\u00b7ta\u00b7kel", "mein", "Hier\u00b7sein", "ihm", "bie\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Auch h\u00e4tt' er, kaum glaublich! noch gar nicht versp\u00fcrt,", "tokens": ["Auch", "h\u00e4tt'", "er", ",", "kaum", "glaub\u00b7lich", "!", "noch", "gar", "nicht", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "ADJD", "$.", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich eine der T\u00f6chter des Hauses verf\u00fchrt!", "tokens": ["Da\u00df", "ich", "ei\u00b7ne", "der", "T\u00f6ch\u00b7ter", "des", "Hau\u00b7ses", "ver\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Und endlich sei Lotte zu reinlich,", "tokens": ["Und", "end\u00b7lich", "sei", "Lot\u00b7te", "zu", "rein\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "PTKA", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und im Punkte der Sitte zu peinlich!", "tokens": ["Und", "im", "Punk\u00b7te", "der", "Sit\u00b7te", "zu", "pein\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich m\u00fcsse, im Fall da\u00df die Dinge nicht bald,", "tokens": ["Ich", "m\u00fcs\u00b7se", ",", "im", "Fall", "da\u00df", "die", "Din\u00b7ge", "nicht", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPRART", "NN", "KOUS", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die verdr\u00fc\u00dflichen, Wandelung n\u00e4hmen,", "tokens": ["Die", "ver\u00b7dr\u00fc\u00df\u00b7li\u00b7chen", ",", "Wan\u00b7de\u00b7lung", "n\u00e4h\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bei Vermeidung rechtsexecutiver Gewalt,", "tokens": ["Bei", "Ver\u00b7mei\u00b7dung", "rech\u00b7tsex\u00b7e\u00b7cu\u00b7ti\u00b7ver", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Das Logis mich zu r\u00e4umen bequemen.", "tokens": ["Das", "Lo\u00b7gis", "mich", "zu", "r\u00e4u\u00b7men", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wonach dann den Rest meiner Miethe-Caution,", "tokens": ["Wo\u00b7nach", "dann", "den", "Rest", "mei\u00b7ner", "Mie\u00b7the\u00b7Cau\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Von der er ein Achtel zur\u00fcckgezahlt schon,", "tokens": ["Von", "der", "er", "ein", "Ach\u00b7tel", "zu\u00b7r\u00fcck\u00b7ge\u00b7zahlt", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Er mir w\u00fcrde nach Landes-Manieren", "tokens": ["Er", "mir", "w\u00fcr\u00b7de", "nach", "Lan\u00b7des\u00b7Ma\u00b7nie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VAFIN", "APPR", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "\u00bbauf ewige Zeit reserviren.\u00ab", "tokens": ["\u00bb", "auf", "e\u00b7wi\u00b7ge", "Zeit", "re\u00b7ser\u00b7vi\u00b7ren", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ich schwur bei den G\u00f6ttern, ich w\u00fcrde sofort", "tokens": ["Ich", "schwur", "bei", "den", "G\u00f6t\u00b7tern", ",", "ich", "w\u00fcr\u00b7de", "so\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So handeln nur, wie's ihm gefiele,", "tokens": ["So", "han\u00b7deln", "nur", ",", "wie's", "ihm", "ge\u00b7fie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und warf, zum Beweise wie ernst mir mein Wort,", "tokens": ["Und", "warf", ",", "zum", "Be\u00b7wei\u00b7se", "wie", "ernst", "mir", "mein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "KOKOM", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zur Th\u00fcr' ihn hinaus auf die Diele,", "tokens": ["Zur", "Th\u00fcr'", "ihn", "hin\u00b7aus", "auf", "die", "Die\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APZR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schmi\u00df ihm verschiedentlich Waschgeschirr nach,", "tokens": ["Und", "schmi\u00df", "ihm", "ver\u00b7schie\u00b7dent\u00b7lich", "Waschge\u00b7schirr", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das ihn und die W\u00e4nde befluthend zerbrach,", "tokens": ["Das", "ihn", "und", "die", "W\u00e4n\u00b7de", "be\u00b7flut\u00b7hend", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KON", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und werde ihm k\u00fcnftig bereiten", "tokens": ["Und", "wer\u00b7de", "ihm", "k\u00fcnf\u00b7tig", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Noch weitere Aufmerksamkeiten.", "tokens": ["Noch", "wei\u00b7te\u00b7re", "Auf\u00b7merk\u00b7sam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}