{"textgrid.poem.66421": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Dudeldum", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei Dudeldum! so greif", "tokens": ["Ei", "Du\u00b7del\u00b7dum", "!", "so", "greif"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$.", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In dein Klavier so steif!", "tokens": ["In", "dein", "Kla\u00b7vier", "so", "steif", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zwar sind die Finger brav,", "tokens": ["Zwar", "sind", "die", "Fin\u00b7ger", "brav", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nie fehlt's in der Octav',", "tokens": ["Nie", "fehlt's", "in", "der", "Oc\u00b7ta\u00b7v'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Noch in der Quint' und Terz;", "tokens": ["Noch", "in", "der", "Quint'", "und", "Terz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur Eines fehlt \u2013 das Herz!", "tokens": ["Nur", "Ei\u00b7nes", "fehlt", "\u2013", "das", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}