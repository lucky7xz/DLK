{"textgrid.poem.61641": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begeistrung ist aus Gott ein Funken,", "tokens": ["Be\u00b7geis\u00b7trung", "ist", "aus", "Gott", "ein", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ruht gleich ihm voll Sch\u00f6pferlust", "tokens": ["Sie", "ruht", "gleich", "ihm", "voll", "Sch\u00f6p\u00b7fer\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz ins geliebte Werk versunken", "tokens": ["Ganz", "ins", "ge\u00b7lieb\u00b7te", "Werk", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwebt doch dr\u00fcber klarbewu\u00dft.", "tokens": ["Und", "schwebt", "doch", "dr\u00fc\u00b7ber", "klar\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}