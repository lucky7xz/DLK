{"textgrid.poem.67040": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Klagen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stella! ach! wir leiden viel! wann nur das Grab \u2013", "tokens": ["Stel\u00b7la", "!", "ach", "!", "wir", "lei\u00b7den", "viel", "!", "wann", "nur", "das", "Grab", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "XY", "$.", "PPER", "VVFIN", "ADV", "$.", "PWAV", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Komme! komme, k\u00fchles Grab! nimm uns beide!", "tokens": ["Kom\u00b7me", "!", "kom\u00b7me", ",", "k\u00fch\u00b7les", "Grab", "!", "nimm", "uns", "bei\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$,", "ADJA", "NN", "$.", "VVIMP", "PPER", "PIS", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Siehe Stellas Tr\u00e4nen, komme,", "tokens": ["Sie\u00b7he", "Stel\u00b7las", "Tr\u00e4\u00b7nen", ",", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchles, ruhiges Grab.", "tokens": ["K\u00fch\u00b7les", ",", "ru\u00b7hi\u00b7ges", "Grab", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "O ihr Menschen! o so gerne wollt ich euch", "tokens": ["O", "ihr", "Men\u00b7schen", "!", "o", "so", "ger\u00b7ne", "wollt", "ich", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$.", "FM", "ADV", "ADV", "VMFIN", "PPER", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Alle lieben, warm und treu! o ihr Menschen,", "tokens": ["Al\u00b7le", "lie\u00b7ben", ",", "warm", "und", "treu", "!", "o", "ihr", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$.", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sehet, diese Stella ha\u00dft ihr!", "tokens": ["Se\u00b7het", ",", "die\u00b7se", "Stel\u00b7la", "ha\u00dft", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott vergebe es euch!", "tokens": ["Gott", "ver\u00b7ge\u00b7be", "es", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Rei\u00dft sie nur hinweg von mir! Qu\u00e4ler! ihr!", "tokens": ["Rei\u00dft", "sie", "nur", "hin\u00b7weg", "von", "mir", "!", "Qu\u00e4\u00b7ler", "!", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$.", "NN", "$.", "PPER", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ich will schweigen \u2013 Gott \u2013 Gott wird reden.", "tokens": ["Ich", "will", "schwei\u00b7gen", "\u2013", "Gott", "\u2013", "Gott", "wird", "re\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "NN", "$(", "NN", "VAFIN", "VVINF", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lebe wohl \u2013 ich sterbe bald \u2013 O", "tokens": ["Le\u00b7be", "wohl", "\u2013", "ich", "ster\u00b7be", "bald", "\u2013", "O"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "ADV", "$(", "PPER", "VVFIN", "ADV", "$(", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stella! Stella, vergi\u00df mich.", "tokens": ["Stel\u00b7la", "!", "Stel\u00b7la", ",", "ver\u00b7gi\u00df", "mich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$,", "VVIMP", "PPER", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Viele Wonnenaugenblicke gabst du mir \u2013", "tokens": ["Vie\u00b7le", "Won\u00b7nen\u00b7au\u00b7gen\u00b7bli\u00b7cke", "gabst", "du", "mir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Vater, Vater! bebt ich oft auf zum Ewgen,", "tokens": ["Va\u00b7ter", ",", "Va\u00b7ter", "!", "bebt", "ich", "oft", "auf", "zum", "Ew\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "APPR", "APPRART", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Sieh, ich liebe sie so rein, dein Auge,", "tokens": ["Sieh", ",", "ich", "lie\u00b7be", "sie", "so", "rein", ",", "dein", "Au\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vater, sieht ja mein Herz.", "tokens": ["Va\u00b7ter", ",", "sieht", "ja", "mein", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Stella! weinen werd ich bis ans Grab um dich,", "tokens": ["Stel\u00b7la", "!", "wei\u00b7nen", "werd", "ich", "bis", "ans", "Grab", "um", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVINF", "VAFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Weinen, Stella, du um mich \u2013 weinen! aber", "tokens": ["Wei\u00b7nen", ",", "Stel\u00b7la", ",", "du", "um", "mich", "\u2013", "wei\u00b7nen", "!", "a\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "PPER", "APPR", "PPER", "$(", "VVINF", "$.", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Am Gerichtstag will ichs sagen", "tokens": ["Am", "Ge\u00b7richts\u00b7tag", "will", "ichs", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorm versammelten Erdkreis:", "tokens": ["Vorm", "ver\u00b7sam\u00b7mel\u00b7ten", "Erd\u00b7kreis", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Diese sinds, die Stella qu\u00e4lten \u2013 aber nein!", "tokens": ["Die\u00b7se", "sinds", ",", "die", "Stel\u00b7la", "qu\u00e4l\u00b7ten", "\u2013", "a\u00b7ber", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "NE", "VVFIN", "$(", "ADV", "PTKANT", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Gott im Himmel! nein! vergib diesen Qu\u00e4lern.", "tokens": ["Gott", "im", "Him\u00b7mel", "!", "nein", "!", "ver\u00b7gib", "die\u00b7sen", "Qu\u00e4\u00b7lern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "PTKANT", "$.", "VVIMP", "PDAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "La\u00df mich sterben \u2013 oder tragen", "tokens": ["La\u00df", "mich", "ster\u00b7ben", "\u2013", "o\u00b7der", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$(", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Leiden \u2013 mein Gott.", "tokens": ["Die\u00b7se", "Lei\u00b7den", "\u2013", "mein", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}