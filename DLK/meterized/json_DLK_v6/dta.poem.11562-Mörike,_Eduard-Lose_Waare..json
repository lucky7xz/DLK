{"dta.poem.11562": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Lose Waare.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "\u201etinte! Tinte kauft ab! Sch\u00f6n schwarze Tinte verkauf' ich:\u201c", "tokens": ["\u201e", "tin\u00b7te", "!", "Tin\u00b7te", "kauft", "ab", "!", "Sch\u00f6n", "schwar\u00b7ze", "Tin\u00b7te", "ver\u00b7kauf'", "ich", ":", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$.", "NN", "VVFIN", "PTKVZ", "$.", "NE", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Rief ein B\u00fcblein gar hell Stra\u00dfen hinauf und hinab.", "tokens": ["Rief", "ein", "B\u00fcb\u00b7lein", "gar", "hell", "Stra\u00b7\u00dfen", "hin\u00b7auf", "und", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Lachend traf sein feuriger Blick mich oben im Fenster,", "tokens": ["La\u00b7chend", "traf", "sein", "feu\u00b7ri\u00b7ger", "Blick", "mich", "o\u00b7ben", "im", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Eh' ich mich's irgend versah, huscht er ins Zimmer herein.", "tokens": ["Eh'", "ich", "mich's", "ir\u00b7gend", "ver\u00b7sah", ",", "huscht", "er", "ins", "Zim\u00b7mer", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Knabe, dich rief Niemand! \u2014 \u201eHerr! meine Waare ver-", "tokens": ["Kna\u00b7be", ",", "dich", "rief", "Nie\u00b7mand", "!", "\u201e", "Herr", "!", "mei\u00b7ne", "Waa\u00b7re", "ver"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PIS", "$.", "$(", "$(", "NN", "$.", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und sein F\u00e4\u00dfchen behend schwang er vom R\u00fccken", "tokens": ["Und", "sein", "F\u00e4\u00df\u00b7chen", "be\u00b7hend", "schwang", "er", "vom", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da verschob sich das halbzerrissene J\u00e4ckchen ein wenig", "tokens": ["Da", "ver\u00b7schob", "sich", "das", "halb\u00b7zer\u00b7ris\u00b7se\u00b7ne", "J\u00e4ck\u00b7chen", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "PIS"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "An der Schulter und hell schimmert ein Fl\u00fcgel hervor.", "tokens": ["An", "der", "Schul\u00b7ter", "und", "hell", "schim\u00b7mert", "ein", "Fl\u00fc\u00b7gel", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ei, la\u00df sehen, mein Sohn! du f\u00fchrst auch Federn im", "tokens": ["Ei", ",", "la\u00df", "se\u00b7hen", ",", "mein", "Sohn", "!", "du", "f\u00fchrst", "auch", "Fe\u00b7dern", "im"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "VVINF", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "APPRART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Amor, verkleideter Schelm! soll ich dich rupfen sogleich?", "tokens": ["A\u00b7mor", ",", "ver\u00b7klei\u00b7de\u00b7ter", "Schelm", "!", "soll", "ich", "dich", "rup\u00b7fen", "sog\u00b7leich", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "VMFIN", "PPER", "PRF", "VVFIN", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Und er l\u00e4chelt, entlarvt und legt auf die Lippen den", "tokens": ["Und", "er", "l\u00e4\u00b7chelt", ",", "ent\u00b7larvt", "und", "legt", "auf", "die", "Lip\u00b7pen", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.12": {"text": "\u201estille! sie sind nicht verzollt \u2014 st\u00f6rt die Gesch\u00e4fte", "tokens": ["\u201e", "stil\u00b7le", "!", "sie", "sind", "nicht", "ver\u00b7zollt", "st\u00f6rt", "die", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$(", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Gebt das Gef\u00e4\u00df, ich f\u00fcll' es umsonst, und bleiben wir", "tokens": ["Gebt", "das", "Ge\u00b7f\u00e4\u00df", ",", "ich", "f\u00fcll'", "es", "um\u00b7sonst", ",", "und", "blei\u00b7ben", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PPER"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Dies gesagt und gethan, schl\u00fcpft er zur Th\u00fcre hinaus. \u2014", "tokens": ["Dies", "ge\u00b7sagt", "und", "ge\u00b7than", ",", "schl\u00fcpft", "er", "zur", "Th\u00fc\u00b7re", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVPP", "KON", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Angef\u00fchrt hat er mich doch: denn will ich was N\u00fctzliches", "tokens": ["An\u00b7ge\u00b7f\u00fchrt", "hat", "er", "mich", "doch", ":", "denn", "will", "ich", "was", "N\u00fctz\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PRF", "ADV", "$.", "KON", "VMFIN", "PPER", "PIS", "ADJA"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.16": {"text": "Gleich wird ein Liebesbrief, gleich ein Erotikon draus.", "tokens": ["Gleich", "wird", "ein", "Lie\u00b7bes\u00b7brief", ",", "gleich", "ein", "E\u00b7ro\u00b7ti\u00b7kon", "draus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}