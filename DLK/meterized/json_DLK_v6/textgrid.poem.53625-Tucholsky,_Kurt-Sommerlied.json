{"textgrid.poem.53625": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sommerlied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sommer blaut,", "tokens": ["Wenn", "der", "Som\u00b7mer", "blaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "wenn der Penner klaut,", "tokens": ["wenn", "der", "Pen\u00b7ner", "klaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "wenn der Gastwirt stellt den Garten raus:", "tokens": ["wenn", "der", "Gast\u00b7wirt", "stellt", "den", "Gar\u00b7ten", "raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Pflanzt im Bumslokal", "tokens": ["Pflanzt", "im", "Bums\u00b7lo\u00b7kal"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "sich mit einem Mal", "tokens": ["sich", "mit", "ei\u00b7nem", "Mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "der beliebte Humorist vors volle Haus.", "tokens": ["der", "be\u00b7lieb\u00b7te", "Hu\u00b7mo\u00b7rist", "vors", "vol\u00b7le", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und er tut als wie besoffen,", "tokens": ["Und", "er", "tut", "als", "wie", "be\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOUS", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und er murmelt schwer betroffen \u2013", "tokens": ["und", "er", "mur\u00b7melt", "schwer", "be\u00b7trof\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schnedderedeng \u2013 den Refrain:", "tokens": ["Schned\u00b7de\u00b7re\u00b7deng", "\u2013", "den", "Re\u00b7frain", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "\u00bbund ick immer mitn mit, mitn Schmidt, mitn mit,", "tokens": ["\u00bb", "und", "ick", "im\u00b7mer", "mitn", "mit", ",", "mitn", "Schmidt", ",", "mitn", "mit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ADV", "ADV", "PTKVZ", "$,", "APPR", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und ick immer mitn mit, mitn mit, mitn Schmidt!\u00ab", "tokens": ["und", "ick", "im\u00b7mer", "mitn", "mit", ",", "mitn", "mit", ",", "mitn", "Schmidt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "PTKVZ", "$,", "FM.la", "FM.la", "$,", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mancher Journalist", "tokens": ["Man\u00b7cher", "Jour\u00b7na\u00b7list"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "wei\u00df genau, wies ist,", "tokens": ["wei\u00df", "ge\u00b7nau", ",", "wies", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "wenn der Umsturz alle Seelen fa\u00dft.", "tokens": ["wenn", "der", "Um\u00b7sturz", "al\u00b7le", "See\u00b7len", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und er sichert sich", "tokens": ["Und", "er", "si\u00b7chert", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "leis und vorsichtig,", "tokens": ["leis", "und", "vor\u00b7sich\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "da\u00df er nur den letzten Anschlu\u00df nicht verpa\u00dft.", "tokens": ["da\u00df", "er", "nur", "den", "letz\u00b7ten", "An\u00b7schlu\u00df", "nicht", "ver\u00b7pa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Der Zeitgeist pfeift. Der Zeitgeist lockt.", "tokens": ["Der", "Zeit\u00b7geist", "pfeift", ".", "Der", "Zeit\u00b7geist", "lockt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ganz gesiegt ist halb geschmockt. \u2013", "tokens": ["Und", "ganz", "ge\u00b7siegt", "ist", "halb", "ge\u00b7schmockt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schnedderedeng \u2013 im Refrain:", "tokens": ["Schned\u00b7de\u00b7re\u00b7deng", "\u2013", "im", "Re\u00b7frain", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Und sie immer mitn mit, mitn Schmidt, mitn mit,", "tokens": ["Und", "sie", "im\u00b7mer", "mitn", "mit", ",", "mitn", "Schmidt", ",", "mitn", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "PTKVZ", "$,", "APPR", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und sie immer mitn mit, mitn mit, mitn Schmidt!", "tokens": ["und", "sie", "im\u00b7mer", "mitn", "mit", ",", "mitn", "mit", ",", "mitn", "Schmidt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "PTKVZ", "$,", "FM.la", "FM.la", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Manches Volk ist blind,", "tokens": ["Man\u00b7ches", "Volk", "ist", "blind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Fahnen wehn im Wind,", "tokens": ["Fah\u00b7nen", "wehn", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Idealen geht die Farbe ab.", "tokens": ["I\u00b7dea\u00b7len", "geht", "die", "Far\u00b7be", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sie h\u00e4ngen dran \u2013", "tokens": ["Doch", "sie", "h\u00e4n\u00b7gen", "dran", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "alle, Mann f\u00fcr Mann \u2013", "tokens": ["al\u00b7le", ",", "Mann", "f\u00fcr", "Mann", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "APPR", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "haben nichts gelernt von Wilhelm bis zu Kapp.", "tokens": ["ha\u00b7ben", "nichts", "ge\u00b7lernt", "von", "Wil\u00b7helm", "bis", "zu", "Kapp", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "APPR", "NE", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "F\u00fchrt auch Ludendorff sie in den Scheibenkleister:", "tokens": ["F\u00fchrt", "auch", "Lu\u00b7den\u00b7dorff", "sie", "in", "den", "Schei\u00b7ben\u00b7kleis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "er bleibt doch der gro\u00dfe deutsche Meister \u2013", "tokens": ["er", "bleibt", "doch", "der", "gro\u00b7\u00dfe", "deut\u00b7sche", "Meis\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Schnedderedeng \u2013 im Refrain:", "tokens": ["Schned\u00b7de\u00b7re\u00b7deng", "\u2013", "im", "Re\u00b7frain", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Und sie alle mitn mit, mitn Schmidt, mitn mit,", "tokens": ["Und", "sie", "al\u00b7le", "mitn", "mit", ",", "mitn", "Schmidt", ",", "mitn", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "PTKVZ", "$,", "APPR", "NE", "$,", "FM.la", "FM.la", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "und sie alle mitn mit, mitn mit, mitn Schmidt!", "tokens": ["und", "sie", "al\u00b7le", "mitn", "mit", ",", "mitn", "mit", ",", "mitn", "Schmidt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "PTKVZ", "$,", "FM.la", "FM.la", "$,", "APPR", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}