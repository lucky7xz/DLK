{"dta.poem.19848": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "2.  \n  Weihgesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Morgen ist Dionens Feier.", "tokens": ["Mor\u00b7gen", "ist", "Di\u00b7o\u00b7nens", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmet an den Weihgesang!", "tokens": ["Stim\u00b7met", "an", "den", "Weih\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6ne drein, gew\u00f6lbte Leier!", "tokens": ["T\u00f6\u00b7ne", "drein", ",", "ge\u00b7w\u00f6lb\u00b7te", "Lei\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hall\u2019 am Felsen, Wiederklang!", "tokens": ["Hall'", "am", "Fel\u00b7sen", ",", "Wie\u00b7der\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Morgen bringen ihre Tauben", "tokens": ["Mor\u00b7gen", "brin\u00b7gen", "ih\u00b7re", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie herab in unsern Hain;", "tokens": ["Sie", "her\u00b7ab", "in", "un\u00b7sern", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Morgen, unter Myrtenlauben,", "tokens": ["Mor\u00b7gen", ",", "un\u00b7ter", "Myr\u00b7ten\u00b7lau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ladet sie zu T\u00e4nzen ein;", "tokens": ["La\u00b7det", "sie", "zu", "T\u00e4n\u00b7zen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Morgen, vom erhabnen Throne", "tokens": ["Mor\u00b7gen", ",", "vom", "er\u00b7hab\u00b7nen", "Thro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Winket uns ihr Richterstab,", "tokens": ["Win\u00b7ket", "uns", "ihr", "Rich\u00b7ter\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und sie spricht, zu Straf\u2019 und Lohne,", "tokens": ["Und", "sie", "spricht", ",", "zu", "Straf'", "und", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "G\u00fctevolles Recht herab.", "tokens": ["G\u00fc\u00b7te\u00b7vol\u00b7les", "Recht", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Eilt, den Thron ihr zu erheben!", "tokens": ["Eilt", ",", "den", "Thron", "ihr", "zu", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut der K\u00f6nigin Gebot!", "tokens": ["Thut", "der", "K\u00f6\u00b7ni\u00b7gin", "Ge\u00b7bot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flora sol ihn \u00fcberweben,", "tokens": ["Flo\u00b7ra", "sol", "ihn", "\u00fc\u00b7ber\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Golden, blau und purpurrot.", "tokens": ["Gol\u00b7den", ",", "blau", "und", "pur\u00b7pur\u00b7rot", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Spend\u2019, o Flora, jede Blume,", "tokens": ["Spend'", ",", "o", "Flo\u00b7ra", ",", "je\u00b7de", "Blu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im bunten Enna lacht!", "tokens": ["Die", "im", "bun\u00b7ten", "En\u00b7na", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flora, zu Dionens Ruhme,", "tokens": ["Flo\u00b7ra", ",", "zu", "Di\u00b7o\u00b7nens", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spende deine ganze Pracht!", "tokens": ["Spen\u00b7de", "dei\u00b7ne", "gan\u00b7ze", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Unser prangendes Geleite", "tokens": ["Un\u00b7ser", "pran\u00b7gen\u00b7des", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird am Thron ihr huldigen.", "tokens": ["Wird", "am", "Thron", "ihr", "hul\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sizen werden ihr zur Seite", "tokens": ["Si\u00b7zen", "wer\u00b7den", "ihr", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Amor und die Grazien.", "tokens": ["A\u00b7mor", "und", "die", "Gra\u00b7zi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Alle Nymfen sind geladen.", "tokens": ["Al\u00b7le", "Nym\u00b7fen", "sind", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nymfen aus Gefild\u2019 und Hain,", "tokens": ["Nym\u00b7fen", "aus", "Ge\u00b7fild'", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wasserm\u00e4dchen, Oreaden", "tokens": ["Was\u00b7ser\u00b7m\u00e4d\u00b7chen", ",", "O\u00b7rea\u00b7den"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Werden hier versamlet seyn.", "tokens": ["Wer\u00b7den", "hier", "ver\u00b7sam\u00b7let", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle sind herbei gerufen,", "tokens": ["Al\u00b7le", "sind", "her\u00b7bei", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vor Dionens Angesicht,", "tokens": ["Vor", "Di\u00b7o\u00b7nens", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Mitzusizen, um die Stufen", "tokens": ["Mit\u00b7zu\u00b7si\u00b7zen", ",", "um", "die", "Stu\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ihres Thrones, zu Gericht.", "tokens": ["Ih\u00b7res", "Thro\u00b7nes", ",", "zu", "Ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Schon durchwallt die frohen Haine", "tokens": ["Schon", "durch\u00b7wallt", "die", "fro\u00b7hen", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Cythereens Nymfenschaar.", "tokens": ["Cy\u00b7ther\u00b7eens", "Nym\u00b7fen\u00b7schaar", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Amor flattert mit; doch keine", "tokens": ["A\u00b7mor", "flat\u00b7tert", "mit", ";", "doch", "kei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht sich ihm und der Gefar. \u2014", "tokens": ["Naht", "sich", "ihm", "und", "der", "Ge\u00b7far", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "PPER", "KON", "ART", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Nymfen, die sein K\u00f6cher schrekte,", "tokens": ["Nym\u00b7fen", ",", "die", "sein", "K\u00f6\u00b7cher", "schrek\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wist ihr nicht, was ihm geschehn,", "tokens": ["Wist", "ihr", "nicht", ",", "was", "ihm", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er heut die Waffen strekte,", "tokens": ["Da\u00df", "er", "heut", "die", "Waf\u00b7fen", "strek\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er heut mus wehrlos gehn? \u2014", "tokens": ["Da\u00df", "er", "heut", "mus", "wehr\u00b7los", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Unverbr\u00fcchliche Geseze", "tokens": ["Un\u00b7ver\u00b7br\u00fcch\u00b7li\u00b7che", "Ge\u00b7se\u00b7ze"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wollen, da\u00df sein Bogen heut", "tokens": ["Wol\u00b7len", ",", "da\u00df", "sein", "Bo\u00b7gen", "heut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Keiner Nymfe Brust verleze. \u2014", "tokens": ["Kei\u00b7ner", "Nym\u00b7fe", "Brust", "ver\u00b7le\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber, Nymfen, scheut, o scheut", "tokens": ["A\u00b7ber", ",", "Nym\u00b7fen", ",", "scheut", ",", "o", "scheut"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$,", "FM", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ihn auch nakt! Er \u00fcberlistet,", "tokens": ["Ihn", "auch", "nakt", "!", "Er", "\u00fc\u00b7berl\u00b7is\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Er verlezt euch M\u00e4dchen doch:", "tokens": ["Er", "ver\u00b7lezt", "euch", "M\u00e4d\u00b7chen", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn den Waffenlosen r\u00fcstet", "tokens": ["Denn", "den", "Waf\u00b7fen\u00b7lo\u00b7sen", "r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Seine ganze Sch\u00f6nheit noch.", "tokens": ["Sei\u00b7ne", "gan\u00b7ze", "Sch\u00f6n\u00b7heit", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u0