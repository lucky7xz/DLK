{"textgrid.poem.58033": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "71. Allein oder la\u00df es gar sein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, teil dein Herz doch nicht; dein Alles Gott ergiebe", "tokens": ["Ei", ",", "teil", "dein", "Herz", "doch", "nicht", ";", "dein", "Al\u00b7les", "Gott", "er\u00b7gie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "$.", "PPOSAT", "PIAT", "NN", "VVFIN"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und deine Liebe ganz in ihn allein einf\u00fchr!", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7be", "ganz", "in", "ihn", "al\u00b7lein", "ein\u00b7f\u00fchr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein kleines Herz hat doch noch viel zu wenig Liebe,", "tokens": ["Dein", "klei\u00b7nes", "Herz", "hat", "doch", "noch", "viel", "zu", "we\u00b7nig", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um ein unendlich Gut zu lieben nach Geb\u00fchr.", "tokens": ["Um", "ein", "un\u00b7end\u00b7lich", "Gut", "zu", "lie\u00b7ben", "nach", "Ge\u00b7b\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJD", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}