{"textgrid.poem.32389": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "67. Grabschrift eines Ungl\u00fccklichen, welcher zuletzt in einem Schiffbruche umkam", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier warfen mich die Wellen an das Land.", "tokens": ["Hier", "war\u00b7fen", "mich", "die", "Wel\u00b7len", "an", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier grub mich tot, mit frommer Hand,", "tokens": ["Hier", "grub", "mich", "tot", ",", "mit", "from\u00b7mer", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fischer in den leichten Sand.", "tokens": ["Ein", "Fi\u00b7scher", "in", "den", "leich\u00b7ten", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Mitleid, Leser, ist bei mir nicht angewandt!", "tokens": ["Dein", "Mit\u00b7leid", ",", "Le\u00b7ser", ",", "ist", "bei", "mir", "nicht", "an\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "APPR", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Sturme scheitern und ersaufen,", "tokens": ["Im", "Stur\u00b7me", "schei\u00b7tern", "und", "er\u00b7sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df mir Ungl\u00fccklichem, mit Sturm in Hafen laufen.", "tokens": ["Hie\u00df", "mir", "Un\u00b7gl\u00fcck\u00b7li\u00b7chem", ",", "mit", "Sturm", "in", "Ha\u00b7fen", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}