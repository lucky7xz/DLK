{"textgrid.poem.38746": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. Das Wasser", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blauer, flie\u00dfender Aether,", "tokens": ["Blau\u00b7er", ",", "flie\u00b7\u00dfen\u00b7der", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der von der Berge Gipfel", "tokens": ["Der", "von", "der", "Ber\u00b7ge", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich niedertaucht;", "tokens": ["Sich", "nie\u00b7der\u00b7taucht", ";"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und s\u00fc\u00df gen\u00e4hrt", "tokens": ["Und", "s\u00fc\u00df", "ge\u00b7n\u00e4hrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von strebenden Kindern,", "tokens": ["Von", "stre\u00b7ben\u00b7den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die ihm in die Arme st\u00fcrzen,", "tokens": ["Die", "ihm", "in", "die", "Ar\u00b7me", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Froh lachend an den Busen fliegen,", "tokens": ["Froh", "la\u00b7chend", "an", "den", "Bu\u00b7sen", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Daher mit seinen athmenden Fluten zieht.", "tokens": ["Da\u00b7her", "mit", "sei\u00b7nen", "ath\u00b7men\u00b7den", "Flu\u00b7ten", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nieder gehst du", "tokens": ["Nie\u00b7der", "gehst", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In Andacht,", "tokens": ["In", "An\u00b7dacht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In Demuth,", "tokens": ["In", "De\u00b7muth", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Entfliehst den Gebirgen,", "tokens": ["Ent\u00b7fliehst", "den", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den steilen H\u00f6hen,", "tokens": ["Den", "stei\u00b7len", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und senkst dich seelig sanft in stille Th\u00e4ler.", "tokens": ["Und", "senkst", "dich", "see\u00b7lig", "sanft", "in", "stil\u00b7le", "Th\u00e4\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fort schl\u00e4gst du mit lebenden Pulsen", "tokens": ["Fort", "schl\u00e4gst", "du", "mit", "le\u00b7ben\u00b7den", "Pul\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "In triumphirender Freude,", "tokens": ["In", "tri\u00b7um\u00b7phi\u00b7ren\u00b7der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im ungehemmter Bewegung,", "tokens": ["Im", "un\u00b7ge\u00b7hemm\u00b7ter", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In's ewige Meer,", "tokens": ["In's", "e\u00b7wi\u00b7ge", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das gro\u00dfe, unergr\u00fcndliche, nie erme\u00dfne.", "tokens": ["Das", "gro\u00b7\u00dfe", ",", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7che", ",", "nie", "er\u00b7me\u00df\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dich n\u00e4hren die Wunder der Tiefe,", "tokens": ["Dich", "n\u00e4h\u00b7ren", "die", "Wun\u00b7der", "der", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Du saugst mit Lebensathem", "tokens": ["Du", "saugst", "mit", "Le\u00b7ben\u00b7sa\u00b7them"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die verlassensten, einsamsten Kinder", "tokens": ["Die", "ver\u00b7las\u00b7sens\u00b7ten", ",", "ein\u00b7sams\u00b7ten", "Kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Zu dir ins lichte Leben herauf.", "tokens": ["Zu", "dir", "ins", "lich\u00b7te", "Le\u00b7ben", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Deine Herzens-Adern ziehn sich in den Abgrund,", "tokens": ["Dei\u00b7ne", "Her\u00b7zens\u00b7A\u00b7dern", "ziehn", "sich", "in", "den", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Niemals steigt dein heiliges Blut", "tokens": ["Nie\u00b7mals", "steigt", "dein", "hei\u00b7li\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Mit seinen hohen Str\u00f6men in das Dunkel,", "tokens": ["Mit", "sei\u00b7nen", "ho\u00b7hen", "Str\u00f6\u00b7men", "in", "das", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Du verschm\u00e4hst es.", "tokens": ["Du", "ver\u00b7schm\u00e4hst", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}