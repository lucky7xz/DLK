{"textgrid.poem.50701": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Freundschaft und Ehe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer H\u00fclfe n\u00f6thig hat und, was ihm n\u00fctzt, verstehet,", "tokens": ["Wer", "H\u00fcl\u00b7fe", "n\u00f6\u00b7thig", "hat", "und", ",", "was", "ihm", "n\u00fctzt", ",", "ver\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VAFIN", "KON", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der seh', statt eines Weibs", "tokens": ["Der", "seh'", ",", "statt", "ei\u00b7nes", "Weibs"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Narr ist, der zu Wasser gehet,", "tokens": ["Ein", "Narr", "ist", ",", "der", "zu", "Was\u00b7ser", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er zu Lande reisen kann.", "tokens": ["Wenn", "er", "zu", "Lan\u00b7de", "rei\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}