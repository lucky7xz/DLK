{"textgrid.poem.47777": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[h\u00f6re mir den Prediger]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re mir den Prediger,", "tokens": ["H\u00f6\u00b7re", "mir", "den", "Pre\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen hohler Redeschwall", "tokens": ["Des\u00b7sen", "hoh\u00b7ler", "Re\u00b7de\u00b7schwall"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Geiste Bande flicht,", "tokens": ["Dei\u00b7nem", "Geis\u00b7te", "Ban\u00b7de", "flicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6re mir den Pfaffen nicht!", "tokens": ["H\u00f6\u00b7re", "mir", "den", "Pfaf\u00b7fen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re du die Nachtigall,", "tokens": ["H\u00f6\u00b7re", "du", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die auf ihrer gr\u00fcnen Kanzel", "tokens": ["Die", "auf", "ih\u00b7rer", "gr\u00fc\u00b7nen", "Kan\u00b7zel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "\u00dcber Rosensch\u00f6ne handelt,", "tokens": ["\u00dc\u00b7ber", "Ro\u00b7sen\u00b7sch\u00f6\u00b7ne", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber Lenz und Liebe spricht!", "tokens": ["\u00dc\u00b7ber", "Lenz", "und", "Lie\u00b7be", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}