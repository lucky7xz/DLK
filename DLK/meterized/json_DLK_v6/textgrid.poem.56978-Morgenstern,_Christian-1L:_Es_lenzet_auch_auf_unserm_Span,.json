{"textgrid.poem.56978": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lenzet auch auf unserm Span,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lenzet auch auf unserm Span,", "tokens": ["Es", "len\u00b7zet", "auch", "auf", "un\u00b7serm", "Span", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "o selige Epoche!", "tokens": ["o", "se\u00b7li\u00b7ge", "E\u00b7po\u00b7che", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein H\u00e4lmlein will zum Lichte nahn", "tokens": ["Ein", "H\u00e4lm\u00b7lein", "will", "zum", "Lich\u00b7te", "nahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "aus einem Astwurmloche.", "tokens": ["aus", "ei\u00b7nem", "Ast\u00b7wurm\u00b7lo\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schaukelt bald im Winde hin", "tokens": ["Es", "schau\u00b7kelt", "bald", "im", "Win\u00b7de", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und schaukelt bald drin her.", "tokens": ["und", "schau\u00b7kelt", "bald", "drin", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mir ist beinah, ich w\u00e4re wer,", "tokens": ["Mir", "ist", "bei\u00b7nah", ",", "ich", "w\u00e4\u00b7re", "wer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der ich doch nicht mehr bin..", "tokens": ["der", "ich", "doch", "nicht", "mehr", "bin", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}