{"textgrid.poem.59672": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Des Zweiflers Unruh'", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, wohin, ihr flatternden Gedanken?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "ihr", "flat\u00b7tern\u00b7den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin mit mir im brausenden Gewimmel?", "tokens": ["Wo\u00b7hin", "mit", "mir", "im", "brau\u00b7sen\u00b7den", "Ge\u00b7wim\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was rei\u00dft ihr mich durch aller Himmel Himmel", "tokens": ["Was", "rei\u00dft", "ihr", "mich", "durch", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schlingt um nichts und alles eure Ranken?", "tokens": ["Und", "schlingt", "um", "nichts", "und", "al\u00b7les", "eu\u00b7re", "Ran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "KON", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir fliegen hoch. Sind wir so hoch geboren?", "tokens": ["Wir", "flie\u00b7gen", "hoch", ".", "Sind", "wir", "so", "hoch", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und warnt uns nicht, was Fabel klingt und Sage,", "tokens": ["Und", "warnt", "uns", "nicht", ",", "was", "Fa\u00b7bel", "klingt", "und", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Weisen Lehre und der Helden Klage,", "tokens": ["Der", "Wei\u00b7sen", "Leh\u00b7re", "und", "der", "Hel\u00b7den", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Frommen Seufzer und der Spott des Toren?", "tokens": ["Der", "From\u00b7men", "Seuf\u00b7zer", "und", "der", "Spott", "des", "To\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie warnen: Kinder fl\u00fcchtiger Sekunden,", "tokens": ["Sie", "war\u00b7nen", ":", "Kin\u00b7der", "fl\u00fcch\u00b7ti\u00b7ger", "Se\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie rei\u00dft euch doch der wilde Wahnsinn hinnen?", "tokens": ["Wie", "rei\u00dft", "euch", "doch", "der", "wil\u00b7de", "Wahn\u00b7sinn", "hin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Menschen schaffen, bauen, denken, sinnen,", "tokens": ["Was", "Men\u00b7schen", "schaf\u00b7fen", ",", "bau\u00b7en", ",", "den\u00b7ken", ",", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird ihnen gleich ein Morgentraum erfunden.", "tokens": ["Wird", "ih\u00b7nen", "gleich", "ein", "Mor\u00b7gen\u00b7traum", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, da\u00df wir durch die h\u00f6chsten Himmelsh\u00f6hen", "tokens": ["Ja", ",", "da\u00df", "wir", "durch", "die", "h\u00f6chs\u00b7ten", "Him\u00b7mels\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch die tiefsten H\u00f6llen m\u00fcssen schweifen,", "tokens": ["Und", "durch", "die", "tiefs\u00b7ten", "H\u00f6l\u00b7len", "m\u00fcs\u00b7sen", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach Unergreiflichkeiten m\u00fcssen greifen,", "tokens": ["Nach", "Un\u00b7er\u00b7greif\u00b7lich\u00b7kei\u00b7ten", "m\u00fcs\u00b7sen", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist das lange Weh der \u00e4ltsten Wehen.", "tokens": ["Das", "ist", "das", "lan\u00b7ge", "Weh", "der", "\u00e4lts\u00b7ten", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da spielt die Hoffart und ihr Sohn, der Zweifel,", "tokens": ["Da", "spielt", "die", "Hof\u00b7fart", "und", "ihr", "Sohn", ",", "der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, schon Gesell von Adam, unserm Ahnen;", "tokens": ["Ach", ",", "schon", "Ge\u00b7sell", "von", "A\u00b7dam", ",", "un\u00b7serm", "Ah\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sind Soldaten unter seinen Fahnen", "tokens": ["Wir", "sind", "Sol\u00b7da\u00b7ten", "un\u00b7ter", "sei\u00b7nen", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und folgen ihres bunten Trugs Gewaifel.", "tokens": ["Und", "fol\u00b7gen", "ih\u00b7res", "bun\u00b7ten", "Trugs", "Ge\u00b7wai\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er ruft: Mir nach zum Sieg, ihr tapfern Knechte!", "tokens": ["Er", "ruft", ":", "Mir", "nach", "zum", "Sieg", ",", "ihr", "tap\u00b7fern", "Knech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "APPR", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir nach, zum Dienst das Geisterreich zu zwingen!", "tokens": ["Mir", "nach", ",", "zum", "Dienst", "das", "Geis\u00b7ter\u00b7reich", "zu", "zwin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir m\u00fcssen durch zum Lichtesurborn dringen;", "tokens": ["Wir", "m\u00fcs\u00b7sen", "durch", "zum", "Lich\u00b7te\u00b7sur\u00b7born", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Feigen unten lassen wir die N\u00e4chte.", "tokens": ["Den", "Fei\u00b7gen", "un\u00b7ten", "las\u00b7sen", "wir", "die", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das ist's, das ist der alte Fluch hienieden:", "tokens": ["Das", "ist's", ",", "das", "ist", "der", "al\u00b7te", "Fluch", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir jagen nach den bunten Zauberbildern,", "tokens": ["Wir", "ja\u00b7gen", "nach", "den", "bun\u00b7ten", "Zau\u00b7ber\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis wir im wirren Geisterkrieg verwildern,", "tokens": ["Bis", "wir", "im", "wir\u00b7ren", "Geis\u00b7ter\u00b7krieg", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets weiter weg von Einfalt, Ruh' und Frieden.", "tokens": ["Stets", "wei\u00b7ter", "weg", "von", "Ein\u00b7falt", ",", "Ruh'", "und", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Verwirrt hat unser Abc die L\u00fcge,", "tokens": ["Ver\u00b7wirrt", "hat", "un\u00b7ser", "Abc", "die", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwischt das klare Antlitz reiner Z\u00fcge,", "tokens": ["Ver\u00b7wischt", "das", "kla\u00b7re", "Ant\u00b7litz", "rei\u00b7ner", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des G\u00f6tterbilds der Liebe, Lust und Milde.", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7bilds", "der", "Lie\u00b7be", ",", "Lust", "und", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch ist des Bildes Bild herabgekommen,", "tokens": ["Doch", "ist", "des", "Bil\u00b7des", "Bild", "her\u00b7ab\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Armen Reichtum und das Licht der Blinden,", "tokens": ["Des", "Ar\u00b7men", "Reich\u00b7tum", "und", "das", "Licht", "der", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Edelstein, den Kunst und List nicht finden,", "tokens": ["Der", "E\u00b7del\u00b7stein", ",", "den", "Kunst", "und", "List", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nur den Stillen leuchtet und den Frommen.", "tokens": ["Der", "nur", "den", "Stil\u00b7len", "leuch\u00b7tet", "und", "den", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}