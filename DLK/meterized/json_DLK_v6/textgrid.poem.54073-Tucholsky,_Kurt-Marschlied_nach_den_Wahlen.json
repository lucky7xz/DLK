{"textgrid.poem.54073": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Marschlied nach den Wahlen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht an dem hamburger Hafen", "tokens": ["Es", "steht", "an", "dem", "ham\u00b7bur\u00b7ger", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein riesiger Bismarck aus Stein.", "tokens": ["ein", "rie\u00b7si\u00b7ger", "Bis\u00b7marck", "aus", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Schlaf seines Ruhmes zu schlafen", "tokens": ["Den", "Schlaf", "sei\u00b7nes", "Ruh\u00b7mes", "zu", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "so steht er da gro\u00df und allein.", "tokens": ["so", "steht", "er", "da", "gro\u00df", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Er h\u00f6rte Alldeutschlands Gebrummel", "tokens": ["Er", "h\u00f6r\u00b7te", "A\u00b7lldeutschlands", "Ge\u00b7brum\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "im Schmuck seines sp\u00e4rlichen Haars . . .", "tokens": ["im", "Schmuck", "sei\u00b7nes", "sp\u00e4r\u00b7li\u00b7chen", "Haars", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Die Zeitungen schrien: Hummel! Hummel!", "tokens": ["Die", "Zei\u00b7tun\u00b7gen", "schri\u00b7en", ":", "Hum\u00b7mel", "!", "Hum\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$.", "NE", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Bismarck, der dachte: Mars! Mars!", "tokens": ["Und", "Bis\u00b7marck", ",", "der", "dach\u00b7te", ":", "Mars", "!", "Mars", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "VVFIN", "$.", "NN", "$.", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Mit Genu\u00df: Mars \u2013 Mars \u2013", "tokens": ["Mit", "Ge\u00b7nu\u00df", ":", "Mars", "\u2013", "Mars", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "$(", "NE", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Mit Genu\u00df: Mars \u2013 Mars!", "tokens": ["Mit", "Ge\u00b7nu\u00df", ":", "Mars", "\u2013", "Mars", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "$(", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und Bismarck, der dachte: Mars \u2013 Mars \u2013!", "tokens": ["Und", "Bis\u00b7marck", ",", "der", "dach\u00b7te", ":", "Mars", "\u2013", "Mars", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "VVFIN", "$.", "NN", "$(", "NE", "$(", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Der Bismarck, der hat wohl drei Haare \u2013", "tokens": ["Der", "Bis\u00b7marck", ",", "der", "hat", "wohl", "drei", "Haa\u00b7re", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "CARD", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "denn daran erkennt man den Mann.", "tokens": ["denn", "da\u00b7ran", "er\u00b7kennt", "man", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und langsam vergingen die Jahre,", "tokens": ["Und", "lang\u00b7sam", "ver\u00b7gin\u00b7gen", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und es kamen die Wahlen heran.", "tokens": ["und", "es", "ka\u00b7men", "die", "Wah\u00b7len", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Doch als nun der Bismarck gerochen,", "tokens": ["Doch", "als", "nun", "der", "Bis\u00b7marck", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "wer heute im Reichstagshaus \u2013:", "tokens": ["wer", "heu\u00b7te", "im", "Reichs\u00b7tags\u00b7haus", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "da hat er kein Wort mehr gesprochen", "tokens": ["da", "hat", "er", "kein", "Wort", "mehr", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und ri\u00df sich die Haare aus!", "tokens": ["und", "ri\u00df", "sich", "die", "Haa\u00b7re", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mit Genu\u00df: Hummel \u2013 Hummel \u2013", "tokens": ["Mit", "Ge\u00b7nu\u00df", ":", "Hum\u00b7mel", "\u2013", "Hum\u00b7mel", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "$(", "NE", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Mit Genu\u00df: Mars \u2013 Mars \u2013", "tokens": ["Mit", "Ge\u00b7nu\u00df", ":", "Mars", "\u2013", "Mars", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "$(", "NE", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "und ri\u00df sich die Haare aus \u2013!", "tokens": ["und", "ri\u00df", "sich", "die", "Haa\u00b7re", "aus", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPR", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun kann er ruhig schlafen.", "tokens": ["Nun", "kann", "er", "ru\u00b7hig", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und da\u00df die Geschichte auch wahr \u2013:", "tokens": ["Und", "da\u00df", "die", "Ge\u00b7schich\u00b7te", "auch", "wahr", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "$(", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Seht nach im hamburger Hafen \u2013", "tokens": ["Seht", "nach", "im", "ham\u00b7bur\u00b7ger", "Ha\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Genu\u00df: Hummel \u2013 Hummel!", "tokens": ["Mit", "Ge\u00b7nu\u00df", ":", "Hum\u00b7mel", "\u2013", "Hum\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "$(", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mit Genu\u00df: Mars \u2013 Mars!", "tokens": ["Mit", "Ge\u00b7nu\u00df", ":", "Mars", "\u2013", "Mars", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "$(", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der Bismarck, der hat ja kein Haar \u2013!", "tokens": ["Der", "Bis\u00b7marck", ",", "der", "hat", "ja", "kein", "Haar", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "PIAT", "NN", "$(", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}