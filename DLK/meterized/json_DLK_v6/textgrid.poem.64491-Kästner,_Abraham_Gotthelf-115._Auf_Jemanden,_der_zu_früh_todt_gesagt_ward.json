{"textgrid.poem.64491": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "115. Auf Jemanden, der zu fr\u00fch todt gesagt ward", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fama, reicher noch an Zungen als an Ohren,", "tokens": ["Die", "Fa\u00b7ma", ",", "rei\u00b7cher", "noch", "an", "Zun\u00b7gen", "als", "an", "Oh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bereitet ihm zu fr\u00fch sein Grab.", "tokens": ["Be\u00b7rei\u00b7tet", "ihm", "zu", "fr\u00fch", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch w\u00e4lzt er sich, durchs Paradies der Thoren", "tokens": ["Noch", "w\u00e4lzt", "er", "sich", ",", "durchs", "Pa\u00b7ra\u00b7dies", "der", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den breiten, lust'gen Weg hinab.", "tokens": ["Den", "brei\u00b7ten", ",", "lust'\u00b7gen", "Weg", "hin\u00b7ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}