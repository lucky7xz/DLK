{"dta.poem.15764": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Baumeisterin Natur kannst du an ihrer Schwelle", "tokens": ["Bau\u00b7meis\u00b7te\u00b7rin", "Na\u00b7tur", "kannst", "du", "an", "ih\u00b7rer", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Belauschen wie den Flei\u00df der Bienen in der Zelle.", "tokens": ["Be\u00b7lau\u00b7schen", "wie", "den", "Flei\u00df", "der", "Bie\u00b7nen", "in", "der", "Zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehest wie sie baun, nach kannst du's doch nicht machen,", "tokens": ["Du", "sie\u00b7hest", "wie", "sie", "baun", ",", "nach", "kannst", "du's", "doch", "nicht", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VVINF", "$,", "APPR", "VMFIN", "PIS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und merkest nur daran: nicht Einsicht macht die Sachen.", "tokens": ["Und", "mer\u00b7kest", "nur", "da\u00b7ran", ":", "nicht", "Ein\u00b7sicht", "macht", "die", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$.", "PTKNEG", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als Sch\u00f6pfer- aber und Naturkraft wirkt zugleich", "tokens": ["Als", "Sch\u00f6p\u00b7fer", "a\u00b7ber", "und", "Na\u00b7tur\u00b7kraft", "wirkt", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "TRUNC", "KON", "KON", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Kunstsinn, der vollbringt Kunstreiches einsichtreich.", "tokens": ["Nur", "Kunst\u00b7sinn", ",", "der", "voll\u00b7bringt", "Kuns\u00b7trei\u00b7ches", "ein\u00b7sicht\u00b7reich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "VVFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der blinde Bildungstrieb, des Wissens scharfe Flammen,", "tokens": ["Der", "blin\u00b7de", "Bil\u00b7dungs\u00b7trieb", ",", "des", "Wis\u00b7sens", "schar\u00b7fe", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind beide mangelhaft, vollkommen nur zusammen.", "tokens": ["Sind", "bei\u00b7de", "man\u00b7gel\u00b7haft", ",", "voll\u00b7kom\u00b7men", "nur", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$,", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}