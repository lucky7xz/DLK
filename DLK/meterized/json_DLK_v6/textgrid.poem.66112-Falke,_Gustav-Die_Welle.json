{"textgrid.poem.66112": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Welle", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die wolfgehetzten Schafe, \u00e4ngsten", "tokens": ["Wie", "die", "wolf\u00b7ge\u00b7hetz\u00b7ten", "Scha\u00b7fe", ",", "\u00e4ngs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor dem Nordsturm her die wei\u00dfen Wellen,", "tokens": ["Vor", "dem", "Nord\u00b7sturm", "her", "die", "wei\u00b7\u00dfen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oder wie ein Trupp von scheuen Hengsten,", "tokens": ["O\u00b7der", "wie", "ein", "Trupp", "von", "scheu\u00b7en", "Hengs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die vorm Lasso durch die Steppe schnellen.", "tokens": ["Die", "vorm", "Las\u00b7so", "durch", "die", "Step\u00b7pe", "schnel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nein, ein Rudel kommt von bangen Weibern,", "tokens": ["Nein", ",", "ein", "Ru\u00b7del", "kommt", "von", "ban\u00b7gen", "Wei\u00b7bern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die, gedr\u00e4ngt um ihre K\u00f6nigin,", "tokens": ["Die", ",", "ge\u00b7dr\u00e4ngt", "um", "ih\u00b7re", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sie besch\u00fctzend mit den nackten Leibern,", "tokens": ["Sie", "be\u00b7sch\u00fct\u00b7zend", "mit", "den", "nack\u00b7ten", "Lei\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Fliehen vor dem k\u00fchnen Wiking hin.", "tokens": ["Flie\u00b7hen", "vor", "dem", "k\u00fch\u00b7nen", "Wi\u00b7king", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und in Mitten sie, die angstgejagte,", "tokens": ["Und", "in", "Mit\u00b7ten", "sie", ",", "die", "angst\u00b7ge\u00b7jag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Schamgequ\u00e4lte. In den eigenen Reichen,", "tokens": ["Scham\u00b7ge\u00b7qu\u00e4l\u00b7te", ".", "In", "den", "ei\u00b7ge\u00b7nen", "Rei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wo der R\u00e4uber ihr zu trotzen wagte,", "tokens": ["Wo", "der", "R\u00e4u\u00b7ber", "ihr", "zu", "trot\u00b7zen", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine Fl\u00fcchtige, sieht sie kein Entweichen.", "tokens": ["Ei\u00b7ne", "Fl\u00fcch\u00b7ti\u00b7ge", ",", "sieht", "sie", "kein", "Ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "N\u00e4her des Verha\u00dften Siegestrompeten,", "tokens": ["N\u00e4\u00b7her", "des", "Ver\u00b7ha\u00df\u00b7ten", "Sie\u00b7ge\u00b7strom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ach, wohin, vor der gewissen Schmach?", "tokens": ["Ach", ",", "wo\u00b7hin", ",", "vor", "der", "ge\u00b7wis\u00b7sen", "Schmach", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.7": {"text": "Lachend wird er in den Staub sie treten,", "tokens": ["La\u00b7chend", "wird", "er", "in", "den", "Staub", "sie", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und ihr Todbett wird ihm Brautgemach.", "tokens": ["Und", "ihr", "Tod\u00b7bett", "wird", "ihm", "Braut\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Fort, nur fort! Mit starken Armen teilt sie", "tokens": ["Fort", ",", "nur", "fort", "!", "Mit", "star\u00b7ken", "Ar\u00b7men", "teilt", "sie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "PTKVZ", "$.", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den bet\u00e4ubten Kn\u00e4ul der Dienerinnen.", "tokens": ["Den", "be\u00b7t\u00e4ub\u00b7ten", "Kn\u00e4ul", "der", "Die\u00b7ne\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unbehindert, freiren Fu\u00dfes, eilt sie", "tokens": ["Un\u00b7be\u00b7hin\u00b7dert", ",", "frei\u00b7ren", "Fu\u00b7\u00dfes", ",", "eilt", "sie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weit voraus, als w\u00e4r noch ein Entrinnen.", "tokens": ["Weit", "vo\u00b7raus", ",", "als", "w\u00e4r", "noch", "ein", "Ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KOKOM", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch der J\u00e4ger l\u00e4\u00dft das Wild nicht fahren,", "tokens": ["Doch", "der", "J\u00e4\u00b7ger", "l\u00e4\u00dft", "das", "Wild", "nicht", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schon ber\u00fchrt sein hei\u00dfer Atem sie,", "tokens": ["Schon", "be\u00b7r\u00fchrt", "sein", "hei\u00b7\u00dfer", "A\u00b7tem", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und an den entflochtenen feuchten Haaren", "tokens": ["Und", "an", "den", "ent\u00b7floch\u00b7te\u00b7nen", "feuch\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Zerrt er die Ersch\u00f6pfte in die Knie.", "tokens": ["Zerrt", "er", "die", "Er\u00b7sch\u00f6pf\u00b7te", "in", "die", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Schwanken. St\u00fcrzen. Zu des Starken F\u00fc\u00dfen", "tokens": ["Schwan\u00b7ken", ".", "St\u00fcr\u00b7zen", ".", "Zu", "des", "Star\u00b7ken", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Strecken kraftlos sich die sch\u00f6nen Glieder,", "tokens": ["Stre\u00b7cken", "kraft\u00b7los", "sich", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und er taumelt, seine Gier zu b\u00fc\u00dfen,", "tokens": ["Und", "er", "tau\u00b7melt", ",", "sei\u00b7ne", "Gier", "zu", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf die weichen Liebeskissen nieder.", "tokens": ["Auf", "die", "wei\u00b7chen", "Lie\u00b7bes\u00b7kis\u00b7sen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schrei der Scham. Ein purpurn Widerstreben.", "tokens": ["Schrei", "der", "Scham", ".", "Ein", "pur\u00b7purn", "Wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Herrenrecht und Sieg und Flammenbrust.", "tokens": ["Her\u00b7ren\u00b7recht", "und", "Sieg", "und", "Flam\u00b7men\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Schande w\u00fcrgt ein k\u00f6nigliches Leben,", "tokens": ["Schan\u00b7de", "w\u00fcrgt", "ein", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und der Seufzer stirbt im Ach der Lust.", "tokens": ["Und", "der", "Seuf\u00b7zer", "stirbt", "im", "Ach", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}