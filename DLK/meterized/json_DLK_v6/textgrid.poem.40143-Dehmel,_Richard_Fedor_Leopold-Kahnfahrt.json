{"textgrid.poem.40143": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Kahnfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sacht von Baum zu Baum die lauen", "tokens": ["Sacht", "von", "Baum", "zu", "Baum", "die", "lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "APPR", "NE", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wellen meinen Nachen schaukeln,", "tokens": ["Wel\u00b7len", "mei\u00b7nen", "Na\u00b7chen", "schau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Traumesbl\u00fcten gleich die blauen", "tokens": ["Trau\u00b7mes\u00b7bl\u00fc\u00b7ten", "gleich", "die", "blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schilf libellen mich umgaukeln,", "tokens": ["Schilf", "li\u00b7bel\u00b7len", "mich", "um\u00b7gau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schatten k\u00fcssen die schlummernde Flut.", "tokens": ["Schat\u00b7ten", "k\u00fcs\u00b7sen", "die", "schlum\u00b7mern\u00b7de", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Durch die dunkeln Locken der Erlen,", "tokens": ["Durch", "die", "dun\u00b7keln", "Lo\u00b7cken", "der", "Er\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "welch ein funkelndes Verschwenden,", "tokens": ["welch", "ein", "fun\u00b7keln\u00b7des", "Ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "streut die Sonne aus goldenen H\u00e4nden", "tokens": ["streut", "die", "Son\u00b7ne", "aus", "gol\u00b7de\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "silberne Perlen", "tokens": ["sil\u00b7ber\u00b7ne", "Per\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "in die smaragdene Schale der Flut.", "tokens": ["in", "die", "sma\u00b7rag\u00b7de\u00b7ne", "Scha\u00b7le", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "An den grauen S\u00e4ulen schweben", "tokens": ["An", "den", "grau\u00b7en", "S\u00e4u\u00b7len", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "still nach Oben meine Tr\u00e4ume,", "tokens": ["still", "nach", "O\u00b7ben", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die B\u00e4ume", "tokens": ["wo", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "sehnend ihre Arme heben", "tokens": ["seh\u00b7nend", "ih\u00b7re", "Ar\u00b7me", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "in des Himmels leuchtende Flut.", "tokens": ["in", "des", "Him\u00b7mels", "leuch\u00b7ten\u00b7de", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Leise aus dem lichten Scheine", "tokens": ["Lei\u00b7se", "aus", "dem", "lich\u00b7ten", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weht ein Blatt zu meinen F\u00fc\u00dfen", "tokens": ["weht", "ein", "Blatt", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nieder, \u2013 und des Friedens reine", "tokens": ["nie\u00b7der", ",", "\u2013", "und", "des", "Frie\u00b7dens", "rei\u00b7ne"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "$(", "KON", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wei\u00dfe Taube seh' ich gr\u00fc\u00dfen,", "tokens": ["wei\u00b7\u00dfe", "Tau\u00b7be", "seh'", "ich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "fernher gr\u00fc\u00dfen", "tokens": ["fern\u00b7her", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "meiner Seele dunkle Flut.", "tokens": ["mei\u00b7ner", "See\u00b7le", "dunk\u00b7le", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}