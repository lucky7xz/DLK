{"dta.poem.16176": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "230.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Punkt ist eins f\u00fcr sich, zwei Punkte sind der Strich,", "tokens": ["Der", "Punkt", "ist", "eins", "f\u00fcr", "sich", ",", "zwei", "Punk\u00b7te", "sind", "der", "Strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "APPR", "PRF", "$,", "CARD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei Striche Fl\u00e4chenraum, vier Fl\u00e4chen k\u00f6rperlich.", "tokens": ["Drei", "Stri\u00b7che", "Fl\u00e4\u00b7chen\u00b7raum", ",", "vier", "Fl\u00e4\u00b7chen", "k\u00f6r\u00b7per\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$,", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sobald die Vierzahl ist, eins zwei drei vier, vorhanden,", "tokens": ["So\u00b7bald", "die", "Vier\u00b7zahl", "ist", ",", "eins", "zwei", "drei", "vier", ",", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "CARD", "CARD", "CARD", "CARD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist aus dem Punkt, dem Nichts, die K\u00f6rperwelt entstanden.", "tokens": ["Ist", "aus", "dem", "Punkt", ",", "dem", "Nichts", ",", "die", "K\u00f6r\u00b7per\u00b7welt", "ent\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und aus eins zwei drei vier mu\u00df alle Zahl bestehn,", "tokens": ["Und", "aus", "eins", "zwei", "drei", "vier", "mu\u00df", "al\u00b7le", "Zahl", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "CARD", "CARD", "CARD", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wer vier drei zwei eins zusammenz\u00e4hlt, hat zehn.", "tokens": ["Denn", "wer", "vier", "drei", "zwei", "eins", "zu\u00b7sam\u00b7men\u00b7z\u00e4hlt", ",", "hat", "zehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "CARD", "CARD", "CARD", "PIS", "VVFIN", "$,", "VAFIN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}