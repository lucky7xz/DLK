{"dta.poem.10662": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv Westwind/ der im Lentz den Lust der Felder heget/", "tokens": ["Dv", "West\u00b7wind", "/", "der", "im", "Lentz", "den", "Lust", "der", "Fel\u00b7der", "he\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ART", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Hastu gar keinen Staub gebracht mit dir anher/", "tokens": ["Has\u00b7tu", "gar", "kei\u00b7nen", "Staub", "ge\u00b7bracht", "mit", "dir", "an\u00b7her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "NN", "VVPP", "APPR", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den mein geliebter Buhl an seinen F\u00fcssen treget?", "tokens": ["Den", "mein", "ge\u00b7lieb\u00b7ter", "Buhl", "an", "sei\u00b7nen", "F\u00fcs\u00b7sen", "tre\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach hastu/ wenn sein Hertz au\u00df Liebe sich bewege", "tokens": ["Ach", "has\u00b7tu", "/", "wenn", "sein", "Hertz", "au\u00df", "Lie\u00b7be", "sich", "be\u00b7we\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$(", "KOUS", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht seines Athems was gefangen ohngefehr/", "tokens": ["Nicht", "sei\u00b7nes", "A\u00b7thems", "was", "ge\u00b7fan\u00b7gen", "ohn\u00b7ge\u00b7fehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "PWS", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd jhn durch deine Lufft geraubt/ als du vnd er", "tokens": ["Vnd", "jhn", "durch", "dei\u00b7ne", "Lufft", "ge\u00b7raubt", "/", "als", "du", "vnd", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$(", "KOUS", "PPER", "KON", "PPER"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der Venus den Geruch der Lieblichkeit erreget?", "tokens": ["Der", "Ve\u00b7nus", "den", "Ge\u00b7ruch", "der", "Lieb\u00b7lich\u00b7keit", "er\u00b7re\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Viel mehr ist Spanien von mir als Rom geschetzt/", "tokens": ["Viel", "mehr", "ist", "Spa\u00b7ni\u00b7en", "von", "mir", "als", "Rom", "ge\u00b7schetzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "APPR", "PPER", "KOUS", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob es gleich vberal mit vielem Blut genetzt/", "tokens": ["Ob", "es", "gleich", "vbe\u00b7ral", "mit", "vie\u00b7lem", "Blut", "ge\u00b7netzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PIS", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Weil mein Buhl drinnen ist/ so mu\u00df ich es erh\u00f6hen.", "tokens": ["Weil", "mein", "Buhl", "drin\u00b7nen", "ist", "/", "so", "mu\u00df", "ich", "es", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "$(", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach Westwind/ hole mir ein st\u00e4ublin diser stund/", "tokens": ["Ach", "West\u00b7wind", "/", "ho\u00b7le", "mir", "ein", "st\u00e4ub\u00b7lin", "di\u00b7ser", "stund", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein einig seufftzerlin au\u00df seinem roten Mund/", "tokens": ["Ein", "ei\u00b7nig", "seufft\u00b7zer\u00b7lin", "au\u00df", "sei\u00b7nem", "ro\u00b7ten", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier thue es in den Brieff/ darauff die Reimen stehen.", "tokens": ["Hier", "thue", "es", "in", "den", "Brieff", "/", "dar\u00b7auff", "die", "Rei\u00b7men", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}