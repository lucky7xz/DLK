{"textgrid.poem.60144": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund drey Ungewitter hiengen", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund drey Ungewitter hiengen", "tokens": ["O", "Freund", "drey", "Un\u00b7ge\u00b7wit\u00b7ter", "hien\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herunter drohend \u00fcber mir;", "tokens": ["Her\u00b7un\u00b7ter", "dro\u00b7hend", "\u00fc\u00b7ber", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch konnt ich unersch\u00fcttert singen:", "tokens": ["Doch", "konnt", "ich", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott, du bist gro\u00df! dich loben wir!", "tokens": ["Gott", ",", "du", "bist", "gro\u00df", "!", "dich", "lo\u00b7ben", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er fuhr auf Wolken. Schrecklich rollten", "tokens": ["Er", "fuhr", "auf", "Wol\u00b7ken", ".", "Schreck\u00b7lich", "roll\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die R\u00e4der seines Wagens fort", "tokens": ["Die", "R\u00e4\u00b7der", "sei\u00b7nes", "Wa\u00b7gens", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Donner, die uns t\u00f6dten sollten,", "tokens": ["Und", "Don\u00b7ner", ",", "die", "uns", "t\u00f6d\u00b7ten", "soll\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwarteten sein leztes Wort.", "tokens": ["Er\u00b7war\u00b7te\u00b7ten", "sein", "lez\u00b7tes", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gluth flog von seinem Angesichte", "tokens": ["Gluth", "flog", "von", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rund um ihn her, als s\u00e4\u00dfe schon,", "tokens": ["Rund", "um", "ihn", "her", ",", "als", "s\u00e4\u00b7\u00dfe", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zum feyerlichen Weltgerichte,", "tokens": ["Zum", "fey\u00b7er\u00b7li\u00b7chen", "Welt\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Richter, auf dem Wolken Thron.", "tokens": ["Der", "Rich\u00b7ter", ",", "auf", "dem", "Wol\u00b7ken", "Thron", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Frommen beteten entgegen;", "tokens": ["Die", "From\u00b7men", "be\u00b7te\u00b7ten", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Furcht nahm das Herz des S\u00fcnders ein.", "tokens": ["Furcht", "nahm", "das", "Herz", "des", "S\u00fcn\u00b7ders", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So zittern Sclaven vor den Schl\u00e4gen", "tokens": ["So", "zit\u00b7tern", "Scla\u00b7ven", "vor", "den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Herren, den sie sonst nicht scheun.", "tokens": ["Des", "Her\u00b7ren", ",", "den", "sie", "sonst", "nicht", "scheun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So st\u00fcrzet, bey emp\u00f6rter Welle", "tokens": ["So", "st\u00fcr\u00b7zet", ",", "bey", "em\u00b7p\u00f6r\u00b7ter", "Wel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der rohe Schiffmann auf das Knie;", "tokens": ["Der", "ro\u00b7he", "Schiff\u00b7mann", "auf", "das", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und so f\u00e4llt, an des Grabes Schwelle", "tokens": ["Und", "so", "f\u00e4llt", ",", "an", "des", "Gra\u00b7bes", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Frey-Geist in Melancholie.", "tokens": ["Der", "Frey\u00b7Geist", "in", "Me\u00b7lan\u00b7cho\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Der j\u00fcngsten Gattin, weiches Herze,", "tokens": ["Der", "j\u00fcng\u00b7sten", "Gat\u00b7tin", ",", "wei\u00b7ches", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sanft wie Blumen auf der Flur,", "tokens": ["So", "sanft", "wie", "Blu\u00b7men", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erstaunte vor der Wolken Schw\u00e4rze", "tokens": ["Er\u00b7staun\u00b7te", "vor", "der", "Wol\u00b7ken", "Schw\u00e4r\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlte Schrecken der Natur.", "tokens": ["Und", "f\u00fchl\u00b7te", "Schre\u00b7cken", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In ihres Freundes Arm geschlossen,", "tokens": ["In", "ih\u00b7res", "Freun\u00b7des", "Arm", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verseufzte sie die Furcht, und lag", "tokens": ["Ver\u00b7seufz\u00b7te", "sie", "die", "Furcht", ",", "und", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An seiner Brust, als Strahlen schossen,", "tokens": ["An", "sei\u00b7ner", "Brust", ",", "als", "Strah\u00b7len", "schos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Nacht verwandelten in Tag;", "tokens": ["Und", "Nacht", "ver\u00b7wan\u00b7del\u00b7ten", "in", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Als wieder uns mit Kriegs-Ger\u00fcste", "tokens": ["Als", "wie\u00b7der", "uns", "mit", "Kriegs\u00b7Ge\u00b7r\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ober-Welt bewafnet schien,", "tokens": ["Die", "O\u00b7ber\u00b7Welt", "be\u00b7waf\u00b7net", "schien", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Babylons, und Tyrus L\u00fcste,", "tokens": ["Und", "Ba\u00b7by\u00b7lons", ",", "und", "Ty\u00b7rus", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "KON", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Magdeburg, gen Himmel schrien.", "tokens": ["Aus", "Mag\u00b7de\u00b7burg", ",", "gen", "Him\u00b7mel", "schri\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch st\u00e4rker, als des Frevlers S\u00fcnde,", "tokens": ["Doch", "st\u00e4r\u00b7ker", ",", "als", "des", "Frev\u00b7lers", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War des Gerechten Bittgeschrey.", "tokens": ["War", "des", "Ge\u00b7rech\u00b7ten", "Bitt\u00b7ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott sprach! da f\u00fchrten Wirbelwinde", "tokens": ["Gott", "sprach", "!", "da", "f\u00fchr\u00b7ten", "Wir\u00b7bel\u00b7win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Donnerwagen schnell vorbey.", "tokens": ["Den", "Don\u00b7ner\u00b7wa\u00b7gen", "schnell", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er fuhr herauf, und ihm entgegen", "tokens": ["Er", "fuhr", "her\u00b7auf", ",", "und", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lobjubelte der weite Raum;", "tokens": ["Lob\u00b7ju\u00b7bel\u00b7te", "der", "wei\u00b7te", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und auf uns tr\u00e4ufelte nur Seegen", "tokens": ["Und", "auf", "uns", "tr\u00e4u\u00b7fel\u00b7te", "nur", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Herab von seines Kleides Saum.", "tokens": ["Her\u00b7ab", "von", "sei\u00b7nes", "Klei\u00b7des", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}