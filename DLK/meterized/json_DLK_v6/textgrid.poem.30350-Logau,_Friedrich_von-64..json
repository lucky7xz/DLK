{"textgrid.poem.30350": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundin de\u00df Ochsens, Pasiphae, h\u00f6re,", "tokens": ["Freun\u00b7din", "de\u00df", "Och\u00b7sens", ",", "Pa\u00b7si\u00b7phae", ",", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie man dir b\u00f6slich stahl weiland dein Ehre!", "tokens": ["Wie", "man", "dir", "b\u00f6s\u00b7lich", "stahl", "wei\u00b7land", "dein", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00dcblich ists heute noch; artliche Kinder", "tokens": ["\u00dcb\u00b7lich", "ists", "heu\u00b7te", "noch", ";", "art\u00b7li\u00b7che", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$.", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wehlen zu M\u00e4nnern wie Esel so Rinder.", "tokens": ["Weh\u00b7len", "zu", "M\u00e4n\u00b7nern", "wie", "E\u00b7sel", "so", "Rin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KOKOM", "NN", "ADV", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}