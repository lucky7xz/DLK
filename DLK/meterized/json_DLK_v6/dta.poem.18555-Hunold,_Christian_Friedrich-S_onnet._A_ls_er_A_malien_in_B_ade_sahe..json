{"dta.poem.18555": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "S onnet.  \n  A ls er  A malien in  B ade  \n sahe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Hilff Himmel welcher Schmuck der Perlen weissen Glieder!", "tokens": ["Hilff", "Him\u00b7mel", "wel\u00b7cher", "Schmuck", "der", "Per\u00b7len", "weis\u00b7sen", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PWAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df mir der zarte Leib an seiner Bl\u00f6sse sehn!", "tokens": ["Lie\u00df", "mir", "der", "zar\u00b7te", "Leib", "an", "sei\u00b7ner", "Bl\u00f6s\u00b7se", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Br\u00fcste lagen hier gantz ungew\u00f6hnlich sch\u00f6n.", "tokens": ["Die", "Br\u00fcs\u00b7te", "la\u00b7gen", "hier", "gantz", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die H\u00e4nde spritzten sie an Bauche hin und wieder/", "tokens": ["Die", "H\u00e4n\u00b7de", "spritz\u00b7ten", "sie", "an", "Bau\u00b7che", "hin", "und", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie hub das eine Bein zu waschen auff und nieder/", "tokens": ["Sie", "hub", "das", "ei\u00b7ne", "Bein", "zu", "wa\u00b7schen", "auff", "und", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "NN", "PTKZU", "VVINF", "APPR", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df mir das Paradie\u00df recht offen konnte stehn.", "tokens": ["Da\u00df", "mir", "das", "Pa\u00b7ra\u00b7die\u00df", "recht", "of\u00b7fen", "konn\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich sang': Amalia/ la\u00df mich doch zu dir gehn/", "tokens": ["Ich", "sang'", ":", "A\u00b7ma\u00b7lia", "/", "la\u00df", "mich", "doch", "zu", "dir", "gehn", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "$(", "VVIMP", "PPER", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Alleine Scham und Zorn verstimmten meine Lieder.", "tokens": ["Al\u00b7lei\u00b7ne", "Scham", "und", "Zorn", "ver\u00b7stimm\u00b7ten", "mei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach Venus unsrer Zeit! du bist Diana nicht/", "tokens": ["Ach", "Ve\u00b7nus", "uns\u00b7rer", "Zeit", "!", "du", "bist", "Di\u00b7a\u00b7na", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "NE", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die in den Brunnen gleich des Todes Urtheil spricht/", "tokens": ["Die", "in", "den", "Brun\u00b7nen", "gleich", "des", "To\u00b7des", "Ur\u00b7theil", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch sucht dein strenger Grimm mein Leben zu verk\u00fcrtzen/", "tokens": ["Doch", "sucht", "dein", "stren\u00b7ger", "Grimm", "mein", "Le\u00b7ben", "zu", "ver\u00b7k\u00fcrt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NE", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So f\u00e4llt Narcissus dort in Brunnen/ den er sieht:", "tokens": ["So", "f\u00e4llt", "Nar\u00b7cis\u00b7sus", "dort", "in", "Brun\u00b7nen", "/", "den", "er", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "NN", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weil nun mein stoltzer Geist zu gleichen Spiegel flieht/", "tokens": ["Weil", "nun", "mein", "stolt\u00b7zer", "Geist", "zu", "glei\u00b7chen", "Spie\u00b7gel", "flieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So kanst du mich zur Straff' in deinen Brunnen st\u00fcrtzen.", "tokens": ["So", "kanst", "du", "mich", "zur", "Straff'", "in", "dei\u00b7nen", "Brun\u00b7nen", "st\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}