: ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Doch mitten in der weiten W\u00fcste", "tokens": ["Doch", "mit\u00b7ten", "in", "der", "wei\u00b7ten", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Reicht Gott und Tugend ihm die Arme.", "tokens": ["Reicht", "Gott", "und", "Tu\u00b7gend", "ihm", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Du h\u00f6chster Vater aller Welt,", "tokens": ["Du", "h\u00f6chs\u00b7ter", "Va\u00b7ter", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Gott auch des kleinsten Wurms, la\u00df uns in Schwei\u00df und Sorgen", "tokens": ["Gott", "auch", "des", "kleins\u00b7ten", "Wurms", ",", "la\u00df", "uns", "in", "Schwei\u00df", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.59": {"text": "Die gantze Last der Tugend tragen;", "tokens": ["Die", "gant\u00b7ze", "Last", "der", "Tu\u00b7gend", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Nur aber la\u00df mich nie in Niedertr\u00e4chtigkeit", "tokens": ["Nur", "a\u00b7ber", "la\u00df", "mich", "nie", "in", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Der schwartzen Bosheit dienstbar werden.", "tokens": ["Der", "schwart\u00b7zen", "Bos\u00b7heit", "dienst\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Verbleibe du mir stets, o meine Poesie,", "tokens": ["Ver\u00b7blei\u00b7be", "du", "mir", "stets", ",", "o", "mei\u00b7ne", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.63": {"text": "In deiner hohen Reinigkeit.", "tokens": ["In", "dei\u00b7ner", "ho\u00b7hen", "Rei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Las sie zum Dienst der schn\u00f6den Seelen,", "tokens": ["Las", "sie", "zum", "Dienst", "der", "schn\u00f6\u00b7den", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.65": {"text": "O Himmel, nie erniedrigt werden.", "tokens": ["O", "Him\u00b7mel", ",", "nie", "er\u00b7nied\u00b7rigt", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Was vor ein anmuthsreicher Strahl", "tokens": ["Was", "vor", "ein", "an\u00b7muths\u00b7rei\u00b7cher", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Durchschimmert meine Nacht und mahlt die finstern Nebel,", "tokens": ["Durch\u00b7schim\u00b7mert", "mei\u00b7ne", "Nacht", "und", "mahlt", "die", "fins\u00b7tern", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Aus welchem freudenvollen Himmel \u2013 \u2013 \u2013", "tokens": ["Aus", "wel\u00b7chem", "freu\u00b7den\u00b7vol\u00b7len", "Him\u00b7mel", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "O, Doris, bist du es, du l\u00e4chelst, und es taget.", "tokens": ["O", ",", "Do\u00b7ris", ",", "bist", "du", "es", ",", "du", "l\u00e4\u00b7chelst", ",", "und", "es", "ta\u00b7get", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VAFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Dein Blick erheitert unsre Tugend,", "tokens": ["Dein", "Blick", "er\u00b7hei\u00b7tert", "uns\u00b7re", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.71": {"text": "Und du lachst auch, du kleine Munterkeit,", "tokens": ["Und", "du", "lachst", "auch", ",", "du", "klei\u00b7ne", "Mun\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.72": {"text": "Du, der verschwundnen Tage Lust,", "tokens": ["Du", ",", "der", "ver\u00b7schwund\u00b7nen", "Ta\u00b7ge", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "O Hilas, auf der Doris Schoosse.", "tokens": ["O", "Hi\u00b7las", ",", "auf", "der", "Do\u00b7ris", "Schoos\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Ja schertzt vergn\u00fcgt. Die\u00df tilgt mein Sorgen.", "tokens": ["Ja", "schertzt", "ver\u00b7gn\u00fcgt", ".", "Die\u00df", "tilgt", "mein", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "VVPP", "$.", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}