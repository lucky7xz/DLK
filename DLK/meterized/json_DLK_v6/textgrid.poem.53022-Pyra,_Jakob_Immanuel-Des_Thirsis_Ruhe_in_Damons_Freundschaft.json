{"textgrid.poem.53022": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des Thirsis Ruhe in Damons Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4tzbarste St\u00fctze meines Wohls,", "tokens": ["Sch\u00e4tz\u00b7bars\u00b7te", "St\u00fct\u00b7ze", "mei\u00b7nes", "Wohls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mitleidender Gefehrt, trostreichster, liebster Zeuge", "tokens": ["Mit\u00b7lei\u00b7den\u00b7der", "Ge\u00b7fehrt", ",", "trost\u00b7reichs\u00b7ter", ",", "liebs\u00b7ter", "Zeu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der stillen Sorgen banger Tage,", "tokens": ["Der", "stil\u00b7len", "Sor\u00b7gen", "ban\u00b7ger", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du unbestechlicher Bewahrer meiner Treu,", "tokens": ["Du", "un\u00b7be\u00b7stech\u00b7li\u00b7cher", "Be\u00b7wah\u00b7rer", "mei\u00b7ner", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und der, in deinem edlen Busen", "tokens": ["Und", "der", ",", "in", "dei\u00b7nem", "ed\u00b7len", "Bu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ern\u00e4hrten, ewgen Gluth der Redlichkeit.", "tokens": ["Er\u00b7n\u00e4hr\u00b7ten", ",", "ew\u00b7gen", "Gluth", "der", "Red\u00b7lich\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach Freund, ach Damon, la\u00df mein Hertz", "tokens": ["Ach", "Freund", ",", "ach", "Da\u00b7mon", ",", "la\u00df", "mein", "Hertz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "XY", "NE", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In deinem mir stets ofnen Hertzen", "tokens": ["In", "dei\u00b7nem", "mir", "stets", "of\u00b7nen", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Freystatt f\u00fcr den Kummer suchen.", "tokens": ["Die", "Freys\u00b7tatt", "f\u00fcr", "den", "Kum\u00b7mer", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ja, ja, du \u00f6fnest deinen Arm!", "tokens": ["Ja", ",", "ja", ",", "du", "\u00f6f\u00b7nest", "dei\u00b7nen", "Arm", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich flieh ins Heiligthum von deinem ofnen Hertzen.", "tokens": ["Ich", "flieh", "ins", "Hei\u00b7lig\u00b7thum", "von", "dei\u00b7nem", "of\u00b7nen", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hier leg ich meine Lasten nieder,", "tokens": ["Hier", "leg", "ich", "mei\u00b7ne", "Las\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und mein beklemmter Geist erholt sich, sch\u00f6pfet Luft.", "tokens": ["Und", "mein", "be\u00b7klemm\u00b7ter", "Geist", "er\u00b7holt", "sich", ",", "sch\u00f6p\u00b7fet", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O welch ein Einflu\u00df s\u00fcsses Trostes!", "tokens": ["O", "welch", "ein", "Ein\u00b7flu\u00df", "s\u00fcs\u00b7ses", "Tros\u00b7tes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "O welche Linderung, o welch ein Meer voll Lust", "tokens": ["O", "wel\u00b7che", "Lin\u00b7de\u00b7rung", ",", "o", "welch", "ein", "Meer", "voll", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "$,", "FM", "PWAT", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ergiesset sich in meine Brust!", "tokens": ["Er\u00b7gies\u00b7set", "sich", "in", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "O gl\u00fcckliche, o hohe St\u00e4rckung!", "tokens": ["O", "gl\u00fcck\u00b7li\u00b7che", ",", "o", "ho\u00b7he", "St\u00e4r\u00b7ckung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "O theure Freundschaft sey gesegnet!", "tokens": ["O", "theu\u00b7re", "Freund\u00b7schaft", "sey", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ja, Freund, sonst find ich nirgends Ruh,", "tokens": ["Ja", ",", "Freund", ",", "sonst", "find", "ich", "nir\u00b7gends", "Ruh", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Als nur in Damons Brust, und dort in Gottes Himmel,", "tokens": ["Als", "nur", "in", "Da\u00b7mons", "Brust", ",", "und", "dort", "in", "Got\u00b7tes", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NE", "NN", "$,", "KON", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gest\u00e4rckt durch mein und deine Tugend;", "tokens": ["Ge\u00b7st\u00e4rckt", "durch", "mein", "und", "dei\u00b7ne", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Tugend, die nur jetzt der Weisheit stilles Licht,", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "nur", "jetzt", "der", "Weis\u00b7heit", "stil\u00b7les", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von Gott geschenckt, im Nebel f\u00fchret.", "tokens": ["Von", "Gott", "ge\u00b7schenckt", ",", "im", "Ne\u00b7bel", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ich f\u00fchle das Gewicht von der Be\u00e4ngstigung,", "tokens": ["Ich", "f\u00fch\u00b7le", "das", "Ge\u00b7wicht", "von", "der", "Be\u00b7\u00e4ngs\u00b7ti\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich walle oft in tr\u00fcber Nacht,", "tokens": ["Ich", "wal\u00b7le", "oft", "in", "tr\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wie mitten in dem Sturm der Wellen,", "tokens": ["Wie", "mit\u00b7ten", "in", "dem", "Sturm", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Jedoch ihr Schimmer theilt die Schatten.", "tokens": ["Je\u00b7doch", "ihr", "Schim\u00b7mer", "theilt", "die", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Ich gebe keine Gro\u00dfmuth vor,", "tokens": ["Ich", "ge\u00b7be", "kei\u00b7ne", "Gro\u00df\u00b7muth", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die aus der Felsen-Brust der Menschen ausgestossen;", "tokens": ["Die", "aus", "der", "Fel\u00b7sen\u00b7Brust", "der", "Men\u00b7schen", "aus\u00b7ge\u00b7stos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Gott wills, ich f\u00fchle meine Sorgen;", "tokens": ["Gott", "wills", ",", "ich", "f\u00fch\u00b7le", "mei\u00b7ne", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ein Hiob geht bey mir zehntausend Catons vor.", "tokens": ["Ein", "Hiob", "geht", "bey", "mir", "zehn\u00b7tau\u00b7send", "Ca\u00b7tons", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPER", "VVPP", "NE", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Der tobet wider Welt und Himmel;", "tokens": ["Der", "to\u00b7bet", "wi\u00b7der", "Welt", "und", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der tr\u00e4gt die Last und seufzt, doch murrt er nicht dabey.", "tokens": ["Der", "tr\u00e4gt", "die", "Last", "und", "seufzt", ",", "doch", "murrt", "er", "nicht", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Jedoch, das eitle Zeitliche", "tokens": ["Je\u00b7doch", ",", "das", "eit\u00b7le", "Zeit\u00b7li\u00b7che"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.35": {"text": "Ist wohl vielleicht des P\u00f6bels Sorge?", "tokens": ["Ist", "wohl", "viel\u00b7leicht", "des", "P\u00f6\u00b7bels", "Sor\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Freund, kan sonst was die Tugend dr\u00fccken?", "tokens": ["Freund", ",", "kan", "sonst", "was", "die", "Tu\u00b7gend", "dr\u00fc\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "ADV", "PWS", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Doch Damon f\u00fchlet es ja auch.", "tokens": ["Doch", "Da\u00b7mon", "f\u00fch\u00b7let", "es", "ja", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ja, Freund, dein Beyspiel ist das Zeugni\u00df dieser Wahrheit.", "tokens": ["Ja", ",", "Freund", ",", "dein", "Bey\u00b7spiel", "ist", "das", "Zeug\u00b7ni\u00df", "die\u00b7ser", "Wahr\u00b7heit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Tugendhaftige, der P\u00f6bel,", "tokens": ["Der", "Tu\u00b7gend\u00b7haf\u00b7ti\u00b7ge", ",", "der", "P\u00f6\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Stehn unter einer Last, doch himmelweit entfernt.", "tokens": ["Stehn", "un\u00b7ter", "ei\u00b7ner", "Last", ",", "doch", "him\u00b7mel\u00b7weit", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Des P\u00f6bels Ungl\u00fcck sind die Laster,", "tokens": ["Des", "P\u00f6\u00b7bels", "Un\u00b7gl\u00fcck", "sind", "die", "Las\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Des Tugendhaftigen nichts, als sein Ungl\u00fcck selbst.", "tokens": ["Des", "Tu\u00b7gend\u00b7haf\u00b7ti\u00b7gen", "nichts", ",", "als", "sein", "Un\u00b7gl\u00fcck", "selbst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "KOUS", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der P\u00f6bel mehrt sich seine Quaal", "tokens": ["Der", "P\u00f6\u00b7bel", "mehrt", "sich", "sei\u00b7ne", "Qua\u00b7al"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Durch Niedertr\u00e4chtigkeit und Klagen.", "tokens": ["Durch", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit", "und", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Hier seufzt der Mensch, die Tugend tr\u00f6stet;", "tokens": ["Hier", "seufzt", "der", "Mensch", ",", "die", "Tu\u00b7gend", "tr\u00f6s\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Wie, wenn der Sturmwind den Orest", "tokens": ["Wie", ",", "wenn", "der", "Sturm\u00b7wind", "den", "O\u00b7rest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Von seinem Pylades in eine W\u00fcste schmeisset;", "tokens": ["Von", "sei\u00b7nem", "Py\u00b7la\u00b7des", "in", "ei\u00b7ne", "W\u00fcs\u00b7te", "schmeis\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der an dem Ufer gantz verlassen,", "tokens": ["Der", "an", "dem", "U\u00b7fer", "gantz", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Der mitten aus der Fluth die H\u00e4nde ringt und streckt,", "tokens": ["Der", "mit\u00b7ten", "aus", "der", "Fluth", "die", "H\