, "line.4": {"text": "Inde\u00df ihr gold'nes Haar im Winde wallte?", "tokens": ["In\u00b7de\u00df", "ihr", "gold'\u00b7nes", "Haar", "im", "Win\u00b7de", "wall\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Dort ist der Osten, dort, drei Schuh im Grund,", "tokens": ["Dort", "ist", "der", "Os\u00b7ten", ",", "dort", ",", "drei", "Schuh", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "$,", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort steht die Urne und in ihrem Rund", "tokens": ["Dort", "steht", "die", "Ur\u00b7ne", "und", "in", "ih\u00b7rem", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein wildes Herz zerst\u00e4ubt zu Aschenflocken;", "tokens": ["Ein", "wil\u00b7des", "Herz", "zer\u00b7st\u00e4ubt", "zu", "A\u00b7schen\u00b7flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier lagert sich der Traum vom Opferhain,", "tokens": ["Hier", "la\u00b7gert", "sich", "der", "Traum", "vom", "Op\u00b7fer\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und finster sch\u00fctteln \u00fcber diesen Stein", "tokens": ["Und", "fi