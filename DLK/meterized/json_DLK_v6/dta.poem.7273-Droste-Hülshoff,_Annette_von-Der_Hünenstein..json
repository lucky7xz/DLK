{"dta.poem.7273": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der H\u00fcnenstein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Zeit der Scheide zwischen Nacht und Tag,", "tokens": ["Zur", "Zeit", "der", "Schei\u00b7de", "zwi\u00b7schen", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als wie ein siecher Greis die Haide lag", "tokens": ["Als", "wie", "ein", "sie\u00b7cher", "Greis", "die", "Hai\u00b7de", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ihr Gest\u00f6hn des Mooses Teppich regte,", "tokens": ["Und", "ihr", "Ge\u00b7st\u00f6hn", "des", "Moo\u00b7ses", "Tep\u00b7pich", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Krankhafte Funken im verwirrten Haar", "tokens": ["Krank\u00b7haf\u00b7te", "Fun\u00b7ken", "im", "ver\u00b7wirr\u00b7ten", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Elektrisch blitzten, und, ein dunkler Mahr,", "tokens": ["E\u00b7lekt\u00b7risch", "blitz\u00b7ten", ",", "und", ",", "ein", "dunk\u00b7ler", "Mahr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sich \u00fcber sie die Wolkenschichte legte;", "tokens": ["Sich", "\u00fc\u00b7ber", "sie", "die", "Wol\u00b7ken\u00b7schich\u00b7te", "leg\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu dieser D\u00e4mmerstunde war's, als ich", "tokens": ["Zu", "die\u00b7ser", "D\u00e4m\u00b7mer\u00b7stun\u00b7de", "wa\u00b7r's", ",", "als", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Einsam hinaus mit meinen Sorgen schlich,", "tokens": ["Ein\u00b7sam", "hin\u00b7aus", "mit", "mei\u00b7nen", "Sor\u00b7gen", "schlich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und wenig dachte, was es drau\u00dfen treibe.", "tokens": ["Und", "we\u00b7nig", "dach\u00b7te", ",", "was", "es", "drau\u00b7\u00dfen", "trei\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachdenklich schritt ich, und bemerkte nicht", "tokens": ["Nach\u00b7denk\u00b7lich", "schritt", "ich", ",", "und", "be\u00b7merk\u00b7te", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Krautes Wallen und des Wurmes Licht,", "tokens": ["Des", "Krau\u00b7tes", "Wal\u00b7len", "und", "des", "Wur\u00b7mes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich sah auch nicht, als stieg die Mondesscheibe.", "tokens": ["Ich", "sah", "auch", "nicht", ",", "als", "stieg", "die", "Mon\u00b7des\u00b7schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "KOUS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Grad war der Weg, ganz sonder Steg und Bruch;", "tokens": ["Grad", "war", "der", "Weg", ",", "ganz", "son\u00b7der", "Steg", "und", "Bruch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So tr\u00e4umt ich fort und, wie ein schlechtes Buch,", "tokens": ["So", "tr\u00e4umt", "ich", "fort", "und", ",", "wie", "ein", "schlech\u00b7tes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Pfennigs-Magazin uns auf der Reise", "tokens": ["Ein", "Pfen\u00b7nigs\u00b7Ma\u00b7ga\u00b7zin", "uns", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Station zu Stationen plagt,", "tokens": ["Von", "Sta\u00b7ti\u00b7on", "zu", "Sta\u00b7ti\u00b7o\u00b7nen", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hab' zehnmal Weggeworf'nes ich benagt,", "tokens": ["Hab'", "zehn\u00b7mal", "Weg\u00b7ge\u00b7wor\u00b7f'\u00b7nes", "ich", "be\u00b7nagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und fortgeleiert \u00fcberdr\u00fc\u00df'ge Weise.", "tokens": ["Und", "fort\u00b7ge\u00b7lei\u00b7ert", "\u00fc\u00b7ber\u00b7dr\u00fc\u00df'\u00b7ge", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Entw\u00fcrfe wurden aus Entw\u00fcrfen reif,", "tokens": ["Ent\u00b7w\u00fcr\u00b7fe", "wur\u00b7den", "aus", "Ent\u00b7w\u00fcr\u00b7fen", "reif", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch, wie die Schlange packt den eignen Schweif,", "tokens": ["Doch", ",", "wie", "die", "Schlan\u00b7ge", "packt", "den", "eig\u00b7nen", "Schweif", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fand ich mich immer auf derselben Stelle;", "tokens": ["Fand", "ich", "mich", "im\u00b7mer", "auf", "der\u00b7sel\u00b7ben", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da pl\u00f6tzlich fuhr ein plumper Schr\u00f6ter jach", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "fuhr", "ein", "plum\u00b7per", "Schr\u00f6\u00b7ter", "jach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An's Auge mir, ich schreckte auf und lag", "tokens": ["An's", "Au\u00b7ge", "mir", ",", "ich", "schreck\u00b7te", "auf", "und", "lag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Am Grund, um mich des Haidekrautes Welle.", "tokens": ["Am", "Grund", ",", "um", "mich", "des", "Hai\u00b7de\u00b7krau\u00b7tes", "Wel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seltsames Lager, das ich mir erkor!", "tokens": ["Selt\u00b7sa\u00b7mes", "La\u00b7ger", ",", "das", "ich", "mir", "er\u00b7kor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zur Rechten, Linken schwoll Gestein empor,", "tokens": ["Zur", "Rech\u00b7ten", ",", "Lin\u00b7ken", "schwoll", "Ge\u00b7stein", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gewalt'ge Bl\u00f6cke, rohe Porphirbrode;", "tokens": ["Ge\u00b7walt'\u00b7ge", "Bl\u00f6\u00b7cke", ",", "ro\u00b7he", "Por\u00b7phir\u00b7bro\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir \u00fcberm Haupte reckte sich der Bau,", "tokens": ["Mir", "\u00fc\u00b7berm", "Haup\u00b7te", "reck\u00b7te", "sich", "der", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Langhaar'ge Flechten r\u00fchrten meine Brau,", "tokens": ["Lang\u00b7haa\u00b7r'\u00b7ge", "Flech\u00b7ten", "r\u00fchr\u00b7ten", "mei\u00b7ne", "Brau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und mir zu F\u00fc\u00dfen schwankt' die Ginsterlode.", "tokens": ["Und", "mir", "zu", "F\u00fc\u00b7\u00dfen", "schwankt'", "die", "Gins\u00b7ter\u00b7lo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich wu\u00dfte gleich, es war ein H\u00fcnengrab,", "tokens": ["Ich", "wu\u00df\u00b7te", "gleich", ",", "es", "war", "ein", "H\u00fc\u00b7nen\u00b7grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und fester dr\u00fcckt' ich meine Stirn hinab,", "tokens": ["Und", "fes\u00b7ter", "dr\u00fcckt'", "ich", "mei\u00b7ne", "Stirn", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Woll\u00fcstig saugend an des Grauens S\u00fc\u00dfe,", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "sau\u00b7gend", "an", "des", "Grau\u00b7ens", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bis es mit eis'gen Krallen mich gepackt,", "tokens": ["Bis", "es", "mit", "eis'\u00b7gen", "Kral\u00b7len", "mich", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis wie ein Gletscher-Bronn des Blutes Takt", "tokens": ["Bis", "wie", "ein", "Glet\u00b7scher\u00b7Bronn", "des", "Blu\u00b7tes", "Takt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aufquoll und h\u00e4mmert' unterm Mantelvlie\u00dfe.", "tokens": ["Auf\u00b7quoll", "und", "h\u00e4m\u00b7mert'", "un\u00b7term", "Man\u00b7tel\u00b7vlie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Decke \u00fcber mir, gesunken, schief,", "tokens": ["Die", "De\u00b7cke", "\u00fc\u00b7ber", "mir", ",", "ge\u00b7sun\u00b7ken", ",", "schief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An der so bla\u00df geh\u00e4rmt das Mondlicht schlief,", "tokens": ["An", "der", "so", "bla\u00df", "ge\u00b7h\u00e4rmt", "das", "Mond\u00b7licht", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eine Wittwe an des Gatten Grabe;", "tokens": ["Wie", "ei\u00b7ne", "Witt\u00b7we", "an", "des", "Gat\u00b7ten", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Hirtenfeuer Kohlenscheite sahn", "tokens": ["Vom", "Hir\u00b7ten\u00b7feu\u00b7er", "Koh\u00b7len\u00b7schei\u00b7te", "sahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So leichenbrandig durch den Thimian,", "tokens": ["So", "lei\u00b7chen\u00b7bran\u00b7dig", "durch", "den", "Thi\u00b7mi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich sie abw\u00e4rts schnellte mit dem Stabe.", "tokens": ["Da\u00df", "ich", "sie", "ab\u00b7w\u00e4rts", "schnell\u00b7te", "mit", "dem", "Sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Husch fuhr ein Kibitz schreiend aus dem Moos;", "tokens": ["Husch", "fuhr", "ein", "Ki\u00b7bitz", "schrei\u00b7end", "aus", "dem", "Moos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Ich lachte auf; doch trug wie b\u00fcgellos", "tokens": ["Ich", "lach\u00b7te", "auf", ";", "doch", "trug", "wie", "b\u00fc\u00b7gel\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "KOKOM", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich Phantasie weit \u00fcber Spalt und Barren.", "tokens": ["Mich", "Phan\u00b7ta\u00b7sie", "weit", "\u00fc\u00b7ber", "Spalt", "und", "Bar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dem Wind hab' ich gelauscht so scharf gespannt,", "tokens": ["Dem", "Wind", "hab'", "ich", "ge\u00b7lauscht", "so", "scharf", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als bring er Kunde aus dem Geisterland,", "tokens": ["Als", "bring", "er", "Kun\u00b7de", "aus", "dem", "Geis\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und immer mu\u00dft ich an die Decke starren.", "tokens": ["Und", "im\u00b7mer", "mu\u00dft", "ich", "an", "die", "De\u00b7cke", "star\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word"