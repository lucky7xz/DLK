{"textgrid.poem.64516": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "140. Die Zimmertaxe in G\u00f6ttingen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bem\u00fcht hat ", "tokens": ["Be\u00b7m\u00fcht", "hat"], "token_info": ["word", "word"], "pos": ["VVPP", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Gern schlo\u00df der Bursch ihm auf, und \u00e4ngstlich mancher Wirth;", "tokens": ["Gern", "schlo\u00df", "der", "Bursch", "ihm", "auf", ",", "und", "\u00e4ngst\u00b7lich", "man\u00b7cher", "Wirth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Hauses Lag' und Bau, und was hinein geh\u00f6rt,", "tokens": ["Des", "Hau\u00b7ses", "Lag'", "und", "Bau", ",", "und", "was", "hin\u00b7ein", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "KON", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tapete, Spiegel, Tisch, bestimmt des Zimmers Werth.", "tokens": ["Ta\u00b7pe\u00b7te", ",", "Spie\u00b7gel", ",", "Tisch", ",", "be\u00b7stimmt", "des", "Zim\u00b7mers", "Werth", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "J\u00fcngst wollt' er einer Frau den Preis geringer setzen;", "tokens": ["J\u00fcngst", "wollt'", "er", "ei\u00b7ner", "Frau", "den", "Preis", "ge\u00b7rin\u00b7ger", "set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Herr, sprach sie, lassen sich auch meine M\u00e4dchen sch\u00e4tzen?", "tokens": ["Herr", ",", "sprach", "sie", ",", "las\u00b7sen", "sich", "auch", "mei\u00b7ne", "M\u00e4d\u00b7chen", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}