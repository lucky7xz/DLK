{"textgrid.poem.37165": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er plaget mich, ich soll ihn k\u00fcssen.", "tokens": ["Er", "pla\u00b7get", "mich", ",", "ich", "soll", "ihn", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, nein! das w\u00fcrd ich theuer b\u00fc\u00dfen,", "tokens": ["Nein", ",", "nein", "!", "das", "w\u00fcrd", "ich", "theu\u00b7er", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Mutter sagt, ich solls nicht thun.", "tokens": ["Denn", "Mut\u00b7ter", "sagt", ",", "ich", "solls", "nicht", "thun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbeut ihm seine nichts, ei nun!", "tokens": ["Ver\u00b7beut", "ihm", "sei\u00b7ne", "nichts", ",", "ei", "nun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "PIS", "$,", "ITJ", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kann er mich ja k\u00fcssen!", "tokens": ["So", "kann", "er", "mich", "ja", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}