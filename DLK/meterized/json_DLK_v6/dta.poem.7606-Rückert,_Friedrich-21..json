{"dta.poem.7606": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weltherrscher Raghu kehrt vom Welterobrungszug", "tokens": ["Welt\u00b7herr\u00b7scher", "Ra\u00b7ghu", "kehrt", "vom", "Wel\u00b7te\u00b7ro\u00b7brungs\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sieger heim und bringt Weltsch\u00e4tze mit genug.", "tokens": ["Als", "Sie\u00b7ger", "heim", "und", "bringt", "Welt\u00b7sch\u00e4t\u00b7ze", "mit", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "KON", "VVFIN", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00e4tze theilet er beim Siegesopfer aus,", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "thei\u00b7let", "er", "beim", "Sie\u00b7ge\u00b7sop\u00b7fer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": " und hat nun keinen Schatz als seinen Ruhm im Haus.", "tokens": ["und", "hat", "nun", "kei\u00b7nen", "Schatz", "als", "sei\u00b7nen", "Ruhm", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "KOKOM", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihm kommt ein frommer Gast; wie soll er ihn empfangen?", "tokens": ["Ihm", "kommt", "ein", "from\u00b7mer", "Gast", ";", "wie", "soll", "er", "ihn", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "PWAV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Goldgef\u00e4\u00dfe sind dem Hauswirth ausgegangen.", "tokens": ["Die", "Gold\u00b7ge\u00b7f\u00e4\u00b7\u00dfe", "sind", "dem", "Haus\u00b7wirth", "aus\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein h\u00f6lzernes Gef\u00e4\u00df, gef\u00fcllt mit Willkommsflut,", "tokens": ["Ein", "h\u00f6l\u00b7zer\u00b7nes", "Ge\u00b7f\u00e4\u00df", ",", "ge\u00b7f\u00fcllt", "mit", "Will\u00b7komms\u00b7flut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt er entgegen ihm, und gr\u00fc\u00dfet wohlgemuth.", "tokens": ["Tr\u00e4gt", "er", "ent\u00b7ge\u00b7gen", "ihm", ",", "und", "gr\u00fc\u00b7\u00dfet", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den ehrenden Besuch, sprich, was ihn mir gebracht?", "tokens": ["Den", "eh\u00b7ren\u00b7den", "Be\u00b7such", ",", "sprich", ",", "was", "ihn", "mir", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gew\u00e4hrt ist dein Gesuch, wenn's steht in meiner Macht.", "tokens": ["Ge\u00b7w\u00e4hrt", "ist", "dein", "Ge\u00b7such", ",", "wenn's", "steht", "in", "mei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch jener, der erkennt die Armuth an den Zeichen,", "tokens": ["Doch", "je\u00b7ner", ",", "der", "er\u00b7kennt", "die", "Ar\u00b7muth", "an", "den", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will ohne Wortverlust zur\u00fcck bescheiden weichen.", "tokens": ["Will", "oh\u00b7ne", "Wort\u00b7ver\u00b7lust", "zu\u00b7r\u00fcck", "be\u00b7schei\u00b7den", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch als mit bittendem Befehl der K\u00f6nig dringt,", "tokens": ["Doch", "als", "mit", "bit\u00b7ten\u00b7dem", "Be\u00b7fehl", "der", "K\u00f6\u00b7nig", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagt er das wichtige Anliegen, das ihn bringt:", "tokens": ["Sagt", "er", "das", "wich\u00b7ti\u00b7ge", "An\u00b7lie\u00b7gen", ",", "das", "ihn", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Lernt' ich zw\u00f6lf Jahre lang, nun hab' ich ausgelernt.", "tokens": ["Lernt'", "ich", "zw\u00f6lf", "Jah\u00b7re", "lang", ",", "nun", "hab'", "ich", "aus\u00b7ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Als er mich nun entlie\u00df, den Sch\u00fcler und den Sohn,", "tokens": ["Als", "er", "mich", "nun", "ent\u00b7lie\u00df", ",", "den", "Sch\u00fc\u00b7ler", "und", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fragt' ich beim Abschied ihn: was ist der Lehre Lohn?", "tokens": ["Fragt'", "ich", "beim", "Ab\u00b7schied", "ihn", ":", "was", "ist", "der", "Leh\u00b7re", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "$.", "PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Er sprach: \u201eF\u00fcrs Wissen ist kein ird'scher Lohn beschieden;", "tokens": ["Er", "sprach", ":", "\u201e", "F\u00fcrs", "Wis\u00b7sen", "ist", "kein", "ird'\u00b7scher", "Lohn", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "NN", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin mit langem Dienst und treuem Flei\u00df zufrieden.\u201c", "tokens": ["Ich", "bin", "mit", "lan\u00b7gem", "Dienst", "und", "treu\u00b7em", "Flei\u00df", "zu\u00b7frie\u00b7den", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Doch als ich ungest\u00fcm mit meinen Bitten drang,", "tokens": ["Doch", "als", "ich", "un\u00b7ge\u00b7st\u00fcm", "mit", "mei\u00b7nen", "Bit\u00b7ten", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergriff der Zorn ihn, den kein Weiser selbst bezwang.", "tokens": ["Er\u00b7griff", "der", "Zorn", "ihn", ",", "den", "kein", "Wei\u00b7ser", "selbst", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,", "PRELS", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Er rief: \u201eUnd wenn du Lohn denn bietest, wahnumh\u00fcllt;", "tokens": ["Er", "rief", ":", "\u201e", "Und", "wenn", "du", "Lohn", "denn", "bie\u00b7test", ",", "wah\u00b7num\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "KON", "KOUS", "PPER", "NN", "KON", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Lohn sei soviel Gold, als Raghu's Schatzhaus f\u00fcllt.\u201c", "tokens": ["Der", "Lohn", "sei", "so\u00b7viel", "Gold", ",", "als", "Raghu's", "Schatz\u00b7haus", "f\u00fcllt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,", "KOUS", "NE", "NE", "VVFIN", "$.", "$("], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Dein Holzkrug laut genug sagt was dein Schatzhaus fasse;", "tokens": ["Dein", "Holz\u00b7krug", "laut", "ge\u00b7nug", "sagt", "was", "dein", "Schatz\u00b7haus", "fas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum mit Entschuldigung, o K\u00f6nig, mich entlasse!", "tokens": ["Drum", "mit", "Ent\u00b7schul\u00b7di\u00b7gung", ",", "o", "K\u00f6\u00b7nig", ",", "mich", "ent\u00b7las\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Ich seh', o K\u00f6nig, wohl, dir blieb kein Eigenthum,", "tokens": ["Ich", "seh'", ",", "o", "K\u00f6\u00b7nig", ",", "wohl", ",", "dir", "blieb", "kein", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "ADV", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als unver\u00e4u\u00dferlich allein der eigne Ruhm.", "tokens": ["Als", "un\u00b7ver\u00b7\u00e4u\u00b7\u00dfer\u00b7lich", "al\u00b7lein", "der", "eig\u00b7ne", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Doch K\u00f6nig Raghu spricht: Ist mir der Ruhm geblieben,", "tokens": ["Doch", "K\u00f6\u00b7nig", "Ra\u00b7ghu", "spricht", ":", "Ist", "mir", "der", "Ruhm", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "$.", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was w\u00e4r' er, wenn er nicht auch Gold h\u00e4tt' aufgetrieben?", "tokens": ["Was", "w\u00e4r'", "er", ",", "wenn", "er", "nicht", "auch", "Gold", "h\u00e4tt'", "auf\u00b7ge\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Geruh' in meinem Haus als Gast dich auszuruhn;", "tokens": ["Ge\u00b7ruh'", "in", "mei\u00b7nem", "Haus", "als", "Gast", "dich", "aus\u00b7zu\u00b7ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KOUS", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachts denken wir wol aus, was wir am Morgen thun.", "tokens": ["Nachts", "den\u00b7ken", "wir", "wol", "aus", ",", "was", "wir", "am", "Mor\u00b7gen", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Der K\u00f6nig sinnt bei Nacht: wo soll ichs her bekommen?", "tokens": ["Der", "K\u00f6\u00b7nig", "sinnt", "bei", "Nacht", ":", "wo", "soll", "ichs", "her", "be\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "PWAV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den K\u00f6n'gen rings umher hab' ich es l\u00e4ngst genommen.", "tokens": ["Den", "K\u00f6n'\u00b7gen", "rings", "um\u00b7her", "hab'", "ich", "es", "l\u00e4ngst", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Doch auf dem Goldberg wohnt ", "tokens": ["Doch", "auf", "dem", "Gold\u00b7berg", "wohnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der hat bei mir sein Amt vers\u00e4umt als Schatzhaush\u00fcter.", "tokens": ["Der", "hat", "bei", "mir", "sein", "Amt", "ver\u00b7s\u00e4umt", "als", "Schatz\u00b7haus\u00b7h\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Und Raghu in der Nacht l\u00e4\u00dft rufen aus mit Schall:", "tokens": ["Und", "Ra\u00b7ghu", "in", "der", "Nacht", "l\u00e4\u00dft", "ru\u00b7fen", "aus", "mit", "Schall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "VVFIN", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Gott Kuwera droht von Raghu Ueberfall.", "tokens": ["Dem", "Gott", "Ku\u00b7we\u00b7ra", "droht", "von", "Ra\u00b7ghu", "Ue\u00b7ber\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Noch in der Nacht besteigt er seines Ruhmes Wagen,", "tokens": ["Noch", "in", "der", "Nacht", "be\u00b7steigt", "er", "sei\u00b7nes", "Ruh\u00b7mes", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mit dem Tag ihn soll zum Sieg am Goldberg tragen.", "tokens": ["Der", "mit", "dem", "Tag", "ihn", "soll", "zum", "Sieg", "am", "Gold\u00b7berg", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VMFIN", "APPRART", "NN", "APPRART", "NN",