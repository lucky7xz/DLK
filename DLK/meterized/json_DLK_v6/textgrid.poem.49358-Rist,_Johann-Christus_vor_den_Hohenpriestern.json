{"textgrid.poem.49358": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Christus vor den Hohenpriestern", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hin ist die Nacht, der Tag bricht an,", "tokens": ["Hin", "ist", "die", "Nacht", ",", "der", "Tag", "bricht", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Morgenr\u00f6te malt den Himmel,", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "malt", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt erwacht, und jederman", "tokens": ["Die", "Welt", "er\u00b7wacht", ",", "und", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erregt sein t\u00e4gliches Get\u00fcmmel:", "tokens": ["Er\u00b7regt", "sein", "t\u00e4g\u00b7li\u00b7ches", "Ge\u00b7t\u00fcm\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da wird das Gotteslamm gerissen", "tokens": ["Da", "wird", "das", "Got\u00b7tes\u00b7lamm", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sehr grimmig vor den hohen Rat,", "tokens": ["Sehr", "grim\u00b7mig", "vor", "den", "ho\u00b7hen", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als ihn die Zunft der Diener hat", "tokens": ["Als", "ihn", "die", "Zunft", "der", "Die\u00b7ner", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die ganze Nacht her\u00fcmgeschmissen.", "tokens": ["Die", "gan\u00b7ze", "Nacht", "he\u00b7r\u00fcm\u00b7ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es samlet sich die leichte Rott", "tokens": ["Es", "sam\u00b7let", "sich", "die", "leich\u00b7te", "Rott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und l\u00e4sset vor dem Richtstuhl f\u00fchren", "tokens": ["Und", "l\u00e4s\u00b7set", "vor", "dem", "Richt\u00b7stuhl", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Spott den hochgelobten Gott,", "tokens": ["Mit", "Spott", "den", "hoch\u00b7ge\u00b7lob\u00b7ten", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der selber pr\u00fcfet Herz und Nieren;", "tokens": ["Der", "sel\u00b7ber", "pr\u00fc\u00b7fet", "Herz", "und", "Nie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ja, der sich niemals hat emp\u00f6ret,", "tokens": ["Ja", ",", "der", "sich", "nie\u00b7mals", "hat", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PRF", "ADV", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mu\u00df aller Aufruhr sch\u00fcldig sein;", "tokens": ["Mu\u00df", "al\u00b7ler", "Auf\u00b7ruhr", "sch\u00fcl\u00b7dig", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Man sagt ihm ins Gesicht hinein,", "tokens": ["Man", "sagt", "ihm", "ins", "Ge\u00b7sicht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Gottl\u00e4stern sei von ihm geh\u00f6ret.", "tokens": ["Gott\u00b7l\u00e4s\u00b7tern", "sei", "von", "ihm", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sobald der Judas nun vernimt,", "tokens": ["So\u00b7bald", "der", "Ju\u00b7das", "nun", "ver\u00b7nimt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie sch\u00e4ndlich seine That gelungen", "tokens": ["Wie", "sch\u00e4nd\u00b7lich", "sei\u00b7ne", "That", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und alles das, was angestimt,", "tokens": ["Und", "al\u00b7les", "das", ",", "was", "an\u00b7ge\u00b7stimt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Von jedem werde nachgesungen,", "tokens": ["Von", "je\u00b7dem", "wer\u00b7de", "nach\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df n\u00e4mlich unser Heil sol sterben:", "tokens": ["Da\u00df", "n\u00e4m\u00b7lich", "un\u00b7ser", "Heil", "sol", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Da \u00fcberf\u00e4llt ihn Reu und Schmerz,", "tokens": ["Da", "\u00fc\u00b7berf\u00b7\u00e4llt", "ihn", "Reu", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das qu\u00e4let nun sein falsches Herz,", "tokens": ["Das", "qu\u00e4\u00b7let", "nun", "sein", "fal\u00b7sches", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So da\u00df er sp\u00fcret sein Verderben.", "tokens": ["So", "da\u00df", "er", "sp\u00fc\u00b7ret", "sein", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "\u00bbach, ruft er, was hab' ich gethan!", "tokens": ["\u00bb", "ach", ",", "ruft", "er", ",", "was", "hab'", "ich", "ge\u00b7than", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPER", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Mein Herr ist ohne Schuld verraten.", "tokens": ["Mein", "Herr", "ist", "oh\u00b7ne", "Schuld", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ihr Richter, legt doch ab den Wahn,", "tokens": ["Ihr", "Rich\u00b7ter", ",", "legt", "doch", "ab", "den", "Wahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Verfluchet meine b\u00f6sen Thaten;", "tokens": ["Ver\u00b7flu\u00b7chet", "mei\u00b7ne", "b\u00f6\u00b7sen", "Tha\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Nemt hin eur Geld, das mich verf\u00fchret,", "tokens": ["Nemt", "hin", "eur", "Geld", ",", "das", "mich", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Eur Geld, das mir schafft schwere Pein!\u00ab", "tokens": ["Eur", "Geld", ",", "das", "mir", "schafft", "schwe\u00b7re", "Pein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Die Richter sprechen alle: \u00bbNein,", "tokens": ["Die", "Rich\u00b7ter", "spre\u00b7chen", "al\u00b7le", ":", "\u00bb", "Nein", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$.", "$(", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Es wird nicht mehr von uns ber\u00fchret.\u00ab", "tokens": ["Es", "wird", "nicht", "mehr", "von", "uns", "be\u00b7r\u00fch\u00b7ret", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "O Bubenst\u00fcck, o falscher Ku\u00df,", "tokens": ["O", "Bu\u00b7ben\u00b7st\u00fcck", ",", "o", "fal\u00b7scher", "Ku\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der diesen Mann zur H\u00f6llen sendet,", "tokens": ["Der", "die\u00b7sen", "Mann", "zur", "H\u00f6l\u00b7len", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Verzweiflung machet ihm den Schlu\u00df,", "tokens": ["Ver\u00b7zwei\u00b7flung", "ma\u00b7chet", "ihm", "den", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Indem ein Strick sein Leben endet,", "tokens": ["In\u00b7dem", "ein", "Strick", "sein", "Le\u00b7ben", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ein Strick treibt aus sein' arme Seele,", "tokens": ["Ein", "Strick", "treibt", "aus", "sein'", "ar\u00b7me", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Sein Bauch zerbricht als ein Geschw\u00fcr,", "tokens": ["Sein", "Bauch", "zer\u00b7bricht", "als", "ein", "Ge\u00b7schw\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Sein Eingeweide dringt herf\u00fcr,", "tokens": ["Sein", "Ein\u00b7ge\u00b7wei\u00b7de", "dringt", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Der Geist fleugt in des Satans H\u00f6le.", "tokens": ["Der", "Geist", "fleugt", "in", "des", "Sa\u00b7tans", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Herr Jesu, der du durch den Rat", "tokens": ["Herr", "Je\u00b7su", ",", "der", "du", "durch", "den", "Rat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Des Todes sch\u00fcldig bist erkl\u00e4ret,", "tokens": ["Des", "To\u00b7des", "sch\u00fcl\u00b7dig", "bist", "er\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Vergib mir doch die Missethat,", "tokens": ["Ver\u00b7gib", "mir", "doch", "die", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Die mich wie Sand am Meer' beschweret!", "tokens": ["Die", "mich", "wie", "Sand", "am", "Meer'", "be\u00b7schwe\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Ach Herr, es ist mir unvergessen,", "tokens": ["Ach", "Herr", ",", "es", "ist", "mir", "un\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df ich geh\u00f6r' in diese Rott;", "tokens": ["Da\u00df", "ich", "ge\u00b7h\u00f6r'", "in", "die\u00b7se", "Rott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Als du verdammet bist, mein Gott,", "tokens": ["Als", "du", "ver\u00b7dam\u00b7met", "bist", ",", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Bin ich beim Priester mit gesessen.", "tokens": ["Bin", "ich", "beim", "Pries\u00b7ter", "mit", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Mein Heiland, du wirst hingef\u00fchrt", "tokens": ["Mein", "Hei\u00b7land", ",", "du", "wirst", "hin\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Zu solchen Richtern, die nicht wissen", "tokens": ["Zu", "sol\u00b7chen", "Rich\u00b7tern", ",", "die", "nicht", "wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Was deiner Herlichkeit geb\u00fchrt,", "tokens": ["Was", "dei\u00b7ner", "Her\u00b7lich\u00b7keit", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Die Blut zu st\u00fcrzen sind geflissen.", "tokens": ["Die", "Blut", "zu", "st\u00fcr\u00b7zen", "sind", "ge\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Warum hast du die\u00df uns gestanden?", "tokens": ["Wa\u00b7rum", "hast", "du", "die\u00df", "uns", "ge\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Darum, auf da\u00df ich w\u00fcrde nicht,", "tokens": ["Da\u00b7rum", ",", "auf", "da\u00df", "ich", "w\u00fcr\u00b7de", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "KOUS", "PPER", "VAFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Wenn du wirst kommen zum Gericht,", "tokens": ["Wenn", "du", "wirst", "kom\u00b7men", "zum", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "O Gott, vor deinem Stuhl zu Schanden", "tokens": ["O", "Gott", ",", "vor", "dei\u00b7nem", "Stuhl", "zu", "Schan\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Ach, gib mir einen tapfern Mut,", "tokens": ["Ach", ",", "gib", "mir", "ei\u00b7nen", "tap\u00b7fern", "Mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Da\u00df ich ja nimmermehr erlige;", "tokens": ["Da\u00df", "ich", "ja", "nim\u00b7mer\u00b7mehr", "er\u00b7li\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.59": {"text": "Wenn mich versehrt des Kreuzes Glut,", "tokens": ["Wenn", "mich", "ver\u00b7sehrt", "des", "Kreu\u00b7zes", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "So hilf mir, da\u00df ich fr\u00f6lich siege.", "tokens": ["So", "hilf", "mir", ",", "da\u00df", "ich", "fr\u00f6\u00b7lich", "sie\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.61": {"text": "Dein bitters Leiden kan erquicken,", "tokens": ["Dein", "bit\u00b7ters", "Lei\u00b7den", "kan", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "O treuer Gott, mein mattes Herz,", "tokens": ["O", "treu\u00b7er", "Gott", ",", "mein", "mat\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Da\u00df weder Tod, noch Not, noch Schmerz", "tokens": ["Da\u00df", "we\u00b7der", "Tod", ",", "noch", "Not", ",", "noch", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KON", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Dasselbe k\u00f6nnen unterdr\u00fccken.", "tokens": ["Das\u00b7sel\u00b7be", "k\u00f6n\u00b7nen", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.65": {"text": "Es tr\u00f6ste mich zur jeden Zeit,", "tokens": ["Es", "tr\u00f6s\u00b7te", "mich", "zur", "je\u00b7den", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Besonders in der H\u00f6llen Schrecken,", "tokens": ["Be\u00b7son\u00b7ders", "in", "der", "H\u00f6l\u00b7len", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.67": {"text": "Des andern Lebens S\u00fc\u00dfigkeit,", "tokens": ["Des", "an\u00b7dern", "Le\u00b7bens", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Zu welchem du bald wirst erwecken", "tokens": ["Zu", "wel\u00b7chem", "du", "bald", "wirst", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "Die Gl\u00e4ubigen, die dich geliebet:", "tokens": ["Die", "Gl\u00e4u\u00b7bi\u00b7gen", ",", "die", "dich", "ge\u00b7lie\u00b7bet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.70": {"text": "Nach solchem Leben seufz' ich sehr;", "tokens": ["Nach", "sol\u00b7chem", "Le\u00b7ben", "seuf\u00b7z'", "ich", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.71": {"text": "Da wirst du geben Freud und Ehr'", "tokens": ["Da", "wirst", "du", "ge\u00b7ben", "Freud", "und", "Ehr'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "Uns, die wir lebten so betr\u00fcbet.", "tokens": ["Uns", ",", "die", "wir", "leb\u00b7ten", "so", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.73": {"text": "Ein Gasthaus nenn' ich diese Welt", "tokens": ["Ein", "Gast\u00b7haus", "nenn'", "ich", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.74": {"text": "Und nicht das Vaterland der Frommen;", "tokens": ["Und", "nicht", "das", "Va\u00b7ter\u00b7land", "der", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.75": {"text": "Du hast ja, Herr, ein Haus bestellt", "tokens": ["Du", "hast", "ja", ",", "Herr", ",", "ein", "Haus", "be\u00b7stellt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.76": {"text": "Vor alle, welche zu dir kommen;", "tokens": ["Vor", "al\u00b7le", ",", "wel\u00b7che", "zu", "dir", "kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.77": {"text": "Dahin nun wollen wir uns schwingen,", "tokens": ["Da\u00b7hin", "nun", "wol\u00b7len", "wir", "uns", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.78": {"text": "Was geht uns dieses Erdreich an?", "tokens": ["Was", "geht", "uns", "die\u00b7ses", "Er\u00b7dreich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.79": {"text": "Hilf, da\u00df wir bald in Kanaan", "tokens": ["Hilf", ",", "da\u00df", "wir", "bald", "in", "Ka\u00b7naan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.80": {"text": "Der Ewigkeit ein Liedlein singen.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "ein", "Lied\u00b7lein", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.81": {"text": "O wolte Gott, es k\u00e4m' herbei", "tokens": ["O", "wol\u00b7te", "Gott", ",", "es", "k\u00e4m'", "her\u00b7bei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VMFIN", "NN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.82": {"text": "Die Stund', in der ich solt' ablegen", "tokens": ["Die", "Stund'", ",", "in", "der", "ich", "solt'", "ab\u00b7le\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.83": {"text": "Des Fleisches Last und werden frei", "tokens": ["Des", "Flei\u00b7sches", "Last", "und", "wer\u00b7den", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.84": {"text": "Von S\u00fcnden, die sich stets noch regen!", "tokens": ["Von", "S\u00fcn\u00b7den", ",", "die", "sich", "stets", "noch", "re\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.85": {"text": "O, solt' in jennem Freudenleben", "tokens": ["O", ",", "solt'", "in", "jen\u00b7nem", "Freu\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.86": {"text": "Mein Seelichen sehn f\u00fcr und f\u00fcr", "tokens": ["Mein", "See\u00b7li\u00b7chen", "sehn", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.87": {"text": "Die Feinde ligen unter dir,", "tokens": ["Die", "Fein\u00b7de", "li\u00b7gen", "un\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.88": {"text": "Wie wolt' ich meine Stimm erheben!", "tokens": ["Wie", "wolt'", "ich", "mei\u00b7ne", "Stimm", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.89": {"text": "O Jesu, Herr der Herlichkeit,", "tokens": ["O", "Je\u00b7su", ",", "Herr", "der", "Her\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.90": {"text": "O s\u00fc\u00dfer Trost der armen S\u00fcnder,", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Trost", "der", "ar\u00b7men", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.91": {"text": "O ewig Gott, Mensch in der Zeit,", "tokens": ["O", "e\u00b7wig", "Gott", ",", "Mensch", "in", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.92": {"text": "Du liebest ja die Menschenkinder;", "tokens": ["Du", "lie\u00b7best", "ja", "die", "Men\u00b7schen\u00b7kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.93": {"text": "Wie freundlich hast du dich erwiesen,", "tokens": ["Wie", "freund\u00b7lich", "hast", "du", "dich", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.94": {"text": "Der du des bittern Sterbens Not", "tokens": ["Der", "du", "des", "bit\u00b7tern", "Ster\u00b7bens", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.95": {"text": "Get\u00f6tet hast durch deinen Tod!", "tokens": ["Ge\u00b7t\u00f6\u00b7tet", "hast", "durch", "dei\u00b7nen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.96": {"text": "Sei hier und dort von mir gepriesen!", "tokens": ["Sei", "hier", "und", "dort", "von", "mir", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}