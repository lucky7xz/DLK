{"textgrid.poem.41479": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Wettstreit", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dchen und mein Wein,", "tokens": ["Mein", "M\u00e4d\u00b7chen", "und", "mein", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die wollen sich entzwein.", "tokens": ["Die", "wol\u00b7len", "sich", "ent\u00b7zwein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich den Zwist entscheide,", "tokens": ["Ob", "ich", "den", "Zwist", "ent\u00b7schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird noch die Frage sein.", "tokens": ["Wird", "noch", "die", "Fra\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich suche mich durch Beide", "tokens": ["Ich", "su\u00b7che", "mich", "durch", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Stillen zu erfreun.", "tokens": ["Im", "Stil\u00b7len", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie gibt mir gr\u00f6\u00dfre Freude:", "tokens": ["Sie", "gibt", "mir", "gr\u00f6\u00df\u00b7re", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch \u00f6ftre gibt der Wein.", "tokens": ["Doch", "\u00f6ft\u00b7re", "gibt", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}