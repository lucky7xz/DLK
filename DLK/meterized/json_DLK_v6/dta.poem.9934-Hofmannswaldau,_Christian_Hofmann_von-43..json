{"dta.poem.9934": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus mir mach ich mir ein kleid/ dieser nur alleine siehet/", "tokens": ["Aus", "mir", "mach", "ich", "mir", "ein", "kleid", "/", "die\u00b7ser", "nur", "al\u00b7lei\u00b7ne", "sie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "ART", "NN", "$(", "PDS", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ob ich gut sey oder schlimm/ der mir aus dasselbe ziehet.", "tokens": ["Ob", "ich", "gut", "sey", "o\u00b7der", "schlimm", "/", "der", "mir", "aus", "das\u00b7sel\u00b7be", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADJD", "$(", "PRELS", "PPER", "APPR", "PDAT", "VVFIN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jtzund bin ich rund/ bald eckicht/ itzund gro\u00df/ bald wieder klein/", "tokens": ["Jt\u00b7zund", "bin", "ich", "rund", "/", "bald", "ec\u00b7kicht", "/", "it\u00b7zund", "gro\u00df", "/", "bald", "wie\u00b7der", "klein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "$(", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nach dem ort/ wo man mich machet/ mu\u00df ich auch genen\u0303et seyn.", "tokens": ["Nach", "dem", "ort", "/", "wo", "man", "mich", "ma\u00b7chet", "/", "mu\u00df", "ich", "auch", "ge\u00b7ne\u00f1et", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PWAV", "PIS", "PPER", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}