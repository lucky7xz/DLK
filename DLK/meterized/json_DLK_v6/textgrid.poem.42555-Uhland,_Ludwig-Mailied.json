{"textgrid.poem.42555": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mailied", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenig hab ich noch empfunden", "tokens": ["We\u00b7nig", "hab", "ich", "noch", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der werten Fr\u00fchlingszeit;", "tokens": ["Von", "der", "wer\u00b7ten", "Fr\u00fch\u00b7lings\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All die Lust und Lieblichkeit", "tokens": ["All", "die", "Lust", "und", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat zu mir nicht Bahn gefunden.", "tokens": ["Hat", "zu", "mir", "nicht", "Bahn", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! was soll ein Herz dabei,", "tokens": ["Ach", "!", "was", "soll", "ein", "Herz", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VMFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das sich so zerrissen f\u00fchlet?", "tokens": ["Das", "sich", "so", "zer\u00b7ris\u00b7sen", "f\u00fch\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "VVPP", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Jetzt empfind ich erst den Mai,", "tokens": ["Jetzt", "emp\u00b7find", "ich", "erst", "den", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seit der Sturm in Bl\u00fcten w\u00fchlet.", "tokens": ["Seit", "der", "Sturm", "in", "Bl\u00fc\u00b7ten", "w\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}