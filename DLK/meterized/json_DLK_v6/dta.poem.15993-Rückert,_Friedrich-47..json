{"dta.poem.15993": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zusammen traten einst Gewalt und Macht und St\u00e4rke,", "tokens": ["Zu\u00b7sam\u00b7men", "tra\u00b7ten", "einst", "Ge\u00b7walt", "und", "Macht", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gemeinschaftliche Hand anlegend einem Werke.", "tokens": ["Ge\u00b7mein\u00b7schaft\u00b7li\u00b7che", "Hand", "an\u00b7le\u00b7gend", "ei\u00b7nem", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Mit Waffen die Gewalt, die St\u00e4rke mit dem Arm", "tokens": ["Mit", "Waf\u00b7fen", "die", "Ge\u00b7walt", ",", "die", "St\u00e4r\u00b7ke", "mit", "dem", "Arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ger\u00fcstet, und die Macht mit einem Dienerschwarm.", "tokens": ["Ge\u00b7r\u00fcs\u00b7tet", ",", "und", "die", "Macht", "mit", "ei\u00b7nem", "Die\u00b7ner\u00b7schwarm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch w\u00e4re nicht hinzu getreten auch die Kraft,", "tokens": ["Doch", "w\u00e4\u00b7re", "nicht", "hin\u00b7zu", "ge\u00b7tre\u00b7ten", "auch", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PTKVZ", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "W\u00e4r' ihr gesammtes Werk geblieben st\u00fcmperhaft.", "tokens": ["W\u00e4r'", "ihr", "ge\u00b7samm\u00b7tes", "Werk", "ge\u00b7blie\u00b7ben", "st\u00fcm\u00b7per\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur wenig richten aus Gewalt und Macht und St\u00e4rke,", "tokens": ["Nur", "we\u00b7nig", "rich\u00b7ten", "aus", "Ge\u00b7walt", "und", "Macht", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O K\u00f6nig, wo die Kraft des Geistes fehlt, das merke.", "tokens": ["O", "K\u00f6\u00b7nig", ",", "wo", "die", "Kraft", "des", "Geis\u00b7tes", "fehlt", ",", "das", "mer\u00b7ke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn g\u00f6ttlich ist die Kraft, und weltlich jene drei;", "tokens": ["Denn", "g\u00f6tt\u00b7lich", "ist", "die", "Kraft", ",", "und", "welt\u00b7lich", "je\u00b7ne", "drei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "PDAT", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was kann die Erde thun, steht nicht der Himmel bei!", "tokens": ["Was", "kann", "die", "Er\u00b7de", "thun", ",", "steht", "nicht", "der", "Him\u00b7mel", "bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}