{"textgrid.poem.58699": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "11. Die Inwohnung Jesu", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist aufgefahren \u00fcber alle Himmel, auf da\u00df er alles erf\u00fcllte. Ephes. 4, 10", "tokens": ["Er", "ist", "auf\u00b7ge\u00b7fah\u00b7ren", "\u00fc\u00b7ber", "al\u00b7le", "Him\u00b7mel", ",", "auf", "da\u00df", "er", "al\u00b7les", "er\u00b7f\u00fcll\u00b7te", ".", "E\u00b7phes", ".", "4", ",", "10"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$,", "APPR", "KOUS", "PPER", "PIS", "VVFIN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+-+--+--+--+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Jesus wollte drum den Himmel \u00fcbersteigen,", "tokens": ["Mein", "Je\u00b7sus", "woll\u00b7te", "drum", "den", "Him\u00b7mel", "\u00fc\u00b7bers\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VMFIN", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er sich innigst k\u00f6nnt' zu meinem Herzen neigen;", "tokens": ["Da\u00df", "er", "sich", "in\u00b7nigst", "k\u00f6nnt'", "zu", "mei\u00b7nem", "Her\u00b7zen", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich soll sein Himmel sein, er ist so nahe mir,", "tokens": ["Ich", "soll", "sein", "Him\u00b7mel", "sein", ",", "er", "ist", "so", "na\u00b7he", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm, Jesu, nimm mich ein, komm, f\u00fclle mich mit dir!", "tokens": ["Komm", ",", "Je\u00b7su", ",", "nimm", "mich", "ein", ",", "komm", ",", "f\u00fcl\u00b7le", "mich", "mit", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "VVIMP", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}