{"textgrid.poem.37827": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Liebesdienst", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Markgraf \u00fcber dem Rhein,", "tokens": ["Es", "war", "ein", "Mark\u00b7graf", "\u00fc\u00b7ber", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der hatte drey sch\u00f6ne T\u00f6chterlein;", "tokens": ["Der", "hat\u00b7te", "drey", "sch\u00f6\u00b7ne", "T\u00f6ch\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zwey T\u00f6chterlein fr\u00fch heirathen weg,", "tokens": ["Zwey", "T\u00f6ch\u00b7ter\u00b7lein", "fr\u00fch", "hei\u00b7ra\u00b7then", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die dritt hat ihn ins Grab gelegt.", "tokens": ["Die", "dritt", "hat", "ihn", "ins", "Grab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann ging sie singen vor Schwesters Th\u00fcr:", "tokens": ["Dann", "ging", "sie", "sin\u00b7gen", "vor", "Schwes\u00b7ters", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbach braucht ihr keine Dienstmagd hier?\u00ab", "tokens": ["\u00bb", "ach", "braucht", "ihr", "kei\u00b7ne", "Dienst\u00b7magd", "hier", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbei M\u00e4dchen, du bist mir viel zu fein,", "tokens": ["\u00bb", "ei", "M\u00e4d\u00b7chen", ",", "du", "bist", "mir", "viel", "zu", "fein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du gehst gern mit den Herrelein.\u00ab", "tokens": ["Du", "gehst", "gern", "mit", "den", "Her\u00b7re\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbach nein! ach nein! das thu ich nicht,", "tokens": ["\u00bb", "ach", "nein", "!", "ach", "nein", "!", "das", "thu", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$.", "XY", "PTKANT", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich so mit den Herrlein geh!\u00ab", "tokens": ["Da\u00df", "ich", "so", "mit", "den", "Herr\u00b7lein", "geh", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie dingt das M\u00e4gdlein ein halbes Jahr,", "tokens": ["Sie", "dingt", "das", "M\u00e4gd\u00b7lein", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das M\u00e4gdlein dient ihr sieben Jahr.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "dient", "ihr", "sie\u00b7ben", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als die sieben Jahr um warn,", "tokens": ["Und", "als", "die", "sie\u00b7ben", "Jahr", "um", "warn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "CARD", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wurd das M\u00e4gdlein t\u00e4glich krank;", "tokens": ["Da", "wurd", "das", "M\u00e4gd\u00b7lein", "t\u00e4g\u00b7lich", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsag M\u00e4gdlein, wenn du krank willst seyn,", "tokens": ["\u00bb", "sag", "M\u00e4gd\u00b7lein", ",", "wenn", "du", "krank", "willst", "seyn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sag mir, wer sind die Aeltern dein?\u00ab", "tokens": ["So", "sag", "mir", ",", "wer", "sind", "die", "A\u00b7el\u00b7tern", "dein", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "VAFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbmein Vater war Markgraf \u00fcber dem Rhein,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", "war", "Mark\u00b7graf", "\u00fc\u00b7ber", "dem", "Rhein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NE", "APPR", "ART", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ich bin sein j\u00fcngstes T\u00f6chterlein.\u00ab", "tokens": ["Und", "ich", "bin", "sein", "j\u00fcngs\u00b7tes", "T\u00f6ch\u00b7ter\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbach nein! ach nein, das glaub ich nicht,", "tokens": ["\u00bb", "ach", "nein", "!", "ach", "nein", ",", "das", "glaub", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$.", "XY", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du meine j\u00fcngste Schwester bist!\u00ab", "tokens": ["Da\u00df", "du", "mei\u00b7ne", "j\u00fcngs\u00b7te", "Schwes\u00b7ter", "bist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbund wenn du mir's nicht glauben willst,", "tokens": ["\u00bb", "und", "wenn", "du", "mir's", "nicht", "glau\u00b7ben", "willst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "NE", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So geh nur an meine Kiste hin,", "tokens": ["So", "geh", "nur", "an", "mei\u00b7ne", "Kis\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Daran wird es geschrieben stehn.\u00ab", "tokens": ["Da\u00b7ran", "wird", "es", "ge\u00b7schrie\u00b7ben", "stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "VVINF", "$.", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und als sie an die Kiste kam,", "tokens": ["Und", "als", "sie", "an", "die", "Kis\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da rannen ihr die Backen ab:", "tokens": ["Da", "ran\u00b7nen", "ihr", "die", "Ba\u00b7cken", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach bringt mir Weck, ach bringt mir Wein,", "tokens": ["\u00bb", "ach", "bringt", "mir", "Weck", ",", "ach", "bringt", "mir", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist mein j\u00fcngstes Schwesterlein!\u00ab", "tokens": ["Das", "ist", "mein", "j\u00fcngs\u00b7tes", "Schwes\u00b7ter\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbich will auch kein Weck, ich will auch kein Wein,", "tokens": ["\u00bb", "ich", "will", "auch", "kein", "Weck", ",", "ich", "will", "auch", "kein", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Will nur ein kleines L\u00e4delein,", "tokens": ["Will", "nur", "ein", "klei\u00b7nes", "L\u00e4\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darin ich will begraben seyn.\u00ab", "tokens": ["Da\u00b7rin", "ich", "will", "be\u00b7gra\u00b7ben", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}