{"textgrid.poem.43710": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine Seemann", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein Schiff gebauet,", "tokens": ["Ich", "hab'", "ein", "Schiff", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seht her! ich setz' es aus.", "tokens": ["Seht", "her", "!", "ich", "setz'", "es", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es segelt flott vom Lande", "tokens": ["Es", "se\u00b7gelt", "flott", "vom", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hin\u00fcber nach dem Strande,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "nach", "dem", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trotz Wind und Wellenbraus.", "tokens": ["Trotz", "Wind", "und", "Wel\u00b7len\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und s\u00e4\u00df' ich selber drinnen", "tokens": ["Und", "s\u00e4\u00df'", "ich", "sel\u00b7ber", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als lust'ger Steuermann,", "tokens": ["Als", "lust'\u00b7ger", "Steu\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollt's nach allen Seiten", "tokens": ["Ich", "wollt's", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Laviren, dreh'n und leiten,", "tokens": ["La\u00b7vi\u00b7ren", ",", "dreh'n", "und", "lei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "CARD", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wie's mein Vater kann.", "tokens": ["So", "wie's", "mein", "Va\u00b7ter", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn ich gr\u00f6\u00dfer werde,", "tokens": ["Und", "wenn", "ich", "gr\u00f6\u00b7\u00dfer", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon freu' ich mich, juchhe!", "tokens": ["Schon", "freu'", "ich", "mich", ",", "juch\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nichts h\u00e4lt mich mehr am Lande", "tokens": ["Nichts", "h\u00e4lt", "mich", "mehr", "am", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im \u00f6den D\u00fcnensande,", "tokens": ["Im", "\u00f6\u00b7den", "D\u00fc\u00b7nen\u00b7san\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich will, ich mu\u00df zur See!", "tokens": ["Ich", "will", ",", "ich", "mu\u00df", "zur", "See", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In meinem rothen Hemde,", "tokens": ["In", "mei\u00b7nem", "ro\u00b7then", "Hem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit meinem runden Hut,", "tokens": ["Mit", "mei\u00b7nem", "run\u00b7den", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So fahr' ich als Matrose", "tokens": ["So", "fahr'", "ich", "als", "Mat\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchs wilde Meergetose", "tokens": ["Durchs", "wil\u00b7de", "Meer\u00b7ge\u00b7to\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voll Ruh und frohen Muth.", "tokens": ["Voll", "Ruh", "und", "fro\u00b7hen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}