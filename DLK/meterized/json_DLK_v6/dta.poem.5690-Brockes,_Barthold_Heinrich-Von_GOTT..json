{"dta.poem.5690": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus diesem G\u00f6ttlichen und ew'gen Grunde", "tokens": ["Aus", "die\u00b7sem", "G\u00f6tt\u00b7li\u00b7chen", "und", "ew'\u00b7gen", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam alles, und entstunde,", "tokens": ["Kam", "al\u00b7les", ",", "und", "ent\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6mmt alles, und entsteht. Mein Wesen ist ein Geist.", "tokens": ["K\u00f6mmt", "al\u00b7les", ",", "und", "ent\u00b7steht", ".", "Mein", "We\u00b7sen", "ist", "ein", "Geist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KON", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Geist der\u2019s Vorzugs-Recht der Unverg\u00e4nglichkeit", "tokens": ["Ein", "Geist", "der's", "Vor\u00b7zugs\u00b7Recht", "der", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Besitzet und geneusst.", "tokens": ["Be\u00b7sit\u00b7zet", "und", "ge\u00b7neusst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An Jhm und seiner Krafft Beschaffenheit", "tokens": ["An", "Jhm", "und", "sei\u00b7ner", "Krafft", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ereignet sich der mindste Zweifel nicht:", "tokens": ["Er\u00b7eig\u00b7net", "sich", "der", "minds\u00b7te", "Zwei\u00b7fel", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vielmehr betrifft der Zweifel blo\u00df allein", "tokens": ["Viel\u00b7mehr", "be\u00b7tr\u00b7ifft", "der", "Zwei\u00b7fel", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die Vorw\u00fcrff\u2019, welche leiblich seyn.", "tokens": ["Die", "Vor\u00b7w\u00fcrff'", ",", "wel\u00b7che", "leib\u00b7lich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich dencke; Darum mu\u00df die Seele, da\u00df sie sey,", "tokens": ["Ich", "den\u00b7cke", ";", "Da\u00b7rum", "mu\u00df", "die", "See\u00b7le", ",", "da\u00df", "sie", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PAV", "VMFIN", "ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Unwidersprechlich wissen.", "tokens": ["Un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "wis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Allein indem ich wei\u00df; ich bin; werd\u2019 ich dabey", "tokens": ["Al\u00b7lein", "in\u00b7dem", "ich", "wei\u00df", ";", "ich", "bin", ";", "werd'", "ich", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "$.", "VAFIN", "PPER", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "An Dingen, die so vielerley,", "tokens": ["An", "Din\u00b7gen", ",", "die", "so", "vie\u00b7ler\u00b7ley", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ob sie auch das sind, was sie scheinen; zweiflen m\u00fcssen.", "tokens": ["Ob", "sie", "auch", "das", "sind", ",", "was", "sie", "schei\u00b7nen", ";", "zwei\u00b7flen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$.", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ob die mit Glanz erf\u00fcllte Pracht,", "tokens": ["Ob", "die", "mit", "Glanz", "er\u00b7f\u00fcll\u00b7te", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Tag und Himmel helle macht,", "tokens": ["Die", "Tag", "und", "Him\u00b7mel", "hel\u00b7le", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ob so viel C\u00f6rper, die entstehn und sich verlieren,", "tokens": ["Ob", "so", "viel", "C\u00f6r\u00b7per", ",", "die", "ent\u00b7stehn", "und", "sich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "PRELS", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit einer W\u00fcrcklichkeit mir meine Sinne r\u00fchren.", "tokens": ["Mit", "ei\u00b7ner", "W\u00fcrck\u00b7lich\u00b7keit", "mir", "mei\u00b7ne", "Sin\u00b7ne", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}