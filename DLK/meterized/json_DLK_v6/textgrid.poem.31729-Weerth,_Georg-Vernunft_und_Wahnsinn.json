{"textgrid.poem.31729": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vernunft und Wahnsinn", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Morgen tr\u00e4umt nicht, was der Abend bringt,", "tokens": ["Dem", "Mor\u00b7gen", "tr\u00e4umt", "nicht", ",", "was", "der", "A\u00b7bend", "bringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn l\u00e4chelnd wohl aus rosenrotem Osten", "tokens": ["Wenn", "l\u00e4\u00b7chelnd", "wohl", "aus", "ro\u00b7sen\u00b7ro\u00b7tem", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein erster Strahl durch Wald und Fluren dringt,", "tokens": ["Sein", "ers\u00b7ter", "Strahl", "durch", "Wald", "und", "Flu\u00b7ren", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Taues frische Perlensaat zu kosten,", "tokens": ["Des", "Tau\u00b7es", "fri\u00b7sche", "Per\u00b7len\u00b7saat", "zu", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ihr Erwachen hell die Amsel preist", "tokens": ["Wenn", "ihr", "Er\u00b7wa\u00b7chen", "hell", "die", "Am\u00b7sel", "preist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Hirsche wandeln zu des Tales Bronnen,", "tokens": ["Und", "Hir\u00b7sche", "wan\u00b7deln", "zu", "des", "Ta\u00b7les", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn um die Gletscher still der Adler kreist,", "tokens": ["Wenn", "um", "die", "Glet\u00b7scher", "still", "der", "Ad\u00b7ler", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich in der Fr\u00fche heil'gem Licht zu sonnen.", "tokens": ["Sich", "in", "der", "Fr\u00fc\u00b7he", "heil'\u00b7gem", "Licht", "zu", "son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Blau schaut die Blume aus des Feldes Garben,", "tokens": ["Blau", "schaut", "die", "Blu\u00b7me", "aus", "des", "Fel\u00b7des", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf Moor und Weiher schwankt des Schilfes Kranz.", "tokens": ["Auf", "Moor", "und", "Wei\u00b7her", "schwankt", "des", "Schil\u00b7fes", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es flie\u00dft der Strom in Regenbogenfarben", "tokens": ["Es", "flie\u00dft", "der", "Strom", "in", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Meere, wiegend seiner Wellen Tanz.", "tokens": ["Zum", "Mee\u00b7re", ",", "wie\u00b7gend", "sei\u00b7ner", "Wel\u00b7len", "Tanz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und rauschend im gewalt'gen Wogenliede", "tokens": ["Und", "rau\u00b7schend", "im", "ge\u00b7walt'\u00b7gen", "Wo\u00b7gen\u00b7lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dehnt unabsehbar sich die gr\u00fcne Flut \u2013", "tokens": ["Dehnt", "un\u00b7ab\u00b7seh\u00b7bar", "sich", "die", "gr\u00fc\u00b7ne", "Flut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Und Freude nur und wundervoller Friede", "tokens": ["Und", "Freu\u00b7de", "nur", "und", "wun\u00b7der\u00b7vol\u00b7ler", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf Festland, Insel und Gew\u00e4ssern ruht.", "tokens": ["Auf", "Fest\u00b7land", ",", "In\u00b7sel", "und", "Ge\u00b7w\u00e4s\u00b7sern", "ruht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch wie zum Mittag wandelt sich der Morgen,", "tokens": ["Doch", "wie", "zum", "Mit\u00b7tag", "wan\u00b7delt", "sich", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00fcllt sich in Schleier auch des Tages Pracht.", "tokens": ["H\u00fcllt", "sich", "in", "Schlei\u00b7er", "auch", "des", "Ta\u00b7ges", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was einer fr\u00fchen Stunde tief verborgen,", "tokens": ["Was", "ei\u00b7ner", "fr\u00fc\u00b7hen", "Stun\u00b7de", "tief", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es bricht herein mit Angst und Graus und Nacht.", "tokens": ["Es", "bricht", "her\u00b7ein", "mit", "Angst", "und", "Graus", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel t\u00f6nt von rasselnden Gewittern,", "tokens": ["Der", "Him\u00b7mel", "t\u00f6nt", "von", "ras\u00b7seln\u00b7den", "Ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Erde zuckt und birst zu j\u00e4hem Spalt,", "tokens": ["Die", "Er\u00b7de", "zuckt", "und", "birst", "zu", "j\u00e4\u00b7hem", "Spalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und heulend \u00fcber Fels und Eichensplittern", "tokens": ["Und", "heu\u00b7lend", "\u00fc\u00b7ber", "Fels", "und", "Ei\u00b7chen\u00b7split\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Sturm entfesselt seine Bahnen wallt.", "tokens": ["Der", "Sturm", "ent\u00b7fes\u00b7selt", "sei\u00b7ne", "Bah\u00b7nen", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es rast die Brandung an zerfetzten K\u00fcsten,", "tokens": ["Es", "rast", "die", "Bran\u00b7dung", "an", "zer\u00b7fetz\u00b7ten", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Dunkel herrscht, bis aus entw\u00f6lkten H\u00f6hn,", "tokens": ["Und", "Dun\u00b7kel", "herrscht", ",", "bis", "aus", "ent\u00b7w\u00f6lk\u00b7ten", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ob sie nichts von Sturm und Wetter w\u00fc\u00dften,", "tokens": ["Als", "ob", "sie", "nichts", "von", "Sturm", "und", "Wet\u00b7ter", "w\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sterne ruhig strahlend niedersehn.", "tokens": ["Die", "Ster\u00b7ne", "ru\u00b7hig", "strah\u00b7lend", "nie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und die vom Staub bis auf zum Firmamente", "tokens": ["Und", "die", "vom", "Staub", "bis", "auf", "zum", "Fir\u00b7ma\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "APPR", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gew\u00e4lzt sich mit d\u00e4monischer Gewalt:", "tokens": ["Ge\u00b7w\u00e4lzt", "sich", "mit", "d\u00e4\u00b7mo\u00b7ni\u00b7scher", "Ge\u00b7walt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie schlummern dann, die starken Elemente,", "tokens": ["Sie", "schlum\u00b7mern", "dann", ",", "die", "star\u00b7ken", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bis sie ein neuer Kampf zusammenballt.", "tokens": ["Bis", "sie", "ein", "neu\u00b7er", "Kampf", "zu\u00b7sam\u00b7men\u00b7ballt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So ewiglich, mit wechselndem Gestalten,", "tokens": ["So", "e\u00b7wig\u00b7lich", ",", "mit", "wech\u00b7seln\u00b7dem", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sklavischen Laufes rollt und kreist das All!", "tokens": ["Skla\u00b7vi\u00b7schen", "Lau\u00b7fes", "rollt", "und", "kreist", "das", "All", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nicht sch\u00f6ner mag sich die Natur entfalten,", "tokens": ["Nicht", "sch\u00f6\u00b7ner", "mag", "sich", "die", "Na\u00b7tur", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch wenden sich als zu gewohntem Fall.", "tokens": ["Noch", "wen\u00b7den", "sich", "als", "zu", "ge\u00b7wohn\u00b7tem", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Welt und Welten aneinander bannte", "tokens": ["Die", "Welt", "und", "Wel\u00b7ten", "an\u00b7ein\u00b7an\u00b7der", "bann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit unerbittlicher Notwendigkeit:", "tokens": ["Mit", "un\u00b7er\u00b7bitt\u00b7li\u00b7cher", "Not\u00b7wen\u00b7dig\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nur in den Geistern ihrer Menschen brannte", "tokens": ["Nur", "in", "den", "Geis\u00b7tern", "ih\u00b7rer", "Men\u00b7schen", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie fort zu schrankenloser Herrlichkeit!", "tokens": ["Sie", "fort", "zu", "schran\u00b7ken\u00b7lo\u00b7ser", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Seit von der Lippe greiser Patriarchen", "tokens": ["Seit", "von", "der", "Lip\u00b7pe", "grei\u00b7ser", "Pat\u00b7ri\u00b7ar\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Weisheit blumenreiche Rede flo\u00df,", "tokens": ["Der", "Weis\u00b7heit", "blu\u00b7men\u00b7rei\u00b7che", "Re\u00b7de", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis wo die Sch\u00e4del st\u00fcrzender Monarchen", "tokens": ["Bis", "wo", "die", "Sch\u00e4\u00b7del", "st\u00fcr\u00b7zen\u00b7der", "Mon\u00b7ar\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerstampft der Freiheit jugendliches Ro\u00df:", "tokens": ["Zer\u00b7stampft", "der", "Frei\u00b7heit", "ju\u00b7gend\u00b7li\u00b7ches", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hat die Natur mit ihrer Donnerstimme", "tokens": ["Hat", "die", "Na\u00b7tur", "mit", "ih\u00b7rer", "Don\u00b7ner\u00b7stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gesungen stets den mahnenden Gesang,", "tokens": ["Ge\u00b7sun\u00b7gen", "stets", "den", "mah\u00b7nen\u00b7den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df jeder folge seinem Gram und Grimme", "tokens": ["Da\u00df", "je\u00b7der", "fol\u00b7ge", "sei\u00b7nem", "Gram", "und", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie seines Herzens liebevollem Drang.", "tokens": ["Wie", "sei\u00b7nes", "Her\u00b7zens", "lie\u00b7be\u00b7vol\u00b7lem", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die gleich der M\u00f6we keck die See umschwanken,", "tokens": ["Die", "gleich", "der", "M\u00f6\u00b7we", "keck", "die", "See", "um\u00b7schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "