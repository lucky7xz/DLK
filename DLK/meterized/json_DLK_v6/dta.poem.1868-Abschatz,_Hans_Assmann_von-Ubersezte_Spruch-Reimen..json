{"dta.poem.1868": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen Glantz verf\u00e4llt/ doch steigt sie wieder auff;", "tokens": ["Der", "Son\u00b7nen", "Glantz", "ver\u00b7f\u00e4llt", "/", "doch", "steigt", "sie", "wie\u00b7der", "auff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was von dem Winter stirbt/ sieht man im Sommer", "tokens": ["Was", "von", "dem", "Win\u00b7ter", "stirbt", "/", "sieht", "man", "im", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN", "PIS", "APPRART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "bl\u00fchn.", "tokens": ["bl\u00fchn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Beschl\u00fcssen wir einmahl den kurtzen Lebens-Lauff/", "tokens": ["Be\u00b7schl\u00fcs\u00b7sen", "wir", "ein\u00b7mahl", "den", "kurt\u00b7zen", "Le\u00b7bens\u00b7Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schlaffen wir hernach die lange Nacht dahin.", "tokens": ["So", "schlaf\u00b7fen", "wir", "her\u00b7nach", "die", "lan\u00b7ge", "Nacht", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wenn wir uns nur wohl zur s\u00fcssen Ruhe strecken/", "tokens": ["Doch", "wenn", "wir", "uns", "nur", "wohl", "zur", "s\u00fcs\u00b7sen", "Ru\u00b7he", "stre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So folget endlich auch ein freudig Aufferwecken.", "tokens": ["So", "fol\u00b7get", "end\u00b7lich", "auch", "ein", "freu\u00b7dig", "Auf\u00b7fer\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}