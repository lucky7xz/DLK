{"textgrid.poem.25327": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sie rings mich umflattern,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie rings mich umflattern,", "tokens": ["Wie", "sie", "rings", "mich", "um\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die platten Kobolde des Alltags!", "tokens": ["Die", "plat\u00b7ten", "Ko\u00b7bol\u00b7de", "des", "All\u00b7tags", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie sie sich an mich h\u00e4ngen,", "tokens": ["Wie", "sie", "sich", "an", "mich", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glatt und klein,", "tokens": ["Glatt", "und", "klein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Am Rock mich zerren,", "tokens": ["Am", "Rock", "mich", "zer\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mich nieder zu st\u00fcrzen von meiner sonnigen Felsenh\u00f6he", "tokens": ["Mich", "nie\u00b7der", "zu", "st\u00fcr\u00b7zen", "von", "mei\u00b7ner", "son\u00b7ni\u00b7gen", "Fel\u00b7sen\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Hinab in's Thal,", "tokens": ["Hin\u00b7ab", "in's", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bis ich auch so bin,", "tokens": ["Bis", "ich", "auch", "so", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "So klug und fromm und matt und kalt", "tokens": ["So", "klug", "und", "fromm", "und", "matt", "und", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie die Andern ...", "tokens": ["Wie", "die", "An\u00b7dern", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "O du brausende Sturmnacht,", "tokens": ["O", "du", "brau\u00b7sen\u00b7de", "Sturm\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die du \u00fcber der erbebenden Herbsthaide", "tokens": ["Die", "du", "\u00fc\u00b7ber", "der", "er\u00b7be\u00b7ben\u00b7den", "Herbst\u00b7hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dein jauchzendes Lied geigst,", "tokens": ["Dein", "jauch\u00b7zen\u00b7des", "Lied", "geigst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kraft will ich mir trinken an deinen str\u00f6menden Quellen,", "tokens": ["Kraft", "will", "ich", "mir", "trin\u00b7ken", "an", "dei\u00b7nen", "str\u00f6\u00b7men\u00b7den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Jungfrische Kraft zu nimmer verlodernder Leidenschaft!", "tokens": ["Jung\u00b7fri\u00b7sche", "Kraft", "zu", "nim\u00b7mer", "ver\u00b7lo\u00b7dern\u00b7der", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wie das gellt und flammt und rauscht! ...", "tokens": ["Wie", "das", "gellt", "und", "flammt", "und", "rauscht", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PDS", "VVPP", "KON", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und jetzt \u2013", "tokens": ["Und", "jetzt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "War's nicht mein Name, der aus Nacht und Sturm", "tokens": ["Wa\u00b7r's", "nicht", "mein", "Na\u00b7me", ",", "der", "aus", "Nacht", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Wildlockend zu mir drang?", "tokens": ["Wild\u00b7lo\u00b7ckend", "zu", "mir", "drang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Du rufst ... Ich komme ... Hinaus!", "tokens": ["Du", "rufst", "...", "Ich", "kom\u00b7me", "...", "Hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Lachend pre\u00dft mich der Sturm in die feuchten Arme,", "tokens": ["La\u00b7chend", "pre\u00dft", "mich", "der", "Sturm", "in", "die", "feuch\u00b7ten", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Doch ich rei\u00dfe mich los und st\u00fcrme weiter,", "tokens": ["Doch", "ich", "rei\u00b7\u00dfe", "mich", "los", "und", "st\u00fcr\u00b7me", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Die H\u00f6he empor, bis die Lichter der Stadt untertauchen", "tokens": ["Die", "H\u00f6\u00b7he", "em\u00b7por", ",", "bis", "die", "Lich\u00b7ter", "der", "Stadt", "un\u00b7ter\u00b7tau\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+--++-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "In den Fluten der Nacht ...", "tokens": ["In", "den", "Flu\u00b7ten", "der", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Hei! Hexentanz auf sturmgepeitschter Haide!", "tokens": ["Hei", "!", "He\u00b7xen\u00b7tanz", "auf", "sturm\u00b7ge\u00b7peitschter", "Hai\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.16": {"text": "Blaue Flammen zucken \u00fcber windzerriss'ne wei\u00dfe M\u00e4ntel,", "tokens": ["Blau\u00b7e", "Flam\u00b7men", "zu\u00b7cken", "\u00fc\u00b7ber", "wind\u00b7zer\u00b7riss'\u00b7ne", "wei\u00b7\u00dfe", "M\u00e4n\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Die im Tanze flattern zum Liede des Sturms!", "tokens": ["Die", "im", "Tan\u00b7ze", "flat\u00b7tern", "zum", "Lie\u00b7de", "des", "Sturms", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und dazwischen Johlen und Jauchzen,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "Joh\u00b7len", "und", "Jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.19": {"text": "Grell und tausendstimmig ...", "tokens": ["Grell", "und", "tau\u00b7sends\u00b7tim\u00b7mig", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Da \u2013 pl\u00f6tzlich wird's still: Schweigen verzittert", "tokens": ["Da", "\u2013", "pl\u00f6tz\u00b7lich", "wird's", "still", ":", "Schwei\u00b7gen", "ver\u00b7zit\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$(", "ADJD", "VAFIN", "ADJD", "$.", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "In atemloses Lauschen.", "tokens": ["In", "a\u00b7tem\u00b7lo\u00b7ses", "Lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und dann: Flammender Lichtschein vom Walde her,", "tokens": ["Und", "dann", ":", "Flam\u00b7men\u00b7der", "Licht\u00b7schein", "vom", "Wal\u00b7de", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Hufschlag dr\u00f6hnt, Raben rauschen,", "tokens": ["Huf\u00b7schlag", "dr\u00f6hnt", ",", "Ra\u00b7ben", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.24": {"text": "Er naht:", "tokens": ["Er", "naht", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Still und ernst kommt er geritten,", "tokens": ["Still", "und", "ernst", "kommt", "er", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Das eine Auge vom Hut beschattet.", "tokens": ["Das", "ei\u00b7ne", "Au\u00b7ge", "vom", "Hut", "be\u00b7schat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Das er hin gab heiliger Weisheit zum Pfande,", "tokens": ["Das", "er", "hin", "gab", "hei\u00b7li\u00b7ger", "Weis\u00b7heit", "zum", "Pfan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Er, der Gott einst war, als noch ein Volk", "tokens": ["Er", ",", "der", "Gott", "einst", "war", ",", "als", "noch", "ein", "Volk"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "ADV", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Trotzigen, sonnigen Jugendmutes voll durch W\u00e4lder jagte,", "tokens": ["Trot\u00b7zi\u00b7gen", ",", "son\u00b7ni\u00b7gen", "Ju\u00b7gend\u00b7mu\u00b7tes", "voll", "durch", "W\u00e4l\u00b7der", "jag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.30": {"text": "Er, der Allwisser, der wissend k\u00e4mpfte", "tokens": ["Er", ",", "der", "All\u00b7wis\u00b7ser", ",", "der", "wis\u00b7send", "k\u00e4mpf\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Und hinsank aus Walhalls rauchender Flammenflut,", "tokens": ["Und", "hin\u00b7sank", "aus", "Wal\u00b7halls", "rau\u00b7chen\u00b7der", "Flam\u00b7men\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Er, der Treue H\u00fcter, dem sein Volk die Treue brach", "tokens": ["Er", ",", "der", "Treu\u00b7e", "H\u00fc\u00b7ter", ",", "dem", "sein", "Volk", "die", "Treu\u00b7e", "brach"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.33": {"text": "Um eines fremden Gottes willen ...", "tokens": ["Um", "ei\u00b7nes", "frem\u00b7den", "Got\u00b7tes", "wil\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Jetzt trifft mich sein Auge,", "tokens": ["Jetzt", "trifft", "mich", "sein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Dies Auge voll flammend-ungemess'ner Kraft,", "tokens": ["Dies", "Au\u00b7ge", "voll", "flam\u00b7men\u00b7dun\u00b7ge\u00b7mess'\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Das doch so mild-g\u00fctig blicken kann", "tokens": ["Das", "doch", "so", "mild\u00b7g\u00fc\u00b7tig", "bli\u00b7cken", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVINF", "VMFIN"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.37": {"text": "Wie tauender Mondschein,", "tokens": ["Wie", "tau\u00b7en\u00b7der", "Mond\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Und ich st\u00fcrze hin und umklammere sein Knie,", "tokens": ["Und", "ich", "st\u00fcr\u00b7ze", "hin", "und", "um\u00b7klam\u00b7me\u00b7re", "sein", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Und meine Stimme bebt:", "tokens": ["Und", "mei\u00b7ne", "Stim\u00b7me", "bebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "\u00bbder du selber ungez\u00fcgelt trotzige Kraft bist,", "tokens": ["\u00bb", "der", "du", "sel\u00b7ber", "un\u00b7ge\u00b7z\u00fc\u00b7gelt", "trot\u00b7zi\u00b7ge", "Kraft", "bist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ADV", "ADJD", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.41": {"text": "Wahr' mich vor Unkraft!", "tokens": ["Wahr'", "mich", "vor", "Un\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Sieh'! Ich kann ja nicht sprechen,", "tokens": ["Sieh'", "!", "Ich", "kann", "ja", "nicht", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.43": {"text": "Meine Gedanken sch\u00e4umen auf", "tokens": ["Mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "sch\u00e4u\u00b7men", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.44": {"text": "Wie des Wildbachs braune Flut \u2013", "tokens": ["Wie", "des", "Wild\u00b7bachs", "brau\u00b7ne", "Flut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Kann nicht sprechen; aber du, der du einst", "tokens": ["Kann", "nicht", "spre\u00b7chen", ";", "a\u00b7ber", "du", ",", "der", "du", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$.", "KON", "PPER", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.46": {"text": "Gleichen Kampf k\u00e4mpftest,", "tokens": ["Glei\u00b7chen", "Kampf", "k\u00e4mpf\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Mu\u00dft mich verstehen und mu\u00dft mich sch\u00fctzen", "tokens": ["Mu\u00dft", "mich", "ver\u00b7ste\u00b7hen", "und", "mu\u00dft", "mich", "sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VMFIN", "PPER", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.48": {"text": "Vor denen dort unten ...\u00ab", "tokens": ["Vor", "de\u00b7nen", "dort", "un\u00b7ten", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "$(", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Eine Hand auf meinem Haupt.", "tokens": ["Ei\u00b7ne", "Hand", "auf", "mei\u00b7nem", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Und eine milde Stimme:", "tokens": ["Und", "ei\u00b7ne", "mil\u00b7de", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "\u00bbkomm' mit!\u00ab", "tokens": ["\u00bb", "komm'", "mit", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Und pl\u00f6tzlich b\u00e4umt sich ein Ro\u00df mir unter den Schenkeln,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "b\u00e4umt", "sich", "ein", "Ro\u00df", "mir", "un\u00b7ter", "den", "Schen\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Peitschen gellen, Lichter wirbeln auf und schrille Rufe,", "tokens": ["Peit\u00b7schen", "gel\u00b7len", ",", "Lich\u00b7ter", "wir\u00b7beln", "auf", "und", "schril\u00b7le", "Ru\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Und durch die zitternde Nacht prasselt das Heer", "tokens": ["Und", "durch", "die", "zit\u00b7tern\u00b7de", "Nacht", "pras\u00b7selt", "das", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie lodernde