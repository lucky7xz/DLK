{"textgrid.poem.39770": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilf Jahre sind's, ich steh' in Schweden,", "tokens": ["Eilf", "Jah\u00b7re", "sin\u00b7d's", ",", "ich", "steh'", "in", "Schwe\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Bootes Dampfrauch hinter mir.", "tokens": ["Des", "Boo\u00b7tes", "Dam\u00b7pfrauch", "hin\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir, geliebtes Kind, von dir!", "tokens": ["Von", "dir", ",", "ge\u00b7lieb\u00b7tes", "Kind", ",", "von", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verzweifelt st\u00fcrzt mit wildem Sch\u00e4umen", "tokens": ["Ver\u00b7zwei\u00b7felt", "st\u00fcrzt", "mit", "wil\u00b7dem", "Sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ganzer See dem Meere zu:", "tokens": ["Ein", "gan\u00b7zer", "See", "dem", "Mee\u00b7re", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ri\u00df nach langen Hoffnungstr\u00e4umen", "tokens": ["So", "ri\u00df", "nach", "lan\u00b7gen", "Hoff\u00b7nungs\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Tod mein Leben aus der Ruh'.", "tokens": ["Dein", "Tod", "mein", "Le\u00b7ben", "aus", "der", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier steh' ich in der Wellen Brandung,", "tokens": ["Hier", "steh'", "ich", "in", "der", "Wel\u00b7len", "Bran\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehne mich, und suche dich,", "tokens": ["Und", "seh\u00b7ne", "mich", ",", "und", "su\u00b7che", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und find' im Strudel keine Landung;", "tokens": ["Und", "find'", "im", "Stru\u00b7del", "kei\u00b7ne", "Lan\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, Tod und Nacht umbrausen mich!", "tokens": ["Ach", ",", "Tod", "und", "Nacht", "um\u00b7brau\u00b7sen", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und deiner Mutter mu\u00df ich denken:", "tokens": ["Und", "dei\u00b7ner", "Mut\u00b7ter", "mu\u00df", "ich", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie diese Fichten h\u00e4ngt ihr Mut,", "tokens": ["Wie", "die\u00b7se", "Fich\u00b7ten", "h\u00e4ngt", "ihr", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sich in ew'gem Thaue tr\u00e4nken", "tokens": ["Die", "sich", "in", "ew'\u00b7gem", "Thau\u00b7e", "tr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Haupt und Wurzel in der Flut. \u2013", "tokens": ["Mit", "Haupt", "und", "Wur\u00b7zel", "in", "der", "Flut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch sieh! es funkeln alle Wellen,", "tokens": ["Doch", "sieh", "!", "es", "fun\u00b7keln", "al\u00b7le", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und pl\u00f6tzlich gl\u00fcht der Hain in Pracht,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "gl\u00fcht", "der", "Hain", "in", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Abendsonne Stralen quellen", "tokens": ["Der", "A\u00b7bend\u00b7son\u00b7ne", "Stra\u00b7len", "quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck aus Schwarzwaldwassernacht.", "tokens": ["Zu\u00b7r\u00fcck", "aus", "Schwarz\u00b7wald\u00b7was\u00b7ser\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und dr\u00e4ng' ein Augenwink vom Himmel", "tokens": ["Und", "dr\u00e4ng'", "ein", "Au\u00b7gen\u00b7wink", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht auch ins finstre Herz hinab?", "tokens": ["Nicht", "auch", "ins", "finst\u00b7re", "Herz", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er spielt im Wogenstaubgewimmel,", "tokens": ["Er", "spielt", "im", "Wo\u00b7gen\u00b7staub\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er perlt im Thau auf einem Grab.", "tokens": ["Er", "perlt", "im", "Thau", "auf", "ei\u00b7nem", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}