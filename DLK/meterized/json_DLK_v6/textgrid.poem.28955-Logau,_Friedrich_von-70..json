{"textgrid.poem.28955": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr reich bistu und auch sehr karg, Chrysophilus; mich d\u00fcnckt,", "tokens": ["Sehr", "reich", "bis\u00b7tu", "und", "auch", "sehr", "karg", ",", "Chry\u00b7so\u00b7phi\u00b7lus", ";", "mich", "d\u00fcnckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADV", "ADV", "ADJD", "$,", "NE", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Gold, wann es gefangen liegt, nicht mehr als Eisen bringt.", "tokens": ["Das", "Gold", ",", "wann", "es", "ge\u00b7fan\u00b7gen", "liegt", ",", "nicht", "mehr", "als", "Ei\u00b7sen", "bringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,", "PTKNEG", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}