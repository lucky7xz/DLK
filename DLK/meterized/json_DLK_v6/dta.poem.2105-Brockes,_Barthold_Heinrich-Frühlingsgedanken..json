{"dta.poem.2105": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welchem fast f\u00fcr Lust erstaunenden Vergn\u00fcgen", "tokens": ["Mit", "wel\u00b7chem", "fast", "f\u00fcr", "Lust", "er\u00b7stau\u00b7nen\u00b7den", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bemerken wir mit Recht in dieser Zeit", "tokens": ["Be\u00b7mer\u00b7ken", "wir", "mit", "Recht", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der itzt erscheinenden Gesch\u00f6pfe Herrlichkeit,", "tokens": ["Der", "itzt", "er\u00b7schei\u00b7nen\u00b7den", "Ge\u00b7sch\u00f6p\u00b7fe", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die, eh\u2019 man es bemerkt, uns vor den Augen liegen.", "tokens": ["Die", ",", "eh'", "man", "es", "be\u00b7merkt", ",", "uns", "vor", "den", "Au\u00b7gen", "lie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein d\u00fcnnes Gr\u00fcn bedeckt die Felder,", "tokens": ["Ein", "d\u00fcn\u00b7nes", "Gr\u00fcn", "be\u00b7deckt", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gr\u00fcner Duft umw\u00f6lkt die W\u00e4lder,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Duft", "um\u00b7w\u00f6lkt", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man siehet \u00fcberall aus d\u00fcrren Fl\u00e4chen", "tokens": ["Man", "sie\u00b7het", "\u00fc\u00b7be\u00b7rall", "aus", "d\u00fcr\u00b7ren", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich junges Gras und Kr\u00e4uter stechen,", "tokens": ["Sich", "jun\u00b7ges", "Gras", "und", "Kr\u00e4u\u00b7ter", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch in der Luft, je mehr und mehr,", "tokens": ["Auch", "in", "der", "Luft", ",", "je", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein zart durchsichtigs Bl\u00e4tterheer", "tokens": ["Ein", "zart", "durch\u00b7sich\u00b7tigs", "Bl\u00e4t\u00b7ter\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Aus aufgesprengten Knospen brechen,", "tokens": ["Aus", "auf\u00b7ge\u00b7spreng\u00b7ten", "Knos\u00b7pen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die denn, so bald sie hier und dar", "tokens": ["Die", "denn", ",", "so", "bald", "sie", "hier", "und", "dar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "ADV", "ADV", "PPER", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gebohren, wiederum geb\u00e4hren,", "tokens": ["Ge\u00b7boh\u00b7ren", ",", "wie\u00b7de\u00b7rum", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und eine junge Schattenschaar,", "tokens": ["Und", "ei\u00b7ne", "jun\u00b7ge", "Schat\u00b7ten\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch deren Dunkelheit ihr helles Gr\u00fcn", "tokens": ["Durch", "de\u00b7ren", "Dun\u00b7kel\u00b7heit", "ihr", "hel\u00b7les", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sich noch zu mehren schien,", "tokens": ["Sich", "noch", "zu", "meh\u00b7ren", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Uns erst zur Augenlust, nachher zum Schutz gewehren.", "tokens": ["Uns", "erst", "zur", "Au\u00b7gen\u00b7lust", ",", "nach\u00b7her", "zum", "Schutz", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auf Wiesen gl\u00e4nzt, bald hier bald dort,", "tokens": ["Auf", "Wie\u00b7sen", "gl\u00e4nzt", ",", "bald", "hier", "bald", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ein von der Sonnen Licht durchstraltes Spierchen Gras,", "tokens": ["Ein", "von", "der", "Son\u00b7nen", "Licht", "durch\u00b7stral\u00b7tes", "Spier\u00b7chen", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nat\u00fcrlich, als ein gr\u00fcnes Glas;", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", ",", "als", "ein", "gr\u00fc\u00b7nes", "Glas", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da andre, welche niedrig stehn", "tokens": ["Da", "and\u00b7re", ",", "wel\u00b7che", "nied\u00b7rig", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und noch beschattet sind, durch ihre Dunkelheit", "tokens": ["Und", "noch", "be\u00b7schat\u00b7tet", "sind", ",", "durch", "ih\u00b7re", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der erstern helles Gr\u00fcn, als eine Fulg\u2019, erh\u00f6hn,", "tokens": ["Der", "ers\u00b7tern", "hel\u00b7les", "Gr\u00fcn", ",", "als", "ei\u00b7ne", "Ful\u00b7g'", ",", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Zu noch vermehrter Lieblichkeit.", "tokens": ["Zu", "noch", "ver\u00b7mehr\u00b7ter", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Den jungen Bl\u00e4tterchen, die auf den Zweigen", "tokens": ["Den", "jun\u00b7gen", "Bl\u00e4t\u00b7ter\u00b7chen", ",", "die", "auf", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "So durch- als angestralt sich zeigen,", "tokens": ["So", "durch", "als", "an\u00b7ge\u00b7stralt", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KOKOM", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Bem\u00fchet sich, dem so schon sch\u00f6nen Gr\u00fcnen", "tokens": ["Be\u00b7m\u00fc\u00b7het", "sich", ",", "dem", "so", "schon", "sch\u00f6\u00b7nen", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.28": {"text": "Das dunkle Blau der reinen Luft, als eine Fulge, noch zu", "tokens": ["Das", "dunk\u00b7le", "Blau", "der", "rei\u00b7nen", "Luft", ",", "als", "ei\u00b7ne", "Ful\u00b7ge", ",", "noch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.29": {"text": "dienen,", "tokens": ["die\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Um ihre Lieblichkeit noch besser zu erh\u00f6hn.", "tokens": ["Um", "ih\u00b7re", "Lieb\u00b7lich\u00b7keit", "noch", "bes\u00b7ser", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}