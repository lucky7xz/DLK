{"textgrid.poem.49230": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie lockt und jagt, sie k\u00f6mt und fleucht, sie ha\u00dft und liebet,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie lockt und jagt, sie k\u00f6mt und fleucht, sie ha\u00dft und liebet,", "tokens": ["Sie", "lockt", "und", "jagt", ",", "sie", "k\u00f6mt", "und", "fleucht", ",", "sie", "ha\u00dft", "und", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist b\u00f6s' und gleichwol gut, sagt zu und helt's doch nicht;", "tokens": ["Ist", "b\u00f6s'", "und", "gleich\u00b7wol", "gut", ",", "sagt", "zu", "und", "helt's", "doch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "ADJD", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin wie Tantalus, dieweil man mir nichts gibet,", "tokens": ["Ich", "bin", "wie", "Tan\u00b7ta\u00b7lus", ",", "die\u00b7weil", "man", "mir", "nichts", "gi\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NE", "$,", "KOUS", "PIS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu leschen meinen Durst, da doch kein Trank gebricht.", "tokens": ["Zu", "le\u00b7schen", "mei\u00b7nen", "Durst", ",", "da", "doch", "kein", "Trank", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,", "KOUS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}