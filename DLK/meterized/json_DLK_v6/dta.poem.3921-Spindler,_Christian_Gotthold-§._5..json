{"dta.poem.3921": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 5.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in der Liebe sich zu viel erk\u00fchnen solte,", "tokens": ["Wer", "in", "der", "Lie\u00b7be", "sich", "zu", "viel", "er\u00b7k\u00fch\u00b7nen", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "PTKA", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mehr als erbar ist, die Hand gebrauchen wolte,", "tokens": ["Und", "mehr", "als", "er\u00b7bar", "ist", ",", "die", "Hand", "ge\u00b7brau\u00b7chen", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJD", "VAFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem wird, nachdem er sich zu l\u00fcstern hat gewagt,", "tokens": ["Dem", "wird", ",", "nach\u00b7dem", "er", "sich", "zu", "l\u00fcs\u00b7tern", "hat", "ge\u00b7wagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vier auch acht Wochen lang, die ", "tokens": ["Vier", "auch", "acht", "Wo\u00b7chen", "lang", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "ADV", "CARD", "NN", "ADJD", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}