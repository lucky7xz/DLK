{"textgrid.poem.55494": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "74", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wandelt hin, lebendige Gestalten,", "tokens": ["So", "wan\u00b7delt", "hin", ",", "le\u00b7ben\u00b7di\u00b7ge", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewegten Lebens reichliche Gebilde,", "tokens": ["Be\u00b7weg\u00b7ten", "Le\u00b7bens", "reich\u00b7li\u00b7che", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem sch\u00f6nsten Tage lasset Liebe walten,", "tokens": ["Dem", "sch\u00f6ns\u00b7ten", "Ta\u00b7ge", "las\u00b7set", "Lie\u00b7be", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Reihen schm\u00fcckt elysische Gefilde.", "tokens": ["Im", "Rei\u00b7hen", "schm\u00fcckt", "e\u00b7ly\u00b7si\u00b7sche", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ergetzen sollt ihr, geistreich unterhalten,", "tokens": ["Er\u00b7get\u00b7zen", "sollt", "ihr", ",", "geist\u00b7reich", "un\u00b7ter\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Belehren auch und warnen freundlichst milde.", "tokens": ["Be\u00b7leh\u00b7ren", "auch", "und", "war\u00b7nen", "freund\u00b7lichst", "mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Dichter alle segnet euch zum Frieden,", "tokens": ["Der", "Dich\u00b7ter", "al\u00b7le", "seg\u00b7net", "euch", "zum", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Abwesend sei es oder abgeschieden.", "tokens": ["Ab\u00b7we\u00b7send", "sei", "es", "o\u00b7der", "ab\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}