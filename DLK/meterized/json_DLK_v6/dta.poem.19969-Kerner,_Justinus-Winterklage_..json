{"dta.poem.19969": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Winterklage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann in lichten Sommertagen", "tokens": ["Wann", "in", "lich\u00b7ten", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leiden dieses Herz getragen,", "tokens": ["Lei\u00b7den", "die\u00b7ses", "Herz", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlug es bald am Wiesenbach,", "tokens": ["Schlug", "es", "bald", "am", "Wie\u00b7sen\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald in Waldes D\u00e4mmerungen,", "tokens": ["Bald", "in", "Wal\u00b7des", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Nachtigall gesungen,", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gall", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mildern Melodien nach.", "tokens": ["Mil\u00b7dern", "Me\u00b7lo\u00b7dien", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Jezt in tr\u00fcben Wintertagen,", "tokens": ["Jezt", "in", "tr\u00fc\u00b7ben", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! wer stillet seine Klagen?", "tokens": ["Ach", "!", "wer", "stil\u00b7let", "sei\u00b7ne", "Kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachtigall und Wiesenbach?", "tokens": ["Nach\u00b7ti\u00b7gall", "und", "Wie\u00b7sen\u00b7bach", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiesenbach liegt eng gebunden,", "tokens": ["Wie\u00b7sen\u00b7bach", "liegt", "eng", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nachtigall hat Tod gefunden,", "tokens": ["Nach\u00b7ti\u00b7gall", "hat", "Tod", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Singt nicht mehr die Blumen wach.", "tokens": ["Singt", "nicht", "mehr", "die", "Blu\u00b7men", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen auch sind rings verdorben,", "tokens": ["Blu\u00b7men", "auch", "sind", "rings", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter Erde ist gestorben,", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "ist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Kind verwaist, allein.", "tokens": ["Und", "ihr", "Kind", "ver\u00b7waist", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam blickt's in blaue Ferne,", "tokens": ["Ein\u00b7sam", "blickt's", "in", "blau\u00b7e", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm! so rufen alle Sterne,", "tokens": ["Komm", "!", "so", "ru\u00b7fen", "al\u00b7le", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier ist ew'ger Maienschein!", "tokens": ["Hier", "ist", "ew'\u00b7ger", "Mai\u00b7en\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Herz! so h\u00f6r' denn auf zu schlagen!", "tokens": ["Herz", "!", "so", "h\u00f6r'", "denn", "auf", "zu", "schla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh! in diesen tr\u00fcben Tagen", "tokens": ["Sieh", "!", "in", "die\u00b7sen", "tr\u00fc\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt kein Vogel, wallt kein Bach.", "tokens": ["Singt", "kein", "Vo\u00b7gel", ",", "wallt", "kein", "Bach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst dich nicht gefangen geben,", "tokens": ["Willst", "dich", "nicht", "ge\u00b7fan\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Treibst mit schmerzlich bangem Beben", "tokens": ["Treibst", "mit", "schmerz\u00b7lich", "ban\u00b7gem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Well' der andern nach!", "tokens": ["Ei\u00b7ne", "Well'", "der", "an\u00b7dern", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}