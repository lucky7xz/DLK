{"textgrid.poem.54021": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Parlament", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob die Sozialisten in den Reichstag ziehn \u2013", "tokens": ["Ob", "die", "So\u00b7zi\u00b7a\u00b7lis\u00b7ten", "in", "den", "Reichs\u00b7tag", "ziehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der Vater Wirth will nach links entfliehn,", "tokens": ["Ob", "der", "Va\u00b7ter", "Wirth", "will", "nach", "links", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VMFIN", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "oder ob er kuscht wegen Disziplin \u2013", "tokens": ["o\u00b7der", "ob", "er", "kuscht", "we\u00b7gen", "Dis\u00b7zip\u00b7lin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ob die Volkspartei mit den Schiele-Augen", "tokens": ["Ob", "die", "Volks\u00b7par\u00b7tei", "mit", "den", "Schie\u00b7le\u00b7Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.7": {"text": "einen hinmacht mitten ins Lokal", "tokens": ["ei\u00b7nen", "hin\u00b7macht", "mit\u00b7ten", "ins", "Lo\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "und den Demokraten auf die H\u00fchneraugen . . .", "tokens": ["und", "den", "De\u00b7mo\u00b7kra\u00b7ten", "auf", "die", "H\u00fch\u00b7ner\u00b7au\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Plakate kleben an den Mauern \u2013", "tokens": ["Die", "Pla\u00b7ka\u00b7te", "kle\u00b7ben", "an", "den", "Mau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "mit dem Schmus f\u00fcr St\u00e4dter und f\u00fcr Bauern:", "tokens": ["mit", "dem", "Schmus", "f\u00fcr", "St\u00e4d\u00b7ter", "und", "f\u00fcr", "Bau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbzw\u00f6lfte Stunde!\u00ab \u2013 \u00bbSoll die Schande dauern?\u00ab", "tokens": ["\u00bb", "zw\u00f6lf\u00b7te", "Stun\u00b7de", "!", "\u00ab", "\u2013", "\u00bb", "Soll", "die", "Schan\u00b7de", "dau\u00b7ern", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "$(", "$(", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Is ja janz ejal!", "tokens": ["Is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kennt ihr jene, die dahinter sitzen", "tokens": ["Kennt", "ihr", "je\u00b7ne", ",", "die", "da\u00b7hin\u00b7ter", "sit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "PAV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und die Schn\u00fcre ziehn bei jeder Wahl?", "tokens": ["und", "die", "Schn\u00fc\u00b7re", "ziehn", "bei", "je\u00b7der", "Wahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ob im Bockbiersaal die Propagandafritzen", "tokens": ["Ob", "im", "Bock\u00b7bier\u00b7saal", "die", "Pro\u00b7pa\u00b7gan\u00b7da\u00b7frit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "sich halb heiser br\u00fcllen und dabei B\u00e4che schwitzen \u2013:", "tokens": ["sich", "halb", "hei\u00b7ser", "br\u00fcl\u00b7len", "und", "da\u00b7bei", "B\u00e4\u00b7che", "schwit\u00b7zen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVINF", "KON", "PAV", "NN", "VVINF", "$(", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ob die Funktion\u00e4re ganz und gar verrosten \u2013", "tokens": ["Ob", "die", "Funk\u00b7ti\u00b7o\u00b7n\u00e4\u00b7re", "ganz", "und", "gar", "ver\u00b7ros\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "KON", "ADV", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der sch\u00f6ne Rudi den Ministerposten", "tokens": ["Ob", "der", "sch\u00f6\u00b7ne", "Ru\u00b7di", "den", "Mi\u00b7nis\u00b7ter\u00b7pos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NE", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "endlich kriegt \u2013 (das wird nicht billig kosten):", "tokens": ["end\u00b7lich", "kriegt", "\u2013", "(", "das", "wird", "nicht", "bil\u00b7lig", "kos\u00b7ten", ")", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "$(", "PDS", "VAFIN", "PTKNEG", "ADJD", "VVINF", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dein Geschick, Deutschland, machen Industrien,", "tokens": ["Dein", "Ge\u00b7schick", ",", "Deutschland", ",", "ma\u00b7chen", "In\u00b7dust\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Banken und die Schiffahrtskompanien \u2013", "tokens": ["Ban\u00b7ken", "und", "die", "Schif\u00b7fahrts\u00b7kom\u00b7pa\u00b7ni\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "welch ein Bumstheater ist die Wahl!", "tokens": ["welch", "ein", "Bumst\u00b7he\u00b7a\u00b7ter", "ist", "die", "Wahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Reg dich auf und reg dich ab im Grimme!", "tokens": ["Reg", "dich", "auf", "und", "reg", "dich", "ab", "im", "Grim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "W\u00e4hle, w\u00e4hle! Doch des Volkes Stimme", "tokens": ["W\u00e4h\u00b7le", ",", "w\u00e4h\u00b7le", "!", "Doch", "des", "Vol\u00b7kes", "Stim\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "$.", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "is ja janz ejal!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "is ja janz ejal \u2013!", "tokens": ["is", "ja", "janz", "e\u00b7jal", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "ADV", "ADV", "ADJD", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}