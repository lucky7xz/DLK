{"textgrid.poem.40792": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meiner Hoffnung fromme Blume, die ich heimlich n\u00e4hrt' und tr\u00e4nkt',", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meiner Hoffnung fromme Blume, die ich heimlich n\u00e4hrt' und tr\u00e4nkt',", "tokens": ["Mei\u00b7ner", "Hoff\u00b7nung", "from\u00b7me", "Blu\u00b7me", ",", "die", "ich", "heim\u00b7lich", "n\u00e4hrt'", "und", "tr\u00e4nkt'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hielt in stiller Todesahnung schon ihr rosig Haupt gesenkt;", "tokens": ["Hielt", "in", "stil\u00b7ler", "To\u00b7de\u00b7sah\u00b7nung", "schon", "ihr", "ro\u00b7sig", "Haupt", "ge\u00b7senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Lenz und Licht umsonst erharrend, siechte sie schon lebensmatt,", "tokens": ["Lenz", "und", "Licht", "um\u00b7sonst", "er\u00b7har\u00b7rend", ",", "siech\u00b7te", "sie", "schon", "le\u00b7bens\u00b7matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ach und seine gr\u00fcne Flagge strich besiegt ihr welkes Blatt.", "tokens": ["Ach", "und", "sei\u00b7ne", "gr\u00fc\u00b7ne", "Flag\u00b7ge", "strich", "be\u00b7siegt", "ihr", "wel\u00b7kes", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "PWAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Die\u00df geschah zur Zeit, als oben sprach der Herr vom Wolkenthron:", "tokens": ["Die\u00df", "ge\u00b7schah", "zur", "Zeit", ",", "als", "o\u00b7ben", "sprach", "der", "Herr", "vom", "Wol\u00b7ken\u00b7thron", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbhast du meines Zornes Boten, Erde, so vergessen schon,", "tokens": ["\u00bb", "hast", "du", "mei\u00b7nes", "Zor\u00b7nes", "Bo\u00b7ten", ",", "Er\u00b7de", ",", "so", "ver\u00b7ges\u00b7sen", "schon", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Den verk\u00fcndet Bluttrabanten, dem gefolgt Brand und Entsetzen,", "tokens": ["Den", "ver\u00b7k\u00fcn\u00b7det", "Blut\u00b7tra\u00b7ban\u00b7ten", ",", "dem", "ge\u00b7folgt", "Brand", "und", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,", "PRELS", "VVPP", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df, nachzitternd noch, du wieder opferst schon den alten G\u00f6tzen?", "tokens": ["Da\u00df", ",", "nach\u00b7zit\u00b7ternd", "noch", ",", "du", "wie\u00b7der", "op\u00b7ferst", "schon", "den", "al\u00b7ten", "G\u00f6t\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVPP", "ADV", "$,", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Steige, zweiter Engel, nieder ohne Schwert und Blut und Brand!", "tokens": ["Stei\u00b7ge", ",", "zwei\u00b7ter", "En\u00b7gel", ",", "nie\u00b7der", "oh\u00b7ne", "Schwert", "und", "Blut", "und", "Brand", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PTKVZ", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schwing' als richtend Schwert ein F\u00fcllhorn duft'ger Fr\u00fccht' in deiner Hand,", "tokens": ["Schwing'", "als", "rich\u00b7tend", "Schwert", "ein", "F\u00fcll\u00b7horn", "duft'\u00b7ger", "Fr\u00fccht'", "in", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADJD", "NN", "ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nimm zu Fl\u00fcgeln wei\u00dfe Bl\u00fcthen, Fr\u00fchlings Sonnengold zu Locken;", "tokens": ["Nimm", "zu", "Fl\u00fc\u00b7geln", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7then", ",", "Fr\u00fch\u00b7lings", "Son\u00b7nen\u00b7gold", "zu", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJA", "NN", "$,", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "So, moderne Pest, nun walle s\u00e4uselnd hin auf Zefirs Socken!\u00ab", "tokens": ["So", ",", "mo\u00b7der\u00b7ne", "Pest", ",", "nun", "wal\u00b7le", "s\u00e4u\u00b7selnd", "hin", "auf", "Ze\u00b7firs", "So\u00b7cken", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "ADJD", "ADV", "APPR", "NE", "NN", "$.", "$("], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und der Engel flog vom Osten, wo der Tag wohnt und der Zaar,", "tokens": ["Und", "der", "En\u00b7gel", "flog", "vom", "Os\u00b7ten", ",", "wo", "der", "Tag", "wohnt", "und", "der", "Zaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Stumm uns n\u00e4her, immer n\u00e4her, ird'schen Augen unsichtbar,", "tokens": ["Stumm", "uns", "n\u00e4\u00b7her", ",", "im\u00b7mer", "n\u00e4\u00b7her", ",", "ird'\u00b7schen", "Au\u00b7gen", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Seine luft'gen Bahnen zeigte doch auf Erden, Meil' auf Meile,", "tokens": ["Sei\u00b7ne", "luft'\u00b7gen", "Bah\u00b7nen", "zeig\u00b7te", "doch", "auf", "Er\u00b7den", ",", "Meil'", "auf", "Mei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der gefallnen Leichen stumme, unabsehbar lange Zeile.", "tokens": ["Der", "ge\u00b7fall\u00b7nen", "Lei\u00b7chen", "stum\u00b7me", ",", "un\u00b7ab\u00b7seh\u00b7bar", "lan\u00b7ge", "Zei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Sommer war's, zum Herbst sich neigend, sch\u00f6ne, klare, sonn'ge Tage;", "tokens": ["Som\u00b7mer", "wa\u00b7r's", ",", "zum", "Herbst", "sich", "nei\u00b7gend", ",", "sch\u00f6\u00b7ne", ",", "kla\u00b7re", ",", "sonn'\u00b7ge", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPRART", "NN", "PRF", "ADJD", "$,", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Sieh, das Volk, hinaus luftwandelt's nach dem Felde, nach dem Hage;", "tokens": ["Sieh", ",", "das", "Volk", ",", "hin\u00b7aus", "luft\u00b7wan\u00b7delt's", "nach", "dem", "Fel\u00b7de", ",", "nach", "dem", "Ha\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "APZR", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-++-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Weh, es zielt mit Sonnenstrahlen jetzt auf euer Herz der Tod!", "tokens": ["Weh", ",", "es", "zielt", "mit", "Son\u00b7nen\u00b7strah\u00b7len", "jetzt", "auf", "eu\u00b7er", "Herz", "der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Weh, es k\u00fchlt in Baumesschatten euch des Lebens Schwei\u00df der Tod!", "tokens": ["Weh", ",", "es", "k\u00fchlt", "in", "Bau\u00b7mes\u00b7schat\u00b7ten", "euch", "des", "Le\u00b7bens", "Schwei\u00df", "der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Diesen d\u00fcrstet, \u2013 o wie lieblich dort die frische Quelle singt!", "tokens": ["Die\u00b7sen", "d\u00fcrs\u00b7tet", ",", "\u2013", "o", "wie", "lieb\u00b7lich", "dort", "die", "fri\u00b7sche", "Quel\u00b7le", "singt", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "$(", "FM", "KOKOM", "ADJD", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seht an ihrem Born ihn liegen: Tod ist's, was sie rauscht und klingt!", "tokens": ["Seht", "an", "ih\u00b7rem", "Born", "ihn", "lie\u00b7gen", ":", "Tod", "ist's", ",", "was", "sie", "rauscht", "und", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jener Knabe lechzt nach Labung, \u2013 Trauben winken wangenroth;", "tokens": ["Je\u00b7ner", "Kna\u00b7be", "lechzt", "nach", "La\u00b7bung", ",", "\u2013", "Trau\u00b7ben", "win\u00b7ken", "wan\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "NN", "$,", "$(", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Heuer gibt's ein reiches Lesen, doch der Weinstock tr\u00e4gt nur Tod!", "tokens": ["Heu\u00b7er", "gibt's", "ein", "rei\u00b7ches", "Le\u00b7sen", ",", "doch", "der", "Wein\u00b7stock", "tr\u00e4gt", "nur", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Schw\u00e4rmerische Seele, wandle nur im s\u00fc\u00dfen Mondenschein:", "tokens": ["Schw\u00e4r\u00b7me\u00b7ri\u00b7sche", "See\u00b7le", ",", "wand\u00b7le", "nur", "im", "s\u00fc\u00b7\u00dfen", "Mon\u00b7den\u00b7schein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aus des Lebens Jammerthalen wird dir bald Erl\u00f6sung sein! \u2013", "tokens": ["Aus", "des", "Le\u00b7bens", "Jam\u00b7mer\u00b7tha\u00b7len", "wird", "dir", "bald", "Er\u00b7l\u00f6\u00b7sung", "sein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "PPER", "ADV", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Greiser Vater, euren Segen, eh' vergl\u00fcht das Abendroth!", "tokens": ["Grei\u00b7ser", "Va\u00b7ter", ",", "eu\u00b7ren", "Se\u00b7gen", ",", "eh'", "ver\u00b7gl\u00fcht", "das", "A\u00b7ben\u00b7droth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Weh dir, guter Sohn, als Segen quillt aus Vaters Hand dir Tod!", "tokens": ["Weh", "dir", ",", "gu\u00b7ter", "Sohn", ",", "als", "Se\u00b7gen", "quillt", "aus", "Va\u00b7ters", "Hand", "dir", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "NN", "VVFIN", "APPR", "NN", "NN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Weiche Kissen, Tafftgardinen! S\u00fc\u00dfen Ku\u00df auf rothen Mund! \u2013", "tokens": ["Wei\u00b7che", "Kis\u00b7sen", ",", "Tafft\u00b7gar\u00b7di\u00b7nen", "!", "S\u00fc\u00b7\u00dfen", "Ku\u00df", "auf", "ro\u00b7then", "Mund", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$.", "NN", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Weh', der Ku\u00df des Liebchens siegelt Tod auf deiner Lippen Rund! \u2013", "tokens": ["Weh'", ",", "der", "Ku\u00df", "des", "Lieb\u00b7chens", "sie\u00b7gelt", "Tod", "auf", "dei\u00b7ner", "Lip\u00b7pen", "Rund", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Reu'ger S\u00fcnder, nimm die Hostie am Altar im Kerzenstrahl!", "tokens": ["Reu'\u00b7ger", "S\u00fcn\u00b7der", ",", "nimm", "die", "Hos\u00b7tie", "am", "Al\u00b7tar", "im", "Ker\u00b7zen\u00b7strahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das Vers\u00f6hnungsmal der Reue ist dein letztes Abendmahl! \u2013", "tokens": ["Das", "Ver\u00b7s\u00f6h\u00b7nungs\u00b7mal", "der", "Reu\u00b7e", "ist", "dein", "letz\u00b7tes", "A\u00b7bend\u00b7mahl", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Zeit der Reu' ist's und Bekehrung; wie das Volk der Priester rennt!", "tokens": ["Zeit", "der", "Reu'", "ist's", "und", "Be\u00b7keh\u00b7rung", ";", "wie", "das", "Volk", "der", "Pries\u00b7ter", "rennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "NE", "KON", "NN", "$.", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Todesfeindschaft sucht Vers\u00f6hnung, Ehebruch und Mord bekennt,", "tokens": ["To\u00b7des\u00b7feind\u00b7schaft", "sucht", "Ver\u00b7s\u00f6h\u00b7nung", ",", "E\u00b7heb\u00b7ruch", "und", "Mord", "be\u00b7kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "NN", "KON", "NN