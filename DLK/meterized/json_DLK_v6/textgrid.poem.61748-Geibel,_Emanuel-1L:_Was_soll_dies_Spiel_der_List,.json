{"textgrid.poem.61748": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was soll dies Spiel der List,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll dies Spiel der List,", "tokens": ["Was", "soll", "dies", "Spiel", "der", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dies Klirren mit dem Schwerte,", "tokens": ["Dies", "Klir\u00b7ren", "mit", "dem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ob nach Raub und Zwist", "tokens": ["Als", "ob", "nach", "Raub", "und", "Zwist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das deutsche Volk begehrte?", "tokens": ["Das", "deut\u00b7sche", "Volk", "be\u00b7gehr\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein treuer Wunsch allein", "tokens": ["Ein", "treu\u00b7er", "Wunsch", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Steht uns ins Herz gegraben:", "tokens": ["Steht", "uns", "ins", "Herz", "ge\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen einig sein", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wollen Frieden haben.", "tokens": ["Und", "wol\u00b7len", "Frie\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mag jeder, wie's ihm klug", "tokens": ["Mag", "je\u00b7der", ",", "wie's", "ihm", "klug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PIS", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bed\u00fcnkt, sein Haus verwalten!", "tokens": ["Be\u00b7d\u00fcnkt", ",", "sein", "Haus", "ver\u00b7wal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind uns selbst genug", "tokens": ["Wir", "sind", "uns", "selbst", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und lassen gern ihn schalten.", "tokens": ["Und", "las\u00b7sen", "gern", "ihn", "schal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns ist's nicht Gall' im Wein,", "tokens": ["Uns", "ist's", "nicht", "Gall'", "im", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn andre froh sich laben;", "tokens": ["Wenn", "and\u00b7re", "froh", "sich", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen einig sein", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wollen Frieden haben.", "tokens": ["Und", "wol\u00b7len", "Frie\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur, wie wir ohne Groll", "tokens": ["Nur", ",", "wie", "wir", "oh\u00b7ne", "Groll"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Recht des Nachbars ehren,", "tokens": ["Das", "Recht", "des", "Nach\u00b7bars", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So fordern wir, man soll", "tokens": ["So", "for\u00b7dern", "wir", ",", "man", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIS", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auch unsres uns gew\u00e4hren.", "tokens": ["Auch", "uns\u00b7res", "uns", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein Vormund red' uns drein", "tokens": ["Kein", "Vor\u00b7mund", "red'", "uns", "drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie willenlosen Knaben;", "tokens": ["Wie", "wil\u00b7len\u00b7lo\u00b7sen", "Kna\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen einig sein", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wollen Frieden haben.", "tokens": ["Und", "wol\u00b7len", "Frie\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir wollen endlich fest", "tokens": ["Wir", "wol\u00b7len", "end\u00b7lich", "fest"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ausbaun die deutschen Hallen,", "tokens": ["Aus\u00b7baun", "die", "deut\u00b7schen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht, wie sie Ost und West,", "tokens": ["Nicht", ",", "wie", "sie", "Ost", "und", "West", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nein, wie sie ", "tokens": ["Nein", ",", "wie", "sie"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Reicht uns die Hand am Main,", "tokens": ["Reicht", "uns", "die", "Hand", "am", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Bayern und ihr Schwaben!", "tokens": ["Ihr", "Bay\u00b7ern", "und", "ihr", "Schwa\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen einig sein", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wollen Frieden haben.", "tokens": ["Und", "wol\u00b7len", "Frie\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir hassen's insgesamt,", "tokens": ["Wir", "has\u00b7sen's", "ins\u00b7ge\u00b7samt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um eitlen Ruhm zu fechten,", "tokens": ["Um", "eit\u00b7len", "Ruhm", "zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hoch zur Notwehr flammt", "tokens": ["Doch", "hoch", "zur", "Not\u00b7wehr", "flammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Schwert in unsrer Rechten.", "tokens": ["Das", "Schwert", "in", "uns\u00b7rer", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem St\u00f6renfried allein", "tokens": ["Dem", "St\u00f6\u00b7ren\u00b7fried", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sei's in die Brust gegraben!", "tokens": ["Sei's", "in", "die", "Brust", "ge\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen einig sein", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wollen Frieden haben.", "tokens": ["Und", "wol\u00b7len", "Frie\u00b7den", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}