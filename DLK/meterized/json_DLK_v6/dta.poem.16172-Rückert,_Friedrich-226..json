{"dta.poem.16172": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "226.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die helle Gotteswelt, wie steht sie voll Gebilde", "tokens": ["Die", "hel\u00b7le", "Got\u00b7tes\u00b7welt", ",", "wie", "steht", "sie", "voll", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00f6nleuchtender, wie hell voll Blumen ein Gefilde.", "tokens": ["Sch\u00f6n\u00b7leuch\u00b7ten\u00b7der", ",", "wie", "hell", "voll", "Blu\u00b7men", "ein", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und was du selber thust, und was du selber bist,", "tokens": ["Und", "was", "du", "sel\u00b7ber", "thust", ",", "und", "was", "du", "sel\u00b7ber", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "KON", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O f\u00fchle wie's voll Lust Blum' unter Blumen ist.", "tokens": ["O", "f\u00fch\u00b7le", "wie's", "voll", "Lust", "Blum'", "un\u00b7ter", "Blu\u00b7men", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "NN", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So bl\u00fche dich nur aus, so dufte nur und lebe;", "tokens": ["So", "bl\u00fc\u00b7he", "dich", "nur", "aus", ",", "so", "duf\u00b7te", "nur", "und", "le\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und pfl\u00fcckt man dich zum Straus, vor Blumentod nicht bebe!", "tokens": ["Und", "pfl\u00fcckt", "man", "dich", "zum", "Straus", ",", "vor", "Blu\u00b7men\u00b7tod", "nicht", "be\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$,", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}