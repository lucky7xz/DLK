{"textgrid.poem.43127": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Uns're Schiffe willst Du lenken", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns're Schiffe willst Du lenken", "tokens": ["Un\u00b7s'\u00b7re", "Schif\u00b7fe", "willst", "Du", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nun nach einem gleichen Ziel?!", "tokens": ["Nun", "nach", "ei\u00b7nem", "glei\u00b7chen", "Ziel", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern Dir, losgerissen treib' ich,", "tokens": ["Fern", "Dir", ",", "los\u00b7ge\u00b7ris\u00b7sen", "treib'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVIZU", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4ngst der wilden St\u00fcrme Spiel.", "tokens": ["L\u00e4ngst", "der", "wil\u00b7den", "St\u00fcr\u00b7me", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcrchte Du das b\u00f6se Zischen,", "tokens": ["F\u00fcrch\u00b7te", "Du", "das", "b\u00f6\u00b7se", "Zi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kalte Grollen, f\u00fcrcht' das Meer,", "tokens": ["Kal\u00b7te", "Grol\u00b7len", ",", "f\u00fcrcht'", "das", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lass' mich ringen mit den Wogen,", "tokens": ["Lass'", "mich", "rin\u00b7gen", "mit", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einsam, haltlos, ohne Wehr!", "tokens": ["Ein\u00b7sam", ",", "halt\u00b7los", ",", "oh\u00b7ne", "Wehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bleibe still und unbek\u00fcmmert", "tokens": ["Blei\u00b7be", "still", "und", "un\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ferne mir und nah' dem Strand,", "tokens": ["Fer\u00b7ne", "mir", "und", "nah'", "dem", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bald entsinket ja das Ruder", "tokens": ["Bald", "ent\u00b7sin\u00b7ket", "ja", "das", "Ru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Meiner kraftlos m\u00fcden Hand \u2013", "tokens": ["Mei\u00b7ner", "kraft\u00b7los", "m\u00fc\u00b7den", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Oder \u2013 st\u00fcrze muthig nach mir,", "tokens": ["O\u00b7der", "\u2013", "st\u00fcr\u00b7ze", "mut\u00b7hig", "nach", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Wenn mein Fahrzeug untergeht \u2013", "tokens": ["Wenn", "mein", "Fahr\u00b7zeug", "un\u00b7ter\u00b7geht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch zum ", "tokens": ["Doch", "zum"], "token_info": ["word", "word"], "pos": ["KON", "APPRART"], "meter": "+-", "measure": "trochaic.single"}}}}}