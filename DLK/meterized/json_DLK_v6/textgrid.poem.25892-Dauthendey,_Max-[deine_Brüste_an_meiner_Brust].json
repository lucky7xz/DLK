{"textgrid.poem.25892": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[deine Br\u00fcste an meiner Brust]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Br\u00fcste an meiner Brust.", "tokens": ["Dei\u00b7ne", "Br\u00fcs\u00b7te", "an", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Seelen \u00f6ffnen ihr Grab.", "tokens": ["Die", "See\u00b7len", "\u00f6ff\u00b7nen", "ihr", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich sah durch die geschlossenen Augen,", "tokens": ["Ich", "sah", "durch", "die", "ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sonne sank in dir hinab.", "tokens": ["Die", "Son\u00b7ne", "sank", "in", "dir", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah noch hinter der Sonne die Tiefen,", "tokens": ["Ich", "sah", "noch", "hin\u00b7ter", "der", "Son\u00b7ne", "die", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Urweltraum, wo alle Lebenskeime schliefen.", "tokens": ["Den", "Ur\u00b7welt\u00b7raum", ",", "wo", "al\u00b7le", "Le\u00b7bens\u00b7kei\u00b7me", "schlie\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sehr einfach still war es umher,", "tokens": ["Sehr", "ein\u00b7fach", "still", "war", "es", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wir waren unendlich gro\u00df,", "tokens": ["Und", "wir", "wa\u00b7ren", "un\u00b7end\u00b7lich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wir waren alles und wu\u00dften nichts mehr,", "tokens": ["Wir", "wa\u00b7ren", "al\u00b7les", "und", "wu\u00df\u00b7ten", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KON", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wu\u00dften blo\u00df, da\u00df wir selig waren.", "tokens": ["Wu\u00df\u00b7ten", "blo\u00df", ",", "da\u00df", "wir", "se\u00b7lig", "wa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}