{"textgrid.poem.60289": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Scheideblick", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ein unergr\u00fcndlich Wonnemeer", "tokens": ["Als", "ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "Won\u00b7ne\u00b7meer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Strahlte mir dein tiefer Seelenblick;", "tokens": ["Strahl\u00b7te", "mir", "dein", "tie\u00b7fer", "See\u00b7len\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Scheiden mu\u00dft ich ohne Wiederkehr,", "tokens": ["Schei\u00b7den", "mu\u00dft", "ich", "oh\u00b7ne", "Wie\u00b7der\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich habe scheidend all mein Gl\u00fcck", "tokens": ["Und", "ich", "ha\u00b7be", "schei\u00b7dend", "all", "mein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Still versenkt in dieses tiefe Meer.", "tokens": ["Still", "ver\u00b7senkt", "in", "die\u00b7ses", "tie\u00b7fe", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}