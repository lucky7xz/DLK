{"textgrid.poem.54070": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Bergen liegt der Schatten,", "tokens": ["Auf", "den", "Ber\u00b7gen", "liegt", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der See ist dunkelgr\u00fcn.", "tokens": ["und", "der", "See", "ist", "dun\u00b7kel\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Sechs-Mark-f\u00fcnfzig-Platten", "tokens": ["Von", "den", "Sechs\u00b7Mark\u00b7f\u00fcnf\u00b7zig\u00b7Plat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "singt Maria Ivog\u00fcn . . .", "tokens": ["singt", "Ma\u00b7ria", "I\u00b7vo\u00b7g\u00fcn", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "NE", "NE", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Horch, die sch\u00f6ne Melodie:", "tokens": ["Horch", ",", "die", "sch\u00f6\u00b7ne", "Me\u00b7lo\u00b7die", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbtralah\u00fc \u2013 lah\u00fc \u2013 lahi!\u00ab", "tokens": ["\u00bb", "tra\u00b7la\u00b7h\u00fc", "\u2013", "la\u00b7h\u00fc", "\u2013", "la\u00b7hi", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "XY", "$(", "PTKVZ", "$(", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dumpf t\u00f6nts von der Kegelbahn \u2013 \u2013 \u2013", "tokens": ["Dumpf", "t\u00f6nts", "von", "der", "Ke\u00b7gel\u00b7bahn", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$(", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": " . . . Was hast du am Tag getan \u2013?", "tokens": [".", ".", ".", "Was", "hast", "du", "am", "Tag", "ge\u00b7tan", "\u2013", "?"], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$.", "$.", "$.", "PWS", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hast du einen Brief geschrieben?", "tokens": ["Hast", "du", "ei\u00b7nen", "Brief", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du im B\u00fcro gepennt?", "tokens": ["Hast", "du", "im", "B\u00fc\u00b7ro", "ge\u00b7pennt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast du Unkeuschheit getrieben?", "tokens": ["Hast", "du", "Un\u00b7keuschheit", "ge\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nahmst du ", "tokens": ["Nahmst", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "als Bankier der Industrie . . .", "tokens": ["als", "Ban\u00b7kier", "der", "In\u00b7dust\u00b7rie", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tralah\u00fc \u2013 lah\u00fc \u2013 lahi \u2013", "tokens": ["Tra\u00b7la\u00b7h\u00fc", "\u2013", "la\u00b7h\u00fc", "\u2013", "la\u00b7hi", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "PTKVZ", "$(", "XY", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Singt sie nicht wie Marzipan!", "tokens": ["Singt", "sie", "nicht", "wie", "Mar\u00b7zi\u00b7pan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": " . . . Was hast du am Tag getan?", "tokens": [".", ".", ".", "Was", "hast", "du", "am", "Tag", "ge\u00b7tan", "?"], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "PWS", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Hast des Staates du im stillen", "tokens": ["Hast", "des", "Staa\u00b7tes", "du", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dankbar-demutsvoll gedacht?", "tokens": ["dank\u00b7ba\u00b7rde\u00b7muts\u00b7voll", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast du Margot Abf\u00fchrpillen,", "tokens": ["Hast", "du", "Mar\u00b7got", "Ab\u00b7f\u00fchr\u00b7pil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sie wollte, mitgebracht?", "tokens": ["die", "sie", "woll\u00b7te", ",", "mit\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRELS", "PPER", "VMFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dachtest du, wie Hitler schrie . . .", "tokens": ["Dach\u00b7test", "du", ",", "wie", "Hit\u00b7ler", "schrie", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tralah\u00fc \u2013 lah\u00fc \u2013 lahi \u2013", "tokens": ["Tra\u00b7la\u00b7h\u00fc", "\u2013", "la\u00b7h\u00fc", "\u2013", "la\u00b7hi", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "PTKVZ", "$(", "XY", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "mit dem bierigen Organ \u2013 \u2013 \u2013", "tokens": ["mit", "dem", "bie\u00b7ri\u00b7gen", "Or\u00b7gan", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was hast du am Tag getan?", "tokens": ["Was", "hast", "du", "am", "Tag", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Morgen, denkst du, bin ich schlauer.", "tokens": ["Mor\u00b7gen", ",", "denkst", "du", ",", "bin", "ich", "schlau\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgen fang ichs richtig an.", "tokens": ["Mor\u00b7gen", "fang", "ichs", "rich\u00b7tig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder \u2013 St\u00e4dter oder Bauer \u2013", "tokens": ["Je\u00b7der", "\u2013", "St\u00e4d\u00b7ter", "o\u00b7der", "Bau\u00b7er", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ist zur Nacht ein kluger Mann.", "tokens": ["ist", "zur", "Nacht", "ein", "klu\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber welche Ironie \u2013", "tokens": ["A\u00b7ber", "wel\u00b7che", "I\u00b7ro\u00b7nie", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tralah\u00fc \u2013 lah\u00fc \u2013 lahi \u2013:", "tokens": ["Tra\u00b7la\u00b7h\u00fc", "\u2013", "la\u00b7h\u00fc", "\u2013", "la\u00b7hi", "\u2013", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$(", "PTKVZ", "$(", "XY", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Morgen leben alle Leute", "tokens": ["Mor\u00b7gen", "le\u00b7ben", "al\u00b7le", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "egalweg genau wie heute.", "tokens": ["e\u00b7gal\u00b7weg", "ge\u00b7nau", "wie", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}