{"dta.poem.666": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! diese sind die wenn der frost wird schwinden/ ", "tokens": ["Ach", "!", "die\u00b7se", "sind", "die", "wenn", "der", "frost", "wird", "schwin\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "KOUS", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dieser Wetter sturm vergangen.", "tokens": ["Wenn", "die\u00b7ser", "Wet\u00b7ter", "sturm", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd \u00e4cker/ b\u00e4um\u2019 vnd felder prangen", "tokens": ["Vnd", "\u00e4c\u00b7ker", "/", "b\u00e4um'", "vnd", "fel\u00b7der", "pran\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$(", "VVIMP", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit h\u00f6chster lust/ sich fr\u00f6lich werden finden/", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "lust", "/", "sich", "fr\u00f6\u00b7lich", "wer\u00b7den", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PRF", "ADJD", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vnd ohne tr\u00fcbsal mit einander lachen. ", "tokens": ["Vnd", "oh\u00b7ne", "tr\u00fcb\u00b7sal", "mit", "ein\u00b7an\u00b7der", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch sich an dieser saate frucht/", "tokens": ["Auch", "sich", "an", "die\u00b7ser", "saa\u00b7te", "frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach der betr\u00fcbten tage flucht/", "tokens": ["Nach", "der", "be\u00b7tr\u00fcb\u00b7ten", "ta\u00b7ge", "flucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Mit scharff gewetzten ", "tokens": ["Mit", "scharff", "ge\u00b7wetz\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}}}}}