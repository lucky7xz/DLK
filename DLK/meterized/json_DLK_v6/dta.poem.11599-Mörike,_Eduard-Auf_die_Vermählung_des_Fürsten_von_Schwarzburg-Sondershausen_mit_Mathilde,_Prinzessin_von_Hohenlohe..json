{"dta.poem.11599": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Verm\u00e4hlung des  \n F\u00fcrsten von Schwarzburg-Sondershausen  \n mit  \n Mathilde, Prinzessin von Hohenlohe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Hebt euch, sanftbeschwingte Lieder,", "tokens": ["Hebt", "euch", ",", "sanft\u00b7be\u00b7schwing\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und empfangt ein edel Paar!", "tokens": ["Und", "emp\u00b7fangt", "ein", "e\u00b7del", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ew'ge Liebe blick' hernieder!", "tokens": ["Ew'\u00b7ge", "Lie\u00b7be", "blick'", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dir schm\u00fcckt sich der Altar;", "tokens": ["Denn", "dir", "schm\u00fcckt", "sich", "der", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo der F\u00fcrst, der Hochbegl\u00fcckte,", "tokens": ["Wo", "der", "F\u00fcrst", ",", "der", "Hoch\u00b7be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Staunend deiner Wege denkt,", "tokens": ["Stau\u00b7nend", "dei\u00b7ner", "We\u00b7ge", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief in Wehmuth die entz\u00fcckte", "tokens": ["Tief", "in", "Weh\u00b7muth", "die", "ent\u00b7z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "ART", "ADJA"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Braut die reine Stirne senkt.", "tokens": ["Braut", "die", "rei\u00b7ne", "Stir\u00b7ne", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leis' auf goldner Wage w\u00e4get,", "tokens": ["Leis'", "auf", "gold\u00b7ner", "Wa\u00b7ge", "w\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel, g\u00fctig Ihr Geschick,", "tokens": ["En\u00b7gel", ",", "g\u00fc\u00b7tig", "Ihr", "Ge\u00b7schick", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu ew'gen Kr\u00e4nzen leget", "tokens": ["Und", "zu", "ew'\u00b7gen", "Kr\u00e4n\u00b7zen", "le\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes holde Erdengl\u00fcck!", "tokens": ["Je\u00b7des", "hol\u00b7de", "Er\u00b7den\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}