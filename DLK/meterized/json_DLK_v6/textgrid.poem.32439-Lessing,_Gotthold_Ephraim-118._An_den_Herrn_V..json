{"textgrid.poem.32439": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "118. An den Herrn V.", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ladest zwanzig Schmauser ein,", "tokens": ["Du", "la\u00b7dest", "zwan\u00b7zig", "Schmau\u00b7ser", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon ich keinen kenn'; und dann mich oben drein.", "tokens": ["Wo\u00b7von", "ich", "kei\u00b7nen", "kenn'", ";", "und", "dann", "mich", "o\u00b7ben", "drein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "VVFIN", "$.", "KON", "ADV", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch z\u00fcrnst du, und erstaunst, warum ich nicht erscheine?", "tokens": ["Doch", "z\u00fcrnst", "du", ",", "und", "er\u00b7staunst", ",", "wa\u00b7rum", "ich", "nicht", "er\u00b7schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich schmause, Freund, nicht gern alleine.", "tokens": ["Ich", "schmau\u00b7se", ",", "Freund", ",", "nicht", "gern", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}