{"textgrid.poem.24890": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Halb schon versch\u00fcttet von dem weh'nden Sande", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halb schon versch\u00fcttet von dem weh'nden Sande", "tokens": ["Halb", "schon", "ver\u00b7sch\u00fct\u00b7tet", "von", "dem", "weh'n\u00b7den", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ragt einsam dies verfallne Grab;", "tokens": ["Ragt", "ein\u00b7sam", "dies", "ver\u00b7fall\u00b7ne", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne flammt darauf in lohem Brande,", "tokens": ["Die", "Son\u00b7ne", "flammt", "da\u00b7rauf", "in", "lo\u00b7hem", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie vor Aeonen, noch herab.", "tokens": ["Wie", "vor", "A\u00b7e\u00b7o\u00b7nen", ",", "noch", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In keinem Grashalm, nicht im d\u00fcrrsten Moose", "tokens": ["In", "kei\u00b7nem", "Gras\u00b7halm", ",", "nicht", "im", "d\u00fcrrs\u00b7ten", "Moo\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ringsum von Leben eine Spur;", "tokens": ["Ring\u00b7sum", "von", "Le\u00b7ben", "ei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Weit dehnen sich bis in das Grenzenlose", "tokens": ["Weit", "deh\u00b7nen", "sich", "bis", "in", "das", "Gren\u00b7zen\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Himmel und die W\u00fcste nur.", "tokens": ["Der", "Him\u00b7mel", "und", "die", "W\u00fcs\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Bilder seh' ich auf dem Stein und Zeichen", "tokens": ["Und", "Bil\u00b7der", "seh'", "ich", "auf", "dem", "Stein", "und", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einer Schrift, die keiner kennt,", "tokens": ["In", "ei\u00b7ner", "Schrift", ",", "die", "kei\u00b7ner", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gestalten, die der V\u00f6lker keinem gleichen,", "tokens": ["Ge\u00b7stal\u00b7ten", ",", "die", "der", "V\u00f6l\u00b7ker", "kei\u00b7nem", "glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So viele die Geschichte nennt.", "tokens": ["So", "vie\u00b7le", "die", "Ge\u00b7schich\u00b7te", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wen birgt das Grabmal? Eines K\u00f6nigs Leiche,", "tokens": ["Wen", "birgt", "das", "Grab\u00b7mal", "?", "Ei\u00b7nes", "K\u00f6\u00b7nigs", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hier das Scepter schwang", "tokens": ["Der", "hier", "das", "Scep\u00b7ter", "schwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und stolz hinunter sah auf seine Reiche", "tokens": ["Und", "stolz", "hin\u00b7un\u00b7ter", "sah", "auf", "sei\u00b7ne", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "APPR", "PPOSAT", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Aufgang bis zum Niedergang.", "tokens": ["Vom", "Auf\u00b7gang", "bis", "zum", "Nie\u00b7der\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In Sprachen, nun jahrtausendlang verklungen,", "tokens": ["In", "Spra\u00b7chen", ",", "nun", "jahr\u00b7tau\u00b7send\u00b7lang", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward ihm vielleicht Unsterblichkeit,", "tokens": ["Ward", "ihm", "viel\u00b7leicht", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie den Ges\u00e4ngen, drin sie ihn besungen,", "tokens": ["Wie", "den", "Ge\u00b7s\u00e4n\u00b7gen", ",", "drin", "sie", "ihn", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von seinen Dichtern prophezeit.", "tokens": ["Von", "sei\u00b7nen", "Dich\u00b7tern", "pro\u00b7phe\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vielleicht \u2013 doch nein, nicht einen Laut mehr stammelt", "tokens": ["Viel\u00b7leicht", "\u2013", "doch", "nein", ",", "nicht", "ei\u00b7nen", "Laut", "mehr", "stam\u00b7melt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "ADV", "PTKANT", "$,", "PTKNEG", "ART", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von damals die Erinnerung,", "tokens": ["Von", "da\u00b7mals", "die", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor dem Staube, der sich hier gesammelt,", "tokens": ["Und", "vor", "dem", "Stau\u00b7be", ",", "der", "sich", "hier", "ge\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Scheint jede andre Vorwelt jung.", "tokens": ["Scheint", "je\u00b7de", "and\u00b7re", "Vor\u00b7welt", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer giebt mir Kunde von der Zeit, der langen,", "tokens": ["Wer", "giebt", "mir", "Kun\u00b7de", "von", "der", "Zeit", ",", "der", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schon auf Erden war?", "tokens": ["Die", "schon", "auf", "Er\u00b7den", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer nennt mir eine, die nicht schon vergangen,", "tokens": ["Wer", "nennt", "mir", "ei\u00b7ne", ",", "die", "nicht", "schon", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "$,", "PRELS", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00e4r' es Platos Riesenjahr?", "tokens": ["Und", "w\u00e4r'", "es", "Pla\u00b7tos", "Rie\u00b7sen\u00b7jahr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Selbst f\u00fchl' ich hier das Haupt mir von der Schwinge", "tokens": ["Selbst", "f\u00fchl'", "ich", "hier", "das", "Haupt", "mir", "von", "der", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Todesengels schon umkreist,", "tokens": ["Des", "To\u00b7de\u00b7sen\u00b7gels", "schon", "um\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schwindelnd in die gro\u00dfe Nacht der Dinge", "tokens": ["Und", "schwin\u00b7delnd", "in", "die", "gro\u00b7\u00dfe", "Nacht", "der", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versinkt mit Zagen mir der Geist.", "tokens": ["Ver\u00b7sinkt", "mit", "Za\u00b7gen", "mir", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O Mensch, mit deinem Schaffen, deinem Streben,", "tokens": ["O", "Mensch", ",", "mit", "dei\u00b7nem", "Schaf\u00b7fen", ",", "dei\u00b7nem", "Stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Opfer der Vergessenheit,", "tokens": ["Du", "Op\u00b7fer", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was z\u00e4hlst du deine Jahre? Nur im Leben,", "tokens": ["Was", "z\u00e4hlst", "du", "dei\u00b7ne", "Jah\u00b7re", "?", "Nur", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein im Tod ist keine Zeit.", "tokens": ["Al\u00b7lein", "im", "Tod", "ist", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Im Tod ist keine Zeit. F\u00fchrt er als Beute", "tokens": ["Im", "Tod", "ist", "kei\u00b7ne", "Zeit", ".", "F\u00fchrt", "er", "als", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "$.", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich heute noch zum Hades ein,", "tokens": ["Dich", "heu\u00b7te", "noch", "zum", "Ha\u00b7des", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wirst du in dem Schattenreich noch heute", "tokens": ["So", "wirst", "du", "in", "dem", "Schat\u00b7ten\u00b7reich", "noch", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich alt mit K\u00f6nig Cheops sein.", "tokens": ["Gleich", "alt", "mit", "K\u00f6\u00b7nig", "Cheops", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "NE", "VAINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}