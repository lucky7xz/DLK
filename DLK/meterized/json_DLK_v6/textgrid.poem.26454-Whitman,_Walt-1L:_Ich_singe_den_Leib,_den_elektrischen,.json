{"textgrid.poem.26454": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich singe den Leib, den elektrischen,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe den Leib, den elektrischen,", "tokens": ["Ich", "sin\u00b7ge", "den", "Leib", ",", "den", "e\u00b7lekt\u00b7ri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Heerscharen, die ich liebe, umg\u00fcrten mich und ich umg\u00fcrte sie,", "tokens": ["Die", "Heer\u00b7scha\u00b7ren", ",", "die", "ich", "lie\u00b7be", ",", "um\u00b7g\u00fcr\u00b7ten", "mich", "und", "ich", "um\u00b7g\u00fcr\u00b7te", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+--+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Sie wollen mich nichtlassen, bis ich mit ihnen gehe, f\u00fcr sie einstehe,", "tokens": ["Sie", "wol\u00b7len", "mich", "nicht\u00b7las\u00b7sen", ",", "bis", "ich", "mit", "ih\u00b7nen", "ge\u00b7he", ",", "f\u00fcr", "sie", "ein\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+----+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Da\u00df ich sie l\u00e4utere und sie f\u00fclle mit der F\u00fclle der Seele!", "tokens": ["Da\u00df", "ich", "sie", "l\u00e4u\u00b7te\u00b7re", "und", "sie", "f\u00fcl\u00b7le", "mit", "der", "F\u00fcl\u00b7le", "der", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-++-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Hat man gezweifelt, da\u00df diejenigen sich verstecken, die ihren eigenen Leib verderben?", "tokens": ["Hat", "man", "ge\u00b7zwei\u00b7felt", ",", "da\u00df", "die\u00b7je\u00b7ni\u00b7gen", "sich", "ver\u00b7ste\u00b7cken", ",", "die", "ih\u00b7ren", "ei\u00b7ge\u00b7nen", "Leib", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "KOUS", "PDS", "PRF", "VVINF", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--++--+-+--+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und da\u00df die, welche die Lebendigen sch\u00e4nden, so schlecht sind wie die, welche die Toten sch\u00e4nden?", "tokens": ["Und", "da\u00df", "die", ",", "wel\u00b7che", "die", "Le\u00b7ben\u00b7di\u00b7gen", "sch\u00e4n\u00b7den", ",", "so", "schlecht", "sind", "wie", "die", ",", "wel\u00b7che", "die", "To\u00b7ten", "sch\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "$,", "PRELS", "ART", "NN", "VVINF", "$,", "ADV", "ADJD", "VAFIN", "KOKOM", "ART", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+--+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und da\u00df der Leib nicht v\u00f6llig so viel vermag wie die Seele?", "tokens": ["Und", "da\u00df", "der", "Leib", "nicht", "v\u00f6l\u00b7lig", "so", "viel", "ver\u00b7mag", "wie", "die", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADJD", "ADV", "ADV", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn der Leib nicht die Seele w\u00e4re, was ist dann die Seele?", "tokens": ["Wenn", "der", "Leib", "nicht", "die", "See\u00b7le", "w\u00e4\u00b7re", ",", "was", "ist", "dann", "die", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "VAFIN", "$,", "PWS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}