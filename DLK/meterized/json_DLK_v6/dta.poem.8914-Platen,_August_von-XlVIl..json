{"dta.poem.8914": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlVIl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Such' ich nach g\u00fcnst'gem Ort und g\u00fcnst'ger Stunde;", "tokens": ["Such'", "ich", "nach", "g\u00fcnst'\u00b7gem", "Ort", "und", "g\u00fcnst'\u00b7ger", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch schwebt dein Bild nur stets im Hintergrunde,", "tokens": ["Doch", "schwebt", "dein", "Bild", "nur", "stets", "im", "Hin\u00b7ter\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df die n\u00e4hern Dinge schnell verschwinden.", "tokens": ["In\u00b7de\u00df", "die", "n\u00e4\u00b7hern", "Din\u00b7ge", "schnell", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geselligkeit vermag mich nicht zu binden,", "tokens": ["Ge\u00b7sel\u00b7lig\u00b7keit", "ver\u00b7mag", "mich", "nicht", "zu", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Einsamkeit ertragen blos Gesunde:", "tokens": ["Und", "Ein\u00b7sam\u00b7keit", "er\u00b7tra\u00b7gen", "blos", "Ge\u00b7sun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denk' ich, so sch\u00e4rft des Denkens Pfeil die Wunde,", "tokens": ["Denk'", "ich", ",", "so", "sch\u00e4rft", "des", "Den\u00b7kens", "Pfeil", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schweif' ich m\u00fcssig, klag' ich es den Winden.", "tokens": ["Und", "schweif'", "ich", "m\u00fcs\u00b7sig", ",", "klag'", "ich", "es", "den", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und soll ich je von dieser Pein genesen,", "tokens": ["Und", "soll", "ich", "je", "von", "die\u00b7ser", "Pein", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So werde mir, so zeige dich gewogen,", "tokens": ["So", "wer\u00b7de", "mir", ",", "so", "zei\u00b7ge", "dich", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn du nur fehlst dem Herzen, theures Wesen!", "tokens": ["Denn", "du", "nur", "fehlst", "dem", "Her\u00b7zen", ",", "theu\u00b7res", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich liebte manchen Freund und ward betrogen;", "tokens": ["Ich", "lieb\u00b7te", "man\u00b7chen", "Freund", "und", "ward", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch mag die Welt in diesen Bl\u00e4ttern lesen,", "tokens": ["Doch", "mag", "die", "Welt", "in", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich dich allen Andern vorgezogen.", "tokens": ["Da\u00df", "ich", "dich", "al\u00b7len", "An\u00b7dern", "vor\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIAT", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}