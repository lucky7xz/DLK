{"dta.poem.11026": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gott alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich deine Macht und Weisheit in der Tief und in der\nH\u00f6he,", "tokens": ["Da", "ich", "dei\u00b7ne", "Macht", "und", "Weis\u00b7heit", "in", "der", "Tief", "und", "in", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "O allgegenw\u00e4rtger Sch\u00f6pfer, allenthalben gl\u00e4nzen sehe,", "tokens": ["O", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7ger", "Sch\u00f6p\u00b7fer", ",", "al\u00b7len\u00b7thal\u00b7ben", "gl\u00e4n\u00b7zen", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+---+-+-+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Frag ich die Natur und mich:", "tokens": ["Frag", "ich", "die", "Na\u00b7tur", "und", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer bist du? Und wer bin ich?", "tokens": ["Wer", "bist", "du", "?", "Und", "wer", "bin", "ich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "KON", "PWS", "VAFIN", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Darauf h\u00f6r ich, auf mein Fragen,", "tokens": ["Da\u00b7rauf", "h\u00f6r", "ich", ",", "auf", "mein", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alle Dinge die\u00df mir sagen:", "tokens": ["Al\u00b7le", "Din\u00b7ge", "die\u00df", "mir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201edu bist alles; ich bin nichts. Doch bezeugt auch mein", "tokens": ["\u201e", "du", "bist", "al\u00b7les", ";", "ich", "bin", "nichts", ".", "Doch", "be\u00b7zeugt", "auch", "mein"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PIS", "$.", "PPER", "VAFIN", "PIS", "$.", "KON", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda ich fast vernichtigt werde; ich sey viel, durch deine", "tokens": ["\u201e", "da", "ich", "fast", "ver\u00b7nich\u00b7tigt", "wer\u00b7de", ";", "ich", "sey", "viel", ",", "durch", "dei\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "$,", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}