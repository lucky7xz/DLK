{"textgrid.poem.59692": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Als Thiers die Welschen aufger\u00fchrt hatte", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und brauset der Sturmwind des Krieges heran,", "tokens": ["Und", "brau\u00b7set", "der", "Sturm\u00b7wind", "des", "Krie\u00b7ges", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wollen die Welschen ihn haben,", "tokens": ["Und", "wol\u00b7len", "die", "Wel\u00b7schen", "ihn", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So sammle, mein Deutschland, dich stark wie ein Mann", "tokens": ["So", "samm\u00b7le", ",", "mein", "Deutschland", ",", "dich", "stark", "wie", "ein", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PRF", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und bringe die blutigen Gaben,", "tokens": ["Und", "brin\u00b7ge", "die", "blu\u00b7ti\u00b7gen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und bringe das Schrecken und trage das Grauen", "tokens": ["Und", "brin\u00b7ge", "das", "Schre\u00b7cken", "und", "tra\u00b7ge", "das", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Von all deinen Bergen, aus all deinen Gauen,", "tokens": ["Von", "all", "dei\u00b7nen", "Ber\u00b7gen", ",", "aus", "all", "dei\u00b7nen", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Und klinge die Losung: ", "tokens": ["Und", "klin\u00b7ge", "die", "Lo\u00b7sung", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie wollen's; so rei\u00dfe denn, deutsche Geduld,", "tokens": ["Sie", "wol\u00b7len's", ";", "so", "rei\u00b7\u00dfe", "denn", ",", "deut\u00b7sche", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$.", "ADV", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Rei\u00df durch von dem Belt bis zum Rheine!", "tokens": ["Rei\u00df", "durch", "von", "dem", "Belt", "bis", "zum", "Rhei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "NN", "APPR", "APPRART", "NE", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Wir fordern die lange gestundete Schuld \u2013", "tokens": ["Wir", "for\u00b7dern", "die", "lan\u00b7ge", "ge\u00b7stun\u00b7de\u00b7te", "Schuld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auf! Welsche, und r\u00fchret die Beine!", "tokens": ["Auf", "!", "Wel\u00b7sche", ",", "und", "r\u00fch\u00b7ret", "die", "Bei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wir wollen im Spiele der Schwerter und Lanzen", "tokens": ["Wir", "wol\u00b7len", "im", "Spie\u00b7le", "der", "Schwer\u00b7ter", "und", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Den wilden, den blutigen Tanz mit euch tanzen,", "tokens": ["Den", "wil\u00b7den", ",", "den", "blu\u00b7ti\u00b7gen", "Tanz", "mit", "euch", "tan\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wir klingen die Losung: ", "tokens": ["Wir", "klin\u00b7gen", "die", "Lo\u00b7sung", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein einiges Deutschland, mein k\u00fchnes, heran!", "tokens": ["Mein", "ei\u00b7ni\u00b7ges", "Deutschland", ",", "mein", "k\u00fch\u00b7nes", ",", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir wollen ein Liedlein euch singen", "tokens": ["Wir", "wol\u00b7len", "ein", "Lied\u00b7lein", "euch", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Von dem, was die schleichende List euch gewann,", "tokens": ["Von", "dem", ",", "was", "die", "schlei\u00b7chen\u00b7de", "List", "euch", "ge\u00b7wann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von Stra\u00dfburg und Metz und Lothringen:", "tokens": ["Von", "Stra\u00df\u00b7burg", "und", "Metz", "und", "Loth\u00b7rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zur\u00fcck sollt ihr zahlen, heraus sollt ihr geben!", "tokens": ["Zu\u00b7r\u00fcck", "sollt", "ihr", "zah\u00b7len", ",", "he\u00b7raus", "sollt", "ihr", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So stehe der Kampf uns auf Tod und auf Leben!", "tokens": ["So", "ste\u00b7he", "der", "Kampf", "uns", "auf", "Tod", "und", "auf", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "So klinge die Losung: ", "tokens": ["So", "klin\u00b7ge", "die", "Lo\u00b7sung", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Mein einiges Deutschland, mein freies, heran!", "tokens": ["Mein", "ei\u00b7ni\u00b7ges", "Deutschland", ",", "mein", "frei\u00b7es", ",", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie wollen, sie sollen es haben.", "tokens": ["Sie", "wol\u00b7len", ",", "sie", "sol\u00b7len", "es", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Auf! Sammle und r\u00fcste dich stark wie ein Mann", "tokens": ["Auf", "!", "Samm\u00b7le", "und", "r\u00fcs\u00b7te", "dich", "stark", "wie", "ein", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$.", "NN", "KON", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und bringe die blutigen Gaben!", "tokens": ["Und", "brin\u00b7ge", "die", "blu\u00b7ti\u00b7gen", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Du, das sie nun nimmer mit Listen zersplittern,", "tokens": ["Du", ",", "das", "sie", "nun", "nim\u00b7mer", "mit", "Lis\u00b7ten", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erbrause wie Windsbraut aus schwarzen Gewittern!", "tokens": ["Er\u00b7brau\u00b7se", "wie", "Winds\u00b7braut", "aus", "schwar\u00b7zen", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "So klinge die Losung: ", "tokens": ["So", "klin\u00b7ge", "die", "Lo\u00b7sung", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}