{"textgrid.poem.45417": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Antispekulativ", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer M\u00fchle vergleich ich den Verstand,", "tokens": ["Ei\u00b7ner", "M\u00fch\u00b7le", "ver\u00b7gleich", "ich", "den", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die mahlt, was an Korn sich gesch\u00fcttet fand;", "tokens": ["Die", "mahlt", ",", "was", "an", "Korn", "sich", "ge\u00b7sch\u00fct\u00b7tet", "fand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "APPR", "NN", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Doch geschehen der Sch\u00fcttungen keine,", "tokens": ["Doch", "ge\u00b7sche\u00b7hen", "der", "Sch\u00fct\u00b7tun\u00b7gen", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "PIAT", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "So reiben sich selber die Steine", "tokens": ["So", "rei\u00b7ben", "sich", "sel\u00b7ber", "die", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und erzeugen Staub und Splitter und Sand.", "tokens": ["Und", "er\u00b7zeu\u00b7gen", "Staub", "und", "Split\u00b7ter", "und", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}}}}