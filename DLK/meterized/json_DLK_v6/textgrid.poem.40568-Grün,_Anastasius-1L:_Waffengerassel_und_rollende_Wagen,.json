{"textgrid.poem.40568": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Waffengerassel und rollende Wagen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Waffengerassel und rollende Wagen,", "tokens": ["Waf\u00b7fen\u00b7ge\u00b7ras\u00b7sel", "und", "rol\u00b7len\u00b7de", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Dr\u00f6hnender Taktschritt, Wiehern der Rosse,", "tokens": ["Dr\u00f6h\u00b7nen\u00b7der", "Takt\u00b7schritt", ",", "Wie\u00b7hern", "der", "Ros\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Staubgewirbel und Blitze der M\u00f6rser!", "tokens": ["Staub\u00b7ge\u00b7wir\u00b7bel", "und", "Blit\u00b7ze", "der", "M\u00f6r\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Donnernd fallen die W\u00fcrfel der Schlacht!", "tokens": ["Don\u00b7nernd", "fal\u00b7len", "die", "W\u00fcr\u00b7fel", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ueber den Heeren flattert des Kriegsgotts", "tokens": ["Ue\u00b7ber", "den", "Hee\u00b7ren", "flat\u00b7tert", "des", "Kriegs\u00b7gotts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+++", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Furchtbar-pr\u00e4chtiger, feuriger Vogel,", "tokens": ["Furcht\u00b7ba\u00b7rpr\u00e4ch\u00b7ti\u00b7ger", ",", "feu\u00b7ri\u00b7ger", "Vo\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Lodernden Kamm und leuchtende Fl\u00fcgel", "tokens": ["Lo\u00b7dern\u00b7den", "Kamm", "und", "leuch\u00b7ten\u00b7de", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sch\u00fcttelt im Flug der rothe Hahn.", "tokens": ["Sch\u00fct\u00b7telt", "im", "Flug", "der", "ro\u00b7the", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ihm von den Schwingen tr\u00e4ufelt ein Regen", "tokens": ["Ihm", "von", "den", "Schwin\u00b7gen", "tr\u00e4u\u00b7felt", "ein", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Spr\u00fchender Funkenk\u00f6rner zur Erde,", "tokens": ["Spr\u00fc\u00b7hen\u00b7der", "Fun\u00b7ken\u00b7k\u00f6r\u00b7ner", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie wurfkundiger Hand des S\u00e4manns", "tokens": ["Wie", "wurf\u00b7kun\u00b7di\u00b7ger", "Hand", "des", "S\u00e4\u00b7manns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzende Saatenk\u00f6rner entspr\u00fchn.", "tokens": ["Gl\u00e4n\u00b7zen\u00b7de", "Saa\u00b7ten\u00b7k\u00f6r\u00b7ner", "ent\u00b7spr\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Reich aufsprie\u00dfen die feurigen Saaten,", "tokens": ["Reich", "auf\u00b7sprie\u00b7\u00dfen", "die", "feu\u00b7ri\u00b7gen", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Erst nur sch\u00fcchterne, gl\u00fchende Halme,", "tokens": ["Erst", "nur", "sch\u00fcch\u00b7ter\u00b7ne", ",", "gl\u00fc\u00b7hen\u00b7de", "Hal\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dann, vom Winde bewegt, ein weites", "tokens": ["Dann", ",", "vom", "Win\u00b7de", "be\u00b7wegt", ",", "ein", "wei\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPRART", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wogendes, wallendes Garbenmeer!", "tokens": ["Wo\u00b7gen\u00b7des", ",", "wal\u00b7len\u00b7des", "Gar\u00b7ben\u00b7meer", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Unter den gelben Aehrenfluthen", "tokens": ["Un\u00b7ter", "den", "gel\u00b7ben", "A\u00b7eh\u00b7ren\u00b7flu\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Bl\u00fchn die blauen und purpurnen Fl\u00e4mmchen,", "tokens": ["Bl\u00fchn", "die", "blau\u00b7en", "und", "pur\u00b7pur\u00b7nen", "Fl\u00e4mm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie im Schatten der goldenen Halme", "tokens": ["Wie", "im", "Schat\u00b7ten", "der", "gol\u00b7de\u00b7nen", "Hal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Blaue Kornblum' und feuriger Mohn.", "tokens": ["Blau\u00b7e", "Korn\u00b7blum'", "und", "feu\u00b7ri\u00b7ger", "Mohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "St\u00f6hnen der M\u00fctter, Weinen der Kinder:", "tokens": ["St\u00f6h\u00b7nen", "der", "M\u00fct\u00b7ter", ",", "Wei\u00b7nen", "der", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gr\u00e4\u00dflicher Wachtelschlag in dem Korne!", "tokens": ["Gr\u00e4\u00df\u00b7li\u00b7cher", "Wach\u00b7tel\u00b7schlag", "in", "dem", "Kor\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wimmern der Feuerglocken in L\u00fcften:", "tokens": ["Wim\u00b7mern", "der", "Feu\u00b7er\u00b7glo\u00b7cken", "in", "L\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wirbelnder Lerchensang ob der Saat!", "tokens": ["Wir\u00b7beln\u00b7der", "Ler\u00b7chen\u00b7sang", "ob", "der", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Doch, ein unerm\u00fcdlicher S\u00e4mann,", "tokens": ["Doch", ",", "ein", "un\u00b7er\u00b7m\u00fcd\u00b7li\u00b7cher", "S\u00e4\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fliegt er, neue Saat zu bestellen,", "tokens": ["Fliegt", "er", ",", "neu\u00b7e", "Saat", "zu", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Unbek\u00fcmmert der schwarzen Stoppeln,", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", "der", "schwar\u00b7zen", "Stop\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dr\u00fcber der Herbstwind klagend wallt.", "tokens": ["Dr\u00fc\u00b7ber", "der", "Herbst\u00b7wind", "kla\u00b7gend", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Tief im Gebirg' auf dem Thurm des Kirchleins", "tokens": ["Tief", "im", "Ge\u00b7bir\u00b7g'", "auf", "dem", "Thurm", "des", "Kirch\u00b7leins"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Senkt er zur Rast vom Fluge sich nieder.", "tokens": ["Senkt", "er", "zur", "Rast", "vom", "Flu\u00b7ge", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Horch, draus fluthen so fromme Ges\u00e4nge,", "tokens": ["Horch", ",", "draus", "flut\u00b7hen", "so", "from\u00b7me", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PAV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Horch, draus steigt ein so br\u00fcnstig Gebet!", "tokens": ["Horch", ",", "draus", "steigt", "ein", "so", "br\u00fcns\u00b7tig", "Ge\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PAV", "VVFIN", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Fluchen kennt er und Jammern und Jauchzen,", "tokens": ["Flu\u00b7chen", "kennt", "er", "und", "Jam\u00b7mern", "und", "Jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Fremd doch blieben ihm diese T\u00f6ne,", "tokens": ["Fremd", "doch", "blie\u00b7ben", "ihm", "die\u00b7se", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die ihn jetzt bannen, da er im Lauschen", "tokens": ["Die", "ihn", "jetzt", "ban\u00b7nen", ",", "da", "er", "im", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seine Fl\u00fcgel zu sch\u00fctteln vergi\u00dft.", "tokens": ["Sei\u00b7ne", "Fl\u00fc\u00b7gel", "zu", "sch\u00fct\u00b7teln", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Siehe, da tr\u00e4ufelt ein linder Regen,", "tokens": ["Sie\u00b7he", ",", "da", "tr\u00e4u\u00b7felt", "ein", "lin\u00b7der", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00fchlt und l\u00f6scht ihm die feurigen Schwingen;", "tokens": ["K\u00fchlt", "und", "l\u00f6scht", "ihm", "die", "feu\u00b7ri\u00b7gen", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Statt im reichen Gefieder, am Morgen", "tokens": ["Statt", "im", "rei\u00b7chen", "Ge\u00b7fie\u00b7der", ",", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ragt er als kaltes Eisengeripp.", "tokens": ["Ragt", "er", "als", "kal\u00b7tes", "Ei\u00b7sen\u00b7ge\u00b7ripp", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Und des Kriegsgotts pr\u00e4chtiger Vogel", "tokens": ["Und", "des", "Kriegs\u00b7gotts", "pr\u00e4ch\u00b7ti\u00b7ger", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ward zum Wetterhahne des K\u00fcsters,", "tokens": ["Ward", "zum", "Wet\u00b7ter\u00b7hah\u00b7ne", "des", "K\u00fcs\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kreist und tanzt zum Jubel der Kinder,", "tokens": ["Kreist", "und", "tanzt", "zum", "Ju\u00b7bel", "der", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dreht sich willig nach Wetter und Wind.", "tokens": ["Dreht", "sich", "wil\u00b7lig", "nach", "Wet\u00b7ter", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}