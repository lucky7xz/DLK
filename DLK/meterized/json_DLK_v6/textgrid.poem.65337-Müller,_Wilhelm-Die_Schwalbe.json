{"textgrid.poem.65337": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwalbe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schwalbe m\u00f6cht' ich werden, fliegen grad' in deine Kammer,", "tokens": ["Ei\u00b7ne", "Schwal\u00b7be", "m\u00f6cht'", "ich", "wer\u00b7den", ",", "flie\u00b7gen", "grad'", "in", "dei\u00b7ne", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und auf deines Hauptes Kissen baun ein Nest f\u00fcr meinen Jammer.", "tokens": ["Und", "auf", "dei\u00b7nes", "Haup\u00b7tes", "Kis\u00b7sen", "baun", "ein", "Nest", "f\u00fcr", "mei\u00b7nen", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}