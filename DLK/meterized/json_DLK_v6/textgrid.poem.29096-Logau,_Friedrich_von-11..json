{"textgrid.poem.29096": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier mag auff St. Martin gar ungescheuter Sachen", "tokens": ["Hier", "mag", "auff", "St.", "Mar\u00b7tin", "gar", "un\u00b7ge\u00b7scheu\u00b7ter", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NE", "NE", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein iedes Weib dem Mann ein paar von H\u00f6rnern machen;", "tokens": ["Ein", "ie\u00b7des", "Weib", "dem", "Mann", "ein", "paar", "von", "H\u00f6r\u00b7nern", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "ART", "PIAT", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um diese Zeit und Tag sind H\u00f6rner hier gesund,", "tokens": ["Um", "die\u00b7se", "Zeit", "und", "Tag", "sind", "H\u00f6r\u00b7ner", "hier", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "KON", "NN", "VAFIN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind sonst das gantze Jahr mit wissen nicht vergunt.", "tokens": ["Sind", "sonst", "das", "gant\u00b7ze", "Jahr", "mit", "wis\u00b7sen", "nicht", "ver\u00b7gunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}