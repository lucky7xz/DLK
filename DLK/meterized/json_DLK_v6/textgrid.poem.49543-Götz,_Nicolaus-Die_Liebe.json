{"textgrid.poem.49543": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr V\u00f6gel, ist's Aurorens Strahl,", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "ist's", "Au\u00b7ro\u00b7rens", "Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der euch erweckete, zu singen?", "tokens": ["Der", "euch", "er\u00b7we\u00b7cke\u00b7te", ",", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist es nicht. Aurorens Strahl", "tokens": ["Er", "ist", "es", "nicht", ".", "Au\u00b7ro\u00b7rens", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$.", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann diese Myrthen nicht durchdringen.", "tokens": ["Kann", "die\u00b7se", "Myr\u00b7then", "nicht", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lieb' allein, die Lieb' erweckt euch hie.", "tokens": ["Die", "Lieb'", "al\u00b7lein", ",", "die", "Lieb'", "er\u00b7weckt", "euch", "hie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich kann euch diese Liebe g\u00f6nnen!", "tokens": ["Ich", "kann", "euch", "die\u00b7se", "Lie\u00b7be", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich aber weckt dieselbe nie.", "tokens": ["Mich", "a\u00b7ber", "weckt", "die\u00b7sel\u00b7be", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PDAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie wollte die mich wecken k\u00f6nnen:", "tokens": ["Wie", "woll\u00b7te", "die", "mich", "we\u00b7cken", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die mich, seitdem der West in junge Rosen blies,", "tokens": ["Die", "mich", ",", "seit\u00b7dem", "der", "West", "in", "jun\u00b7ge", "Ro\u00b7sen", "blies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nie schlafen lies, nie schlafen lies?", "tokens": ["Nie", "schla\u00b7fen", "lies", ",", "nie", "schla\u00b7fen", "lies", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$,", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}