{"textgrid.poem.35857": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf allen Bl\u00e4ttern steht geschrieben,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf allen Bl\u00e4ttern steht geschrieben,", "tokens": ["Auf", "al\u00b7len", "Bl\u00e4t\u00b7tern", "steht", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wundergut der Vater ist.", "tokens": ["Wie", "wun\u00b7der\u00b7gut", "der", "Va\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Herz, wie magst du Ihn nicht lieben,", "tokens": ["O", "Herz", ",", "wie", "magst", "du", "Ihn", "nicht", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich aus jeder Blume gr\u00fc\u00dft? \u2013", "tokens": ["Der", "dich", "aus", "je\u00b7der", "Blu\u00b7me", "gr\u00fc\u00dft", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf alle Bl\u00e4ttlein m\u00f6cht' ich schreiben,", "tokens": ["Auf", "al\u00b7le", "Bl\u00e4tt\u00b7lein", "m\u00f6cht'", "ich", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr mein s\u00fc\u00dfer Freund mich liebt,", "tokens": ["Wie", "sehr", "mein", "s\u00fc\u00b7\u00dfer", "Freund", "mich", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all Sein Thun und all Sein Treiben,", "tokens": ["Und", "all", "Sein", "Thun", "und", "all", "Sein", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Er als Mensch f\u00fcr mich ge\u00fcbt.", "tokens": ["Das", "Er", "als", "Mensch", "f\u00fcr", "mich", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KOUS", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf alle Bl\u00e4tter m\u00f6cht' ich malen", "tokens": ["Auf", "al\u00b7le", "Bl\u00e4t\u00b7ter", "m\u00f6cht'", "ich", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Liebsten klares Angesicht,", "tokens": ["Des", "Liebs\u00b7ten", "kla\u00b7res", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch alle Farben, alle Strahlen", "tokens": ["Doch", "al\u00b7le", "Far\u00b7ben", ",", "al\u00b7le", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erreichen Seine Sch\u00f6ne nicht.", "tokens": ["Er\u00b7rei\u00b7chen", "Sei\u00b7ne", "Sch\u00f6\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und allen B\u00e4umen m\u00f6cht' ich sagen", "tokens": ["Und", "al\u00b7len", "B\u00e4u\u00b7men", "m\u00f6cht'", "ich", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Seiner Treue, Seiner Huld,", "tokens": ["Von", "Sei\u00b7ner", "Treu\u00b7e", ",", "Sei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen Steinen mu\u00df ich's klagen,", "tokens": ["Und", "al\u00b7len", "Stei\u00b7nen", "mu\u00df", "ich's", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Ihn get\u00f6dtet meine Schuld.", "tokens": ["Da\u00df", "Ihn", "ge\u00b7t\u00f6d\u00b7tet", "mei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}