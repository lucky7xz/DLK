{"textgrid.poem.61557": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Blauer Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du \u00c4therblau, vom sel'gen Licht getr\u00e4nkt,", "tokens": ["Du", "\u00c4\u00b7ther\u00b7blau", ",", "vom", "sel'\u00b7gen", "Licht", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchsicht'ge Tiefe, drein der Blick sich senkt,", "tokens": ["Durch\u00b7sicht'\u00b7ge", "Tie\u00b7fe", ",", "drein", "der", "Blick", "sich", "senkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis er geblendet taumelt, Abgrund du,", "tokens": ["Bis", "er", "ge\u00b7blen\u00b7det", "tau\u00b7melt", ",", "Ab\u00b7grund", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "$,", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unendlicher, der Heiterkeit und Ruh',", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", ",", "der", "Hei\u00b7ter\u00b7keit", "und", "Ruh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie schafft dein s\u00fc\u00dfer Hauch den Geist mir leicht,", "tokens": ["Wie", "schafft", "dein", "s\u00fc\u00b7\u00dfer", "Hauch", "den", "Geist", "mir", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den staubumschr\u00e4nkten, der dir, ach, nicht gleicht,", "tokens": ["Den", "stau\u00b7bum\u00b7schr\u00e4nk\u00b7ten", ",", "der", "dir", ",", "ach", ",", "nicht", "gleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "$,", "ITJ", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und doch, von deiner Klarheit anger\u00fchrt,", "tokens": ["Und", "doch", ",", "von", "dei\u00b7ner", "Klar\u00b7heit", "an\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In sich den Keim verwandter Zukunft sp\u00fcrt!", "tokens": ["In", "sich", "den", "Keim", "ver\u00b7wand\u00b7ter", "Zu\u00b7kunft", "sp\u00fcrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn schauernd ahnt er, so ges\u00e4ttigt ganz", "tokens": ["Denn", "schau\u00b7ernd", "ahnt", "er", ",", "so", "ge\u00b7s\u00e4t\u00b7tigt", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADV", "VVPP", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Von heil'gem Frieden ruhn im lautern Glanz,", "tokens": ["Von", "heil'\u00b7gem", "Frie\u00b7den", "ruhn", "im", "lau\u00b7tern", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So Licht und Segen str\u00f6men m\u00fchelos", "tokens": ["So", "Licht", "und", "Se\u00b7gen", "str\u00f6\u00b7men", "m\u00fc\u00b7he\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus eigner nie ersch\u00f6pfter F\u00fcllen Scho\u00df \u2013", "tokens": ["Aus", "eig\u00b7ner", "nie", "er\u00b7sch\u00f6pf\u00b7ter", "F\u00fcl\u00b7len", "Scho\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das wird, ob auch nach langer Wandlung Pein,", "tokens": ["Das", "wird", ",", "ob", "auch", "nach", "lan\u00b7ger", "Wand\u00b7lung", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zuletzt die Blume seines Wesens sein.", "tokens": ["Zu\u00b7letzt", "die", "Blu\u00b7me", "sei\u00b7nes", "We\u00b7sens", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}