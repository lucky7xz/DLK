{"dta.poem.1365": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Worte:  \n Ich armer Mensch/ wer will mich erl\u00f6sen  \n von dem Leibe dieses Todes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer macht mich armen Klo\u00df der Erde", "tokens": ["Wer", "macht", "mich", "ar\u00b7men", "Klo\u00df", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Leibe dieses Todes frey?", "tokens": ["Vom", "Lei\u00b7be", "die\u00b7ses", "To\u00b7des", "frey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn k\u00f6mmt die g\u00f6ldne Zeit herbey/", "tokens": ["Wenn", "k\u00f6mmt", "die", "g\u00f6ld\u00b7ne", "Zeit", "her\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich erl\u00f6st und ruhig werde/", "tokens": ["Da\u00df", "ich", "er\u00b7l\u00f6st", "und", "ru\u00b7hig", "wer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df S\u00fcnde/ Noth und K\u00fcmmernisse", "tokens": ["Da\u00df", "S\u00fcn\u00b7de", "/", "Noth", "und", "K\u00fcm\u00b7mer\u00b7nis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich legen unter meine F\u00fcsse?", "tokens": ["Sich", "le\u00b7gen", "un\u00b7ter", "mei\u00b7ne", "F\u00fcs\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie matt ist meine krancke Seele/", "tokens": ["Wie", "matt", "ist", "mei\u00b7ne", "kran\u00b7cke", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der S\u00fcnden Aussatz greifft sie an/", "tokens": ["Der", "S\u00fcn\u00b7den", "Aus\u00b7satz", "greifft", "sie", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie sich nicht erholen kan;", "tokens": ["Da\u00df", "sie", "sich", "nicht", "er\u00b7ho\u00b7len", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr dr\u00e4ut die finstre Schlangen-H\u00f6le/", "tokens": ["Ihr", "dr\u00e4ut", "die", "finst\u00b7re", "Schlan\u00b7gen\u00b7H\u00f6le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ewig heissen Schwei\u00df erwecket/", "tokens": ["Die", "e\u00b7wig", "heis\u00b7sen", "Schwei\u00df", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dennoch nicht das Gifft ablecket.", "tokens": ["Und", "den\u00b7noch", "nicht", "das", "Gifft", "ab\u00b7le\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Richts wird an ihr gesund erfunden/", "tokens": ["Richts", "wird", "an", "ihr", "ge\u00b7sund", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Schmertzen ist der sieche Geist/", "tokens": ["Voll", "Schmert\u00b7zen", "ist", "der", "sie\u00b7che", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Thorheits-Wunden Eiter beist/", "tokens": ["Der", "Thor\u00b7heits\u00b7Wun\u00b7den", "Ei\u00b7ter", "beist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nicht gehefftet noch verbunden/", "tokens": ["Die", "nicht", "ge\u00b7heff\u00b7tet", "noch", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von F\u00fcssen an bi\u00df zu der Scheitel", "tokens": ["Von", "F\u00fcs\u00b7sen", "an", "bi\u00df", "zu", "der", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "KON", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ist sie bekr\u00e4nckt/ verderbt und eitel.", "tokens": ["Ist", "sie", "be\u00b7kr\u00e4nckt", "/", "ver\u00b7derbt", "und", "ei\u00b7tel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$(", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Will sie sich manchmahl gleich gewinnen/", "tokens": ["Will", "sie", "sich", "manch\u00b7mahl", "gleich", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Delilens verdammter Scho\u00df", "tokens": ["Aus", "De\u00b7li\u00b7lens", "ver\u00b7damm\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mit Simsons-Kr\u00e4fften machen lo\u00df/", "tokens": ["Mit", "Sim\u00b7sons\u00b7Kr\u00e4ff\u00b7ten", "ma\u00b7chen", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wird sie ihrer Schwachheit innen/", "tokens": ["Bald", "wird", "sie", "ih\u00b7rer", "Schwach\u00b7heit", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Giebt wieder nach und f\u00e4llt zur\u00fccke", "tokens": ["Giebt", "wie\u00b7der", "nach", "und", "f\u00e4llt", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In neue S\u00fcnd- und Kummer-Stricke.", "tokens": ["In", "neu\u00b7e", "S\u00fcn\u00b7d", "und", "Kum\u00b7mer\u00b7Stri\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Wollen hat sie offt zu leben/", "tokens": ["Das", "Wol\u00b7len", "hat", "sie", "offt", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie GOttes Wort und Ordnung heist/", "tokens": ["Wie", "Got\u00b7tes", "Wort", "und", "Ord\u00b7nung", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie sich aber drauff befleist/", "tokens": ["Wenn", "sie", "sich", "a\u00b7ber", "drauff", "be\u00b7fleist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ihr Verm\u00f6gen widerstreben/", "tokens": ["Will", "ihr", "Ver\u00b7m\u00f6\u00b7gen", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ungewissen Tritte wancken/", "tokens": ["Die", "un\u00b7ge\u00b7wis\u00b7sen", "Trit\u00b7te", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es bleibt am meisten bey Gedancken.", "tokens": ["Es", "bleibt", "am", "meis\u00b7ten", "bey", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie kan nun des Gesetzes Dr\u00e4uen/", "tokens": ["Wie", "kan", "nun", "des", "Ge\u00b7set\u00b7zes", "Dr\u00e4u\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der eignen Thaten b\u00f6ser Grund/", "tokens": ["Der", "eig\u00b7nen", "Tha\u00b7ten", "b\u00f6\u00b7ser", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der allzu ungewisse Bund", "tokens": ["Der", "all\u00b7zu", "un\u00b7ge\u00b7wis\u00b7se", "Bund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sinnen/ die sich selbst zerstreuen/", "tokens": ["Der", "Sin\u00b7nen", "/", "die", "sich", "selbst", "zer\u00b7streu\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht steten Kummer/ Streit und Schrecken", "tokens": ["Nicht", "ste\u00b7ten", "Kum\u00b7mer", "/", "Streit", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In meiner armen Seel erwecken?", "tokens": ["In", "mei\u00b7ner", "ar\u00b7men", "Seel", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Knecht des Todes und der S\u00fcnden/", "tokens": ["Der", "Knecht", "des", "To\u00b7des", "und", "der", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Leib/ in dem die Seele wohnt/", "tokens": ["Der", "Leib", "/", "in", "dem", "die", "See\u00b7le", "wohnt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird eben so wie sie belohnt/", "tokens": ["Wird", "e\u00b7ben", "so", "wie", "sie", "be\u00b7lohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOKOM", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat mit Wehthat zu empfinden/", "tokens": ["Und", "hat", "mit", "Weht\u00b7hat", "zu", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Wollust sch\u00f6ne Fr\u00fcchte weiset/", "tokens": ["Wie", "Wol\u00b7lust", "sch\u00f6\u00b7ne", "Fr\u00fcch\u00b7te", "wei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch nur Sodoms-Aepffel speiset.", "tokens": ["Und", "doch", "nur", "So\u00b7doms\u00