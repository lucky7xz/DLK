{"textgrid.poem.56243": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Blauer Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiter blick ich, ohne Reue", "tokens": ["Hei\u00b7ter", "blick", "ich", ",", "oh\u00b7ne", "Reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Himmels reine Bl\u00e4ue,", "tokens": ["In", "des", "Him\u00b7mels", "rei\u00b7ne", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der Sterne lichtem Gold.", "tokens": ["Zu", "der", "Ster\u00b7ne", "lich\u00b7tem", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Himmel, ist die Freundschaft,", "tokens": ["Ist", "der", "Him\u00b7mel", ",", "ist", "die", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist die Liebe mir doch hold.", "tokens": ["Ist", "die", "Lie\u00b7be", "mir", "doch", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Laure, mein Schicksal, laure!", "tokens": ["Lau\u00b7re", ",", "mein", "Schick\u00b7sal", ",", "lau\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Keine St\u00fcrme, keine Schmerzen,", "tokens": ["Kei\u00b7ne", "St\u00fcr\u00b7me", ",", "kei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitre Ruh im vollen Herzen,", "tokens": ["Heit\u00b7re", "Ruh", "im", "vol\u00b7len", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann es aber anders sein?", "tokens": ["Kann", "es", "a\u00b7ber", "an\u00b7ders", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blauer Himmel, treue Freundschaft,", "tokens": ["Blau\u00b7er", "Him\u00b7mel", ",", "treu\u00b7e", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reiche Liebe sind ja mein.", "tokens": ["Rei\u00b7che", "Lie\u00b7be", "sind", "ja", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Laure, mein Schicksal, laure!", "tokens": ["Lau\u00b7re", ",", "mein", "Schick\u00b7sal", ",", "lau\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Hat das Schicksal arge T\u00fccke,", "tokens": ["Hat", "das", "Schick\u00b7sal", "ar\u00b7ge", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, ich f\u00fcrchte nichts vom Gl\u00fccke,", "tokens": ["Sieh", ",", "ich", "f\u00fcrch\u00b7te", "nichts", "vom", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiter bin ich, wie die Luft.", "tokens": ["Hei\u00b7ter", "bin", "ich", ",", "wie", "die", "Luft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein der Himmel, mein die Freundschaft,", "tokens": ["Mein", "der", "Him\u00b7mel", ",", "mein", "die", "Freund\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "$,", "PPOSAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein die Liebe bis zur Gruft.", "tokens": ["Mein", "die", "Lie\u00b7be", "bis", "zur", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Laure, mein Schicksal, laure!", "tokens": ["Lau\u00b7re", ",", "mein", "Schick\u00b7sal", ",", "lau\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}