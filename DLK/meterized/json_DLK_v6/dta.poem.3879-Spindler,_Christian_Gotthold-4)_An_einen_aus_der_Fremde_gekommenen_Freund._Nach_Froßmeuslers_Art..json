{"dta.poem.3879": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "4) An einen aus der Fremde gekommenen  \n Freund. Nach Fro\u00dfmeuslers Art.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Herren Bruder wohlgemuth,", "tokens": ["Dem", "Her\u00b7ren", "Bru\u00b7der", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All Heil und Segen w\u00fcnschen thut,", "tokens": ["All", "Heil", "und", "Se\u00b7gen", "w\u00fcn\u00b7schen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nebst allen feinen Wohlergehen,", "tokens": ["Nebst", "al\u00b7len", "fei\u00b7nen", "Woh\u00b7ler\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Freund in rauhen ", "tokens": ["Ein", "Freund", "in", "rau\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dein Brief war drob recht wohl gemacht,", "tokens": ["Dein", "Brief", "war", "drob", "recht", "wohl", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich habe mich gar satt gelacht,", "tokens": ["Ich", "ha\u00b7be", "mich", "gar", "satt", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da du mit deiner lieben Hand", "tokens": ["Da", "du", "mit", "dei\u00b7ner", "lie\u00b7ben", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich vergleichest dem Elephant,", "tokens": ["Dich", "ver\u00b7glei\u00b7chest", "dem", "E\u00b7le\u00b7phant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Der aus Moscau gekommen ist", "tokens": ["Der", "aus", "Mo\u00b7scau", "ge\u00b7kom\u00b7men", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vor vierzehn Tagen dieser Frist,", "tokens": ["Vor", "vier\u00b7zehn", "Ta\u00b7gen", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Di\u00df freuet mich nun hertziglich,", "tokens": ["Di\u00df", "freu\u00b7et", "mich", "nun", "hert\u00b7zig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und w\u00fcnsche dir drob inniglich,", "tokens": ["Und", "w\u00fcn\u00b7sche", "dir", "drob", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Da\u00df es dir stets m\u00f6g wohlergahn,", "tokens": ["Da\u00df", "es", "dir", "stets", "m\u00f6g", "woh\u00b7ler\u00b7gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit deinem Weibel Lobesan.", "tokens": ["Mit", "dei\u00b7nem", "Wei\u00b7bel", "Lo\u00b7be\u00b7san", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Leb wohl in allem guten Heil,", "tokens": ["Leb", "wohl", "in", "al\u00b7lem", "gu\u00b7ten", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Di\u00df schreibet dir in aller Eil,", "tokens": ["Di\u00df", "schrei\u00b7bet", "dir", "in", "al\u00b7ler", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ohn alle Sorg und alle Grau\u00df,", "tokens": ["Ohn", "al\u00b7le", "Sorg", "und", "al\u00b7le", "Grau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dein treuer Freund von ", "tokens": ["Dein", "treu\u00b7er", "Freund", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}}}}}