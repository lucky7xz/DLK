{"dta.poem.15822": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr meinet wol, ich schwimm' in lauter Ueberflusse,", "tokens": ["Ihr", "mei\u00b7net", "wol", ",", "ich", "schwimm'", "in", "lau\u00b7ter", "Ue\u00b7ber\u00b7flus\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mir zu Theile sei geworden zuviel Mu\u00dfe.", "tokens": ["Und", "mir", "zu", "Thei\u00b7le", "sei", "ge\u00b7wor\u00b7den", "zu\u00b7viel", "Mu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VAFIN", "VAPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mir ist vom Ueberflu\u00df kein Tr\u00f6pfchen \u00fcberfl\u00fcssig,", "tokens": ["Mir", "ist", "vom", "Ue\u00b7berf\u00b7lu\u00df", "kein", "Tr\u00f6pf\u00b7chen", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn keine Stunde bin ich in der Mu\u00dfe m\u00fc\u00dfig.", "tokens": ["Denn", "kei\u00b7ne", "Stun\u00b7de", "bin", "ich", "in", "der", "Mu\u00b7\u00dfe", "m\u00fc\u00b7\u00dfig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN"