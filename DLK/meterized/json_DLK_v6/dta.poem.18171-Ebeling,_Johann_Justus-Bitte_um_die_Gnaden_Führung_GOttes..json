{"dta.poem.18171": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Bitte um die Gnaden F\u00fchrung  \n GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchre mich GOtt! heiliglich denn dein", "tokens": ["F\u00fch\u00b7re", "mich", "Gott", "!", "hei\u00b7lig\u00b7lich", "denn", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "ADJD", "KON", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Rath ist wunderbar,", "tokens": ["Rath", "ist", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und beh\u00fcte meine Seel unter Mesech vor", "tokens": ["Und", "be\u00b7h\u00fc\u00b7te", "mei\u00b7ne", "Seel", "un\u00b7ter", "Me\u00b7sech", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "APPR"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Gefahr.", "tokens": ["Ge\u00b7fahr", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "La\u00df mich deine Vorsehung weiser Sch\u00f6pfer, stets", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Vor\u00b7se\u00b7hung", "wei\u00b7ser", "Sch\u00f6p\u00b7fer", ",", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,", "ADV"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "verehren!", "tokens": ["ver\u00b7eh\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "St\u00e4rke mich durch deinen Geist wenn ich sie soll an-", "tokens": ["St\u00e4r\u00b7ke", "mich", "durch", "dei\u00b7nen", "Geist", "wenn", "ich", "sie", "soll", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "KOUS", "PPER", "PPER", "VMFIN", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "dern lehren.", "tokens": ["dern", "leh\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Kann ich deren Tieffe nicht mit dem bl\u00f6den Sinn", "tokens": ["Kann", "ich", "de\u00b7ren", "Tief\u00b7fe", "nicht", "mit", "dem", "bl\u00f6\u00b7den", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PDS", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "einsehn,", "tokens": ["ein\u00b7sehn", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "So la\u00df mich ins Heiligthum, hinter ihren Vorhang", "tokens": ["So", "la\u00df", "mich", "ins", "Hei\u00b7lig\u00b7thum", ",", "hin\u00b7ter", "ih\u00b7ren", "Vor\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "gehn,", "tokens": ["gehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Find ich da auch Dunkelheit; so la\u00df mich in seelgen", "tokens": ["Find", "ich", "da", "auch", "Dun\u00b7kel\u00b7heit", ";", "so", "la\u00df", "mich", "in", "seel\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "$.", "ADV", "VVIMP", "PPER", "APPR", "ADJA"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Lichte,", "tokens": ["Lich\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Herr! vor deinen Angesicht preisen deiner Macht", "tokens": ["Herr", "!", "vor", "dei\u00b7nen", "An\u00b7ge\u00b7sicht", "prei\u00b7sen", "dei\u00b7ner", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Gerichte.", "tokens": ["Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}