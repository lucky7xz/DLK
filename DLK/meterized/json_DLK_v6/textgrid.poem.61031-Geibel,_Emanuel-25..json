{"textgrid.poem.61031": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem in Rosen und in Bl\u00fcten", "tokens": ["Wem", "in", "Ro\u00b7sen", "und", "in", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich verliert des Lebens Pfad,", "tokens": ["Sich", "ver\u00b7liert", "des", "Le\u00b7bens", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag die eigne Seele h\u00fcten,", "tokens": ["Mag", "die", "eig\u00b7ne", "See\u00b7le", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn gewi\u00df, die Trauer naht.", "tokens": ["Denn", "ge\u00b7wi\u00df", ",", "die", "Trau\u00b7er", "naht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da ich alle Lust besessen,", "tokens": ["Da", "ich", "al\u00b7le", "Lust", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Liebesblick und Ku\u00df", "tokens": ["Un\u00b7ter", "Lie\u00b7bes\u00b7blick", "und", "Ku\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hatt' ich Sel'ger, ach, vergessen,", "tokens": ["Hatt'", "ich", "Sel'\u00b7ger", ",", "ach", ",", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "ITJ", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich wieder scheiden mu\u00df.", "tokens": ["Da\u00df", "ich", "wie\u00b7der", "schei\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie blickt mich nun die weite", "tokens": ["O", "wie", "blickt", "mich", "nun", "die", "wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welt so kalt und finster an!", "tokens": ["Welt", "so", "kalt", "und", "fins\u00b7ter", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War's doch nur an deiner Seite,", "tokens": ["Wa\u00b7r's", "doch", "nur", "an", "dei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich all mein Gl\u00fcck gewann.", "tokens": ["Da\u00df", "ich", "all", "mein", "Gl\u00fcck", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fr\u00fcher mocht' ich's schon ertragen,", "tokens": ["Fr\u00fc\u00b7her", "mocht'", "ich's", "schon", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Schweifen ohne Licht,", "tokens": ["Die\u00b7ses", "Schwei\u00b7fen", "oh\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mit Blindheit selbst geschlagen,", "tokens": ["Denn", "mit", "Blind\u00b7heit", "selbst", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannt' ich noch die Sonne nicht.", "tokens": ["Kannt'", "ich", "noch", "die", "Son\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber jetzt begreif' ich's nimmer,", "tokens": ["A\u00b7ber", "jetzt", "be\u00b7greif'", "ich's", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was noch bleiben kann f\u00fcr mich. -", "tokens": ["Was", "noch", "blei\u00b7ben", "kann", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch ein Leben ohne Schimmer", "tokens": ["Welch", "ein", "Le\u00b7ben", "oh\u00b7ne", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' ich leben ohne dich!", "tokens": ["Werd'", "ich", "le\u00b7ben", "oh\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}