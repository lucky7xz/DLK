{"textgrid.poem.41150": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Feuerruferin,", "tokens": ["Die\u00b7se", "Feu\u00b7er\u00b7ru\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ries'ger Schmetterling der Nacht,", "tokens": ["Ries'\u00b7ger", "Schmet\u00b7ter\u00b7ling", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flieget, wenn kein Mensch mehr wacht,", "tokens": ["Flie\u00b7get", ",", "wenn", "kein", "Mensch", "mehr", "wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchmal \u00fcber die D\u00e4cher hin.", "tokens": ["Manch\u00b7mal", "\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Dann sich r\u00f6tet rings die Luft,", "tokens": ["Dann", "sich", "r\u00f6\u00b7tet", "rings", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ob's brenne ungeheuer,", "tokens": ["Als", "ob's", "bren\u00b7ne", "un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie voll Verzweiflung ruft", "tokens": ["Und", "wie", "voll", "Ver\u00b7zwei\u00b7flung", "ruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der Luft es: \u00bbFeuer! Feuer!\u00ab", "tokens": ["Aus", "der", "Luft", "es", ":", "\u00bb", "Feu\u00b7er", "!", "Feu\u00b7er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "$.", "$(", "NN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wer es h\u00f6rt, ruft's nach und rennt", "tokens": ["Wer", "es", "h\u00f6rt", ",", "ruft's", "nach", "und", "rennt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Fort und ruft: \u00bbWo brennt ein Haus?\u00ab", "tokens": ["Fort", "und", "ruft", ":", "\u00bb", "Wo", "brennt", "ein", "Haus", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "$(", "PWAV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Doch die R\u00f6te losch schon aus,", "tokens": ["Doch", "die", "R\u00f6\u00b7te", "losch", "schon", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und ringsum es nirgends brennt.", "tokens": ["Und", "ring\u00b7sum", "es", "nir\u00b7gends", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Dann nach sieben Tagen sieht", "tokens": ["Dann", "nach", "sie\u00b7ben", "Ta\u00b7gen", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Klar der W\u00e4chter auf dem Turm", "tokens": ["Klar", "der", "W\u00e4ch\u00b7ter", "auf", "dem", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ein furchtbares Feuer, zieht", "tokens": ["Ein", "furcht\u00b7ba\u00b7res", "Feu\u00b7er", ",", "zieht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Alle Glocken an zum Sturm.", "tokens": ["Al\u00b7le", "Glo\u00b7cken", "an", "zum", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Glocken t\u00f6nen auch vom Land,", "tokens": ["Glo\u00b7cken", "t\u00f6\u00b7nen", "auch", "vom", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Feuerspritzen rasseln her,", "tokens": ["Feu\u00b7er\u00b7sprit\u00b7zen", "ras\u00b7seln", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Doch der Wind weht allzusehr,", "tokens": ["Doch", "der", "Wind", "weht", "all\u00b7zu\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Und zehn H\u00e4user fri\u00dft der Brand.", "tokens": ["Und", "zehn", "H\u00e4u\u00b7ser", "fri\u00dft", "der", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wer die Feuerruferin", "tokens": ["Wer", "die", "Feu\u00b7er\u00b7ru\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Einst im Erdenleben war,", "tokens": ["Einst", "im", "Er\u00b7den\u00b7le\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Das ist jedem Landmann klar,", "tokens": ["Das", "ist", "je\u00b7dem", "Land\u00b7mann", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und kein ", "tokens": ["Und", "kein"], "token_info": ["word", "word"], "pos": ["KON", "PIAT"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Ha! sie war ein b\u00f6ses Weib,", "tokens": ["Ha", "!", "sie", "war", "ein", "b\u00f6\u00b7ses", "Weib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Das erdrosselt ihren Mann,", "tokens": ["Das", "er\u00b7dros\u00b7selt", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Zu verbergen seinen Leib,", "tokens": ["Zu", "ver\u00b7ber\u00b7gen", "sei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Z\u00fcndete das Haus sie an.", "tokens": ["Z\u00fcn\u00b7de\u00b7te", "das", "Haus", "sie", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Zornig wehte dann der Wind,", "tokens": ["Zor\u00b7nig", "weh\u00b7te", "dann", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Immer mehrte sich die Glut,", "tokens": ["Im\u00b7mer", "mehr\u00b7te", "sich", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Zehen H\u00e4user fra\u00df geschwind", "tokens": ["Ze\u00b7hen", "H\u00e4u\u00b7ser", "fra\u00df", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Und sie mit des Feuers Wut.", "tokens": ["Und", "sie", "mit", "des", "Feu\u00b7ers", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Sieben Tag' doch, eh' ein Brand", "tokens": ["Sie\u00b7ben", "Tag'", "doch", ",", "eh'", "ein", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Ruft zu Hilfe Stadt und Land,", "tokens": ["Ruft", "zu", "Hil\u00b7fe", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Packt zu ihrer Bu\u00dfe dann", "tokens": ["Packt", "zu", "ih\u00b7rer", "Bu\u00b7\u00dfe", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Pl\u00f6tzlich sie ein m\u00e4cht'ger Wind,", "tokens": ["Pl\u00f6tz\u00b7lich", "sie", "ein", "m\u00e4cht'\u00b7ger", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Wirbelt mit ihr auf geschwind,", "tokens": ["Wir\u00b7belt", "mit", "ihr", "auf", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Da\u00df den Brand sie sage an.", "tokens": ["Da\u00df", "den", "Brand", "sie", "sa\u00b7ge", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.40": {"text": "Und zerflie\u00dft in rauch'ge Luft.", "tokens": ["Und", "zer\u00b7flie\u00dft", "in", "rauch'\u00b7ge", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}