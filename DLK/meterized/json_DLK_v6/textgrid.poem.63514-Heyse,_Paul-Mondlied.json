{"textgrid.poem.63514": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Mondlied", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandle still den Waldespfad,", "tokens": ["Ich", "wand\u00b7le", "still", "den", "Wal\u00b7des\u00b7pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es dunkelt die Nacht herein.", "tokens": ["Es", "dun\u00b7kelt", "die", "Nacht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Grunde rauscht ein M\u00fchlenrad,", "tokens": ["Im", "Grun\u00b7de", "rauscht", "ein", "M\u00fch\u00b7len\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Grillen Lied f\u00e4llt ein.", "tokens": ["Der", "Gril\u00b7len", "Lied", "f\u00e4llt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie liegt so tief, wie liegt so weit", "tokens": ["Wie", "liegt", "so", "tief", ",", "wie", "liegt", "so", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt im Mondesduft!", "tokens": ["Die", "Welt", "im", "Mon\u00b7des\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Stimme der Waldeinsamkeit", "tokens": ["Die", "Stim\u00b7me", "der", "Wal\u00b7de\u00b7in\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Windess\u00e4useln ruft:", "tokens": ["Im", "Win\u00b7des\u00b7s\u00e4u\u00b7seln", "ruft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wirf ab dein bang ertr\u00e4umtes Weh,", "tokens": ["Wirf", "ab", "dein", "bang", "er\u00b7tr\u00e4um\u00b7tes", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirf ab die falsche Lust!", "tokens": ["Wirf", "ab", "die", "fal\u00b7sche", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schmelzen hin wie M\u00e4rzenschnee,", "tokens": ["Sie", "schmel\u00b7zen", "hin", "wie", "M\u00e4r\u00b7zen\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00f6de bleibt die Brust.", "tokens": ["Und", "\u00f6\u00b7de", "bleibt", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Blick auf, wo Stern an Stern entbrennt,", "tokens": ["Blick", "auf", ",", "wo", "Stern", "an", "Stern", "ent\u00b7brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprich dein Herz zur Ruh;", "tokens": ["Und", "sprich", "dein", "Herz", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ew'ger als das Firmament,", "tokens": ["Denn", "ew'\u00b7ger", "als", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kleines Licht, bist du!", "tokens": ["Du", "klei\u00b7nes", "Licht", ",", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}