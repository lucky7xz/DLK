{"textgrid.poem.39271": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[im Fluggewimmel meiner Traumgedanken]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fluggewimmel meiner Traumgedanken,", "tokens": ["Im", "Flug\u00b7ge\u00b7wim\u00b7mel", "mei\u00b7ner", "Traum\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich geforscht im Ruhm von alten Tagen,", "tokens": ["Wenn", "ich", "ge\u00b7forscht", "im", "Ruhm", "von", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hab' ich gesehnt mich, Helm und Schwert zu tragen,", "tokens": ["Hab'", "ich", "ge\u00b7sehnt", "mich", ",", "Helm", "und", "Schwert", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "PPER", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Ritter frei im Schlachtenwogenschwanken.", "tokens": ["Ein", "Rit\u00b7ter", "frei", "im", "Schlach\u00b7ten\u00b7wo\u00b7gen\u00b7schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus dem Geschlecht, dem kraftlos feigen, kranken,", "tokens": ["Aus", "dem", "Ge\u00b7schlecht", ",", "dem", "kraft\u00b7los", "fei\u00b7gen", ",", "kran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00f6cht' ich mich heben stolz mit keckem Wagen,", "tokens": ["M\u00f6cht'", "ich", "mich", "he\u00b7ben", "stolz", "mit", "ke\u00b7ckem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann m\u00f6cht' ich hell die Ritterharfe schlagen,", "tokens": ["Dann", "m\u00f6cht'", "ich", "hell", "die", "Rit\u00b7ter\u00b7har\u00b7fe", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn nicht die Fesseln mehr den Geist umranken.", "tokens": ["Wenn", "nicht", "die", "Fes\u00b7seln", "mehr", "den", "Geist", "um\u00b7ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Allt\u00e4glichkeit, du Pest der freien Klarheit,", "tokens": ["All\u00b7t\u00e4g\u00b7lich\u00b7keit", ",", "du", "Pest", "der", "frei\u00b7en", "Klar\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die du erschlaffst die Nerven des Gesanges,", "tokens": ["Die", "du", "er\u00b7schlaffst", "die", "Ner\u00b7ven", "des", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die du erl\u00e4hmst die Geisteskraft, die hohe,", "tokens": ["Die", "du", "er\u00b7l\u00e4hmst", "die", "Geis\u00b7tes\u00b7kraft", ",", "die", "ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dich brechen m\u00f6cht' ich und zur Sternenwahrheit", "tokens": ["Dich", "bre\u00b7chen", "m\u00f6cht'", "ich", "und", "zur", "Ster\u00b7nen\u00b7wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufschwingen mich, im Braus des Sph\u00e4renklanges,", "tokens": ["Auf\u00b7schwin\u00b7gen", "mich", ",", "im", "Braus", "des", "Sph\u00e4\u00b7ren\u00b7klan\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn in Gemeinschaft stirbt des Herzens Lohe.", "tokens": ["Denn", "in", "Ge\u00b7mein\u00b7schaft", "stirbt", "des", "Her\u00b7zens", "Lo\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}