{"dta.poem.8905": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.85", "nl:0.14"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ich halte nie der Seele Muth in Schranken:", "tokens": ["Ich", "hal\u00b7te", "nie", "der", "See\u00b7le", "Muth", "in", "Schran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was w\u00e4ren wir, mit denen Alle zanken,", "tokens": ["Was", "w\u00e4\u00b7ren", "wir", ",", "mit", "de\u00b7nen", "Al\u00b7le", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "APPR", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wir uns selbst das bischen Ruhm entz\u00f6gen?", "tokens": ["Wenn", "wir", "uns", "selbst", "das", "bi\u00b7schen", "Ruhm", "ent\u00b7z\u00f6\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll bergen ich mein innerstes Verm\u00f6gen,", "tokens": ["Soll", "ber\u00b7gen", "ich", "mein", "in\u00b7ners\u00b7tes", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Was ich empfinde zu bekennen schwanken?", "tokens": ["Was", "ich", "emp\u00b7fin\u00b7de", "zu", "be\u00b7ken\u00b7nen", "schwan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sch\u00e4mte mich der eigenen Gedanken,", "tokens": ["Ich", "sch\u00e4m\u00b7te", "mich", "der", "ei\u00b7ge\u00b7nen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie, wie Schwalben, an der Erde fl\u00f6gen.", "tokens": ["Wenn", "sie", ",", "wie", "Schwal\u00b7ben", ",", "an", "der", "Er\u00b7de", "fl\u00f6\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hienieden lohnt's der M\u00fche nicht, zu zagen,", "tokens": ["Hien\u00b7ie\u00b7den", "lohnt's", "der", "M\u00fc\u00b7he", "nicht", ",", "zu", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "PTKNEG", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wahr und frey zu sprechen kleidet Jeden,", "tokens": ["Und", "wahr", "und", "frey", "zu", "spre\u00b7chen", "klei\u00b7det", "Je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da bald wir Alle ruhn in Sarkophagen.", "tokens": ["Da", "bald", "wir", "Al\u00b7le", "ruhn", "in", "Sar\u00b7ko\u00b7pha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es werden Sp\u00e4t're meinen Geist in Eden", "tokens": ["Es", "wer\u00b7den", "Sp\u00e4t'\u00b7re", "mei\u00b7nen", "Geist", "in", "E\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beschw\u00f6ren und entschuldigen und sagen:", "tokens": ["Be\u00b7schw\u00f6\u00b7ren", "und", "ent\u00b7schul\u00b7di\u00b7gen", "und", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er dachte gro\u00df, wie konnt' er kleinlich reden?", "tokens": ["Er", "dach\u00b7te", "gro\u00df", ",", "wie", "konnt'", "er", "klein\u00b7lich", "re\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}