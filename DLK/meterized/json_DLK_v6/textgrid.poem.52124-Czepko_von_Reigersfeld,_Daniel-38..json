{"textgrid.poem.52124": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schwere Steuer macht, Ihr werther Ritters Mann!", "tokens": ["Die", "schwe\u00b7re", "Steu\u00b7er", "macht", ",", "Ihr", "wert\u00b7her", "Rit\u00b7ters", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Eurer Hochzeit ich kein Carmen steuern kan:", "tokens": ["Da\u00df", "Eu\u00b7rer", "Hoch\u00b7zeit", "ich", "kein", "Car\u00b7men", "steu\u00b7ern", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Musen wollen mir ja nicht zu Steuer kommen,", "tokens": ["Die", "Mu\u00b7sen", "wol\u00b7len", "mir", "ja", "nicht", "zu", "Steu\u00b7er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn in der Steuer wird ein Reim nicht angenommen.", "tokens": ["Denn", "in", "der", "Steu\u00b7er", "wird", "ein", "Reim", "nicht", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und Eure Braut, der selbst die Venus Steuer giebt,", "tokens": ["Und", "Eu\u00b7re", "Braut", ",", "der", "selbst", "die", "Ve\u00b7nus", "Steu\u00b7er", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was an Pracht und Zier der Himmel sonsten liebt:", "tokens": ["Und", "was", "an", "Pracht", "und", "Zier", "der", "Him\u00b7mel", "sons\u00b7ten", "liebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "KON", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdient, da\u00df diesen Tag ber\u00fchmte Seelen feyern,", "tokens": ["Ver\u00b7di\u00b7ent", ",", "da\u00df", "die\u00b7sen", "Tag", "be\u00b7r\u00fchm\u00b7te", "See\u00b7len", "fey\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PDAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ihrem gro\u00dfen Ruhm erw\u00fcnschte Sachen steuern.", "tokens": ["Und", "ih\u00b7rem", "gro\u00b7\u00dfen", "Ruhm", "er\u00b7w\u00fcnschte", "Sa\u00b7chen", "steu\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Jedoch wo kommen, Herr, mehr solche Steuern ein,", "tokens": ["Je\u00b7doch", "wo", "kom\u00b7men", ",", "Herr", ",", "mehr", "sol\u00b7che", "Steu\u00b7ern", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VVINF", "$,", "NN", "$,", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(die andern mag ich nicht) an Hoheit, Witz und Schein,", "tokens": ["(", "die", "an\u00b7dern", "mag", "ich", "nicht", ")", "an", "Ho\u00b7heit", ",", "Witz", "und", "Schein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VMFIN", "PPER", "PTKNEG", "$(", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als selbst in eurer Brust die Steuer abzuf\u00fchren,", "tokens": ["Als", "selbst", "in", "eu\u00b7rer", "Brust", "die", "Steu\u00b7er", "ab\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum k\u00f6nt ihr blo\u00df die Welt mit ihrem Lobe zieren.", "tokens": ["Drum", "k\u00f6nt", "ihr", "blo\u00df", "die", "Welt", "mit", "ih\u00b7rem", "Lo\u00b7be", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und weil ihr euren Stand ver\u00e4ndert, nehmts in acht,", "tokens": ["Und", "weil", "ihr", "eu\u00b7ren", "Stand", "ver\u00b7\u00e4n\u00b7dert", ",", "nehmts", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr Nimptsch, da\u00df nicht das Ammt auch \u00e4ndre diese Nacht:", "tokens": ["Herr", "Nimptsch", ",", "da\u00df", "nicht", "das", "Ammt", "auch", "\u00e4nd\u00b7re", "die\u00b7se", "Nacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KOUS", "PTKNEG", "ART", "NN", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn [es] ist mir recht (es sind der Liebe s\u00fc\u00dfe Gaben)", "tokens": ["Denn", "es", "ist", "mir", "recht", "(", "es", "sind", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Ga\u00b7ben", ")"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "$(", "VAFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So wird die sch\u00f6ne Braut die Einnahm heute haben.", "tokens": ["So", "wird", "die", "sch\u00f6\u00b7ne", "Braut", "die", "Ein\u00b7nahm", "heu\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}