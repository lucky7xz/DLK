{"textgrid.poem.38705": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Weinbl\u00fcthe", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts auf der Erde kann feiner, \u00e4therischer, lieblicher duften,", "tokens": ["Nichts", "auf", "der", "Er\u00b7de", "kann", "fei\u00b7ner", ",", "\u00e4t\u00b7he\u00b7ri\u00b7scher", ",", "lieb\u00b7li\u00b7cher", "duf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VMFIN", "ADJD", "$,", "ADJA", "$,", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Bl\u00fcthe des Weinstocks, als du, die noch kein Dichter besang.", "tokens": ["Bl\u00fc\u00b7the", "des", "Wein\u00b7stocks", ",", "als", "du", ",", "die", "noch", "kein", "Dich\u00b7ter", "be\u00b7sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wahrlich! des Holden, das noch durch Lieder kein Sterblicher ehrte,", "tokens": ["Wahr\u00b7lich", "!", "des", "Hol\u00b7den", ",", "das", "noch", "durch", "Lie\u00b7der", "kein", "Sterb\u00b7li\u00b7cher", "ehr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ist, wie des N\u00fctzlichen viel, das noch kein Sterblicher that.", "tokens": ["Ist", ",", "wie", "des", "N\u00fctz\u00b7li\u00b7chen", "viel", ",", "das", "noch", "kein", "Sterb\u00b7li\u00b7cher", "that", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}