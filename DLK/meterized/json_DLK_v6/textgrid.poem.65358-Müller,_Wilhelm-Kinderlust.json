{"textgrid.poem.65358": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Kinderlust", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun feget aus den alten Staub", "tokens": ["Nun", "fe\u00b7get", "aus", "den", "al\u00b7ten", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht die Laube blank!", "tokens": ["Und", "macht", "die", "Lau\u00b7be", "blank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft ja kein schwarzes Winterlaub", "tokens": ["La\u00dft", "ja", "kein", "schwar\u00b7zes", "Win\u00b7ter\u00b7laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir liegen auf der Bank!", "tokens": ["Mir", "lie\u00b7gen", "auf", "der", "Bank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die erste wei\u00dfe Bl\u00fcthe flog", "tokens": ["Die", "ers\u00b7te", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7the", "flog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir heut' in's Angesicht.", "tokens": ["Mir", "heut'", "in's", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willkommen, Lenz! Ich lebe noch", "tokens": ["Will\u00b7kom\u00b7men", ",", "Lenz", "!", "Ich", "le\u00b7be", "noch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00df von Leide nicht.", "tokens": ["Und", "wei\u00df", "von", "Lei\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und schaue hell, wie du, hinein", "tokens": ["Und", "schau\u00b7e", "hell", ",", "wie", "du", ",", "hin\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Gottes sch\u00f6ne Welt,", "tokens": ["In", "Got\u00b7tes", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6cht' ein kleiner Bube sein", "tokens": ["Und", "m\u00f6cht'", "ein", "klei\u00b7ner", "Bu\u00b7be", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kollern durch das Feld.", "tokens": ["Und", "kol\u00b7lern", "durch", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O seht, da pl\u00e4tschern schon am See", "tokens": ["O", "seht", ",", "da", "pl\u00e4t\u00b7schern", "schon", "am", "See"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieben Kindelein,", "tokens": ["Die", "lie\u00b7ben", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ziehn die Hemdchen in die H\u00f6h',", "tokens": ["Und", "ziehn", "die", "Hemd\u00b7chen", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollen gern hinein.", "tokens": ["Und", "wol\u00b7len", "gern", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie lockt der warme Sonnenschein,", "tokens": ["Wie", "lockt", "der", "war\u00b7me", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der auf dem Spiegel ruht!", "tokens": ["Der", "auf", "dem", "Spie\u00b7gel", "ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist kein Fu\u00df zu weich, zu klein,", "tokens": ["Da", "ist", "kein", "Fu\u00df", "zu", "weich", ",", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er probt, wie 's Wasser thut.", "tokens": ["Er", "probt", ",", "wie", "'s", "Was\u00b7ser", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich sitz' und seh' dem Spiele zu,", "tokens": ["Ich", "sitz'", "und", "seh'", "dem", "Spie\u00b7le", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und spiel' im Herzen auch.", "tokens": ["Und", "spiel'", "im", "Her\u00b7zen", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du lieber Lenz, ein Kind bist du,", "tokens": ["Du", "lie\u00b7ber", "Lenz", ",", "ein", "Kind", "bist", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcbest Kinderbrauch.", "tokens": ["Und", "\u00fc\u00b7best", "Kin\u00b7der\u00b7brauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie viel du hast, du wei\u00dft es kaum,", "tokens": ["Wie", "viel", "du", "hast", ",", "du", "wei\u00dft", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00fcttest Alles aus.", "tokens": ["Und", "sch\u00fct\u00b7test", "Al\u00b7les", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nehmt, Kinder, nehmt! Es ist kein Traum!", "tokens": ["Nehmt", ",", "Kin\u00b7der", ",", "nehmt", "!", "Es", "ist", "kein", "Traum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kommt aus Gottes Haus.", "tokens": ["Es", "kommt", "aus", "Got\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wenn du nun ganz fertig bist,", "tokens": ["Und", "wenn", "du", "nun", "ganz", "fer\u00b7tig", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast keine Blume mehr,", "tokens": ["Hast", "kei\u00b7ne", "Blu\u00b7me", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann gehst du wieder ohne Frist,", "tokens": ["Dann", "gehst", "du", "wie\u00b7der", "oh\u00b7ne", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Abschied wird dir schwer.", "tokens": ["Kein", "Ab\u00b7schied", "wird", "dir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und rufst dem Bruder Sommer zu:", "tokens": ["Und", "rufst", "dem", "Bru\u00b7der", "Som\u00b7mer", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringst du die Fr\u00fcchte her?", "tokens": ["Bringst", "du", "die", "Fr\u00fcch\u00b7te", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich versprach, das halte du!", "tokens": ["Was", "ich", "ver\u00b7sprach", ",", "das", "hal\u00b7te", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ei, ei, dein Korb ist schwer!", "tokens": ["Ei", ",", "ei", ",", "dein", "Korb", "ist", "schwer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}