{"dta.poem.17765": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "10  Der Abgeleibte Seelige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "O GOtt wiewol ist mir! mein Leiden ist verschwunden", "tokens": ["O", "Gott", "wie\u00b7wol", "ist", "mir", "!", "mein", "Lei\u00b7den", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADV", "VAFIN", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schmertzen sind dahin/ die Tr\u00fcbsal hat ein End\u2019", "tokens": ["Die", "Schmert\u00b7zen", "sind", "da\u00b7hin", "/", "die", "Tr\u00fcb\u00b7sal", "hat", "ein", "End'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PAV", "$(", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alles Hertzeleid ist von mir abgewend:", "tokens": ["Und", "al\u00b7les", "Hert\u00b7ze\u00b7leid", "ist", "von", "mir", "ab\u00b7ge\u00b7wend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin nu K\u00e4rkerlo\u00df und seeliglich entbunden:", "tokens": ["Ich", "bin", "nu", "K\u00e4r\u00b7ker\u00b7lo\u00df", "und", "see\u00b7lig\u00b7lich", "ent\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe Freudenreich gesiegt und \u00fcberwunden:", "tokens": ["Ich", "ha\u00b7be", "Freu\u00b7den\u00b7reich", "ge\u00b7siegt", "und", "\u00fc\u00b7berw\u00b7un\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Feind ber\u00fchrt mich mehr/ und was man b\u00f6se nen\u0304t:", "tokens": ["Kein", "Feind", "be\u00b7r\u00fchrt", "mich", "mehr", "/", "und", "was", "man", "b\u00f6\u00b7se", "nen\u0304t", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$(", "KON", "PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird mit keinem Weh mein fr\u00f6lich seyn getrent.", "tokens": ["Es", "wird", "mit", "kei\u00b7nem", "Weh", "mein", "fr\u00f6\u00b7lich", "seyn", "ge\u00b7trent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "PPOSAT", "ADJD", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich habe wahre Ruh/ und wahre Lust gefunden.", "tokens": ["Ich", "ha\u00b7be", "wah\u00b7re", "Ruh", "/", "und", "wah\u00b7re", "Lust", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Himmel lacht mich an/ die Engel nehmen mich", "tokens": ["Der", "Him\u00b7mel", "lacht", "mich", "an", "/", "die", "En\u00b7gel", "neh\u00b7men", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sambt allen Heiligen mit Freuden unter sich.", "tokens": ["Sambt", "al\u00b7len", "Hei\u00b7li\u00b7gen", "mit", "Freu\u00b7den", "un\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich bin so voller Trosts da\u00df ich fast \u00fcberfliesse:", "tokens": ["Ich", "bin", "so", "vol\u00b7ler", "Trosts", "da\u00df", "ich", "fast", "\u00fc\u00b7berf\u00b7lies\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich habe was ich wil/ und wil was ich geniesse:", "tokens": ["Ich", "ha\u00b7be", "was", "ich", "wil", "/", "und", "wil", "was", "ich", "ge\u00b7nies\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PWS", "PPER", "VMFIN", "$(", "KON", "VMFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich habe nu genug: man f\u00fchrt mich wie ich bin", "tokens": ["Ich", "ha\u00b7be", "nu", "ge\u00b7nug", ":", "man", "f\u00fchrt", "mich", "wie", "ich", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$.", "PIS", "VVFIN", "PPER", "KOKOM", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu meinem Br\u00e4utigam und s\u00fcssen JEsu hin.", "tokens": ["Zu", "mei\u00b7nem", "Br\u00e4u\u00b7ti\u00b7gam", "und", "s\u00fcs\u00b7sen", "Je\u00b7su", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}