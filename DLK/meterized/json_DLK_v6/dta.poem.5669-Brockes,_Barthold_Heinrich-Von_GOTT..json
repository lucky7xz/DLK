{"dta.poem.5669": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er ist. Der's Wesen gibt, mu\u00df selbst ein Wesen haben.", "tokens": ["Er", "ist", ".", "Der's", "We\u00b7sen", "gibt", ",", "mu\u00df", "selbst", "ein", "We\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ew'ge Seyn, so unser Seyn gemacht,", "tokens": ["Das", "Ew'\u00b7ge", "Seyn", ",", "so", "un\u00b7ser", "Seyn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Derjenige, wodurch ich bin hervor gebracht", "tokens": ["Der\u00b7je\u00b7ni\u00b7ge", ",", "wo\u00b7durch", "ich", "bin", "her\u00b7vor", "ge\u00b7bracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PWAV", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dessen Gaben,", "tokens": ["Und", "des\u00b7sen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mein Seyn und Wesen blo\u00df allein;", "tokens": ["Mein", "Seyn", "und", "We\u00b7sen", "blo\u00df", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df selber ja nothwendig seyn.", "tokens": ["Mu\u00df", "sel\u00b7ber", "ja", "noth\u00b7wen\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er ist ein Geist, ER ist der H\u00f6chste Geist.", "tokens": ["Er", "ist", "ein", "Geist", ",", "Er", "ist", "der", "H\u00f6chs\u00b7te", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich dencke. Die\u00df geschicht durch JHR, und die\u00df ist wahr,", "tokens": ["Ich", "den\u00b7cke", ".", "Die\u00df", "ge\u00b7schicht", "durch", "JhR", ",", "und", "die\u00df", "ist", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VVPP", "APPR", "NE", "$,", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da ER sich unserm Geist so offenbar", "tokens": ["Da", "Er", "sich", "un\u00b7serm", "Geist", "so", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So augenscheinlich weist.", "tokens": ["So", "au\u00b7gen\u00b7schein\u00b7lich", "weist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Von Ewigkeit", "tokens": ["Von", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Mu\u00df was gewesen seyn. W\u00e4r eine Zeit,", "tokens": ["Mu\u00df", "was", "ge\u00b7we\u00b7sen", "seyn", ".", "W\u00e4r", "ei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VAPP", "VAINF", "$.", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In welcher nichts war, je vorhanden,", "tokens": ["In", "wel\u00b7cher", "nichts", "war", ",", "je", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}