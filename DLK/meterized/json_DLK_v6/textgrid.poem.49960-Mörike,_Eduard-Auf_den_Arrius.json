{"textgrid.poem.49960": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Arrius", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Handelte; ", "tokens": ["Han\u00b7del\u00b7te", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Und er glaubte dir sch\u00f6n ganz \u00fcber die Ma\u00dfen zu reden,", "tokens": ["Und", "er", "glaub\u00b7te", "dir", "sch\u00f6n", "ganz", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn er sein ", "tokens": ["Wenn", "er", "sein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Sicherlich hatten Mama, Oheim, Gro\u00dfmutter und \u2013 vater", "tokens": ["Si\u00b7cher\u00b7lich", "hat\u00b7ten", "Ma\u00b7ma", ",", "O\u00b7heim", ",", "Gro\u00df\u00b7mut\u00b7ter", "und", "\u2013", "va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,", "NN", "KON", "$(", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "(diese von Mutterseit) eben die Sprache beliebt.", "tokens": ["(", "die\u00b7se", "von", "Mut\u00b7ter\u00b7seit", ")", "e\u00b7ben", "die", "Spra\u00b7che", "be\u00b7liebt", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "APPR", "NN", "$(", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.6": {"text": "Wie er nach Syrien ging, da w\u00fcnschten wir unseren Ohren", "tokens": ["Wie", "er", "nach", "Sy\u00b7ri\u00b7en", "ging", ",", "da", "w\u00fcnschten", "wir", "un\u00b7se\u00b7ren", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NE", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "---+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Gl\u00fcck, und nat\u00fcrlich, wie sonst, h\u00f6rte man jegliches Wort.", "tokens": ["Gl\u00fcck", ",", "und", "na\u00b7t\u00fcr\u00b7lich", ",", "wie", "sonst", ",", "h\u00f6r\u00b7te", "man", "jeg\u00b7li\u00b7ches", "Wort", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "$,", "PWAV", "ADV", "$,", "VVFIN", "PIS", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Ja wir glaubten uns los und ledig der Plage f\u00fcr immer,", "tokens": ["Ja", "wir", "glaub\u00b7ten", "uns", "los", "und", "le\u00b7dig", "der", "Pla\u00b7ge", "f\u00fcr", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "PTKVZ", "KON", "ADJD", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Als man, o Schreckenspost! pl\u00f6tzlich die Kunde vernahm:", "tokens": ["Als", "man", ",", "o", "Schre\u00b7ckens\u00b7post", "!", "pl\u00f6tz\u00b7lich", "die", "Kun\u00b7de", "ver\u00b7nahm", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "FM", "NN", "$.", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Seit Herr Arrius \u00fcber das Meer ging, gibt es in aller", "tokens": ["Seit", "Herr", "Ar\u00b7rius", "\u00fc\u00b7ber", "das", "Meer", "ging", ",", "gibt", "es", "in", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Welt kein Jonisches mehr, aber ein ", "tokens": ["Welt", "kein", "Jo\u00b7ni\u00b7sches", "mehr", ",", "a\u00b7ber", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PIAT", "NN", "ADV", "$,", "ADV", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}