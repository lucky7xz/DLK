{"textgrid.poem.35518": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "6. Purgatorio", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh ein, o Schmerz,", "tokens": ["Zieh", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und weihe dies Herz,", "tokens": ["Und", "wei\u00b7he", "dies", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das lange sich deiner gewehrt hat!", "tokens": ["Das", "lan\u00b7ge", "sich", "dei\u00b7ner", "ge\u00b7wehrt", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "PPOSAT", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und in flammendem Groll", "tokens": ["Und", "in", "flam\u00b7men\u00b7dem", "Groll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Gegen des Lebens Zoll,", "tokens": ["Ge\u00b7gen", "des", "Le\u00b7bens", "Zoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Gegen deine Macht sich emp\u00f6rt hat!", "tokens": ["Ge\u00b7gen", "dei\u00b7ne", "Macht", "sich", "em\u00b7p\u00f6rt", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zieh ein, o Schmerz,", "tokens": ["Zieh", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und l\u00e4utre dies Herz \u2013", "tokens": ["Und", "l\u00e4ut\u00b7re", "dies", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich geb' es besiegt dir zu eigen!", "tokens": ["Ich", "geb'", "es", "be\u00b7siegt", "dir", "zu", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und erbarmungslos", "tokens": ["Und", "er\u00b7bar\u00b7mungs\u00b7los"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Entl\u00f6s deinem Scho\u00df", "tokens": ["Ent\u00b7l\u00f6s", "dei\u00b7nem", "Scho\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "----+", "measure": "unknown.measure.single"}, "line.6": {"text": "Der Qualen nachtlockigen Reigen!", "tokens": ["Der", "Qua\u00b7len", "nacht\u00b7lo\u00b7cki\u00b7gen", "Rei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zieh ein, o Schmerz,", "tokens": ["Zieh", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und heil'ge dies Herz \u2013", "tokens": ["Und", "heil'\u00b7ge", "dies", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Furch deine Flammenspuren!", "tokens": ["Furch", "dei\u00b7ne", "Flam\u00b7men\u00b7spu\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was morsch ist, zerbrich,", "tokens": ["Was", "morsch", "ist", ",", "zer\u00b7brich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bis das Gemeine entwich,", "tokens": ["Bis", "das", "Ge\u00b7mei\u00b7ne", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Und die Flitter von dannen fuhren!", "tokens": ["Und", "die", "Flit\u00b7ter", "von", "dan\u00b7nen", "fuh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Zieh ein, o Schmerz,", "tokens": ["Zieh", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und pflanze ins Herz", "tokens": ["Und", "pflan\u00b7ze", "ins", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was gesucht ich so lang", "tokens": ["Was", "ge\u00b7sucht", "ich", "so", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In gl\u00fchendem Drang,", "tokens": ["In", "gl\u00fc\u00b7hen\u00b7dem", "Drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Zieh' ein, o Schmerz,", "tokens": ["Zieh'", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ents\u00fcnd'ge dies Herz \u2013", "tokens": ["Ent\u00b7s\u00fcn\u00b7d'\u00b7ge", "dies", "Herz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Ich geb' es besiegt dir zu eigen! \u2013", "tokens": ["Ich", "geb'", "es", "be\u00b7siegt", "dir", "zu", "ei\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bis in flammender Pracht", "tokens": ["Bis", "in", "flam\u00b7men\u00b7der", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Aus Schl\u00fcnden der Nacht", "tokens": ["Aus", "Schl\u00fcn\u00b7den", "der", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der Erl\u00f6sung Sonnen mir steigen! ...", "tokens": ["Der", "Er\u00b7l\u00f6\u00b7sung", "Son\u00b7nen", "mir", "stei\u00b7gen", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVINF", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}