{"dta.poem.6664": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt es nicht offtermals, da\u00df, mitten in der Stille,", "tokens": ["Kommt", "es", "nicht", "off\u00b7ter\u00b7mals", ",", "da\u00df", ",", "mit\u00b7ten", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man f\u00fchlet im verstopfften Kopff", "tokens": ["Man", "f\u00fch\u00b7let", "im", "ver\u00b7stopff\u00b7ten", "Kopff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Klingen, T\u00f6nen, ein Geklopff,", "tokens": ["Ein", "Klin\u00b7gen", ",", "T\u00f6\u00b7nen", ",", "ein", "Ge\u00b7klopff", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ja von \u00e4usserlichen Dingen,", "tokens": ["Die", "ja", "von", "\u00e4us\u00b7ser\u00b7li\u00b7chen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Unstreitig nicht entstehen noch entspringen.", "tokens": ["Un\u00b7strei\u00b7tig", "nicht", "ent\u00b7ste\u00b7hen", "noch", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es lassen uns ein Heulen, ein Gebr\u00fclle,", "tokens": ["Es", "las\u00b7sen", "uns", "ein", "Heu\u00b7len", ",", "ein", "Ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch ein Geknirsch\u2019 gewisse Anf\u00e4ll\u2019 h\u00f6ren,", "tokens": ["Durch", "ein", "Ge\u00b7knir\u00b7sch'", "ge\u00b7wis\u00b7se", "An\u00b7f\u00e4ll'", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die uns mit Schmertz und Pein", "tokens": ["Die", "uns", "mit", "Schmertz", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zum \u00f6fftern j\u00e4mmerlich beschweren,", "tokens": ["Zum", "\u00f6ff\u00b7tern", "j\u00e4m\u00b7mer\u00b7lich", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und die doch blo\u00df in uns erzeuget seyn.", "tokens": ["Und", "die", "doch", "blo\u00df", "in", "uns", "er\u00b7zeu\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "APPR", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}