{"textgrid.poem.65780": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "54", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, wie ist Sch\u00f6nheit zwiefach sch\u00f6n und hehr,", "tokens": ["O", ",", "wie", "ist", "Sch\u00f6n\u00b7heit", "zwie\u00b7fach", "sch\u00f6n", "und", "hehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "NN", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie der Wahrheit goldner Schmuck erhebt!", "tokens": ["Wenn", "sie", "der", "Wahr\u00b7heit", "gold\u00b7ner", "Schmuck", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Ros' ist lieblich, aber lieblicher", "tokens": ["Die", "Ros'", "ist", "lieb\u00b7lich", ",", "a\u00b7ber", "lieb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Macht sie der Wohlgeruch, der in ihr lebt.", "tokens": ["Macht", "sie", "der", "Wohl\u00b7ge\u00b7ruch", ",", "der", "in", "ihr", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Hagebutten sind so zart gemalt,", "tokens": ["Die", "Ha\u00b7ge\u00b7but\u00b7ten", "sind", "so", "zart", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie duft'ger Rosen hohe Purpurglut,", "tokens": ["Wie", "duft'\u00b7ger", "Ro\u00b7sen", "ho\u00b7he", "Pur\u00b7pur\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bedornt wie sie; am West entfaltet prahlt", "tokens": ["Be\u00b7dornt", "wie", "sie", ";", "am", "West", "ent\u00b7fal\u00b7tet", "prahlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KOKOM", "PPER", "$.", "APPRART", "NN", "APPO", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihr Kn\u00f6splein mit demselben \u00dcbermut.", "tokens": ["Ihr", "Kn\u00f6s\u00b7plein", "mit", "dem\u00b7sel\u00b7ben", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch, weil ihr Wert nur Schein ist, leben sie", "tokens": ["Doch", ",", "weil", "ihr", "Wert", "nur", "Schein", "ist", ",", "le\u00b7ben", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "ADV", "NN", "VAFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Unangesehn, verwelken unempfunden", "tokens": ["Un\u00b7an\u00b7ge\u00b7sehn", ",", "ver\u00b7wel\u00b7ken", "un\u00b7em\u00b7pfun\u00b7den"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zu stillem Tode; s\u00fc\u00dfe Rosen nie:", "tokens": ["Zu", "stil\u00b7lem", "To\u00b7de", ";", "s\u00fc\u00b7\u00dfe", "Ro\u00b7sen", "nie", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus ihrem s\u00fc\u00dfen Tod wird s\u00fc\u00df'rer Duft entbunden.", "tokens": ["Aus", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Tod", "wird", "s\u00fc\u00df'\u00b7rer", "Duft", "ent\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So auch aus dir, du Sch\u00f6ner, Lieber! zieht", "tokens": ["So", "auch", "aus", "dir", ",", "du", "Sch\u00f6\u00b7ner", ",", "Lie\u00b7ber", "!", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "PPER", "NN", "$,", "ADJD", "$.", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Aus dir die Wahrheit, wenn du welkst, mein Lied.", "tokens": ["Aus", "dir", "die", "Wahr\u00b7heit", ",", "wenn", "du", "welkst", ",", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}