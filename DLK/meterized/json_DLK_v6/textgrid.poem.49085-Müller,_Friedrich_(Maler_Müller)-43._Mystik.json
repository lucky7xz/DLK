{"textgrid.poem.49085": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "43. Mystik", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja doch, das Mystische sch\u00e4tz' ich; das Brautlied von Amor und Psyche,", "tokens": ["Ja", "doch", ",", "das", "Mys\u00b7ti\u00b7sche", "sch\u00e4tz'", "ich", ";", "das", "Braut\u00b7lied", "von", "A\u00b7mor", "und", "Psy\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn sich der irdischen Form neu verm\u00e4let der Geist.", "tokens": ["Wenn", "sich", "der", "ir\u00b7di\u00b7schen", "Form", "neu", "ver\u00b7m\u00e4\u00b7let", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus dem hohen Naturwerk, in den Bildern der K\u00fcnste", "tokens": ["Aus", "dem", "ho\u00b7hen", "Na\u00b7tur\u00b7werk", ",", "in", "den", "Bil\u00b7dern", "der", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Spricht es erfreulich mich an, reicht mit der Bl\u00fcte die Frucht.", "tokens": ["Spricht", "es", "er\u00b7freu\u00b7lich", "mich", "an", ",", "reicht", "mit", "der", "Bl\u00fc\u00b7te", "die", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Pfiffig bedienet, sammt dem, was aus den St\u00e4llen man fegt.", "tokens": ["Pfif\u00b7fig", "be\u00b7die\u00b7net", ",", "sammt", "dem", ",", "was", "aus", "den", "St\u00e4l\u00b7len", "man", "fegt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "ART", "$,", "PRELS", "APPR", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}