{"textgrid.poem.58909": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "128. Immer gleichm\u00fctig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Freude, Trost und S\u00fc\u00dfigkeiten", "tokens": ["In", "Freu\u00b7de", ",", "Trost", "und", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erfreue dich nicht allzusehr;", "tokens": ["Er\u00b7freu\u00b7e", "dich", "nicht", "all\u00b7zu\u00b7sehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In D\u00fcrre, Dunkelheit und Leiden", "tokens": ["In", "D\u00fcr\u00b7re", ",", "Dun\u00b7kel\u00b7heit", "und", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei still und Gott im Glauben ehr!", "tokens": ["Sei", "still", "und", "Gott", "im", "Glau\u00b7ben", "ehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "NN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}