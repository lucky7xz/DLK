{"dta.poem.7451": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von B\u00fcchern liegt vor mir ein Perserheer,", "tokens": ["Von", "B\u00fc\u00b7chern", "liegt", "vor", "mir", "ein", "Per\u00b7ser\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch keins kann mir den Unmut ganz verwischen;", "tokens": ["Doch", "keins", "kann", "mir", "den", "Un\u00b7mut", "ganz", "ver\u00b7wi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Unwillig schwingt der Kritiker den Speer:", "tokens": ["Un\u00b7wil\u00b7lig", "schwingt", "der", "Kri\u00b7ti\u00b7ker", "den", "Speer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum die fremde Kost auf unsern Tischen?", "tokens": ["Wa\u00b7rum", "die", "frem\u00b7de", "Kost", "auf", "un\u00b7sern", "Ti\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warum nach Gold in fremden Fl\u00fcssen fischen?", "tokens": ["Wa\u00b7rum", "nach", "Gold", "in", "frem\u00b7den", "Fl\u00fcs\u00b7sen", "fi\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist unsre Heimat, unser Herz so leer?", "tokens": ["Ist", "uns\u00b7re", "Hei\u00b7mat", ",", "un\u00b7ser", "Herz", "so", "leer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geh' wieder in dein K\u00e4mmerlein und dichte!", "tokens": ["Geh'", "wie\u00b7der", "in", "dein", "K\u00e4m\u00b7mer\u00b7lein", "und", "dich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Brauchst keinen Turban, keine welschen Blousen;", "tokens": ["Brauchst", "kei\u00b7nen", "Tur\u00b7ban", ",", "kei\u00b7ne", "wel\u00b7schen", "Blou\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Z\u00fcnd' deinen Zunder an am eignen Lichte!", "tokens": ["Z\u00fcnd'", "dei\u00b7nen", "Zun\u00b7der", "an", "am", "eig\u00b7nen", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Greif', S\u00e4nger, wieder in den eignen Busen,", "tokens": ["Greif'", ",", "S\u00e4n\u00b7ger", ",", "wie\u00b7der", "in", "den", "eig\u00b7nen", "Bu\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deines eignen theuern Volks Geschichte!", "tokens": ["In", "dei\u00b7nes", "eig\u00b7nen", "theu\u00b7ern", "Volks", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da, oder nirgends wohnen deine Musen.", "tokens": ["Da", ",", "o\u00b7der", "nir\u00b7gends", "woh\u00b7nen", "dei\u00b7ne", "Mu\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}