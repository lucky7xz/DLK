{"dta.poem.7900": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6.  Fr\u00fchlingslied des Recensenten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling ist\u2019s, ich lass\u2019 es gelten,", "tokens": ["Fr\u00fch\u00b7ling", "ist's", ",", "ich", "lass'", "es", "gel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich freut\u2019s, ich mu\u00df gestehen,", "tokens": ["Und", "mich", "freut's", ",", "ich", "mu\u00df", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man kann spazieren gehen,", "tokens": ["Da\u00df", "man", "kann", "spa\u00b7zie\u00b7ren", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne just sich zu erk\u00e4lten.", "tokens": ["Oh\u00b7ne", "just", "sich", "zu", "er\u00b7k\u00e4l\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "St\u00f6rche kommen an und Schwalben,", "tokens": ["St\u00f6r\u00b7che", "kom\u00b7men", "an", "und", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht zu fr\u00fche, nicht zu fr\u00fche!", "tokens": ["Nicht", "zu", "fr\u00fc\u00b7he", ",", "nicht", "zu", "fr\u00fc\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "ADJA", "$,", "PTKNEG", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fche nur, mein B\u00e4umchen, bl\u00fche!", "tokens": ["Bl\u00fc\u00b7he", "nur", ",", "mein", "B\u00e4um\u00b7chen", ",", "bl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinethalben, meinethalben!", "tokens": ["Mei\u00b7net\u00b7hal\u00b7ben", ",", "mei\u00b7net\u00b7hal\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja! ich f\u00fchl\u2019 ein wenig Wonne,", "tokens": ["Ja", "!", "ich", "f\u00fchl'", "ein", "we\u00b7nig", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn die Lerche singt ertr\u00e4glich,", "tokens": ["Denn", "die", "Ler\u00b7che", "singt", "er\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Philomele nicht allt\u00e4glich,", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", "nicht", "all\u00b7t\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht so \u00fcbel scheint die Sonne.", "tokens": ["Nicht", "so", "\u00fc\u00b7bel", "scheint", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df es Keinen \u00fcberrasche,", "tokens": ["Da\u00df", "es", "Kei\u00b7nen", "\u00fc\u00b7berr\u00b7a\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich im gr\u00fcnen Feld zu sehen!", "tokens": ["Mich", "im", "gr\u00fc\u00b7nen", "Feld", "zu", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht verschm\u00e4h\u2019 ich auszugehen,", "tokens": ["Nicht", "ver\u00b7schm\u00e4h'", "ich", "aus\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kleistens Fr\u00fchling in der Tasche.", "tokens": ["Kleis\u00b7tens", "Fr\u00fch\u00b7ling", "in", "der", "Ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}