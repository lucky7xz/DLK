{"dta.poem.10308": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Una ex hisre morieris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es flammt der Horizont des hei\u00dfen Tages.             ", "tokens": ["Es", "flammt", "der", "Ho\u00b7ri\u00b7zont", "des", "hei\u00b7\u00dfen", "Ta\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schmetterlinge Fl\u00fcgelschlag ist h\u00f6rbar,", "tokens": ["Der", "Schmet\u00b7ter\u00b7lin\u00b7ge", "Fl\u00fc\u00b7gel\u00b7schlag", "ist", "h\u00f6r\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So still ruht Baum und Blatt im Sonnenschein.", "tokens": ["So", "still", "ruht", "Baum", "und", "Blatt", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf fernem Steig klingt schwach des G\u00e4rtners Harke.", "tokens": ["Auf", "fer\u00b7nem", "Steig", "klingt", "schwach", "des", "G\u00e4rt\u00b7ners", "Har\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201ein einer dieser Stunden wirst du sterben!\u201c", "tokens": ["\u201e", "in", "ei\u00b7ner", "die\u00b7ser", "Stun\u00b7den", "wirst", "du", "ster\u00b7ben", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PIS", "PDAT", "NN", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steht auf der Sonnenuhr im gro\u00dfen Garten,", "tokens": ["Steht", "auf", "der", "Son\u00b7ne\u00b7nuhr", "im", "gro\u00b7\u00dfen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf dessen Weiser sich ein alter Spatz", "tokens": ["Auf", "des\u00b7sen", "Wei\u00b7ser", "sich", "ein", "al\u00b7ter", "Spatz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den unscheinbaren Kragen emsig putzt", "tokens": ["Den", "un\u00b7schein\u00b7ba\u00b7ren", "Kra\u00b7gen", "em\u00b7sig", "putzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schnell das schiefgebogne K\u00f6pfchen kraut.", "tokens": ["Und", "schnell", "das", "schief\u00b7ge\u00b7bog\u00b7ne", "K\u00f6pf\u00b7chen", "kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann fliegt er fort, im Kirschenbaum zu landen.", "tokens": ["Dann", "fliegt", "er", "fort", ",", "im", "Kir\u00b7schen\u00b7baum", "zu", "lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch unterwegs schl\u00e4gt ihn der b\u00f6se Falk.", "tokens": ["Doch", "un\u00b7ter\u00b7wegs", "schl\u00e4gt", "ihn", "der", "b\u00f6\u00b7se", "Falk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In einer dieser Stunden wirst du sterben.", "tokens": ["In", "ei\u00b7ner", "die\u00b7ser", "Stun\u00b7den", "wirst", "du", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PDAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bewegung. Menschen. Nackte braune Arme", "tokens": ["Be\u00b7we\u00b7gung", ".", "Men\u00b7schen", ".", "Nack\u00b7te", "brau\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tragen zum Teich ein breites Fischernetz,", "tokens": ["Tra\u00b7gen", "zum", "Teich", "ein", "brei\u00b7tes", "Fi\u00b7scher\u00b7netz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und warten dann gehorsam auf Befehl", "tokens": ["Und", "war\u00b7ten", "dann", "ge\u00b7hor\u00b7sam", "auf", "Be\u00b7fehl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Anfang.", "tokens": ["Zum", "An\u00b7fang", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Und trotz der Schw\u00fcle naht in schwerem Sammet", "tokens": ["Und", "trotz", "der", "Schw\u00fc\u00b7le", "naht", "in", "schwe\u00b7rem", "Sam\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die junge, wundersch\u00f6ne K\u00f6nigin.", "tokens": ["Die", "jun\u00b7ge", ",", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf blonder Pagen Armen schl\u00e4ft die Schleppe.", "tokens": ["Auf", "blon\u00b7der", "Pa\u00b7gen", "Ar\u00b7men", "schl\u00e4ft", "die", "Schlep\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rechts tr\u00e4gt das Dach, den riesigen Sonnenschirm,", "tokens": ["Rechts", "tr\u00e4gt", "das", "Dach", ",", "den", "rie\u00b7si\u00b7gen", "Son\u00b7nen\u00b7schirm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein Mohrenknab\u2019 in gelb und rother Seide.", "tokens": ["Ein", "Moh\u00b7ren\u00b7knab'", "in", "gelb", "und", "ro\u00b7ther", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Links h\u00e4lt ein schlanker Fant im Puffenwams,", "tokens": ["Links", "h\u00e4lt", "ein", "schlan\u00b7ker", "Fant", "im", "Puf\u00b7fen\u00b7wams", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit dem sie huldvoll spricht, den gleichen Schritt;", "tokens": ["Mit", "dem", "sie", "huld\u00b7voll", "spricht", ",", "den", "glei\u00b7chen", "Schritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im schaukelnden Gehenke blitzt sein Dolch.", "tokens": ["Im", "schau\u00b7keln\u00b7den", "Ge\u00b7hen\u00b7ke", "blitzt", "sein", "Dolch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Kammerherr vom Tag und ihre Damen", "tokens": ["Der", "Kam\u00b7mer\u00b7herr", "vom", "Tag", "und", "ih\u00b7re", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Folgen in ehrerbietiger Entfernung.", "tokens": ["Fol\u00b7gen", "in", "ehr\u00b7er\u00b7bie\u00b7ti\u00b7ger", "Ent\u00b7fer\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Indessen ist die F\u00fcrstin angekommen", "tokens": ["In\u00b7des\u00b7sen", "ist", "die", "F\u00fcrs\u00b7tin", "an\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und hat im Marmorsessel Platz genommen,", "tokens": ["Und", "hat", "im", "Mar\u00b7mor\u00b7ses\u00b7sel", "Platz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den Fu\u00df auf rasch gelegten Teppich setzend.", "tokens": ["Den", "Fu\u00df", "auf", "rasch", "ge\u00b7leg\u00b7ten", "Tep\u00b7pich", "set\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Zug beginnt, ganz wie zu Petri Tagen:", "tokens": ["Der", "Zug", "be\u00b7ginnt", ",", "ganz", "wie", "zu", "Pe\u00b7tri", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "KOKOM", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Netze zappeln Karpfen und Karauschen", "tokens": ["Im", "Net\u00b7ze", "zap\u00b7peln", "Karp\u00b7fen", "und", "Ka\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit dummen Augen, schnappend, schwer ge\u00e4ngstigt.", "tokens": ["Mit", "dum\u00b7men", "Au\u00b7gen", ",", "schnap\u00b7pend", ",", "schwer", "ge\u00b7\u00e4ngs\u00b7tigt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hoheit lacht, die Kavaliere l\u00e4cheln,", "tokens": ["Die", "Ho\u00b7heit", "lacht", ",", "die", "Ka\u00b7va\u00b7lie\u00b7re", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es grinst der Mohr, die blonden Pagen kichern.", "tokens": ["Es", "grinst", "der", "Mohr", ",", "die", "blon\u00b7den", "Pa\u00b7gen", "ki\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und in der allgemeinen Lustigkeit,", "tokens": ["Und", "in", "der", "all\u00b7ge\u00b7mei\u00b7nen", "Lus\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das braune Auge pl\u00f6tzlich aufschlagend", "tokens": ["Das", "brau\u00b7ne", "Au\u00b7ge", "pl\u00f6tz\u00b7lich", "auf\u00b7schla\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Zum schlanken Fant im blauen Puffenwams,", "tokens": ["Zum", "schlan\u00b7ken", "Fant", "im", "blau\u00b7en", "Puf\u00b7fen\u00b7wams", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Fl\u00fcstert harmlos die junge K\u00f6nigin:", "tokens": ["Fl\u00fcs\u00b7tert", "harm\u00b7los", "die", "jun\u00b7ge", "K\u00f6\u00b7ni\u00b7gin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Bei Mondesaufgang an der Sonnenuhr.", "tokens": ["Bei", "Mon\u00b7des\u00b7auf\u00b7gang", "an", "der", "Son\u00b7ne\u00b7nuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da st\u00fcrzt ein Pfeil aus dunklem Tannenbusch,", "tokens": ["Da", "st\u00fcrzt", "ein", "Pfeil", "aus", "dunk\u00b7lem", "Tan\u00b7nen\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschnitzt aus eines plumpen St\u00f6res Gr\u00e4te,", "tokens": ["Ge\u00b7schnitzt", "aus", "ei\u00b7nes", "plum\u00b7pen", "St\u00f6\u00b7res", "Gr\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lust in\u2019s liebesehnsuchtsvolle Herz", "tokens": ["Mit", "Lust", "in's", "lie\u00b7be\u00b7sehn\u00b7suchts\u00b7vol\u00b7le", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der jungen, wundersch\u00f6nen K\u00f6nigin.", "tokens": ["Der", "jun\u00b7gen", ",", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "In einer dieser Stunden wirst du sterben.", "tokens": ["In", "ei\u00b7ner", "die\u00b7ser", "Stun\u00b7den", "wirst", "du", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PDAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}