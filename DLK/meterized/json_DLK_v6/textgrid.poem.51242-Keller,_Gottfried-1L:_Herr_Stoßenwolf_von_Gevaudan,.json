{"textgrid.poem.51242": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Sto\u00dfenwolf von Gevaudan,", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Sto\u00dfenwolf von Gevaudan,", "tokens": ["Herr", "Sto\u00b7\u00dfen\u00b7wolf", "von", "Ge\u00b7vau\u00b7dan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Bischof, sitzt bei Tische;", "tokens": ["Der", "Bi\u00b7schof", ",", "sitzt", "bei", "Ti\u00b7sche", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er bietet seinen G\u00e4sten an", "tokens": ["Er", "bie\u00b7tet", "sei\u00b7nen", "G\u00e4s\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die allersch\u00f6nsten Fische.", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Haupt des Ebers stellt sich dar,", "tokens": ["Das", "Haupt", "des", "E\u00b7bers", "stellt", "sich", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Untadelig geraten;", "tokens": ["Un\u00b7ta\u00b7de\u00b7lig", "ge\u00b7ra\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann aber folgen, Paar auf Paar,", "tokens": ["Dann", "a\u00b7ber", "fol\u00b7gen", ",", "Paar", "auf", "Paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Absonderliche Braten.", "tokens": ["Ab\u00b7son\u00b7der\u00b7li\u00b7che", "Bra\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Zwei Hasen kommen ohne Kopf", "tokens": ["Zwei", "Ha\u00b7sen", "kom\u00b7men", "oh\u00b7ne", "Kopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Silber angefahren,", "tokens": ["Auf", "Sil\u00b7ber", "an\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Marmotten sind im g\u00fcldnen Topf,", "tokens": ["Mar\u00b7mot\u00b7ten", "sind", "im", "g\u00fcld\u00b7nen", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch schwanzlos zu gewahren.", "tokens": ["Doch", "schwanz\u00b7los", "zu", "ge\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dem Birkhuhn fehlt ein Fl\u00fcgel hier,", "tokens": ["Dem", "Birk\u00b7huhn", "fehlt", "ein", "Fl\u00fc\u00b7gel", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schenkel dort dem Hahne:", "tokens": ["Ein", "Schen\u00b7kel", "dort", "dem", "Hah\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit arg zerzauster Federzier", "tokens": ["Mit", "arg", "zer\u00b7zaus\u00b7ter", "Fe\u00b7der\u00b7zier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaun traurig die Fasane.", "tokens": ["Schaun", "trau\u00b7rig", "die", "Fa\u00b7sa\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Dem jungen Reh ist das Genick", "tokens": ["Dem", "jun\u00b7gen", "Reh", "ist", "das", "Ge\u00b7nick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdreht und ganz zerschmissen", "tokens": ["Ver\u00b7dreht", "und", "ganz", "zer\u00b7schmis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, wie mit Klaun, ein gutes St\u00fcck", "tokens": ["Und", ",", "wie", "mit", "Klaun", ",", "ein", "gu\u00b7tes", "St\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Ziemer weggerissen.", "tokens": ["Vom", "Zie\u00b7mer", "weg\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch alles ist mit feiner Kunst", "tokens": ["Doch", "al\u00b7les", "ist", "mit", "fei\u00b7ner", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereitet nach der Sitte;", "tokens": ["Be\u00b7rei\u00b7tet", "nach", "der", "Sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig Heinrich schl\u00fcrft den Dunst,", "tokens": ["Der", "K\u00f6\u00b7nig", "Hein\u00b7rich", "schl\u00fcrft", "den", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Frankenreich der Dritte.", "tokens": ["Vom", "Fran\u00b7ken\u00b7reich", "der", "Drit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er schl\u00fcrft und i\u00dft sich schweigend satt;", "tokens": ["Er", "schl\u00fcrft", "und", "i\u00dft", "sich", "schwei\u00b7gend", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch als er nun gegessen,", "tokens": ["Doch", "als", "er", "nun", "ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ruft er: \u00bbIch glaub, der Teufel hat", "tokens": ["Ruft", "er", ":", "\u00bb", "Ich", "glaub", ",", "der", "Teu\u00b7fel", "hat"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor uns zu Tisch gesessen!\u00ab", "tokens": ["Vor", "uns", "zu", "Tisch", "ge\u00b7ses\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVPP", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Der Bischof lacht: \u00bbVergebung, Sire!", "tokens": ["Der", "Bi\u00b7schof", "lacht", ":", "\u00bb", "Ver\u00b7ge\u00b7bung", ",", "Si\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schlimm ist's nicht beschaffen!", "tokens": ["So", "schlimm", "ist's", "nicht", "be\u00b7schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur meine J\u00e4ger naschen mir", "tokens": ["Nur", "mei\u00b7ne", "J\u00e4\u00b7ger", "na\u00b7schen", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von allem, was sie raffen!", "tokens": ["Von", "al\u00b7lem", ",", "was", "sie", "raf\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Adler sind's im Bergrevier;", "tokens": ["Die", "Ad\u00b7ler", "sin\u00b7d's", "im", "Berg\u00b7re\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An jenen Felsenkronen", "tokens": ["An", "je\u00b7nen", "Fel\u00b7sen\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4ngt Horst an Horst, wo dienstbar mir", "tokens": ["H\u00e4ngt", "Horst", "an", "Horst", ",", "wo", "dienst\u00b7bar", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "APPR", "NE", "$,", "PWAV", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wilden V\u00f6gel wohnen.", "tokens": ["Die", "wil\u00b7den", "V\u00f6\u00b7gel", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Bei jedem Nest klebt an der Wand", "tokens": ["Bei", "je\u00b7dem", "Nest", "klebt", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Ritzen still ein Bauer,", "tokens": ["In", "Rit\u00b7zen", "still", "ein", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem Kn\u00fcppel in der Hand,", "tokens": ["Mit", "ei\u00b7nem", "Kn\u00fcp\u00b7pel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt sich auf der Lauer.", "tokens": ["Und", "h\u00e4lt", "sich", "auf", "der", "Lau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ist dann das Wildbret eingetan", "tokens": ["Ist", "dann", "das", "Wild\u00b7bret", "ein\u00b7ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom alten Adlerpaare,", "tokens": ["Vom", "al\u00b7ten", "Ad\u00b7ler\u00b7paa\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So macht sich jener flugs daran,", "tokens": ["So", "macht", "sich", "je\u00b7ner", "flugs", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PDAT", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald nur fort die Aare.", "tokens": ["So\u00b7bald", "nur", "fort", "die", "Aa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er kapert von dem blut'gen Stein", "tokens": ["Er", "ka\u00b7pert", "von", "dem", "blut'\u00b7gen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Beste mir zuhanden;", "tokens": ["Das", "Bes\u00b7te", "mir", "zu\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zuweilen f\u00e4llt ein B\u00e4uerlein", "tokens": ["Zu\u00b7wei\u00b7len", "f\u00e4llt", "ein", "B\u00e4u\u00b7er\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich freilich auch zuschanden.", "tokens": ["Sich", "frei\u00b7lich", "auch", "zu\u00b7schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Damit die Brut nicht fl\u00fcgge wird,", "tokens": ["Da\u00b7mit", "die", "Brut", "nicht", "fl\u00fcg\u00b7ge", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlie\u00dft man sie fest am Felsen,", "tokens": ["Schlie\u00dft", "man", "sie", "fest", "am", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis sich ein neu Geschlecht gebiert", "tokens": ["Bis", "sich", "ein", "neu", "Ge\u00b7schlecht", "ge\u00b7biert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "ADJD", "NN", "VVPP"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit nackten Hungerh\u00e4lsen", "tokens": ["Mit", "nack\u00b7ten", "Hun\u00b7ger\u00b7h\u00e4l\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und rastlos fliegen ab und zu", "tokens": ["Und", "rast\u00b7los", "flie\u00b7gen", "ab", "und", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Alten um die Nahrung.", "tokens": ["Die", "Al\u00b7ten", "um", "die", "Nah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So \u00fcben wir in aller Ruh", "tokens": ["So", "\u00fc\u00b7ben", "wir", "in", "al\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nutzens kluge Wahrung.\u00ab", "tokens": ["Des", "Nut\u00b7zens", "klu\u00b7ge", "Wah\u00b7rung", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da schreit der K\u00f6nig Sausewind", "tokens": ["Da", "schreit", "der", "K\u00f6\u00b7nig", "Sau\u00b7se\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schl\u00e4gt sich an die H\u00fcften:", "tokens": ["Und", "schl\u00e4gt", "sich", "an", "die", "H\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhie zeigt es sich, was Pfaffen sind!", "tokens": ["\u00bb", "hie", "zeigt", "es", "sich", ",", "was", "Pfaf\u00b7fen", "sind", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}