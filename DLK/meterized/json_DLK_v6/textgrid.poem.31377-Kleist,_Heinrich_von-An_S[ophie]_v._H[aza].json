{"textgrid.poem.31377": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "An S[ophie] v. H[aza]", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Bl\u00fcmchen, das, dem Tal entbl\u00fcht,", "tokens": ["Das", "Bl\u00fcm\u00b7chen", ",", "das", ",", "dem", "Tal", "ent\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir Ruhe gibt und Stille,", "tokens": ["Dir", "Ru\u00b7he", "gibt", "und", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Krampf dir durch die Nerve gl\u00fcht,", "tokens": ["Wenn", "Krampf", "dir", "durch", "die", "Ner\u00b7ve", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das nennst du die Kamille.", "tokens": ["Das", "nennst", "du", "die", "Ka\u00b7mil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, die, wenn Krampf das Herz umstrickt,", "tokens": ["Du", ",", "die", ",", "wenn", "Krampf", "das", "Herz", "um\u00b7strickt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "$,", "KOUS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Freundin, aus der F\u00fclle", "tokens": ["O", "Freun\u00b7din", ",", "aus", "der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Der Brust, mir so viel St\u00e4rkung schickt,", "tokens": ["Der", "Brust", ",", "mir", "so", "viel", "St\u00e4r\u00b7kung", "schickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}