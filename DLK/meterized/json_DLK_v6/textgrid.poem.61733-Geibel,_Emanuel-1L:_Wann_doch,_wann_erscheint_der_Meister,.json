{"textgrid.poem.61733": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann doch, wann erscheint der Meister,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann doch, wann erscheint der Meister,", "tokens": ["Wann", "doch", ",", "wann", "er\u00b7scheint", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der, o Deutschland, dich erbaut,", "tokens": ["Der", ",", "o", "Deutschland", ",", "dich", "er\u00b7baut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "FM", "NE", "$,", "PPER", "VVPP", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie die Sehnsucht edler Geister", "tokens": ["Wie", "die", "Sehn\u00b7sucht", "ed\u00b7ler", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahnungsvoll dich l\u00e4ngst geschaut:", "tokens": ["Ah\u00b7nungs\u00b7voll", "dich", "l\u00e4ngst", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eins nach au\u00dfen, schwertgewaltig", "tokens": ["Eins", "nach", "au\u00b7\u00dfen", ",", "schwert\u00b7ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ADV", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ein hoch Panier geschart!", "tokens": ["Um", "ein", "hoch", "Pa\u00b7nier", "ge\u00b7schart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Innen reich und vielgestaltig,", "tokens": ["In\u00b7nen", "reich", "und", "viel\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Stamm nach seiner Art!", "tokens": ["Je\u00b7der", "Stamm", "nach", "sei\u00b7ner", "Art", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht ihr, wie der Regenbogen", "tokens": ["Seht", "ihr", ",", "wie", "der", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort in sieben Farben quillt?", "tokens": ["Dort", "in", "sie\u00b7ben", "Far\u00b7ben", "quillt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch hoch und fest gezogen", "tokens": ["Den\u00b7noch", "hoch", "und", "fest", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00f6lbt er sich, der Eintracht Bild.", "tokens": ["W\u00f6lbt", "er", "sich", ",", "der", "Ein\u00b7tracht", "Bild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf der Harfe laut und leise", "tokens": ["Auf", "der", "Har\u00b7fe", "laut", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind gespannt der Saiten viel;", "tokens": ["Sind", "ge\u00b7spannt", "der", "Sai\u00b7ten", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede t\u00f6nt nach ihrer Weise,", "tokens": ["Je\u00b7de", "t\u00f6nt", "nach", "ih\u00b7rer", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch gibt's ein klares Spiel.", "tokens": ["Den\u00b7noch", "gibt's", "ein", "kla\u00b7res", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, wann rauschen so verschlungen", "tokens": ["O", ",", "wann", "rau\u00b7schen", "so", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Farben, S\u00fcd und Nord!", "tokens": ["Eu\u00b7re", "Far\u00b7ben", ",", "S\u00fcd", "und", "Nord", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harfenspiel der deutschen Zungen,", "tokens": ["Har\u00b7fen\u00b7spiel", "der", "deut\u00b7schen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann erklingst du im Akkord!", "tokens": ["Wann", "er\u00b7klingst", "du", "im", "Ak\u00b7kord", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+++", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "La\u00df mich's einmal noch vernehmen,", "tokens": ["La\u00df", "mich's", "ein\u00b7mal", "noch", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich's einmal, Herr, noch sehn!", "tokens": ["La\u00df", "mich's", "ein\u00b7mal", ",", "Herr", ",", "noch", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "NN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann will ich's ohne Gr\u00e4men", "tokens": ["Und", "dann", "will", "ich's", "oh\u00b7ne", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsern V\u00e4tern melden gehn.", "tokens": ["Un\u00b7sern", "V\u00e4\u00b7tern", "mel\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}