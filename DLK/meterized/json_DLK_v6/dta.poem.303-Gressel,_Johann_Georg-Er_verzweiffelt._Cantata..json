{"dta.poem.303": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verzweiffelt.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Blitzet ihr Wolcken/ und donnert ihr L\u00fcffte/", "tokens": ["Blit\u00b7zet", "ihr", "Wol\u00b7cken", "/", "und", "don\u00b7nert", "ihr", "L\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Speyet Pech/ Schwefel ihr h\u00f6llischen Kl\u00fcffte/", "tokens": ["Spey\u00b7et", "Pech", "/", "Schwe\u00b7fel", "ihr", "h\u00f6l\u00b7li\u00b7schen", "Kl\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Fallet ihr Berge doch auf mich zu:", "tokens": ["Fal\u00b7let", "ihr", "Ber\u00b7ge", "doch", "auf", "mich", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Brechet ihr Gr\u00fcnde der dunckelen Erden/", "tokens": ["Bre\u00b7chet", "ihr", "Gr\u00fcn\u00b7de", "der", "dun\u00b7cke\u00b7len", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Wollet ihr Himmel nicht g\u00fcnstig mir werden/", "tokens": ["Wol\u00b7let", "ihr", "Him\u00b7mel", "nicht", "g\u00fcns\u00b7tig", "mir", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "PPER", "VAFIN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Bringe mich Abgrund endlich zur Ruh.", "tokens": ["Brin\u00b7ge", "mich", "Ab\u00b7grund", "end\u00b7lich", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Auf/ auf mein Geist", "tokens": ["Auf", "/", "auf", "mein", "Geist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "La\u00df Dolch und Degen klingen", "tokens": ["La\u00df", "Dolch", "und", "De\u00b7gen", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was gifftig heist", "tokens": ["Was", "giff\u00b7tig", "heist"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mu\u00df zu dem Hertzen dringen/", "tokens": ["Mu\u00df", "zu", "dem", "Hert\u00b7zen", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Den Lebens-Faden ab/", "tokens": ["Den", "Le\u00b7bens\u00b7Fa\u00b7den", "ab", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Fort/ fort/ ich sehe schon die H\u00f6lle und das Grab.", "tokens": ["Fort", "/", "fort", "/", "ich", "se\u00b7he", "schon", "die", "H\u00f6l\u00b7le", "und", "das", "Grab", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PTKVZ", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich kan/ ich darf/ ich will nicht mehr am Leben seyn/", "tokens": ["Ich", "kan", "/", "ich", "darf", "/", "ich", "will", "nicht", "mehr", "am", "Le\u00b7ben", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr Furien heraus verk\u00fcrtzet meine Pein.", "tokens": ["Ihr", "Fu\u00b7ri\u00b7en", "he\u00b7raus", "ver\u00b7k\u00fcrt\u00b7zet", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APZR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fort auf! fort auf!", "tokens": ["Fort", "auf", "!", "fort", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PTKVZ", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Brechet/ reisset", "tokens": ["Bre\u00b7chet", "/", "reis\u00b7set"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mich als wie ein zartes Laub/", "tokens": ["Mich", "als", "wie", "ein", "zar\u00b7tes", "Laub", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Tretet/ schmeisset", "tokens": ["Tre\u00b7tet", "/", "schmeis\u00b7set"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$(", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Meine Glieder in den Staub", "tokens": ["Mei\u00b7ne", "Glie\u00b7der", "in", "den", "Staub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fort auf! fort auf!", "tokens": ["Fort", "auf", "!", "fort", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PTKVZ", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mischt Gifft in Drachen Blut/", "tokens": ["Mischt", "Gifft", "in", "Dra\u00b7chen", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schenckt Ertz/ das fliesset/ ein/", "tokens": ["Schenckt", "Ertz", "/", "das", "flies\u00b7set", "/", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "PDS", "VVFIN", "$(", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu l\u00f6schen meine Gluth", "tokens": ["Zu", "l\u00f6\u00b7schen", "mei\u00b7ne", "Gluth"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die heisse Liebes-Pein/", "tokens": ["Die", "heis\u00b7se", "Lie\u00b7bes\u00b7Pein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die mich ums Leben bringet", "tokens": ["Die", "mich", "ums", "Le\u00b7ben", "brin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und zur Verzweifflung zwinget.", "tokens": ["Und", "zur", "Ver\u00b7zweif\u00b7flung", "zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Brechet ihr Gr\u00fcnde/ heckt h\u00f6llische Flammen/", "tokens": ["Bre\u00b7chet", "ihr", "Gr\u00fcn\u00b7de", "/", "heckt", "h\u00f6l\u00b7li\u00b7sche", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$(", "VVFIN", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Kommet ihr Flammen all\u2019 auf mich zusammen/", "tokens": ["Kom\u00b7met", "ihr", "Flam\u00b7men", "all'", "auf", "mich", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIS", "APPR", "PPER", "PTKVZ", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Blitzet ihr Himmel/ mehret den Strahl:", "tokens": ["Blit\u00b7zet", "ihr", "Him\u00b7mel", "/", "meh\u00b7ret", "den", "Strahl", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hemmet den Jammer/ und endet mein Leben/", "tokens": ["Hem\u00b7met", "den", "Jam\u00b7mer", "/", "und", "en\u00b7det", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Sch\u00e4rffet euch Dolche den Rest mir zu geben/", "tokens": ["Sch\u00e4rf\u00b7fet", "euch", "Dol\u00b7che", "den", "Rest", "mir", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "K\u00fcrtzet das Elend/ endet die Quaal.", "tokens": ["K\u00fcrt\u00b7zet", "das", "E\u00b7lend", "/", "en\u00b7det", "die", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}