{"textgrid.poem.24183": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was willst du alles wissen?", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was willst du alles wissen?", "tokens": ["Was", "willst", "du", "al\u00b7les", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oh Weiser, sei kein Thor!", "tokens": ["Oh", "Wei\u00b7ser", ",", "sei", "kein", "Thor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer klug ist, zieht dem Leben", "tokens": ["Wer", "klug", "ist", ",", "zieht", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Leis einen Schleier vor.", "tokens": ["Leis", "ei\u00b7nen", "Schlei\u00b7er", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Nackte ist das Leere,", "tokens": ["Das", "Nack\u00b7te", "ist", "das", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du es nicht verkl\u00e4rst,", "tokens": ["Wenn", "du", "es", "nicht", "ver\u00b7kl\u00e4rst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und keine Sch\u00f6nheit w\u00e4re,", "tokens": ["Und", "kei\u00b7ne", "Sch\u00f6n\u00b7heit", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn du kein Seher w\u00e4rst.", "tokens": ["Wenn", "du", "kein", "Se\u00b7her", "w\u00e4rst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}