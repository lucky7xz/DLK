{"dta.poem.18678": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Wink .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Und doch haben sie Recht die ich schelte:", "tokens": ["Und", "doch", "ha\u00b7ben", "sie", "Recht", "die", "ich", "schel\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Denn dass ein Wort nicht einfach gelte", "tokens": ["Denn", "dass", "ein", "Wort", "nicht", "ein\u00b7fach", "gel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das m\u00fcsste sich wohl von selbst verstehn.", "tokens": ["Das", "m\u00fcss\u00b7te", "sich", "wohl", "von", "selbst", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Wort ist ein F\u00e4cher! Zwischen den St\u00e4ben", "tokens": ["Das", "Wort", "ist", "ein", "F\u00e4\u00b7cher", "!", "Zwi\u00b7schen", "den", "St\u00e4\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "NN", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Blicken ein Paar sch\u00f6ne Augen hervor.", "tokens": ["Bli\u00b7cken", "ein", "Paar", "sch\u00f6\u00b7ne", "Au\u00b7gen", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Der F\u00e4cher ist nur ein lieblicher Flor,", "tokens": ["Der", "F\u00e4\u00b7cher", "ist", "nur", "ein", "lieb\u00b7li\u00b7cher", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er verdeckt mir zwar das Gesicht;", "tokens": ["Er", "ver\u00b7deckt", "mir", "zwar", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Aber das M\u00e4dchen verbirgt er nicht,", "tokens": ["A\u00b7ber", "das", "M\u00e4d\u00b7chen", "ver\u00b7birgt", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Weil das sch\u00f6nste was sie besitzt", "tokens": ["Weil", "das", "sch\u00f6ns\u00b7te", "was", "sie", "be\u00b7sitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "PWS", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Das Auge, mir in\u2019s Auge blitzt.", "tokens": ["Das", "Au\u00b7ge", ",", "mir", "in's", "Au\u00b7ge", "blitzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}