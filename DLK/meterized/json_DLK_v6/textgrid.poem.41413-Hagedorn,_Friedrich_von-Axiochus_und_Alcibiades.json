{"textgrid.poem.41413": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Axiochus und Alcibiades", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Axiochus, ein Schalk von schmeichelhaften Sitten,", "tokens": ["A\u00b7xio\u00b7chus", ",", "ein", "Schalk", "von", "schmei\u00b7chel\u00b7haf\u00b7ten", "Sit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Alcibiades, der Stutzer von Athen,", "tokens": ["Und", "Al\u00b7ci\u00b7bi\u00b7a\u00b7des", ",", "der", "Stut\u00b7zer", "von", "A\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zween Freunde gleicher Art, bei M\u00e4dchen wohl gelitten,", "tokens": ["Zween", "Freun\u00b7de", "glei\u00b7cher", "Art", ",", "bei", "M\u00e4d\u00b7chen", "wohl", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schlau, feurig, jung, galant, beredt und wundersch\u00f6n,", "tokens": ["Schlau", ",", "feu\u00b7rig", ",", "jung", ",", "ga\u00b7lant", ",", "be\u00b7redt", "und", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verst\u00e4rkten da die Treu', wo manche sie verscherzen;", "tokens": ["Ver\u00b7st\u00e4rk\u00b7ten", "da", "die", "Treu'", ",", "wo", "man\u00b7che", "sie", "ver\u00b7scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was beiden reizend schien, hie\u00df beiden auch gemein.", "tokens": ["Was", "bei\u00b7den", "rei\u00b7zend", "schien", ",", "hie\u00df", "bei\u00b7den", "auch", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "VVFIN", "$,", "VVFIN", "PIAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Fand einer keine Lust, den eignen Schatz zu herzen,", "tokens": ["Fand", "ei\u00b7ner", "kei\u00b7ne", "Lust", ",", "den", "eig\u00b7nen", "Schatz", "zu", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So stellte sich daf\u00fcr des andern M\u00e4dchen ein.", "tokens": ["So", "stell\u00b7te", "sich", "da\u00b7f\u00fcr", "des", "an\u00b7dern", "M\u00e4d\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie artig jede war, dient wenig zur Geschichte:", "tokens": ["Wie", "ar\u00b7tig", "je\u00b7de", "war", ",", "dient", "we\u00b7nig", "zur", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "VAFIN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "G'nug, da\u00df die eine drauf ein T\u00f6chterchen gebar,", "tokens": ["G'\u00b7nug", ",", "da\u00df", "die", "ei\u00b7ne", "drauf", "ein", "T\u00f6ch\u00b7ter\u00b7chen", "ge\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ART", "PAV", "ART", "NN", "ADJD", "$,"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Die in den Windeln schon liebreizend von Gesichte,", "tokens": ["Die", "in", "den", "Win\u00b7deln", "schon", "lieb\u00b7rei\u00b7zend", "von", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Und Helenen vielleicht an Z\u00fcgen \u00e4hnlich war.", "tokens": ["Und", "He\u00b7le\u00b7nen", "viel\u00b7leicht", "an", "Z\u00fc\u00b7gen", "\u00e4hn\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Flugs sieht man beiderseits zur kleinen Doris eilen,", "tokens": ["Flugs", "sieht", "man", "bei\u00b7der\u00b7seits", "zur", "klei\u00b7nen", "Do\u00b7ris", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPRART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein jeder nennet sie sein wahres Ebenbild,", "tokens": ["Ein", "je\u00b7der", "nen\u00b7net", "sie", "sein", "wah\u00b7res", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und will das Vaterrecht nicht mit dem Freunde theilen,", "tokens": ["Und", "will", "das", "Va\u00b7ter\u00b7recht", "nicht", "mit", "dem", "Freun\u00b7de", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das Recht, das sie zugleich mit Lust und Neid erf\u00fcllt.", "tokens": ["Das", "Recht", ",", "das", "sie", "zu\u00b7gleich", "mit", "Lust", "und", "Neid", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jedoch, als Doris nur, der Mutter nachzuahmen,", "tokens": ["Je\u00b7doch", ",", "als", "Do\u00b7ris", "nur", ",", "der", "Mut\u00b7ter", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NE", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und K\u00fcsse zu verstehn, sich alt genug befand,", "tokens": ["Und", "K\u00fcs\u00b7se", "zu", "ver\u00b7stehn", ",", "sich", "alt", "ge\u00b7nug", "be\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$,", "PRF", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Entsagten beiderseits dem ernsten Vaternamen,", "tokens": ["Ent\u00b7sag\u00b7ten", "bei\u00b7der\u00b7seits", "dem", "erns\u00b7ten", "Va\u00b7ter\u00b7na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und suchten Gegengunst, die Pflicht und Furcht nicht band.", "tokens": ["Und", "such\u00b7ten", "Ge\u00b7gen\u00b7gunst", ",", "die", "Pflicht", "und", "Furcht", "nicht", "band", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ART", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der eine sprach: du bist der Vater zu dem Kinde;", "tokens": ["Der", "ei\u00b7ne", "sprach", ":", "du", "bist", "der", "Va\u00b7ter", "zu", "dem", "Kin\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dies ist dein Aug' und Mund. Was kann dir gleicher sein?", "tokens": ["Dies", "ist", "dein", "Aug'", "und", "Mund", ".", "Was", "kann", "dir", "glei\u00b7cher", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$.", "PWS", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Halt! rief der andre drauf, auf mich, auf mich die S\u00fcnde!", "tokens": ["Halt", "!", "rief", "der", "and\u00b7re", "drauf", ",", "auf", "mich", ",", "auf", "mich", "die", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "ART", "PIS", "PTKVZ", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Herr Schwager, glaube mir, sie stammt von dir allein.", "tokens": ["Herr", "Schwa\u00b7ger", ",", "glau\u00b7be", "mir", ",", "sie", "stammt", "von", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}