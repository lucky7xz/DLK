{"textgrid.poem.65145": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutsche Sprache! Zaubergarten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsche Sprache! Zaubergarten", "tokens": ["Deut\u00b7sche", "Spra\u00b7che", "!", "Zau\u00b7berg\u00b7ar\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, mit Blumen aller Arten", "tokens": ["Du", ",", "mit", "Blu\u00b7men", "al\u00b7ler", "Ar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reich und wundersam bekr\u00e4nzt!", "tokens": ["Reich", "und", "wun\u00b7der\u00b7sam", "be\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schacht, in dessen dunkeln G\u00e4ngen", "tokens": ["Schacht", ",", "in", "des\u00b7sen", "dun\u00b7keln", "G\u00e4n\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gold und Eisen sich vermengen", "tokens": ["Gold", "und", "Ei\u00b7sen", "sich", "ver\u00b7men\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Lichtkarfunkel gl\u00e4nzt!", "tokens": ["Und", "der", "Licht\u00b7kar\u00b7fun\u00b7kel", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meer, aus dessen Wogenf\u00fclle", "tokens": ["Meer", ",", "aus", "des\u00b7sen", "Wo\u00b7gen\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Schleier, ohne H\u00fclle", "tokens": ["Oh\u00b7ne", "Schlei\u00b7er", ",", "oh\u00b7ne", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich die reinste Sch\u00f6nheit hebt!", "tokens": ["Sich", "die", "reins\u00b7te", "Sch\u00f6n\u00b7heit", "hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Luft aus klaren Aetherh\u00f6hen,", "tokens": ["Luft", "aus", "kla\u00b7ren", "A\u00b7e\u00b7ther\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Die mit ihrem frischen Wehen", "tokens": ["Die", "mit", "ih\u00b7rem", "fri\u00b7schen", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie des Morgens Hauch belebt!", "tokens": ["Wie", "des", "Mor\u00b7gens", "Hauch", "be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tief ist deine Macht begr\u00fcndet,", "tokens": ["Tief", "ist", "dei\u00b7ne", "Macht", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einz'ges Band, das uns verbindet", "tokens": ["Einz'\u00b7ges", "Band", ",", "das", "uns", "ver\u00b7bin\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so fest zusammenh\u00e4lt,", "tokens": ["Und", "so", "fest", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df von Deutschlands Ruhm und Leide", "tokens": ["Da\u00df", "von", "Deutschlands", "Ruhm", "und", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN", "KON", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Jenseits der Atlantis Scheide", "tokens": ["Jen\u00b7seits", "der", "At\u00b7lan\u00b7tis", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Deutsche Herzen noch geschwellt!", "tokens": ["Deut\u00b7sche", "Her\u00b7zen", "noch", "ge\u00b7schwellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}