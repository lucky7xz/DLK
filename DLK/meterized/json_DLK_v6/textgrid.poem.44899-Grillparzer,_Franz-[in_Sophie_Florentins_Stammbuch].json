{"textgrid.poem.44899": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in Sophie Florentins Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Ungl\u00fcck fr\u00fch dem Vaterhaus entwandt,", "tokens": ["Durch", "Un\u00b7gl\u00fcck", "fr\u00fch", "dem", "Va\u00b7ter\u00b7haus", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fliehst du nun auch dein m\u00fctterliches Land,", "tokens": ["Fliehst", "du", "nun", "auch", "dein", "m\u00fct\u00b7ter\u00b7li\u00b7ches", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Halb froh und wieder halb mit stillen Schmerzen.", "tokens": ["Halb", "froh", "und", "wie\u00b7der", "halb", "mit", "stil\u00b7len", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Getrost! Es bleibt ein unverg\u00e4nglich Band,", "tokens": ["Ge\u00b7trost", "!", "Es", "bleibt", "ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Band", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn nicht an einem Ort liegt unser Vaterland,", "tokens": ["Denn", "nicht", "an", "ei\u00b7nem", "Ort", "liegt", "un\u00b7ser", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es liegt im Umkreis uns verwandter Herzen.", "tokens": ["Es", "liegt", "im", "Um\u00b7kreis", "uns", "ver\u00b7wand\u00b7ter", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}