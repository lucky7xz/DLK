{"dta.poem.2891": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weltklugheit r\u00e4th dir an: verachte keinen Mann!", "tokens": ["Welt\u00b7klug\u00b7heit", "r\u00e4\u00b7th", "dir", "an", ":", "ver\u00b7ach\u00b7te", "kei\u00b7nen", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Du wei\u00dft nicht, wie er dir noch n\u00fctzen, schaden kann.", "tokens": ["Du", "wei\u00dft", "nicht", ",", "wie", "er", "dir", "noch", "n\u00fct\u00b7zen", ",", "scha\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "ADV", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Liebe gibt dir ein: lieb' alles gro\u00df und klein!", "tokens": ["Die", "Lie\u00b7be", "gibt", "dir", "ein", ":", "lieb'", "al\u00b7les", "gro\u00df", "und", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der h\u00f6chsten Liebe werth wirst du dadurch allein.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Lie\u00b7be", "werth", "wirst", "du", "da\u00b7durch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "PPER", "PAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O sieh, den Streit der Welt vers\u00f6hnt ein Gotteshauch!", "tokens": ["O", "sieh", ",", "den", "Streit", "der", "Welt", "ver\u00b7s\u00f6hnt", "ein", "Got\u00b7tes\u00b7hauch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Himmelsliebe hat, der hat Weltklugheit auch.", "tokens": ["Wer", "Him\u00b7mels\u00b7lie\u00b7be", "hat", ",", "der", "hat", "Welt\u00b7klug\u00b7heit", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "PRELS", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}