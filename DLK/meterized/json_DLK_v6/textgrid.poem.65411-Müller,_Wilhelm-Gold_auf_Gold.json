{"textgrid.poem.65411": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gold auf Gold", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was will der goldne Reif in deinem Haar?", "tokens": ["Was", "will", "der", "gold\u00b7ne", "Reif", "in", "dei\u00b7nem", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O sieh, er m\u00f6chte sich so gern verstecken", "tokens": ["O", "sieh", ",", "er", "m\u00f6ch\u00b7te", "sich", "so", "gern", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VMFIN", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sich mit deinem Golde \u00fcberdecken,", "tokens": ["Und", "sich", "mit", "dei\u00b7nem", "Gol\u00b7de", "\u00fc\u00b7berd\u00b7e\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn seines ist nicht halb so licht und klar.", "tokens": ["Denn", "sei\u00b7nes", "ist", "nicht", "halb", "so", "licht", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit feinem Gold wird grobes \u00fcberzogen:", "tokens": ["Mit", "fei\u00b7nem", "Gold", "wird", "gro\u00b7bes", "\u00fc\u00b7berz\u00b7o\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So zieh' dein Haar um diesen armen Bogen.", "tokens": ["So", "zieh'", "dein", "Haar", "um", "die\u00b7sen", "ar\u00b7men", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}