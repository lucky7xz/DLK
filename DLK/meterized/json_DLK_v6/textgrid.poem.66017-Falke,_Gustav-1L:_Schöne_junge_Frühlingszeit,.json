{"textgrid.poem.66017": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6ne junge Fr\u00fchlingszeit,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne junge Fr\u00fchlingszeit,", "tokens": ["Sch\u00f6\u00b7ne", "jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leerst dein F\u00fcllhorn auf mich nieder,", "tokens": ["Leerst", "dein", "F\u00fcll\u00b7horn", "auf", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebst der Seele Fl\u00fcgel wieder", "tokens": ["Giebst", "der", "See\u00b7le", "Fl\u00fc\u00b7gel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Liedern Munterkeit.", "tokens": ["Und", "den", "Lie\u00b7dern", "Mun\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun der letzte Schnee zerweicht,", "tokens": ["Nun", "der", "letz\u00b7te", "Schnee", "zer\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Busch und Baum in S\u00e4ften schwellen,", "tokens": ["Busch", "und", "Baum", "in", "S\u00e4f\u00b7ten", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, in all den frischen Quellen", "tokens": ["Ach", ",", "in", "all", "den", "fri\u00b7schen", "Quel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Baden sich die Sinne leicht.", "tokens": ["Ba\u00b7den", "sich", "die", "Sin\u00b7ne", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Liebe kommt auf Zeh'n,", "tokens": ["Und", "die", "Lie\u00b7be", "kommt", "auf", "Zeh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein K\u00e4tzchen, hinterm R\u00fccken:", "tokens": ["Wie", "ein", "K\u00e4tz\u00b7chen", ",", "hin\u00b7term", "R\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm, wir wollen Veilchen pfl\u00fccken.", "tokens": ["Komm", ",", "wir", "wol\u00b7len", "Veil\u00b7chen", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es giebt kein Widerstehn.", "tokens": ["Und", "es", "giebt", "kein", "Wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}