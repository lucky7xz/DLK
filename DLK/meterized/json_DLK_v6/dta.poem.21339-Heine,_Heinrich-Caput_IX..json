{"dta.poem.21339": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie blickte mich an so bedeutungsvoll,", "tokens": ["Sie", "blick\u00b7te", "mich", "an", "so", "be\u00b7deu\u00b7tungs\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So innig, so treu, so wehe!", "tokens": ["So", "in\u00b7nig", ",", "so", "treu", ",", "so", "we\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Besa\u00df eine sch\u00f6ne Seele gewi\u00df,", "tokens": ["Be\u00b7sa\u00df", "ei\u00b7ne", "sch\u00f6\u00b7ne", "See\u00b7le", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch war das Fleisch sehr z\u00e4he.", "tokens": ["Doch", "war", "das", "Fleisch", "sehr", "z\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch einen Schweinskopf trug man auf", "tokens": ["Auch", "ei\u00b7nen", "Schweins\u00b7kopf", "trug", "man", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einer zinnernen Sch\u00fcssel;", "tokens": ["In", "ei\u00b7ner", "zin\u00b7ner\u00b7nen", "Sch\u00fcs\u00b7sel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch immer schm\u00fcckt man den Schweinen bei uns", "tokens": ["Noch", "im\u00b7mer", "schm\u00fcckt", "man", "den", "Schwei\u00b7nen", "bei", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Lorbeerbl\u00e4ttern den R\u00fcssel.", "tokens": ["Mit", "Lor\u00b7beer\u00b7bl\u00e4t\u00b7tern", "den", "R\u00fcs\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}