{"textgrid.poem.63075": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Fieber", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00d6fter kommen Chausseearbeiter", "tokens": ["\u00d6f\u00b7ter", "kom\u00b7men", "Chaus\u00b7see\u00b7ar\u00b7bei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und hacken Steine klein.", "tokens": ["Und", "ha\u00b7cken", "Stei\u00b7ne", "klein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und stellen eine Leiter", "tokens": ["Und", "stel\u00b7len", "ei\u00b7ne", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An und klopfen die Steine in meinen Sch\u00e4del ein.", "tokens": ["An", "und", "klop\u00b7fen", "die", "Stei\u00b7ne", "in", "mei\u00b7nen", "Sch\u00e4\u00b7del", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der wird wie eine Stra\u00dfe so hart,", "tokens": ["Der", "wird", "wie", "ei\u00b7ne", "Stra\u00b7\u00dfe", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00dcber die eine Trambahn, eine Mistfuhre, ein Leichenwagen knarrt.", "tokens": ["\u00dc\u00b7ber", "die", "ei\u00b7ne", "Tram\u00b7bahn", ",", "ei\u00b7ne", "Mist\u00b7fuh\u00b7re", ",", "ein", "Lei\u00b7chen\u00b7wa\u00b7gen", "knarrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+---+-+-+", "measure": "iambic.octa.plus.invert"}}}}}