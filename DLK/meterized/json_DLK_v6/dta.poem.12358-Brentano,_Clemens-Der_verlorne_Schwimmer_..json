{"dta.poem.12358": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der verlorne Schwimmer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wirbt ein sch\u00f6ner Knabe               ", "tokens": ["Es", "wirbt", "ein", "sch\u00f6\u00b7ner", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da \u00fcberm breiten See,", "tokens": ["Da", "\u00fc\u00b7berm", "brei\u00b7ten", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um eines K\u00f6nigs Tochter,", "tokens": ["Um", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Leid geschah ihm Weh.", "tokens": ["Nach", "Leid", "ge\u00b7schah", "ihm", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201each Knabe, lieber Buhle,", "tokens": ["\u201e", "ach", "Kna\u00b7be", ",", "lie\u00b7ber", "Buh\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ewie gern w\u00e4r ich bey dir,", "tokens": ["\u201e", "wie", "gern", "w\u00e4r", "ich", "bey", "dir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eso flie\u00dfen nun zwey Wasser", "tokens": ["\u201e", "so", "flie\u00b7\u00dfen", "nun", "zwey", "Was\u00b7ser"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewohl zwischen mir und dir.", "tokens": ["\u201e", "wohl", "zwi\u00b7schen", "mir", "und", "dir", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201edas eine sind die Thr\u00e4nen,", "tokens": ["\u201e", "das", "ei\u00b7ne", "sind", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edas andre ist der See,", "tokens": ["\u201e", "das", "and\u00b7re", "ist", "der", "See", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees wird von meinen Thr\u00e4nen,", "tokens": ["\u201e", "es", "wird", "von", "mei\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewohl tiefer noch der See.\u201c", "tokens": ["\u201e", "wohl", "tie\u00b7fer", "noch", "der", "See", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja wie auf dem Pokale", "tokens": ["Ja", "wie", "auf", "dem", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Spiel ein Lichtlein schwebt,", "tokens": ["Zum", "Spiel", "ein", "Licht\u00b7lein", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn es beim hohen Mahle,", "tokens": ["Wenn", "es", "beim", "ho\u00b7hen", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf K\u00f6nigs Wohlseyn geht.", "tokens": ["Auf", "K\u00f6\u00b7nigs", "Wohl\u00b7seyn", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So setzt sie auf das Wasser", "tokens": ["So", "setzt", "sie", "auf", "das", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Licht auf leichtes Holz,", "tokens": ["Ein", "Licht", "auf", "leich\u00b7tes", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das treibet Wind und Wasser,", "tokens": ["Das", "trei\u00b7bet", "Wind", "und", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihrem Buhlen stolz.", "tokens": ["Zu", "ih\u00b7rem", "Buh\u00b7len", "stolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Als der es aufgefangen,", "tokens": ["Als", "der", "es", "auf\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er rief aus voller Brust:", "tokens": ["Er", "rief", "aus", "vol\u00b7ler", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201emein Stern ist aufgegangen,", "tokens": ["\u201e", "mein", "Stern", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eich schiff ihm nach mit Lust.\u201c", "tokens": ["\u201e", "ich", "schiff", "ihm", "nach", "mit", "Lust", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Lichtlein auf den H\u00e4nden,", "tokens": ["Das", "Licht\u00b7lein", "auf", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er schwamm zum Liebchen her,", "tokens": ["Er", "schwamm", "zum", "Lieb\u00b7chen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo mag er hin sich wenden,", "tokens": ["Wo", "mag", "er", "hin", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich seh sein Licht nicht mehr?", "tokens": ["Ich", "seh", "sein", "Licht", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Liegt er in ihrem Schoo\u00dfe,", "tokens": ["Liegt", "er", "in", "ih\u00b7rem", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Lichtlein wendet ab?", "tokens": ["Sein", "Licht\u00b7lein", "wen\u00b7det", "ab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liegt er im Wasserschlosse,", "tokens": ["Liegt", "er", "im", "Was\u00b7ser\u00b7schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "In einem nassen Grab?", "tokens": ["In", "ei\u00b7nem", "nas\u00b7sen", "Grab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}