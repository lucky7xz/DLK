{"textgrid.poem.40869": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was schmettert die Trompete? Das ist der Fritz von Horn:", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schmettert die Trompete? Das ist der Fritz von Horn:", "tokens": ["Was", "schmet\u00b7tert", "die", "Trom\u00b7pe\u00b7te", "?", "Das", "ist", "der", "Fritz", "von", "Horn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "NE", "APPR", "NE", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbmein F\u00fcrst, ich habe geb\u00e4ndigt des grimmen Ebers Zorn.\u00ab", "tokens": ["\u00bb", "mein", "F\u00fcrst", ",", "ich", "ha\u00b7be", "ge\u00b7b\u00e4n\u00b7digt", "des", "grim\u00b7men", "E\u00b7bers", "Zorn", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer naht mit fliegenden Fahnen? Von Nassau Herr Engelbrecht:", "tokens": ["Wer", "naht", "mit", "flie\u00b7gen\u00b7den", "Fah\u00b7nen", "?", "Von", "Nas\u00b7sau", "Herr", "En\u00b7gel\u00b7brecht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$.", "APPR", "NE", "NN", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbmein F\u00fcrst, ", "tokens": ["\u00bb", "mein", "F\u00fcrst", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Was deutet der Ruf der Glocken, der von den Th\u00fcrmen klingt?", "tokens": ["Was", "deu\u00b7tet", "der", "Ruf", "der", "Glo\u00b7cken", ",", "der", "von", "den", "Th\u00fcr\u00b7men", "klingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Stadt Gent auf sammt'nem Kissen die goldnen Schl\u00fcssel bringt.", "tokens": ["Stadt", "Gent", "auf", "sammt'\u00b7nem", "Kis\u00b7sen", "die", "gold\u00b7nen", "Schl\u00fcs\u00b7sel", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Max, was gl\u00fcht dein Antlitz, was zittert dein starker Arm?", "tokens": ["O", "Max", ",", "was", "gl\u00fcht", "dein", "Ant\u00b7litz", ",", "was", "zit\u00b7tert", "dein", "star\u00b7ker", "Arm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dein Sohn h\u00e4ngt dir nun wieder an Mund und Busen warm!", "tokens": ["Dein", "Sohn", "h\u00e4ngt", "dir", "nun", "wie\u00b7der", "an", "Mund", "und", "Bu\u00b7sen", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Gl\u00fcck auf! Wie reich vom Auge der Freudenquell ihm springt!", "tokens": ["Gl\u00fcck", "auf", "!", "Wie", "reich", "vom", "Au\u00b7ge", "der", "Freu\u00b7den\u00b7quell", "ihm", "springt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie er das Kn\u00e4blein k\u00fcsset, wie froh er's herzt und schwingt!", "tokens": ["Wie", "er", "das", "Kn\u00e4\u00b7blein", "k\u00fcs\u00b7set", ",", "wie", "froh", "er's", "herzt", "und", "schwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbei, Bube, sag', was gl\u00e4nzt dir am Haupte wie Heiligenglanz?", "tokens": ["\u00bb", "ei", ",", "Bu\u00b7be", ",", "sag'", ",", "was", "gl\u00e4nzt", "dir", "am", "Haup\u00b7te", "wie", "Hei\u00b7li\u00b7gen\u00b7glanz", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "KOKOM", "NE", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sind's deines Vaters Thr\u00e4nen? ist es dein Perlenkranz?\u00ab", "tokens": ["Sin\u00b7d's", "dei\u00b7nes", "Va\u00b7ters", "Thr\u00e4\u00b7nen", "?", "ist", "es", "dein", "Per\u00b7len\u00b7kranz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$.", "VAFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbein Narre darf nicht weinen!\u00ab denkt Kunz im Hintergrund;", "tokens": ["\u00bb", "ein", "Nar\u00b7re", "darf", "nicht", "wei\u00b7nen", "!", "\u00ab", "denkt", "Kunz", "im", "Hin\u00b7ter\u00b7grund", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$.", "$(", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch eine salz'ge Thr\u00e4ne rinnt ihm dabei in den Mund.", "tokens": ["Doch", "ei\u00b7ne", "sa\u00b7lz'\u00b7ge", "Thr\u00e4\u00b7ne", "rinnt", "ihm", "da\u00b7bei", "in", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PAV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+++-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "\u00bbsingt einst von unsern Thaten ein S\u00e4nger sein Gedicht,", "tokens": ["\u00bb", "singt", "einst", "von", "un\u00b7sern", "Tha\u00b7ten", "ein", "S\u00e4n\u00b7ger", "sein", "Ge\u00b7dicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Fehlt's doch dem Gauch an Thr\u00e4nen und R\u00fchrung mind'stens nicht.\u00ab", "tokens": ["Fehlt's", "doch", "dem", "Gauch", "an", "Thr\u00e4\u00b7nen", "und", "R\u00fch\u00b7rung", "min\u00b7d'\u00b7stens", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}}}}