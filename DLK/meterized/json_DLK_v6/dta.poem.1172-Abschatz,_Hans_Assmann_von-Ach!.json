{"dta.poem.1172": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ach!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "en:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst/ was sagen will di\u00df Ach!", "tokens": ["Du", "fragst", "/", "was", "sa\u00b7gen", "will", "di\u00df", "Ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "VVINF", "VMFIN", "PDS", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ich bey deiner Ankunfft sprach?", "tokens": ["Das", "ich", "bey", "dei\u00b7ner", "An\u00b7kunfft", "sprach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sprach: Ach! seht die holden Wangen/", "tokens": ["Es", "sprach", ":", "Ach", "!", "seht", "die", "hol\u00b7den", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "$.", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seht die beliebte Fillis an;", "tokens": ["Seht", "die", "be\u00b7lieb\u00b7te", "Fil\u00b7lis", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kommt auff Rosen-voller Bahn", "tokens": ["Da", "kommt", "auff", "Ro\u00b7sen\u00b7vol\u00b7ler", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Tod/ mein s\u00fcsser Tod/ gegangen.", "tokens": ["Mein", "Tod", "/", "mein", "s\u00fcs\u00b7ser", "Tod", "/", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}