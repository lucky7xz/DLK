{"textgrid.poem.50832": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Einkehr unterhalb des Rheinfalls", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da rauscht das gr\u00fcne Wogenband", "tokens": ["Da", "rauscht", "das", "gr\u00fc\u00b7ne", "Wo\u00b7gen\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Rheines Wald und Au entlang:", "tokens": ["Des", "Rhei\u00b7nes", "Wald", "und", "Au", "ent\u00b7lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jenseits mein lieb Badenserland,", "tokens": ["Jen\u00b7seits", "mein", "lieb", "Ba\u00b7den\u00b7ser\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hier schon Schweizer Felsenhang!", "tokens": ["Und", "hier", "schon", "Schwei\u00b7zer", "Fel\u00b7sen\u00b7hang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da zieht er hin, aus tiefer Brust", "tokens": ["Da", "zieht", "er", "hin", ",", "aus", "tie\u00b7fer", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit langsam stolzem Odemzug;", "tokens": ["Mit", "lang\u00b7sam", "stol\u00b7zem", "O\u00b7dem\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber ihm spielt Sonnenlust", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "spielt", "Son\u00b7nen\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Eichenrauschen, Falkenflug!", "tokens": ["Und", "Ei\u00b7chen\u00b7rau\u00b7schen", ",", "Fal\u00b7ken\u00b7flug", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Schlo\u00df, kein Dom ist in der N\u00e4h,", "tokens": ["Kein", "Schlo\u00df", ",", "kein", "Dom", "ist", "in", "der", "N\u00e4h", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur W\u00e4lder schauen in die Flut;", "tokens": ["Nur", "W\u00e4l\u00b7der", "schau\u00b7en", "in", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Deutschland schwimmt ein zitternd Reh", "tokens": ["Von", "Deutschland", "schwimmt", "ein", "zit\u00b7ternd", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Her\u00fcber, wo es \u2013 auch nicht ruht!", "tokens": ["Her\u00b7\u00fc\u00b7ber", ",", "wo", "es", "\u2013", "auch", "nicht", "ruht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "$(", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und in der Stromeseinsamkeit", "tokens": ["Und", "in", "der", "Stro\u00b7me\u00b7sein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verge\u00df ich all den alten Span,", "tokens": ["Ver\u00b7ge\u00df", "ich", "all", "den", "al\u00b7ten", "Span", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versenke den verj\u00e4hrten Streit", "tokens": ["Ver\u00b7sen\u00b7ke", "den", "ver\u00b7j\u00e4hr\u00b7ten", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und hebe hell zu singen an:", "tokens": ["Und", "he\u00b7be", "hell", "zu", "sin\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwohl mir, da\u00df ich dich endlich fand,", "tokens": ["\u00bb", "wohl", "mir", ",", "da\u00df", "ich", "dich", "end\u00b7lich", "fand", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du stiller Ort am alten Rhein,", "tokens": ["Du", "stil\u00b7ler", "Ort", "am", "al\u00b7ten", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ungest\u00f6rt und ungekannt", "tokens": ["Wo", "un\u00b7ge\u00b7st\u00f6rt", "und", "un\u00b7ge\u00b7kannt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich Schweizer darf ", "tokens": ["Ich", "Schwei\u00b7zer", "darf"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wo ich hin\u00fcber rufen mag,", "tokens": ["Wo", "ich", "hin\u00b7\u00fc\u00b7ber", "ru\u00b7fen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was freudig mir das Herz bewegt,", "tokens": ["Was", "freu\u00b7dig", "mir", "das", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo der klare Wellenschlag", "tokens": ["Und", "wo", "der", "kla\u00b7re", "Wel\u00b7len\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Widerhall zur\u00fcck mir tr\u00e4gt!", "tokens": ["Den", "Wi\u00b7der\u00b7hall", "zu\u00b7r\u00fcck", "mir", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O steigt zum Himmel, Lied und Wort!", "tokens": ["O", "steigt", "zum", "Him\u00b7mel", ",", "Lied", "und", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt jubelnd ob dem tiefen Rhein!", "tokens": ["Schwebt", "ju\u00b7belnd", "ob", "dem", "tie\u00b7fen", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "KOUS", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier ist ein stiller Freiheitsport,", "tokens": ["Hier", "ist", "ein", "stil\u00b7ler", "Frei\u00b7heits\u00b7port", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier sind wir mit dem Rhein allein!\u00ab", "tokens": ["Hier", "sind", "wir", "mit", "dem", "Rhein", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NE", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da raschelt's dr\u00fcben, und der Scherg", "tokens": ["Da", "ra\u00b7schelt's", "dr\u00fc\u00b7ben", ",", "und", "der", "Scherg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauscht zweigef\u00e4rbt durchs dunkle Gr\u00fcn \u2013", "tokens": ["Lauscht", "zwei\u00b7ge\u00b7f\u00e4rbt", "durchs", "dunk\u00b7le", "Gr\u00fcn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fliehe schnell hinan den Berg:", "tokens": ["Ich", "flie\u00b7he", "schnell", "hi\u00b7nan", "den", "Berg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du stiller Ort am Rhein \u2013 fahr hin!", "tokens": ["Du", "stil\u00b7ler", "Ort", "am", "Rhein", "\u2013", "fahr", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NE", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}