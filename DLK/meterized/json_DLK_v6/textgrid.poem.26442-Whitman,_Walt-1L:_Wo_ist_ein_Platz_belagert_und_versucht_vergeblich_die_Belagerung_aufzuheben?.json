{"textgrid.poem.26442": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo ist ein Platz belagert und versucht vergeblich die Belagerung aufzuheben?", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist ein Platz belagert und versucht vergeblich die Belagerung aufzuheben?", "tokens": ["Wo", "ist", "ein", "Platz", "be\u00b7la\u00b7gert", "und", "ver\u00b7sucht", "ver\u00b7geb\u00b7lich", "die", "Be\u00b7la\u00b7ge\u00b7rung", "auf\u00b7zu\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "KON", "VVFIN", "ADJD", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ho! Dahin sende ich einen Kommandanten \u2013 geschwind, tapfer und unsterblich,", "tokens": ["Ho", "!", "Da\u00b7hin", "sen\u00b7de", "ich", "ei\u00b7nen", "Kom\u00b7man\u00b7dan\u00b7ten", "\u2013", "ge\u00b7schwind", ",", "tap\u00b7fer", "und", "uns\u00b7terb\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PAV", "VVFIN", "PPER", "ART", "NN", "$(", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+--+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Reiter und Fu\u00dfvolk, Kanonen und Kanoniere,", "tokens": ["Rei\u00b7ter", "und", "Fu\u00df\u00b7volk", ",", "Ka\u00b7no\u00b7nen", "und", "Ka\u00b7no\u00b7nie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Die furchtbarsten, die jemals Feuer gaben!", "tokens": ["Die", "furcht\u00b7bars\u00b7ten", ",", "die", "je\u00b7mals", "Feu\u00b7er", "ga\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}