"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind des Todes Geschmeid.", "tokens": ["Sind", "des", "To\u00b7des", "Ge\u00b7schmeid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Alle Blumen, die sprie\u00dfen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", ",", "die", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sind ein l\u00e4chelnder Schmerz;", "tokens": ["Sind", "ein", "l\u00e4\u00b7cheln\u00b7der", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit L\u00e4cheln will sich erschlie\u00dfen", "tokens": ["Mit", "L\u00e4\u00b7cheln", "will", "sich", "er\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Fr\u00fchlings beklommnes Herz.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "be\u00b7klomm\u00b7nes", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit L\u00e4cheln will sich erschlie\u00dfen", "tokens": ["Mit", "L\u00e4\u00b7cheln", "will", "sich", "er\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Herz, das beklommene;", "tokens": ["Das", "Herz", ",", "das", "be\u00b7klom\u00b7me\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Wie bald mu\u00df es verdrie\u00dfen", "tokens": ["Wie", "bald", "mu\u00df", "es", "ver\u00b7drie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Unternommene.", "tokens": ["Das", "Un\u00b7ter\u00b7nom\u00b7me\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Wie bald mu\u00df es verdrie\u00dfen,", "tokens": ["Wie", "bald", "mu\u00df", "es", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was s\u00fc\u00dfe Lust war vorher!", "tokens": ["Was", "s\u00fc\u00b7\u00dfe", "Lust", "war", "vor\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Alle W\u00e4sserlein flie\u00dfen", "tokens": ["Al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In das bittere Meer.", "tokens": ["In", "das", "bit\u00b7te\u00b7re", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}