{"dta.poem.11917": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das heilige Sanskrit, das vorl\u00e4ngst sich verloren", "tokens": ["Das", "hei\u00b7li\u00b7ge", "Sans\u00b7krit", ",", "das", "vor\u00b7l\u00e4ngst", "sich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Menschenmunde, nennt drei Dinge zweigeboren.", "tokens": ["Vom", "Men\u00b7schen\u00b7mun\u00b7de", ",", "nennt", "drei", "Din\u00b7ge", "zwei\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zuerst den Vogel, der als Ei zum erstenmale", "tokens": ["Zu\u00b7erst", "den", "Vo\u00b7gel", ",", "der", "als", "Ei", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NE", "$,", "PRELS", "KOUS", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geboren ist, sodann zum andern aus der Schale.", "tokens": ["Ge\u00b7bo\u00b7ren", "ist", ",", "so\u00b7dann", "zum", "an\u00b7dern", "aus", "der", "Scha\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ADV", "APPRART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann nennt es so den Zahn, der in des Menschen Munde", "tokens": ["Dann", "nennt", "es", "so", "den", "Zahn", ",", "der", "in", "des", "Men\u00b7schen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst schw\u00e4cher einmal w\u00e4chst, und dann auf festerm Grunde.", "tokens": ["Erst", "schw\u00e4\u00b7cher", "ein\u00b7mal", "w\u00e4chst", ",", "und", "dann", "auf", "fes\u00b7term", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$,", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zum dritten nennt es so den Weisen, den zumeist", "tokens": ["Zum", "drit\u00b7ten", "nennt", "es", "so", "den", "Wei\u00b7sen", ",", "den", "zu\u00b7meist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter erst gebar, und wieder dann der Geist.", "tokens": ["Die", "Mut\u00b7ter", "erst", "ge\u00b7bar", ",", "und", "wie\u00b7der", "dann", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Vogel, zweimal nicht geboren, bleibt im Nest,", "tokens": ["Der", "Vo\u00b7gel", ",", "zwei\u00b7mal", "nicht", "ge\u00b7bo\u00b7ren", ",", "bleibt", "im", "Nest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ADV", "PTKNEG", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zweimal nicht der Zahn geboren steht nicht fest.", "tokens": ["Und", "zwei\u00b7mal", "nicht", "der", "Zahn", "ge\u00b7bo\u00b7ren", "steht", "nicht", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ART", "NN", "VVPP", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Weise wird nicht fest in Weisheit stehn noch fliegen,", "tokens": ["Der", "Wei\u00b7se", "wird", "nicht", "fest", "in", "Weis\u00b7heit", "stehn", "noch", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er der anderen Geburt nicht ist entstiegen.", "tokens": ["Wenn", "er", "der", "an\u00b7de\u00b7ren", "Ge\u00b7burt", "nicht", "ist", "ent\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}