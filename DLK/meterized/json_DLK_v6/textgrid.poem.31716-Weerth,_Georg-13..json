{"textgrid.poem.31716": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Feste lief ich wohl", "tokens": ["Zu", "Fes\u00b7te", "lief", "ich", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von hier bis nach Tirol,", "tokens": ["Von", "hier", "bis", "nach", "Ti\u00b7rol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich lief drei Meilen weiter,", "tokens": ["Ich", "lief", "drei", "Mei\u00b7len", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich liefe froh und heiter", "tokens": ["Ich", "lie\u00b7fe", "froh", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr eine Kanne Wein", "tokens": ["F\u00fcr", "ei\u00b7ne", "Kan\u00b7ne", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bis in den Mond hinein.", "tokens": ["Bis", "in", "den", "Mond", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "W\u00e4r ich ein hohes Tier", "tokens": ["W\u00e4r", "ich", "ein", "ho\u00b7hes", "Tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und h\u00f6rte alles mir,", "tokens": ["Und", "h\u00f6r\u00b7te", "al\u00b7les", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und t\u00e4t in meinen Reichen", "tokens": ["Und", "t\u00e4t", "in", "mei\u00b7nen", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne nie erbleichen \u2013", "tokens": ["Die", "Son\u00b7ne", "nie", "er\u00b7blei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ger\u00e4t' am Rhein die Rebe nicht,", "tokens": ["Ge\u00b7r\u00e4t'", "am", "Rhein", "die", "Re\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich war ein armer Wicht.", "tokens": ["Ich", "war", "ein", "ar\u00b7mer", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}