{"textgrid.poem.32299": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00fcsse", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Neid, o Kind,", "tokens": ["Der", "Neid", ",", "o", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Z\u00e4hlt unsre K\u00fcsse:", "tokens": ["Z\u00e4hlt", "uns\u00b7re", "K\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Drum k\u00fc\u00df' geschwind", "tokens": ["Drum", "k\u00fc\u00df'", "ge\u00b7schwind"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ein Tausend K\u00fcsse;", "tokens": ["Ein", "Tau\u00b7send", "K\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Geschwind du mich,", "tokens": ["Ge\u00b7schwind", "du", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Geschwind ich dich!", "tokens": ["Ge\u00b7schwind", "ich", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Geschwind, geschwind,", "tokens": ["Ge\u00b7schwind", ",", "ge\u00b7schwind", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "O Laura, k\u00fcsse", "tokens": ["O", "Lau\u00b7ra", ",", "k\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Manch Tausend K\u00fcsse:", "tokens": ["Manch", "Tau\u00b7send", "K\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Damit er sich", "tokens": ["Da\u00b7mit", "er", "sich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Verz\u00e4hlen m\u00fcsse.", "tokens": ["Ver\u00b7z\u00e4h\u00b7len", "m\u00fcs\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}