{"textgrid.poem.28961": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was Cassandra propheceyte,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Cassandra propheceyte,", "tokens": ["Was", "Cas\u00b7sand\u00b7ra", "pro\u00b7phe\u00b7cey\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ward geh\u00f6rt und nicht geglaubt.", "tokens": ["Ward", "ge\u00b7h\u00f6rt", "und", "nicht", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Falschheit ist bey Hof erlaubt;", "tokens": ["Falschheit", "ist", "bey", "Hof", "er\u00b7laubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Warheit treibt man auff die Seite.", "tokens": ["War\u00b7heit", "treibt", "man", "auff", "die", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}