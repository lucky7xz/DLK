{"textgrid.poem.25551": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! gelassen, nicht mit Grimme,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! gelassen, nicht mit Grimme,", "tokens": ["Ach", "!", "ge\u00b7las\u00b7sen", ",", "nicht", "mit", "Grim\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVPP", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bitt' ich dich noch einmal um Geh\u00f6r;", "tokens": ["Bitt'", "ich", "dich", "noch", "ein\u00b7mal", "um", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "PRF", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oder kennst du meine Stimme,", "tokens": ["O\u00b7der", "kennst", "du", "mei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dir sonst bezaubernd klang, nicht mehr?", "tokens": ["Die", "dir", "sonst", "be\u00b7zau\u00b7bernd", "klang", ",", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VVFIN", "$,", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00fcrchtest du, der Schwermuth Klagen", "tokens": ["F\u00fcrch\u00b7test", "du", ",", "der", "Schwer\u00b7muth", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6chten dir am Herzen nagen?", "tokens": ["M\u00f6ch\u00b7ten", "dir", "am", "Her\u00b7zen", "na\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcrchte nichts! ich liebe dich zu sehr!", "tokens": ["F\u00fcrch\u00b7te", "nichts", "!", "ich", "lie\u00b7be", "dich", "zu", "sehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "PPER", "VVFIN", "PPER", "PTKA", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Kannst du itzt dich noch besinnen,", "tokens": ["Kannst", "du", "itzt", "dich", "noch", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Armes M\u00e4dchen, so besinne dich!", "tokens": ["Ar\u00b7mes", "M\u00e4d\u00b7chen", ",", "so", "be\u00b7sin\u00b7ne", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Glaube, meine Thr\u00e4nen rinnen", "tokens": ["Glau\u00b7be", ",", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "rin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mehr um deine Blindheit, als f\u00fcr mich.", "tokens": ["Mehr", "um", "dei\u00b7ne", "Blind\u00b7heit", ",", "als", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "KOUS", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Sey aus meinem Arm' entronnen,", "tokens": ["Sey", "aus", "mei\u00b7nem", "Arm'", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "St\u00fcrze nur nicht unbesonnen", "tokens": ["St\u00fcr\u00b7ze", "nur", "nicht", "un\u00b7be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ohn' Erretten in den Abgrund dich!", "tokens": ["Ohn'", "Er\u00b7ret\u00b7ten", "in", "den", "Ab\u00b7grund", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Welcher Trank hat deine Sinnen,", "tokens": ["Wel\u00b7cher", "Trank", "hat", "dei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Diese Sclaven, wider dich emp\u00f6rt?", "tokens": ["Die\u00b7se", "Scla\u00b7ven", ",", "wi\u00b7der", "dich", "em\u00b7p\u00f6rt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Gibt es itzt noch Zauberinnen,", "tokens": ["Gibt", "es", "itzt", "noch", "Zau\u00b7be\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Gab auch die Natur dem Weibe", "tokens": ["Gab", "auch", "die", "Na\u00b7tur", "dem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Schw\u00e4chern Geist, bei sch\u00f6nerm Leibe,", "tokens": ["Schw\u00e4\u00b7chern", "Geist", ",", "bei", "sch\u00f6\u00b7nerm", "Lei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dennoch ward so schwach er nicht gen\u00e4hrt.", "tokens": ["Den\u00b7noch", "ward", "so", "schwach", "er", "nicht", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Zwar die Liebe trotzt Barbaren", "tokens": ["Zwar", "die", "Lie\u00b7be", "trotzt", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Thr\u00e4nen f\u00fcr den Ku\u00df der Hirtin ab,", "tokens": ["Thr\u00e4\u00b7nen", "f\u00fcr", "den", "Ku\u00df", "der", "Hir\u00b7tin", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Lehret den Verschwender sparen,", "tokens": ["Leh\u00b7ret", "den", "Ver\u00b7schwen\u00b7der", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Oeffnet Geitzigen des Goldes Grab;", "tokens": ["Oeff\u00b7net", "Geit\u00b7zi\u00b7gen", "des", "Gol\u00b7des", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Bricht, wie Glas, durch zarte H\u00e4nde,", "tokens": ["Bricht", ",", "wie", "Glas", ",", "durch", "zar\u00b7te", "H\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "St\u00e4b' und Riegel; hohe W\u00e4nde", "tokens": ["St\u00e4b'", "und", "Rie\u00b7gel", ";", "ho\u00b7he", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Springt die Feigheit selbst durch sie herab.", "tokens": ["Springt", "die", "Feig\u00b7heit", "selbst", "durch", "sie", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "La\u00df sie mit den Thoren scherzen,", "tokens": ["La\u00df", "sie", "mit", "den", "Tho\u00b7ren", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Mit zwo edlen Seelen scherzt sie nicht!", "tokens": ["Mit", "zwo", "ed\u00b7len", "See\u00b7len", "scherzt", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Durch die Sympathie der Herzen", "tokens": ["Durch", "die", "Sym\u00b7pa\u00b7thie", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Lockt sie hier, und dort, mit dem Gesicht'.", "tokens": ["Lockt", "sie", "hier", ",", "und", "dort", ",", "mit", "dem", "Ge\u00b7sicht'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Was den stillen Hirsch emp\u00f6ret,", "tokens": ["Was", "den", "stil\u00b7len", "Hirsch", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Selbst was T\u00e4ubchen girren lehret,", "tokens": ["Selbst", "was", "T\u00e4ub\u00b7chen", "gir\u00b7ren", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Das verdient den Namen ", "tokens": ["Das", "ver\u00b7dient", "den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.36": {"text": "Nicht des ", "tokens": ["Nicht", "des"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Nicht der Wollust sey mein Herz geweiht.", "tokens": ["Nicht", "der", "Wol\u00b7lust", "sey", "mein", "Herz", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Aber etwas von dem allen", "tokens": ["A\u00b7ber", "et\u00b7was", "von", "dem", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "M\u00f6g' in meinem Blute wallen,", "tokens": ["M\u00f6g'", "in", "mei\u00b7nem", "Blu\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Wo die Tugend Ebb' und Flut gebeut.", "tokens": ["Wo", "die", "Tu\u00b7gend", "Ebb'", "und", "Flut", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Diese Mischung tr\u00e4nkt mit Freuden,", "tokens": ["Die\u00b7se", "Misc\u00b7hung", "tr\u00e4nkt", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Die von Tausenden nur Einer kennt.", "tokens": ["Die", "von", "Tau\u00b7sen\u00b7den", "nur", "Ei\u00b7ner", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Aber hatte nicht uns beiden", "tokens": ["A\u00b7ber", "hat\u00b7te", "nicht", "uns", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Die\u00df Geheimni\u00df die Natur geg\u00f6nnt?", "tokens": ["Die\u00df", "Ge\u00b7heim\u00b7ni\u00df", "die", "Na\u00b7tur", "ge\u00b7g\u00f6nnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Wird \u2013 wie soll ich wohl ihn nennen? \u2013", "tokens": ["Wird", "\u2013", "wie", "soll", "ich", "wohl", "ihn", "nen\u00b7nen", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$(", "PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Wird auch der es jemals kennen,", "tokens": ["Wird", "auch", "der", "es", "je\u00b7mals", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Welchem itzt dein Fu\u00df entgegen rennt?", "tokens": ["Wel\u00b7chem", "itzt", "dein", "Fu\u00df", "ent\u00b7ge\u00b7gen", "rennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.50": {"text": "Wird sein Herz wie Wachs zerflie\u00dfen?", "tokens": ["Wird", "sein", "Herz", "wie", "Wachs", "zer\u00b7flie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Wenn er ja noch deine Lieder liest!", "tokens": ["Wenn", "er", "ja", "noch", "dei\u00b7ne", "Lie\u00b7der", "liest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.52": {"text": "Wird sein Geist den Ku\u00df vers\u00fc\u00dfen,", "tokens": ["Wird", "sein", "Geist", "den", "Ku\u00df", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Den sein Mund von deinem Munde k\u00fc\u00dft?", "tokens": ["Den", "sein", "Mund", "von", "dei\u00b7nem", "Mun\u00b7de", "k\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.54": {"text": "Wird vor deinen Melodien", "tokens": ["Wird", "vor", "dei\u00b7nen", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Wohl sein Eigensinn entfliehen,", "tokens": ["Wohl", "sein", "Ei\u00b7gen\u00b7sinn", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wenn der Ekel seine Freuden fri\u00dft?", "tokens": ["Wenn", "der", "E\u00b7kel", "sei\u00b7ne", "Freu\u00b7den", "fri\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure":