{"textgrid.poem.56480": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie gr\u00fc\u00dfen dich in deinem schwarzen Sarge", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie gr\u00fc\u00dfen dich in deinem schwarzen Sarge", "tokens": ["Sie", "gr\u00fc\u00b7\u00dfen", "dich", "in", "dei\u00b7nem", "schwar\u00b7zen", "Sar\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und flattern \u00fcber dich wie Fr\u00fchlingswind.", "tokens": ["Und", "flat\u00b7tern", "\u00fc\u00b7ber", "dich", "wie", "Fr\u00fch\u00b7lings\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Nachtigallen r\u00fchren sie das karge,", "tokens": ["Wie", "Nach\u00b7ti\u00b7gal\u00b7len", "r\u00fch\u00b7ren", "sie", "das", "kar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wachsbleiche Haupt mit ihren Klagen lind.", "tokens": ["Wachs\u00b7blei\u00b7che", "Haupt", "mit", "ih\u00b7ren", "Kla\u00b7gen", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Sammeth\u00e4nden wollen sie dich gr\u00fc\u00dfen", "tokens": ["Mit", "Sam\u00b7me\u00b7th\u00e4n\u00b7den", "wol\u00b7len", "sie", "dich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von meiner Qual. Und wie ein Weinblatt rot,", "tokens": ["Von", "mei\u00b7ner", "Qual", ".", "Und", "wie", "ein", "Wein\u00b7blatt", "rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So taumeln ihre K\u00fcsse dir zu F\u00fc\u00dfen,", "tokens": ["So", "tau\u00b7meln", "ih\u00b7re", "K\u00fcs\u00b7se", "dir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ziehn wie Tauben sanft um deinen Tod.", "tokens": ["Und", "ziehn", "wie", "Tau\u00b7ben", "sanft", "um", "dei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KOKOM", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie schwingen \u00fcber dir die Fackelbr\u00e4nde,", "tokens": ["Sie", "schwin\u00b7gen", "\u00fc\u00b7ber", "dir", "die", "Fa\u00b7ckel\u00b7br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die furchtbar wecken auf die schwarze Nacht.", "tokens": ["Die", "furcht\u00b7bar", "we\u00b7cken", "auf", "die", "schwar\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie geben dir in deine wei\u00dfen H\u00e4nde", "tokens": ["Sie", "ge\u00b7ben", "dir", "in", "dei\u00b7ne", "wei\u00b7\u00dfen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00e4nen von Stein, die ich dir dargebracht.", "tokens": ["Tr\u00e4\u00b7nen", "von", "Stein", ",", "die", "ich", "dir", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Sie laden D\u00fcfte aus den Duft-Amphoren", "tokens": ["Sie", "la\u00b7den", "D\u00fcf\u00b7te", "aus", "den", "Duft\u00b7Am\u00b7pho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcbersch\u00fctten dich mit Ambra ganz.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten", "dich", "mit", "A\u00b7mbra", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein schwarzes Haar steht auf, an Himmels Toren,", "tokens": ["Dein", "schwar\u00b7zes", "Haar", "steht", "auf", ",", "an", "Him\u00b7mels", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eines Sterngew\u00f6lkes d\u00fcnner Glanz.", "tokens": ["Wie", "ei\u00b7nes", "Stern\u00b7ge\u00b7w\u00f6l\u00b7kes", "d\u00fcn\u00b7ner", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie werden gro\u00dfe Pyramiden bauen,", "tokens": ["Sie", "wer\u00b7den", "gro\u00b7\u00dfe", "Py\u00b7ra\u00b7mi\u00b7den", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darauf sie t\u00fcrmen deinen schwarzen Schrein.", "tokens": ["Da\u00b7rauf", "sie", "t\u00fcr\u00b7men", "dei\u00b7nen", "schwar\u00b7zen", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann wirst du in die wilde Sonne schauen,", "tokens": ["Dann", "wirst", "du", "in", "die", "wil\u00b7de", "Son\u00b7ne", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die in dein Blut st\u00fcrzt wie ein dunkler Wein.", "tokens": ["Die", "in", "dein", "Blut", "st\u00fcrzt", "wie", "ein", "dunk\u00b7ler", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}