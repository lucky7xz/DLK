{"dta.poem.3072": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "118.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch, r\u00fchme dich nicht stolz, da\u00df du ein Gut gewannst,", "tokens": ["Mensch", ",", "r\u00fch\u00b7me", "dich", "nicht", "stolz", ",", "da\u00df", "du", "ein", "Gut", "ge\u00b7wannst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du nicht wei\u00dft wiebald du es verlieren kannst.", "tokens": ["Weil", "du", "nicht", "wei\u00dft", "wie\u00b7bald", "du", "es", "ver\u00b7lie\u00b7ren", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch r\u00fchme dich nur nicht, da\u00df du ein Wissen hast;", "tokens": ["Auch", "r\u00fch\u00b7me", "dich", "nur", "nicht", ",", "da\u00df", "du", "ein", "Wis\u00b7sen", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer's nicht zu brauchen wei\u00df, dem ist es eine Last.", "tokens": ["Wer's", "nicht", "zu", "brau\u00b7chen", "wei\u00df", ",", "dem", "ist", "es", "ei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie leiblicher Besitz kann auch dein geistiger schwinden;", "tokens": ["Wie", "leib\u00b7li\u00b7cher", "Be\u00b7sitz", "kann", "auch", "dein", "geis\u00b7ti\u00b7ger", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VMFIN", "ADV", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann, wenn du sonst nichts hast, wirst du dich arm empfinden.", "tokens": ["Dann", ",", "wenn", "du", "sonst", "nichts", "hast", ",", "wirst", "du", "dich", "arm", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wenn du gut bist, das allein wird nie geraubt;", "tokens": ["Doch", "wenn", "du", "gut", "bist", ",", "das", "al\u00b7lein", "wird", "nie", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des r\u00fchme dich nicht, doch freu dich! das ist erlaubt.", "tokens": ["Des", "r\u00fch\u00b7me", "dich", "nicht", ",", "doch", "freu", "dich", "!", "das", "ist", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADJD", "PPER", "$.", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}