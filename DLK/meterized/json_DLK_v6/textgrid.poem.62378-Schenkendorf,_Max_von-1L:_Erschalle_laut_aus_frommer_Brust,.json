{"textgrid.poem.62378": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erschalle laut aus frommer Brust,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erschalle laut aus frommer Brust,", "tokens": ["Er\u00b7schal\u00b7le", "laut", "aus", "from\u00b7mer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O heller Klang der Lieder!", "tokens": ["O", "hel\u00b7ler", "Klang", "der", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr, der Ursprung unsrer Lust,", "tokens": ["Der", "Herr", ",", "der", "Ur\u00b7sprung", "uns\u00b7rer", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt segnend auf uns nieder.", "tokens": ["Blickt", "seg\u00b7nend", "auf", "uns", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der nimmer von den Seinen l\u00e4\u00dft,", "tokens": ["Der", "nim\u00b7mer", "von", "den", "Sei\u00b7nen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Mit reicher Huld gesegnet.", "tokens": ["Mit", "rei\u00b7cher", "Huld", "ge\u00b7seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der auf dem Friedensbogen stand,", "tokens": ["Der", "auf", "dem", "Frie\u00b7dens\u00b7bo\u00b7gen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als es genug geregnet,", "tokens": ["Als", "es", "ge\u00b7nug", "ge\u00b7reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der segnend in dem fremden Land", "tokens": ["Der", "seg\u00b7nend", "in", "dem", "frem\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Abraham begegnet,", "tokens": ["Dem", "Ab\u00b7ra\u00b7ham", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00e4\u00dft schon so viel tausend Jahr'", "tokens": ["Er", "l\u00e4\u00dft", "schon", "so", "viel", "tau\u00b7send", "Jahr'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die, welche sein wird, ist, und war,", "tokens": ["Die", ",", "wel\u00b7che", "sein", "wird", ",", "ist", ",", "und", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "VAINF", "VAFIN", "$,", "VAFIN", "$,", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ew'ge Liebe walten.", "tokens": ["Die", "ew'\u00b7ge", "Lie\u00b7be", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Komm' aus dem alten Cedernhain,", "tokens": ["Komm'", "aus", "dem", "al\u00b7ten", "Ce\u00b7dern\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Patriarchen-H\u00fctten,", "tokens": ["Aus", "Pa\u00b7tri\u00b7a\u00b7rchen\u00b7H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm', Einfalt, z\u00fcchtig, fromm und fein", "tokens": ["Komm'", ",", "Ein\u00b7falt", ",", "z\u00fcch\u00b7tig", ",", "fromm", "und", "fein"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit kindlich milden Sitten!", "tokens": ["Mit", "kind\u00b7lich", "mil\u00b7den", "Sit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df unser Leben, unsern Gang,", "tokens": ["La\u00df", "un\u00b7ser", "Le\u00b7ben", ",", "un\u00b7sern", "Gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies Haus auch bleiben dir zum Dank", "tokens": ["Dies", "Haus", "auch", "blei\u00b7ben", "dir", "zum", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Bild aus alten Zeiten.", "tokens": ["Ein", "Bild", "aus", "al\u00b7ten", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Vater, freundlich, stark und mild,", "tokens": ["O", "Va\u00b7ter", ",", "freund\u00b7lich", ",", "stark", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hier im Hause waltet,", "tokens": ["Der", "hier", "im", "Hau\u00b7se", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bist uns des ew'gen Vaters Bild,", "tokens": ["Bist", "uns", "des", "ew'\u00b7gen", "Va\u00b7ters", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nimmermehr veraltet.", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "ver\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bl\u00fche fort in Gottes St\u00e4rk',", "tokens": ["So", "bl\u00fc\u00b7he", "fort", "in", "Got\u00b7tes", "St\u00e4rk'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich r\u00fcstig stets zum frommen Werk,", "tokens": ["Gleich", "r\u00fcs\u00b7tig", "stets", "zum", "from\u00b7men", "Werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du theure Zier der Greise.", "tokens": ["Du", "theu\u00b7re", "Zier", "der", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Zeit geht ihren ernsten Gang,", "tokens": ["Die", "Zeit", "geht", "ih\u00b7ren", "erns\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Gott ist's, dem wir wallen;", "tokens": ["Doch", "Gott", "ist's", ",", "dem", "wir", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hallelujah, der Lobgesang,", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", ",", "der", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Soll ewig ihm erschallen!", "tokens": ["Soll", "e\u00b7wig", "ihm", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "F\u00fchr' lang noch seine Kinder an", "tokens": ["F\u00fchr'", "lang", "noch", "sei\u00b7ne", "Kin\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum s\u00fc\u00dfen Lobe Gottes.", "tokens": ["Zum", "s\u00fc\u00b7\u00dfen", "Lo\u00b7be", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}