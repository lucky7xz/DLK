{"dta.poem.1612": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Alrunens Warnung an Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Deutschland! mercke wohl/ was ich dir mit Verdru\u00df/", "tokens": ["Mein", "Deutschland", "!", "mer\u00b7cke", "wohl", "/", "was", "ich", "dir", "mit", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "$(", "PWS", "PPER", "PPER", "APPR", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "(doch hats der Himmel so verhangen) melden mu\u00df?", "tokens": ["(", "doch", "hats", "der", "Him\u00b7mel", "so", "ver\u00b7han\u00b7gen", ")", "mel\u00b7den", "mu\u00df", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$(", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Herman hat zwar izt der R\u00f6mer stoltze Pracht", "tokens": ["Dein", "Her\u00b7man", "hat", "zwar", "izt", "der", "R\u00f6\u00b7mer", "stolt\u00b7ze", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch wohlerfochtnen Sieg zu unsern F\u00fcssen bracht/", "tokens": ["Durch", "woh\u00b7ler\u00b7focht\u00b7nen", "Sieg", "zu", "un\u00b7sern", "F\u00fcs\u00b7sen", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die W\u00f6lffin ist erlegt durch deinen k\u00fchnen Arm/", "tokens": ["Die", "W\u00f6lf\u00b7fin", "ist", "er\u00b7legt", "durch", "dei\u00b7nen", "k\u00fch\u00b7nen", "Arm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Affter-Gott August frist sich in Leyd und Harm/", "tokens": ["Der", "Aff\u00b7ter\u00b7Gott", "Au\u00b7gust", "frist", "sich", "in", "Leyd", "und", "Harm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der frey-gemachte Rhein hebt nun sein Haubt empor/", "tokens": ["Der", "frey\u00b7ge\u00b7mach\u00b7te", "Rhein", "hebt", "nun", "sein", "Haubt", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Weser dringet sich der frechen Tyber vor:", "tokens": ["Die", "We\u00b7ser", "drin\u00b7get", "sich", "der", "fre\u00b7chen", "Ty\u00b7ber", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach aber! da\u00df dein Gl\u00fcck auch m\u00f6chte feste stehn/", "tokens": ["Ach", "a\u00b7ber", "!", "da\u00df", "dein", "Gl\u00fcck", "auch", "m\u00f6ch\u00b7te", "fes\u00b7te", "stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "KOUS", "PPOSAT", "NN", "ADV", "VMFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und nicht manch rauher Sturm auff deine Scheitel gehn!", "tokens": ["Und", "nicht", "manch", "rau\u00b7her", "Sturm", "auff", "dei\u00b7ne", "Schei\u00b7tel", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein Unfall spinnet sich aus deiner eignen Brust.", "tokens": ["Dein", "Un\u00b7fall", "spin\u00b7net", "sich", "aus", "dei\u00b7ner", "eig\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df du durch Einigkeit gesiegt/ ist dir bewust.", "tokens": ["Da\u00df", "du", "durch", "Ei\u00b7nig\u00b7keit", "ge\u00b7siegt", "/", "ist", "dir", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$(", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Zwietracht wetzet schon auff dich ihr gifftigs Schwerdt/", "tokens": ["Die", "Zwiet\u00b7racht", "wet\u00b7zet", "schon", "auff", "dich", "ihr", "giff\u00b7tigs", "Schwerdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und l\u00e4st nicht eher ab bi\u00df dirs ins Hertze f\u00e4hrt.", "tokens": ["Und", "l\u00e4st", "nicht", "e\u00b7her", "ab", "bi\u00df", "dirs", "ins", "Hert\u00b7ze", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "APPR", "PIS", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man neidet Hermans Mutt/ verkleinert seinen Ruhm/", "tokens": ["Man", "nei\u00b7det", "Her\u00b7mans", "Mutt", "/", "ver\u00b7klei\u00b7nert", "sei\u00b7nen", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "NE", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gibt f\u00fcr/ er achte dich sein dienstbars Eigenthum:", "tokens": ["Gibt", "f\u00fcr", "/", "er", "ach\u00b7te", "dich", "sein", "dienst\u00b7bars", "Ei\u00b7gen\u00b7thum", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "$(", "PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es will jedweder F\u00fcrst bey dir ein K\u00f6nig seyn/", "tokens": ["Es", "will", "jed\u00b7we\u00b7der", "F\u00fcrst", "bey", "dir", "ein", "K\u00f6\u00b7nig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPER", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und fragt nicht/ ob dazu die Mittel treffen ein;", "tokens": ["Und", "fragt", "nicht", "/", "ob", "da\u00b7zu", "die", "Mit\u00b7tel", "tref\u00b7fen", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$(", "KOUS", "PAV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drauff folget Reyd und Ha\u00df/ samt Zwietracht/ Mord und List", "tokens": ["Drauff", "fol\u00b7get", "Reyd", "und", "Ha\u00df", "/", "samt", "Zwiet\u00b7racht", "/", "Mord", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "KON", "NN", "$(", "APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bi\u00df du der Tummelplatz auch fremder Waffen bist.", "tokens": ["Bi\u00df", "du", "der", "Tum\u00b7mel\u00b7platz", "auch", "frem\u00b7der", "Waf\u00b7fen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zwar wirst du auff einmahl zu Bodem nicht gelegt/", "tokens": ["Zwar", "wirst", "du", "auff", "ein\u00b7mahl", "zu", "Bo\u00b7dem", "nicht", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADV", "APPR", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil sich dein Mutt noch offt in gr\u00f6ster Ohnmacht regt.", "tokens": ["Weil", "sich", "dein", "Mutt", "noch", "offt", "in", "gr\u00f6s\u00b7ter", "Ohn\u00b7macht", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Es schleust dir Welschland nie so strenge F\u00e4ssel an/", "tokens": ["Es", "schleust", "dir", "Wel\u00b7schland", "nie", "so", "stren\u00b7ge", "F\u00e4s\u00b7sel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADV", "ADV", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df sie nicht manchmahl noch dein Arm zubrechen kan/", "tokens": ["Da\u00df", "sie", "nicht", "manch\u00b7mahl", "noch", "dein", "Arm", "zu\u00b7bre\u00b7chen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du dringest K\u00e4yser aus/ und setzest K\u00e4yser ein/", "tokens": ["Du", "drin\u00b7gest", "K\u00e4y\u00b7ser", "aus", "/", "und", "set\u00b7zest", "K\u00e4y\u00b7ser", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$(", "KON", "VVFIN", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch must du fremder Macht Geh\u00fclff und Werckzeug seyn.", "tokens": ["Doch", "must", "du", "frem\u00b7der", "Macht", "Ge\u00b7h\u00fclff", "und", "Wer\u00b7ck\u00b7zeug", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Beute/ die du hast erfochten/ ziehet Rom/", "tokens": ["Die", "Beu\u00b7te", "/", "die", "du", "hast", "er\u00b7foch\u00b7ten", "/", "zie\u00b7het", "Rom", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VAFIN", "VVINF", "$(", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die W\u00f6lffin n\u00e4hrt dein Blutt/ dein Schwei\u00df den Tiberstrom/", "tokens": ["Die", "W\u00f6lf\u00b7fin", "n\u00e4hrt", "dein", "Blutt", "/", "dein", "Schwei\u00df", "den", "Ti\u00b7ber\u00b7strom", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJD", "$(", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie f\u00fchrt dich durch die Welt in Kriegen hin und her/", "tokens": ["Sie", "f\u00fchrt", "dich", "durch", "die", "Welt", "in", "Krie\u00b7gen", "hin", "und", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nacht dich an Ruhme reich/ an Volck und Tugend leer.", "tokens": ["Nacht", "dich", "an", "Ruh\u00b7me", "reich", "/", "an", "Volck", "und", "Tu\u00b7gend", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "ADJD", "$(", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.31": {"text": "Ihr falsch-vermummter Sinn/ die L\u00fcste fremder Welt/", "tokens": ["Ihr", "falschver\u00b7mumm\u00b7ter", "Sinn", "/", "die", "L\u00fcs\u00b7te", "frem\u00b7der", "Welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "Die uns noch unbekant und manches Reich gef\u00e4llt/", "tokens": ["Die", "uns", "noch", "un\u00b7be\u00b7kant", "und", "man\u00b7ches", "Reich", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KON", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sind deiner Dienste Sold/ den man als eine Pest", "tokens": ["Sind", "dei\u00b7ner", "Diens\u00b7te", "Sold", "/", "den", "man", "als", "ei\u00b7ne", "Pest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$(", "ART", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein junges Volck mit sich zu Hause nehmen l\u00e4st.", "tokens": ["Dein", "jun\u00b7ges", "Volck", "mit", "sich", "zu", "Hau\u00b7se", "neh\u00b7men", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PRF", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Indessen finden sich entlegne V\u00f6lcker zu/", "tokens": ["In\u00b7des\u00b7sen", "fin\u00b7den", "sich", "ent\u00b7leg\u00b7ne", "V\u00f6l\u00b7cker", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die schm\u00e4lern deine Gr\u00e4ntz und st\u00f6ren deine Ruh/", "tokens": ["Die", "schm\u00e4\u00b7lern", "dei\u00b7ne", "Gr\u00e4ntz", "und", "st\u00f6\u00b7ren", "dei\u00b7ne", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Mannschafft ist zerstrent/ die Mannheit ist zu weich/", "tokens": ["Die", "Mann\u00b7schafft", "ist", "zer\u00b7strent", "/", "die", "Mann\u00b7heit", "ist", "zu", "weich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Einigkeit zutrennt: so f\u00e4llt das deutsche Reich!", "tokens": ["Die", "Ei\u00b7nig\u00b7keit", "zu\u00b7trennt", ":", "so", "f\u00e4llt", "das", "deut\u00b7sche", "Reich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein Theil von deinem Volck erwehlt der Gallen Land/", "tokens": ["Ein", "Theil", "von", "dei\u00b7nem", "Volck", "er\u00b7wehlt", "der", "Gal\u00b7len", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "L\u00e4st deutschen Mund und Sinn/ ein Theil sucht seinen Stand/", "tokens": ["L\u00e4st", "deut\u00b7schen", "Mund", "und", "Sinn", "/", "ein", "Theil", "sucht", "sei\u00b7nen", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "A