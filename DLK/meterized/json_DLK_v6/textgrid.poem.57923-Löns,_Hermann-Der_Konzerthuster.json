{"textgrid.poem.57923": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Konzerthuster", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hat einen richtigen Schnupfen,", "tokens": ["Er", "hat", "ei\u00b7nen", "rich\u00b7ti\u00b7gen", "Schnup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr Husten ist auch nicht ganz schlecht,", "tokens": ["Ihr", "Hus\u00b7ten", "ist", "auch", "nicht", "ganz", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einer Tour niest er und prustet,", "tokens": ["In", "ei\u00b7ner", "Tour", "niest", "er", "und", "prus\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie kr\u00f6chert sich sch\u00f6n was zurecht.", "tokens": ["Sie", "kr\u00f6\u00b7chert", "sich", "sch\u00f6n", "was", "zu\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PWS", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die beiden sind Kunstenthusiasten,", "tokens": ["Die", "bei\u00b7den", "sind", "Kuns\u00b7ten\u00b7thu\u00b7si\u00b7as\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In keinem Konzert fehlen sie,", "tokens": ["In", "kei\u00b7nem", "Kon\u00b7zert", "feh\u00b7len", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Kammermusik oder W\u00fcllner,", "tokens": ["Ob", "Kam\u00b7mer\u00b7mu\u00b7sik", "o\u00b7der", "W\u00fcll\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man h\u00f6rt ihr Gehust und Hatschi.", "tokens": ["Man", "h\u00f6rt", "ihr", "Ge\u00b7hust", "und", "Hat\u00b7schi", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "KON", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Tivoli spielt Sarasate,", "tokens": ["Im", "Ti\u00b7vo\u00b7li", "spielt", "Sa\u00b7ra\u00b7sa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die beiden, die sitzen ganz vorn,", "tokens": ["Die", "bei\u00b7den", ",", "die", "sit\u00b7zen", "ganz", "vorn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie hustet bei jeder Piece,", "tokens": ["Sie", "hus\u00b7tet", "bei", "je\u00b7der", "Pie\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Er dr\u00f6hnt wie ein Turmw\u00e4chterhorn.", "tokens": ["Er", "dr\u00f6hnt", "wie", "ein", "Turm\u00b7w\u00e4ch\u00b7ter\u00b7horn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ihr Enthusiasmus ist r\u00fchrend,", "tokens": ["Ihr", "En\u00b7thu\u00b7si\u00b7as\u00b7mus", "ist", "r\u00fch\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kunst, die ist ihnen kein Spa\u00df,", "tokens": ["Die", "Kunst", ",", "die", "ist", "ih\u00b7nen", "kein", "Spa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und geht's ihnen auch noch so elend,", "tokens": ["Und", "geht's", "ih\u00b7nen", "auch", "noch", "so", "e\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kommen und husten uns was.", "tokens": ["Sie", "kom\u00b7men", "und", "hus\u00b7ten", "uns", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}