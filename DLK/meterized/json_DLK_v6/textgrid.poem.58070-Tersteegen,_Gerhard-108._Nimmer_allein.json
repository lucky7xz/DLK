{"textgrid.poem.58070": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "108. Nimmer allein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ich je geh' und steh', auch sitze und arbeite,", "tokens": ["Wo", "ich", "je", "geh'", "und", "steh'", ",", "auch", "sit\u00b7ze", "und", "ar\u00b7bei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da bin ich nicht allein, mein Freund steht mir zur Seite;", "tokens": ["Da", "bin", "ich", "nicht", "al\u00b7lein", ",", "mein", "Freund", "steht", "mir", "zur", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Halt' ich mich nah zu ihm und tu, was ihm gef\u00e4llt,", "tokens": ["Halt'", "ich", "mich", "nah", "zu", "ihm", "und", "tu", ",", "was", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "APPR", "PPER", "KON", "NE", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wandelt er mit mir und gern sich zu mir h\u00e4lt.", "tokens": ["So", "wan\u00b7delt", "er", "mit", "mir", "und", "gern", "sich", "zu", "mir", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "KON", "ADV", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}