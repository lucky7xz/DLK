{"textgrid.poem.52231": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Kinderlied", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weihnacht ist ein sch\u00f6nes Fest,", "tokens": ["Weih\u00b7nacht", "ist", "ein", "sch\u00f6\u00b7nes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n f\u00fcr Hohe, sch\u00f6n f\u00fcr Niedre!", "tokens": ["Sch\u00f6n", "f\u00fcr", "Ho\u00b7he", ",", "sch\u00f6n", "f\u00fcr", "Nied\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,", "ADJD", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner, den es traurig l\u00e4\u00dft,", "tokens": ["Kei\u00b7ner", ",", "den", "es", "trau\u00b7rig", "l\u00e4\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auch sonst die Welt ihn widre!", "tokens": ["Wie", "auch", "sonst", "die", "Welt", "ihn", "wid\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch beinah' noch gr\u00f6\u00dfern Spa\u00df", "tokens": ["Doch", "bei\u00b7nah'", "noch", "gr\u00f6\u00b7\u00dfern", "Spa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht uns jetzt Sankt Nikolas \u2013", "tokens": ["Macht", "uns", "jetzt", "Sankt", "Ni\u00b7ko\u00b7las", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVFIN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nikolaus, ja, der Biedre!", "tokens": ["Ni\u00b7ko\u00b7laus", ",", "ja", ",", "der", "Bied\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PTKANT", "$,", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Niklas ist ein braver Mann,", "tokens": ["Nik\u00b7las", "ist", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.2": {"text": "Herzensgut und mild von Sitten;", "tokens": ["Her\u00b7zens\u00b7gut", "und", "mild", "von", "Sit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Niklas hat ein Renngespann", "tokens": ["Nik\u00b7las", "hat", "ein", "Renn\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dahinter einen Schlitten.", "tokens": ["Und", "da\u00b7hin\u00b7ter", "ei\u00b7nen", "Schlit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Hoch im Norden steht sein Haus;", "tokens": ["Hoch", "im", "Nor\u00b7den", "steht", "sein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reiche Gaben teilt er aus,", "tokens": ["Rei\u00b7che", "Ga\u00b7ben", "teilt", "er", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die Kinder h\u00fcbsch ihn bitten.", "tokens": ["Wenn", "die", "Kin\u00b7der", "h\u00fcbsch", "ihn", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spielwerk hat er mancher Art,", "tokens": ["Spiel\u00b7werk", "hat", "er", "man\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterne, B\u00e4nder, goldne Krippchen!", "tokens": ["Ster\u00b7ne", ",", "B\u00e4n\u00b7der", ",", "gold\u00b7ne", "Kripp\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Streicht ihm freundlich drum den Bart,", "tokens": ["Streicht", "ihm", "freund\u00b7lich", "drum", "den", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seid drum artig, liebe B\u00fcbchen!", "tokens": ["Seid", "drum", "ar\u00b7tig", ",", "lie\u00b7be", "B\u00fcb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PAV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ihn recht zu h\u00e4tscheln wei\u00df,", "tokens": ["Wer", "ihn", "recht", "zu", "h\u00e4t\u00b7scheln", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eia, kriegt den besten Preis \u2013", "tokens": ["Ei\u00b7a", ",", "kriegt", "den", "bes\u00b7ten", "Preis", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eins von seinen Zuckerp\u00fcppchen!", "tokens": ["Eins", "von", "sei\u00b7nen", "Zu\u00b7cker\u00b7p\u00fcpp\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eia, sind sie doch wie Wachs \u2013", "tokens": ["Ei\u00b7a", ",", "sind", "sie", "doch", "wie", "Wachs", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "KOKOM", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blond von Haaren, glatt von Wangen!", "tokens": ["Blond", "von", "Haa\u00b7ren", ",", "glatt", "von", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Tiefen seines Sacks", "tokens": ["In", "den", "Tie\u00b7fen", "sei\u00b7nes", "Sacks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmunzelnd h\u00e4lt er sie gefangen,", "tokens": ["Schmun\u00b7zelnd", "h\u00e4lt", "er", "sie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Putzt sie aus mit Zobelschur,", "tokens": ["Putzt", "sie", "aus", "mit", "Zo\u00b7bel\u00b7schur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und in Juchten, denkt euch nur,", "tokens": ["Und", "in", "Juch\u00b7ten", ",", "denkt", "euch", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "L\u00e4\u00dft er ihre F\u00fc\u00dfchen prangen!", "tokens": ["L\u00e4\u00dft", "er", "ih\u00b7re", "F\u00fc\u00df\u00b7chen", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit der n\u00e4chsten Schlittenbahn", "tokens": ["Mit", "der", "n\u00e4chs\u00b7ten", "Schlit\u00b7ten\u00b7bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt er angerutscht aus Norden;", "tokens": ["Kommt", "er", "an\u00b7ge\u00b7rutscht", "aus", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Offen liegt vor ihm der Plan,", "tokens": ["Of\u00b7fen", "liegt", "vor", "ihm", "der", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Pol' ist matt geworden.", "tokens": ["Denn", "der", "Pol'", "ist", "matt", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der mit S\u00e4bel und mit Spie\u00df", "tokens": ["Der", "mit", "S\u00e4\u00b7bel", "und", "mit", "Spie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcrrisch sonst zur\u00fcck ihn wies,", "tokens": ["M\u00fcr\u00b7risch", "sonst", "zu\u00b7r\u00fcck", "ihn", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kniet jetzt auf der Weichsel Borden.", "tokens": ["Kniet", "jetzt", "auf", "der", "Weich\u00b7sel", "Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so ist er bald denn da,", "tokens": ["Und", "so", "ist", "er", "bald", "denn", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie auch Elb' und Oder flute!", "tokens": ["Wie", "auch", "Elb'", "und", "O\u00b7der", "flu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kinderchen, seid artig ja,", "tokens": ["Kin\u00b7der\u00b7chen", ",", "seid", "ar\u00b7tig", "ja", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "ADV", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Denn \u2013 auch strafen kann der Gute!", "tokens": ["Denn", "\u2013", "auch", "stra\u00b7fen", "kann", "der", "Gu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, seid brav, sonst gibt er euch \u2013", "tokens": ["Ja", ",", "seid", "brav", ",", "sonst", "gibt", "er", "euch", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Eia, wer erschrickt denn gleich? \u2013", "tokens": ["Ei\u00b7a", ",", "wer", "er\u00b7schrickt", "denn", "gleich", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein' ich doch ja nur; die \u2013 Rute!", "tokens": ["Mein'", "ich", "doch", "ja", "nur", ";", "die", "\u2013", "Ru\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "$.", "ART", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wohl den Kindern weit und breit,", "tokens": ["Wohl", "den", "Kin\u00b7dern", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Wackern liebend ehren!", "tokens": ["Die", "den", "Wa\u00b7ckern", "lie\u00b7bend", "eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die zu dieser b\u00f6sen Zeit", "tokens": ["Die", "zu", "die\u00b7ser", "b\u00f6\u00b7sen", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz als Kinder ihm geh\u00f6ren!", "tokens": ["Ganz", "als", "Kin\u00b7der", "ihm", "ge\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die als Onkel und Papa", "tokens": ["Die", "als", "On\u00b7kel", "und", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu dir aufschaun, Nikola \u2013", "tokens": ["Zu", "dir", "auf\u00b7schaun", ",", "Ni\u00b7ko\u00b7la", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihnen wirst den Sack du leeren!", "tokens": ["Ih\u00b7nen", "wirst", "den", "Sack", "du", "lee\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum geb\u00fcckt euch und geschmiegt,", "tokens": ["Drum", "ge\u00b7b\u00fcckt", "euch", "und", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Recht mit kindlichem Gem\u00fcte,", "tokens": ["Recht", "mit", "kind\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis es rings nach Juchten riecht,", "tokens": ["Bis", "es", "rings", "nach", "Juch\u00b7ten", "riecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie im Mai nach \u00c4pfelbl\u00fcte!", "tokens": ["Wie", "im", "Mai", "nach", "\u00c4p\u00b7fel\u00b7bl\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis in echtem Zobelhaar", "tokens": ["Bis", "in", "ech\u00b7tem", "Zo\u00b7bel\u00b7haar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcberall und immerdar", "tokens": ["\u00dc\u00b7be\u00b7rall", "und", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir uns freuen seiner G\u00fcte!", "tokens": ["Wir", "uns", "freu\u00b7en", "sei\u00b7ner", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weihnacht ist ein sch\u00f6nes Fest,", "tokens": ["Weih\u00b7nacht", "ist", "ein", "sch\u00f6\u00b7nes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n f\u00fcr Hohe, sch\u00f6n f\u00fcr Niedre!", "tokens": ["Sch\u00f6n", "f\u00fcr", "Ho\u00b7he", ",", "sch\u00f6n", "f\u00fcr", "Nied\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,", "ADJD", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner, den es traurig l\u00e4\u00dft,", "tokens": ["Kei\u00b7ner", ",", "den", "es", "trau\u00b7rig", "l\u00e4\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auch sonst die Welt ihn widre!", "tokens": ["Wie", "auch", "sonst", "die", "Welt", "ihn", "wid\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch den allermeisten Spa\u00df", "tokens": ["Doch", "den", "al\u00b7ler\u00b7meis\u00b7ten", "Spa\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht uns jetzt Sankt Nikolas \u2013", "tokens": ["Macht", "uns", "jetzt", "Sankt", "Ni\u00b7ko\u00b7las", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVFIN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nikolaus, ja, der Biedre!", "tokens": ["Ni\u00b7ko\u00b7laus", ",", "ja", ",", "der", "Bied\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PTKANT", "$,", "ART", "NN", "$."], "m