{"textgrid.poem.57584": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Braune Kormorane flogen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Braune Kormorane flogen", "tokens": ["Brau\u00b7ne", "Kor\u00b7mo\u00b7ra\u00b7ne", "flo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4chtigklafternd \u00fcbers Meer,", "tokens": ["M\u00e4ch\u00b7tig\u00b7klaf\u00b7ternd", "\u00fc\u00b7bers", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Grau und wei\u00dfe M\u00f6wen zogen", "tokens": ["Grau", "und", "wei\u00b7\u00dfe", "M\u00f6\u00b7wen", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreischend um den Dampfer her,", "tokens": ["Krei\u00b7schend", "um", "den", "Damp\u00b7fer", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heringsb\u00e4nke, silberblank", "tokens": ["He\u00b7rings\u00b7b\u00e4n\u00b7ke", ",", "sil\u00b7ber\u00b7blank"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter gr\u00fcnen Wellen,", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7nen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Auf dem Decke lauter Sang", "tokens": ["Auf", "dem", "De\u00b7cke", "lau\u00b7ter", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Narbiger Gesellen.", "tokens": ["Nar\u00b7bi\u00b7ger", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sie stand am Backbord: farblos, ohne Blut", "tokens": ["Sie", "stand", "am", "Back\u00b7bord", ":", "farb\u00b7los", ",", "oh\u00b7ne", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ADJD", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Antlitz war; ihre Auge, blauumr\u00e4ndert,", "tokens": ["Das", "Ant\u00b7litz", "war", ";", "ih\u00b7re", "Au\u00b7ge", ",", "blau\u00b7um\u00b7r\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das starrte trostlos, kalt und unver\u00e4ndert", "tokens": ["Das", "starr\u00b7te", "trost\u00b7los", ",", "kalt", "und", "un\u00b7ver\u00b7\u00e4n\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinunter auf der Ostsee dunkle Flut;", "tokens": ["Hin\u00b7un\u00b7ter", "auf", "der", "Ost\u00b7see", "dunk\u00b7le", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein blaues Kleid umschlo\u00df den schlanken Leib,", "tokens": ["Ein", "blau\u00b7es", "Kleid", "um\u00b7schlo\u00df", "den", "schlan\u00b7ken", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Am schwarzen Hut der Federschmuck vom Reiher,", "tokens": ["Am", "schwar\u00b7zen", "Hut", "der", "Fe\u00b7der\u00b7schmuck", "vom", "Rei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und lustig flatterte der wei\u00dfe Schleier,", "tokens": ["Und", "lus\u00b7tig", "flat\u00b7ter\u00b7te", "der", "wei\u00b7\u00dfe", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Ostwind ein willkommener Zeitvertreib.", "tokens": ["Dem", "Ost\u00b7wind", "ein", "will\u00b7kom\u00b7me\u00b7ner", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "R\u00fcgens Kreidemauern ragten", "tokens": ["R\u00fc\u00b7gens", "Krei\u00b7de\u00b7mau\u00b7ern", "rag\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Meere marmorhell,", "tokens": ["Aus", "dem", "Mee\u00b7re", "mar\u00b7mo\u00b7rhell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dfbem\u00fctzte Wogen jagten", "tokens": ["Wei\u00df\u00b7be\u00b7m\u00fctz\u00b7te", "Wo\u00b7gen", "jag\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Feuersteinger\u00f6ll,", "tokens": ["\u00dc\u00b7ber", "Feu\u00b7ers\u00b7tein\u00b7ge\u00b7r\u00f6ll", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ostwind, Sonne, Fischgeruch,", "tokens": ["Ost\u00b7wind", ",", "Son\u00b7ne", ",", "Fischge\u00b7ruch", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Fernsicht, goldbeschienen,", "tokens": ["Fern\u00b7sicht", ",", "gold\u00b7be\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Scherze, froher Seemannsfluch,", "tokens": ["Scher\u00b7ze", ",", "fro\u00b7her", "See\u00b7manns\u00b7fluch", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ausgela\u00dfne Mienen.", "tokens": ["Aus\u00b7ge\u00b7la\u00df\u00b7ne", "Mie\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sie seufzte, langsam hob sich ihre Brust,", "tokens": ["Sie", "seufz\u00b7te", ",", "lang\u00b7sam", "hob", "sich", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wei\u00dfe H\u00e4ndchen st\u00fctzte m\u00fcd die Schl\u00e4fe,", "tokens": ["Das", "wei\u00b7\u00dfe", "H\u00e4nd\u00b7chen", "st\u00fctz\u00b7te", "m\u00fcd", "die", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So jung, und schon des Lebens letzter Hefe", "tokens": ["So", "jung", ",", "und", "schon", "des", "Le\u00b7bens", "letz\u00b7ter", "He\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fadbittrer Nachgeschmack nach wenig Lust.", "tokens": ["Fad\u00b7bit\u00b7trer", "Nach\u00b7ge\u00b7schmack", "nach", "we\u00b7nig", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu der Kaj\u00fcte ging ihr m\u00fcder Schritt,", "tokens": ["Zu", "der", "Ka\u00b7j\u00fc\u00b7te", "ging", "ihr", "m\u00fc\u00b7der", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Arme hingen schlaff am Leib hernieder,", "tokens": ["Die", "Ar\u00b7me", "hin\u00b7gen", "schlaff", "am", "Leib", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kontrast; \u00bbSo leben wir \u2013\u00ab und andre Lieder,", "tokens": ["Kont\u00b7rast", ";", "\u00bb", "So", "le\u00b7ben", "wir", "\u2013", "\u00ab", "und", "and\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "ADV", "VVFIN", "PPER", "$(", "$(", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie ging, und meine Augen nahm sie mit.", "tokens": ["Sie", "ging", ",", "und", "mei\u00b7ne", "Au\u00b7gen", "nahm", "sie", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gellend in den blauen L\u00fcften", "tokens": ["Gel\u00b7lend", "in", "den", "blau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scholl des Adlers Hungerschrei,", "tokens": ["Scholl", "des", "Ad\u00b7lers", "Hun\u00b7ger\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den wei\u00dfen Kreidekl\u00fcften", "tokens": ["An", "den", "wei\u00b7\u00dfen", "Krei\u00b7de\u00b7kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scho\u00df der Dampfer stolz vorbei,", "tokens": ["Scho\u00df", "der", "Damp\u00b7fer", "stolz", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Glatte Robben flohen schnell", "tokens": ["Glat\u00b7te", "Rob\u00b7ben", "flo\u00b7hen", "schnell"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach dem flachen Strande,", "tokens": ["Nach", "dem", "fla\u00b7chen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kurze Wellen gl\u00fchten hell", "tokens": ["Kur\u00b7ze", "Wel\u00b7len", "gl\u00fch\u00b7ten", "hell"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf im Sonnenbrande.", "tokens": ["Auf", "im", "Son\u00b7nen\u00b7bran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sie trat aus der Kaj\u00fcte leichenbla\u00df,", "tokens": ["Sie", "trat", "aus", "der", "Ka\u00b7j\u00fc\u00b7te", "lei\u00b7chen\u00b7bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das blaue Auge schwarz vom Tod umschattet,", "tokens": ["Das", "blau\u00b7e", "Au\u00b7ge", "schwarz", "vom", "Tod", "um\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lippen fahl, vom Todesku\u00df ermattet,", "tokens": ["Die", "Lip\u00b7pen", "fahl", ",", "vom", "To\u00b7des\u00b7ku\u00df", "er\u00b7mat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von der Stirne lief das kalte Na\u00df;", "tokens": ["Und", "von", "der", "Stir\u00b7ne", "lief", "das", "kal\u00b7te", "Na\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie sah sich hilflos, hilfeflehend um:", "tokens": ["Sie", "sah", "sich", "hil\u00b7flos", ",", "hil\u00b7fe\u00b7fle\u00b7hend", "um", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbherr Kapit\u00e4n, ich wollte Ihnen sagen \u2013\u00ab", "tokens": ["\u00bb", "herr", "Ka\u00b7pi\u00b7t\u00e4n", ",", "ich", "woll\u00b7te", "Ih\u00b7nen", "sa\u00b7gen", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kein Mensch wird ihren Willen mehr erfragen, \u2013", "tokens": ["Kein", "Mensch", "wird", "ih\u00b7ren", "Wil\u00b7len", "mehr", "er\u00b7fra\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie wankte, fsiel, und ward f\u00fcr immer stumm.", "tokens": ["Sie", "wank\u00b7te", ",", "fsiel", ",", "und", "ward", "f\u00fcr", "im\u00b7mer", "stumm", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "KON", "VAFIN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Langsam sank des Nebels Laken", "tokens": ["Lang\u00b7sam", "sank", "des", "Ne\u00b7bels", "La\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Meer und Buchengr\u00fcn,", "tokens": ["\u00dc\u00b7ber", "Meer", "und", "Bu\u00b7chen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Leuchtschiffs Eisenhaken", "tokens": ["An", "des", "Leucht\u00b7schiffs", "Ei\u00b7sen\u00b7ha\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwache Feuerpunkte gl\u00fchn,", "tokens": ["Schwa\u00b7che", "Feu\u00b7er\u00b7punk\u00b7te", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schnell die Sonne ward entthront,", "tokens": ["Schnell", "die", "Son\u00b7ne", "ward", "ent\u00b7thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Licht und Glut zerflogen,", "tokens": ["Licht", "und", "Glut", "zer\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und der totenbleiche Mond", "tokens": ["Und", "der", "to\u00b7ten\u00b7blei\u00b7che", "Mond"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": 