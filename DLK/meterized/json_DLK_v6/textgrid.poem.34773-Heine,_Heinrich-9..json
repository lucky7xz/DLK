{"textgrid.poem.34773": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser Liebe toller Fasching,", "tokens": ["Die\u00b7ser", "Lie\u00b7be", "tol\u00b7ler", "Fa\u00b7sching", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Taumel unsrer Herzen,", "tokens": ["Die\u00b7ser", "Tau\u00b7mel", "uns\u00b7rer", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht zu Ende, und ern\u00fcchtert", "tokens": ["Geht", "zu", "En\u00b7de", ",", "und", "er\u00b7n\u00fcch\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00e4hnen wir einander an!", "tokens": ["G\u00e4h\u00b7nen", "wir", "ein\u00b7an\u00b7der", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ausgetrunken ist der Kelch,", "tokens": ["Aus\u00b7ge\u00b7trun\u00b7ken", "ist", "der", "Kelch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mir Sinnenrausch gef\u00fcllt war,", "tokens": ["Der", "mir", "Sin\u00b7nen\u00b7rausch", "ge\u00b7f\u00fcllt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4umend, lodernd, bis am Rande;", "tokens": ["Sch\u00e4u\u00b7mend", ",", "lo\u00b7dernd", ",", "bis", "am", "Ran\u00b7de", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgetrunken ist der Kelch.", "tokens": ["Aus\u00b7ge\u00b7trun\u00b7ken", "ist", "der", "Kelch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es verstummen auch die Geigen,", "tokens": ["Es", "ver\u00b7stum\u00b7men", "auch", "die", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die zum Tanze m\u00e4chtig spielten,", "tokens": ["Die", "zum", "Tan\u00b7ze", "m\u00e4ch\u00b7tig", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Tanz der Leidenschaft;", "tokens": ["Zu", "dem", "Tanz", "der", "Lei\u00b7den\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch die Geigen, sie verstummen.", "tokens": ["Auch", "die", "Gei\u00b7gen", ",", "sie", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Es erl\u00f6schen auch die Lampen,", "tokens": ["Es", "er\u00b7l\u00f6\u00b7schen", "auch", "die", "Lam\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das wilde Licht ergossen", "tokens": ["Die", "das", "wil\u00b7de", "Licht", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den bunten Mummenschanz;", "tokens": ["Auf", "den", "bun\u00b7ten", "Mum\u00b7men\u00b7schanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch die Lampen, sie erl\u00f6schen.", "tokens": ["Auch", "die", "Lam\u00b7pen", ",", "sie", "er\u00b7l\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Morgen kommt der Aschenmittwoch,", "tokens": ["Mor\u00b7gen", "kommt", "der", "A\u00b7schen\u00b7mitt\u00b7woch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich zeichne deine Stirne", "tokens": ["Und", "ich", "zeich\u00b7ne", "dei\u00b7ne", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Aschenkreuz und spreche:", "tokens": ["Mit", "dem", "A\u00b7schen\u00b7kreuz", "und", "spre\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbweib, bedenke, da\u00df du Staub bist.\u00ab", "tokens": ["\u00bb", "weib", ",", "be\u00b7den\u00b7ke", ",", "da\u00df", "du", "Staub", "bist", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}