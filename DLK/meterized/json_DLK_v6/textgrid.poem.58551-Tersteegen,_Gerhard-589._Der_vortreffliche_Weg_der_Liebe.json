{"textgrid.poem.58551": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "589. Der vortreffliche Weg der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strebt alle nach den besten Gaben,", "tokens": ["Strebt", "al\u00b7le", "nach", "den", "bes\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will nur reine Liebe haben!", "tokens": ["Ich", "will", "nur", "rei\u00b7ne", "Lie\u00b7be", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Freundes Lieb': mein's Herzens Siegel,", "tokens": ["Des", "Freun\u00b7des", "Lieb'", ":", "mein's", "Her\u00b7zens", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bruderlieb': mein's Wandels Spiegel.", "tokens": ["Die", "Bru\u00b7der\u00b7lieb'", ":", "mein's", "Wan\u00b7dels", "Spie\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}