{"dta.poem.1616": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Dachses Grabschrifft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein strenger Katzen-Feind/ ein schlauer Hasen-", "tokens": ["Hier", "liegt", "ein", "stren\u00b7ger", "Kat\u00b7zen\u00b7Feind", "/", "ein", "schlau\u00b7er", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4nger/", "tokens": ["F\u00e4n\u00b7ger", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein St\u00fcrmer mancher festen Grufft/ ein k\u00fchner Fuchs-Be-", "tokens": ["Ein", "St\u00fcr\u00b7mer", "man\u00b7cher", "fes\u00b7ten", "Grufft", "/", "ein", "k\u00fch\u00b7ner", "Fuchs\u00b7Be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dr\u00e4nger/", "tokens": ["dr\u00e4n\u00b7ger", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Man sah kaum so viel Haar auff seinen abgelebten R\u00fccken/", "tokens": ["Man", "sah", "kaum", "so", "viel", "Haar", "auff", "sei\u00b7nen", "ab\u00b7ge\u00b7leb\u00b7ten", "R\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Als er sein Lebenlang hat Wild zu Grabe helffen schicken.", "tokens": ["Als", "er", "sein", "Le\u00b7ben\u00b7lang", "hat", "Wild", "zu", "Gra\u00b7be", "helf\u00b7fen", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "NE", "APPR", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Man wird/ wie er zugleich der Hund und J\u00e4ger sey gewesen/", "tokens": ["Man", "wird", "/", "wie", "er", "zu\u00b7gleich", "der", "Hund", "und", "J\u00e4\u00b7ger", "sey", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "PWAV", "PPER", "ADV", "ART", "NN", "KON", "NN", "VAFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Mit Wunder in den Chronicken ber\u00fchmter Hunde lesen.", "tokens": ["Mit", "Wun\u00b7der", "in", "den", "Chro\u00b7ni\u00b7cken", "be\u00b7r\u00fchm\u00b7ter", "Hun\u00b7de", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Die Treue/ die er Lebenslang erzeiget Herr und Frauen/", "tokens": ["Die", "Treu\u00b7e", "/", "die", "er", "Le\u00b7bens\u00b7lang", "er\u00b7zei\u00b7get", "Herr", "und", "Frau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Die\u00df er an der Gespielin noch in seinem Tode schauen.", "tokens": ["Die\u00df", "er", "an", "der", "Ge\u00b7spie\u00b7lin", "noch", "in", "sei\u00b7nem", "To\u00b7de", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Wie sie zusammen W\u00e4ydewerck ins zehnde Jahr getrieben/", "tokens": ["Wie", "sie", "zu\u00b7sam\u00b7men", "W\u00e4y\u00b7de\u00b7werck", "ins", "zehn\u00b7de", "Jahr", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "So sind sie auch in kurtzer Frist von gleichem Tode blieben.", "tokens": ["So", "sind", "sie", "auch", "in", "kurt\u00b7zer", "Frist", "von", "glei\u00b7chem", "To\u00b7de", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Sieht gleich die Welt kein Ebenbild von seinen Kindern leben/", "tokens": ["Sieht", "gleich", "die", "Welt", "kein", "E\u00b7ben\u00b7bild", "von", "sei\u00b7nen", "Kin\u00b7dern", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "So wird ihm der gestirnte Hund doch seine Stelle geben.", "tokens": ["So", "wird", "ihm", "der", "ge\u00b7stirn\u00b7te", "Hund", "doch", "sei\u00b7ne", "Stel\u00b7le", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}