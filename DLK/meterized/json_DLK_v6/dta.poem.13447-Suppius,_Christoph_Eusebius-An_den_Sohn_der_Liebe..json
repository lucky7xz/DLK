{"dta.poem.13447": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "An den Sohn der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrst der K\u00fcsse! kleiner Sohn der Liebe!", "tokens": ["F\u00fcrst", "der", "K\u00fcs\u00b7se", "!", "klei\u00b7ner", "Sohn", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieh doch nur, wie sehr ich mich betr\u00fcbe,", "tokens": ["Sieh", "doch", "nur", ",", "wie", "sehr", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PWAV", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hast du ja mit niemand sonst Erbarmen,", "tokens": ["Hast", "du", "ja", "mit", "nie\u00b7mand", "sonst", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PIS", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Chloris hatte mir ihr Herz gegeben,", "tokens": ["Chlo\u00b7ris", "hat\u00b7te", "mir", "ihr", "Herz", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die du erst verwundet; ja sie eben", "tokens": ["Die", "du", "erst", "ver\u00b7wun\u00b7det", ";", "ja", "sie", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "$.", "ADV", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zieht es mit verachtungsvollem Blicke", "tokens": ["Zieht", "es", "mit", "ver\u00b7ach\u00b7tungs\u00b7vol\u00b7lem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Kind! bestrafe doch die Ungetreue,", "tokens": ["Kind", "!", "be\u00b7stra\u00b7fe", "doch", "die", "Un\u00b7ge\u00b7treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Martre sie durch eine sp\u00e4te Reue,", "tokens": ["Mar\u00b7tre", "sie", "durch", "ei\u00b7ne", "sp\u00e4\u00b7te", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df sie brennen, klagen, seufzen, schmachten,", "tokens": ["La\u00df", "sie", "bren\u00b7nen", ",", "kla\u00b7gen", ",", "seuf\u00b7zen", ",", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}