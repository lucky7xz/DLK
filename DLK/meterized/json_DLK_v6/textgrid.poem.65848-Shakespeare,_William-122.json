{"textgrid.poem.65848": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "122", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein T\u00e4flein, dein Geschenk, ist mein Gem\u00fct,", "tokens": ["Dein", "T\u00e4\u00b7flein", ",", "dein", "Ge\u00b7schenk", ",", "ist", "mein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit dauerndem Ged\u00e4chtnis vollgeschrieben,", "tokens": ["Mit", "dau\u00b7ern\u00b7dem", "Ge\u00b7d\u00e4cht\u00b7nis", "voll\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das alle d\u00fcrft'ge Schranken \u00fcberflieht,", "tokens": ["Das", "al\u00b7le", "d\u00fcrft'\u00b7ge", "Schran\u00b7ken", "\u00fc\u00b7berf\u00b7lieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Zeit zum Trutz auf ewig fortzulieben:", "tokens": ["Der", "Zeit", "zum", "Trutz", "auf", "e\u00b7wig", "fort\u00b7zu\u00b7lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Solang zumindest, als Naturgeschick", "tokens": ["So\u00b7lang", "zu\u00b7min\u00b7dest", ",", "als", "Na\u00b7tur\u00b7ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gem\u00fct und Hirn zum Leben mag verbinden.", "tokens": ["Ge\u00b7m\u00fct", "und", "Hirn", "zum", "Le\u00b7ben", "mag", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis beides nicht sein Teil an dir zur\u00fcck", "tokens": ["Bis", "bei\u00b7des", "nicht", "sein", "Teil", "an", "dir", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem Moder gibt, kann nie dein Bild verschwinden.", "tokens": ["Dem", "Mo\u00b7der", "gibt", ",", "kann", "nie", "dein", "Bild", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zu eng war das Gef\u00e4\u00df f\u00fcr solchen Zweck;", "tokens": ["Zu", "eng", "war", "das", "Ge\u00b7f\u00e4\u00df", "f\u00fcr", "sol\u00b7chen", "Zweck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kein Kerbholz braucht's, dein Lieben einzuzeichnen;", "tokens": ["Kein", "Kerb\u00b7holz", "braucht's", ",", "dein", "Lie\u00b7ben", "ein\u00b7zu\u00b7zeich\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Darum war ich so k\u00fchn und gab es weg,", "tokens": ["Da\u00b7rum", "war", "ich", "so", "k\u00fchn", "und", "gab", "es", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auf Tafeln stolz, die dich mir fester eignen.", "tokens": ["Auf", "Ta\u00b7feln", "stolz", ",", "die", "dich", "mir", "fes\u00b7ter", "eig\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PRELS", "PPER", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Braucht' ich, um dein zu denken, andrer Spur,", "tokens": ["Braucht'", "ich", ",", "um", "dein", "zu", "den\u00b7ken", ",", "an\u00b7drer", "Spur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "PTKZU", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Verge\u00dflichkeit in mir bewies es nur.", "tokens": ["Ver\u00b7ge\u00df\u00b7lich\u00b7keit", "in", "mir", "be\u00b7wies", "es", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}