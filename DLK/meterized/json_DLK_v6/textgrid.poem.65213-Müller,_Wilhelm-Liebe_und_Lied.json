{"textgrid.poem.65213": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Lied", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Fr\u00fchling aus der H\u00f6he", "tokens": ["Als", "der", "Fr\u00fch\u00b7ling", "aus", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flog in unsre Th\u00e4ler nieder,", "tokens": ["Flog", "in", "uns\u00b7re", "Th\u00e4\u00b7ler", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df er ein Paar Blumen fallen", "tokens": ["Lie\u00df", "er", "ein", "Paar", "Blu\u00b7men", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem vollen Kranz der Stirne.", "tokens": ["Aus", "dem", "vol\u00b7len", "Kranz", "der", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich sucht' und fand die Blumen,", "tokens": ["Und", "ich", "sucht'", "und", "fand", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo der Quelle rasches Silber", "tokens": ["Wo", "der", "Quel\u00b7le", "ra\u00b7sches", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Stille stand in Lust und Staunen.", "tokens": ["Stil\u00b7le", "stand", "in", "Lust", "und", "Stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Quelle, sage mir, ich bitte,", "tokens": ["Quel\u00b7le", ",", "sa\u00b7ge", "mir", ",", "ich", "bit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie die beiden Blumen hei\u00dfen,", "tokens": ["Wie", "die", "bei\u00b7den", "Blu\u00b7men", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die an deinem Ufer liegen.", "tokens": ["Die", "an", "dei\u00b7nem", "U\u00b7fer", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ein M\u00e4gdlein sprang vor\u00fcber,", "tokens": ["Und", "ein", "M\u00e4gd\u00b7lein", "sprang", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und ein V\u00f6glein h\u00f6rt' ich singen;", "tokens": ["Und", "ein", "V\u00f6\u00b7glein", "h\u00f6rt'", "ich", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und die Quelle sprach: Die eine", "tokens": ["Und", "die", "Quel\u00b7le", "sprach", ":", "Die", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "ART", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Von den Blumen hei\u00dft die Liebe,", "tokens": ["Von", "den", "Blu\u00b7men", "hei\u00dft", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und das Lied hei\u00dft jene andre:", "tokens": ["Und", "das", "Lied", "hei\u00dft", "je\u00b7ne", "and\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PDAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Nimm sie auf und la\u00df mich ziehen!", "tokens": ["Nimm", "sie", "auf", "und", "la\u00df", "mich", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}