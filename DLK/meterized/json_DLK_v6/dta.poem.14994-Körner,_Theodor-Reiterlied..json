{"dta.poem.14994": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Reiterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.85", "af:0.14"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Frei vor dir liegt die Welt,", "tokens": ["Frei", "vor", "dir", "liegt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie auch des Feindes List und Trug", "tokens": ["Wie", "auch", "des", "Fein\u00b7des", "List", "und", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns rings umgattert h\u00e4lt.", "tokens": ["Uns", "rings", "um\u00b7gat\u00b7tert", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Steig, edles Ro\u00df, und b\u00e4ume dich,", "tokens": ["Steig", ",", "ed\u00b7les", "Ro\u00df", ",", "und", "b\u00e4u\u00b7me", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Dort winkt der Eichenkranz!", "tokens": ["Dort", "winkt", "der", "Ei\u00b7chen\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Streich aus, streich aus, und trage mich", "tokens": ["Streich", "aus", ",", "streich", "aus", ",", "und", "tra\u00b7ge", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum lust'gen Schwerdtertanz.", "tokens": ["Zum", "lust'\u00b7gen", "Schwerd\u00b7ter\u00b7tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hoch in den L\u00fcften, unbesiegt,", "tokens": ["Hoch", "in", "den", "L\u00fcf\u00b7ten", ",", "un\u00b7be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht frischer Reitersmuth!", "tokens": ["Geht", "fri\u00b7scher", "Rei\u00b7ters\u00b7muth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was unter ihm im Staube liegt,", "tokens": ["Was", "un\u00b7ter", "ihm", "im", "Stau\u00b7be", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Engt nicht das freie Blut.", "tokens": ["Engt", "nicht", "das", "frei\u00b7e", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weit hinter ihm liegt Sorg' und Noth,", "tokens": ["Weit", "hin\u00b7ter", "ihm", "liegt", "Sor\u00b7g'", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und Weib und Kind und Heerd,", "tokens": ["Und", "Weib", "und", "Kind", "und", "Heerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor ihm nur Freiheit oder Tod,", "tokens": ["Vor", "ihm", "nur", "Frei\u00b7heit", "o\u00b7der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und neben ihm das Schwerdt.", "tokens": ["Und", "ne\u00b7ben", "ihm", "das", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So geht's zum lust'gen Hochzeitsfest,", "tokens": ["So", "geht's", "zum", "lust'\u00b7gen", "Hoch\u00b7zeits\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Brautkranz ist der Preis,", "tokens": ["Der", "Braut\u00b7kranz", "ist", "der", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer das Liebchen warten l\u00e4\u00dft,", "tokens": ["Und", "wer", "das", "Lieb\u00b7chen", "war\u00b7ten", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den bannt der freie Kreis.", "tokens": ["Den", "bannt", "der", "frei\u00b7e", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Ehre ist der Hochzeitgast,", "tokens": ["Die", "Eh\u00b7re", "ist", "der", "Hoch\u00b7zeit\u00b7gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Vaterland die Braut;", "tokens": ["Das", "Va\u00b7ter\u00b7land", "die", "Braut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer sie recht br\u00fcnstiglich umfa\u00dft,", "tokens": ["Wer", "sie", "recht", "br\u00fcns\u00b7tig\u00b7lich", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den hat der Tod getraut.", "tokens": ["Den", "hat", "der", "Tod", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gar s\u00fc\u00df mag solch ein Schlummer seyn", "tokens": ["Gar", "s\u00fc\u00df", "mag", "solch", "ein", "Schlum\u00b7mer", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PIAT", "ART", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In solcher Liebesnacht;", "tokens": ["In", "sol\u00b7cher", "Lie\u00b7bes\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Liebchens Armen schl\u00e4fst du ein,", "tokens": ["In", "Lieb\u00b7chens", "Ar\u00b7men", "schl\u00e4fst", "du", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getreu von ihr bewacht.", "tokens": ["Ge\u00b7treu", "von", "ihr", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn der Eiche gr\u00fcnes Holz", "tokens": ["Und", "wenn", "der", "Ei\u00b7che", "gr\u00fc\u00b7nes", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die neuen Bl\u00e4tter schwellt,", "tokens": ["Die", "neu\u00b7en", "Bl\u00e4t\u00b7ter", "schwellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So weckt sie dich mit freud'gem Stolz,", "tokens": ["So", "weckt", "sie", "dich", "mit", "freu\u00b7d'\u00b7gem", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zur ew'gen Freiheitswelt.", "tokens": ["Zur", "ew'\u00b7gen", "Frei\u00b7heits\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum wie sie f\u00e4llt und wie sie steigt,", "tokens": ["Drum", "wie", "sie", "f\u00e4llt", "und", "wie", "sie", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schicksals rasche Bahn,", "tokens": ["Des", "Schick\u00b7sals", "ra\u00b7sche", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin das Gl\u00fcck der Schlachten neigt,", "tokens": ["Wo\u00b7hin", "das", "Gl\u00fcck", "der", "Schlach\u00b7ten", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schauens ruhig an.", "tokens": ["Wir", "schau\u00b7ens", "ru\u00b7hig", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr deutsche Freiheit woll'n wir stehn,", "tokens": ["F\u00fcr", "deut\u00b7sche", "Frei\u00b7heit", "woll'n", "wir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei's nun im Grabes Schoo\u00df,", "tokens": ["Sei's", "nun", "im", "Gra\u00b7bes", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sei's oben auf des Sieges H\u00f6hn,", "tokens": ["Sei's", "o\u00b7ben", "auf", "des", "Sie\u00b7ges", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir preisen unser Loos.", "tokens": ["Wir", "prei\u00b7sen", "un\u00b7ser", "Loos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn uns Gott den Sieg gew\u00e4hrt,", "tokens": ["Und", "wenn", "uns", "Gott", "den", "Sieg", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hilft euch euer Spott? \u2014", "tokens": ["Was", "hilft", "euch", "eu\u00b7er", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja! Gottes Arm f\u00fchrt unser Schwerdt,", "tokens": ["Ja", "!", "Got\u00b7tes", "Arm", "f\u00fchrt", "un\u00b7ser", "Schwerdt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Schild ist Gott!", "tokens": ["Und", "un\u00b7ser", "Schild", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon st\u00fcrmt es m\u00e4chtig rings umher,", "tokens": ["Schon", "st\u00fcrmt", "es", "m\u00e4ch\u00b7tig", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum, edler Hengst, frisch auf!", "tokens": ["Drum", ",", "ed\u00b7ler", "Hengst", ",", "frisch", "auf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn die Welt voll Teufel w\u00e4r',", "tokens": ["Und", "wenn", "die", "Welt", "voll", "Teu\u00b7fel", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Weg geht mitten drauf.", "tokens": ["Dein", "Weg", "geht", "mit\u00b7ten", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}