{"textgrid.poem.23886": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Des Narren Regenlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regen\u00f6de, regen\u00f6de", "tokens": ["Re\u00b7ge\u00b7n\u00f6\u00b7de", ",", "re\u00b7ge\u00b7n\u00f6\u00b7de"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel, Land und See;", "tokens": ["Him\u00b7mel", ",", "Land", "und", "See", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Lust ist Last geworden,", "tokens": ["Al\u00b7le", "Lust", "ist", "Last", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NE", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Herz thut weh.", "tokens": ["Und", "das", "Herz", "thut", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Graugespinstig h\u00e4lt ein Nebel", "tokens": ["Grau\u00b7ge\u00b7spins\u00b7tig", "h\u00e4lt", "ein", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Sein in Haft,", "tokens": ["Al\u00b7les", "Sein", "in", "Haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weher Mut weint in die Weiten,", "tokens": ["We\u00b7her", "Mut", "weint", "in", "die", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Krank ist jede Kraft.", "tokens": ["Krank", "ist", "je\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die Prinzessin sitzt im Turme;", "tokens": ["Die", "Prin\u00b7zes\u00b7sin", "sitzt", "im", "Tur\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihre Harfe klingt,", "tokens": ["Ih\u00b7re", "Har\u00b7fe", "klingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich h\u00f6r, wie ihre Seele", "tokens": ["Und", "ich", "h\u00f6r", ",", "wie", "ih\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcde Sehnsucht singt.", "tokens": ["M\u00fc\u00b7de", "Sehn\u00b7sucht", "singt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Regen\u00f6de, regen\u00f6de", "tokens": ["Re\u00b7ge\u00b7n\u00f6\u00b7de", ",", "re\u00b7ge\u00b7n\u00f6\u00b7de"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel, Land und See;", "tokens": ["Him\u00b7mel", ",", "Land", "und", "See", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Lust ist Last geworden,", "tokens": ["Al\u00b7le", "Lust", "ist", "Last", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NE", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Herz thut weh.", "tokens": ["Und", "das", "Herz", "thut", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}