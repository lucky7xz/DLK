{"textgrid.poem.62843": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Stolz des Alterthums, Timanth,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Stolz des Alterthums, Timanth,", "tokens": ["Der", "Stolz", "des", "Al\u00b7ter\u00b7thums", ",", "Ti\u00b7man\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Versucht es einst den Zevs zu malen.", "tokens": ["Ver\u00b7sucht", "es", "einst", "den", "Zevs", "zu", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wild rollt sein Aug und jede Hand", "tokens": ["Wild", "rollt", "sein", "Aug", "und", "je\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "KON", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Scho\u00df einen Strom von Donnerstralen.", "tokens": ["Scho\u00df", "ei\u00b7nen", "Strom", "von", "Don\u00b7ner\u00b7stra\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ein Bauer sah ihm gierig zu", "tokens": ["Ein", "Bau\u00b7er", "sah", "ihm", "gie\u00b7rig", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sagte: Warum l\u00e4ssest du", "tokens": ["Und", "sag\u00b7te", ":", "Wa\u00b7rum", "l\u00e4s\u00b7sest", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus jeder Hand ihm Blitze regnen?", "tokens": ["Aus", "je\u00b7der", "Hand", "ihm", "Blit\u00b7ze", "reg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit welcher soll er uns denn segnen?", "tokens": ["Mit", "wel\u00b7cher", "soll", "er", "uns", "denn", "seg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}