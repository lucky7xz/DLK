{"textgrid.poem.38794": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[thr\u00e4nen, ihr lichtbeschwingten Wunderkinder]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thr\u00e4nen, ihr lichtbeschwingten Wunderkinder,", "tokens": ["Thr\u00e4\u00b7nen", ",", "ihr", "licht\u00b7be\u00b7schwing\u00b7ten", "Wun\u00b7der\u00b7kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihr heilgen Bothen, die aus dunkeln Schachten,", "tokens": ["Ihr", "heil\u00b7gen", "Bo\u00b7then", ",", "die", "aus", "dun\u00b7keln", "Schach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu denen keine Strahlen Zeugni\u00df brachten,", "tokens": ["Zu", "de\u00b7nen", "kei\u00b7ne", "Strah\u00b7len", "Zeug\u00b7ni\u00df", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch unerforschte Wege mit gelinder", "tokens": ["Durch", "un\u00b7er\u00b7forschte", "We\u00b7ge", "mit", "ge\u00b7lin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gewalt hinbrechen: wann das Herz in blinder", "tokens": ["Ge\u00b7walt", "hin\u00b7bre\u00b7chen", ":", "wann", "das", "Herz", "in", "blin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "PWAV", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verh\u00fcllung klagte, Sinnen nicht mehr dachten,", "tokens": ["Ver\u00b7h\u00fcl\u00b7lung", "klag\u00b7te", ",", "Sin\u00b7nen", "nicht", "mehr", "dach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Glaub' und Hoffnung nur als Traum verlachten,", "tokens": ["Und", "Glaub'", "und", "Hoff\u00b7nung", "nur", "als", "Traum", "ver\u00b7lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben starb, und Lieb und Andacht minder", "tokens": ["Das", "Le\u00b7ben", "starb", ",", "und", "Lieb", "und", "An\u00b7dacht", "min\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon leuchten, fast erlischt der letzte Schimmer:", "tokens": ["Schon", "leuch\u00b7ten", ",", "fast", "er\u00b7lischt", "der", "letz\u00b7te", "Schim\u00b7mer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann blickt aus ferner W\u00fcst' ein alt Erinnern,", "tokens": ["Dann", "blickt", "aus", "fer\u00b7ner", "W\u00fcst'", "ein", "alt", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seelge R\u00fchrung winkt, ein schmelzend Sehnen", "tokens": ["Und", "seel\u00b7ge", "R\u00fch\u00b7rung", "winkt", ",", "ein", "schmel\u00b7zend", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "W\u00e4chst nah und n\u00e4her: pl\u00f6tzlich durch die Tr\u00fcmmer", "tokens": ["W\u00e4chst", "nah", "und", "n\u00e4\u00b7her", ":", "pl\u00f6tz\u00b7lich", "durch", "die", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$.", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bricht wie ein Blitz durch jede Kraft des Innern", "tokens": ["Bricht", "wie", "ein", "Blitz", "durch", "je\u00b7de", "Kraft", "des", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Liebesgru\u00df und gl\u00e4nzt in Sieger-Thr\u00e4nen.", "tokens": ["Der", "Lie\u00b7bes\u00b7gru\u00df", "und", "gl\u00e4nzt", "in", "Sie\u00b7ger\u00b7Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}