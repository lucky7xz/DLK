{"textgrid.poem.62143": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Hannah Thorsch", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Bl\u00fcte abgefallen! \u2013", "tokens": ["Ei\u00b7ne", "Bl\u00fc\u00b7te", "ab\u00b7ge\u00b7fal\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, die lieblichste von allen,", "tokens": ["Ach", ",", "die", "lieb\u00b7lichs\u00b7te", "von", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Hannah fiel,", "tokens": ["Uns\u00b7re", "Han\u00b7nah", "fiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hin ist Lust und Spiel.", "tokens": ["Hin", "ist", "Lust", "und", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alle Freuden j\u00e4h verhallen,", "tokens": ["Al\u00b7le", "Freu\u00b7den", "j\u00e4h", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagen \u00fcberall erschallen,", "tokens": ["Kla\u00b7gen", "\u00fc\u00b7be\u00b7rall", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, noch oft und viel,", "tokens": ["Ach", ",", "noch", "oft", "und", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ohne Zweck und Ziel. \u2013", "tokens": ["Oh\u00b7ne", "Zweck", "und", "Ziel", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Klaget nicht, die Seelen leben,", "tokens": ["Kla\u00b7get", "nicht", ",", "die", "See\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fccklicher sie sich erheben,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "sie", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Welt zu Welt sie schweben \u2013", "tokens": ["Und", "von", "Welt", "zu", "Welt", "sie", "schwe\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz entr\u00fcckt dem niedren Staube,", "tokens": ["Ganz", "ent\u00b7r\u00fcckt", "dem", "nied\u00b7ren", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keinem Schmerze je zum Raube:", "tokens": ["Kei\u00b7nem", "Schmer\u00b7ze", "je", "zum", "Rau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das ist des Deisten Glaube!", "tokens": ["Das", "ist", "des", "De\u00b7is\u00b7ten", "Glau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}