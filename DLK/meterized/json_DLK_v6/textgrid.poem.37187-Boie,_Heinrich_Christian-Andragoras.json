{"textgrid.poem.37187": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Andragoras", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00f6hlich schmauste mit uns Andragoras, fr\u00f6hlich auch w\u00fcnscht er", "tokens": ["Fr\u00f6h\u00b7lich", "schmaus\u00b7te", "mit", "uns", "An\u00b7dra\u00b7go\u00b7ras", ",", "fr\u00f6h\u00b7lich", "auch", "w\u00fcnscht", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "NE", "$,", "ADJD", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Gute Nacht, und fr\u00fch fand man im Lager ihn todt.", "tokens": ["Gu\u00b7te", "Nacht", ",", "und", "fr\u00fch", "fand", "man", "im", "La\u00b7ger", "ihn", "todt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "PIS", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Was so schnell ihn get\u00f6dtet, den bl\u00fchenden J\u00fcngling, das fragst Du?", "tokens": ["Was", "so", "schnell", "ihn", "ge\u00b7t\u00f6d\u00b7tet", ",", "den", "bl\u00fc\u00b7hen\u00b7den", "J\u00fcng\u00b7ling", ",", "das", "fragst", "Du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Freund, er hatte den Arzt Bullus im Traume gesehn.", "tokens": ["Freund", ",", "er", "hat\u00b7te", "den", "Arzt", "Bul\u00b7lus", "im", "Trau\u00b7me", "ge\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}