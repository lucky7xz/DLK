{"textgrid.poem.48118": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: ... Und aufw\u00e4rts schlug aus Schnee und Eis", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "... Und aufw\u00e4rts schlug aus Schnee und Eis", "tokens": ["...", "Und", "auf\u00b7w\u00e4rts", "schlug", "aus", "Schnee", "und", "Eis"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Riesenbrand zum Himmelsdom,", "tokens": ["der", "Rie\u00b7sen\u00b7brand", "zum", "Him\u00b7mels\u00b7dom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und niederw\u00e4rts rann rot und hei\u00df", "tokens": ["und", "nie\u00b7der\u00b7w\u00e4rts", "rann", "rot", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Herzblut in den starren Strom", "tokens": ["das", "Herz\u00b7blut", "in", "den", "star\u00b7ren", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und ward wie Tau. Und Schu\u00df auf Schu\u00df", "tokens": ["und", "ward", "wie", "Tau", ".", "Und", "Schu\u00df", "auf", "Schu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "KOKOM", "NN", "$.", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "verkrachte in den Flockentanz:", "tokens": ["ver\u00b7krach\u00b7te", "in", "den", "Flo\u00b7cken\u00b7tanz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "das war der warme Liebesgru\u00df", "tokens": ["das", "war", "der", "war\u00b7me", "Lie\u00b7bes\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "vom V\u00e4terchen des Vaterlands.", "tokens": ["vom", "V\u00e4\u00b7ter\u00b7chen", "des", "Va\u00b7ter\u00b7lands", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie stiegen aus des Elends Schoo\u00df", "tokens": ["Sie", "stie\u00b7gen", "aus", "des", "E\u00b7lends", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie Nachtgetier aus d\u00fcstrem Grund.", "tokens": ["wie", "Nacht\u00b7ge\u00b7tier", "aus", "d\u00fcst\u00b7rem", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbein wenig Sonne unsrem Los!\u00ab \u2013", "tokens": ["\u00bb", "ein", "we\u00b7nig", "Son\u00b7ne", "uns\u00b7rem", "Los", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch baten sie mit blassem Mund.", "tokens": ["Noch", "ba\u00b7ten", "sie", "mit", "blas\u00b7sem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch trugen sie des Heilands Holz,", "tokens": ["Noch", "tru\u00b7gen", "sie", "des", "Hei\u00b7lands", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "des Zaren Bild in erster Reih' \u2013", "tokens": ["des", "Za\u00b7ren", "Bild", "in", "ers\u00b7ter", "Reih'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "da: von der Sehne flog der Bolz", "tokens": ["da", ":", "von", "der", "Seh\u00b7ne", "flog", "der", "Bolz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und schlug das bleiche Bild entzwei!", "tokens": ["und", "schlug", "das", "blei\u00b7che", "Bild", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun war's geschehn. Das war der Krieg.", "tokens": ["Nun", "wa\u00b7r's", "ge\u00b7schehn", ".", "Das", "war", "der", "Krieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das erste Opfer lag im Schnee \u2013", "tokens": ["Das", "ers\u00b7te", "Op\u00b7fer", "lag", "im", "Schnee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und \u00fcber Kreuz und Krone stieg", "tokens": ["und", "\u00fc\u00b7ber", "Kreuz", "und", "Kro\u00b7ne", "stieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die rote Fahne in die H\u00f6h'.", "tokens": ["die", "ro\u00b7te", "Fah\u00b7ne", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Brausen klang wie Fr\u00fchlingsflut,", "tokens": ["Ein", "Brau\u00b7sen", "klang", "wie", "Fr\u00fch\u00b7lings\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein Echo dr\u00f6hnte dumpf und hohl . . .", "tokens": ["ein", "E\u00b7cho", "dr\u00f6hn\u00b7te", "dumpf", "und", "hohl", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie hei\u00dfer Volkszorn wuchs die Glut", "tokens": ["Wie", "hei\u00b7\u00dfer", "Volks\u00b7zorn", "wuchs", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "in Moskwa und Sewastopol.", "tokens": ["in", "Mosk\u00b7wa", "und", "Se\u00b7was\u00b7to\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Das Reich zerbricht. Die Zwingburg birst.", "tokens": ["Das", "Reich", "zer\u00b7bricht", ".", "Die", "Zwing\u00b7burg", "birst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volkes tiefste Kraft wird wach.", "tokens": ["Des", "Vol\u00b7kes", "tiefs\u00b7te", "Kraft", "wird", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm hilft kein Gott, ihm hilft kein F\u00fcrst", "tokens": ["Ihm", "hilft", "kein", "Gott", ",", "ihm", "hilft", "kein", "F\u00fcrst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "aus seiner tausendj\u00e4hrigen Schmach.", "tokens": ["aus", "sei\u00b7ner", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "Schmach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sein eigner Retter soll es sein \u2013", "tokens": ["Sein", "eig\u00b7ner", "Ret\u00b7ter", "soll", "es", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "hell klingt sein Ruf wie hallend Erz \u2013", "tokens": ["hell", "klingt", "sein", "Ruf", "wie", "hal\u00b7lend", "Erz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "KOKOM", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und aufrecht steht's in blutiger Pein", "tokens": ["und", "auf\u00b7recht", "steht's", "in", "blu\u00b7ti\u00b7ger", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "und hebt die Toten sonnenw\u00e4rts.", "tokens": ["und", "hebt", "die", "To\u00b7ten", "son\u00b7nen\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und den dies Land einst Vater hie\u00df,", "tokens": ["Und", "den", "dies", "Land", "einst", "Va\u00b7ter", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PDS", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wo weilt er heut am Tag der Not?", "tokens": ["wo", "weilt", "er", "heut", "am", "Tag", "der", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Feigling, der sein Volk verlie\u00df!", "tokens": ["Ein", "Feig\u00b7ling", ",", "der", "sein", "Volk", "ver\u00b7lie\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinem Volke ist er tot . . .", "tokens": ["Und", "sei\u00b7nem", "Vol\u00b7ke", "ist", "er", "tot", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun st\u00fcrze, was im Innern kracht,", "tokens": ["Nun", "st\u00fcr\u00b7ze", ",", "was", "im", "In\u00b7nern", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und lodre, was da g\u00e4rt und gl\u00fcht,", "tokens": ["und", "lod\u00b7re", ",", "was", "da", "g\u00e4rt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und leuchte auf aus tiefster Nacht", "tokens": ["und", "leuch\u00b7te", "auf", "aus", "tiefs\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der Tag, wo rot die Freiheit bl\u00fcht!", "tokens": ["der", "Tag", ",", "wo", "rot", "die", "Frei\u00b7heit", "bl\u00fcht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}