{"textgrid.poem.36349": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Siderie", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnte Trost vom Himmel scheinen,", "tokens": ["O", "k\u00f6nn\u00b7te", "Trost", "vom", "Him\u00b7mel", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Hoffnung in dein sanftes Herz!", "tokens": ["Und", "Hoff\u00b7nung", "in", "dein", "sanf\u00b7tes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Leid, ich macht' es gern zum meinen,", "tokens": ["Dein", "Leid", ",", "ich", "macht'", "es", "gern", "zum", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mitklagen wollt' ich, mit dir weinen", "tokens": ["Mit\u00b7kla\u00b7gen", "wollt'", "ich", ",", "mit", "dir", "wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und g\u00e4be ganz mich hin dem Schmerz.", "tokens": ["Und", "g\u00e4\u00b7be", "ganz", "mich", "hin", "dem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als Tr\u00e4nen in dein Auge drangen,", "tokens": ["Als", "Tr\u00e4\u00b7nen", "in", "dein", "Au\u00b7ge", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da r\u00fchrt' es tief mir an die Brust,", "tokens": ["Da", "r\u00fchrt'", "es", "tief", "mir", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wir von Trauer all' umfangen,", "tokens": ["Wie", "wir", "von", "Trau\u00b7er", "all'", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Licht der Liebe einzig hangen,", "tokens": ["Am", "Licht", "der", "Lie\u00b7be", "ein\u00b7zig", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Sehnsucht, Schmerz und dunkler Lust.", "tokens": ["In", "Sehn\u00b7sucht", ",", "Schmerz", "und", "dunk\u00b7ler", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was mich bedr\u00e4ngt, das ruht verschlossen,", "tokens": ["Was", "mich", "be\u00b7dr\u00e4ngt", ",", "das", "ruht", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wie in dunkler Grabeskluft;", "tokens": ["So", "wie", "in", "dunk\u00b7ler", "Gra\u00b7bes\u00b7kluft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Anblick hat den Fels erschlossen,", "tokens": ["Dein", "An\u00b7blick", "hat", "den", "Fels", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mild in Tr\u00e4nen hingeflossen", "tokens": ["Da\u00df", "mild", "in", "Tr\u00e4\u00b7nen", "hin\u00b7ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Gram aus seiner alten Gruft.", "tokens": ["Der", "Gram", "aus", "sei\u00b7ner", "al\u00b7ten", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Anblick, Huldin! hat aufs neue", "tokens": ["Dein", "An\u00b7blick", ",", "Hul\u00b7din", "!", "hat", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NE", "$.", "VAFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir innen das Gem\u00fct bewegt,", "tokens": ["Mir", "in\u00b7nen", "das", "Ge\u00b7m\u00fct", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blume der Demut und der Treue!", "tokens": ["Blu\u00b7me", "der", "De\u00b7mut", "und", "der", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da\u00df ich der Erde gern mich freue,", "tokens": ["Da\u00df", "ich", "der", "Er\u00b7de", "gern", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sie noch solche Lilien tr\u00e4gt.", "tokens": ["Wenn", "sie", "noch", "sol\u00b7che", "Li\u00b7li\u00b7en", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wie wir am Abend aufw\u00e4rts schauen,", "tokens": ["Wie", "wir", "am", "A\u00b7bend", "auf\u00b7w\u00e4rts", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wild in st\u00fcrmischer Begier,", "tokens": ["Nicht", "wild", "in", "st\u00fcr\u00b7mi\u00b7scher", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit sanfter Wehmut zu den blauen", "tokens": ["Mit", "sanf\u00b7ter", "Weh\u00b7mut", "zu", "den", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Licht geschm\u00fcckten Sternenauen,", "tokens": ["In", "Licht", "ge\u00b7schm\u00fcck\u00b7ten", "Ster\u00b7ne\u00b7nau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So schau ich, s\u00fc\u00dfe Magd, zu dir.", "tokens": ["So", "schau", "ich", ",", "s\u00fc\u00b7\u00dfe", "Magd", ",", "zu", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich sehe dich mit leisem Beben,", "tokens": ["Ich", "se\u00b7he", "dich", "mit", "lei\u00b7sem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obschon mich keine Furcht bewegt;", "tokens": ["Ob\u00b7schon", "mich", "kei\u00b7ne", "Furcht", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rein bist du, frei, dem Gott ergeben,", "tokens": ["Rein", "bist", "du", ",", "frei", ",", "dem", "Gott", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der, wie Gefahren dich umschweben,", "tokens": ["Der", ",", "wie", "Ge\u00b7fah\u00b7ren", "dich", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Liebend die Arme um dich schl\u00e4gt.", "tokens": ["Lie\u00b7bend", "die", "Ar\u00b7me", "um", "dich", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Der deutschen Frauen Zier und Blume", "tokens": ["Der", "deut\u00b7schen", "Frau\u00b7en", "Zier", "und", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seh ich in deinem Bild erneut,", "tokens": ["Seh", "ich", "in", "dei\u00b7nem", "Bild", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einst zu sch\u00f6nrer Zeiten Ruhme,", "tokens": ["Die", "einst", "zu", "sch\u00f6n\u00b7rer", "Zei\u00b7ten", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In frommer Minne Heiligtume,", "tokens": ["In", "from\u00b7mer", "Min\u00b7ne", "Hei\u00b7lig\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geweihter Helden Aug' erfreut.", "tokens": ["Ge\u00b7weih\u00b7ter", "Hel\u00b7den", "Aug'", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}