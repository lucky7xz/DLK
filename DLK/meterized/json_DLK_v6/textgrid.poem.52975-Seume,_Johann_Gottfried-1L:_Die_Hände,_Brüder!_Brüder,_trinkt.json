{"textgrid.poem.52975": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die H\u00e4nde, Br\u00fcder! Br\u00fcder, trinkt", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00e4nde, Br\u00fcder! Br\u00fcder, trinkt", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "Br\u00fc\u00b7der", "!", "Br\u00fc\u00b7der", ",", "trinkt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$.", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der edeln Traube Feuergeist!", "tokens": ["Der", "e\u00b7deln", "Trau\u00b7be", "Feu\u00b7er\u00b7geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur\u00fcck von hier; fort, wem, wenn Tugend winkt,", "tokens": ["Zu\u00b7r\u00fcck", "von", "hier", ";", "fort", ",", "wem", ",", "wenn", "Tu\u00b7gend", "winkt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "$.", "PTKVZ", "$,", "PWS", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Blut nicht schnell zum Herzen kreist,", "tokens": ["Das", "Blut", "nicht", "schnell", "zum", "Her\u00b7zen", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht schnell die Faust zum Schwerte rei\u00dft!", "tokens": ["Nicht", "schnell", "die", "Faust", "zum", "Schwer\u00b7te", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Bund, der eines Schwurs bedarf,", "tokens": ["Der", "Bund", ",", "der", "ei\u00b7nes", "Schwurs", "be\u00b7darf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ein Insect, das Sectenwuth,", "tokens": ["Ist", "ein", "In\u00b7sect", ",", "das", "Sec\u00b7ten\u00b7wuth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gifthauch voll, in Gottes Garten warf:", "tokens": ["Von", "Gift\u00b7hauch", "voll", ",", "in", "Got\u00b7tes", "Gar\u00b7ten", "warf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weg mit dem Schwur! Wir haben Muth;", "tokens": ["Weg", "mit", "dem", "Schwur", "!", "Wir", "ha\u00b7ben", "Muth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Bund ist sch\u00f6n, die Sache gut.", "tokens": ["Der", "Bund", "ist", "sch\u00f6n", ",", "die", "Sa\u00b7che", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcr Freyheit, die kein F\u00fcrstenknecht,", "tokens": ["F\u00fcr", "Frey\u00b7heit", ",", "die", "kein", "F\u00fcrs\u00b7ten\u00b7knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Demagog, kein Bonze raubt!", "tokens": ["Kein", "De\u00b7ma\u00b7gog", ",", "kein", "Bon\u00b7ze", "raubt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir stehen nur f\u00fcr Pflicht, Vernunft und Recht,", "tokens": ["Wir", "ste\u00b7hen", "nur", "f\u00fcr", "Pflicht", ",", "Ver\u00b7nunft", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie in dem Sturm ein Felsenhaupt,", "tokens": ["Wie", "in", "dem", "Sturm", "ein", "Fel\u00b7sen\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn rechts und links die Woge schnaubt.", "tokens": ["Wenn", "rechts", "und", "links", "die", "Wo\u00b7ge", "schnaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es werde Licht! und weh dem Mann,", "tokens": ["Es", "wer\u00b7de", "Licht", "!", "und", "weh", "dem", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der dieses Licht zu l\u00f6schen wagt;", "tokens": ["Der", "die\u00b7ses", "Licht", "zu", "l\u00f6\u00b7schen", "wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wehe dem, der schw\u00e4rmend zum Vulkan", "tokens": ["Und", "we\u00b7he", "dem", ",", "der", "schw\u00e4r\u00b7mend", "zum", "Vul\u00b7kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Funken, der zum Gl\u00fccke tagt,", "tokens": ["Den", "Fun\u00b7ken", ",", "der", "zum", "Gl\u00fc\u00b7cke", "tagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In des Verderbens Flamme jagt.", "tokens": ["In", "des", "Ver\u00b7der\u00b7bens", "Flam\u00b7me", "jagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf Br\u00fcder, trinkt den heilgen Wein,", "tokens": ["Auf", "Br\u00fc\u00b7der", ",", "trinkt", "den", "heil\u00b7gen", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trinkt ihn zum Bund der Wahrheit hier!", "tokens": ["Trinkt", "ihn", "zum", "Bund", "der", "Wahr\u00b7heit", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir ehren Gott, wenn wir uns menschlich freun,", "tokens": ["Wir", "eh\u00b7ren", "Gott", ",", "wenn", "wir", "uns", "menschlich", "freun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Menschheit ruft, wir leben ihr;", "tokens": ["Die", "Menschheit", "ruft", ",", "wir", "le\u00b7ben", "ihr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und wenn sie fordert, sterben wir.", "tokens": ["Und", "wenn", "sie", "for\u00b7dert", ",", "ster\u00b7ben", "wir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die H\u00e4nde, Br\u00fcder! Br\u00fcder, trinkt", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "Br\u00fc\u00b7der", "!", "Br\u00fc\u00b7der", ",", "trinkt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$.", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der edeln Traube Feuergeist!", "tokens": ["Der", "e\u00b7deln", "Trau\u00b7be", "Feu\u00b7er\u00b7geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur\u00fcck von hier; fort, wem, wenn Tugend winkt,", "tokens": ["Zu\u00b7r\u00fcck", "von", "hier", ";", "fort", ",", "wem", ",", "wenn", "Tu\u00b7gend", "winkt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "$.", "PTKVZ", "$,", "PWS", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Blut nicht schnell zum Herzen kreist,", "tokens": ["Das", "Blut", "nicht", "schnell", "zum", "Her\u00b7zen", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht schnell die Faust zum Schwerte rei\u00dft!", "tokens": ["Nicht", "schnell", "die", "Faust", "zum", "Schwer\u00b7te", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}