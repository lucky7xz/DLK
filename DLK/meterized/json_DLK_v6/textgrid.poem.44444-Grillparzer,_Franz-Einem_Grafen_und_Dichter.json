{"textgrid.poem.44444": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Einem Grafen und Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auersperg, du ", "tokens": ["Au\u00b7er\u00b7sperg", ",", "du"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Eines Stamms, der ruhmbelaubt,", "tokens": ["Ei\u00b7nes", "Stamms", ",", "der", "ruhm\u00b7be\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streit nicht mehr im Helmesgitter,", "tokens": ["Streit", "nicht", "mehr", "im", "Hel\u00b7mes\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeig dein freies, edles Haupt!", "tokens": ["Zeig", "dein", "frei\u00b7es", ",", "ed\u00b7les", "Haupt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht mehr ", "tokens": ["Nicht", "mehr"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Reif und hoch, zu hoch dem Zwerg,", "tokens": ["Reif", "und", "hoch", ",", "zu", "hoch", "dem", "Zwerg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Gehst ja in der V\u00e4ter Bahnen,", "tokens": ["Gehst", "ja", "in", "der", "V\u00e4\u00b7ter", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4mpfst f\u00fcr Wahrheit und f\u00fcr Recht;", "tokens": ["K\u00e4mpfst", "f\u00fcr", "Wahr\u00b7heit", "und", "f\u00fcr", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau! es sehn auf dich die Ahnen", "tokens": ["Schau", "!", "es", "sehn", "auf", "dich", "die", "Ah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erkennen ihr Geschlecht.", "tokens": ["Und", "er\u00b7ken\u00b7nen", "ihr", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So wie sie in fernen Tagen,", "tokens": ["So", "wie", "sie", "in", "fer\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als der Muselmann gedr\u00e4ut,", "tokens": ["Als", "der", "Mu\u00b7sel\u00b7mann", "ge\u00b7dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Manche hei\u00dfe Schlacht geschlagen", "tokens": ["Man\u00b7che", "hei\u00b7\u00dfe", "Schlacht", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Vaterherd befreit,", "tokens": ["Und", "den", "Va\u00b7ter\u00b7herd", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ziert den Musenro\u00df-Berittnen,", "tokens": ["Ziert", "den", "Mu\u00b7sen\u00b7ro\u00df\u00b7Be\u00b7ritt\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Sohn, der Kampf zumeist", "tokens": ["Ih\u00b7ren", "Sohn", ",", "der", "Kampf", "zu\u00b7meist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Herz- und Geist-Beschnittnen,", "tokens": ["Mit", "den", "Her\u00b7z", "und", "Geist\u00b7Be\u00b7schnitt\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Den Ungl\u00e4ubgen an den Geist.", "tokens": ["Den", "Un\u00b7gl\u00e4ub\u00b7gen", "an", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und ob Vorteil kaum zu hoffen", "tokens": ["Und", "ob", "Vor\u00b7teil", "kaum", "zu", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem ungleich schweren Krieg,", "tokens": ["In", "dem", "un\u00b7gleich", "schwe\u00b7ren", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei kein Stillstand doch getroffen,", "tokens": ["Sei", "kein", "Still\u00b7stand", "doch", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo nicht weichen schon ein Sieg.", "tokens": ["Wo", "nicht", "wei\u00b7chen", "schon", "ein", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00fcrde selbst das Gl\u00fcck Verr\u00e4ter,", "tokens": ["W\u00fcr\u00b7de", "selbst", "das", "Gl\u00fcck", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4me des Erliegens Tag,", "tokens": ["K\u00e4\u00b7me", "des", "Er\u00b7lie\u00b7gens", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk an jenen deiner V\u00e4ter,", "tokens": ["Denk", "an", "je\u00b7nen", "dei\u00b7ner", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der in Stambuls Kerkern lag.", "tokens": ["Der", "in", "Stam\u00b7buls", "Ker\u00b7kern", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie da der Bostandschi dr\u00e4ute,", "tokens": ["Wie", "da", "der", "Bos\u00b7tand\u00b7schi", "dr\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Grimm des Sultans Angesicht;", "tokens": ["Grimm", "des", "Sul\u00b7tans", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All sein Gl\u00fcck gab er zur Beute,", "tokens": ["All", "sein", "Gl\u00fcck", "gab", "er", "zur", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Doch des Busens Wahrheit nicht.", "tokens": ["Doch", "des", "Bu\u00b7sens", "Wahr\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Welkte fern den heimschen Triften,", "tokens": ["Welk\u00b7te", "fern", "den", "heim\u00b7schen", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starb getrennt von Kind und Weib,", "tokens": ["Starb", "ge\u00b7trennt", "von", "Kind", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von zwei dargebotnen Giften", "tokens": ["Von", "zwei", "dar\u00b7ge\u00b7bot\u00b7nen", "Gif\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Trank er jenes f\u00fcr den Leib.", "tokens": ["Trank", "er", "je\u00b7nes", "f\u00fcr", "den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Also bleib am Rechten hangen,", "tokens": ["Al\u00b7so", "bleib", "am", "Rech\u00b7ten", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ob dich die Welt verl\u00e4\u00dft,", "tokens": ["Und", "ob", "dich", "die", "Welt", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sie dich aussp\u00e4hn, binden, fangen,", "tokens": ["Sie", "dich", "aus\u00b7sp\u00e4hn", ",", "bin\u00b7den", ",", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Halte du am Glauben fest,", "tokens": ["Hal\u00b7te", "du", "am", "Glau\u00b7ben", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Da\u00df, wenn einst zerst\u00e4ubt die Gitter", "tokens": ["Da\u00df", ",", "wenn", "einst", "zer\u00b7st\u00e4ubt", "die", "Git\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings um all, was gut und wahr,", "tokens": ["Rings", "um", "all", ",", "was", "gut", "und", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man dich gr\u00fc\u00dft als ", "tokens": ["Man", "dich", "gr\u00fc\u00dft", "als"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "PPER", "VVFIN", "KOKOM"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "In der Nachgekommnen Schar.", "tokens": ["In", "der", "Nach\u00b7ge\u00b7komm\u00b7nen", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Br\u00fccken, die nicht abgetragen,", "tokens": ["Br\u00fc\u00b7cken", ",", "die", "nicht", "ab\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Stamm und Gl\u00fcck entzweit,", "tokens": ["Ha\u00b7ben", "Stamm", "und", "Gl\u00fcck", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns vielmehr la\u00df Br\u00fccken schlagen", "tokens": ["Uns", "viel\u00b7mehr", "la\u00df", "Br\u00fc\u00b7cken", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIMP", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die be\u00dfre Enkelzeit!", "tokens": ["In", "die", "be\u00df\u00b7re", "En\u00b7kel\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}