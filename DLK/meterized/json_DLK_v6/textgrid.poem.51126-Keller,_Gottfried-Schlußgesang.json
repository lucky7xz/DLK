{"textgrid.poem.51126": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00dfgesang", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00dft auf den Ring, drin wir in Frieden tagten,", "tokens": ["Schlie\u00dft", "auf", "den", "Ring", ",", "drin", "wir", "in", "Frie\u00b7den", "tag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus treuer Brust entbietend unsern Rat!", "tokens": ["Aus", "treu\u00b7er", "Brust", "ent\u00b7bie\u00b7tend", "un\u00b7sern", "Rat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Zweifel fliehn, die lang am Herzen nagten,", "tokens": ["Die", "Zwei\u00b7fel", "fliehn", ",", "die", "lang", "am", "Her\u00b7zen", "nag\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit uns schreitet froh der Geist der Tat.", "tokens": ["Und", "mit", "uns", "schrei\u00b7tet", "froh", "der", "Geist", "der", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es mu\u00df, la\u00dft's laut erschallen,", "tokens": ["Es", "mu\u00df", ",", "la\u00dft's", "laut", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die letzte Zwingburg fallen!", "tokens": ["Die", "letz\u00b7te", "Zwing\u00b7burg", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann wall empor aus deiner dumpfen Gruft,", "tokens": ["Dann", "wall", "em\u00b7por", "aus", "dei\u00b7ner", "dum\u00b7pfen", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Seele, frei, wie Gottes goldne Luft!", "tokens": ["O", "See\u00b7le", ",", "frei", ",", "wie", "Got\u00b7tes", "gold\u00b7ne", "Luft", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Bergen rauscht's wie unsichtbare Fahnen,", "tokens": ["Von", "Ber\u00b7gen", "rauscht's", "wie", "un\u00b7sicht\u00b7ba\u00b7re", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Fl\u00fchen ruft's wie leise Geisterwacht;", "tokens": ["Von", "Fl\u00fc\u00b7hen", "ruft's", "wie", "lei\u00b7se", "Geis\u00b7ter\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gelagert lauscht das Schattenheer der Ahnen,", "tokens": ["Ge\u00b7la\u00b7gert", "lauscht", "das", "Schat\u00b7ten\u00b7heer", "der", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die uns den Leib von Ketten frei gemacht.", "tokens": ["Die", "uns", "den", "Leib", "von", "Ket\u00b7ten", "frei", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun t\u00f6nt ihr Sang hernieder", "tokens": ["Nun", "t\u00f6nt", "ihr", "Sang", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und hallt vom Felsen wider:", "tokens": ["Und", "hallt", "vom", "Fel\u00b7sen", "wi\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df dich nicht reun, lebendiges Geschlecht,", "tokens": ["La\u00df", "dich", "nicht", "reun", ",", "le\u00b7ben\u00b7di\u00b7ges", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "In deiner Zeit zu finden auch dein Recht!", "tokens": ["In", "dei\u00b7ner", "Zeit", "zu", "fin\u00b7den", "auch", "dein", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tut auf den Ring und zieht ihn weit und weiter", "tokens": ["Tut", "auf", "den", "Ring", "und", "zieht", "ihn", "weit", "und", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch tausend Boten \u00fcber Berg und Tal!", "tokens": ["Durch", "tau\u00b7send", "Bo\u00b7ten", "\u00fc\u00b7ber", "Berg", "und", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald gl\u00fcht der Bund und flammet stet und heiter,", "tokens": ["Bald", "gl\u00fcht", "der", "Bund", "und", "flam\u00b7met", "stet", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den V\u00f6lkern all ein friedlich Feuermal.", "tokens": ["Den", "V\u00f6l\u00b7kern", "all", "ein", "fried\u00b7lich", "Feu\u00b7er\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was schlecht ist, soll zerrinnen,", "tokens": ["Was", "schlecht", "ist", ",", "soll", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die L\u00fcge nicht gewinnen!", "tokens": ["Die", "L\u00fc\u00b7ge", "nicht", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein furchtlos Herz und offne Bruderhand", "tokens": ["Ein", "furcht\u00b7los", "Herz", "und", "off\u00b7ne", "Bru\u00b7der\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gewinnt den Sieg im alten Heimatland!", "tokens": ["Ge\u00b7winnt", "den", "Sieg", "im", "al\u00b7ten", "Hei\u00b7mat\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}