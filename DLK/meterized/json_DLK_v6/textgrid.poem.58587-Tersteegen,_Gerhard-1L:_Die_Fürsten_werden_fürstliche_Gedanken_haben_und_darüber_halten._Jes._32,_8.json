{"textgrid.poem.58587": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die F\u00fcrsten werden f\u00fcrstliche Gedanken haben und dar\u00fcber halten. Jes. 32, 8", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die F\u00fcrsten werden f\u00fcrstliche Gedanken haben und dar\u00fcber halten. Jes. 32, 8", "tokens": ["Die", "F\u00fcrs\u00b7ten", "wer\u00b7den", "f\u00fcrst\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", "ha\u00b7ben", "und", "da\u00b7r\u00fc\u00b7ber", "hal\u00b7ten", ".", "Jes", ".", "32", ",", "8"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "VAFIN", "KON", "PAV", "VVINF", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ein F\u00fcrst und Herr der Welt sollt' ich in Adam sein", "tokens": ["Ein", "F\u00fcrst", "und", "Herr", "der", "Welt", "sollt'", "ich", "in", "A\u00b7dam", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "VMFIN", "PPER", "APPR", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werde nun ein Sklav' aus Adams Stamm geboren;", "tokens": ["Und", "wer\u00b7de", "nun", "ein", "Skla\u00b7v'", "aus", "A\u00b7dams", "Stamm", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In ersten Adelsstand f\u00fchrt Christus wieder ein,", "tokens": ["In", "ers\u00b7ten", "A\u00b7delss\u00b7tand", "f\u00fchrt", "Chris\u00b7tus", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hat mich ihm zur Braut und K\u00f6nigin erkoren.", "tokens": ["Er", "hat", "mich", "ihm", "zur", "Braut", "und", "K\u00f6\u00b7ni\u00b7gin", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weg, weg denn, Kreatur, du bist mir viel zu schlecht,", "tokens": ["Weg", ",", "weg", "denn", ",", "Kre\u00b7a\u00b7tur", ",", "du", "bist", "mir", "viel", "zu", "schlecht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Geist und Herz und Sinn soll sich in Gott erheben,", "tokens": ["Mein", "Geist", "und", "Herz", "und", "Sinn", "soll", "sich", "in", "Gott", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "NN", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit gro\u00dfen Dingen nur ein F\u00fcrst zu handeln pflegt \u2013", "tokens": ["Mit", "gro\u00b7\u00dfen", "Din\u00b7gen", "nur", "ein", "F\u00fcrst", "zu", "han\u00b7deln", "pflegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sollt' ich nicht standsgem\u00e4\u00df vor Gottes Augen leben!", "tokens": ["Sollt'", "ich", "nicht", "stands\u00b7ge\u00b7m\u00e4\u00df", "vor", "Got\u00b7tes", "Au\u00b7gen", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}