{"dta.poem.20920": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Balduin an Judith.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Kan Judith durch den Dunst des Traurens et-", "tokens": ["Kan", "Ju\u00b7dith", "durch", "den", "Dunst", "des", "Trau\u00b7rens", "et"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "APPR", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was lesen/", "tokens": ["was", "le\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Beschwemmt die heisse Fluth nicht gantz ihr", "tokens": ["Be\u00b7schwemmt", "die", "heis\u00b7se", "Fluth", "nicht", "gantz", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sch\u00f6nes Licht/", "tokens": ["sch\u00f6\u00b7nes", "Licht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So fall auf dessen Brief/ der stets ihr Knecht gewesen/", "tokens": ["So", "fall", "auf", "des\u00b7sen", "Brief", "/", "der", "stets", "ihr", "Knecht", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PRELAT", "NN", "$(", "ART", "ADV", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein angenehmer Blick/ der Sinn und Siegel bricht.", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Blick", "/", "der", "Sinn", "und", "Sie\u00b7gel", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein weinen solte zwar zu deinen Thr\u00e4nen fl\u00fcssen/", "tokens": ["Mein", "wei\u00b7nen", "sol\u00b7te", "zwar", "zu", "dei\u00b7nen", "Thr\u00e4\u00b7nen", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und durch ein gleiches Ach begleiten deine Noth/", "tokens": ["Und", "durch", "ein", "glei\u00b7ches", "Ach", "be\u00b7glei\u00b7ten", "dei\u00b7ne", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es solte dieser Brief von nichts/ als Seuffzen wissen/", "tokens": ["Es", "sol\u00b7te", "die\u00b7ser", "Brief", "von", "nichts", "/", "als", "Seuff\u00b7zen", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "APPR", "PIS", "$(", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und blo\u00df in dem bestehn/ ist denn dein Adolph todt?", "tokens": ["Und", "blo\u00df", "in", "dem", "be\u00b7stehn", "/", "ist", "denn", "dein", "A\u00b7dolph", "todt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "VVINF", "$(", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wei\u00df/ ich solte nicht die treuen Seuffzer st\u00f6hren/", "tokens": ["Ich", "wei\u00df", "/", "ich", "sol\u00b7te", "nicht", "die", "treu\u00b7en", "Seuff\u00b7zer", "st\u00f6h\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ihrem K\u00f6nige bezahlen wahre Schuldt/", "tokens": ["Die", "ih\u00b7rem", "K\u00f6\u00b7ni\u00b7ge", "be\u00b7zah\u00b7len", "wah\u00b7re", "Schuldt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch heisse Liebe will nichts von Verzuge h\u00f6ren/", "tokens": ["Doch", "heis\u00b7se", "Lie\u00b7be", "will", "nichts", "von", "Ver\u00b7zu\u00b7ge", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du kennst ihr Feuer wohl/ es ist voll Ungedult.", "tokens": ["Du", "kennst", "ihr", "Feu\u00b7er", "wohl", "/", "es", "ist", "voll", "Un\u00b7ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$(", "PPER", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Judith dencke doch wer diesen Brief geschrieben/", "tokens": ["Und", "Ju\u00b7dith", "den\u00b7cke", "doch", "wer", "die\u00b7sen", "Brief", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "PWS", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du weist es gar genung/ es ist desselben Handt/", "tokens": ["Du", "weist", "es", "gar", "ge\u00b7nung", "/", "es", "ist", "des\u00b7sel\u00b7ben", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$(", "PPER", "VAFIN", "PDS", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der durch der Jahre Lauf dir rein und treu verblieben/", "tokens": ["Der", "durch", "der", "Jah\u00b7re", "Lauf", "dir", "rein", "und", "treu", "ver\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ja stets gefochten hat vor deines Vatern Landt.", "tokens": ["Ja", "stets", "ge\u00b7foch\u00b7ten", "hat", "vor", "dei\u00b7nes", "Va\u00b7tern", "Landt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gedencke K\u00f6nigin an unsrer Jugend Flammen/", "tokens": ["Ge\u00b7den\u00b7cke", "K\u00f6\u00b7ni\u00b7gin", "an", "uns\u00b7rer", "Ju\u00b7gend", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie mich das zarte Garn der sch\u00f6nen Augen fieng/", "tokens": ["Wie", "mich", "das", "zar\u00b7te", "Garn", "der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "fi\u00b7eng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Wie uns offt unverhofft der Vater fand beysammen/", "tokens": ["Wie", "uns", "offt", "un\u00b7ver\u00b7hofft", "der", "Va\u00b7ter", "fand", "bey\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da nichts als Lieb und Lust mit uns zu Rathe gieng.", "tokens": ["Da", "nichts", "als", "Lieb", "und", "Lust", "mit", "uns", "zu", "Ra\u00b7the", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "KON", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Erwege/ wie ich dich oft in den Morgenstunden", "tokens": ["Er\u00b7we\u00b7ge", "/", "wie", "ich", "dich", "oft", "in", "den", "Mor\u00b7gen\u00b7stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als der gekr\u00f6hnte Lentz mit Bluhmen sich geziert/", "tokens": ["Als", "der", "ge\u00b7kr\u00f6hn\u00b7te", "Lentz", "mit", "Bluh\u00b7men", "sich", "ge\u00b7ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dich Bluhme dieser Zeit bey Rosen habe funden/", "tokens": ["Dich", "Bluh\u00b7me", "die\u00b7ser", "Zeit", "bey", "Ro\u00b7sen", "ha\u00b7be", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PDAT", "NN", "APPR", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und deine Hand gek\u00fcst/ die hundert Lilgen f\u00fchrt.", "tokens": ["Und", "dei\u00b7ne", "Hand", "ge\u00b7k\u00fcst", "/", "die", "hun\u00b7dert", "Lil\u00b7gen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$(", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie oft hab ich gesagt; von tausent Nachtigalen", "tokens": ["Wie", "oft", "hab", "ich", "ge\u00b7sagt", ";", "von", "tau\u00b7sent", "Nach\u00b7ti\u00b7ga\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$.", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ist deiner Stimmen Klang/ O Sch\u00f6ne/ zugericht/", "tokens": ["Ist", "dei\u00b7ner", "Stim\u00b7men", "Klang", "/", "O", "Sch\u00f6\u00b7ne", "/", "zu\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$(", "NE", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie sch\u00f6n auch die Natur kan die Granaten mahlen/", "tokens": ["Wie", "sch\u00f6n", "auch", "die", "Na\u00b7tur", "kan", "die", "Gra\u00b7na\u00b7ten", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So gleichen sie gewis doch deinen Lippen nicht.", "tokens": ["So", "glei\u00b7chen", "sie", "ge\u00b7wis", "doch", "dei\u00b7nen", "Lip\u00b7pen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie ofte hab ich dir die fl\u00fcchtigen Narzissen", "tokens": ["Wie", "of\u00b7te", "hab", "ich", "dir", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mit Rosen untermengt auf deine Brust gelegt?", "tokens": ["Mit", "Ro\u00b7sen", "un\u00b7ter\u00b7mengt", "auf", "dei\u00b7ne", "Brust", "ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und hab aus Schertz gesagt: Jhr Bluhmen solt\u2019 es", "tokens": ["Und", "hab", "aus", "Schertz", "ge\u00b7sagt", ":", "Ihr", "Bluh\u00b7men", "solt'", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$.", "PPOSAT", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "wissen/", "tokens": ["wis\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Da\u00df auch der Winter hier Euch gleichen Zierath hegt.", "tokens": ["Da\u00df", "auch", "der", "Win\u00b7ter", "hier", "Euch", "glei\u00b7chen", "Zie\u00b7rath", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df hier ein warmer Schnee mit Bluhmen ist um-", "tokens": ["Da\u00df", "hier", "ein", "war\u00b7mer", "Schnee", "mit", "Bluh\u00b7men", "ist", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "geben/", "tokens": ["ge\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Dem Luft und Jahres Zeit kein Blat versehren kan;", "tokens": ["Dem", "Luft", "und", "Jah\u00b7res", "Zeit", "kein", "Blat", "ver\u00b7seh\u00b7ren", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und da\u00df den Rosen/ so auf gleichen Bergen schweben/", "tokens": ["Und", "da\u00df", "den", "Ro\u00b7sen", "/", "so", "auf", "glei\u00b7chen", "Ber\u00b7gen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$(", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Kein Nordwind noch zur Zeit hat einig Leid gethan.", "tokens": ["Kein", "Nord\u00b7wind", "noch", "zur", "Zeit", "hat", "ei\u00b7nig", "Leid", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "VAFIN", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie w\u00fcnscht ich dazumahl ein Lusthau\u00df hier zubauen/", "tokens": ["Wie", "w\u00fcnscht", "ich", "da\u00b7zu\u00b7mahl", "ein", "Lust\u00b7hau\u00df", "hier", "zu\u00b7bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Doch das Verh\u00e4ngn\u00fc\u00df ri\u00df den ersten Grundstein ein/", "tokens": ["Doch", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "ri\u00df", "den", "ers\u00b7ten", "Grund\u00b7stein", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich muste dich betr\u00fcbt in fremden H\u00e4nden schauen/", "tokens": ["Ich", "mus\u00b7te", "dich", "be\u00b7tr\u00fcbt", "in", "frem\u00b7den", "H\u00e4n\u00b7den", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Du soltest K\u00f6nigin und ich ein Sclave seyn.", "tokens": ["Du