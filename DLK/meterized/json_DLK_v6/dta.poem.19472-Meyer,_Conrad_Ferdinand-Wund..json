{"dta.poem.19472": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Walde fl\u00fccht' ich, ein gehetztes Wild,", "tokens": ["Zu", "Wal\u00b7de", "fl\u00fccht'", "ich", ",", "ein", "ge\u00b7hetz\u00b7tes", "Wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Inde\u00df der Abendhimmel purpurn quillt.", "tokens": ["In\u00b7de\u00df", "der", "A\u00b7bend\u00b7him\u00b7mel", "pur\u00b7purn", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich lieg und keuche. Zu mir rinnt herein", "tokens": ["Ich", "lieg", "und", "keu\u00b7che", ".", "Zu", "mir", "rinnt", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "$.", "APPR", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein stilles Bluten \u00fcber Moos und Stein.", "tokens": ["Ein", "stil\u00b7les", "Blu\u00b7ten", "\u00fc\u00b7ber", "Moos", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}