{"textgrid.poem.55686": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Vorgesang", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe geliebt", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7liebt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie Dichter lieben:", "tokens": ["Wie", "Dich\u00b7ter", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Aus tiefster Seele,", "tokens": ["Aus", "tiefs\u00b7ter", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit jedem Atom,", "tokens": ["Mit", "je\u00b7dem", "A\u00b7tom", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Schrankenlos und unbek\u00fcmmert", "tokens": ["Schran\u00b7ken\u00b7los", "und", "un\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um Menschensatzung.", "tokens": ["Um", "Men\u00b7schen\u00b7sat\u00b7zung", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der uferlose Strom", "tokens": ["Der", "u\u00b7fer\u00b7lo\u00b7se", "Strom"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Rasender Liebeswonnen", "tokens": ["Ra\u00b7sen\u00b7der", "Lie\u00b7bes\u00b7won\u00b7nen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Durchzog in wilden Wirbeln", "tokens": ["Durch\u00b7zog", "in", "wil\u00b7den", "Wir\u00b7beln"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein hochaufschlagendes Herz,", "tokens": ["Mein", "hoch\u00b7auf\u00b7schla\u00b7gen\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und in mir lebt", "tokens": ["Und", "in", "mir", "lebt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Leuchtende Erinnerung", "tokens": ["Leuch\u00b7ten\u00b7de", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+--", "measure": "dactylic.init"}, "line.7": {"text": "An liebesschw\u00fcle Stunden.", "tokens": ["An", "lie\u00b7bes\u00b7schw\u00fc\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aber auch der gierdelosen Liebe", "tokens": ["A\u00b7ber", "auch", "der", "gier\u00b7de\u00b7lo\u00b7sen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Milderes Feuer", "tokens": ["Mil\u00b7de\u00b7res", "Feu\u00b7er"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kennt meine Seele:", "tokens": ["Kennt", "mei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich habe geliebt,", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie Dichter lieben:", "tokens": ["Wie", "Dich\u00b7ter", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Anbetend, innig und keusch,", "tokens": ["An\u00b7be\u00b7tend", ",", "in\u00b7nig", "und", "keusch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "In tiefstem Schweigen,", "tokens": ["In", "tiefs\u00b7tem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In lautloser Seligkeit!", "tokens": ["In", "laut\u00b7lo\u00b7ser", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ob ich auch hohes Gl\u00fcck genossen \u2013", "tokens": ["A\u00b7ber", "ob", "ich", "auch", "ho\u00b7hes", "Gl\u00fcck", "ge\u00b7nos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mehr noch hab' ich gelitten,", "tokens": ["Mehr", "noch", "hab'", "ich", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Durch meine Seele zogen", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "G\u00f6tterwonnen und Titanenqualen!", "tokens": ["G\u00f6t\u00b7ter\u00b7won\u00b7nen", "und", "Ti\u00b7ta\u00b7nen\u00b7qua\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}