{"textgrid.poem.66205": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Ziethen", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandrer, steh", "tokens": ["Wand\u00b7rer", ",", "steh"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Im Kriege", "tokens": ["Im", "Krie\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wie der Seesturm wild;", "tokens": ["Wie", "der", "Sees\u00b7turm", "wild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Im Frieden", "tokens": ["Im", "Frie\u00b7den"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wie das Maienl\u00fcftchen mild.", "tokens": ["Wie", "das", "Mai\u00b7en\u00b7l\u00fcft\u00b7chen", "mild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein deutscher Mann,", "tokens": ["Ein", "deut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ein Held, ein Patriot,", "tokens": ["Ein", "Held", ",", "ein", "Pat\u00b7ri\u00b7ot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Noch mehr,", "tokens": ["Noch", "mehr", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "++", "measure": "spondeus"}, "line.10": {"text": "Ein Christ war Er.", "tokens": ["Ein", "Christ", "war", "Er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}