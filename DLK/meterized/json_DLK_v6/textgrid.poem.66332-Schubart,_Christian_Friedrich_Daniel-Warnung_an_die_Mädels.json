{"textgrid.poem.66332": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Warnung an die M\u00e4dels", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sah ein M\u00e4del jung und zart", "tokens": ["Es", "sah", "ein", "M\u00e4\u00b7del", "jung", "und", "zart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst einen Offizier.", "tokens": ["Einst", "ei\u00b7nen", "Of\u00b7fi\u00b7zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Port d'Epee, die Achselschnur", "tokens": ["Das", "Port", "d'\u00b7Epee", ",", "die", "Ach\u00b7sel\u00b7schnur"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine modische Frisur", "tokens": ["Und", "sei\u00b7ne", "mo\u00b7di\u00b7sche", "Fri\u00b7sur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Behagte trefflich ihr.", "tokens": ["Be\u00b7hag\u00b7te", "treff\u00b7lich", "ihr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Willst mich? so fragt der Offizier", "tokens": ["Willst", "mich", "?", "so", "fragt", "der", "Of\u00b7fi\u00b7zier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4del; husch: Ich will!", "tokens": ["Das", "M\u00e4\u00b7del", ";", "husch", ":", "Ich", "will", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "$.", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drauf war das P\u00e4rlein wohlgemuth,", "tokens": ["Drauf", "war", "das", "P\u00e4r\u00b7lein", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ging's dem Weiblein anfangs gut,", "tokens": ["Auch", "ging's", "dem", "Weib\u00b7lein", "an\u00b7fangs", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hatte H\u00fcll' und F\u00fcll'.", "tokens": ["Sie", "hat\u00b7te", "H\u00fcll'", "und", "F\u00fcll'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lang der Krieger fechten kann,", "tokens": ["So", "lang", "der", "Krie\u00b7ger", "fech\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt man ihn lieb und werth;", "tokens": ["H\u00e4lt", "man", "ihn", "lieb", "und", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Doch wenn ihm Jugendkraft gebricht,", "tokens": ["Doch", "wenn", "ihm", "Ju\u00b7gend\u00b7kraft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird des F\u00fcrsten Angesicht", "tokens": ["So", "wird", "des", "F\u00fcrs\u00b7ten", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar bald von ihm gekehrt.", "tokens": ["Gar", "bald", "von", "ihm", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Taratara! ins Feld! ins Feld!", "tokens": ["Ta\u00b7ra\u00b7ta\u00b7ra", "!", "ins", "Feld", "!", "ins", "Feld", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "NN", "$.", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Das arme Weiblein schreit;", "tokens": ["Das", "ar\u00b7me", "Weib\u00b7lein", "schreit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn bald Kart\u00e4tschenfeuer kam", "tokens": ["Denn", "bald", "Kar\u00b7t\u00e4t\u00b7schen\u00b7feu\u00b7er", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und scho\u00df ihr liebes M\u00e4nnchen lahm,", "tokens": ["Und", "scho\u00df", "ihr", "lie\u00b7bes", "M\u00e4nn\u00b7chen", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei aller Tapferkeit.", "tokens": ["Bei", "al\u00b7ler", "Tap\u00b7fer\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da lag der lahme Krieger da,", "tokens": ["Da", "lag", "der", "lah\u00b7me", "Krie\u00b7ger", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn heilte keine Kur;", "tokens": ["Ihn", "heil\u00b7te", "kei\u00b7ne", "Kur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hinkte nun am Kr\u00fcckenstab;", "tokens": ["Er", "hink\u00b7te", "nun", "am", "Kr\u00fc\u00b7cken\u00b7stab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der F\u00fcrst ihm seinen Abschied gab,", "tokens": ["Der", "F\u00fcrst", "ihm", "sei\u00b7nen", "Ab\u00b7schied", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch in Gnaden nur.", "tokens": ["Je\u00b7doch", "in", "Gna\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das arme P\u00e4rchen wandert nun", "tokens": ["Das", "ar\u00b7me", "P\u00e4r\u00b7chen", "wan\u00b7dert", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Bettelfuhren fort.", "tokens": ["Auf", "Bet\u00b7tel\u00b7fuh\u00b7ren", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch endlich ging der lahme Held", "tokens": ["Doch", "end\u00b7lich", "ging", "der", "lah\u00b7me", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Strohsack aus der Welt", "tokens": ["Von", "ei\u00b7nem", "Stroh\u00b7sack", "aus", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kam an bessern Ort.", "tokens": ["Und", "kam", "an", "bes\u00b7sern", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da jammert nun das arme Weib", "tokens": ["Da", "jam\u00b7mert", "nun", "das", "ar\u00b7me", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Grab und spricht:", "tokens": ["Auf", "sei\u00b7nem", "Grab", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbihr M\u00e4dels, alles in der Welt,", "tokens": ["\u00bb", "ihr", "M\u00e4\u00b7dels", ",", "al\u00b7les", "in", "der", "Welt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nehmt M\u00e4nner, wie es euch gef\u00e4llt,", "tokens": ["Nehmt", "M\u00e4n\u00b7ner", ",", "wie", "es", "euch", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur einen Kriegsmann nicht!\u00ab", "tokens": ["Nur", "ei\u00b7nen", "Kriegs\u00b7mann", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}