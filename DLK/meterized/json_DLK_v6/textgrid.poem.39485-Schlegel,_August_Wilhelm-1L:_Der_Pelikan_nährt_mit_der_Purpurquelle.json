{"textgrid.poem.39485": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Pelikan n\u00e4hrt mit der Purpurquelle", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pelikan n\u00e4hrt mit der Purpurquelle", "tokens": ["Der", "Pe\u00b7li\u00b7kan", "n\u00e4hrt", "mit", "der", "Pur\u00b7pur\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aus seinem Busen die geliebten Jungen;", "tokens": ["Aus", "sei\u00b7nem", "Bu\u00b7sen", "die", "ge\u00b7lieb\u00b7ten", "Jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Adler lehrt, der Sonne zugeschwungen,", "tokens": ["Der", "Ad\u00b7ler", "lehrt", ",", "der", "Son\u00b7ne", "zu\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den jungen Adler tragen ihre Helle;", "tokens": ["Den", "jun\u00b7gen", "Ad\u00b7ler", "tra\u00b7gen", "ih\u00b7re", "Hel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der gro\u00dfe Leu, w\u00fcrdig der Oberstelle,", "tokens": ["Der", "gro\u00b7\u00dfe", "Leu", ",", "w\u00fcr\u00b7dig", "der", "O\u00b7bers\u00b7tel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Belebt das Junge, so, von ihm entsprungen,", "tokens": ["Be\u00b7lebt", "das", "Jun\u00b7ge", ",", "so", ",", "von", "ihm", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie todt erst daliegt, mit der Kraft der Lungen", "tokens": ["Wie", "todt", "erst", "da\u00b7liegt", ",", "mit", "der", "Kraft", "der", "Lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und haucht es an, da\u00df Lebensw\u00e4rm' es schwelle.", "tokens": ["Und", "haucht", "es", "an", ",", "da\u00df", "Le\u00b7bens\u00b7w\u00e4rm'", "es", "schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sind ein Vorbild edler Menschengaben:", "tokens": ["Sie", "sind", "ein", "Vor\u00b7bild", "ed\u00b7ler", "Men\u00b7schen\u00b7ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So opfert gern sich selbst wohlthuende Milde,", "tokens": ["So", "op\u00b7fert", "gern", "sich", "selbst", "wohlt\u00b7hu\u00b7en\u00b7de", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PRF", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Muth kann durch Thaten J\u00fcnger sich erziehen.", "tokens": ["Muth", "kann", "durch", "Tha\u00b7ten", "J\u00fcn\u00b7ger", "sich", "er\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein dem Genius nur ward es verliehen,", "tokens": ["Al\u00b7lein", "dem", "Ge\u00b7nius", "nur", "ward", "es", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df schon sein reiner Hauch beseelend bilde;", "tokens": ["Da\u00df", "schon", "sein", "rei\u00b7ner", "Hauch", "be\u00b7see\u00b7lend", "bil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum ist er zu dem Herrscherrang erhaben.", "tokens": ["Drum", "ist", "er", "zu", "dem", "Herr\u00b7scher\u00b7rang", "er\u00b7ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}