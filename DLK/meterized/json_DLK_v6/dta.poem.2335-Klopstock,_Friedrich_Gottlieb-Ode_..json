{"dta.poem.2335": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Ode .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1751", "urn": "urn:nbn:de:kobv:b4-200905198258", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welchen K\u00f6nig der Gott \u00fcber die K\u00f6nige", "tokens": ["Wel\u00b7chen", "K\u00f6\u00b7nig", "der", "Gott", "\u00fc\u00b7ber", "die", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Mit einweihendem Blick, als er geboren ward,", "tokens": ["Mit", "ein\u00b7wei\u00b7hen\u00b7dem", "Blick", ",", "als", "er", "ge\u00b7bo\u00b7ren", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Vom Olympus her sah, der wird ein Menschenfreund", "tokens": ["Vom", "O\u00b7lym\u00b7pus", "her", "sah", ",", "der", "wird", "ein", "Men\u00b7schen\u00b7freund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "APZR", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und des Vaterlands Vater seyn.", "tokens": ["Und", "des", "Va\u00b7ter\u00b7lands", "Va\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Jhm winkt schimmernder Ruhm, und die Unsterblichkeit,", "tokens": ["Jhm", "winkt", "schim\u00b7mern\u00b7der", "Ruhm", ",", "und", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Viel zu theuer durchs Blut bl\u00fchender J\u00fcnglinge,", "tokens": ["Viel", "zu", "theu\u00b7er", "durchs", "Blut", "bl\u00fc\u00b7hen\u00b7der", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und der Mutter und Braut n\u00e4chtliche Thr\u00e4n, erkauft,", "tokens": ["Und", "der", "Mut\u00b7ter", "und", "Braut", "n\u00e4cht\u00b7li\u00b7che", "Thr\u00e4n", ",", "er\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In das eiserne Feld umsonst.", "tokens": ["In", "das", "ei\u00b7ser\u00b7ne", "Feld", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Niemals weint er beym Bild eines Eroberers,", "tokens": ["Nie\u00b7mals", "weint", "er", "beym", "Bild", "ei\u00b7nes", "Er\u00b7o\u00b7be\u00b7rers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seines gleichen zu seyn! Schon, da sein menschlichs Herz", "tokens": ["Sei\u00b7nes", "glei\u00b7chen", "zu", "seyn", "!", "Schon", ",", "da", "sein", "menschlichs", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "PTKZU", "VAINF", "$.", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Kaum zu f\u00fchlen begann, war der Eroberer,", "tokens": ["Kaum", "zu", "f\u00fch\u00b7len", "be\u00b7gann", ",", "war", "der", "Er\u00b7o\u00b7be\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00fcr den G\u00f6ttlichen, viel zu klein!", "tokens": ["F\u00fcr", "den", "G\u00f6tt\u00b7li\u00b7chen", ",", "viel", "zu", "klein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Aber Thr\u00e4nen nach Ruhm, welcher erhabner ist,", "tokens": ["A\u00b7ber", "Thr\u00e4\u00b7nen", "nach", "Ruhm", ",", "wel\u00b7cher", "er\u00b7hab\u00b7ner", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Keines H\u00f6flings bedarf, Thr\u00e4nen, geliebt zu seyn", "tokens": ["Kei\u00b7nes", "H\u00f6f\u00b7lings", "be\u00b7darf", ",", "Thr\u00e4\u00b7nen", ",", "ge\u00b7liebt", "zu", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "NN", "$,", "VVPP", "PTKZU", "VAINF"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vom gl\u00fcckseligen Volk! weckten den J\u00fcngling oft", "tokens": ["Vom", "gl\u00fcck\u00b7se\u00b7li\u00b7gen", "Volk", "!", "weck\u00b7ten", "den", "J\u00fcng\u00b7ling", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In der Stunde der Mitternacht.", "tokens": ["In", "der", "Stun\u00b7de", "der", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Wenn der S\u00e4ugling im Arm hoffender M\u00fctter schlief,", "tokens": ["Wenn", "der", "S\u00e4ug\u00b7ling", "im", "Arm", "hof\u00b7fen\u00b7der", "M\u00fct\u00b7ter", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Einst ein gl\u00fccklicher Mann! Wenn sich des Greises Blick", "tokens": ["Einst", "ein", "gl\u00fcck\u00b7li\u00b7cher", "Mann", "!", "Wenn", "sich", "des", "Grei\u00b7ses", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sanft in Schlummer verlor, und itzt verj\u00fcnget ward,", "tokens": ["Sanft", "in", "Schlum\u00b7mer", "ver\u00b7lor", ",", "und", "itzt", "ver\u00b7j\u00fcn\u00b7get", "ward", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$,", "KON", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Noch den Vater des Volks zu sehn.", "tokens": ["Noch", "den", "Va\u00b7ter", "des", "Volks", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Lange sinnt er ihm nach, welch ein Gedank es ist:", "tokens": ["Lan\u00b7ge", "sinnt", "er", "ihm", "nach", ",", "welch", "ein", "Ge\u00b7dank", "es", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PWAT", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gott nachahmen, und selbst Sch\u00f6pfer des Gl\u00fcckes seyn", "tokens": ["Gott", "nac\u00b7hah\u00b7men", ",", "und", "selbst", "Sch\u00f6p\u00b7fer", "des", "Gl\u00fc\u00b7ckes", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "ADV", "NN", "ART", "NN", "VAINF"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Vieler Tausend! Er hat eilend die H\u00f6h erreicht,", "tokens": ["Vie\u00b7ler", "Tau\u00b7send", "!", "Er", "hat", "ei\u00b7lend", "die", "H\u00f6h", "er\u00b7reicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VAFIN", "VVPP", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und entschlie\u00dft sich, wie Gott zu seyn.", "tokens": ["Und", "ent\u00b7schlie\u00dft", "sich", ",", "wie", "Gott", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWAV", "NN", "PTKZU", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wie das ernste Gericht furchtbar die Wagschal nimmt,", "tokens": ["Wie", "das", "erns\u00b7te", "Ge\u00b7richt", "furcht\u00b7bar", "die", "Wag\u00b7schal", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und die K\u00f6nige w\u00e4gt, wenn sie gestorben sind;", "tokens": ["Und", "die", "K\u00f6\u00b7ni\u00b7ge", "w\u00e4gt", ",", "wenn", "sie", "ge\u00b7stor\u00b7ben", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Also w\u00e4gt er sich selbst jede der Thaten vor,", "tokens": ["Al\u00b7so", "w\u00e4gt", "er", "sich", "selbst", "je\u00b7de", "der", "Tha\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PIAT", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Die sein Leben bezeichnen soll!", "tokens": ["Die", "sein", "Le\u00b7ben", "be\u00b7zeich\u00b7nen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Ist ein Christ! Und belohnt redliche Thaten erst!", "tokens": ["Ist", "ein", "Christ", "!", "Und", "be\u00b7lohnt", "red\u00b7li\u00b7che", "Tha\u00b7ten", "erst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$.", "KON", "VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Alsdann schaut auch sein Blick l\u00e4chelnd auf die herab,", "tokens": ["Als\u00b7dann", "schaut", "auch", "sein", "Blick", "l\u00e4\u00b7chelnd", "auf", "die", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "APPR", "ART", "ADV", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Die der Muse sich weihn, welche das weiche Herz", "tokens": ["Die", "der", "Mu\u00b7se", "sich", "weihn", ",", "wel\u00b7che", "das", "wei\u00b7che", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PRF", "VVINF", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Tugendhafter und edler macht:", "tokens": ["Tu\u00b7gend\u00b7haf\u00b7ter", "und", "ed\u00b7ler", "macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Winkt dem stummen Verdienst, das in der Ferne steht!", "tokens": ["Winkt", "dem", "stum\u00b7men", "Ver\u00b7dienst", ",", "das", "in", "der", "Fer\u00b7ne", "steht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durch sein Muster gereizt lernt es Unsterblichkeit;", "tokens": ["Durch", "sein", "Mus\u00b7ter", "ge\u00b7reizt", "lernt", "es", "U\u00b7nsterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn er wandelt allein, ohne der Muse Lied,", "tokens": ["Denn", "er", "wan\u00b7delt", "al\u00b7lein", ",", "oh\u00b7ne", "der", "Mu\u00b7se", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sichern Wegs, zur Unsterblichkeit.", "tokens": ["Si\u00b7chern", "Wegs", ",", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Die du von dem Olymp Gott, den Me\u00dfias, sangst,", "tokens": ["Die", "du", "von", "dem", "O\u00b7lymp", "Gott", ",", "den", "Me\u00b7\u00dfias", ",", "sangst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Fromme S\u00e4ngerinn, und itzt zu den H\u00f6hen eilst,", "tokens": ["From\u00b7me", "S\u00e4n\u00b7ge\u00b7rinn", ",", "und", "itzt", "zu", "den", "H\u00f6\u00b7hen", "eilst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wo das heilige Lob jener Monarchen t\u00f6nt,", "tokens": ["Wo", "das", "hei\u00b7li\u00b7ge", "Lob", "je\u00b7ner", "Mon\u00b7ar\u00b7chen", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Nachahmer der Gottheit sind,", "tokens": ["Die", "Nac\u00b7hah\u00b7mer", "der", "Got\u00b7theit", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wag auch diesen Flug noch! Nenne den Namen selbst,", "tokens": ["Wag", "auch", "die\u00b7sen", "Flug", "noch", "!", "Nen\u00b7ne", "den", "Na\u00b7men", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PDAT", "NN", "ADV", "$.", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der in deinem Gesang k\u00fcnftig oft t\u00f6nen wird,", "tokens": ["Der", "in", "dei\u00b7nem", "Ge\u00b7sang", "k\u00fcnf\u00b7tig", "oft", "t\u00f6\u00b7nen", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADJD", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wenn du einst von dem Gl\u00fcck, das nur die Tugend lohnt,", "tokens": ["Wenn", "du", "einst", "von", "dem", "Gl\u00fcck", ",", "das", "nur", "die", "Tu\u00b7gend", "lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und von frommen Monarchen singst.", "tokens": ["Und", "von", "from\u00b7men", "Mon\u00b7ar\u00b7chen", "singst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "mete