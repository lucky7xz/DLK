{"dta.poem.6010": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stellte Numa dort, (de\u00df gro\u00df-und weiser Geist", "tokens": ["Es", "stell\u00b7te", "Nu\u00b7ma", "dort", ",", "(", "de\u00df", "gro\u00df\u00b7\u00b7und", "wei\u00b7ser", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ADV", "$,", "$(", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kunst zu herrschen lernt, in seiner Einsamkeit,", "tokens": ["Die", "Kunst", "zu", "herr\u00b7schen", "lernt", ",", "in", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der das Regiment von Rom, zur Sicherheit,", "tokens": ["Und", "der", "das", "Re\u00b7gi\u00b7ment", "von", "Rom", ",", "zur", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "APPR", "NE", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf Ordnungen, Gesetz und Regeln wusst\u2019 zu gr\u00fcnden,", "tokens": ["Auf", "Ord\u00b7nun\u00b7gen", ",", "Ge\u00b7setz", "und", "Re\u00b7geln", "wusst'", "zu", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und mit der Tapfferkeit die Weisheit zu verbinden:", "tokens": ["Und", "mit", "der", "Tapf\u00b7fer\u00b7keit", "die", "Weis\u00b7heit", "zu", "ver\u00b7bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Weltweis\u2019 und zugleich auch Geistlich war;)", "tokens": ["Der", "Welt\u00b7weis'", "und", "zu\u00b7gleich", "auch", "Geist\u00b7lich", "war", ";)"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["ART", "NN", "KON", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Welt Figur in einem Tempel dar:", "tokens": ["Der", "Welt", "Fi\u00b7gur", "in", "ei\u00b7nem", "Tem\u00b7pel", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In deren Mitte brannt Geheimni\u00dfvoll und rein,", "tokens": ["In", "de\u00b7ren", "Mit\u00b7te", "brannt", "Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von einem ew\u2019gen Feur, ein lichter Schein:", "tokens": ["Von", "ei\u00b7nem", "ew'\u00b7gen", "Feur", ",", "ein", "lich\u00b7ter", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dadurch er die Figur und Ordnungen der Welt,", "tokens": ["Da\u00b7durch", "er", "die", "Fi\u00b7gur", "und", "Ord\u00b7nun\u00b7gen", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Wie sie ", "tokens": ["Wie", "sie"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Woselbst die Sonn\u2019 im Mittel-Punct, den Schein,", "tokens": ["Wo\u00b7selbst", "die", "Sonn'", "im", "Mit\u00b7tel\u00b7Punct", ",", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dadurch die Ding\u2019 erw\u00e4rmt, belebt, gef\u00e4rbet seyn,", "tokens": ["Da\u00b7durch", "die", "Ding'", "er\u00b7w\u00e4rmt", ",", "be\u00b7lebt", ",", "ge\u00b7f\u00e4r\u00b7bet", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$,", "VVPP", "$,", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ins Reich der L\u00fcffte schickt. Es war die Erde nicht", "tokens": ["Ins", "Reich", "der", "L\u00fcff\u00b7te", "schickt", ".", "Es", "war", "die", "Er\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Mittel-Punct am Himmlischen Saphir,", "tokens": ["Der", "Mit\u00b7tel\u00b7Punct", "am", "Himm\u00b7li\u00b7schen", "Sa\u00b7phir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Er konnte sie nicht unbeweglich sch\u00e4tzen.", "tokens": ["Er", "konn\u00b7te", "sie", "nicht", "un\u00b7be\u00b7weg\u00b7lich", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Es kam hingegen ihm viel leicht und besser f\u00fcr,", "tokens": ["Es", "kam", "hin\u00b7ge\u00b7gen", "ihm", "viel", "leicht", "und", "bes\u00b7ser", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "KON", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den Strahlen reichen Thron der Sonnen fest zu setzen.", "tokens": ["Den", "Strah\u00b7len", "rei\u00b7chen", "Thron", "der", "Son\u00b7nen", "fest", "zu", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}