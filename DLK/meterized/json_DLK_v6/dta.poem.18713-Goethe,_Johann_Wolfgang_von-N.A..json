{"dta.poem.18713": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Medschnun heisst \u2014 ich will nicht sagen", "tokens": ["Medsc\u00b7hnun", "heisst", "ich", "will", "nicht", "sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dass es grad\u2019 ein Toller heisse;", "tokens": ["Dass", "es", "grad'", "ein", "Tol\u00b7ler", "heis\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr m\u00fcsst mich nicht verklagen", "tokens": ["Doch", "ihr", "m\u00fcsst", "mich", "nicht", "ver\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass ich mich als Medschnun preise.", "tokens": ["Dass", "ich", "mich", "als", "Medsc\u00b7hnun", "prei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Brust, die redlich volle,", "tokens": ["Wenn", "die", "Brust", ",", "die", "red\u00b7lich", "vol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich entladet euch zu retten,", "tokens": ["Sich", "ent\u00b7la\u00b7det", "euch", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft ihr nicht: das ist der Tolle!", "tokens": ["Ruft", "ihr", "nicht", ":", "das", "ist", "der", "Tol\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holet Stricke, schaffet Ketten!", "tokens": ["Ho\u00b7let", "Stri\u00b7cke", ",", "schaf\u00b7fet", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn ihr zuletzt in Fesseln", "tokens": ["Und", "wenn", "ihr", "zu\u00b7letzt", "in", "Fes\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht die Kl\u00fcgeren verschmachten,", "tokens": ["Seht", "die", "Kl\u00fc\u00b7ge\u00b7ren", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sengt es euch wie Feuernesseln", "tokens": ["Sengt", "es", "euch", "wie", "Feu\u00b7er\u00b7nes\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das vergebens zu betrachten.", "tokens": ["Das", "ver\u00b7ge\u00b7bens", "zu", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}