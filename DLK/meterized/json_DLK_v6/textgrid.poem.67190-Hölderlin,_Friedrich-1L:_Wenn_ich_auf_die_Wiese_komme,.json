{"textgrid.poem.67190": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich auf die Wiese komme,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich auf die Wiese komme,", "tokens": ["Wenn", "ich", "auf", "die", "Wie\u00b7se", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich auf dem Felde jetzt,", "tokens": ["Wenn", "ich", "auf", "dem", "Fel\u00b7de", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Bin ich noch der Zahme, Fromme,", "tokens": ["Bin", "ich", "noch", "der", "Zah\u00b7me", ",", "From\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie von Dornen unverletzt.", "tokens": ["Wie", "von", "Dor\u00b7nen", "un\u00b7ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Gewand in Winden wehet,", "tokens": ["Mein", "Ge\u00b7wand", "in", "Win\u00b7den", "we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Geist mir lustig fragt,", "tokens": ["Wie", "der", "Geist", "mir", "lus\u00b7tig", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Worin Inneres bestehet,", "tokens": ["Wo\u00b7rin", "In\u00b7ne\u00b7res", "be\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis Aufl\u00f6sung diesem tagt.", "tokens": ["Bis", "Auf\u00b7l\u00f6\u00b7sung", "die\u00b7sem", "tagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O vor diesem sanften Bilde,", "tokens": ["O", "vor", "die\u00b7sem", "sanf\u00b7ten", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die gr\u00fcnen B\u00e4ume stehn,", "tokens": ["Wo", "die", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie vor einer Schenke Schilde", "tokens": ["Wie", "vor", "ei\u00b7ner", "Schen\u00b7ke", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ich kaum vor\u00fcbergehn.", "tokens": ["Kann", "ich", "kaum", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Ruh an stillen Tagen", "tokens": ["Denn", "die", "Ruh", "an", "stil\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00fcnkt entschieden trefflich mir,", "tokens": ["D\u00fcnkt", "ent\u00b7schie\u00b7den", "treff\u00b7lich", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses mu\u00dft du gar nicht fragen,", "tokens": ["Die\u00b7ses", "mu\u00dft", "du", "gar", "nicht", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn ich soll antworten dir.", "tokens": ["Wenn", "ich", "soll", "ant\u00b7wor\u00b7ten", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber zu dem sch\u00f6nen Bache", "tokens": ["A\u00b7ber", "zu", "dem", "sch\u00f6\u00b7nen", "Ba\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Such ich einen Lustweg wohl,", "tokens": ["Such", "ich", "ei\u00b7nen", "Lust\u00b7weg", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, als wie in dem Gemache,", "tokens": ["Der", ",", "als", "wie", "in", "dem", "Ge\u00b7ma\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleicht durchs Ufer wild und hohl,", "tokens": ["Schleicht", "durchs", "U\u00b7fer", "wild", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Steg dar\u00fcber gehet,", "tokens": ["Wo", "der", "Steg", "da\u00b7r\u00fc\u00b7ber", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gehts den sch\u00f6nen Wald hinauf,", "tokens": ["Gehts", "den", "sch\u00f6\u00b7nen", "Wald", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo der Wind den Steg umwehet,", "tokens": ["Wo", "der", "Wind", "den", "Steg", "um\u00b7we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht das Auge fr\u00f6hlich auf.", "tokens": ["Sieht", "das", "Au\u00b7ge", "fr\u00f6h\u00b7lich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Droben auf des H\u00fcgels Gipfel", "tokens": ["Dro\u00b7ben", "auf", "des", "H\u00fc\u00b7gels", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitz ich manchen Nachmittag,", "tokens": ["Sitz", "ich", "man\u00b7chen", "Nach\u00b7mit\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Wind umsaust die Wipfel,", "tokens": ["Wenn", "der", "Wind", "um\u00b7saust", "die", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei des Turmes Glockenschlag,", "tokens": ["Bei", "des", "Tur\u00b7mes", "Glo\u00b7cken\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Betrachtung gibt dem Herzen", "tokens": ["Und", "Be\u00b7trach\u00b7tung", "gibt", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frieden, wie das Bild auch ist,", "tokens": ["Frie\u00b7den", ",", "wie", "das", "Bild", "auch", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Beruhigung den Schmerzen,", "tokens": ["Und", "Be\u00b7ru\u00b7hi\u00b7gung", "den", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Welche reimt Verstand und List.", "tokens": ["Wel\u00b7che", "reimt", "Ver\u00b7stand", "und", "List", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Holde Landschaft! wo die Stra\u00dfe", "tokens": ["Hol\u00b7de", "Land\u00b7schaft", "!", "wo", "die", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten durch sehr eben geht,", "tokens": ["Mit\u00b7ten", "durch", "sehr", "e\u00b7ben", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Mond aufsteigt, der blasse,", "tokens": ["Wo", "der", "Mond", "auf\u00b7steigt", ",", "der", "blas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Abendwind entsteht,", "tokens": ["Wenn", "der", "Ab\u00b7end\u00b7wind", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Natur sehr einf\u00e4ltig,", "tokens": ["Wo", "die", "Na\u00b7tur", "sehr", "ein\u00b7f\u00e4l\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Wo die Berg erhaben stehn,", "tokens": ["Wo", "die", "Berg", "er\u00b7ha\u00b7ben", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Geh ich heim zuletzt, haush\u00e4ltig,", "tokens": ["Geh", "ich", "heim", "zu\u00b7letzt", ",", "haus\u00b7h\u00e4l\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "ADV", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dort nach goldnem Wein zu sehn.", "tokens": ["Dort", "nach", "gold\u00b7nem", "Wein", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}