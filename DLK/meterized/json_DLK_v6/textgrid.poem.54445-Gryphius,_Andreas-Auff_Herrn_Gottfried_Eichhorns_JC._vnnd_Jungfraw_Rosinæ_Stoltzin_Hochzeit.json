{"textgrid.poem.54445": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Auff Herrn Gottfried Eichhorns JC. vnnd Jungfraw Rosin\u00e6 Stoltzin Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob gleich der weisse Schnee jtzt Berg vnd Thal bedecket/", "tokens": ["Ob", "gleich", "der", "weis\u00b7se", "Schnee", "jtzt", "Berg", "vnd", "Thal", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "V\u00f1 manch geschwinder Flu\u00df zeucht einen Harnisch an/", "tokens": ["V\u00f1", "manch", "ge\u00b7schwin\u00b7der", "Flu\u00df", "zeucht", "ei\u00b7nen", "Har\u00b7nisch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dem Er sich des Zorns der K\u00e4lte wehren kan/", "tokens": ["In", "dem", "Er", "sich", "des", "Zorns", "der", "K\u00e4l\u00b7te", "weh\u00b7ren", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor welcher jeder Baum steht gantz vnd gar erschrecket.", "tokens": ["Vor", "wel\u00b7cher", "je\u00b7der", "Baum", "steht", "gantz", "vnd", "gar", "er\u00b7schre\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "VVFIN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob gleich der bleiche Frost die scharffe Seen\u00df au\u00dfrecket/", "tokens": ["Ob", "gleich", "der", "blei\u00b7che", "Frost", "die", "scharf\u00b7fe", "Seen\u00df", "au\u00df\u00b7re\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "V\u00f1 alle Bl\u00fcmlin f\u00e4lt/ so ", "tokens": ["V\u00f1", "al\u00b7le", "Bl\u00fcm\u00b7lin", "f\u00e4lt", "/", "so"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PIAT", "NN", "VVFIN", "$(", "ADV"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Der Erden vmb vnnd vmb zuvor hat sehen lan/", "tokens": ["Der", "Er\u00b7den", "vmb", "vnnd", "vmb", "zu\u00b7vor", "hat", "se\u00b7hen", "lan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "APPR", "ADV", "VAFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat doch die Lieb in Euch ein grosses Fewr erwecket/", "tokens": ["Hat", "doch", "die", "Lieb", "in", "Euch", "ein", "gros\u00b7ses", "Fewr", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Herr Gottfrid/ vnnd darzu noch eine Rose bracht/", "tokens": ["Herr", "Gott\u00b7frid", "/", "vnnd", "dar\u00b7zu", "noch", "ei\u00b7ne", "Ro\u00b7se", "bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "KON", "PAV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df Ihr des Winters sch\u00e4rff vnd rasen sicher lacht!", "tokens": ["Da\u00df", "Ihr", "des", "Win\u00b7ters", "sch\u00e4rff", "vnd", "ra\u00b7sen", "si\u00b7cher", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Woll Euch/ vnd aber wol! ist was das Ihr begehren", "tokens": ["Woll", "Euch", "/", "vnd", "a\u00b7ber", "wol", "!", "ist", "was", "das", "Ihr", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$(", "KON", "ADV", "ADV", "$.", "VAFIN", "PWS", "ART", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch mehr vom Himmel m\u00f6gt/ wenn Euch zu dieser Zeit", "tokens": ["Noch", "mehr", "vom", "Him\u00b7mel", "m\u00f6gt", "/", "wenn", "Euch", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "VMFIN", "$(", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So sch\u00f6ne Rosen sind mit solcher w\u00e4rm bereit/", "tokens": ["So", "sch\u00f6\u00b7ne", "Ro\u00b7sen", "sind", "mit", "sol\u00b7cher", "w\u00e4rm", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird Euch der Herbst gewi\u00df viel sch\u00f6ner Fr\u00fcchte geben.", "tokens": ["Wird", "Euch", "der", "Herbst", "ge\u00b7wi\u00df", "viel", "sch\u00f6\u00b7ner", "Fr\u00fcch\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}