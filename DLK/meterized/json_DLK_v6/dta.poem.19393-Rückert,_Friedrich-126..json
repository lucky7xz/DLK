{"dta.poem.19393": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "126.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dis Wort hat der Profet gesagt den Muselmanen;", "tokens": ["Dis", "Wort", "hat", "der", "Pro\u00b7fet", "ge\u00b7sagt", "den", "Mu\u00b7sel\u00b7ma\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "NN", "VVPP", "ART", "NN", "$."], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "La\u00df dir gesagt es seyn, o Sch\u00fcler des Brahmanen:", "tokens": ["La\u00df", "dir", "ge\u00b7sagt", "es", "seyn", ",", "o", "Sch\u00fc\u00b7ler", "des", "Brah\u00b7ma\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "PPER", "VAINF", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht jeder log, wer dir die Wahrheit vorenthielt;", "tokens": ["Nicht", "je\u00b7der", "log", ",", "wer", "dir", "die", "Wahr\u00b7heit", "vor\u00b7ent\u00b7hielt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "$,", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat villeicht dadurch dein wahres Wohl bezielt.", "tokens": ["Er", "hat", "vil\u00b7leicht", "da\u00b7durch", "dein", "wah\u00b7res", "Wohl", "be\u00b7zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entzweiter Freunde Zwist hat er dadurch geschlichtet,", "tokens": ["Ent\u00b7zwei\u00b7ter", "Freun\u00b7de", "Zwist", "hat", "er", "da\u00b7durch", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er nicht jedem gleich, was jeder sprach, berichtet;", "tokens": ["Da\u00df", "er", "nicht", "je\u00b7dem", "gleich", ",", "was", "je\u00b7der", "sprach", ",", "be\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PIS", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn er, einander sie zu machen wohlgeneigt,", "tokens": ["Wenn", "er", ",", "ein\u00b7an\u00b7der", "sie", "zu", "ma\u00b7chen", "wohl\u00b7ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRF", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gute Rede sagt, das b\u00f6se Wort verschweigt.", "tokens": ["Die", "gu\u00b7te", "Re\u00b7de", "sagt", ",", "das", "b\u00f6\u00b7se", "Wort", "ver\u00b7schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er will das Herz mit Flei\u00df dir machen k\u00fchl, nicht hei\u00df;", "tokens": ["Er", "will", "das", "Herz", "mit", "Flei\u00df", "dir", "ma\u00b7chen", "k\u00fchl", ",", "nicht", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "PPER", "VVINF", "ADJD", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott sagt dir auch zum Gl\u00fcck nicht alles, was er wei\u00df.", "tokens": ["Gott", "sagt", "dir", "auch", "zum", "Gl\u00fcck", "nicht", "al\u00b7les", ",", "was", "er", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}