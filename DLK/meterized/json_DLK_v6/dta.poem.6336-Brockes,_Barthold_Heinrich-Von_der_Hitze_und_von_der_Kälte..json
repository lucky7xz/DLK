{"dta.poem.6336": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denn alles, welches sich in einem zarten Dufft", "tokens": ["Denn", "al\u00b7les", ",", "wel\u00b7ches", "sich", "in", "ei\u00b7nem", "zar\u00b7ten", "Dufft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Freyheit setzt, und sich vom C\u00f6rper trennt,", "tokens": ["In", "Frey\u00b7heit", "setzt", ",", "und", "sich", "vom", "C\u00f6r\u00b7per", "trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da es dem Triebe folgt vom ersten Element,", "tokens": ["Da", "es", "dem", "Trie\u00b7be", "folgt", "vom", "ers\u00b7ten", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nimmt die Bewegung und den Schein", "tokens": ["Nimmt", "die", "Be\u00b7we\u00b7gung", "und", "den", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Vom Feur allein.", "tokens": ["Vom", "Feur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}