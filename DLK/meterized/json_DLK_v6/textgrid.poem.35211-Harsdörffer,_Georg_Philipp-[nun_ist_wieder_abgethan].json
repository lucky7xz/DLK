{"textgrid.poem.35211": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[nun ist wieder abgethan]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist wieder abgethan", "tokens": ["Nun", "ist", "wie\u00b7der", "ab\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd dem leichten Wind vertrauet/", "tokens": ["Vnd", "dem", "leich\u00b7ten", "Wind", "ver\u00b7trau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich bracher Sch\u00e4fersmann", "tokens": ["Was", "ich", "bra\u00b7cher", "Sch\u00e4\u00b7fers\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Einsamkeit erbauet.", "tokens": ["In", "der", "Ein\u00b7sam\u00b7keit", "er\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leichtlich hat der Wind vernichtet/", "tokens": ["Leicht\u00b7lich", "hat", "der", "Wind", "ver\u00b7nich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was mit leichter M\u00fch erdichtet.", "tokens": ["Was", "mit", "leich\u00b7ter", "M\u00fch", "er\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}