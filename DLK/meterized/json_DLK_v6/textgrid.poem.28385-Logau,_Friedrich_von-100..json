{"textgrid.poem.28385": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was n\u00fctzlich offters ist, ist allemahl nicht ehrlich;", "tokens": ["Was", "n\u00fctz\u00b7lich", "off\u00b7ters", "ist", ",", "ist", "al\u00b7le\u00b7mahl", "nicht", "ehr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "VAFIN", "$,", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was b\u00e4urisch etwa n\u00fctzt, n\u00fctzt allemal nicht herrlich.", "tokens": ["Was", "b\u00e4u\u00b7risch", "et\u00b7wa", "n\u00fctzt", ",", "n\u00fctzt", "al\u00b7le\u00b7mal", "nicht", "herr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was zeihen sich dann die, die einem F\u00fcrsten rathen,", "tokens": ["Was", "zei\u00b7hen", "sich", "dann", "die", ",", "die", "ei\u00b7nem", "F\u00fcrs\u00b7ten", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu richten so den Nutz, wie kaum die Bauren thaten?", "tokens": ["Zu", "rich\u00b7ten", "so", "den", "Nutz", ",", "wie", "kaum", "die", "Bau\u00b7ren", "tha\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Nutz bekummen sie; der F\u00fcrst bek\u00fcmmt zu nemen,", "tokens": ["Den", "Nutz", "be\u00b7kum\u00b7men", "sie", ";", "der", "F\u00fcrst", "be\u00b7k\u00fcmmt", "zu", "ne\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil wenig Ehre bleibt, gemeiniglich das sch\u00e4men.", "tokens": ["Weil", "we\u00b7nig", "Eh\u00b7re", "bleibt", ",", "ge\u00b7mei\u00b7nig\u00b7lich", "das", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,", "ADV", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}