{"dta.poem.19031": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "I.   Uber den Heyland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fcrchte mich deswegen doch kein Haar:", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "des\u00b7we\u00b7gen", "doch", "kein", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Glaub ist Sieg, mein Zweck ist ", "tokens": ["Mein", "Glaub", "ist", "Sieg", ",", "mein", "Zweck", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Alles! meine gantze Welt!", "tokens": ["Mein", "Al\u00b7les", "!", "mei\u00b7ne", "gant\u00b7ze", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Freund! der ewig Farbe h\u00e4lt,", "tokens": ["Mein", "Freund", "!", "der", "e\u00b7wig", "Far\u00b7be", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein wei\u00df- und rother Br\u00e4utigam!", "tokens": ["Mein", "wei\u00df", "und", "ro\u00b7ther", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein immerw\u00e4hrend Oster-Lamm!", "tokens": ["Mein", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "Os\u00b7ter\u00b7Lamm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Leit-Stern! meine Liebe! meine Zier!", "tokens": ["Mein", "Leit\u00b7S\u00b7tern", "!", "mei\u00b7ne", "Lie\u00b7be", "!", "mei\u00b7ne", "Zier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sey ewiglich mein Steinritz, mein Panier!", "tokens": ["Sey", "e\u00b7wig\u00b7lich", "mein", "Stein\u00b7ritz", ",", "mein", "Pa\u00b7nier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hast du mich in der Zeit gewollt,", "tokens": ["Hast", "du", "mich", "in", "der", "Zeit", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die R\u00e4der schnell von dannen rollt?", "tokens": ["Die", "R\u00e4\u00b7der", "schnell", "von", "dan\u00b7nen", "rollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So mi\u00df mir selbst die Stunden ab!", "tokens": ["So", "mi\u00df", "mir", "selbst", "die", "Stun\u00b7den", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sey meiner Reise Wander-Stab!", "tokens": ["Sey", "mei\u00b7ner", "Rei\u00b7se", "Wan\u00b7der\u00b7Stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sey meines Thuns sein Sch\u00f6pffer! f\u00fchre mich", "tokens": ["Sey", "mei\u00b7nes", "Thuns", "sein", "Sch\u00f6pf\u00b7fer", "!", "f\u00fch\u00b7re", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In allem dir zu wandeln w\u00fcrdiglich!", "tokens": ["In", "al\u00b7lem", "dir", "zu", "wan\u00b7deln", "w\u00fcr\u00b7dig\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "PTKZU", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Soll ich viel Jahr im Karren fort;", "tokens": ["Soll", "ich", "viel", "Jahr", "im", "Kar\u00b7ren", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So zeige mir den Ruhe-Port,", "tokens": ["So", "zei\u00b7ge", "mir", "den", "Ru\u00b7he\u00b7Port", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Von ferne zeige mir die Stadt,", "tokens": ["Von", "fer\u00b7ne", "zei\u00b7ge", "mir", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die deine Hand bereitet hat,", "tokens": ["Die", "dei\u00b7ne", "Hand", "be\u00b7rei\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das g\u00fcldne Seraphinen Liebes-Licht:", "tokens": ["Das", "g\u00fcld\u00b7ne", "Se\u00b7ra\u00b7phi\u00b7nen", "Lie\u00b7bes\u00b7Licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So schrecket mich die lange Reise nicht.", "tokens": ["So", "schre\u00b7cket", "mich", "die", "lan\u00b7ge", "Rei\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und wenn ich meiner Br\u00fcder Zahl", "tokens": ["Und", "wenn", "ich", "mei\u00b7ner", "Br\u00fc\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nach deiner holden Gnaden-Wahl", "tokens": ["Nach", "dei\u00b7ner", "hol\u00b7den", "Gna\u00b7den\u00b7Wahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "An meinem Theile auch erf\u00fcllt;", "tokens": ["An", "mei\u00b7nem", "Thei\u00b7le", "auch", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wenns endlich auch Belohnens gilt:", "tokens": ["Wenns", "end\u00b7lich", "auch", "Be\u00b7loh\u00b7nens", "gilt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So weist du, da\u00df mein Lohn, mein Licht und Ruh", "tokens": ["So", "weist", "du", ",", "da\u00df", "mein", "Lohn", ",", "mein", "Licht", "und", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Nur du alleine werden sollst, Nur Du.", "tokens": ["Nur", "du", "al\u00b7lei\u00b7ne", "wer\u00b7den", "sollst", ",", "Nur", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAINF", "VMFIN", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}