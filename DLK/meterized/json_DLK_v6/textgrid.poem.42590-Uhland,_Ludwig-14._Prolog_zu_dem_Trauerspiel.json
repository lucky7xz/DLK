{"textgrid.poem.42590": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "14. Prolog zu dem Trauerspiel", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ernstes Spiel wird euch vor\u00fcbergehn,", "tokens": ["Ein", "erns\u00b7tes", "Spiel", "wird", "euch", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Vorhang hebt sich \u00fcber einer Welt,", "tokens": ["Der", "Vor\u00b7hang", "hebt", "sich", "\u00fc\u00b7ber", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die l\u00e4ngst hinab ist in der Zeiten Strom,", "tokens": ["Die", "l\u00e4ngst", "hin\u00b7ab", "ist", "in", "der", "Zei\u00b7ten", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und K\u00e4mpfe, l\u00e4ngst schon ausgek\u00e4mpfte, werden", "tokens": ["Und", "K\u00e4mp\u00b7fe", ",", "l\u00e4ngst", "schon", "aus\u00b7ge\u00b7k\u00e4mpf\u00b7te", ",", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "ADV", "ADV", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor euern Augen st\u00fcrmisch sich erneun.", "tokens": ["Vor", "eu\u00b7ern", "Au\u00b7gen", "st\u00fcr\u00b7misch", "sich", "er\u00b7neun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zween M\u00e4nner, edel, bieder, fromm und k\u00fchn,", "tokens": ["Zween", "M\u00e4n\u00b7ner", ",", "e\u00b7del", ",", "bie\u00b7der", ",", "fromm", "und", "k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zween Freunde, treu und fest bis in den Tod,", "tokens": ["Zween", "Freun\u00b7de", ",", "treu", "und", "fest", "bis", "in", "den", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "KON", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Preiswerte Namen deutscher Heldenzeit,", "tokens": ["Preis\u00b7wer\u00b7te", "Na\u00b7men", "deut\u00b7scher", "Hel\u00b7den\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ihr werdet sehn, wie sie, ge\u00e4chtet, irren", "tokens": ["Ihr", "wer\u00b7det", "sehn", ",", "wie", "sie", ",", "ge\u00e4ch\u00b7tet", ",", "ir\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "PPER", "$,", "VVPP", "$,", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und, in Verzweiflung fechtend, untergehn.", "tokens": ["Und", ",", "in", "Ver\u00b7zwei\u00b7flung", "fech\u00b7tend", ",", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist der Fluch des ungl\u00fccksel'gen Landes,", "tokens": ["Das", "ist", "der", "Fluch", "des", "un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Freiheit und Gesetz darniederliegt,", "tokens": ["Wo", "Frei\u00b7heit", "und", "Ge\u00b7setz", "dar\u00b7nie\u00b7der\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich die Besten und die Edelsten", "tokens": ["Da\u00df", "sich", "die", "Bes\u00b7ten", "und", "die", "E\u00b7dels\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verzehren m\u00fcssen in fruchtlosem Harm,", "tokens": ["Ver\u00b7zeh\u00b7ren", "m\u00fcs\u00b7sen", "in", "frucht\u00b7lo\u00b7sem", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Da\u00df, die f\u00fcrs Vaterland am reinsten gl\u00fchn,", "tokens": ["Da\u00df", ",", "die", "f\u00fcrs", "Va\u00b7ter\u00b7land", "am", "reins\u00b7ten", "gl\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "APPRART", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gebrandmarkt werden als des Lands Verr\u00e4ter", "tokens": ["Ge\u00b7brand\u00b7markt", "wer\u00b7den", "als", "des", "Lands", "Ver\u00b7r\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und, die noch j\u00fcngst des Landes Retter hie\u00dfen,", "tokens": ["Und", ",", "die", "noch", "j\u00fcngst", "des", "Lan\u00b7des", "Ret\u00b7ter", "hie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich fl\u00fcchten m\u00fcssen an des Fremden Herd.", "tokens": ["Sich", "fl\u00fcch\u00b7ten", "m\u00fcs\u00b7sen", "an", "des", "Frem\u00b7den", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und w\u00e4hrend so die beste Kraft verdirbt,", "tokens": ["Und", "w\u00e4h\u00b7rend", "so", "die", "bes\u00b7te", "Kraft", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Erbl\u00fchen, wuchernd in der H\u00f6lle Segen,", "tokens": ["Er\u00b7bl\u00fc\u00b7hen", ",", "wu\u00b7chernd", "in", "der", "H\u00f6l\u00b7le", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Gewalttat, Hochmut, Feigheit, Schergendienst.", "tokens": ["Ge\u00b7walt\u00b7tat", ",", "Hoch\u00b7mut", ",", "Feig\u00b7heit", ",", "Scher\u00b7gen\u00b7dienst", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie anders, wenn aus sturmbewegter Zeit", "tokens": ["Wie", "an\u00b7ders", ",", "wenn", "aus", "sturm\u00b7be\u00b7weg\u00b7ter", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gesetz und Ordnung, Freiheit sich und Recht", "tokens": ["Ge\u00b7setz", "und", "Ord\u00b7nung", ",", "Frei\u00b7heit", "sich", "und", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "PRF", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Emporgerungen und sich festgepflanzt!", "tokens": ["Em\u00b7por\u00b7ge\u00b7run\u00b7gen", "und", "sich", "fest\u00b7ge\u00b7pflanzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da dr\u00e4ngen die, so grollend ferne standen,", "tokens": ["Da", "dr\u00e4n\u00b7gen", "die", ",", "so", "grol\u00b7lend", "fer\u00b7ne", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$,", "ADV", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich fr\u00f6hlich wieder in der B\u00fcrger Reihn,", "tokens": ["Sich", "fr\u00f6h\u00b7lich", "wie\u00b7der", "in", "der", "B\u00fcr\u00b7ger", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da wirket jeder Geist und jede Hand", "tokens": ["Da", "wir\u00b7ket", "je\u00b7der", "Geist", "und", "je\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Belebend, f\u00f6rdernd f\u00fcr des Ganzen Wohl,", "tokens": ["Be\u00b7le\u00b7bend", ",", "f\u00f6r\u00b7dernd", "f\u00fcr", "des", "Gan\u00b7zen", "Wohl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da gl\u00e4nzt der Thron, da lebt die Stadt, da gr\u00fcnt", "tokens": ["Da", "gl\u00e4nzt", "der", "Thron", ",", "da", "lebt", "die", "Stadt", ",", "da", "gr\u00fcnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Feld, da blicken M\u00e4nner frei und stolz;", "tokens": ["Das", "Feld", ",", "da", "bli\u00b7cken", "M\u00e4n\u00b7ner", "frei", "und", "stolz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADJA", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Des F\u00fcrsten und des Volkes Rechte sind", "tokens": ["Des", "F\u00fcrs\u00b7ten", "und", "des", "Vol\u00b7kes", "Rech\u00b7te", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verwoben, wie sich Ulm und Reb umschlingen,", "tokens": ["Ver\u00b7wo\u00b7ben", ",", "wie", "sich", "Ulm", "und", "Reb", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PRF", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und f\u00fcr des Heiligtums Verteidigung", "tokens": ["Und", "f\u00fcr", "des", "Hei\u00b7lig\u00b7tums", "Ver\u00b7tei\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Steht jeder freudig ein mit Gut und Blut.", "tokens": ["Steht", "je\u00b7der", "freu\u00b7dig", "ein", "mit", "Gut", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Man rettet gern aus tr\u00fcber Gegenwart", "tokens": ["Man", "ret\u00b7tet", "gern", "aus", "tr\u00fc\u00b7ber", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich in das heitere Gebiet der Kunst,", "tokens": ["Sich", "in", "das", "hei\u00b7te\u00b7re", "Ge\u00b7biet", "der", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcr die Kr\u00e4nkungen der Wirklichkeit", "tokens": ["Und", "f\u00fcr", "die", "Kr\u00e4n\u00b7kun\u00b7gen", "der", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sucht man sich Heilung in des Dichters Tr\u00e4umen.", "tokens": ["Sucht", "man", "sich", "Hei\u00b7lung", "in", "des", "Dich\u00b7ters", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Doch heute \u2013 wen vielleicht der B\u00fchne Spiel", "tokens": ["Doch", "heu\u00b7te", "\u2013", "wen", "viel\u00b7leicht", "der", "B\u00fch\u00b7ne", "Spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PWS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verwundet, der gedenke, sich zum Troste,", "tokens": ["Ver\u00b7wun\u00b7det", ",", "der", "ge\u00b7den\u00b7ke", ",", "sich", "zum", "Tros\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "VVFIN", "$,", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Welch Fest wir wahr und wirklich heut begehn!", "tokens": ["Welch", "Fest", "wir", "wahr", "und", "wirk\u00b7lich", "heut", "be\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "KON", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da mag er sehn, f\u00fcr was die M\u00e4nner sterben.", "tokens": ["Da", "mag", "er", "sehn", ",", "f\u00fcr", "was", "die", "M\u00e4n\u00b7ner", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch steigen G\u00f6tter auf die Erde nieder,", "tokens": ["Noch", "stei\u00b7gen", "G\u00f6t\u00b7ter", "auf", "die", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch treten die Gedanken, die der Mensch", "tokens": ["Noch", "tre\u00b7ten", "die", "Ge\u00b7dan\u00b7ken", ",", "die", "der", "Mensch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die h\u00f6chsten achtet, in das Leben ein.", "tokens": ["Die", "h\u00f6chs\u00b7ten", "ach\u00b7tet", ",", "in", "das", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja! mitten in der wildverworrnen Zeit", "tokens": ["Ja", "!", "mit\u00b7ten", "in", "der", "wild\u00b7ver\u00b7worr\u00b7nen", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ersteht ein F\u00fcrst, vom eignen Geist bewegt,", "tokens": ["Er\u00b7steht", "ein", "F\u00fcrst", ",", "vom", "eig\u00b7nen", "Geist", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und reicht hoc