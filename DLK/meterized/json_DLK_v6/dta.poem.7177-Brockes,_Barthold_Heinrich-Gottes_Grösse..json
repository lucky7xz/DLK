{"dta.poem.7177": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, was gr\u00f6ssers noch zu weisen,", "tokens": ["Ja", ",", "was", "gr\u00f6s\u00b7sers", "noch", "zu", "wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ist doch ein Creis so klein,", "tokens": ["Wie", "ist", "doch", "ein", "Creis", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bey den Millionen Creisen,", "tokens": ["Bey", "den", "Mil\u00b7lion\u00b7en", "Crei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die an solcher R\u00fcnde seyn,", "tokens": ["Die", "an", "sol\u00b7cher", "R\u00fcn\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was gleicht solcher Kugel Dichte,", "tokens": ["Was", "gleicht", "sol\u00b7cher", "Ku\u00b7gel", "Dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Fl\u00e4che, Festigkeit, Gewichte!", "tokens": ["Fl\u00e4\u00b7che", ",", "Fes\u00b7tig\u00b7keit", ",", "Ge\u00b7wich\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun denckt an den Brunn des Lichts;", "tokens": ["Nun", "denckt", "an", "den", "Brunn", "des", "Lichts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gegen Den dies alles nichts!", "tokens": ["Ge\u00b7gen", "Den", "dies", "al\u00b7les", "nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PDS", "PIS", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}