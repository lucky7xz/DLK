{"textgrid.poem.46858": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[so bringt ihr mir noch immer eure Rosen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bringt ihr mir noch immer eure Rosen,", "tokens": ["So", "bringt", "ihr", "mir", "noch", "im\u00b7mer", "eu\u00b7re", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr bringt mir, meine Theuren, theure Rosen.", "tokens": ["Ihr", "bringt", "mir", ",", "mei\u00b7ne", "Theu\u00b7ren", ",", "theu\u00b7re", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie einst im L\u00e4cheln eurer Wangengr\u00fcbchen,", "tokens": ["Wie", "einst", "im", "L\u00e4\u00b7cheln", "eu\u00b7rer", "Wan\u00b7gen\u00b7gr\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bringt ihr nun aus der Gruft mir eure Rosen.", "tokens": ["Bringt", "ihr", "nun", "aus", "der", "Gruft", "mir", "eu\u00b7re", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur dazu ist der Fr\u00fchling mir gekommen,", "tokens": ["Nur", "da\u00b7zu", "ist", "der", "Fr\u00fch\u00b7ling", "mir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df er auf eurem Grab erneure Rosen.", "tokens": ["Da\u00df", "er", "auf", "eu\u00b7rem", "Grab", "er\u00b7neu\u00b7re", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und dazu Fr\u00fch- und Abendroth entglommen,", "tokens": ["Und", "da\u00b7zu", "Fr\u00fch", "und", "A\u00b7ben\u00b7droth", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "TRUNC", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df es auf eurem Grab befeure Rosen.", "tokens": ["Da\u00df", "es", "auf", "eu\u00b7rem", "Grab", "be\u00b7feu\u00b7re", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und nach dem kurzen Rosenfest des Lebens", "tokens": ["Und", "nach", "dem", "kur\u00b7zen", "Ro\u00b7sen\u00b7fest", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Kommt nun der Tod, da\u00df er nachsteure Rosen.", "tokens": ["Kommt", "nun", "der", "Tod", ",", "da\u00df", "er", "nach\u00b7steu\u00b7re", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}