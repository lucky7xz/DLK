{"dta.poem.11052": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ausgesetzte Besserung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Ehr-und Reichthum-f\u00fcchtige sich bis an ihren\nTod bestreben,", "tokens": ["Wie", "Ehr\u00b7\u00b7und", "Reicht\u00b7hum\u00b7f\u00fcch\u00b7ti\u00b7ge", "sich", "bis", "an", "ih\u00b7ren", "Tod", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und immer sich besch\u00e4fftigen, um dann, den Rest von ihrem", "tokens": ["Und", "im\u00b7mer", "sich", "be\u00b7sch\u00e4ff\u00b7ti\u00b7gen", ",", "um", "dann", ",", "den", "Rest", "von", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PRF", "VVPP", "$,", "KOUI", "ADV", "$,", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Des Fleisses Fr\u00fcchte zu geniessen, und, nach getragner Last,", "tokens": ["Des", "Fleis\u00b7ses", "Fr\u00fcch\u00b7te", "zu", "ge\u00b7nies\u00b7sen", ",", "und", ",", "nach", "ge\u00b7trag\u00b7ner", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,", "KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "So sieht man, da\u00df auch die\u00df nicht minder von den Gelehrten", "tokens": ["So", "sieht", "man", ",", "da\u00df", "auch", "die\u00df", "nicht", "min\u00b7der", "von", "den", "Ge\u00b7lehr\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ADV", "PDS", "PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sie sind ", "tokens": ["Sie", "sind"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und, im best\u00e4ndgen Untersuchen, ihr Wissen immer anzuh\u00e4ufen,", "tokens": ["Und", ",", "im", "be\u00b7st\u00e4nd\u00b7gen", "Un\u00b7ter\u00b7su\u00b7chen", ",", "ihr", "Wis\u00b7sen", "im\u00b7mer", "an\u00b7zu\u00b7h\u00e4u\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Um denn von ihrer Theorie den ", "tokens": ["Um", "denn", "von", "ih\u00b7rer", "The\u00b7o\u00b7rie", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu ihrer eignen Besserung, der Praxi sich zu \u00fcbergeben.", "tokens": ["Zu", "ih\u00b7rer", "eig\u00b7nen", "Bes\u00b7se\u00b7rung", ",", "der", "Pra\u00b7xi", "sich", "zu", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}