{"dta.poem.2286": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nt ich nur wie Orpheus spielen,", "tokens": ["K\u00f6nt", "ich", "nur", "wie", "Or\u00b7pheus", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Knaben soltens f\u00fchlen,", "tokens": ["Al\u00b7le", "Kna\u00b7ben", "sol\u00b7tens", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn ich ein St\u00fckk gespielet,", "tokens": ["Und", "wenn", "ich", "ein", "St\u00fckk", "ge\u00b7spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wenn sie den Reitz gef\u00fchlet,", "tokens": ["Und", "wenn", "sie", "den", "Reitz", "ge\u00b7f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Solten sie, bei Spiel und Singen,", "tokens": ["Sol\u00b7ten", "sie", ",", "bei", "Spiel", "und", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle tanzen, alle springen;", "tokens": ["Al\u00b7le", "tan\u00b7zen", ",", "al\u00b7le", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nt ich wie Apelles schildern,", "tokens": ["K\u00f6nt", "ich", "wie", "A\u00b7pel\u00b7les", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O so solt es meinen Bildern", "tokens": ["O", "so", "solt", "es", "mei\u00b7nen", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nicht an Reitz und Sch\u00f6nheit fehlen,", "tokens": ["Nicht", "an", "Reitz", "und", "Sch\u00f6n\u00b7heit", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lauter Knaben wolt ich w\u00e4hlen;", "tokens": ["Lau\u00b7ter", "Kna\u00b7ben", "wolt", "ich", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "K\u00f6nt ich k\u00fcnstlich, wie Propheten,", "tokens": ["K\u00f6nt", "ich", "k\u00fcnst\u00b7lich", ",", "wie", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Menschen wekken, Menschen t\u00f6dten,", "tokens": ["Men\u00b7schen", "wek\u00b7ken", ",", "Men\u00b7schen", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "O so wolt ich Geist und Leben", "tokens": ["O", "so", "wolt", "ich", "Geist", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Allen Knaben wieder geben.", "tokens": ["Al\u00b7len", "Kna\u00b7ben", "wie\u00b7der", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber k\u00f6nt ich meinen Willen", "tokens": ["A\u00b7ber", "k\u00f6nt", "ich", "mei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Durch ein m\u00e4chtig Wort erf\u00fcllen,", "tokens": ["Durch", "ein", "m\u00e4ch\u00b7tig", "Wort", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Einer solte nach dem andern", "tokens": ["Ei\u00b7ner", "sol\u00b7te", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wieder zu den Todten wandern;", "tokens": ["Wie\u00b7der", "zu", "den", "Tod\u00b7ten", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Vieler Knaben Schildereien", "tokens": ["Vie\u00b7ler", "Kna\u00b7ben", "Schil\u00b7de\u00b7rei\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Solten brennend mich erfreuen;", "tokens": ["Sol\u00b7ten", "bren\u00b7nend", "mich", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Viele solten, unzufrieden,", "tokens": ["Vie\u00b7le", "sol\u00b7ten", ",", "un\u00b7zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Gleich vom ersten Tanz erm\u00fcden,", "tokens": ["Gleich", "vom", "ers\u00b7ten", "Tanz", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Stille sitzen, stille stehen,", "tokens": ["Stil\u00b7le", "sit\u00b7zen", ",", "stil\u00b7le", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und die andern tanzen sehen.", "tokens": ["Und", "die", "an\u00b7dern", "tan\u00b7zen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}