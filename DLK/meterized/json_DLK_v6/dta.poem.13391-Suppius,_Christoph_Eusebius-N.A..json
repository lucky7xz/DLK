{"dta.poem.13391": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Da", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "bey", "tokens": ["bey"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Landes-Mutter\nLebensl\u00e4nge", "tokens": ["Der", "Lan\u00b7des\u00b7Mut\u00b7ter", "Le\u00b7bens\u00b7l\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von h\u00f6chster Dauer w\u00fcnscht zu seyn;", "tokens": ["von", "h\u00f6chs\u00b7ter", "Dau\u00b7er", "w\u00fcnscht", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "so m\u00fcsse vor Jhr Gnade finden,", "tokens": ["so", "m\u00fcs\u00b7se", "vor", "Ihr", "Gna\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "was", "tokens": ["was"], "token_info": ["word"], "pos": ["PWS"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "bey sp\u00e4ter Nacht, voll Ehrfurcht weiht", "tokens": ["bey", "sp\u00e4\u00b7ter", "Nacht", ",", "voll", "Ehr\u00b7furcht", "weiht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}