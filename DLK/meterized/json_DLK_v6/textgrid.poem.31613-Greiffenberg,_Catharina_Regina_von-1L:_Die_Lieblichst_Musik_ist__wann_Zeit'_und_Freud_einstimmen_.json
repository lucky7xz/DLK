{"textgrid.poem.31613": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lieblichst Musik ist / wann Zeit' und Freud einstimmen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lieblichst Musik ist / wann Zeit' und Freud einstimmen /", "tokens": ["Die", "Lieb\u00b7lichst", "Mu\u00b7sik", "ist", "/", "wann", "Zeit'", "und", "Freud", "ein\u00b7stim\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$(", "PWAV", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wann Herz und Lufft zugleich / still / klar und heiter seyn /", "tokens": ["wann", "Herz", "und", "Lufft", "zu\u00b7gleich", "/", "still", "/", "klar", "und", "hei\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "$(", "ADJD", "$(", "ADJD", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wann man zugleich empfind der Sonn' und Wonne Schein /", "tokens": ["wann", "man", "zu\u00b7gleich", "emp\u00b7find", "der", "Sonn'", "und", "Won\u00b7ne", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "ART", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wann die Gedanken mit dem Schwalben Wolk-an klimmen /", "tokens": ["wann", "die", "Ge\u00b7dan\u00b7ken", "mit", "dem", "Schwal\u00b7ben", "Wol\u00b7kan", "klim\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und mit dem Sternen-Glanz / die Andachts-Funken glimmen.", "tokens": ["und", "mit", "dem", "Ster\u00b7nen\u00b7Glanz", "/", "die", "An\u00b7dachts\u00b7Fun\u00b7ken", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann flicht sich Lorbeer-Lob in alle Saiten ein /", "tokens": ["Dann", "flicht", "sich", "Lor\u00b7beer\u00b7Lob", "in", "al\u00b7le", "Sai\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPR", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und herrscht der Herzen Heer in seinem Thron allein /", "tokens": ["und", "herrscht", "der", "Her\u00b7zen", "Heer", "in", "sei\u00b7nem", "Thron", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und ist der Tugend Flug erschwungen ihn zu r\u00fchmen.", "tokens": ["und", "ist", "der", "Tu\u00b7gend", "Flug", "er\u00b7schwun\u00b7gen", "ihn", "zu", "r\u00fch\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich brauche mich der Zeit / O Ewigs Wunder wol /", "tokens": ["Ich", "brau\u00b7che", "mich", "der", "Zeit", "/", "O", "E\u00b7wigs", "Wun\u00b7der", "wol", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "NE", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "du Herz-vertheilte Lieb' und eingeherzte Flammen!", "tokens": ["du", "Her\u00b7zvert\u00b7heil\u00b7te", "Lieb'", "und", "ein\u00b7ge\u00b7herz\u00b7te", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach unersch\u00f6pfte Quell / vollkommen doch beysammen!", "tokens": ["Ach", "un\u00b7er\u00b7sch\u00f6pf\u00b7te", "Quell", "/", "voll\u00b7kom\u00b7men", "doch", "bey\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ich f\u00fchl und will wol viel / kan doch nicht / wie ich sol /", "tokens": ["ich", "f\u00fchl", "und", "will", "wol", "viel", "/", "kan", "doch", "nicht", "/", "wie", "ich", "sol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "ADV", "ADV", "$(", "VMFIN", "ADV", "PTKNEG", "$(", "PWAV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "dich prei\u00dfen: Ach verleih solch \u00fcberschw\u00e4nklich Krafft", "tokens": ["dich", "prei\u00b7\u00dfen", ":", "Ach", "ver\u00b7leih", "solch", "\u00fc\u00b7bersc\u00b7hw\u00e4nk\u00b7lich", "Krafft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "VVFIN", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "zu loben / als du mir liest fliessen Freuden-Safft!", "tokens": ["zu", "lo\u00b7ben", "/", "als", "du", "mir", "liest", "flies\u00b7sen", "Freu\u00b7den\u00b7\u00b7S\u00b7afft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "KOUS", "PPER", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--++-+--+", "measure": "iambic.hexa.relaxed"}}}}}