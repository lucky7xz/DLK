{"textgrid.poem.40992": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An ein gr\u00fcnes Glas von Duller", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein gr\u00fcnes Glas, mein Dullerglas!", "tokens": ["Mein", "gr\u00fc\u00b7nes", "Glas", ",", "mein", "Dul\u00b7ler\u00b7glas", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn nun verwelkt liegt Blatt und Gras,", "tokens": ["Wenn", "nun", "ver\u00b7welkt", "liegt", "Blatt", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All Gr\u00fcnes von der Erde wich,", "tokens": ["All", "Gr\u00fc\u00b7nes", "von", "der", "Er\u00b7de", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Greif' ich nach dir und f\u00fclle dich.", "tokens": ["Greif'", "ich", "nach", "dir", "und", "f\u00fcl\u00b7le", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da schaut aus dem smaragdnen Grund", "tokens": ["Da", "schaut", "aus", "dem", "sma\u00b7ragd\u00b7nen", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Erdball wieder gr\u00fcnend rund,", "tokens": ["Der", "Erd\u00b7ball", "wie\u00b7der", "gr\u00fc\u00b7nend", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch das Gr\u00fcne blitzt der Wein", "tokens": ["Und", "durch", "das", "Gr\u00fc\u00b7ne", "blitzt", "der", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie durch Gezweig der Sonne Schein.", "tokens": ["Wie", "durch", "Ge\u00b7zweig", "der", "Son\u00b7ne", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und leer' das Glas ich, f\u00fcllt die Luft", "tokens": ["Und", "leer'", "das", "Glas", "ich", ",", "f\u00fcllt", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Duften wie Waldblumenduft,", "tokens": ["Ein", "Duf\u00b7ten", "wie", "Wald\u00b7blu\u00b7men\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schlag' ans Glas ich, t\u00f6nt ein Klang", "tokens": ["Und", "schlag'", "ans", "Glas", "ich", ",", "t\u00f6nt", "ein", "Klang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie durch Walddunkel Vogelsang.", "tokens": ["Wie", "durch", "Wald\u00b7dun\u00b7kel", "Vo\u00b7gel\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "$."], "meter": "--++-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Doch nicht allein zur Winterszeit", "tokens": ["Doch", "nicht", "al\u00b7lein", "zur", "Win\u00b7ter\u00b7szeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es oft aufs Herze eisig schneit,", "tokens": ["Es", "oft", "aufs", "Her\u00b7ze", "ei\u00b7sig", "schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch Sommers kommt wohl manch ein Tag,", "tokens": ["Auch", "Som\u00b7mers", "kommt", "wohl", "manch", "ein", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Glas, wo ich in dich schauen mag.", "tokens": ["Glas", ",", "wo", "ich", "in", "dich", "schau\u00b7en", "mag."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Mein Dullerglas! dich la\u00df ich nicht,", "tokens": ["Mein", "Dul\u00b7ler\u00b7glas", "!", "dich", "la\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis g\u00e4nzlich voll mein Herz zerbricht,", "tokens": ["Bis", "g\u00e4nz\u00b7lich", "voll", "mein", "Herz", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zersprungen ist es, ach! schon lang!", "tokens": ["Zer\u00b7sprun\u00b7gen", "ist", "es", ",", "ach", "!", "schon", "lang", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "ITJ", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gibt nicht wie du mehr hellen Klang.", "tokens": ["Gibt", "nicht", "wie", "du", "mehr", "hel\u00b7len", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWAV", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hell aber klinge lang noch du!", "tokens": ["Hell", "a\u00b7ber", "klin\u00b7ge", "lang", "noch", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADJD", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und decket Gras mich M\u00fcden zu,", "tokens": ["Und", "de\u00b7cket", "Gras", "mich", "M\u00fc\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nehm' Duller wieder dich nach Haus", "tokens": ["Nehm'", "Dul\u00b7ler", "wie\u00b7der", "dich", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke mein, trinkt er dich aus.", "tokens": ["Und", "den\u00b7ke", "mein", ",", "trinkt", "er", "dich", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "$,", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}