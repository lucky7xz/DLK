{"textgrid.poem.62532": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bl\u00fcten wehen, Lieder klingen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcten wehen, Lieder klingen", "tokens": ["Bl\u00fc\u00b7ten", "we\u00b7hen", ",", "Lie\u00b7der", "klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Lenzes gr\u00fcne Hallen,", "tokens": ["Durch", "des", "Len\u00b7zes", "gr\u00fc\u00b7ne", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebestrunkne Nachtigallen", "tokens": ["Lie\u00b7be\u00b7strunk\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt man von den Rosen singen.", "tokens": ["H\u00f6rt", "man", "von", "den", "Ro\u00b7sen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und auch du, o Liederseele,", "tokens": ["Und", "auch", "du", ",", "o", "Lie\u00b7der\u00b7see\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regest nun die zarten Fl\u00fcgel;", "tokens": ["Re\u00b7gest", "nun", "die", "zar\u00b7ten", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, zu welchem Blumenh\u00fcgel", "tokens": ["Ach", ",", "zu", "wel\u00b7chem", "Blu\u00b7men\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebet meine Philomele?", "tokens": ["Schwe\u00b7bet", "mei\u00b7ne", "Phi\u00b7lo\u00b7me\u00b7le", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur in Sarons Rosend\u00fcften", "tokens": ["Nur", "in", "Sa\u00b7rons", "Ro\u00b7sen\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will sie k\u00fcnftig sich berauschen,", "tokens": ["Will", "sie", "k\u00fcnf\u00b7tig", "sich", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf Engelharfen lauschen,", "tokens": ["Und", "auf", "En\u00b7gel\u00b7har\u00b7fen", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft umspielt von Himmelsl\u00fcften.", "tokens": ["Sanft", "um\u00b7spielt", "von", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sie werden ", "tokens": ["Doch", "sie", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schweige du, mein tiefes Gr\u00e4men!", "tokens": ["Schwei\u00b7ge", "du", ",", "mein", "tie\u00b7fes", "Gr\u00e4\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald wird man ihr Lied vernehmen,", "tokens": ["Bald", "wird", "man", "ihr", "Lied", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo die Lebensb\u00e4che rauschen.", "tokens": ["Wo", "die", "Le\u00b7bens\u00b7b\u00e4\u00b7che", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}