{"textgrid.poem.49521": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sinngedicht", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den F\u00e4cher in der Hand, gieng ich in meinen Garten,", "tokens": ["Den", "F\u00e4\u00b7cher", "in", "der", "Hand", ",", "gieng", "ich", "in", "mei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den jungen Zephir zu erwarten;", "tokens": ["Den", "jun\u00b7gen", "Ze\u00b7phir", "zu", "er\u00b7war\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schnell spielt mein Unterrock, und hebt sich bl\u00e4hend auf.", "tokens": ["Schnell", "spielt", "mein", "Un\u00b7ter\u00b7rock", ",", "und", "hebt", "sich", "bl\u00e4\u00b7hend", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Willkommen, o Favon, der Sch\u00e4ferinnen Diener!", "tokens": ["Will\u00b7kom\u00b7men", ",", "o", "Fa\u00b7von", ",", "der", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "Die\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "So dacht' ich \u2013 \u2013 Aber ach! es war in strengem Lauf", "tokens": ["So", "dacht'", "ich", "\u2013", "\u2013", "A\u00b7ber", "ach", "!", "es", "war", "in", "stren\u00b7gem", "Lauf"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "$(", "KON", "ADV", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein grober Nord: ein Capuciner.", "tokens": ["Ein", "gro\u00b7ber", "Nord", ":", "ein", "Ca\u00b7pu\u00b7ci\u00b7ner", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}