{"dta.poem.233": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Albanien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Albanie, ich bin entz\u00fcndet", "tokens": ["Al\u00b7ba\u00b7nie", ",", "ich", "bin", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zwar durch deine Sch\u00f6nheit-Pracht/", "tokens": ["Und", "zwar", "durch", "dei\u00b7ne", "Sch\u00f6n\u00b7heit\u00b7Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dich sich mein Vergn\u00fcgen gr\u00fcndet;", "tokens": ["Auf", "dich", "sich", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "gr\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die andern sind vor nichts geacht.", "tokens": ["Die", "an\u00b7dern", "sind", "vor", "nichts", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du schl\u00e4gst mein Hertze/", "tokens": ["Du", "schl\u00e4gst", "mein", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Durch einen Strahl/", "tokens": ["Durch", "ei\u00b7nen", "Strahl", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch ist der Schmertze/", "tokens": ["Doch", "ist", "der", "Schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "An statt der Quaal", "tokens": ["An", "statt", "der", "Qua\u00b7al"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ein Freuden-Mahl.", "tokens": ["Ein", "Freu\u00b7den\u00b7Mahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein Geist/ der ist durch dich bestricket/", "tokens": ["Mein", "Geist", "/", "der", "ist", "durch", "dich", "be\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "VAFIN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Lust seh\u2019 ich die Banden an/", "tokens": ["Mit", "Lust", "seh'", "ich", "die", "Ban\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Sch\u00f6nseyn hat mein Hertz ber\u00fccket/", "tokens": ["Dein", "Sch\u00f6n\u00b7seyn", "hat", "mein", "Hertz", "be\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ohne dich nicht leben kan", "tokens": ["So", "oh\u00b7ne", "dich", "nicht", "le\u00b7ben", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir find deine Banden", "tokens": ["Mir", "find", "dei\u00b7ne", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gar sanfft und leicht.", "tokens": ["Gar", "sanfft", "und", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wo ist verhanden", "tokens": ["Wo", "ist", "ver\u00b7han\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die/ so dir gleicht?", "tokens": ["Die", "/", "so", "dir", "gleicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Die Blicke deiner holden Augen", "tokens": ["Die", "Bli\u00b7cke", "dei\u00b7ner", "hol\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die t\u00f6dten und erquicken mich/", "tokens": ["Die", "t\u00f6d\u00b7ten", "und", "er\u00b7qui\u00b7cken", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich mu\u00df die Flammen saugen/", "tokens": ["Und", "wenn", "ich", "mu\u00df", "die", "Flam\u00b7men", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So k\u00f6mmt mein Geist gantz ausser sich.", "tokens": ["So", "k\u00f6mmt", "mein", "Geist", "gantz", "aus\u00b7ser", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich werd\u2019 entz\u00fccket", "tokens": ["Ich", "werd'", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Durch deine Macht/", "tokens": ["Durch", "dei\u00b7ne", "Macht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und auch erquicket", "tokens": ["Und", "auch", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wenn mich anlacht", "tokens": ["Wenn", "mich", "an\u00b7lacht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der Augen Pracht.", "tokens": ["Der", "Au\u00b7gen", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}