{"textgrid.poem.61072": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Reue", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht war schwarz, die Luft war schw\u00fcl,", "tokens": ["Die", "Nacht", "war", "schwarz", ",", "die", "Luft", "war", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich fand nicht Schlaf auf meinem Pf\u00fchl,", "tokens": ["Ich", "fand", "nicht", "Schlaf", "auf", "mei\u00b7nem", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Sinn ward tr\u00fcb und tr\u00fcber;", "tokens": ["Mein", "Sinn", "ward", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da schritten die Tage der alten Zeit", "tokens": ["Da", "schrit\u00b7ten", "die", "Ta\u00b7ge", "der", "al\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Zu langem, langem Zug gereiht", "tokens": ["Zu", "lan\u00b7gem", ",", "lan\u00b7gem", "Zug", "ge\u00b7reiht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wehklagend mir vor\u00fcber:", "tokens": ["Weh\u00b7kla\u00b7gend", "mir", "vor\u00b7\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbdu hattest den Lenz, und du hast ihn entlaubt,", "tokens": ["\u00bb", "du", "hat\u00b7test", "den", "Lenz", ",", "und", "du", "hast", "ihn", "ent\u00b7laubt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du hattest das Heil, und du hast nicht geglaubt,", "tokens": ["Du", "hat\u00b7test", "das", "Heil", ",", "und", "du", "hast", "nicht", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du hattest ein Herz zum Lieben,", "tokens": ["Du", "hat\u00b7test", "ein", "Herz", "zum", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du hast es vert\u00e4ndelt mit eitlem Schein;", "tokens": ["Du", "hast", "es", "ver\u00b7t\u00e4n\u00b7delt", "mit", "eit\u00b7lem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Nun bist du zuletzt allein, allein", "tokens": ["Nun", "bist", "du", "zu\u00b7letzt", "al\u00b7lein", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Mit deinem Jammer geblieben.", "tokens": ["Mit", "dei\u00b7nem", "Jam\u00b7mer", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wie du ringst in bangem Gebet,", "tokens": ["Und", "wie", "du", "ringst", "in", "ban\u00b7gem", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist zu sp\u00e4t, es ist zu sp\u00e4t,", "tokens": ["Es", "ist", "zu", "sp\u00e4t", ",", "es", "ist", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du darfst von Rast nicht wissen;", "tokens": ["Du", "darfst", "von", "Rast", "nicht", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein einsam Herz ist dein Gericht.\u00ab", "tokens": ["Dein", "ein\u00b7sam", "Herz", "ist", "dein", "Ge\u00b7richt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich aber dr\u00fcckte mein Angesicht", "tokens": ["Ich", "a\u00b7ber", "dr\u00fcck\u00b7te", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lautweinend in die Kissen.", "tokens": ["Laut\u00b7wei\u00b7nend", "in", "die", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}