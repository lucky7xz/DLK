{"dta.poem.20938": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .  \n Lots.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich eilt\u2019 aus Sodoma den Flammen zu entgehen/", "tokens": ["Ich", "eilt'", "aus", "So\u00b7do\u00b7ma", "den", "Flam\u00b7men", "zu", "ent\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und konte nicht der Brunst der T\u00f6chter wider stehen/", "tokens": ["Und", "kon\u00b7te", "nicht", "der", "Brunst", "der", "T\u00f6ch\u00b7ter", "wi\u00b7der", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer macht den armen Lot der schweren S\u00fcnden lo\u00df/", "tokens": ["Wer", "macht", "den", "ar\u00b7men", "Lot", "der", "schwe\u00b7ren", "S\u00fcn\u00b7den", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich baute Sodoma auff meiner T\u00f6chter Scho\u00df.", "tokens": ["Ich", "bau\u00b7te", "So\u00b7do\u00b7ma", "auff", "mei\u00b7ner", "T\u00f6ch\u00b7ter", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}