{"dta.poem.20850": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df/ was man in den weiten Schrancken", "tokens": ["Di\u00df", "/", "was", "man", "in", "den", "wei\u00b7ten", "Schran\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$(", "PWS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschen m\u00fchsamen Gedancken/", "tokens": ["Der", "Men\u00b7schen", "m\u00fch\u00b7sa\u00b7men", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Wei\u00dfheit und vor K\u00fcnste sp\u00fchrt/", "tokens": ["Vor", "Wei\u00df\u00b7heit", "und", "vor", "K\u00fcns\u00b7te", "sp\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nur ein wieder holtes Wesen/", "tokens": ["Ist", "nur", "ein", "wie\u00b7der", "hol\u00b7tes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dadurch der kluge Geist versp\u00fchrt:", "tokens": ["Da\u00b7durch", "der", "klu\u00b7ge", "Geist", "ver\u00b7sp\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was er in jener Welt gewesen/", "tokens": ["Was", "er", "in", "je\u00b7ner", "Welt", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein gantzes Thun das denckt darauf/", "tokens": ["Sein", "gant\u00b7zes", "Thun", "das", "denckt", "da\u00b7rauf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PDS", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was er vor langer Zeit getrieben/", "tokens": ["Was", "er", "vor", "lan\u00b7ger", "Zeit", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und durch viel schneller Zeiten Lauf", "tokens": ["Und", "durch", "viel", "schnel\u00b7ler", "Zei\u00b7ten", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "stets unversehret ist geblieben.", "tokens": ["stets", "un\u00b7ver\u00b7seh\u00b7ret", "ist", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}