{"dta.poem.1408": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Uns h\u00e4lt zu dieser Zeit der Hitze Last beladen/", "tokens": ["Uns", "h\u00e4lt", "zu", "die\u00b7ser", "Zeit", "der", "Hit\u00b7ze", "Last", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich GOttes schwerer Zorn und angeerbte Schuld!", "tokens": ["Dich", "Got\u00b7tes", "schwe\u00b7rer", "Zorn", "und", "an\u00b7ge\u00b7erb\u00b7te", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit du kommen m\u00f6gst in deines Sch\u00f6pffers Huld/", "tokens": ["Da\u00b7mit", "du", "kom\u00b7men", "m\u00f6gst", "in", "dei\u00b7nes", "Sch\u00f6pf\u00b7fers", "Huld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll dich der Tauffe Brunn von S\u00fcnden sauber baden/", "tokens": ["Soll", "dich", "der", "Tauf\u00b7fe", "Brunn", "von", "S\u00fcn\u00b7den", "sau\u00b7ber", "ba\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "NN", "APPR", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dir/ o Pathe/ seyn ein Pfand des Thaus der Gnaden.", "tokens": ["Und", "dir", "/", "o", "Pa\u00b7the", "/", "seyn", "ein", "Pfand", "des", "Thaus", "der", "Gna\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "FM", "FM", "$(", "PPOSAT", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}