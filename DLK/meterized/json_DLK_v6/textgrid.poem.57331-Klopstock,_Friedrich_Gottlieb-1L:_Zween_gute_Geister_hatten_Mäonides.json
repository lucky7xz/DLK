{"textgrid.poem.57331": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zween gute Geister hatten M\u00e4onides", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween gute Geister hatten M\u00e4onides", "tokens": ["Zween", "gu\u00b7te", "Geis\u00b7ter", "hat\u00b7ten", "M\u00e4o\u00b7ni\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Maro's Sprachen, Wohlklang und Silbenmass.", "tokens": ["Und", "Ma\u00b7ro's", "Spra\u00b7chen", ",", "Wohl\u00b7klang", "und", "Sil\u00b7ben\u00b7mass", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Dichter wallten, in der Obhut", "tokens": ["Die", "Dich\u00b7ter", "wall\u00b7ten", ",", "in", "der", "Ob\u00b7hut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sichrer, den Weg bis zu uns herunter.", "tokens": ["Sich\u00b7rer", ",", "den", "Weg", "bis", "zu", "uns", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sp\u00e4tern Sprachen haben des Klangs noch wohl;", "tokens": ["Die", "sp\u00e4\u00b7tern", "Spra\u00b7chen", "ha\u00b7ben", "des", "Klangs", "noch", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch auch des Silbenmasses? Statt dessen ist", "tokens": ["Doch", "auch", "des", "Sil\u00b7ben\u00b7mas\u00b7ses", "?", "Statt", "des\u00b7sen", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$.", "NN", "PDS", "VAFIN"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "In sie ein b\u00f6ser Geist, mit plumpen", "tokens": ["In", "sie", "ein", "b\u00f6\u00b7ser", "Geist", ",", "mit", "plum\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00f6rtergepolter, der Reim, gefahren.", "tokens": ["W\u00f6r\u00b7ter\u00b7ge\u00b7pol\u00b7ter", ",", "der", "Reim", ",", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Red' ist der Wohlklang, Rede das Silbenmass;", "tokens": ["Red'", "ist", "der", "Wohl\u00b7klang", ",", "Re\u00b7de", "das", "Sil\u00b7ben\u00b7mass", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allein des Reimes schmetternder Trommelschlag", "tokens": ["Al\u00b7lein", "des", "Rei\u00b7mes", "schmet\u00b7tern\u00b7der", "Trom\u00b7mel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was der? was sagt uns sein Gewirbel,", "tokens": ["Was", "der", "?", "was", "sagt", "uns", "sein", "Ge\u00b7wir\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "$.", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lermend und lermend mit Gleichget\u00f6ne?", "tokens": ["Ler\u00b7mend", "und", "ler\u00b7mend", "mit", "Gleich\u00b7ge\u00b7t\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dank unsern Dichtern! Da sich des Kritlers Ohr,", "tokens": ["Dank", "un\u00b7sern", "Dich\u00b7tern", "!", "Da", "sich", "des", "Krit\u00b7lers", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KOUS", "PRF", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Fern von des Urtheils Stolze, verh\u00f6rete;", "tokens": ["Fern", "von", "des", "Ur\u00b7theils", "Stol\u00b7ze", ",", "ver\u00b7h\u00f6\u00b7re\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "$,", "VVFIN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Verliessen sie mich nicht, und sangen", "tokens": ["Ver\u00b7lies\u00b7sen", "sie", "mich", "nicht", ",", "und", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ohne den Lerm, und im Ton des Griechen.", "tokens": ["Oh\u00b7ne", "den", "Lerm", ",", "und", "im", "Ton", "des", "Grie\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "So weit wie Maro kam und M\u00e4onides", "tokens": ["So", "weit", "wie", "Ma\u00b7ro", "kam", "und", "M\u00e4o\u00b7ni\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Liedestanze, k\u00e4men mit ihrem Reim", "tokens": ["Mit", "Lie\u00b7des\u00b7tan\u00b7ze", ",", "k\u00e4\u00b7men", "mit", "ih\u00b7rem", "Reim"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Die Neuern? unter seinem Schutze", "tokens": ["Die", "Neu\u00b7ern", "?", "un\u00b7ter", "sei\u00b7nem", "Schut\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sichrer im Gange, da ganz hinunter?", "tokens": ["Sich\u00b7rer", "im", "Gan\u00b7ge", ",", "da", "ganz", "hin\u00b7un\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "KOUS", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Dank euch noch Einmal, Dichter! Die Sprache war", "tokens": ["Dank", "euch", "noch", "Ein\u00b7mal", ",", "Dich\u00b7ter", "!", "Die", "Spra\u00b7che", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADV", "$,", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Durch unsern Jambus halb in die Acht erkl\u00e4rt,", "tokens": ["Durch", "un\u00b7sern", "Jam\u00b7bus", "halb", "in", "die", "Acht", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "APPR", "ART", "CARD", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Bann der Leidenschaften Ausdruck,", "tokens": ["Im", "Bann", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Aus\u00b7druck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher dahin mit dem Rithmus str\u00f6met.", "tokens": ["Wel\u00b7cher", "da\u00b7hin", "mit", "dem", "Rith\u00b7mus", "str\u00f6\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn mir der Ruf nicht fabelt; verschm\u00e4het selbst", "tokens": ["Wenn", "mir", "der", "Ruf", "nicht", "fa\u00b7belt", ";", "ver\u00b7schm\u00e4\u00b7het", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$.", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der T\u00f6ne Land diess Neue: und dennoch ist", "tokens": ["Der", "T\u00f6\u00b7ne", "Land", "diess", "Neu\u00b7e", ":", "und", "den\u00b7noch", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PDAT", "ADJA", "$.", "KON", "ADV", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Sprache dort die muttergleichste", "tokens": ["Die", "Spra\u00b7che", "dort", "die", "mut\u00b7ter\u00b7gleichs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den T\u00f6chtern der Romanide.", "tokens": ["Un\u00b7ter", "den", "T\u00f6ch\u00b7tern", "der", "Ro\u00b7ma\u00b7ni\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Weil denn in dieser H\u00f6he die Traub' euch h\u00e4ngt;", "tokens": ["Weil", "denn", "in", "die\u00b7ser", "H\u00f6\u00b7he", "die", "Traub'", "euch", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PDAT", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So hab' ich Freundes Mitleid mit euch, dass sie", "tokens": ["So", "hab'", "ich", "Freun\u00b7des", "Mit\u00b7leid", "mit", "euch", ",", "dass", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "APPR", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So gar es nicht vermag, die sch\u00f6nste", "tokens": ["So", "gar", "es", "nicht", "ver\u00b7mag", ",", "die", "sch\u00f6ns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PTKNEG", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den T\u00f6chtern der Romanide.", "tokens": ["Un\u00b7ter", "den", "T\u00f6ch\u00b7tern", "der", "Ro\u00b7ma\u00b7ni\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Die Spr\u00e4chen alle stutzen, Begeistrung, oft,", "tokens": ["Die", "Spr\u00e4\u00b7chen", "al\u00b7le", "stut\u00b7zen", ",", "Be\u00b7geis\u00b7trung", ",", "oft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gebeutst du, t\u00f6nen soll es, wovon du gl\u00fchst!", "tokens": ["Ge\u00b7beutst", "du", ",", "t\u00f6\u00b7nen", "soll", "es", ",", "wo\u00b7von", "du", "gl\u00fchst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVINF", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Soll dir von allen deinen Flammen", "tokens": ["Soll", "dir", "von", "al\u00b7len", "dei\u00b7nen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Keine bew\u00f6lkender Dampf verh\u00fcllen!", "tokens": ["Kei\u00b7ne", "be\u00b7w\u00f6l\u00b7ken\u00b7der", "Dampf", "ver\u00b7h\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Beklagt den Dichter, wenn es der seinen jetzt", "tokens": ["Be\u00b7klagt", "den", "Dich\u00b7ter", ",", "wenn", "es", "der", "sei\u00b7nen", "jetzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "PPOSAT", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gar an der Nothdurft Scherfe gebricht, ihr jetzt,", "tokens": ["Gar", "an", "der", "Noth\u00b7durft", "Scher\u00b7fe", "ge\u00b7bricht", ",", "ihr", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo sich dem Geist das Wort nicht nachschwingt,", "tokens": ["Wo", "sich", "dem", "Geist", "das", "Wort", "nicht", "nach\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht die Bewegung die Schwesterhand beut:", "tokens": ["Nicht", "die", "Be\u00b7we\u00b7gung", "die", "Schwes\u00b7ter\u00b7hand", "beut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn er in ihr Anlage zum Silbenmass", "tokens": ["Wenn", "er", "in", "ihr", "An\u00b7la\u00b7ge", "zum", "Sil\u00b7ben\u00b7mass"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ausforscht, und gleichwohl sch\u00fcchtern diess Gold nicht gr\u00e4bt;", "tokens": ["Aus\u00b7forscht", ",", "und", "gleich\u00b7wohl", "sch\u00fcch\u00b7tern", "diess", "Gold", "nicht", "gr\u00e4bt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VVFIN", "PDAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00fchlt, wie des Liedes Ernst der Reime", "tokens": ["F\u00fchlt", ",", "wie", "des", "Lie\u00b7des", "Ernst", "der", "Rei\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "NE", "ART", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Spiele belachen, und doch sie mitspielt.", "tokens": ["Spie\u00b7le", "be\u00b7la\u00b7chen", ",", "und", "doch", "sie", "mit\u00b7spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "ADV", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.12": {"line.1": {"text": "Des Guten mangelt viel ihm; des Schlimmen hat", "tokens": ["Des", "Gu\u00b7ten", "man\u00b7gelt", "viel", "ihm", ";", "des", "Schlim\u00b7men", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er viel. Und jetzo komt die Begeisterung,", "tokens": ["Er", "viel", ".", "Und", "jet\u00b7zo", "komt", "die", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gebeut! Schnell blutet sie vom Dolch des", "tokens": ["Ge\u00b7beut", "!", "Schnell", "blu\u00b7tet", "sie", "vom", "Dolch", "des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stamlers! ihr Auge verlischt, sie sinket!", "tokens": ["Stam\u00b7lers", "!", "ihr", "Au\u00b7ge", "ver\u00b7lischt", ",", "sie", "sin\u00b7ket", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}