{"dta.poem.740": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.  An  Lucinden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der zarte Mund? ein K\u00f6cher voller Pfeile ", "tokens": ["Was", "ist", "der", "zar\u00b7te", "Mund", "?", "ein", "K\u00f6\u00b7cher", "vol\u00b7ler", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die manch weiches ", "tokens": ["Durch", "die", "manch", "wei\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Augen heller glantz wird flammen gleich gesch\u00e4tzt/ ", "tokens": ["Der", "Au\u00b7gen", "hel\u00b7ler", "glantz", "wird", "flam\u00b7men", "gleich", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An welchen jeder sich verbrennt in kurtzer weile.", "tokens": ["An", "wel\u00b7chen", "je\u00b7der", "sich", "ver\u00b7brennt", "in", "kurt\u00b7zer", "wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIAT", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die wundersch\u00f6nen Haar sind feste liebes seile/ ", "tokens": ["Die", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Haar", "sind", "fes\u00b7te", "lie\u00b7bes", "sei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer durch der Stirnen glantz nicht wird in euch verhetzt;", "tokens": ["Wer", "durch", "der", "Stir\u00b7nen", "glantz", "nicht", "wird", "in", "euch", "ver\u00b7hetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "PTKNEG", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer sich den Lilien der ", "tokens": ["Wer", "sich", "den", "Li\u00b7li\u00b7en", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df doch gewertig seyn da\u00df jhn die Hand ereile.", "tokens": ["Mu\u00df", "doch", "ge\u00b7wer\u00b7tig", "seyn", "da\u00df", "jhn", "die", "Hand", "er\u00b7ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sprecht jhr/ vnd ist war/ wer vol von zunder steckt ", "tokens": ["So", "sprecht", "jhr", "/", "vnd", "ist", "war", "/", "wer", "vol", "von", "zun\u00b7der", "steckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "VAFIN", "VAFIN", "$(", "PWS", "ADJD", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird leicht zu b\u00f6ser lust/ vnd ewrer lieb erweckt/ ", "tokens": ["Wird", "leicht", "zu", "b\u00f6\u00b7ser", "lust", "/", "vnd", "ew\u00b7rer", "lieb", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKZU", "ADJA", "NN", "$(", "KON", "PPOSAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man kan zu ", "tokens": ["Man", "kan", "zu"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wer aber bey sich selbst/ was jhr f\u00fcr l\u00f6blich acht", "tokens": ["Wer", "a\u00b7ber", "bey", "sich", "selbst", "/", "was", "jhr", "f\u00fcr", "l\u00f6b\u00b7lich", "acht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PRF", "ADV", "$(", "PWS", "PPER", "APPR", "ADJD", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ew\u2019r mehr denn falsches hertz\u2019 vnd schwartz gem\u00fct be-", "tokens": ["Ew'r", "mehr", "denn", "fal\u00b7sches", "hertz'", "vnd", "schwartz", "ge\u00b7m\u00fct", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "KON", "ADJD", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "tracht.", "tokens": ["tracht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Denglaubt mir/ werdet jhr Lucinde nicht entz\u00fcnden.", "tokens": ["Den\u00b7glaubt", "mir", "/", "wer\u00b7det", "jhr", "Lu\u00b7cin\u00b7de", "nicht", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}