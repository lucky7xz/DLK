{"textgrid.poem.62161": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo bist Du hin, Du liebes Tier,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist Du hin, Du liebes Tier,", "tokens": ["Wo", "bist", "Du", "hin", ",", "Du", "lie\u00b7bes", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mir so treu gewesen,", "tokens": ["Das", "mir", "so", "treu", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sich vor Freuden nicht fassen konnt',", "tokens": ["Das", "sich", "vor", "Freu\u00b7den", "nicht", "fas\u00b7sen", "konnt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durft' es in meinen Blicken lesen;", "tokens": ["Durft'", "es", "in", "mei\u00b7nen", "Bli\u00b7cken", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das hoch hinauf zum Wagen sprang", "tokens": ["Das", "hoch", "hin\u00b7auf", "zum", "Wa\u00b7gen", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wonnigem Geschreie,", "tokens": ["Mit", "won\u00b7ni\u00b7gem", "Ge\u00b7schrei\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich nach Haus zur\u00fcckgekehrt:", "tokens": ["Wenn", "ich", "nach", "Haus", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein solches Herz ist Weihe!", "tokens": ["Ein", "sol\u00b7ches", "Herz", "ist", "Wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein solches Herz vergehet nicht,", "tokens": ["Ein", "sol\u00b7ches", "Herz", "ver\u00b7ge\u00b7het", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lebt zu allen Zeiten,", "tokens": ["Es", "lebt", "zu", "al\u00b7len", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele nur erkennt und liebt,", "tokens": ["Die", "See\u00b7le", "nur", "er\u00b7kennt", "und", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Toren es bestreiten.", "tokens": ["Nur", "To\u00b7ren", "es", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}