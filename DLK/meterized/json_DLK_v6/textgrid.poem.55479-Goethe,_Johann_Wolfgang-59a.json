{"textgrid.poem.55479": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "59a", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Geduldetes, Geno\u00dfnes,", "tokens": ["Viel", "Ge\u00b7dul\u00b7de\u00b7tes", ",", "Ge\u00b7no\u00df\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halbverschwiegnes, laut Ergo\u00dfnes", "tokens": ["Halb\u00b7ver\u00b7schwieg\u00b7nes", ",", "laut", "Er\u00b7go\u00df\u00b7nes"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward in ferner Welt vertan;", "tokens": ["Ward", "in", "fer\u00b7ner", "Welt", "ver\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber jene guten Zeiten,", "tokens": ["A\u00b7ber", "je\u00b7ne", "gu\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tiefurts Tal, \u00e4therische Weiten,", "tokens": ["Tie\u00b7furts", "Tal", ",", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "Wei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Gehen dich besonders an.", "tokens": ["Ge\u00b7hen", "dich", "be\u00b7son\u00b7ders", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}