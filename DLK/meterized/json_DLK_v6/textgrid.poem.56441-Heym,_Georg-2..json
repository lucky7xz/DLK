{"textgrid.poem.56441": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels ewiger Schl\u00e4frigkeit entflohen,", "tokens": ["Des", "Him\u00b7mels", "e\u00b7wi\u00b7ger", "Schl\u00e4f\u00b7rig\u00b7keit", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Spinneweben, die der Cherubim", "tokens": ["Den", "Spin\u00b7ne\u00b7we\u00b7ben", ",", "die", "der", "Che\u00b7ru\u00b7bim"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erhobene Nasen schon wie Efeu decken,", "tokens": ["Er\u00b7ho\u00b7be\u00b7ne", "Na\u00b7sen", "schon", "wie", "E\u00b7feu", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+--+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem milden Frieden, der wie \u00d6l so fett,", "tokens": ["Dem", "mil\u00b7den", "Frie\u00b7den", ",", "der", "wie", "\u00d6l", "so", "fett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "KOKOM", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Bettler, lungert in den Ecken faul,", "tokens": ["Ein", "Bett\u00b7ler", ",", "lun\u00b7gert", "in", "den", "E\u00b7cken", "faul", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem Tabaksdunst aus den Pastorenpfeifen,", "tokens": ["Dem", "Ta\u00b7baks\u00b7dunst", "aus", "den", "Pas\u00b7to\u00b7ren\u00b7pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Trinit\u00e4t, die bei den Lobges\u00e4ngen", "tokens": ["Der", "Tri\u00b7ni\u00b7t\u00e4t", ",", "die", "bei", "den", "Lob\u00b7ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von alten Tanten auf dem Sofa schl\u00e4ft,", "tokens": ["Von", "al\u00b7ten", "Tan\u00b7ten", "auf", "dem", "So\u00b7fa", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dem ganzen gro\u00dfen Armenhospital,", "tokens": ["Dem", "gan\u00b7zen", "gro\u00b7\u00dfen", "Ar\u00b7men\u00b7hos\u00b7pi\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u2013 Verdammten selbst wir uns und kamen her", "tokens": ["\u2013", "Ver\u00b7damm\u00b7ten", "selbst", "wir", "uns", "und", "ka\u00b7men", "her"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "ADV", "PPER", "PPER", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auf dieser Insel weite \u00d6digkeit,", "tokens": ["Auf", "die\u00b7ser", "In\u00b7sel", "wei\u00b7te", "\u00d6\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die wie ein Bootskiel in den Wellen steht,", "tokens": ["Die", "wie", "ein", "Boots\u00b7kiel", "in", "den", "Wel\u00b7len", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Um bis zum Ende aller Ewigkeit", "tokens": ["Um", "bis", "zum", "En\u00b7de", "al\u00b7ler", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dem ungeheuren Strome zuzuschaun.", "tokens": ["Dem", "un\u00b7ge\u00b7heu\u00b7ren", "Stro\u00b7me", "zu\u00b7zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}