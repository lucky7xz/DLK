{"textgrid.poem.26744": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ausgeartetes Kind der bessern menschlichen Mutter,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgeartetes Kind der bessern menschlichen Mutter,", "tokens": ["Aus\u00b7ge\u00b7ar\u00b7te\u00b7tes", "Kind", "der", "bes\u00b7sern", "menschli\u00b7chen", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das mit des R\u00f6mers Gewalt paaret des Tyriers List!", "tokens": ["Das", "mit", "des", "R\u00f6\u00b7mers", "Ge\u00b7walt", "paa\u00b7ret", "des", "Ty\u00b7ri\u00b7ers", "List", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aber jener beherrschte mit Kraft die eroberte Erde,", "tokens": ["A\u00b7ber", "je\u00b7ner", "be\u00b7herrschte", "mit", "Kraft", "die", "er\u00b7o\u00b7ber\u00b7te", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Dieser belehrte die Welt, die er mit Klugheit bestahl.", "tokens": ["Die\u00b7ser", "be\u00b7lehr\u00b7te", "die", "Welt", ",", "die", "er", "mit", "Klug\u00b7heit", "be\u00b7stahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Sprich, was r\u00fchmt die Geschichte von dir, Wie der R\u00f6mer erwarbst du", "tokens": ["Sprich", ",", "was", "r\u00fchmt", "die", "Ge\u00b7schich\u00b7te", "von", "dir", ",", "Wie", "der", "R\u00f6\u00b7mer", "er\u00b7warbst", "du"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Mit dem Eisen, was du tyrisch mit Golde regierst.", "tokens": ["Mit", "dem", "Ei\u00b7sen", ",", "was", "du", "ty\u00b7risch", "mit", "Gol\u00b7de", "re\u00b7gierst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}