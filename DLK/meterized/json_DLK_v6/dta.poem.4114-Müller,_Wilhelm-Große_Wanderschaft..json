{"dta.poem.4114": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfe Wanderschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gestern dort und heute hier;", "tokens": ["Ge\u00b7stern", "dort", "und", "heu\u00b7te", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Morgen, wohin ziehen wir?", "tokens": ["Mor\u00b7gen", ",", "wo\u00b7hin", "zie\u00b7hen", "wir", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wi\u00dft ihr wohl das Losungswort,", "tokens": ["Wi\u00dft", "ihr", "wohl", "das", "Lo\u00b7sungs\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das die Welt treibt fort und fort?", "tokens": ["Das", "die", "Welt", "treibt", "fort", "und", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sehet Sonne, Mond und Sterne,", "tokens": ["Se\u00b7het", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie die wandern all' so gerne!", "tokens": ["Wie", "die", "wan\u00b7dern", "all'", "so", "ger\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Auch die Erde macht sich auf", "tokens": ["Auch", "die", "Er\u00b7de", "macht", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Alle Jahr zum frischen Lauf.", "tokens": ["Al\u00b7le", "Jahr", "zum", "fri\u00b7schen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Ei, so la\u00df das Sitzen sein,", "tokens": ["Ei", ",", "so", "la\u00df", "das", "Sit\u00b7zen", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Mensch, du mu\u00dft doch hinterdrein!", "tokens": ["Mensch", ",", "du", "mu\u00dft", "doch", "hin\u00b7ter\u00b7drein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Kind und J\u00fcngling, Mann und Greis,", "tokens": ["Kind", "und", "J\u00fcng\u00b7ling", ",", "Mann", "und", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Also hei\u00dft die Lebensreis'.", "tokens": ["Al\u00b7so", "hei\u00dft", "die", "Le\u00b7bens\u00b7reis'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Ei, wie sch\u00f6ne Kompanei!", "tokens": ["Ei", ",", "wie", "sch\u00f6\u00b7ne", "Kom\u00b7pa\u00b7nei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "F\u00fcrstengunst und Frauentreu'!", "tokens": ["F\u00fcrs\u00b7ten\u00b7gunst", "und", "Frau\u00b7en\u00b7treu'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Frau Fortuna f\u00fchrt uns an,", "tokens": ["Frau", "For\u00b7tu\u00b7na", "f\u00fchrt", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Amor ist der zweite Mann.", "tokens": ["A\u00b7mor", "ist", "der", "zwei\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Auch die Musen k\u00f6nnt ihr sehn", "tokens": ["Auch", "die", "Mu\u00b7sen", "k\u00f6nnt", "ihr", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "All' in Reiseschuhen gehn.", "tokens": ["All'", "in", "Rei\u00b7se\u00b7schu\u00b7hen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Mars f\u00e4hrt auf Aprillenwetter,", "tokens": ["Mars", "f\u00e4hrt", "auf", "Ap\u00b7ril\u00b7len\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Laune hei\u00dft des Ruhmes Vetter.", "tokens": ["Lau\u00b7ne", "hei\u00dft", "des", "Ruh\u00b7mes", "Vet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Liebes Herz, so zieh' nur mit,", "tokens": ["Lie\u00b7bes", "Herz", ",", "so", "zieh'", "nur", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Halte wacker Schritt und Tritt!", "tokens": ["Hal\u00b7te", "wa\u00b7cker", "Schritt", "und", "Tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.34": {"text": "Heute hier und morgen dort,", "tokens": ["Heu\u00b7te", "hier", "und", "mor\u00b7gen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Und zu Haus an jedem Ort.", "tokens": ["Und", "zu", "Haus", "an", "je\u00b7dem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Regen, Sturm und Sonnenschein,", "tokens": ["Re\u00b7gen", ",", "Sturm", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Rebensaft und Gerstenwein.", "tokens": ["Re\u00b7ben\u00b7saft", "und", "Gers\u00b7ten\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.40": {"text": "Heute blond und morgen braun", "tokens": ["Heu\u00b7te", "blond", "und", "mor\u00b7gen", "braun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Ist mein Sch\u00e4tzchen anzuschaun.", "tokens": ["Ist", "mein", "Sch\u00e4tz\u00b7chen", "an\u00b7zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "Kalt und warm und schlicht und kraus,", "tokens": ["Kalt", "und", "warm", "und", "schlicht", "und", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Bienenschwarm und Schneckenhaus.", "tokens": ["Bie\u00b7nen\u00b7schwarm", "und", "Schne\u00b7cken\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.46": {"text": "Heut' hab' ich dies Lied erdacht,", "tokens": ["Heut'", "hab'", "ich", "dies", "Lied", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Morgen wird es ausgelacht.", "tokens": ["Mor\u00b7gen", "wird", "es", "aus\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Wandern, wandern!", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}