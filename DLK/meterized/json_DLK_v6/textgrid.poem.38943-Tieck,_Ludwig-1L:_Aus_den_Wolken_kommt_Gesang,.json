{"textgrid.poem.38943": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus den Wolken kommt Gesang,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus den Wolken kommt Gesang,", "tokens": ["Aus", "den", "Wol\u00b7ken", "kommt", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt aus tiefem Wald hervor,", "tokens": ["Dringt", "aus", "tie\u00b7fem", "Wald", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der V\u00f6gel Wechselchor,", "tokens": ["Ist", "der", "V\u00f6\u00b7gel", "Wech\u00b7selc\u00b7hor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6net nach der Bergeshang. \u2013", "tokens": ["T\u00f6\u00b7net", "nach", "der", "Ber\u00b7ges\u00b7hang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeden Fr\u00fchling singt es wieder, \u2013", "tokens": ["Je\u00b7den", "Fr\u00fch\u00b7ling", "singt", "es", "wie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was verk\u00fcnden ihre Lieder?", "tokens": ["Was", "ver\u00b7k\u00fcn\u00b7den", "ih\u00b7re", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sagt, was will der Kukuk sagen,", "tokens": ["Sagt", ",", "was", "will", "der", "Ku\u00b7kuk", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er durch die Schatten schreit,", "tokens": ["Da\u00df", "er", "durch", "die", "Schat\u00b7ten", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in sch\u00f6nen Sommertagen", "tokens": ["Und", "in", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein so simples Lied erneut?", "tokens": ["Sein", "so", "sim\u00b7ples", "Lied", "er\u00b7neut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er mit Prophetenschnabel", "tokens": ["Da\u00df", "er", "mit", "Pro\u00b7phe\u00b7ten\u00b7schna\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Jahre z\u00e4hlt, ist Fabel.", "tokens": ["Uns\u00b7re", "Jah\u00b7re", "z\u00e4hlt", ",", "ist", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nacht'gall! ringst mit s\u00fc\u00dfen T\u00f6nen", "tokens": ["Nacht'\u00b7gall", "!", "ringst", "mit", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Baumbewachsnen Bach,", "tokens": ["An", "dem", "Baum\u00b7be\u00b7wachs\u00b7nen", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seufzend horchen alle Sch\u00f6nen,", "tokens": ["Seuf\u00b7zend", "hor\u00b7chen", "al\u00b7le", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Echo spricht dir klagend nach,", "tokens": ["E\u00b7cho", "spricht", "dir", "kla\u00b7gend", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcner pranget jede Pflanze,", "tokens": ["Gr\u00fc\u00b7ner", "pran\u00b7get", "je\u00b7de", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie umflossen von dem Glanze.", "tokens": ["Wie", "um\u00b7flos\u00b7sen", "von", "dem", "Glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber wenn nun einer k\u00e4me,", "tokens": ["A\u00b7ber", "wenn", "nun", "ei\u00b7ner", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4te h\u00f6flich vor dich hin,", "tokens": ["Tr\u00e4\u00b7te", "h\u00f6f\u00b7lich", "vor", "dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er dich zwar gern vern\u00e4hme,", "tokens": ["Da\u00df", "er", "dich", "zwar", "gern", "ver\u00b7n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber m\u00f6chtest dich bem\u00fchn,", "tokens": ["A\u00b7ber", "m\u00f6ch\u00b7test", "dich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was du singend wollst beginnen,", "tokens": ["Was", "du", "sin\u00b7gend", "wollst", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm in Prosa zu versinnen.", "tokens": ["Ihm", "in", "Pro\u00b7sa", "zu", "ver\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wollt' Nachtigall auch h\u00f6flich sein", "tokens": ["Wollt'", "Nach\u00b7ti\u00b7gall", "auch", "h\u00f6f\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm Antwort anzuworten,", "tokens": ["Ihm", "Ant\u00b7wort", "an\u00b7zu\u00b7wor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "K\u00e4m' wieder in den Gesang hinein", "tokens": ["K\u00e4m'", "wie\u00b7der", "in", "den", "Ge\u00b7sang", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "APZR"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "In Noten von allen Sorten,", "tokens": ["In", "No\u00b7ten", "von", "al\u00b7len", "Sor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und blitzerte mit s\u00fc\u00dfer Gewalt", "tokens": ["Und", "blit\u00b7zer\u00b7te", "mit", "s\u00fc\u00b7\u00dfer", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Das Lied durch den dunkelgr\u00fcnen Wald.", "tokens": ["Das", "Lied", "durch", "den", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "So Erd' und Himmel mit Farbengepr\u00e4ng,", "tokens": ["So", "Erd'", "und", "Him\u00b7mel", "mit", "Far\u00b7ben\u00b7ge\u00b7pr\u00e4ng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was wollen sie wohl bedeuten?", "tokens": ["Was", "wol\u00b7len", "sie", "wohl", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das bunte Gewimmel von Tongemeng,", "tokens": ["Das", "bun\u00b7te", "Ge\u00b7wim\u00b7mel", "von", "Ton\u00b7ge\u00b7meng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was spricht's zu vern\u00fcnftigen Leuten?", "tokens": ["Was", "spricht's", "zu", "ver\u00b7n\u00fcnf\u00b7ti\u00b7gen", "Leu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ist alles nur leider sein selbst willen da,", "tokens": ["Ist", "al\u00b7les", "nur", "lei\u00b7der", "sein", "selbst", "wil\u00b7len", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "PPOSAT", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Kr\u00e4ht nach unserm Sinne weder Hund noch Hahn.", "tokens": ["Kr\u00e4ht", "nach", "un\u00b7serm", "Sin\u00b7ne", "we\u00b7der", "Hund", "noch", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}