{"dta.poem.20739": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Klagen  \n \u00fcber eine gestorbene Rose  \n an meinen Freund  \n R.  Verstorbenen Apotheker und Naturforscher hierselbst,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den gr\u00fcbchenl\u00e4chelnden J\u00fcnks besang neulich der               ", "tokens": ["Den", "gr\u00fcb\u00b7chen\u00b7l\u00e4\u00b7cheln\u00b7den", "J\u00fcnks", "be\u00b7sang", "neu\u00b7lich", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kleine Birtyll, wegen des anmuthigen Getr\u00e4nkes", "tokens": ["Klei\u00b7ne", "Bir\u00b7tyll", ",", "we\u00b7gen", "des", "an\u00b7mut\u00b7hi\u00b7gen", "Ge\u00b7tr\u00e4n\u00b7kes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Der Cacaobohne mit Zucker gemischt; ich singe", "tokens": ["Der", "Ca\u00b7cao\u00b7boh\u00b7ne", "mit", "Zu\u00b7cker", "ge\u00b7mischt", ";", "ich", "sin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$.", "PPER", "VVFIN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dir dar\u00fcber kein Lied, ich schmeichle Dir nicht", "tokens": ["Dir", "da\u00b7r\u00fc\u00b7ber", "kein", "Lied", ",", "ich", "schmeich\u00b7le", "Dir", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "PIAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wegen Deiner Geschicklichkeit, denn unsern Halbg\u00f6ttern", "tokens": ["We\u00b7gen", "Dei\u00b7ner", "Ge\u00b7schick\u00b7lich\u00b7keit", ",", "denn", "un\u00b7sern", "Halb\u00b7g\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und G\u00f6ttinnen sind jene T\u00e4felchen bekannt, welche", "tokens": ["Und", "G\u00f6t\u00b7tin\u00b7nen", "sind", "je\u00b7ne", "T\u00e4\u00b7fel\u00b7chen", "be\u00b7kannt", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VAFIN", "PDAT", "NN", "PTKVZ", "$,", "PRELS"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du f\u00fcr ihren unterscheidenden Gaumen zubereitest,", "tokens": ["Du", "f\u00fcr", "ih\u00b7ren", "un\u00b7ter\u00b7schei\u00b7den\u00b7den", "Gau\u00b7men", "zu\u00b7be\u00b7rei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ihr freundlicher Lobspruch beim Morgentrinktische", "tokens": ["Ihr", "freund\u00b7li\u00b7cher", "Lob\u00b7spruch", "beim", "Mor\u00b7gen\u00b7trink\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Gehet weit \u00fcber meinen Gesang;", "tokens": ["Ge\u00b7het", "weit", "\u00fc\u00b7ber", "mei\u00b7nen", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Aber fragen will ich dich ehrlicher, gef\u00e4lliger Freund,", "tokens": ["A\u00b7ber", "fra\u00b7gen", "will", "ich", "dich", "ehr\u00b7li\u00b7cher", ",", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "hast", "tokens": ["hast"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Du keinen aromatischen Blumengeist unter den", "tokens": ["Du", "kei\u00b7nen", "a\u00b7rom\u00b7at\u00b7i\u00b7schen", "Blu\u00b7men\u00b7geist", "un\u00b7ter", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "ADJA", "NN", "APPR", "ART"], "meter": "-+---+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Wundern deiner Kunst? Keinen, der meiner", "tokens": ["Wun\u00b7dern", "dei\u00b7ner", "Kunst", "?", "Kei\u00b7nen", ",", "der", "mei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "NN", "$,", "PRELS", "PPOSAT"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Gestorbenen Rose ihren Geruch wieder geben kann?", "tokens": ["Ge\u00b7stor\u00b7be\u00b7nen", "Ro\u00b7se", "ih\u00b7ren", "Ge\u00b7ruch", "wie\u00b7der", "ge\u00b7ben", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "O sie war sch\u00f6n, wie die", "tokens": ["O", "sie", "war", "sch\u00f6n", ",", "wie", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "ADJD", "$,", "PWAV", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Innge Friedrika von Preu\u00dfen;", "tokens": ["Inn\u00b7ge", "Frie\u00b7dri\u00b7ka", "von", "Preu\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sie war eine Knospe. Milon brachte sie", "tokens": ["Sie", "war", "ei\u00b7ne", "Knos\u00b7pe", ".", "Mi\u00b7lon", "brach\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "NE", "VVFIN", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Mir frisch abgebrochen vom Rosenstocke, welchen", "tokens": ["Mir", "frisch", "ab\u00b7ge\u00b7bro\u00b7chen", "vom", "Ro\u00b7sen\u00b7sto\u00b7cke", ",", "wel\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJD", "VVPP", "APPRART", "NN", "$,", "PWAT"], "meter": "-++-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Er selber gepflanzet, gepflegt und durch", "tokens": ["Er", "sel\u00b7ber", "ge\u00b7pflan\u00b7zet", ",", "ge\u00b7pflegt", "und", "durch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVPP", "KON", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Gottesf\u00fcrchtige Fr\u00f6mmigkeit vor sch\u00e4dliche Taue", "tokens": ["Got\u00b7tes\u00b7f\u00fcrch\u00b7ti\u00b7ge", "Fr\u00f6m\u00b7mig\u00b7keit", "vor", "sch\u00e4d\u00b7li\u00b7che", "Tau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.21": {"text": "Bewahret hat. Ich k\u00fc\u00dfte die Knospe,", "tokens": ["Be\u00b7wah\u00b7ret", "hat", ".", "Ich", "k\u00fc\u00df\u00b7te", "die", "Knos\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Die Milon mir gab, noch unaufgef\u00e4chelt vom", "tokens": ["Die", "Mi\u00b7lon", "mir", "gab", ",", "noch", "un\u00b7auf\u00b7ge\u00b7f\u00e4\u00b7chelt", "vom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "APPRART"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "L\u00fcsternen Zephir, noch festverschlossen, wie", "tokens": ["L\u00fcs\u00b7ter\u00b7nen", "Ze\u00b7phir", ",", "noch", "fest\u00b7ver\u00b7schlos\u00b7sen", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VVINF", "$,", "PWAV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.24": {"text": "Ein gl\u00fchendes Herz, welches in sich das Geheimni\u00df", "tokens": ["Ein", "gl\u00fc\u00b7hen\u00b7des", "Herz", ",", "wel\u00b7ches", "in", "sich", "das", "Ge\u00b7heim\u00b7ni\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "ART", "NN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Der Liebe verbirgt. Ich erhielt ihr welkendes", "tokens": ["Der", "Lie\u00b7be", "ver\u00b7birgt", ".", "Ich", "er\u00b7hielt", "ihr", "wel\u00b7ken\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Leben acht Tage lang in einem kleinen Gef\u00e4\u00dfe", "tokens": ["Le\u00b7ben", "acht", "Ta\u00b7ge", "lang", "in", "ei\u00b7nem", "klei\u00b7nen", "Ge\u00b7f\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "CARD", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.27": {"text": "Mit Wasser, wie deine labende Tr\u00e4nke,", "tokens": ["Mit", "Was\u00b7ser", ",", "wie", "dei\u00b7ne", "la\u00b7ben\u00b7de", "Tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Deine Cordialgew\u00e4sser den sterbenden", "tokens": ["Dei\u00b7ne", "Cor\u00b7di\u00b7al\u00b7ge\u00b7w\u00e4s\u00b7ser", "den", "ster\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Kranken aufhalten, so erhielt ich die Rose;", "tokens": ["Kran\u00b7ken", "auf\u00b7hal\u00b7ten", ",", "so", "er\u00b7hielt", "ich", "die", "Ro\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Denn ich gab ihr frisches Wasser, wenn die", "tokens": ["Denn", "ich", "gab", "ihr", "fri\u00b7sches", "Was\u00b7ser", ",", "wenn", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ART"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.31": {"text": "Morgenr\u00f6the mich weckte, und wenn der Sonnen.", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7the", "mich", "weck\u00b7te", ",", "und", "wenn", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "KON", "KOUS", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Wagen hinter dem schattichten Hain im", "tokens": ["Wa\u00b7gen", "hin\u00b7ter", "dem", "schat\u00b7tich\u00b7ten", "Hain", "im"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPRART"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Silbernen Schoo\u00df der Najade zu sinken", "tokens": ["Sil\u00b7ber\u00b7nen", "Schoo\u00df", "der", "Na\u00b7ja\u00b7de", "zu", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.34": {"text": "Schien. \u2014 Am neunten Tage legt ich traurig", "tokens": ["Schien", ".", "Am", "neun\u00b7ten", "Ta\u00b7ge", "legt", "ich", "trau\u00b7rig"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "$(", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Meine geliebte Verblichne in eine Grabkiste", "tokens": ["Mei\u00b7ne", "ge\u00b7lieb\u00b7te", "Ver\u00b7blich\u00b7ne", "in", "ei\u00b7ne", "Grab\u00b7kis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+--+--+-++-", "measure": "dactylic.tri.plus"}, "line.36": {"text": "Von Silber und Perlmutter. Ach! Ihre Farbe", "tokens": ["Von", "Sil\u00b7ber", "und", "Perl\u00b7mut\u00b7ter", ".", "Ach", "!", "Ih\u00b7re", "Far\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ITJ", "$.", "PPOSAT", "NN"], "meter": "-+--++-++-+-", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "gleichet", "tokens": ["glei\u00b7chet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Der Farbe des Angesichts eines gestorbenen M\u00e4dchens,", "tokens": ["Der", "Far\u00b7be", "des", "An\u00b7ge\u00b7sichts", "ei\u00b7nes", "ge\u00b7stor\u00b7be\u00b7nen", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.39": {"text": "Das \u00fcber und \u00fcber bis auf die Lippen \u00e4hrengelb", "tokens": ["Das", "\u00fc\u00b7ber", "und", "\u00fc\u00b7ber", "bis", "auf", "die", "Lip\u00b7pen", "\u00e4h\u00b7ren\u00b7gelb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "KON", "APPR", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Wird. Ich \u00f6ffne die Kiste dreimal des Tages, und", "tokens": ["Wird", ".", "Ich", "\u00f6ff\u00b7ne", "die", "Kis\u00b7te", "drei\u00b7mal", "des", "Ta\u00b7ges", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,", "KON"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Klage die Rose. Sie starb! ihre s\u00fc\u00dfen Ger\u00fcche", "tokens": ["Kla\u00b7ge", "die", "Ro\u00b7se", ".", "Sie", "starb", "!", "ih\u00b7re", "s\u00fc\u00b7\u00dfen", "Ge\u00b7r\u00fc\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.42": {"text": "Verflogen! aber nur eines von ihren Bl\u00e4ttern", "tokens": ["Ver\u00b7flo\u00b7gen", "!", "a\u00b7ber", "nur", "ei\u00b7nes", "von", "ih\u00b7ren", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ADV", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Verlor sie, Zephirus wollt\u2019 es aufhaschen, ich", "tokens": ["Ver\u00b7lor", "sie", ",", "Ze\u00b7phi\u00b7rus", "wollt'", "es", "auf\u00b7ha\u00b7schen", ",", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "NE", "VMFIN", "PPER", "VVINF", "$,", "PPER"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Nahms ihm wieder mit einem Kusse des klagenden", "tokens": ["Nahms", "ihm", "wie\u00b7der", "mit", "ei\u00b7nem", "Kus\u00b7se", "des", "kla\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.45": {"text": "Mundes,", "tokens": ["Mun\u00b7des", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.46": {"text": "Ich weinete Thr\u00e4nen des z\u00e4rtlichsten", "tokens": ["Ich", "wei\u00b7ne\u00b7te", "Thr\u00e4\u00b7nen", "des", "z\u00e4rt\u00b7lichs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+--+--++-", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "Kummers auf die gestorbene Rose; sie verlor", "tokens": ["Kum\u00b7mers", "auf", "die", "ge\u00b7stor\u00b7be\u00b7ne", "Ro\u00b7se", ";", "sie", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+--+