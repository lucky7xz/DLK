{"dta.poem.7768": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Tr\u00f6pfchen, das zur\u00fcck blieb in der Opferschale;", "tokens": ["Ein", "Tr\u00f6pf\u00b7chen", ",", "das", "zu\u00b7r\u00fcck", "blieb", "in", "der", "Op\u00b7fer\u00b7scha\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein K\u00f6rnchen Rei\u00dfes unverzehrt beim Opfermale;", "tokens": ["Ein", "K\u00f6rn\u00b7chen", "Rei\u00b7\u00dfes", "un\u00b7ver\u00b7zehrt", "beim", "Op\u00b7fer\u00b7ma\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein St\u00e4ubchen Aschen, aufbewahrt vom Opferfeuer;", "tokens": ["Ein", "St\u00e4ub\u00b7chen", "A\u00b7schen", ",", "auf\u00b7be\u00b7wahrt", "vom", "Op\u00b7fer\u00b7feu\u00b7er", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die welke Blume, die gedient zur Opfersteuer:", "tokens": ["Die", "wel\u00b7ke", "Blu\u00b7me", ",", "die", "ge\u00b7dient", "zur", "Op\u00b7fer\u00b7steu\u00b7er", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit hoher Andacht nimm, mit tiefer Ehrfurcht du", "tokens": ["Mit", "ho\u00b7her", "An\u00b7dacht", "nimm", ",", "mit", "tie\u00b7fer", "Ehr\u00b7furcht", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dergleichen, was dir gibt dein Lehrer, dein ", "tokens": ["Derg\u00b7lei\u00b7chen", ",", "was", "dir", "gibt", "dein", "Leh\u00b7rer", ",", "dein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht unter sch\u00e4tze du's, nein \u00fcber den Geschenken,", "tokens": ["Nicht", "un\u00b7ter", "sch\u00e4t\u00b7ze", "du's", ",", "nein", "\u00fc\u00b7ber", "den", "Ge\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "VVFIN", "PIS", "$,", "PTKANT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ein Verliebter nimmt zu Liebesangedenken;", "tokens": ["Die", "ein", "Ver\u00b7lieb\u00b7ter", "nimmt", "zu", "Lie\u00b7be\u00b7san\u00b7ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In welchen Liebe glaubt das H\u00f6chste zu erbeuten,", "tokens": ["In", "wel\u00b7chen", "Lie\u00b7be", "glaubt", "das", "H\u00f6chs\u00b7te", "zu", "er\u00b7beu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch das nicht was sie sind, durch das was sie bedeuten.", "tokens": ["Durch", "das", "nicht", "was", "sie", "sind", ",", "durch", "das", "was", "sie", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PTKNEG", "PWS", "PPER", "VAFIN", "$,", "APPR", "ART", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}