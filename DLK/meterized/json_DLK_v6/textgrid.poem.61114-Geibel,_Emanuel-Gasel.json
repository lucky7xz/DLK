{"textgrid.poem.61114": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gasel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Zeit, wenn der Fr\u00fchling die Glut der Rosen entfacht in Athen,", "tokens": ["Zur", "Zeit", ",", "wenn", "der", "Fr\u00fch\u00b7ling", "die", "Glut", "der", "Ro\u00b7sen", "ent\u00b7facht", "in", "A\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie d\u00e4mmert so lieblich als dann die duftige Nacht in Athen!", "tokens": ["Wie", "d\u00e4m\u00b7mert", "so", "lieb\u00b7lich", "als", "dann", "die", "duf\u00b7ti\u00b7ge", "Nacht", "in", "A\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "KOKOM", "ADV", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hoch leuchtet der Mond und bescheint Zypressen und Palmen umher", "tokens": ["Hoch", "leuch\u00b7tet", "der", "Mond", "und", "be\u00b7scheint", "Zyp\u00b7res\u00b7sen", "und", "Pal\u00b7men", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN", "NE", "KON", "NN", "PTKVZ"], "meter": "-+--+---+---+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und marmornen Tempelges\u00e4uls versinkende Pracht in Athen.", "tokens": ["Und", "mar\u00b7mor\u00b7nen", "Tem\u00b7pel\u00b7ge\u00b7s\u00e4uls", "ver\u00b7sin\u00b7ken\u00b7de", "Pracht", "in", "A\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wir aber bekr\u00e4nzen das Haupt und f\u00fcllen den Becher mit Wein,", "tokens": ["Wir", "a\u00b7ber", "be\u00b7kr\u00e4n\u00b7zen", "das", "Haupt", "und", "f\u00fcl\u00b7len", "den", "Be\u00b7cher", "mit", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Gedenkend, wie Sokrates einst die N\u00e4chte verbracht in Athen.", "tokens": ["Ge\u00b7den\u00b7kend", ",", "wie", "Sok\u00b7ra\u00b7tes", "einst", "die", "N\u00e4ch\u00b7te", "ver\u00b7bracht", "in", "A\u00b7then", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "NE", "ADV", "ART", "NN", "VVPP", "APPR", "NE", "$."], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Von Lieb' entspinnt sich Gespr\u00e4ch; denn ob auch Pallas die Burg", "tokens": ["Von", "Lieb'", "ent\u00b7spinnt", "sich", "Ge\u00b7spr\u00e4ch", ";", "denn", "ob", "auch", "Pal\u00b7las", "die", "Burg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "NN", "$.", "KON", "KOUS", "ADV", "NN", "ART", "NN"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Beherrschen mag, Eros der Gott \u00fcbt selige Macht in Athen.", "tokens": ["Be\u00b7herr\u00b7schen", "mag", ",", "E\u00b7ros", "der", "Gott", "\u00fcbt", "se\u00b7li\u00b7ge", "Macht", "in", "A\u00b7then", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "NE", "ART", "NN", "VVFIN", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Zur Rede gesellt sich Musik, leicht sind die Gitarren gestimmt,", "tokens": ["Zur", "Re\u00b7de", "ge\u00b7sellt", "sich", "Mu\u00b7sik", ",", "leicht", "sind", "die", "Gi\u00b7tar\u00b7ren", "ge\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "PRF", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Leicht regt sich des Wechselgesangs melodische Schlacht in Athen.", "tokens": ["Leicht", "regt", "sich", "des", "Wech\u00b7sel\u00b7ge\u00b7sangs", "me\u00b7lo\u00b7di\u00b7sche", "Schlacht", "in", "A\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Da webt manch klassisches Wort, manch leuchtender Name sich ein,", "tokens": ["Da", "webt", "manch", "klas\u00b7si\u00b7sches", "Wort", ",", "manch", "leuch\u00b7ten\u00b7der", "Na\u00b7me", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Denn gro\u00dfer vergangener Zeit Erinnerung wacht in Athen.", "tokens": ["Denn", "gro\u00b7\u00dfer", "ver\u00b7gan\u00b7ge\u00b7ner", "Zeit", "E\u00b7rin\u00b7ne\u00b7rung", "wacht", "in", "A\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Und k\u00fchner erbrauset das Lied; wir spenden aus vollem Pokal", "tokens": ["Und", "k\u00fch\u00b7ner", "er\u00b7brau\u00b7set", "das", "Lied", ";", "wir", "spen\u00b7den", "aus", "vol\u00b7lem", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Den Herrlichen, die einst gek\u00e4mpft, gesungen, gedacht in Athen.", "tokens": ["Den", "Herr\u00b7li\u00b7chen", ",", "die", "einst", "ge\u00b7k\u00e4mpft", ",", "ge\u00b7sun\u00b7gen", ",", "ge\u00b7dacht", "in", "A\u00b7then", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "VVPP", "$,", "VVPP", "APPR", "NE", "$."], "meter": "-+---+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}