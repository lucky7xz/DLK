{"dta.poem.8187": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "O bleibe treu den Todten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Die lebend du betr\u00fcbt;", "tokens": ["Die", "le\u00b7bend", "du", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O bleibe treu den Todten,", "tokens": ["O", "blei\u00b7be", "treu", "den", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die lebend dich geliebt!", "tokens": ["Die", "le\u00b7bend", "dich", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie starben, doch sie blieben", "tokens": ["Sie", "star\u00b7ben", ",", "doch", "sie", "blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Erden wesenlos,", "tokens": ["Auf", "Er\u00b7den", "we\u00b7sen\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis allen ihren Lieben", "tokens": ["Bis", "al\u00b7len", "ih\u00b7ren", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Tod die Augen schlo\u00df.", "tokens": ["Der", "Tod", "die", "Au\u00b7gen", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Indessen du dich herzlich", "tokens": ["In\u00b7des\u00b7sen", "du", "dich", "herz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Lebenslust versenkst,", "tokens": ["In", "Le\u00b7bens\u00b7lust", "ver\u00b7senkst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sehnen sie sich schmerzlich,", "tokens": ["Wie", "seh\u00b7nen", "sie", "sich", "schmerz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihrer du gedenkst!", "tokens": ["Da\u00df", "ih\u00b7rer", "du", "ge\u00b7denkst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie nahen dir in Liebe,", "tokens": ["Sie", "na\u00b7hen", "dir", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allein du f\u00fchlst es nicht;", "tokens": ["Al\u00b7lein", "du", "f\u00fchlst", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schaun dich an so tr\u00fcbe,", "tokens": ["Sie", "schaun", "dich", "an", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du aber siehst es nicht.", "tokens": ["Du", "a\u00b7ber", "siehst", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Br\u00fccke ist zerfallen;", "tokens": ["Die", "Br\u00fc\u00b7cke", "ist", "zer\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun m\u00fchen sie sich bang,", "tokens": ["Nun", "m\u00fc\u00b7hen", "sie", "sich", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Liebeswort zu lallen,", "tokens": ["Ein", "Lie\u00b7bes\u00b7wort", "zu", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das nie hin\u00fcber drang.", "tokens": ["Das", "nie", "hin\u00b7\u00fc\u00b7ber", "drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In ihrem Schattenleben", "tokens": ["In", "ih\u00b7rem", "Schat\u00b7ten\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Qu\u00e4lt Eins sie gar zu sehr:", "tokens": ["Qu\u00e4lt", "Eins", "sie", "gar", "zu", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Herz will dir vergeben,", "tokens": ["Ihr", "Herz", "will", "dir", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Mund vermag's nicht mehr.", "tokens": ["Ihr", "Mund", "ver\u00b7mag's", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O bleibe treu den Todten,", "tokens": ["O", "blei\u00b7be", "treu", "den", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die lebend du betr\u00fcbt;", "tokens": ["Die", "le\u00b7bend", "du", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O bleibe treu den Todten,", "tokens": ["O", "blei\u00b7be", "treu", "den", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die lebend dich geliebt!", "tokens": ["Die", "le\u00b7bend", "dich", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}