{"dta.poem.6681": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sehr vermag ein s\u00fcsses Musiciren,", "tokens": ["Wie", "sehr", "ver\u00b7mag", "ein", "s\u00fcs\u00b7ses", "Mu\u00b7si\u00b7ci\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele, die vergn\u00fcgt und ruhig ist, zu r\u00fchren!", "tokens": ["Die", "See\u00b7le", ",", "die", "ver\u00b7gn\u00fcgt", "und", "ru\u00b7hig", "ist", ",", "zu", "r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "KON", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn eine Menge, d\u00fcnner Sehnen,", "tokens": ["Wenn", "ei\u00b7ne", "Men\u00b7ge", ",", "d\u00fcn\u00b7ner", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wir auf Instrumente dehnen,", "tokens": ["So", "wir", "auf", "Inst\u00b7ru\u00b7men\u00b7te", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Solch\u2019 Rollen, Melodie und Fugen uns gebiert,", "tokens": ["Solch'", "Rol\u00b7len", ",", "Me\u00b7lo\u00b7die", "und", "Fu\u00b7gen", "uns", "ge\u00b7biert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch deren liebliches Gemisch wir seyn ger\u00fchrt.", "tokens": ["Durch", "de\u00b7ren", "lieb\u00b7li\u00b7ches", "Ge\u00b7misch", "wir", "seyn", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}