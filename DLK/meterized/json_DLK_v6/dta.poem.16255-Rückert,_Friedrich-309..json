{"dta.poem.16255": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "309.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Muth ist die beste Kraft, zu allem Guten n\u00f6thig,", "tokens": ["Muth", "ist", "die", "bes\u00b7te", "Kraft", ",", "zu", "al\u00b7lem", "Gu\u00b7ten", "n\u00f6\u00b7thig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PIS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und willig sollst du seyn dazu mit Lust erb\u00f6tig.", "tokens": ["Und", "wil\u00b7lig", "sollst", "du", "seyn", "da\u00b7zu", "mit", "Lust", "er\u00b7b\u00f6\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VAINF", "PAV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Muth ist also gut, und besser noch Gutwillig;", "tokens": ["Der", "Muth", "ist", "al\u00b7so", "gut", ",", "und", "bes\u00b7ser", "noch", "Gut\u00b7wil\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wird aus beiden denn das b\u00f6se Wort Muthwillig?", "tokens": ["Wie", "wird", "aus", "bei\u00b7den", "denn", "das", "b\u00f6\u00b7se", "Wort", "Mut\u00b7hwil\u00b7lig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "PIAT", "KON", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du lernst daraus, o Kind, viel Gutes wird zuletzt", "tokens": ["Du", "lernst", "da\u00b7raus", ",", "o", "Kind", ",", "viel", "Gu\u00b7tes", "wird", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "FM", "NN", "$,", "PIAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein B\u00f6ses, wenn man es verkehrt zusammensetzt.", "tokens": ["Ein", "B\u00f6\u00b7ses", ",", "wenn", "man", "es", "ver\u00b7kehrt", "zu\u00b7sam\u00b7men\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein muth'ger Will' ist gut, noch besser will'ger Muth,", "tokens": ["Ein", "muth'\u00b7ger", "Will'", "ist", "gut", ",", "noch", "bes\u00b7ser", "will'\u00b7ger", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Willmuth und Muthwill' ist eine b\u00f6se Brut.", "tokens": ["Doch", "Will\u00b7muth", "und", "Muthwill'", "ist", "ei\u00b7ne", "b\u00f6\u00b7se", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}}}}