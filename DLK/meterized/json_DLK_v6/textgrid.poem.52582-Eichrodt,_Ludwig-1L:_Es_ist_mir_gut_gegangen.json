{"textgrid.poem.52582": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist mir gut gegangen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mir gut gegangen", "tokens": ["Es", "ist", "mir", "gut", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf dieser schlechten Welt,", "tokens": ["Auf", "die\u00b7ser", "schlech\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wurde nicht gehangen,", "tokens": ["Ich", "wur\u00b7de", "nicht", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wurde kaum geprellt", "tokens": ["Ich", "wur\u00b7de", "kaum", "ge\u00b7prellt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um tausend Lebensfreuden,", "tokens": ["Um", "tau\u00b7send", "Le\u00b7bens\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um jede Hoffnung schier,", "tokens": ["Um", "je\u00b7de", "Hoff\u00b7nung", "schier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Blieb immer \u00fcbrig mir.", "tokens": ["Blieb", "im\u00b7mer", "\u00fcb\u00b7rig", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich brachten b\u00f6se Menschen", "tokens": ["Mich", "brach\u00b7ten", "b\u00f6\u00b7se", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Nichts etwa durch Neid,", "tokens": ["Um", "Nichts", "et\u00b7wa", "durch", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Chinesen und Tschetschenschen", "tokens": ["Chi\u00b7ne\u00b7sen", "und", "Tschet\u00b7schen\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie in Verlegenheit;", "tokens": ["Nie", "in", "Ver\u00b7le\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich haben keine Schlangen", "tokens": ["Mich", "ha\u00b7ben", "kei\u00b7ne", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Reue je gequ\u00e4lt,", "tokens": ["Der", "Reu\u00b7e", "je", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es ist mir gut gegangen", "tokens": ["Es", "ist", "mir", "gut", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf dieser schlechten Welt.", "tokens": ["Auf", "die\u00b7ser", "schlech\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sollt ich ein Ver\u00e4chter", "tokens": ["Wie", "sollt", "ich", "ein", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des lieben Lebens sein?", "tokens": ["Des", "lie\u00b7ben", "Le\u00b7bens", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So Vielen geht es schlechter", "tokens": ["So", "Vie\u00b7len", "geht", "es", "schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch selbstgeschaffne Pein.", "tokens": ["Durch", "selbst\u00b7ge\u00b7schaff\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Ich lasse mich betr\u00fcgen,", "tokens": ["Ich", "las\u00b7se", "mich", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wies Andern wohlgef\u00e4llt,", "tokens": ["Wies", "An\u00b7dern", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und pfeife mit Vergn\u00fcgen", "tokens": ["Und", "pfei\u00b7fe", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Liedchen durch die Welt.", "tokens": ["Mein", "Lied\u00b7chen", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}