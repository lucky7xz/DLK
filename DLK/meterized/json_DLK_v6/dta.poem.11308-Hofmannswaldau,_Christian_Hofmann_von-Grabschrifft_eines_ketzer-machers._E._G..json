{"dta.poem.11308": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines ketzer-machers.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein flegel, der den schwarm der falschen ketzer schlug,", "tokens": ["Ein", "fle\u00b7gel", ",", "der", "den", "schwarm", "der", "fal\u00b7schen", "ket\u00b7zer", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein pfeiler, der den bau des himmels auf sich trug,", "tokens": ["Ein", "pfei\u00b7ler", ",", "der", "den", "bau", "des", "him\u00b7mels", "auf", "sich", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein helm der elerisey, der kirchen pallisade,", "tokens": ["Ein", "helm", "der", "e\u00b7le\u00b7ri\u00b7sey", ",", "der", "kir\u00b7chen", "pal\u00b7li\u00b7sa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "Der pfaffen trommel-spiel, der thorheit retirade,", "tokens": ["Der", "pfaf\u00b7fen", "trom\u00b7mel\u00b7spiel", ",", "der", "thor\u00b7heit", "re\u00b7ti\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der schreiber corporal, des papstes stecken-knecht,", "tokens": ["Der", "schrei\u00b7ber", "cor\u00b7po\u00b7ral", ",", "des", "paps\u00b7tes", "ste\u00b7cken\u00b7knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein zelt, wo neid und ha\u00df, und aberglauben zecht:", "tokens": ["Ein", "zelt", ",", "wo", "neid", "und", "ha\u00df", ",", "und", "a\u00b7berg\u00b7lau\u00b7ben", "zecht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "PWAV", "NN", "KON", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des teufels tummel-platz, der s\u00fcnder marquetender,", "tokens": ["Des", "teu\u00b7fels", "tum\u00b7mel\u00b7platz", ",", "der", "s\u00fcn\u00b7der", "mar\u00b7que\u00b7ten\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der bo\u00dfheit freund und feind, ein sacrament-verschwender,", "tokens": ["Der", "bo\u00df\u00b7heit", "freund", "und", "feind", ",", "ein", "sa\u00b7cra\u00b7ment\u00b7ver\u00b7schwen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der faulen prediger geliebtes pulver-horn,", "tokens": ["Der", "fau\u00b7len", "pre\u00b7di\u00b7ger", "ge\u00b7lieb\u00b7tes", "pul\u00b7ver\u00b7horn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der eyfrer general, der m\u00fcden z\u00e4ncker sporn,", "tokens": ["Der", "ey\u00b7frer", "ge\u00b7ne\u00b7ral", ",", "der", "m\u00fc\u00b7den", "z\u00e4n\u00b7cker", "sporn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schweigt itzt und ruht allhier; Zum deutlichen exempel,", "tokens": ["Schweigt", "itzt", "und", "ruht", "all\u00b7hier", ";", "Zum", "deut\u00b7li\u00b7chen", "ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ADV", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Da\u00df uns Jrenens schlo\u00df, der g\u00f6ldne friedens-tempel,", "tokens": ["Da\u00df", "uns", "Jre\u00b7nens", "schlo\u00df", ",", "der", "g\u00f6ld\u00b7ne", "frie\u00b7dens\u00b7tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Offt mitten in sich schlie\u00dft, wenn man am h\u00e4rtsten k\u00e4mpfft.", "tokens": ["Offt", "mit\u00b7ten", "in", "sich", "schlie\u00dft", ",", "wenn", "man", "am", "h\u00e4rts\u00b7ten", "k\u00e4mpfft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PRF", "VVFIN", "$,", "KOUS", "PIS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er disputirt nicht mehr, die hitz ist gantz ged\u00e4mpfft;", "tokens": ["Er", "dis\u00b7pu\u00b7tirt", "nicht", "mehr", ",", "die", "hitz", "ist", "gantz", "ge\u00b7d\u00e4mpfft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es hat sein m\u00fcder geist den frieden angenommen,", "tokens": ["Es", "hat", "sein", "m\u00fc\u00b7der", "geist", "den", "frie\u00b7den", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nachdem er unverhofft in ketzer-himmel kommen.", "tokens": ["Nach\u00b7dem", "er", "un\u00b7ver\u00b7hofft", "in", "ket\u00b7zer\u00b7him\u00b7mel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}