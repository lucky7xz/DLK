{"textgrid.poem.41358": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Fledermaus und die zwo Wiesel", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam die Fledermaus in einer Wiesel Loch;", "tokens": ["Es", "kam", "die", "Fle\u00b7der\u00b7maus", "in", "ei\u00b7ner", "Wie\u00b7sel", "Loch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die war den M\u00e4usen feind, und sprach: Wie darfst du doch,", "tokens": ["Die", "war", "den", "M\u00e4u\u00b7sen", "feind", ",", "und", "sprach", ":", "Wie", "darfst", "du", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "KON", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der M\u00e4use Mi\u00dfgeburt! dich meinen Augen weisen?", "tokens": ["Der", "M\u00e4u\u00b7se", "Mi\u00df\u00b7ge\u00b7burt", "!", "dich", "mei\u00b7nen", "Au\u00b7gen", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiewol du k\u00f6mmst mir recht; ich wollte so schon speisen.", "tokens": ["Wie\u00b7wol", "du", "k\u00f6mmst", "mir", "recht", ";", "ich", "woll\u00b7te", "so", "schon", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was? schreit die Fledermaus, ich eine Maus? o nein!", "tokens": ["Was", "?", "schreit", "die", "Fle\u00b7der\u00b7maus", ",", "ich", "ei\u00b7ne", "Maus", "?", "o", "nein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$.", "VVFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "$.", "FM", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein gutes Wieselchen, das m\u00f6gt ihr selbst wol sein;", "tokens": ["Mein", "gu\u00b7tes", "Wie\u00b7sel\u00b7chen", ",", "das", "m\u00f6gt", "ihr", "selbst", "wol", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PDS", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die mich zur Maus gemacht, sind L\u00fcgner oder Feinde;", "tokens": ["Die", "mich", "zur", "Maus", "ge\u00b7macht", ",", "sind", "L\u00fcg\u00b7ner", "o\u00b7der", "Fein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Kater unsers Dorfs sind meine besten Freunde.", "tokens": ["Die", "Ka\u00b7ter", "un\u00b7sers", "Dorfs", "sind", "mei\u00b7ne", "bes\u00b7ten", "Freun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es lebe, was gut maust! Ihr wird zuletzt geglaubt;", "tokens": ["Es", "le\u00b7be", ",", "was", "gut", "maust", "!", "Ihr", "wird", "zu\u00b7letzt", "ge\u00b7glaubt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie rettet unversehrt ihr unerkanntes Haubt;", "tokens": ["Sie", "ret\u00b7tet", "un\u00b7ver\u00b7sehrt", "ihr", "un\u00b7er\u00b7kann\u00b7tes", "Haubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und doch ger\u00e4th sie bald, durch ihr Gesicht betrogen,", "tokens": ["Und", "doch", "ge\u00b7r\u00e4\u00b7th", "sie", "bald", ",", "durch", "ihr", "Ge\u00b7sicht", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "In einer andern Bau; die war der Maus gewogen;", "tokens": ["In", "ei\u00b7ner", "an\u00b7dern", "Bau", ";", "die", "war", "der", "Maus", "ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr waren gegentheils die V\u00f6gel ganz verha\u00dft.", "tokens": ["Ihr", "wa\u00b7ren", "ge\u00b7gen\u00b7theils", "die", "V\u00f6\u00b7gel", "ganz", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie fra\u00df, in Hoffnung, schon den ihr zu schlauen Gast.", "tokens": ["Sie", "fra\u00df", ",", "in", "Hoff\u00b7nung", ",", "schon", "den", "ihr", "zu", "schlau\u00b7en", "Gast", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "$,", "ADV", "ART", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es wei\u00df die Fledermaus ihr gl\u00fccklich zu entgehen.", "tokens": ["Es", "wei\u00df", "die", "Fle\u00b7der\u00b7maus", "ihr", "gl\u00fcck\u00b7lich", "zu", "ent\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wof\u00fcr denn, ruft sie aus, werd' ich jetzt angesehen?", "tokens": ["Wo\u00b7f\u00fcr", "denn", ",", "ruft", "sie", "aus", ",", "werd'", "ich", "jetzt", "an\u00b7ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr einen Vogel? Ich? Du, Wiesel, irrest sehr.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Vo\u00b7gel", "?", "Ich", "?", "Du", ",", "Wie\u00b7sel", ",", "ir\u00b7rest", "sehr", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$.", "PPER", "$.", "PPER", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll dies ein Fittig sein? Kennt man nicht M\u00e4use mehr?", "tokens": ["Soll", "dies", "ein", "Fit\u00b7tig", "sein", "?", "Kennt", "man", "nicht", "M\u00e4u\u00b7se", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "VAINF", "$.", "VVFIN", "PIS", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der erste Donnerschlag zerschmettre hier die Katzen!", "tokens": ["Der", "ers\u00b7te", "Don\u00b7ner\u00b7schlag", "zer\u00b7schmett\u00b7re", "hier", "die", "Kat\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die M\u00e4use leben und die Ratzen!", "tokens": ["Die", "M\u00e4u\u00b7se", "le\u00b7ben", "und", "die", "Rat\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Kluger sieht auf Ort und Zeit,", "tokens": ["Ein", "Klu\u00b7ger", "sieht", "auf", "Ort", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Vorsicht, da\u00df man ihn nicht fange.", "tokens": ["Aus", "Vor\u00b7sicht", ",", "da\u00df", "man", "ihn", "nicht", "fan\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ruft mit gleicher Fertigkeit:", "tokens": ["Er", "ruft", "mit", "glei\u00b7cher", "Fer\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es lebe Wolf! Es lebe Lange!", "tokens": ["Es", "le\u00b7be", "Wolf", "!", "Es", "le\u00b7be", "Lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}