{"textgrid.poem.65028": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht wahr, ihr Alle w\u00fcnscht, wenn einst die Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wahr, ihr Alle w\u00fcnscht, wenn einst die Stunde", "tokens": ["Nicht", "wahr", ",", "ihr", "Al\u00b7le", "w\u00fcnscht", ",", "wenn", "einst", "die", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PPER", "PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gekommen, wo die andern W\u00fcnsche enden,", "tokens": ["Ge\u00b7kom\u00b7men", ",", "wo", "die", "an\u00b7dern", "W\u00fcn\u00b7sche", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In eurer Lieben Mitte zu entsenden", "tokens": ["In", "eu\u00b7rer", "Lie\u00b7ben", "Mit\u00b7te", "zu", "ent\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den letzten Hauch vom todesblassen Munde?", "tokens": ["Den", "letz\u00b7ten", "Hauch", "vom", "to\u00b7des\u00b7blas\u00b7sen", "Mun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verlangt es ", "tokens": ["Ver\u00b7langt", "es"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nach solchen Gl\u00fcckes heilig s\u00fc\u00dfen Spenden,", "tokens": ["Nach", "sol\u00b7chen", "Gl\u00fc\u00b7ckes", "hei\u00b7lig", "s\u00fc\u00b7\u00dfen", "Spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ich mich an den holden Fr\u00fchling wenden,", "tokens": ["Mu\u00df", "ich", "mich", "an", "den", "hol\u00b7den", "Fr\u00fch\u00b7ling", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den einz'gen Freund, mit welchem ich im Bunde.", "tokens": ["Den", "einz'\u00b7gen", "Freund", ",", "mit", "wel\u00b7chem", "ich", "im", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und weil kein and'rer Gru\u00df die dunkle Gruft", "tokens": ["Und", "weil", "kein", "an\u00b7d'\u00b7rer", "Gru\u00df", "die", "dunk\u00b7le", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Liebesschimmer sanft mir wird umf\u00e4rben,", "tokens": ["Mit", "Lie\u00b7bes\u00b7schim\u00b7mer", "sanft", "mir", "wird", "um\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht sein Gru\u00df als Licht und Sang und Duft,", "tokens": ["Wenn", "nicht", "sein", "Gru\u00df", "als", "Licht", "und", "Sang", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "KOUS", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "M\u00f6cht ich mir dieses milde Loos erwerben:", "tokens": ["M\u00f6cht", "ich", "mir", "die\u00b7ses", "mil\u00b7de", "Loos", "er\u00b7wer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Zeit der Bl\u00fchten und der sonn'gen Luft", "tokens": ["Zur", "Zeit", "der", "Bl\u00fch\u00b7ten", "und", "der", "sonn'\u00b7gen", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An sch\u00f6nen Fr\u00fchling's sch\u00f6nstem Tag zu sterben!", "tokens": ["An", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7ling's", "sch\u00f6ns\u00b7tem", "Tag", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}