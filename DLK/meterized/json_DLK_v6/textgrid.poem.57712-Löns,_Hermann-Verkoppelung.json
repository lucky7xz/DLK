{"textgrid.poem.57712": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Verkoppelung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht ein Mann durch das bunte Land;", "tokens": ["Es", "geht", "ein", "Mann", "durch", "das", "bun\u00b7te", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Me\u00dfkette h\u00e4lt er in der Hand.", "tokens": ["Die", "Me\u00df\u00b7ket\u00b7te", "h\u00e4lt", "er", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieht vor sich hin und sieht sich um;", "tokens": ["Sieht", "vor", "sich", "hin", "und", "sieht", "sich", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbhier ist ja alles schief und krumm.\u00ab", "tokens": ["\u00bb", "hier", "ist", "ja", "al\u00b7les", "schief", "und", "krumm", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "PIS", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er mi\u00dft wohl hin und mi\u00dft wohl her;", "tokens": ["Er", "mi\u00dft", "wohl", "hin", "und", "mi\u00dft", "wohl", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbhier geht ja alles kreuz und quer!\u00ab", "tokens": ["\u00bb", "hier", "geht", "ja", "al\u00b7les", "kreuz", "und", "quer", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "PIAT", "NN", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er blickt zum Bach im Tale hin;", "tokens": ["Er", "blickt", "zum", "Bach", "im", "Ta\u00b7le", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdas Buschwerk dort hat keinen Sinn!\u00ab", "tokens": ["\u00bb", "das", "Busc\u00b7hwerk", "dort", "hat", "kei\u00b7nen", "Sinn", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zum Teiche zeigt er mit der Hand;", "tokens": ["Zum", "Tei\u00b7che", "zeigt", "er", "mit", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdas gibt ein St\u00fcck Kartoffelland!\u00ab", "tokens": ["\u00bb", "das", "gibt", "ein", "St\u00fcck", "Kar\u00b7tof\u00b7fel\u00b7land", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Weg macht seinen Augen Pein;", "tokens": ["Der", "Weg", "macht", "sei\u00b7nen", "Au\u00b7gen", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbder mu\u00df fortan schnurgerade sein!\u00ab", "tokens": ["\u00bb", "der", "mu\u00df", "for\u00b7tan", "schnur\u00b7ge\u00b7ra\u00b7de", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VMFIN", "ADV", "ADJD", "VAINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die Hecke d\u00fcnket ihm ein Graus;", "tokens": ["Die", "He\u00b7cke", "d\u00fcn\u00b7ket", "ihm", "ein", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdie roden wir nat\u00fcrlich aus!\u00ab", "tokens": ["\u00bb", "die", "ro\u00b7den", "wir", "na\u00b7t\u00fcr\u00b7lich", "aus", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Wildbirnbaum ist ihm zu krumm;", "tokens": ["Der", "Wild\u00b7birn\u00b7baum", "ist", "ihm", "zu", "krumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbden hauen wir als ersten um!\u00ab", "tokens": ["\u00bb", "den", "hau\u00b7en", "wir", "als", "ers\u00b7ten", "um", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PPER", "KOKOM", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Pappel scheint ihm ohne Zweck;", "tokens": ["Die", "Pap\u00b7pel", "scheint", "ihm", "oh\u00b7ne", "Zweck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdie mu\u00df da selbstverst\u00e4ndlich weg!\u00ab", "tokens": ["\u00bb", "die", "mu\u00df", "da", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", "weg", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und also wird mit vieler Kunst", "tokens": ["Und", "al\u00b7so", "wird", "mit", "vie\u00b7ler", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Feldmark regelrecht verhunzt.", "tokens": ["Die", "Feld\u00b7mark", "re\u00b7gel\u00b7recht", "ver\u00b7hunzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}