{"textgrid.poem.34566": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihres Leibes Wunderschaft", "genre": "verse", "period": "N.A.", "pub_year": 1903, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihres Leibes Wunderschaft", "tokens": ["Ih\u00b7res", "Lei\u00b7bes", "Wun\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hat so v\u00f6llig ihn verschn\u00fcret,", "tokens": ["hat", "so", "v\u00f6l\u00b7lig", "ihn", "ver\u00b7schn\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dass ihn f\u00fcrder keine Kraft", "tokens": ["dass", "ihn", "f\u00fcr\u00b7der", "kei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wieder auf zur Freiheit f\u00fchret.", "tokens": ["wie\u00b7der", "auf", "zur", "Frei\u00b7heit", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lachend hat an seiner Not", "tokens": ["La\u00b7chend", "hat", "an", "sei\u00b7ner", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sie das Frauenherz geweidet,", "tokens": ["sie", "das", "Frau\u00b7en\u00b7herz", "ge\u00b7wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wissend, dass er bis zum Tod", "tokens": ["wis\u00b7send", ",", "dass", "er", "bis", "zum", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "duldet, liebt und f\u00fcr sie leidet.", "tokens": ["dul\u00b7det", ",", "liebt", "und", "f\u00fcr", "sie", "lei\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}