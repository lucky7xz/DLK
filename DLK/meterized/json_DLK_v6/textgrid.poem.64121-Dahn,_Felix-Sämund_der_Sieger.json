{"textgrid.poem.64121": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4mund der Sieger", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Odhins Sohn war", "tokens": ["Od\u00b7hins", "Sohn", "war"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "S\u00e4mund, der Sieger,", "tokens": ["S\u00e4\u00b7mund", ",", "der", "Sie\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "S\u00e4mund, der Sieger", "tokens": ["S\u00e4\u00b7mund", ",", "der", "Sie\u00b7ger"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In See und in Saal:", "tokens": ["In", "See", "und", "in", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es mochten ihn M\u00e4nner und Maide,", "tokens": ["Es", "moch\u00b7ten", "ihn", "M\u00e4n\u00b7ner", "und", "Mai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wo er nahte, der m\u00e4chtige Mann!", "tokens": ["Wo", "er", "nah\u00b7te", ",", "der", "m\u00e4ch\u00b7ti\u00b7ge", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Zaubernd zog er", "tokens": ["Zau\u00b7bernd", "zog", "er"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "\u2013 Kein Zweiter zwang ihn \u2013", "tokens": ["\u2013", "Kein", "Zwei\u00b7ter", "zwang", "ihn", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "\u00dcber die Erde", "tokens": ["\u00dc\u00b7ber", "die", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit goldenem Apfel:", "tokens": ["Mit", "gol\u00b7de\u00b7nem", "Ap\u00b7fel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Drob m\u00fchte sich manches M\u00e4dchen", "tokens": ["Drob", "m\u00fch\u00b7te", "sich", "man\u00b7ches", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Umsonst, zu bemeistern den Mann.", "tokens": ["Um\u00b7sonst", ",", "zu", "be\u00b7meis\u00b7tern", "den", "Mann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "In den Frau'nsaal", "tokens": ["In", "den", "Frau'n\u00b7saal"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Freundlich der Fremde", "tokens": ["Freund\u00b7lich", "der", "Frem\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Trat, wo die trefflichen", "tokens": ["Trat", ",", "wo", "die", "treff\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "T\u00f6chter thronen:", "tokens": ["T\u00f6ch\u00b7ter", "thro\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Er war schimmernd und sch\u00f6n zu schauen,", "tokens": ["Er", "war", "schim\u00b7mernd", "und", "sch\u00f6n", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie der schiere Sonnenschein.", "tokens": ["Wie", "der", "schie\u00b7re", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdie den Apfel", "tokens": ["\u00bb", "die", "den", "Ap\u00b7fel"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Achtsam auff\u00e4ngt,", "tokens": ["Acht\u00b7sam", "auf\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Welchen ich werfe,", "tokens": ["Wel\u00b7chen", "ich", "wer\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Darf Wunsch sich w\u00e4hlen:", "tokens": ["Darf", "Wunsch", "sich", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was das minnige M\u00e4dchen meine, \u2013", "tokens": ["Was", "das", "min\u00b7ni\u00b7ge", "M\u00e4d\u00b7chen", "mei\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mag alles, mu\u00df alles ihr sein.", "tokens": ["Mag", "al\u00b7les", ",", "mu\u00df", "al\u00b7les", "ihr", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "VMFIN", "PIS", "PPOSAT", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Aber ins Auge", "tokens": ["A\u00b7ber", "ins", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mu\u00df sie mir aufschau'n,", "tokens": ["Mu\u00df", "sie", "mir", "auf\u00b7schau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4hrend den Wunsch", "tokens": ["W\u00e4h\u00b7rend", "den", "Wunsch"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und den Wurf wir wagen:", "tokens": ["Und", "den", "Wurf", "wir", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und vermag nicht zu haschen die Maid ihn, \u2013", "tokens": ["Und", "ver\u00b7mag", "nicht", "zu", "ha\u00b7schen", "die", "Maid", "ihn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "ART", "NN", "PPER", "$,", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Mu\u00df sie bieten zum Ku\u00df mir den Mund.\u00ab", "tokens": ["Mu\u00df", "sie", "bie\u00b7ten", "zum", "Ku\u00df", "mir", "den", "Mund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Lang durchzog er", "tokens": ["Lang", "durch\u00b7zog", "er"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "L\u00e4chelnd die Lande;", "tokens": ["L\u00e4\u00b7chelnd", "die", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Manches M\u00e4dchen", "tokens": ["Man\u00b7ches", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mu\u00dfte den Mund ihm", "tokens": ["Mu\u00df\u00b7te", "den", "Mund", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Err\u00f6tend, den rosigen, reichen, \u2013", "tokens": ["Er\u00b7r\u00f6\u00b7tend", ",", "den", "ro\u00b7si\u00b7gen", ",", "rei\u00b7chen", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVPP", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Den Rundapfel erreichte sie nicht:", "tokens": ["Den", "Rund\u00b7ap\u00b7fel", "er\u00b7reich\u00b7te", "sie", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Glanz geblendet", "tokens": ["Glanz", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Glitt ihr Blick,", "tokens": ["Glitt", "ihr", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schaute sie scheu", "tokens": ["Schau\u00b7te", "sie", "scheu"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In das Schimmerauge:", "tokens": ["In", "das", "Schim\u00b7mer\u00b7au\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Es umfing ihr wie Ohnmacht den Atem,", "tokens": ["Es", "um\u00b7fing", "ihr", "wie", "Ohn\u00b7macht", "den", "A\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und zur Erde irrte der Apfel.", "tokens": ["Und", "zur", "Er\u00b7de", "irr\u00b7te", "der", "Ap\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Also siegreich", "tokens": ["Al\u00b7so", "sieg\u00b7reich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Segelte S\u00e4mund. \u2013", "tokens": ["Se\u00b7gel\u00b7te", "S\u00e4\u00b7mund", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nun nach Ni\u00f6rdhland", "tokens": ["Nun", "nach", "Ni\u00b7\u00f6rdh\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nahte sein Nachen:", "tokens": ["Nah\u00b7te", "sein", "Na\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da hauste die herrliche Halla,", "tokens": ["Da", "haus\u00b7te", "die", "herr\u00b7li\u00b7che", "Hal\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Herrscherin hehr und hold.", "tokens": ["Die", "Herr\u00b7sche\u00b7rin", "hehr", "und", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie sah vom S\u00f6ller", "tokens": ["Sie", "sah", "vom", "S\u00f6l\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ihn seeher schreiten:", "tokens": ["Ihn", "se\u00b7e\u00b7her", "schrei\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "S\u00e4ttigte \u2013 sicher! \u2013", "tokens": ["S\u00e4t\u00b7tig\u00b7te", "\u2013", "si\u00b7cher", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "$(", "ADJD", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sich der Anschau:", "tokens": ["Sich", "der", "An\u00b7schau", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "\u00bbnun, Frigg und freundliche Freya,", "tokens": ["\u00bb", "nun", ",", "Frigg", "und", "freund\u00b7li\u00b7che", "Frey\u00b7a", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nun befreundet mich morgen fr\u00fch.\u00ab \u2013", "tokens": ["Nun", "be\u00b7freun\u00b7det", "mich", "mor\u00b7gen", "fr\u00fch", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "In den Frau'nsaal", "tokens": ["In", "den", "Frau'n\u00b7saal"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fr\u00fch trat der Fremde:", "tokens": ["Fr\u00fch", "trat", "der", "Frem\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da ragte die Reizende", "tokens": ["Da", "rag\u00b7te", "die", "Rei\u00b7zen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Hoch aus der Reihe:", "tokens": ["Hoch", "aus", "der", "Rei\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.5": {"text": "\u00bbwirf, wirf nur den Apfel! doch wisse", "tokens": ["\u00bb", "wirf", ",", "wirf", "nur", "den", "Ap\u00b7fel", "!", "doch", "wis\u00b7se"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVIMP", "$,", "VVIMP", "ADV", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zugleich auch der Wirtin Wunsch!\u00ab", "tokens": ["Zu\u00b7gleich", "auch", "der", "Wir\u00b7tin", "Wunsch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Schauernd erschaut' er", "tokens": ["Schau\u00b7ernd", "er\u00b7schaut'", "er"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Schimmerndsch\u00f6ne:", "tokens": ["Die", "Schim\u00b7mernd\u00b7sch\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wirre ward ihm,", "tokens": ["Wir\u00b7re", "ward", "ihm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Weh und wonnig:", "tokens": ["Weh", "und", "won\u00b7nig", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und er wu\u00dfte nicht, wie zu werfen", "tokens": ["Und", "er", "wu\u00df\u00b7te", "nicht", ",", "wie", "zu", "wer\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PTKZU", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und er wagte nicht, wegzusehn.", "tokens": [