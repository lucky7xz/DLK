{"dta.poem.4273": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Arioso.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer wollt', in dieser Erndte-Zeit,", "tokens": ["Wer", "wollt'", ",", "in", "die\u00b7ser", "Ernd\u00b7te\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da nicht das Korn nur abgemeyt,", "tokens": ["Da", "nicht", "das", "Korn", "nur", "ab\u00b7ge\u00b7meyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, da des Wetters Heiterkeit", "tokens": ["Nein", ",", "da", "des", "Wet\u00b7ters", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erlaubt, so, wie in vor\u2019gen Jahren,", "tokens": ["Er\u00b7laubt", ",", "so", ",", "wie", "in", "vor'\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die reifen Garben einzufahren,", "tokens": ["Die", "rei\u00b7fen", "Gar\u00b7ben", "ein\u00b7zu\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich nicht der reichen Gaben freuen,", "tokens": ["Sich", "nicht", "der", "rei\u00b7chen", "Ga\u00b7ben", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Dem, Der, da Er sie uns giebet,", "tokens": ["Und", "Dem", ",", "Der", ",", "da", "Er", "sie", "uns", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "ART", "$,", "KOUS", "PPER", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In ihnen zeigt, wie Er uns liebet,", "tokens": ["In", "ih\u00b7nen", "zeigt", ",", "wie", "Er", "uns", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein froh und dankbar Herz nicht weihen!", "tokens": ["Ein", "froh", "und", "dank\u00b7bar", "Herz", "nicht", "wei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man h\u00f6rt itzt manchen frohen Schall,", "tokens": ["Man", "h\u00f6rt", "itzt", "man\u00b7chen", "fro\u00b7hen", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es klatscht der schlanken Geissel Knall,", "tokens": ["Es", "klatscht", "der", "schlan\u00b7ken", "Geis\u00b7sel", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man h\u00f6rt ein Rasseln \u00fcberall,", "tokens": ["Man", "h\u00f6rt", "ein", "Ras\u00b7seln", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man eilet fr\u00f6hlich hin und wieder,", "tokens": ["Man", "ei\u00b7let", "fr\u00f6h\u00b7lich", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bef\u00e4hrt die Aecker auf und nieder,", "tokens": ["Be\u00b7f\u00e4hrt", "die", "A\u00b7e\u00b7cker", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es rollen Wagen voll und leer,", "tokens": ["Es", "rol\u00b7len", "Wa\u00b7gen", "voll", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die langsam hin, die schleunig her,", "tokens": ["Die", "lang\u00b7sam", "hin", ",", "die", "schleu\u00b7nig", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "$,", "PRELS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In einem kurzen Trab\u2019, und eilen", "tokens": ["In", "ei\u00b7nem", "kur\u00b7zen", "Trab'", ",", "und", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den leeren wieder mitzutheilen.", "tokens": ["Den", "lee\u00b7ren", "wie\u00b7der", "mit\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}