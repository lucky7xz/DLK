{"textgrid.poem.26638": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort, wo der Stille bed\u00fcrftig, in abseits gelegener Gasse", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo der Stille bed\u00fcrftig, in abseits gelegener Gasse", "tokens": ["Dort", ",", "wo", "der", "Stil\u00b7le", "be\u00b7d\u00fcrf\u00b7tig", ",", "in", "ab\u00b7seits", "ge\u00b7le\u00b7ge\u00b7ner", "Gas\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJD", "$,", "APPR", "NE", "ADJA", "NN"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Fand der Dichter sein Heim, hebt sich ein gothischer Bau.", "tokens": ["Fand", "der", "Dich\u00b7ter", "sein", "Heim", ",", "hebt", "sich", "ein", "go\u00b7thi\u00b7scher", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Lange steht er noch nicht; ihn schuf das letzte Jahrzehend,", "tokens": ["Lan\u00b7ge", "steht", "er", "noch", "nicht", ";", "ihn", "schuf", "das", "letz\u00b7te", "Jahr\u00b7ze\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und zur Schule geweiht haben ihn V\u00e4ter der Stadt.", "tokens": ["Und", "zur", "Schu\u00b7le", "ge\u00b7weiht", "ha\u00b7ben", "ihn", "V\u00e4\u00b7ter", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "VAFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Eifrige Knaben und M\u00e4dchen besuchen die stattlichen R\u00e4ume,", "tokens": ["Eif\u00b7ri\u00b7ge", "Kna\u00b7ben", "und", "M\u00e4d\u00b7chen", "be\u00b7su\u00b7chen", "die", "statt\u00b7li\u00b7chen", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Wo sich Licht und Luft hell und gedeihlich vertheilt.", "tokens": ["Wo", "sich", "Licht", "und", "Luft", "hell", "und", "ge\u00b7deih\u00b7lich", "ver\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Dort erlernen sie Alles, was noth zu wissen dem Menschen,", "tokens": ["Dort", "er\u00b7ler\u00b7nen", "sie", "Al\u00b7les", ",", "was", "noth", "zu", "wis\u00b7sen", "dem", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PWS", "NN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Denn bequemlich, wie einst, ebnet sich nicht mehr der Pfad.", "tokens": ["Denn", "be\u00b7quem\u00b7lich", ",", "wie", "einst", ",", "eb\u00b7net", "sich", "nicht", "mehr", "der", "Pfad", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADV", "$,", "VVFIN", "PRF", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Lesen und schreiben zu k\u00f6nnen, gen\u00fcgte; mit Fibel und Bibel", "tokens": ["Le\u00b7sen", "und", "schrei\u00b7ben", "zu", "k\u00f6n\u00b7nen", ",", "ge\u00b7n\u00fcg\u00b7te", ";", "mit", "Fi\u00b7bel", "und", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVINF", "PTKZU", "VMINF", "$,", "VVFIN", "$.", "APPR", "NN", "KON", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Und dem Einmaleins reichte vor Zeiten man aus.", "tokens": ["Und", "dem", "Ein\u00b7mal\u00b7eins", "reich\u00b7te", "vor", "Zei\u00b7ten", "man", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "PIS", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Heute ist jegliches Kind bereits ein Gelehrter; wie oft schon", "tokens": ["Heu\u00b7te", "ist", "jeg\u00b7li\u00b7ches", "Kind", "be\u00b7reits", "ein", "Ge\u00b7lehr\u00b7ter", ";", "wie", "oft", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "ART", "NN", "$.", "PWAV", "ADV", "ADV"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Hat mich ergrauenden Mann Weisheit des Sch\u00fclers besch\u00e4mt.", "tokens": ["Hat", "mich", "er\u00b7grau\u00b7en\u00b7den", "Mann", "Weis\u00b7heit", "des", "Sch\u00fc\u00b7lers", "be\u00b7sch\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Aber betrachtend verweil' ich mich gern, wenn das knirpsige V\u00f6lklein,", "tokens": ["A\u00b7ber", "be\u00b7trach\u00b7tend", "ver\u00b7weil'", "ich", "mich", "gern", ",", "wenn", "das", "knirp\u00b7si\u00b7ge", "V\u00f6l\u00b7klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Bunt durcheinander gemischt, wimmelnd den Th\u00fcren entstr\u00f6mt.", "tokens": ["Bunt", "durch\u00b7ein\u00b7an\u00b7der", "ge\u00b7mischt", ",", "wim\u00b7melnd", "den", "Th\u00fc\u00b7ren", "ent\u00b7str\u00f6mt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$,", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Welche F\u00fclle des Lebens in all den verschied'nen Gestalten,", "tokens": ["Wel\u00b7che", "F\u00fcl\u00b7le", "des", "Le\u00b7bens", "in", "all", "den", "ver\u00b7schie\u00b7d'\u00b7nen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+---+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Theils wie von Rubens, van Dyk \u2013 theils wie von Cranach gemalt!", "tokens": ["Theils", "wie", "von", "Ru\u00b7bens", ",", "van", "Dyk", "\u2013", "theils", "wie", "von", "Cra\u00b7nach", "ge\u00b7malt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NE", "$,", "NE", "NE", "$(", "ADV", "KOKOM", "APPR", "NE", "VVPP", "$."], "meter": "+--+---+-+++-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Fr\u00fch verr\u00e4th sich in Gang und Geberde das innerste Wesen,", "tokens": ["Fr\u00fch", "ver\u00b7r\u00e4\u00b7th", "sich", "in", "Gang", "und", "Ge\u00b7ber\u00b7de", "das", "in\u00b7ners\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Und dem erkennenden Blick zeigt sich das Werdende schon.", "tokens": ["Und", "dem", "er\u00b7ken\u00b7nen\u00b7den", "Blick", "zeigt", "sich", "das", "Wer\u00b7den\u00b7de", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Schm\u00e4chtiger Knabe, erhobenen Haupts hinwandelnd im Schwarme,", "tokens": ["Schm\u00e4ch\u00b7ti\u00b7ger", "Kna\u00b7be", ",", "er\u00b7ho\u00b7be\u00b7nen", "Haupts", "hin\u00b7wan\u00b7delnd", "im", "Schwar\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "In dir reift mir gewi\u00df bald ein College heran.", "tokens": ["In", "dir", "reift", "mir", "ge\u00b7wi\u00df", "bald", "ein", "Col\u00b7le\u00b7ge", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Dichtest du etwa schon jetzt an einem veristischen Drama,", "tokens": ["Dich\u00b7test", "du", "et\u00b7wa", "schon", "jetzt", "an", "ei\u00b7nem", "ve\u00b7ris\u00b7ti\u00b7schen", "Dra\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.22": {"text": "Das in der Klinik beginnt und am Seciertisch verl\u00e4uft?", "tokens": ["Das", "in", "der", "Kli\u00b7nik", "be\u00b7ginnt", "und", "am", "Se\u00b7cier\u00b7tisch", "ver\u00b7l\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Und du, niedliche Kleine, mit gro\u00dfen, beweglichen Augen,", "tokens": ["Und", "du", ",", "nied\u00b7li\u00b7che", "Klei\u00b7ne", ",", "mit", "gro\u00b7\u00dfen", ",", "be\u00b7weg\u00b7li\u00b7chen", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "ADJA", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Ahnst du Novellen bereits, \u00fcpp'ger als die des Boccaz?", "tokens": ["Ahnst", "du", "No\u00b7vel\u00b7len", "be\u00b7reits", ",", "\u00fcpp'\u00b7ger", "als", "die", "des", "Boc\u00b7caz", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$,", "ADJD", "KOKOM", "ART", "ART", "NN", "$."], "meter": "-+-+--+--++--+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Freieste Liebe versprichst du, indessen breitspurig die Freundin", "tokens": ["Frei\u00b7es\u00b7te", "Lie\u00b7be", "ver\u00b7sprichst", "du", ",", "in\u00b7des\u00b7sen", "breit\u00b7spu\u00b7rig", "die", "Freun\u00b7din"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "An der Seite dir stapft, reizlos verschnittenen Haars.", "tokens": ["An", "der", "Sei\u00b7te", "dir", "stapft", ",", "reiz\u00b7los", "ver\u00b7schnit\u00b7te\u00b7nen", "Haars", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Diese, ich seh's, w\u00e4lzt unter der wuchtigen Stirn schon die Frage,", "tokens": ["Die\u00b7se", ",", "ich", "seh's", ",", "w\u00e4lzt", "un\u00b7ter", "der", "wuch\u00b7ti\u00b7gen", "Stirn", "schon", "die", "Fra\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPER", "NE", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Wie man das M\u00e4nnergeschlecht g\u00e4nzlich vom Erdball verdr\u00e4ngt.", "tokens": ["Wie", "man", "das", "M\u00e4n\u00b7ner\u00b7ge\u00b7schlecht", "g\u00e4nz\u00b7lich", "vom", "Erd\u00b7ball", "ver\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Ja, hier bereitet sich vor in allen Phasen die Zukunft,", "tokens": ["Ja", ",", "hier", "be\u00b7rei\u00b7tet", "sich", "vor", "in", "al\u00b7len", "Pha\u00b7sen", "die", "Zu\u00b7kunft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PRF", "APPR", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Achtlos trippeln an mir ihre Vertreter vorbei:", "tokens": ["Acht\u00b7los", "trip\u00b7peln", "an", "mir", "ih\u00b7re", "Ver\u00b7tre\u00b7ter", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Wahrer des ewigen Friedens, Begr\u00fcnder der gleichesten Gleichheit,", "tokens": ["Wah\u00b7rer", "des", "e\u00b7wi\u00b7gen", "Frie\u00b7dens", ",", "Be\u00b7gr\u00fcn\u00b7der", "der", "glei\u00b7ches\u00b7ten", "Gleich\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Weltbefreier vom Gift schn\u00f6den Mikrobengez\u00fcchts;", "tokens": ["Welt\u00b7be\u00b7frei\u00b7er", "vom", "Gift", "schn\u00f6\u00b7den", "Mik\u00b7ro\u00b7ben\u00b7ge\u00b7z\u00fcchts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Maler der vierten Dimension \u2013 und Entdecker der f\u00fcnften,", "tokens": ["Ma\u00b7ler", "der", "vier\u00b7ten", "Di\u00b7men\u00b7si\u00b7on", "\u2013", "und", "Ent\u00b7de\u00b7cker", "der", "f\u00fcnf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "KON", "NN", "ART", "ADJA", "$,"], "meter": "+--+-+--+-+---+-", "measure": "iambic.hexa.invert"}, "line.34": {"text": "Die mit Gespenstern bereits speisen vertraulich zu Nacht.", "tokens": ["Die", "mit", "Ge\u00b7spens\u00b7tern", "be\u00b7reits", "spei\u00b7sen", "ver\u00b7trau\u00b7lich", "zu", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Aber gedeiht nur und bl\u00fcht, ihr kleinen Erneu'rer der Menschheit \u2013", "tokens": ["A\u00b7ber", "ge\u00b7deiht", "nur", "und", "bl\u00fcht", ",", "ihr", "klei\u00b7nen", "Er\u00b7neu'\u00b7rer", "der", "Menschheit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "ART", "NN", "$("], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.36": {"text": "Wachsen die B\u00e4ume doch nicht gleich in den Himmel hinein!", "tokens": ["Wach\u00b7sen", "die", "B\u00e4u\u00b7me", "doch", "nicht", "gleich", "in", "den", "Him\u00b7mel", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}