{"textgrid.poem.51042": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man merkte, da\u00df der Wein geraten war:", "tokens": ["Man", "merk\u00b7te", ",", "da\u00df", "der", "Wein", "ge\u00b7ra\u00b7ten", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der alte Bettler wankte aus dem Tor,", "tokens": ["Der", "al\u00b7te", "Bett\u00b7ler", "wank\u00b7te", "aus", "dem", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wangen gl\u00fchend wie ein Rosenflor,", "tokens": ["Die", "Wan\u00b7gen", "gl\u00fc\u00b7hend", "wie", "ein", "Ro\u00b7sen\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mutwillig flatterte sein Silberhaar.", "tokens": ["Mut\u00b7wil\u00b7lig", "flat\u00b7ter\u00b7te", "sein", "Sil\u00b7ber\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Und vor und hinter ihm die Kinderschar", "tokens": ["Und", "vor", "und", "hin\u00b7ter", "ihm", "die", "Kin\u00b7der\u00b7schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umdr\u00e4ngt' ihn, wie ein Klein-Bacchantenchor,", "tokens": ["Um\u00b7dr\u00e4ngt'", "ihn", ",", "wie", "ein", "Klein\u00b7Bac\u00b7chan\u00b7ten\u00b7chor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Draus ragte schwank der Selige empor,", "tokens": ["Draus", "rag\u00b7te", "schwank", "der", "Se\u00b7li\u00b7ge", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich spiegelnd in den hundert \u00c4uglein klar.", "tokens": ["Sich", "spie\u00b7gelnd", "in", "den", "hun\u00b7dert", "\u00c4ug\u00b7lein", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Morgen, als die Kinderlein noch schliefen,", "tokens": ["Am", "Mor\u00b7gen", ",", "als", "die", "Kin\u00b7der\u00b7lein", "noch", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jungen Tr\u00e4umen drollig angelacht,", "tokens": ["Von", "jun\u00b7gen", "Tr\u00e4u\u00b7men", "drol\u00b7lig", "an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sah man den braunen Wald von Silber triefen.", "tokens": ["Sah", "man", "den", "brau\u00b7nen", "Wald", "von", "Sil\u00b7ber", "trie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es war ein Reif gefallen \u00fcber Nacht;", "tokens": ["Es", "war", "ein", "Reif", "ge\u00b7fal\u00b7len", "\u00fc\u00b7ber", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Alte lag erfroren in dem tiefen", "tokens": ["Der", "Al\u00b7te", "lag", "er\u00b7fro\u00b7ren", "in", "dem", "tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geb\u00fcsch, vom Rausch im Himmel aufgewacht.", "tokens": ["Ge\u00b7b\u00fcsch", ",", "vom", "Rausch", "im", "Him\u00b7mel", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}