{"textgrid.poem.37906": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsblumen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzlich thut mich erfreuen,", "tokens": ["Herz\u00b7lich", "thut", "mich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die fr\u00f6hliche Sommer-Zeit,", "tokens": ["Die", "fr\u00f6h\u00b7li\u00b7che", "Som\u00b7mer\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "All mein Gebl\u00fct erneuen,", "tokens": ["All", "mein", "Ge\u00b7bl\u00fct", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der May in Wollust freut,", "tokens": ["Der", "May", "in", "Wol\u00b7lust", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Lerch thut sich erschwingen", "tokens": ["Die", "Lerch", "thut", "sich", "er\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit ihrem hellen Schall,", "tokens": ["Mit", "ih\u00b7rem", "hel\u00b7len", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lieblich die V\u00f6gel singen,", "tokens": ["Lieb\u00b7lich", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Dazu die Nachtigall.", "tokens": ["Da\u00b7zu", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kukuk mit seinem Schreien,", "tokens": ["Der", "Ku\u00b7kuk", "mit", "sei\u00b7nem", "Schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Macht fr\u00f6hlich jedermann,", "tokens": ["Macht", "fr\u00f6h\u00b7lich", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Abends fr\u00f6hlich reihen,", "tokens": ["Des", "A\u00b7bends", "fr\u00f6h\u00b7lich", "rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die M\u00e4dlein wohlgethan,", "tokens": ["Die", "M\u00e4d\u00b7lein", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Spazieren zu den Brunnen,", "tokens": ["Spa\u00b7zie\u00b7ren", "zu", "den", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bekr\u00e4nzen sie zur Zeit,", "tokens": ["Be\u00b7kr\u00e4n\u00b7zen", "sie", "zur", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Alle Welt sich freut in Wonnen,", "tokens": ["Al\u00b7le", "Welt", "sich", "freut", "in", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit Reisen fern und weit.", "tokens": ["Mit", "Rei\u00b7sen", "fern", "und", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es gr\u00fcnet in dem Walde,", "tokens": ["Es", "gr\u00fc\u00b7net", "in", "dem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Blumen bl\u00fchen frey,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fc\u00b7hen", "frey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die R\u00f6\u00dflein auf dem Felde,", "tokens": ["Die", "R\u00f6\u00df\u00b7lein", "auf", "dem", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Farben mancherley,", "tokens": ["Von", "Far\u00b7ben", "man\u00b7cher\u00b7ley", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Bl\u00fcmlein steht im Garten,", "tokens": ["Ein", "Bl\u00fcm\u00b7lein", "steht", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das hei\u00dft, Vergi\u00df nit mein,", "tokens": ["Das", "hei\u00dft", ",", "Ver\u00b7gi\u00df", "nit", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVIMP", "PTKNEG", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das edle Kraut zu warten,", "tokens": ["Das", "ed\u00b7le", "Kraut", "zu", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Macht guten Augenschein.", "tokens": ["Macht", "gu\u00b7ten", "Au\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Kraut w\u00e4chst in der Aue,", "tokens": ["Ein", "Kraut", "w\u00e4chst", "in", "der", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Namen Wohlgemuth,", "tokens": ["Mit", "Na\u00b7men", "Wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liebt sehr die sch\u00f6nen Frauen,", "tokens": ["Liebt", "sehr", "die", "sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu die Holder-Bl\u00fcth,", "tokens": ["Da\u00b7zu", "die", "Hol\u00b7der\u00b7B\u00b7l\u00fcth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die wei\u00df und rothe Rosen,", "tokens": ["Die", "wei\u00df", "und", "ro\u00b7the", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00e4lt man in gro\u00dfer Acht,", "tokens": ["H\u00e4lt", "man", "in", "gro\u00b7\u00dfer", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Thut's Geld darum verlosen,", "tokens": ["Thut's", "Geld", "da\u00b7rum", "ver\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sch\u00f6ne Kr\u00e4nze daraus macht.", "tokens": ["Sch\u00f6\u00b7ne", "Kr\u00e4n\u00b7ze", "da\u00b7raus", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Kraut, Je l\u00e4nger je lieber,", "tokens": ["Das", "Kraut", ",", "Je", "l\u00e4n\u00b7ger", "je", "lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An manchem Ende bl\u00fcht,", "tokens": ["An", "man\u00b7chem", "En\u00b7de", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bringt oft ein heimlich Fieber,", "tokens": ["Bringt", "oft", "ein", "heim\u00b7lich", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer sich nicht daf\u00fcr h\u00fct,", "tokens": ["Wer", "sich", "nicht", "da\u00b7f\u00fcr", "h\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "PAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab es wohl vernommen,", "tokens": ["Ich", "hab", "es", "wohl", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was dieses Kraut vermag,", "tokens": ["Was", "die\u00b7ses", "Kraut", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch kann man dem vorkommen,", "tokens": ["Doch", "kann", "man", "dem", "vor\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wem lieb ist jeder Tag.", "tokens": ["Wem", "lieb", "ist", "je\u00b7der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Des Morgens in dem Thaue,", "tokens": ["Des", "Mor\u00b7gens", "in", "dem", "Thau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die M\u00e4dlein grasen gehn,", "tokens": ["Die", "M\u00e4d\u00b7lein", "gra\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar lieblich sich anschauen,", "tokens": ["Gar", "lieb\u00b7lich", "sich", "an\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey sch\u00f6nen Bl\u00fcmlein stehn,", "tokens": ["Bey", "sch\u00f6\u00b7nen", "Bl\u00fcm\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daraus sie Kr\u00e4nzlein machen", "tokens": ["Da\u00b7raus", "sie", "Kr\u00e4nz\u00b7lein", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schenkens ihrem Schatz,", "tokens": ["Und", "schen\u00b7kens", "ih\u00b7rem", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Thun freundlich ihn anlachen,", "tokens": ["Thun", "freund\u00b7lich", "ihn", "an\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und geben ihm ein Schmatz.", "tokens": ["Und", "ge\u00b7ben", "ihm", "ein", "Schmatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Darum lob ich den Sommer,", "tokens": ["Da\u00b7rum", "lob", "ich", "den", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dazu den Mayen gut,", "tokens": ["Da\u00b7zu", "den", "Ma\u00b7yen", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wendet allen Kummer,", "tokens": ["Der", "wen\u00b7det", "al\u00b7len", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bringt viel Freud und Muth,", "tokens": ["Und", "bringt", "viel", "Freud", "und", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Zeit will ich genie\u00dfen,", "tokens": ["Der", "Zeit", "will", "ich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dieweil ich Pfenning hab,", "tokens": ["Die\u00b7weil", "ich", "Pfen\u00b7ning", "hab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und den es thut verdrie\u00dfen,", "tokens": ["Und", "den", "es", "thut", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der fall die Stiegen herab.", "tokens": ["Der", "fall", "die", "Stie\u00b7gen", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}