{"textgrid.poem.52503": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "23. Die Eiche des Bundes der Rhetier", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eiche des Bundes der Freiheit! dich splittern nicht z\u00fcndende Blitze,", "tokens": ["Ei\u00b7che", "des", "Bun\u00b7des", "der", "Frei\u00b7heit", "!", "dich", "split\u00b7tern", "nicht", "z\u00fcn\u00b7den\u00b7de", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und kein schneidendes Beil droht dir Verheerung und Fall.", "tokens": ["Und", "kein", "schnei\u00b7den\u00b7des", "Beil", "droht", "dir", "Ver\u00b7hee\u00b7rung", "und", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber, wer sch\u00fctzet die Wurzel vor heimlich verderbender F\u00e4ulnis?", "tokens": ["A\u00b7ber", ",", "wer", "sch\u00fct\u00b7zet", "die", "Wur\u00b7zel", "vor", "heim\u00b7lich", "ver\u00b7der\u00b7ben\u00b7der", "F\u00e4ul\u00b7nis", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ach! das gef\u00e4hrlichste Gift ist, das im Innern schleicht.", "tokens": ["Ach", "!", "das", "ge\u00b7f\u00e4hr\u00b7lichs\u00b7te", "Gift", "ist", ",", "das", "im", "In\u00b7nern", "schleicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}