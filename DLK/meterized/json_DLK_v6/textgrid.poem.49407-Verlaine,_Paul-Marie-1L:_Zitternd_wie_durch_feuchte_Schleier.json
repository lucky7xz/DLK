{"textgrid.poem.49407": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zitternd wie durch feuchte Schleier", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zitternd wie durch feuchte Schleier", "tokens": ["Zit\u00b7ternd", "wie", "durch", "feuch\u00b7te", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwimmt der Abendstern im Weiher,", "tokens": ["Schwimmt", "der", "A\u00b7bends\u00b7tern", "im", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und der Fischer z\u00fcndet Feuer.", "tokens": ["Und", "der", "Fi\u00b7scher", "z\u00fcn\u00b7det", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute oder nie ist's Zeit,", "tokens": ["Heu\u00b7te", "o\u00b7der", "nie", "ist's", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass Ihr Herrn verwegen seid,", "tokens": ["Dass", "Ihr", "Herrn", "ver\u00b7we\u00b7gen", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig wag' ich jeden Streit.", "tokens": ["Lus\u00b7tig", "wag'", "ich", "je\u00b7den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf der Laute klimpert Lieder", "tokens": ["Auf", "der", "Lau\u00b7te", "klim\u00b7pert", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Athis und blickt gl\u00fchend nieder", "tokens": ["A\u00b7this", "und", "blickt", "gl\u00fc\u00b7hend", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zu der spr\u00f6den Chloris Mieder.", "tokens": ["Zu", "der", "spr\u00f6\u00b7den", "Chlo\u00b7ris", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Egl\u00e9 beichtet dem Abb\u00e9", "tokens": ["Egl\u00e9", "beich\u00b7tet", "dem", "Ab\u00b7b\u00e9"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Allzugern ihr Liebesweh,", "tokens": ["All\u00b7zu\u00b7gern", "ihr", "Lie\u00b7bes\u00b7weh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Graf ist toll wie je.", "tokens": ["Und", "der", "Graf", "ist", "toll", "wie", "je", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Mondlicht blinkt schon in den R\u00e4umen,", "tokens": ["Mond\u00b7licht", "blinkt", "schon", "in", "den", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Kahn streicht ohne S\u00e4umen", "tokens": ["Und", "der", "Kahn", "streicht", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der Wasser stilles Tr\u00e4umen.", "tokens": ["Durch", "der", "Was\u00b7ser", "stil\u00b7les", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}