{"textgrid.poem.65770": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "44", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r meines Fleisches z\u00e4her Stoff Gedanke,", "tokens": ["W\u00e4r", "mei\u00b7nes", "Flei\u00b7sches", "z\u00e4\u00b7her", "Stoff", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann hielt mich neidische Entfernung nicht;", "tokens": ["Dann", "hielt", "mich", "nei\u00b7di\u00b7sche", "Ent\u00b7fer\u00b7nung", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn allem Raum zum Trotz entfl\u00f6h ich jeder Schranke,", "tokens": ["Denn", "al\u00b7lem", "Raum", "zum", "Trotz", "ent\u00b7fl\u00f6h", "ich", "je\u00b7der", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "APPRART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die mich verbannt aus deinem Angesicht.", "tokens": ["Die", "mich", "ver\u00b7bannt", "aus", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann g\u00e4lt mir gleich, ob auch am fernsten Strande", "tokens": ["Dann", "g\u00e4lt", "mir", "gleich", ",", "ob", "auch", "am", "ferns\u00b7ten", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Fu\u00df st\u00fcnd, weit von dir; denn unumschr\u00e4nkt", "tokens": ["Mein", "Fu\u00df", "st\u00fcnd", ",", "weit", "von", "dir", ";", "denn", "un\u00b7um\u00b7schr\u00e4nkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADJD", "APPR", "PPER", "$.", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Springt der Gedanke \u00fcber Meer und Lande", "tokens": ["Springt", "der", "Ge\u00b7dan\u00b7ke", "\u00fc\u00b7ber", "Meer", "und", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "So schnell als er den Ort, wohin er fliehn will, denkt.", "tokens": ["So", "schnell", "als", "er", "den", "Ort", ",", "wo\u00b7hin", "er", "fliehn", "will", ",", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch ach! Tod ist dies Denken: ", "tokens": ["Doch", "ach", "!", "Tod", "ist", "dies", "Den\u00b7ken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "NN", "VAFIN", "PDS", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Zu sein, um Welten weit dir nachzufliehn;", "tokens": ["Zu", "sein", ",", "um", "Wel\u00b7ten", "weit", "dir", "nach\u00b7zu\u00b7fliehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,", "KOUI", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und da\u00df ich so am Gram der lahmen Zeiten kranke,", "tokens": ["Und", "da\u00df", "ich", "so", "am", "Gram", "der", "lah\u00b7men", "Zei\u00b7ten", "kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn Erd' und Wasser mich zu Boden ziehn,", "tokens": ["Wenn", "Erd'", "und", "Was\u00b7ser", "mich", "zu", "Bo\u00b7den", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PWS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die tr\u00e4gen Elemente, die mich nur", "tokens": ["Die", "tr\u00e4\u00b7gen", "E\u00b7le\u00b7men\u00b7te", ",", "die", "mich", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Mit Tr\u00e4nen n\u00e4hren, ihres Jammers Spur.", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "n\u00e4h\u00b7ren", ",", "ih\u00b7res", "Jam\u00b7mers", "Spur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}