{"textgrid.poem.50622": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Melintes, den der Feinde Macht", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Melintes, den der Feinde Macht", "tokens": ["Me\u00b7lin\u00b7tes", ",", "den", "der", "Fein\u00b7de", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um seine Wohlfahrt hat gebracht,", "tokens": ["Um", "sei\u00b7ne", "Wohl\u00b7fahrt", "hat", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem sprach man tr\u00f6stlich also zu:", "tokens": ["Dem", "sprach", "man", "tr\u00f6st\u00b7lich", "al\u00b7so", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbmelintes, stelle dich zur Ruh',", "tokens": ["\u00bb", "me\u00b7lin\u00b7tes", ",", "stel\u00b7le", "dich", "zur", "Ruh'", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil deiner Unschuld Nichts gebricht", "tokens": ["Weil", "dei\u00b7ner", "Un\u00b7schuld", "Nichts", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Jeder von dir r\u00fchmlich spricht,", "tokens": ["Und", "Je\u00b7der", "von", "dir", "r\u00fchm\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sich mit jenen nicht verschworen.\u00ab", "tokens": ["Der", "sich", "mit", "je\u00b7nen", "nicht", "ver\u00b7schwo\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "APPR", "PDS", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Melintes h\u00f6rt' es an und rief:", "tokens": ["Me\u00b7lin\u00b7tes", "h\u00f6rt'", "es", "an", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}