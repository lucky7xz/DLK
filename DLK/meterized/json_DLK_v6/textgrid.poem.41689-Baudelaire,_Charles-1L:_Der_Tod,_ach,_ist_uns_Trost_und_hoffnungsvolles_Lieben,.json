{"textgrid.poem.41689": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Tod, ach, ist uns Trost und hoffnungsvolles Lieben,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod, ach, ist uns Trost und hoffnungsvolles Lieben,", "tokens": ["Der", "Tod", ",", "ach", ",", "ist", "uns", "Trost", "und", "hoff\u00b7nungs\u00b7vol\u00b7les", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "VAFIN", "PPER", "NN", "KON", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist des Lebens Ziel, die Kraft, die uns durchdringt,", "tokens": ["Er", "ist", "des", "Le\u00b7bens", "Ziel", ",", "die", "Kraft", ",", "die", "uns", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist der Zaubertrank, von dessen Rausch getrieben", "tokens": ["Er", "ist", "der", "Zau\u00b7ber\u00b7trank", ",", "von", "des\u00b7sen", "Rausch", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir mutvoll weitergehn, bis da\u00df der Abend sinkt.", "tokens": ["Wir", "mut\u00b7voll", "wei\u00b7ter\u00b7gehn", ",", "bis", "da\u00df", "der", "A\u00b7bend", "sinkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KON", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch Sturmwind, Reif und Schnee, die eisig niederstieben,", "tokens": ["Durch", "Sturm\u00b7wind", ",", "Reif", "und", "Schnee", ",", "die", "ei\u00b7sig", "nie\u00b7ders\u00b7tie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist er die Klarheit, die durchs Dunkel zitternd blinkt;", "tokens": ["Ist", "er", "die", "Klar\u00b7heit", ",", "die", "durchs", "Dun\u00b7kel", "zit\u00b7ternd", "blinkt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die gro\u00dfe Herberg, wie sie in dem Buch geschrieben,", "tokens": ["Die", "gro\u00b7\u00dfe", "Her\u00b7berg", ",", "wie", "sie", "in", "dem", "Buch", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo man sich setzen kann, wo Schlaf und Speise winkt.", "tokens": ["Wo", "man", "sich", "set\u00b7zen", "kann", ",", "wo", "Schlaf", "und", "Spei\u00b7se", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "VVINF", "VMFIN", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er ist ein Engel, der des tiefen Schlafs Begl\u00fcckung", "tokens": ["Er", "ist", "ein", "En\u00b7gel", ",", "der", "des", "tie\u00b7fen", "Schlafs", "Be\u00b7gl\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Zauberh\u00e4nden h\u00e4lt und selgen Traums Verz\u00fcckung,", "tokens": ["In", "Zau\u00b7ber\u00b7h\u00e4n\u00b7den", "h\u00e4lt", "und", "sel\u00b7gen", "Traums", "Ver\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der ein weiches Bett den nackten Armen macht;", "tokens": ["Und", "der", "ein", "wei\u00b7ches", "Bett", "den", "nack\u00b7ten", "Ar\u00b7men", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er ist der G\u00f6tter Ruhm, des Erntesegens Milde,", "tokens": ["Er", "ist", "der", "G\u00f6t\u00b7ter", "Ruhm", ",", "des", "Ern\u00b7te\u00b7se\u00b7gens", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Armen Gold, sein alt und heimatlich Gefilde,", "tokens": ["Des", "Ar\u00b7men", "Gold", ",", "sein", "alt", "und", "hei\u00b7mat\u00b7lich", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das weiterschlo\u00dfne Tor zu neuer Himmel Pracht!", "tokens": ["Das", "wei\u00b7ter\u00b7schlo\u00df\u00b7ne", "Tor", "zu", "neu\u00b7er", "Him\u00b7mel", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}