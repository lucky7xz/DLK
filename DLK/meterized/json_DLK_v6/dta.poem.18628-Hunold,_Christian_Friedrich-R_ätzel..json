{"dta.poem.18628": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "R \u00e4tzel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Wer gl\u00fccklich rahten kan: Der thu es sonder Lachen/", "tokens": ["Wer", "gl\u00fcck\u00b7lich", "rah\u00b7ten", "kan", ":", "Der", "thu", "es", "son\u00b7der", "La\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$.", "ART", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Hamburg wird man es fast alle Tage machen:", "tokens": ["In", "Ham\u00b7burg", "wird", "man", "es", "fast", "al\u00b7le", "Ta\u00b7ge", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIS", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ein rundtes Loch mit Haaren wohl versehn/", "tokens": ["Es", "ist", "ein", "rund\u00b7tes", "Loch", "mit", "Haa\u00b7ren", "wohl", "ver\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darnach der Appetit den M\u00e4nnern pflegt zu stehn/", "tokens": ["Dar\u00b7nach", "der", "Ap\u00b7pe\u00b7tit", "den", "M\u00e4n\u00b7nern", "pflegt", "zu", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Leibes bester Theil pflegt sich mit ihm zu paaren/", "tokens": ["Des", "Lei\u00b7bes", "bes\u00b7ter", "Theil", "pflegt", "sich", "mit", "ihm", "zu", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und fast ein jeder w\u00fcnscht mit Freuden nein zu fahren/", "tokens": ["Und", "fast", "ein", "je\u00b7der", "w\u00fcnscht", "mit", "Freu\u00b7den", "nein", "zu", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "VVFIN", "APPR", "NN", "PTKANT", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das eng zusammen h\u00e4lt/ und h\u00fcbsche W\u00e4rme giebt/", "tokens": ["Das", "eng", "zu\u00b7sam\u00b7men", "h\u00e4lt", "/", "und", "h\u00fcb\u00b7sche", "W\u00e4r\u00b7me", "giebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "$(", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In diesem Loch' ist man bey Jungfern sehr beliebt.", "tokens": ["In", "die\u00b7sem", "Loch'", "ist", "man", "bey", "Jung\u00b7fern", "sehr", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIS", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "(paruqve)", "tokens": ["(", "pa\u00b7ruq\u00b7ve", ")"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "FM", "$("], "meter": "-+-", "measure": "amphibrach.single"}}}}}