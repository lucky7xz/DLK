{"textgrid.poem.38681": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lied aus der Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann, in des Abends letztem Scheine,", "tokens": ["Wann", ",", "in", "des", "A\u00b7bends", "letz\u00b7tem", "Schei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir eine l\u00e4chelnde Gestalt,", "tokens": ["Dir", "ei\u00b7ne", "l\u00e4\u00b7cheln\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Am Rasensitz im Eichenhaine,", "tokens": ["Am", "Ra\u00b7sen\u00b7sitz", "im", "Ei\u00b7chen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wink und Gru\u00df vor\u00fcberwallt:", "tokens": ["Mit", "Wink", "und", "Gru\u00df", "vor\u00b7\u00fc\u00b7berw\u00b7allt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist des Freundes treuer Geist,", "tokens": ["Das", "ist", "des", "Freun\u00b7des", "treu\u00b7er", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Freud' und Frieden dir verhei\u00dft.", "tokens": ["Der", "Freud'", "und", "Frie\u00b7den", "dir", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann in des Mondes D\u00e4mmerlichte", "tokens": ["Wann", "in", "des", "Mon\u00b7des", "D\u00e4m\u00b7mer\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich deiner Liebe Traum versch\u00f6nt,", "tokens": ["Sich", "dei\u00b7ner", "Lie\u00b7be", "Traum", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Cytisus und Weymutsfichte", "tokens": ["Durch", "Cy\u00b7ti\u00b7sus", "und", "Wey\u00b7muts\u00b7fich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Melodisches Ges\u00e4usel t\u00f6nt,", "tokens": ["Me\u00b7lo\u00b7di\u00b7sches", "Ge\u00b7s\u00e4u\u00b7sel", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und Ahndung dir den Busen hebt:", "tokens": ["Und", "Ahn\u00b7dung", "dir", "den", "Bu\u00b7sen", "hebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist mein Geist der dich umschwebt.", "tokens": ["Das", "ist", "mein", "Geist", "der", "dich", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchlst du, beim seligen Verlieren", "tokens": ["F\u00fchlst", "du", ",", "beim", "se\u00b7li\u00b7gen", "Ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In des Vergangnen Zauberland,", "tokens": ["In", "des", "Ver\u00b7gang\u00b7nen", "Zau\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein lindes, geistiges Ber\u00fchren,", "tokens": ["Ein", "lin\u00b7des", ",", "geis\u00b7ti\u00b7ges", "Be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Zefyrs Ku\u00df, an Lipp' und Hand,", "tokens": ["Wie", "Ze\u00b7fyrs", "Ku\u00df", ",", "an", "Lipp'", "und", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wankt der Kerze flatternd Licht:", "tokens": ["Und", "wankt", "der", "Ker\u00b7ze", "flat\u00b7ternd", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist mein Geist, o zweifle nicht!", "tokens": ["Das", "ist", "mein", "Geist", ",", "o", "zweif\u00b7le", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "FM", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rst du, beim Silberglanz der Sterne,", "tokens": ["H\u00f6rst", "du", ",", "beim", "Sil\u00b7ber\u00b7glanz", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leis' im verschwiegnen K\u00e4mmerlein,", "tokens": ["Leis'", "im", "ver\u00b7schwieg\u00b7nen", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich Aeolsharfen aus der Ferne,", "tokens": ["Gleich", "A\u00b7e\u00b7ols\u00b7har\u00b7fen", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Bundeswort: Auf ewig dein!", "tokens": ["Das", "Bun\u00b7des\u00b7wort", ":", "Auf", "e\u00b7wig", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schlummre sanft; es ist mein Geist,", "tokens": ["Dann", "schlumm\u00b7re", "sanft", ";", "es", "ist", "mein", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Freud' und Frieden dir verhei\u00dft.", "tokens": ["Der", "Freud'", "und", "Frie\u00b7den", "dir", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}