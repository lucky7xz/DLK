{"dta.poem.15660": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Alterthum beschrieb mit lebensvollen Bildern", "tokens": ["Das", "Al\u00b7ter\u00b7thum", "be\u00b7schrieb", "mit", "le\u00b7bens\u00b7vol\u00b7len", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Himmel, die verkl\u00e4rt dort oben Ird'sches schildern.", "tokens": ["Den", "Him\u00b7mel", ",", "die", "ver\u00b7kl\u00e4rt", "dort", "o\u00b7ben", "Ird'\u00b7sches", "schil\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den gro\u00df' und kleinen Hund, den gro\u00df' und kleinen B\u00e4ren,", "tokens": ["Den", "gro\u00df'", "und", "klei\u00b7nen", "Hund", ",", "den", "gro\u00df'", "und", "klei\u00b7nen", "B\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den L\u00f6wen und den Stier siehst du sich dort verkl\u00e4ren.", "tokens": ["Den", "L\u00f6\u00b7wen", "und", "den", "Stier", "siehst", "du", "sich", "dort", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Krone funkelt und die goldne Leier t\u00f6nt;", "tokens": ["Die", "Kro\u00b7ne", "fun\u00b7kelt", "und", "die", "gold\u00b7ne", "Lei\u00b7er", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschen h\u00f6chster Schmuck, wie ist er dort versch\u00f6nt!", "tokens": ["Der", "Men\u00b7schen", "h\u00f6chs\u00b7ter", "Schmuck", ",", "wie", "ist", "er", "dort", "ver\u00b7sch\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wo vom Alterthum ein R\u00e4umchen leer geblieben,", "tokens": ["Doch", "wo", "vom", "Al\u00b7ter\u00b7thum", "ein", "R\u00e4um\u00b7chen", "leer", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hat dort unsre Zeit der Sternkart' eingeschrieben?", "tokens": ["Was", "hat", "dort", "uns\u00b7re", "Zeit", "der", "Stern\u00b7kart'", "ein\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gar sehr Bezeichnendes f\u00fcr unser k\u00fcnstlich Treiben,", "tokens": ["Gar", "sehr", "Be\u00b7zeich\u00b7nen\u00b7des", "f\u00fcr", "un\u00b7ser", "k\u00fcnst\u00b7lich", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das todte, dessen Ruhm soll dort unsterblich bleiben:", "tokens": ["Das", "tod\u00b7te", ",", "des\u00b7sen", "Ruhm", "soll", "dort", "uns\u00b7terb\u00b7lich", "blei\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELAT", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Triangel, Pendeluhr, Luftpump' und Seekompa\u00df,", "tokens": ["Tri\u00b7an\u00b7gel", ",", "Pen\u00b7de\u00b7luhr", ",", "Luft\u00b7pump'", "und", "See\u00b7kom\u00b7pa\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zirkel und Lineal, Fernrohr und Winkelma\u00df;", "tokens": ["Zir\u00b7kel", "und", "Li\u00b7ne\u00b7al", ",", "Fern\u00b7rohr", "und", "Win\u00b7kel\u00b7ma\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Buchdruckerpressen und Elektrisirmaschinen,", "tokens": ["Buch\u00b7dru\u00b7cker\u00b7pres\u00b7sen", "und", "E\u00b7lekt\u00b7ri\u00b7sir\u00b7ma\u00b7schi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und derlei; welcher Blick kann sich erfreun an ihnen?", "tokens": ["Und", "der\u00b7lei", ";", "wel\u00b7cher", "Blick", "kann", "sich", "er\u00b7freun", "an", "ih\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWAT", "NN", "VMFIN", "PRF", "VVINF", "APPR", "PPER", "$."], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Zum Gl\u00fccke sind sie meist halb oder ganz und gar", "tokens": ["Zum", "Gl\u00fc\u00b7cke", "sind", "sie", "meist", "halb", "o\u00b7der", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Augen ohne Glas und Sehrohr unsichtbar.", "tokens": ["Den", "Au\u00b7gen", "oh\u00b7ne", "Glas", "und", "Seh\u00b7rohr", "un\u00b7sicht\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}