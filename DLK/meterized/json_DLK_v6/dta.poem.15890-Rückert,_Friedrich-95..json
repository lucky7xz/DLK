{"dta.poem.15890": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, warum die Welt uns so gar ungleich h\u00e4lt,", "tokens": ["Du", "fragst", ",", "wa\u00b7rum", "die", "Welt", "uns", "so", "gar", "un\u00b7gleich", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alles ihr an dem, am andern nichts gef\u00e4llt.", "tokens": ["Da\u00df", "al\u00b7les", "ihr", "an", "dem", ",", "am", "an\u00b7dern", "nichts", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "$,", "APPRART", "ADJA", "PIS", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Was hilft es, junger Freund, dagegen sich erbo\u00dfen?", "tokens": ["Was", "hilft", "es", ",", "jun\u00b7ger", "Freund", ",", "da\u00b7ge\u00b7gen", "sich", "er\u00b7bo\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PAV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Kopf ist hart genug, dran unsern einzusto\u00dfen.", "tokens": ["Ihr", "Kopf", "ist", "hart", "ge\u00b7nug", ",", "dran", "un\u00b7sern", "ein\u00b7zu\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$,", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Welt auf ihre Art \u00fcbt auch in ihren Sachen", "tokens": ["Die", "Welt", "auf", "ih\u00b7re", "Art", "\u00fcbt", "auch", "in", "ih\u00b7ren", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gerechtigkeit, du mu\u00dft dir nur gerecht sie machen.", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", ",", "du", "mu\u00dft", "dir", "nur", "ge\u00b7recht", "sie", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "An wen sie einmal glaubt, dem wird sie viel verzeihn;", "tokens": ["An", "wen", "sie", "ein\u00b7mal", "glaubt", ",", "dem", "wird", "sie", "viel", "ver\u00b7zeihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sie noch zweifelt, wird sie \u00fcber alles schrei'n.", "tokens": ["Wo", "sie", "noch", "zwei\u00b7felt", ",", "wird", "sie", "\u00fc\u00b7ber", "al\u00b7les", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PIS", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum lerne nur vorerst ihr Zutraun zu verdienen,", "tokens": ["Drum", "ler\u00b7ne", "nur", "vor\u00b7erst", "ihr", "Zu\u00b7traun", "zu", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis sie gehorchen dir, bequeme du dich ihnen.", "tokens": ["Bis", "sie", "ge\u00b7hor\u00b7chen", "dir", ",", "be\u00b7que\u00b7me", "du", "dich", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}