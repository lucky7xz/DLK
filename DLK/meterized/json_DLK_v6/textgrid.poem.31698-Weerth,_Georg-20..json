{"textgrid.poem.31698": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es weht schon durch die Gassen", "tokens": ["Es", "weht", "schon", "durch", "die", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der k\u00fchle Abendwind,", "tokens": ["Der", "k\u00fch\u00b7le", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ich bin ein verlassen,", "tokens": ["Und", "ich", "bin", "ein", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein armes Menschenkind.", "tokens": ["Ein", "ar\u00b7mes", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah den Mond erscheinen,", "tokens": ["Ich", "sah", "den", "Mond", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der durch die Wolken bricht,", "tokens": ["Der", "durch", "die", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wei\u00df nicht: soll ich weinen,", "tokens": ["Und", "wei\u00df", "nicht", ":", "soll", "ich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Oder wein ich lieber nicht.", "tokens": ["O\u00b7der", "wein", "ich", "lie\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott gr\u00fc\u00df dich, alte Schenke,", "tokens": ["Gott", "gr\u00fc\u00df", "dich", ",", "al\u00b7te", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit deinem runden Schild;", "tokens": ["Mit", "dei\u00b7nem", "run\u00b7den", "Schild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O gib ein gut Getr\u00e4nke,", "tokens": ["O", "gib", "ein", "gut", "Ge\u00b7tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das meinen Kummer stillt;", "tokens": ["Das", "mei\u00b7nen", "Kum\u00b7mer", "stillt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df balde ich versetzet", "tokens": ["Da\u00df", "bal\u00b7de", "ich", "ver\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ins Land der Tr\u00e4umerein,", "tokens": ["Ins", "Land", "der", "Tr\u00e4u\u00b7me\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo sich das Herz ergetzet", "tokens": ["Wo", "sich", "das", "Herz", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An buntem M\u00e4rchenschein.", "tokens": ["An", "bun\u00b7tem", "M\u00e4r\u00b7chen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da drau\u00dfen rauscht die Erle", "tokens": ["Da", "drau\u00b7\u00dfen", "rauscht", "die", "Er\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und pocht ans Fenster leis,", "tokens": ["Und", "pocht", "ans", "Fens\u00b7ter", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier innen steigt die Perle", "tokens": ["Hier", "in\u00b7nen", "steigt", "die", "Per\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Glase silberwei\u00df.", "tokens": ["Im", "Gla\u00b7se", "sil\u00b7ber\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist der Wein, der m\u00e4hlich", "tokens": ["Das", "ist", "der", "Wein", ",", "der", "m\u00e4h\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das arme Herz begl\u00fcckt", "tokens": ["Das", "ar\u00b7me", "Herz", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mich so zauberselig", "tokens": ["Und", "mich", "so", "zau\u00b7ber\u00b7se\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Erde ganz entr\u00fcckt.", "tokens": ["Der", "Er\u00b7de", "ganz", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von hohen Linden tr\u00e4um ich,", "tokens": ["Von", "ho\u00b7hen", "Lin\u00b7den", "tr\u00e4um", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die auf den Wiesen stehn,", "tokens": ["Die", "auf", "den", "Wie\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gipfel bl\u00fctens\u00e4umig", "tokens": ["Die", "Gip\u00b7fel", "bl\u00fc\u00b7ten\u00b7s\u00e4u\u00b7mig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Mondenglanze wehn.", "tokens": ["Im", "Mon\u00b7den\u00b7glan\u00b7ze", "wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie werfen ihren Schatten", "tokens": ["Sie", "wer\u00b7fen", "ih\u00b7ren", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An Quellen frisch und klar,", "tokens": ["An", "Quel\u00b7len", "frisch", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort tanzt auf gr\u00fcnen Matten", "tokens": ["Dort", "tanzt", "auf", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die leichte Elfenschar.", "tokens": ["Die", "leich\u00b7te", "El\u00b7fen\u00b7schar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es thront die K\u00f6niginne", "tokens": ["Es", "thront", "die", "K\u00f6\u00b7ni\u00b7gin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihres Lagers Rund,", "tokens": ["In", "ih\u00b7res", "La\u00b7gers", "Rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zuckt die gl\u00fchnde Minne", "tokens": ["Der", "zuckt", "die", "gl\u00fchn\u00b7de", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um Wang und Rosenmund,", "tokens": ["Um", "Wang", "und", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der leuchtet in den Blicken", "tokens": ["Der", "leuch\u00b7tet", "in", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein blaues Sternenlicht,", "tokens": ["Ein", "blau\u00b7es", "Ster\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sch\u00f6ne Locken nicken", "tokens": ["Und", "sch\u00f6\u00b7ne", "Lo\u00b7cken", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinab in ihr Gesicht.", "tokens": ["Hin\u00b7ab", "in", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die schwebenden Gestalten,", "tokens": ["Die", "schwe\u00b7ben\u00b7den", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sind sie schlank und zart,", "tokens": ["Wie", "sind", "sie", "schlank", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ihren H\u00e4nden halten", "tokens": ["In", "ih\u00b7ren", "H\u00e4n\u00b7den", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie Blumen seltner Art.", "tokens": ["Sie", "Blu\u00b7men", "selt\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um nackte Schultern rauschen", "tokens": ["Um", "nack\u00b7te", "Schul\u00b7tern", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die luft'gen Schleier weit,", "tokens": ["Die", "luft'\u00b7gen", "Schlei\u00b7er", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und \u00fcpp'ge Glieder lauschen", "tokens": ["Und", "\u00fcpp'\u00b7ge", "Glie\u00b7der", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus knappem Seidenkleid.", "tokens": ["Aus", "knap\u00b7pem", "Sei\u00b7den\u00b7kleid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie drehn die kleinen F\u00fc\u00dfe", "tokens": ["Sie", "drehn", "die", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach s\u00fc\u00dfer Melodei", "tokens": ["Nach", "s\u00fc\u00b7\u00dfer", "Me\u00b7lo\u00b7dei"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und winken schnelle Gr\u00fc\u00dfe", "tokens": ["Und", "win\u00b7ken", "schnel\u00b7le", "Gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und huschen rasch vorbei;", "tokens": ["Und", "hu\u00b7schen", "rasch", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ringen und umschlingen", "tokens": ["Sie", "rin\u00b7gen", "und", "um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich mit den Armen hold,", "tokens": ["Sich", "mit", "den", "Ar\u00b7men", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie k\u00fcssen sich und schwingen", "tokens": ["Sie", "k\u00fcs\u00b7sen", "sich", "und", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das volle Lockengold.", "tokens": ["Das", "vol\u00b7le", "Lo\u00b7cken\u00b7gold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie singen wundert\u00f6nig,", "tokens": ["Sie", "sin\u00b7gen", "wun\u00b7der\u00b7t\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie singen hell und rein \u2013", "tokens": ["Sie", "sin\u00b7gen", "hell", "und", "rein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ich will euer K\u00f6nig,", "tokens": ["Und", "ich", "will", "eu\u00b7er", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Elfenkinder, sein.", "tokens": ["Ihr", "El\u00b7fen\u00b7kin\u00b7der", ",", "sein", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An bl\u00fchenden Lindenb\u00e4umen,", "tokens": ["An", "bl\u00fc\u00b7hen\u00b7den", "Lin\u00b7den\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In stiller Mondenpracht,", "tokens": ["In", "stil\u00b7ler", "Mon\u00b7den\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da will ich lieben, tr\u00e4umen", "tokens": ["Da", "will", "ich", "lie\u00b7ben", ",", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit euch die ganze Nacht.", "tokens": ["Mit", "euch", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ha, wenn auf zarter Lippen", "tokens": ["Ha", ",", "wenn", "auf", "zar\u00b7ter", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Hellgl\u00fchndem Purpursamt,", "tokens": ["Hell\u00b7gl\u00fchn\u00b7dem", "Pur\u00b7pur\u00b7samt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den s\u00fc\u00dfen Tau zu nippen,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Tau", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein wildes K\u00fcssen flammt,", "tokens": ["Mein", "wil\u00b7des", "K\u00fcs\u00b7sen", "flammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sinkst du K\u00f6niginne", "tokens": ["Da", "sinkst", "du", "K\u00f6\u00b7ni\u00b7gin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herab von deinem Thron \u2013", "tokens": ["Her\u00b7ab", "von", "dei\u00b7nem", "Thron", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es siegt mit seiner Minne", "tokens": ["Es", "siegt", "mit", "sei\u00b7ner", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der k\u00fchne Erdensohn!", "tokens": ["Der", "k\u00fch\u00b7ne", "Er\u00b7den\u00b7sohn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da wacht' ich auf \u2013 es gingen", "tokens": ["Da", "wacht'", "ich", "auf", "\u2013", "es", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schenkenlichter aus,", "tokens": ["Die", "Schen\u00b7ken\u00b7lich\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Lachen und mit Singen", "tokens": ["Mit", "La\u00b7chen", "und", "mit", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zog jeder Gast nach Haus.", "tokens": ["Zog", "je\u00b7der", "Gast", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Nacht lag auf den Gassen,", "tokens": ["Die", "Nacht", "lag", "auf", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kalt pfiff vorbei der Wind,", "tokens": ["Kalt", "pfiff", "vor\u00b7bei", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ich war ein verlassen,", "tokens": ["Und", "ich", "war", "ein", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein armes Menschenkind.", "tokens": ["Ein", "ar\u00b7mes", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}