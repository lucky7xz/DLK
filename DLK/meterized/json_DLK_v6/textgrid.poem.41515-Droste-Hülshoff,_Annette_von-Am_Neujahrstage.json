{"textgrid.poem.41515": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Neujahrstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Auge sinkt, die Sinne wollen scheiden:", "tokens": ["Das", "Au\u00b7ge", "sinkt", ",", "die", "Sin\u00b7ne", "wol\u00b7len", "schei\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbfahr wohl, du altes Jahr, mit Freud' und Leiden!", "tokens": ["\u00bb", "fahr", "wohl", ",", "du", "al\u00b7tes", "Jahr", ",", "mit", "Freud'", "und", "Lei\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel schenkt ein neues, wenn er will.\u00ab", "tokens": ["Der", "Him\u00b7mel", "schenkt", "ein", "neu\u00b7es", ",", "wenn", "er", "will", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,", "KOUS", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So neigt der Mensch sein Haupt an Gottes G\u00fcte,", "tokens": ["So", "neigt", "der", "Mensch", "sein", "Haupt", "an", "Got\u00b7tes", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die alte f\u00e4llt, es keimt die neue Bl\u00fcte", "tokens": ["Die", "al\u00b7te", "f\u00e4llt", ",", "es", "keimt", "die", "neu\u00b7e", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus Eis und Schnee, die Pflanze Gottes, still.", "tokens": ["Aus", "Eis", "und", "Schnee", ",", "die", "Pflan\u00b7ze", "Got\u00b7tes", ",", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Nacht entflieht, der Schlaf den Augenlidern:", "tokens": ["Die", "Nacht", "ent\u00b7flieht", ",", "der", "Schlaf", "den", "Au\u00b7gen\u00b7li\u00b7dern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwillkommen junger Tag mit deinen Br\u00fcdern!", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "jun\u00b7ger", "Tag", "mit", "dei\u00b7nen", "Br\u00fc\u00b7dern", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo bist du denn, du liebes neues Jahr?\u00ab", "tokens": ["Wo", "bist", "du", "denn", ",", "du", "lie\u00b7bes", "neu\u00b7es", "Jahr", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da steht es in des Morgenlichtes Prangen,", "tokens": ["Da", "steht", "es", "in", "des", "Mor\u00b7gen\u00b7lich\u00b7tes", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es hat die ganze Erde rings umfangen", "tokens": ["Es", "hat", "die", "gan\u00b7ze", "Er\u00b7de", "rings", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schaut ihm in die Augen ernst und klar.", "tokens": ["Und", "schaut", "ihm", "in", "die", "Au\u00b7gen", "ernst", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbgegr\u00fc\u00dft du Menschenherz mit deinen Schw\u00e4chen,", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00dft", "du", "Men\u00b7schen\u00b7herz", "mit", "dei\u00b7nen", "Schw\u00e4\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Herz voll Kraft und Reue und Gebrechen,", "tokens": ["Du", "Herz", "voll", "Kraft", "und", "Reu\u00b7e", "und", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bringe neue Pr\u00fcfungszeit vom Herrn!\u00ab", "tokens": ["Ich", "brin\u00b7ge", "neu\u00b7e", "Pr\u00fc\u00b7fungs\u00b7zeit", "vom", "Herrn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbgegr\u00fc\u00dft du neues Jahr mit deinen Freuden,", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00dft", "du", "neu\u00b7es", "Jahr", "mit", "dei\u00b7nen", "Freu\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Leben ist so s\u00fc\u00df, und w\u00e4ren's Leiden,", "tokens": ["Das", "Le\u00b7ben", "ist", "so", "s\u00fc\u00df", ",", "und", "w\u00e4\u00b7ren's", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ach, alles nimmt man mit dem Leben gern!\u00ab", "tokens": ["Ach", ",", "al\u00b7les", "nimmt", "man", "mit", "dem", "Le\u00b7ben", "gern", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbo Menschenherz, wie ist dein Haus zerfallen!", "tokens": ["\u00bb", "o", "Men\u00b7schen\u00b7herz", ",", "wie", "ist", "dein", "Haus", "zer\u00b7fal\u00b7len", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie magst du doch, du Erbe jener Hallen,", "tokens": ["Wie", "magst", "du", "doch", ",", "du", "Er\u00b7be", "je\u00b7ner", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "PPER", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie magst du wohnen in so w\u00fcstem Graus!\u00ab", "tokens": ["Wie", "magst", "du", "woh\u00b7nen", "in", "so", "w\u00fcs\u00b7tem", "Graus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbo neues Jahr, ich bin ja nie daheime!", "tokens": ["\u00bb", "o", "neu\u00b7es", "Jahr", ",", "ich", "bin", "ja", "nie", "da\u00b7hei\u00b7me", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Wandersmann durchzieh ich ferne R\u00e4ume,", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", "durch\u00b7zieh", "ich", "fer\u00b7ne", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es hei\u00dft wohl so, es ist doch nicht mein Haus.\u00ab", "tokens": ["Es", "hei\u00dft", "wohl", "so", ",", "es", "ist", "doch", "nicht", "mein", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbo Menschenherz, was hast du denn zu treiben,", "tokens": ["\u00bb", "o", "Men\u00b7schen\u00b7herz", ",", "was", "hast", "du", "denn", "zu", "trei\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du nicht kannst in deiner Heimat bleiben", "tokens": ["Da\u00df", "du", "nicht", "kannst", "in", "dei\u00b7ner", "Hei\u00b7mat", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und halten sie bereit f\u00fcr deinen Herrn?\u00ab", "tokens": ["Und", "hal\u00b7ten", "sie", "be\u00b7reit", "f\u00fcr", "dei\u00b7nen", "Herrn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbo neues Jahr, du mu\u00dft noch viel erfahren;", "tokens": ["\u00bb", "o", "neu\u00b7es", "Jahr", ",", "du", "mu\u00dft", "noch", "viel", "er\u00b7fah\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kennst du nicht Krieg und Seuchen und Gefahren!", "tokens": ["Kennst", "du", "nicht", "Krieg", "und", "Seu\u00b7chen", "und", "Ge\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und meine liebsten Sorgen wohnen fern.\u00ab", "tokens": ["Und", "mei\u00b7ne", "liebs\u00b7ten", "Sor\u00b7gen", "woh\u00b7nen", "fern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbo Menschenherz, kannst du denn alles zwingen?", "tokens": ["\u00bb", "o", "Men\u00b7schen\u00b7herz", ",", "kannst", "du", "denn", "al\u00b7les", "zwin\u00b7gen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df dir der Himmel Tau und Regen bringen,", "tokens": ["Mu\u00df", "dir", "der", "Him\u00b7mel", "Tau", "und", "Re\u00b7gen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00f6ffnet sich die Erde deinem Wort?\u00ab", "tokens": ["Und", "\u00f6ff\u00b7net", "sich", "die", "Er\u00b7de", "dei\u00b7nem", "Wort", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbach nein! ich kann nur sehn und mich betr\u00fcben,", "tokens": ["\u00bb", "ach", "nein", "!", "ich", "kann", "nur", "sehn", "und", "mich", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PTKANT", "$.", "PPER", "VMFIN", "ADV", "VVINF", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es ist noch leider nach wie vor geblieben", "tokens": ["Es", "ist", "noch", "lei\u00b7der", "nach", "wie", "vor", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "KOKOM", "APPR", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und geht die angewiesnen Wege fort.\u00ab", "tokens": ["Und", "geht", "die", "an\u00b7ge\u00b7wi\u00b7es\u00b7nen", "We\u00b7ge", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbo t\u00fcckisch Herz, du willst es nur nicht sagen,", "tokens": ["\u00bb", "o", "t\u00fc\u00b7ckisch", "Herz", ",", "du", "willst", "es", "nur", "nicht", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJD", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt hat ihre Zelte aufgeschlagen,", "tokens": ["Die", "Welt", "hat", "ih\u00b7re", "Zel\u00b7te", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin labt sie dich mit ihrem Taumelwein.\u00ab", "tokens": ["Drin", "labt", "sie", "dich", "mit", "ih\u00b7rem", "Tau\u00b7mel\u00b7wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.5": {"text": "Sein Schaum hei\u00dft S\u00fcnde und sein Trank Gereuen,", "tokens": ["Sein", "Schaum", "hei\u00dft", "S\u00fcn\u00b7de", "und", "sein", "Trank", "Ge\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zudem l\u00e4\u00dft mich die Sorge nie allein.\u00ab", "tokens": ["Zu\u00b7dem", "l\u00e4\u00dft", "mich", "die", "Sor\u00b7ge", "nie", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbh\u00f6r an, o Herz, ich will es dir verk\u00fcnden,", "tokens": ["\u00bb", "h\u00f6r", "an", ",", "o", "Herz", ",", "ich", "will", "es", "dir", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "FM", "NN", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willst du den Pfeil in seinem Fluge binden?", "tokens": ["Willst", "du", "den", "Pfeil", "in", "sei\u00b7nem", "Flu\u00b7ge", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du siehst sein Ziel nicht, hat er darum keins?\u00ab", "tokens": ["Du", "siehst", "sein", "Ziel", "nicht", ",", "hat", "er", "da\u00b7rum", "keins", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "VAFIN", "PPER", "PAV", "PIAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbich wei\u00df es wohl, uns ist ein Tag bereitet,", "tokens": ["\u00bb", "ich", "wei\u00df", "es", "wohl", ",", "uns", "ist", "ein", "Tag", "be\u00b7rei\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da wird es klar, wie alles wohl geleitet,", "tokens": ["Da", "wird", "es", "klar", ",", "wie", "al\u00b7les", "wohl", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und all die tausend Ziele dennoch eins.\u00ab", "tokens": ["Und", "all", "die", "tau\u00b7send", "Zie\u00b7le", "den\u00b7noch", "eins", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "ART", "CARD", "NN", "ADV", "PIS", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbo Herz, du bist von Torheit ganz befangen!", "tokens": ["\u00bb", "o", "Herz", ",", "du", "bist", "von", "Tor\u00b7heit", "ganz", "be\u00b7fan\u00b7gen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "wo