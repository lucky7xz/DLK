eter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00fcfte sie mit deinen Seufzern aus!", "tokens": ["Und", "l\u00fcf\u00b7te", "sie", "mit", "dei\u00b7nen", "Seuf\u00b7zern", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und willst du treu die Blicke aufw\u00e4rts wenden,", "tokens": ["Und", "willst", "du", "treu", "die", "Bli\u00b7cke", "auf\u00b7w\u00e4rts", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird der Herr sein heilig Bild dir senden,", "tokens": ["So", "wird", "der", "Herr", "sein", "hei\u00b7lig", "Bild", "dir", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du es hegst, in Glauben und Vertraun,", "tokens": ["Da\u00df", "du", "es", "hegst", ",", "in", "Glau\u00b7ben", "und", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann darf ich einst an deinem Kranze winden,", "tokens": ["Dann", "darf", "ich", "einst", "an", "dei\u00b7nem", "Kran\u00b7ze", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sollte dich das neue Jahr noch finden,", "tokens": ["Und", "soll\u00b7te", "dich", "das", "neu\u00b7e", "Jahr", "noch", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So m\u00f6g' es in ein Gottesh\u00e4uslein schaun!\u00ab", "tokens": ["So", "m\u00f6g'", "es", "in", "ein", "Got\u00b7tes\u00b7h\u00e4us\u00b7lein", "schaun", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}