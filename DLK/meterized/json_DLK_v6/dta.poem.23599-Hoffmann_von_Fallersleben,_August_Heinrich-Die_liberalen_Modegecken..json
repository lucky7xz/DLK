{"dta.poem.23599": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die liberalen Modegecken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du schw\u00f6rest Allem Untergang", "tokens": ["Du", "schw\u00f6\u00b7rest", "Al\u00b7lem", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was je dich hemmt in deinem Frieden,", "tokens": ["Was", "je", "dich", "hemmt", "in", "dei\u00b7nem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verfluchest den Gewissenszwang", "tokens": ["Ver\u00b7flu\u00b7chest", "den", "Ge\u00b7wis\u00b7sens\u00b7zwang"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Geistesdruck hienieden;", "tokens": ["Und", "Geis\u00b7tes\u00b7druck", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du schreist nach Freiheit, schreist nach Recht", "tokens": ["Du", "schreist", "nach", "Frei\u00b7heit", ",", "schreist", "nach", "Recht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Anblick gro\u00dfer Kriegesheere,", "tokens": ["Im", "An\u00b7blick", "gro\u00b7\u00dfer", "Krie\u00b7ges\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O du gro\u00dfm\u00e4uliges Geschlecht,", "tokens": ["O", "du", "gro\u00df\u00b7m\u00e4u\u00b7li\u00b7ges", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und dich beherrscht die Schneiderscheere!", "tokens": ["Und", "dich", "be\u00b7herrscht", "die", "Schnei\u00b7der\u00b7schee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}