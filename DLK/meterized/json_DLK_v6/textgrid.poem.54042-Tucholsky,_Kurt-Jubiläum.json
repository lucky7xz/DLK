{"textgrid.poem.54042": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Jubil\u00e4um", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid ihr alle noch da \u2013?", "tokens": ["Seid", "ihr", "al\u00b7le", "noch", "da", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "PPER", "PIS", "ADV", "ADV", "$(", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ja \u2013?", "tokens": ["Ja", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PTKANT", "$(", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Immer dieselben Offiziere,", "tokens": ["Im\u00b7mer", "die\u00b7sel\u00b7ben", "Of\u00b7fi\u00b7zie\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "dieselben Verschw\u00f6rungs-Kavaliere,", "tokens": ["die\u00b7sel\u00b7ben", "Ver\u00b7schw\u00f6\u00b7rungs\u00b7Ka\u00b7va\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "unf\u00e4hig, etwas Gescheites zu werden,", "tokens": ["un\u00b7f\u00e4\u00b7hig", ",", "et\u00b7was", "Ge\u00b7schei\u00b7tes", "zu", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "ewige, ewige Landsknechte auf Erden;", "tokens": ["e\u00b7wi\u00b7ge", ",", "e\u00b7wi\u00b7ge", "Lands\u00b7knech\u00b7te", "auf", "Er\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "dieselbe Wichtigkeit mit \u203aKurieren\u2039,", "tokens": ["die\u00b7sel\u00b7be", "Wich\u00b7tig\u00b7keit", "mit", "\u203a", "Ku\u00b7rie\u00b7ren", "\u2039", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "derselbe Rummel im Organisieren . . .", "tokens": ["der\u00b7sel\u00b7be", "Rum\u00b7mel", "im", "Or\u00b7ga\u00b7ni\u00b7sie\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn im Felde das Saufen . . . das gute Essen . . .", "tokens": ["Denn", "im", "Fel\u00b7de", "das", "Sau\u00b7fen", ".", ".", ".", "das", "gu\u00b7te", "Es\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$.", "$.", "$.", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "das k\u00f6nnen die Herren nun mal nicht vergessen.", "tokens": ["das", "k\u00f6n\u00b7nen", "die", "Her\u00b7ren", "nun", "mal", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Immer noch Ansprachen mit Hurra . . .", "tokens": ["Im\u00b7mer", "noch", "An\u00b7spra\u00b7chen", "mit", "Hur\u00b7ra", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "NE", "$.", "$.", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Seid ihr auch alle da \u2013?", "tokens": ["Seid", "ihr", "auch", "al\u00b7le", "da", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "PPER", "ADV", "PIS", "ADV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ja\u2013?", "tokens": ["Ja", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PTKANT", "$(", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Ihr habt so viel Geld. Von K\u00f6ln bis Berlin", "tokens": ["Ihr", "habt", "so", "viel", "Geld", ".", "Von", "K\u00f6ln", "bis", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$.", "APPR", "NE", "APPR", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "spenden die notleidenden Industrien;", "tokens": ["spen\u00b7den", "die", "not\u00b7lei\u00b7den\u00b7den", "In\u00b7dust\u00b7ri\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "und es spendet auch voller Saft und Kraft", "tokens": ["und", "es", "spen\u00b7det", "auch", "vol\u00b7ler", "Saft", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "die arme, notleidende Landwirtschaft.", "tokens": ["die", "ar\u00b7me", ",", "not\u00b7lei\u00b7den\u00b7de", "Land\u00b7wirt\u00b7schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und mit diesem Geld ist es euch gelungen:", "tokens": ["Und", "mit", "die\u00b7sem", "Geld", "ist", "es", "euch", "ge\u00b7lun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "ihr habt auch scharenweise die Jungen.", "tokens": ["ihr", "habt", "auch", "scha\u00b7ren\u00b7wei\u00b7se", "die", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und was f\u00fcr Jugend!", "tokens": ["Und", "was", "f\u00fcr", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die mu\u00df man sehen,", "tokens": ["Die", "mu\u00df", "man", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "die Uniformen, die mit euch gehen:", "tokens": ["die", "U\u00b7nif\u00b7or\u00b7men", ",", "die", "mit", "euch", "ge\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Eine verbr\u00fcllte, verhetzte Masse,", "tokens": ["Ei\u00b7ne", "ver\u00b7br\u00fcll\u00b7te", ",", "ver\u00b7hetz\u00b7te", "Mas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "mit der ganzen Sehnsucht zur blonden Rasse,", "tokens": ["mit", "der", "gan\u00b7zen", "Sehn\u00b7sucht", "zur", "blon\u00b7den", "Ras\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "die nun einmal jeden entflammt,", "tokens": ["die", "nun", "ein\u00b7mal", "je\u00b7den", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "der aus Promenadenmischungen stammt.", "tokens": ["der", "aus", "Pro\u00b7me\u00b7na\u00b7den\u00b7mi\u00b7schun\u00b7gen", "stammt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Die Gehirne verkleistert im achtzehnten Jahr,", "tokens": ["Die", "Ge\u00b7hir\u00b7ne", "ver\u00b7kleis\u00b7tert", "im", "acht\u00b7zehn\u00b7ten", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Deutschland im Maul und Schuppen im Haar . . .", "tokens": ["Deutschland", "im", "Maul", "und", "Schup\u00b7pen", "im", "Haar", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "NN", "APPRART", "NN", "$.", "$.", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Abschaum der B\u00fcrger vom Belt bis zum Rhein \u2013", "tokens": ["Ab\u00b7schaum", "der", "B\u00fcr\u00b7ger", "vom", "Belt", "bis", "zum", "Rhein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "APPR", "APPRART", "NE", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.17": {"text": "Und das soll Deutschlands Zukunft sein \u2013?", "tokens": ["Und", "das", "soll", "Deutschlands", "Zu\u00b7kunft", "sein", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VMFIN", "NE", "NN", "VAINF", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Euch st\u00f6rt doch kein republikanisches Schwein?", "tokens": ["Euch", "st\u00f6rt", "doch", "kein", "re\u00b7pub\u00b7li\u00b7ka\u00b7ni\u00b7sches", "Schwein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Nein \u2013?", "tokens": ["Nein", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PTKANT", "$(", "$."], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Die Republikaner sehen in Ruh", "tokens": ["Die", "Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", "se\u00b7hen", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "euerm klirrenden Get\u00fcmmel zu.", "tokens": ["eu\u00b7erm", "klir\u00b7ren\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kein Staatsanwalt t\u00e4t ein W\u00f6rtlein sagen \u2013", "tokens": ["Kein", "Staats\u00b7an\u00b7walt", "t\u00e4t", "ein", "W\u00f6rt\u00b7lein", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "er mu\u00df ja die Kommunisten jagen.", "tokens": ["er", "mu\u00df", "ja", "die", "Kom\u00b7mu\u00b7nis\u00b7ten", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und sie sehen nicht, was in der Reichswehr geschieht . . .", "tokens": ["Und", "sie", "se\u00b7hen", "nicht", ",", "was", "in", "der", "Reichs\u00b7wehr", "ge\u00b7schieht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "Es ist immer dasselbe alte Lied:", "tokens": ["Es", "ist", "im\u00b7mer", "das\u00b7sel\u00b7be", "al\u00b7te", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der B\u00fcrger hofft. Und zieht einen Flunsch.", "tokens": ["Der", "B\u00fcr\u00b7ger", "hofft", ".", "Und", "zieht", "ei\u00b7nen", "Flunsch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und hat im ganzen nur einen Wunsch:", "tokens": ["Und", "hat", "im", "gan\u00b7zen", "nur", "ei\u00b7nen", "Wunsch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Es soll sich nichts \u00e4ndern. Die Bahnen solln gehn.", "tokens": ["Es", "soll", "sich", "nichts", "\u00e4n\u00b7dern", ".", "Die", "Bah\u00b7nen", "solln", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIS", "VVINF", "$.", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Er will ins Gesch\u00e4ft, um Viertel zehn . . .", "tokens": ["Er", "will", "ins", "Ge\u00b7sch\u00e4ft", ",", "um", "Vier\u00b7tel", "zehn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "$,", "KOUI", "NN", "CARD", "$.", "$.", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Das ist schon wahr. Das mu\u00df man begreifen.", "tokens": ["Das", "ist", "schon", "wahr", ".", "Das", "mu\u00df", "man", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$.", "PDS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ihr habt auch schon recht, darauf zu pfeifen.", "tokens": ["Ihr", "habt", "auch", "schon", "recht", ",", "da\u00b7rauf", "zu", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Ihr verge\u00dft nur: die Leute eurer Partie", "tokens": ["Ihr", "ver\u00b7ge\u00dft", "nur", ":", "die", "Leu\u00b7te", "eu\u00b7rer", "Par\u00b7tie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "sind genau dieselben B\u00fcrger wie die!", "tokens": ["sind", "ge\u00b7nau", "die\u00b7sel\u00b7ben", "B\u00fcr\u00b7ger", "wie", "die", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PDAT", "NN", "KOKOM", "ART", "$."], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.15": {"text": "Nur lauter. Nur d\u00fcmmer. Nur mit mehr Geschrei.", "tokens": ["Nur", "lau\u00b7ter", ".", "Nur", "d\u00fcm\u00b7mer", ".", "Nur", "mit", "mehr", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "ADJD", "$.", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und was gerne pr\u00fcgelt, ist auch dabei.", "tokens": ["Und", "was", "ger\u00b7ne", "pr\u00fc\u00b7gelt", ",", "ist", "auch", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Seid ihr alle wieder da \u2013?", "tokens": ["Seid", "ihr", "al\u00b7le", "wie\u00b7der", "da", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "PPER", "PIS", "ADV", "ADV", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja \u2013?", "tokens": ["Ja", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PTKANT", "$(", "$."], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Na, dann man los! La\u00dft die Gewehre knallen!", "tokens": ["Na", ",", "dann", "man", "los", "!", "La\u00dft", "die", "Ge\u00b7weh\u00b7re", 