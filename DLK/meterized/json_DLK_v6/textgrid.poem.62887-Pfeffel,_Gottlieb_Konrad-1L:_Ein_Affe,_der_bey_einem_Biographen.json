{"textgrid.poem.62887": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Affe, der bey einem Biographen", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Affe, der bey einem Biographen", "tokens": ["Ein", "Af\u00b7fe", ",", "der", "bey", "ei\u00b7nem", "Bio\u00b7gra\u00b7phen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Famulus gedient, zerbrach sein Joch,", "tokens": ["Als", "Fa\u00b7mu\u00b7lus", "ge\u00b7dient", ",", "zer\u00b7brach", "sein", "Joch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kam an des L\u00f6wen Hof und ward wie alle Sklaven", "tokens": ["Kam", "an", "des", "L\u00f6\u00b7wen", "Hof", "und", "ward", "wie", "al\u00b7le", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN", "KON", "VAFIN", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Schmeichler, der im Staube kroch.", "tokens": ["Ein", "Schmeich\u00b7ler", ",", "der", "im", "Stau\u00b7be", "kroch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr K\u00f6nig, sprach er einst im Ton des Patrioten,", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "sprach", "er", "einst", "im", "Ton", "des", "Pat\u00b7ri\u00b7o\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie k\u00f6mmt es, da\u00df kein Annalist,", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "da\u00df", "kein", "An\u00b7na\u00b7list", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Sammler gro\u00dfer Anekdoten,", "tokens": ["Kein", "Samm\u00b7ler", "gro\u00b7\u00dfer", "A\u00b7nek\u00b7do\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In deinem Reich bestellet ist?", "tokens": ["In", "dei\u00b7nem", "Reich", "be\u00b7stel\u00b7let", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie manchen sch\u00f6nen Zug von Tapferkeit und Treue,", "tokens": ["Wie", "man\u00b7chen", "sch\u00f6\u00b7nen", "Zug", "von", "Tap\u00b7fer\u00b7keit", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von Weisheit, Gro\u00dfmuth, edler Reue,", "tokens": ["Von", "Weis\u00b7heit", ",", "Gro\u00df\u00b7muth", ",", "ed\u00b7ler", "Reu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von Mutterpflicht, Geduld und stiller Fr\u00f6mmigkeit", "tokens": ["Von", "Mut\u00b7ter\u00b7pflicht", ",", "Ge\u00b7duld", "und", "stil\u00b7ler", "Fr\u00f6m\u00b7mig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verschlingt der Ocean der Zeit!", "tokens": ["Ver\u00b7schlingt", "der", "O\u00b7cean", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Auf deinen Wink bin ich bereit,", "tokens": ["Auf", "dei\u00b7nen", "Wink", "bin", "ich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die hohen Tugenden, die Krieg und Frieden", "tokens": ["Die", "ho\u00b7hen", "Tu\u00b7gen\u00b7den", ",", "die", "Krieg", "und", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "In unserm Staat erzeugt, vom libyschen Alciden,", "tokens": ["In", "un\u00b7serm", "Staat", "er\u00b7zeugt", ",", "vom", "li\u00b7by\u00b7schen", "Al\u00b7ci\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "(hier b\u00fcckte sich der Biograph)", "tokens": ["(", "hier", "b\u00fcck\u00b7te", "sich", "der", "Bio\u00b7graph", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Bis zum bescheidnen, frommen Schaf,", "tokens": ["Bis", "zum", "be\u00b7scheid\u00b7nen", ",", "from\u00b7men", "Schaf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In thierischen Ephemeriden,", "tokens": ["In", "thie\u00b7ri\u00b7schen", "E\u00b7phe\u00b7me\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der grauen Ewigkeit zu weyhn.", "tokens": ["Der", "grau\u00b7en", "E\u00b7wig\u00b7keit", "zu", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Kerl, fiel der Gro\u00dfsultan ihm ein,", "tokens": ["Kerl", ",", "fiel", "der", "Gro\u00df\u00b7sul\u00b7tan", "ihm", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Du schwatzest wie ein Mensch aus den polierten Staaten", "tokens": ["Du", "schwat\u00b7zest", "wie", "ein", "Mensch", "aus", "den", "po\u00b7lier\u00b7ten", "Staa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Des Occidents, wo gute Thaten", "tokens": ["Des", "Oc\u00b7ci\u00b7dents", ",", "wo", "gu\u00b7te", "Tha\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So selten sind, da\u00df man sie z\u00e4hlen kann:", "tokens": ["So", "sel\u00b7ten", "sind", ",", "da\u00df", "man", "sie", "z\u00e4h\u00b7len", "kann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "R\u00fchrt deine Faust hier nur den Griffel an,", "tokens": ["R\u00fchrt", "dei\u00b7ne", "Faust", "hier", "nur", "den", "Grif\u00b7fel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "So la\u00df ich dich lebendig braten.", "tokens": ["So", "la\u00df", "ich", "dich", "le\u00b7ben\u00b7dig", "bra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}