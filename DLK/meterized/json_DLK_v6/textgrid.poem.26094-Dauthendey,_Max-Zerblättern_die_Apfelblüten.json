{"textgrid.poem.26094": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Zerbl\u00e4ttern die Apfelbl\u00fcten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kleines feines Papier zerbl\u00e4ttern die Apfelbl\u00fcten,", "tokens": ["Wie", "klei\u00b7nes", "fei\u00b7nes", "Pa\u00b7pier", "zer\u00b7bl\u00e4t\u00b7tern", "die", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schier ein Atemhauch entf\u00fchrt sie dir,", "tokens": ["Schier", "ein", "A\u00b7tem\u00b7hauch", "ent\u00b7f\u00fchrt", "sie", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kannst sie mit keiner Hand vorsichtig h\u00fcten.", "tokens": ["Kannst", "sie", "mit", "kei\u00b7ner", "Hand", "vor\u00b7sich\u00b7tig", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sind wie ein rosiger Hauch, der \u00fcber Nacht entstand,", "tokens": ["Sind", "wie", "ein", "ro\u00b7si\u00b7ger", "Hauch", ",", "der", "\u00fc\u00b7ber", "Nacht", "ent\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sie entschweben auch, eh du's gedacht;", "tokens": ["Und", "sie", "ent\u00b7schwe\u00b7ben", "auch", ",", "eh", "du's", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Haben gl\u00fcckliche Augenblicke in die Leere gebracht.", "tokens": ["Ha\u00b7ben", "gl\u00fcck\u00b7li\u00b7che", "Au\u00b7gen\u00b7bli\u00b7cke", "in", "die", "Lee\u00b7re", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+--+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sind wie Liebessekunden fl\u00fcchtig entschwunden.", "tokens": ["Sind", "wie", "Lie\u00b7bes\u00b7se\u00b7kun\u00b7den", "fl\u00fcch\u00b7tig", "ent\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "ADJD", "VVPP", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Waren in Gedanken unendlich gro\u00df, regnen zur Erde lautlos", "tokens": ["Wa\u00b7ren", "in", "Ge\u00b7dan\u00b7ken", "un\u00b7end\u00b7lich", "gro\u00df", ",", "reg\u00b7nen", "zur", "Er\u00b7de", "laut\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "ADJD", "$,", "ADJA", "APPRART", "NN", "ADJD"], "meter": "+-+-+-++--+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Und liegen dir wie ein Bl\u00e4ttlein Papier unscheinbar im Scho\u00df.", "tokens": ["Und", "lie\u00b7gen", "dir", "wie", "ein", "Bl\u00e4tt\u00b7lein", "Pa\u00b7pier", "un\u00b7schein\u00b7bar", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", 