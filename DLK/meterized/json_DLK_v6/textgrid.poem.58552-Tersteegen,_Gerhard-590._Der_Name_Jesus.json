{"textgrid.poem.58552": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "590. Der Name Jesus", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der s\u00fc\u00dfe Name Jesus sei", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Na\u00b7me", "Je\u00b7sus", "sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir ewig s\u00fc\u00df und t\u00e4glich neu,", "tokens": ["Dir", "e\u00b7wig", "s\u00fc\u00df", "und", "t\u00e4g\u00b7lich", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Lieben und zum Leiden kr\u00e4ftig", "tokens": ["Zum", "Lie\u00b7ben", "und", "zum", "Lei\u00b7den", "kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "KON", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zur Vollendung sehr gesch\u00e4ftig!", "tokens": ["Und", "zur", "Vol\u00b7len\u00b7dung", "sehr", "ge\u00b7sch\u00e4f\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}