{"textgrid.poem.44518": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Hamlet", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht ein K\u00f6nigssohn im kalten Norden,", "tokens": ["Es", "geht", "ein", "K\u00f6\u00b7nigs\u00b7sohn", "im", "kal\u00b7ten", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem man des Lebens Urquell dort erschlug,", "tokens": ["Dem", "man", "des", "Le\u00b7bens", "Ur\u00b7quell", "dort", "er\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Thron besitzen, die den Vater morden,", "tokens": ["Den", "Thron", "be\u00b7sit\u00b7zen", ",", "die", "den", "Va\u00b7ter", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Mutter teilt des M\u00f6rders Lust und Trug.", "tokens": ["Die", "Mut\u00b7ter", "teilt", "des", "M\u00f6r\u00b7ders", "Lust", "und", "Trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es mu\u00df Hyperion dem Satyr weichen,", "tokens": ["Es", "mu\u00df", "Hy\u00b7pe\u00b7ri\u00b7on", "dem", "Sa\u00b7tyr", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lumpenk\u00f6nig ist zu schaden klug.", "tokens": ["Der", "Lum\u00b7pen\u00b7k\u00f6\u00b7nig", "ist", "zu", "scha\u00b7den", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In all den angestammten weiten Reichen", "tokens": ["In", "all", "den", "an\u00b7ge\u00b7stamm\u00b7ten", "wei\u00b7ten", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kaum noch ein Ort, der zum Asyl genug.", "tokens": ["Kaum", "noch", "ein", "Ort", ",", "der", "zum", "A\u00b7syl", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Rosenkranz und G\u00fcldenstern, Gervinus \u2013", "tokens": ["Und", "Ro\u00b7sen\u00b7kranz", "und", "G\u00fcl\u00b7dens\u00b7tern", ",", "Ger\u00b7vi\u00b7nus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NE", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Polonius wollt ich sagen, wie ich mu\u00df \u2013", "tokens": ["Po\u00b7lo\u00b7ni\u00b7us", "wollt", "ich", "sa\u00b7gen", ",", "wie", "ich", "mu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Sie spreiten aus ihr langgedehntes Minus,", "tokens": ["Sie", "sprei\u00b7ten", "aus", "ihr", "lang\u00b7ge\u00b7dehn\u00b7tes", "Mi\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Zunge, steilrecht, bildet es zum Plus.", "tokens": ["Die", "Zun\u00b7ge", ",", "steil\u00b7recht", ",", "bil\u00b7det", "es", "zum", "Plus", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch an Ophelien wird es nimmer fehlen,", "tokens": ["Auch", "an", "O\u00b7phe\u00b7li\u00b7en", "wird", "es", "nim\u00b7mer", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Das Herz, zumal bei Weibern, hat nicht Rast,", "tokens": ["Das", "Herz", ",", "zu\u00b7mal", "bei", "Wei\u00b7bern", ",", "hat", "nicht", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN", "$,", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Sturme, wie der Schiffe, so der Seelen,", "tokens": ["Im", "Stur\u00b7me", ",", "wie", "der", "Schif\u00b7fe", ",", "so", "der", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mehrt selbst die reichste Ladung nur die Last.", "tokens": ["Mehrt", "selbst", "die", "reichs\u00b7te", "La\u00b7dung", "nur", "die", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da mahnt denn alle Welt zum Wirken, Handeln!", "tokens": ["Da", "mahnt", "denn", "al\u00b7le", "Welt", "zum", "Wir\u00b7ken", ",", "Han\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der Hebel braucht doch Ort und Statt,", "tokens": ["Al\u00b7lein", "der", "He\u00b7bel", "braucht", "doch", "Ort", "und", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der st\u00e4rkste Sinn mu\u00df sich in Mi\u00dfmut wandeln,", "tokens": ["Der", "st\u00e4rks\u00b7te", "Sinn", "mu\u00df", "sich", "in", "Mi\u00df\u00b7mut", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fehlt erst der Raum zum Anlauf und zur Tat.", "tokens": ["Fehlt", "erst", "der", "Raum", "zum", "An\u00b7lauf", "und", "zur", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}