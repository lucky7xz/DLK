{"textgrid.poem.43793": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Winterabend, das ist die Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winterabend, das ist die Zeit", "tokens": ["Der", "Win\u00b7ter\u00b7a\u00b7bend", ",", "das", "ist", "die", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Arbeit und der Fr\u00f6hlichkeit.", "tokens": ["Der", "Ar\u00b7beit", "und", "der", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Andern n\u00e4hen, stricken und spinnen,", "tokens": ["Wenn", "die", "An\u00b7dern", "n\u00e4\u00b7hen", ",", "stri\u00b7cken", "und", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dann m\u00fcssen wir Kinder auch was beginnen,", "tokens": ["Dann", "m\u00fcs\u00b7sen", "wir", "Kin\u00b7der", "auch", "was", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wir d\u00fcrfen nicht m\u00fc\u00dfig sitzen und ruh'n,", "tokens": ["Wir", "d\u00fcr\u00b7fen", "nicht", "m\u00fc\u00b7\u00dfig", "sit\u00b7zen", "und", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir haben auch unser Theil zu thun:", "tokens": ["Wir", "ha\u00b7ben", "auch", "un\u00b7ser", "Theil", "zu", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir m\u00fcssen zu morgen uns vorbereiten", "tokens": ["Wir", "m\u00fcs\u00b7sen", "zu", "mor\u00b7gen", "uns", "vor\u00b7be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "PPER", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und vollenden unsere Schularbeiten,", "tokens": ["Und", "voll\u00b7en\u00b7den", "un\u00b7se\u00b7re", "Schul\u00b7ar\u00b7bei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und sind wir fertig mit Lesen und Schreiben,", "tokens": ["Und", "sind", "wir", "fer\u00b7tig", "mit", "Le\u00b7sen", "und", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann k\u00f6nnen wir unsere Kurzweil treiben.", "tokens": ["Dann", "k\u00f6n\u00b7nen", "wir", "un\u00b7se\u00b7re", "Kurz\u00b7weil", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und ist der Abend auch noch so lang,", "tokens": ["Und", "ist", "der", "A\u00b7bend", "auch", "noch", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir k\u00fcrzen ihn mit Spiel und Gesang,", "tokens": ["Wir", "k\u00fcr\u00b7zen", "ihn", "mit", "Spiel", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und wer dann ein h\u00fcbsches R\u00e4thsel kann,", "tokens": ["Und", "wer", "dann", "ein", "h\u00fcb\u00b7sches", "R\u00e4th\u00b7sel", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der sagt's, und wir fangen zu rathen an.", "tokens": ["Der", "sagt's", ",", "und", "wir", "fan\u00b7gen", "zu", "ra\u00b7then", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}