{"textgrid.poem.37393": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin ein armer Schreiber nur,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein armer Schreiber nur,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "Schrei\u00b7ber", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab weder Haus noch Acker,", "tokens": ["Hab", "we\u00b7der", "Haus", "noch", "A\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch freut mich jede Kreatur,", "tokens": ["Doch", "freut", "mich", "je\u00b7de", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sogar der Spatz, der Racker.", "tokens": ["So\u00b7gar", "der", "Spatz", ",", "der", "Ra\u00b7cker", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er baut von Federn, Haar und Stroh", "tokens": ["Er", "baut", "von", "Fe\u00b7dern", ",", "Haar", "und", "Stroh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Nest geschwind und fl\u00fcchtig,", "tokens": ["Sein", "Nest", "ge\u00b7schwind", "und", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er denkt, die Sache geht schon so,", "tokens": ["Er", "denkt", ",", "die", "Sa\u00b7che", "geht", "schon", "so", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00f6nheit ist nicht wichtig.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "nicht", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn man den H\u00fchnern Futter streut,", "tokens": ["Wenn", "man", "den", "H\u00fch\u00b7nern", "Fut\u00b7ter", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich mengt er sich dazwischen,", "tokens": ["Gleich", "mengt", "er", "sich", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um schlau und voller R\u00fchrigkeit", "tokens": ["Um", "schlau", "und", "vol\u00b7ler", "R\u00fch\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein K\u00f6rnlein zu erwischen.", "tokens": ["Sein", "K\u00f6rn\u00b7lein", "zu", "er\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Maik\u00e4fer liebt er ungemein,", "tokens": ["Mai\u00b7k\u00e4\u00b7fer", "liebt", "er", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wei\u00df sie zu behandeln;", "tokens": ["Er", "wei\u00df", "sie", "zu", "be\u00b7han\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hackt die Fl\u00fcgel, zwackt das Bein", "tokens": ["Er", "hackt", "die", "Fl\u00fc\u00b7gel", ",", "zwackt", "das", "Bein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und knackt sie auf wie Mandeln.", "tokens": ["Und", "knackt", "sie", "auf", "wie", "Man\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im Kirschenbaum fri\u00dft er verschmitzt", "tokens": ["Im", "Kir\u00b7schen\u00b7baum", "fri\u00dft", "er", "ver\u00b7schmitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fleisch der Beeren gerne;", "tokens": ["Das", "Fleisch", "der", "Bee\u00b7ren", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann hat, wer diesen Baum besitzt,", "tokens": ["Dann", "hat", ",", "wer", "die\u00b7sen", "Baum", "be\u00b7sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachher die sch\u00f6nsten Kerne.", "tokens": ["Nach\u00b7her", "die", "sch\u00f6ns\u00b7ten", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es f\u00e4llt ein Schu\u00df. Der Spatz entfleucht", "tokens": ["Es", "f\u00e4llt", "ein", "Schu\u00df", ".", "Der", "Spatz", "ent\u00b7fleucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ordnet sein Gefieder.", "tokens": ["Und", "ord\u00b7net", "sein", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr heute bleibt er weg vielleicht,", "tokens": ["F\u00fcr", "heu\u00b7te", "bleibt", "er", "weg", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch morgen kommt er wieder.", "tokens": ["Doch", "mor\u00b7gen", "kommt", "er", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und ist es Winterzeit und hat's", "tokens": ["Und", "ist", "es", "Win\u00b7ter\u00b7zeit", "und", "hat's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschneit auf alle D\u00e4cher,", "tokens": ["Ge\u00b7schneit", "auf", "al\u00b7le", "D\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verhungern tut kein rechter Spatz,", "tokens": ["Ver\u00b7hun\u00b7gern", "tut", "kein", "rech\u00b7ter", "Spatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er kennt im Dach die L\u00f6cher.", "tokens": ["Er", "kennt", "im", "Dach", "die", "L\u00f6\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich rief: Spatz komm, ich f\u00fcttre dich!", "tokens": ["Ich", "rief", ":", "Spatz", "komm", ",", "ich", "f\u00fctt\u00b7re", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er fa\u00dft mich scharf ins Auge.", "tokens": ["Er", "fa\u00dft", "mich", "scharf", "ins", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er scheint zu glauben, da\u00df auch ich", "tokens": ["Er", "scheint", "zu", "glau\u00b7ben", ",", "da\u00df", "auch", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$,", "KOUS", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grunde nicht viel tauge.", "tokens": ["Im", "Grun\u00b7de", "nicht", "viel", "tau\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}