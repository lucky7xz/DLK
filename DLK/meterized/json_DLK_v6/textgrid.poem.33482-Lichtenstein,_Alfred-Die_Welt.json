{"textgrid.poem.33482": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Tage stampfen \u00fcber Menschentiere,", "tokens": ["Viel", "Ta\u00b7ge", "stamp\u00b7fen", "\u00fc\u00b7ber", "Men\u00b7schen\u00b7tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In weichen Meeren fliegen Hungerhaie.", "tokens": ["In", "wei\u00b7chen", "Mee\u00b7ren", "flie\u00b7gen", "Hun\u00b7ger\u00b7hai\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Kaffeeh\u00e4usern glitzern K\u00f6pfe, Biere.", "tokens": ["In", "Kaf\u00b7fee\u00b7h\u00e4u\u00b7sern", "glit\u00b7zern", "K\u00f6p\u00b7fe", ",", "Bie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An einem Mann zerrei\u00dfen M\u00e4dchenschreie.", "tokens": ["An", "ei\u00b7nem", "Mann", "zer\u00b7rei\u00b7\u00dfen", "M\u00e4d\u00b7chen\u00b7schrei\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gewitter st\u00fcrzen. W\u00e4lderwinde blaken.", "tokens": ["Ge\u00b7wit\u00b7ter", "st\u00fcr\u00b7zen", ".", "W\u00e4l\u00b7der\u00b7win\u00b7de", "bla\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebete kneten Fraun in d\u00fcnnen H\u00e4nden:", "tokens": ["Ge\u00b7be\u00b7te", "kne\u00b7ten", "Fraun", "in", "d\u00fcn\u00b7nen", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Herr Gott m\u00f6ge einen Engel senden.", "tokens": ["Der", "Herr", "Gott", "m\u00f6\u00b7ge", "ei\u00b7nen", "En\u00b7gel", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Fetzen Mondlicht schimmert in Kloaken.", "tokens": ["Ein", "Fet\u00b7zen", "Mond\u00b7licht", "schim\u00b7mert", "in", "Klo\u00b7a\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Buchleser hocken still auf ihrem Leibe.", "tokens": ["Buch\u00b7le\u00b7ser", "ho\u00b7cken", "still", "auf", "ih\u00b7rem", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Abend taucht die Welt in lila Laugen.", "tokens": ["Ein", "A\u00b7bend", "taucht", "die", "Welt", "in", "li\u00b7la", "Lau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Oberk\u00f6rper schwebt in einer Scheibe.", "tokens": ["Ein", "O\u00b7ber\u00b7k\u00f6r\u00b7per", "schwebt", "in", "ei\u00b7ner", "Schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tief aus dem Hirne sinken seine Augen.", "tokens": ["Tief", "aus", "dem", "Hir\u00b7ne", "sin\u00b7ken", "sei\u00b7ne", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}