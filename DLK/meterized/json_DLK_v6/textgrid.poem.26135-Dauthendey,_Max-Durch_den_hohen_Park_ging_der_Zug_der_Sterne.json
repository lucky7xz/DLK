{"textgrid.poem.26135": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Durch den hohen Park ging der Zug der Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den hohen Park ging der Zug der Sterne,", "tokens": ["Durch", "den", "ho\u00b7hen", "Park", "ging", "der", "Zug", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Jeder Stern eine kleine Laterne durch die B\u00e4ume trug.", "tokens": ["Je\u00b7der", "Stern", "ei\u00b7ne", "klei\u00b7ne", "La\u00b7ter\u00b7ne", "durch", "die", "B\u00e4u\u00b7me", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+----+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und einige gingen unter die B\u00fcsche nieder,", "tokens": ["Und", "ei\u00b7ni\u00b7ge", "gin\u00b7gen", "un\u00b7ter", "die", "B\u00fc\u00b7sche", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo die Nachtigall unter dem Flieder anschlug.", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gall", "un\u00b7ter", "dem", "Flie\u00b7der", "an\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Sonst aber fuhr Dunkelheit durch die L\u00fcfte", "tokens": ["Sonst", "a\u00b7ber", "fuhr", "Dun\u00b7kel\u00b7heit", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und machte sich unter den Sternen breit.", "tokens": ["Und", "mach\u00b7te", "sich", "un\u00b7ter", "den", "Ster\u00b7nen", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die armen Sterne, die \u00fcber der Zeit hingehen,", "tokens": ["Die", "ar\u00b7men", "Ster\u00b7ne", ",", "die", "\u00fc\u00b7ber", "der", "Zeit", "hin\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "F\u00fchlen nur von der Erdenliebe Lied und D\u00fcfte herwehen", "tokens": ["F\u00fch\u00b7len", "nur", "von", "der", "Er\u00b7den\u00b7lie\u00b7be", "Lied", "und", "D\u00fcf\u00b7te", "her\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN", "KON", "NN", "VVINF"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Und haben noch nie wie der Tag deine liebroten Wangen gesehen.", "tokens": ["Und", "ha\u00b7ben", "noch", "nie", "wie", "der", "Tag", "dei\u00b7ne", "lie\u00b7bro\u00b7ten", "Wan\u00b7gen", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "KOKOM", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+---+-+--+-", "measure": "amphibrach.tri.plus"}}}}}