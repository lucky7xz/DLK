{"textgrid.poem.25981": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Manch Tag, der ist wie's Leben lang", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manch Tag, der ist wie's Leben lang,", "tokens": ["Manch", "Tag", ",", "der", "ist", "wie's", "Le\u00b7ben", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VAFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn's Sch\u00e4tzlein fehlt.", "tokens": ["Wenn's", "Sch\u00e4tz\u00b7lein", "fehlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Taumelst wie ein Falter den Weg entlang,", "tokens": ["Tau\u00b7melst", "wie", "ein", "Fal\u00b7ter", "den", "Weg", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "F\u00fchlst dich kahl wie der Baum,", "tokens": ["F\u00fchlst", "dich", "kahl", "wie", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Den der Wind absch\u00e4lt.", "tokens": ["Den", "der", "Wind", "ab\u00b7sch\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ein Span steckt dir im Hals, im steifen,", "tokens": ["Ein", "Span", "steckt", "dir", "im", "Hals", ",", "im", "stei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kannst kein Lied mehr pfeifen,", "tokens": ["Kannst", "kein", "Lied", "mehr", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und alle Weg' durch den Wald gehauen,", "tokens": ["Und", "al\u00b7le", "Weg'", "durch", "den", "Wald", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die sind unendlich anzuschauen.", "tokens": ["Die", "sind", "un\u00b7end\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gehst du zu ein St\u00fcck,", "tokens": ["Gehst", "du", "zu", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Zieht jemand dich am Rock zur\u00fcck.", "tokens": ["Zieht", "je\u00b7mand", "dich", "am", "Rock", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bald bist du tot, wei\u00dft's ganz bestimmt,", "tokens": ["Bald", "bist", "du", "tot", ",", "wei\u00dft's", "ganz", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn niemand dir die Sehnsucht nimmt.", "tokens": ["Wenn", "nie\u00b7mand", "dir", "die", "Sehn\u00b7sucht", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}