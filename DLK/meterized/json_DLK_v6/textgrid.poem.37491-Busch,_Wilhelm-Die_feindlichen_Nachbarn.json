{"textgrid.poem.37491": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die feindlichen Nachbarn", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Maler und ein Musikus,", "tokens": ["Ein", "Ma\u00b7ler", "und", "ein", "Mu\u00b7si\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So Wand an Wand, das gibt Verdru\u00df.", "tokens": ["So", "Wand", "an", "Wand", ",", "das", "gibt", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$,", "PDS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Besonders wird das Saitenspiel", "tokens": ["Be\u00b7son\u00b7ders", "wird", "das", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Nebenmenschen oft zuviel.", "tokens": ["Dem", "Ne\u00b7ben\u00b7men\u00b7schen", "oft", "zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon hat der Maler, sehr verdrossen,", "tokens": ["Schon", "hat", "der", "Ma\u00b7ler", ",", "sehr", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich die Ohren zugeschlossen.", "tokens": ["Sich", "die", "Oh\u00b7ren", "zu\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch so ein rechtes Flageolett", "tokens": ["Doch", "so", "ein", "rech\u00b7tes", "Fla\u00b7ge\u00b7o\u00b7lett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dringt durch. \u2013 Der Maler kriecht ins Bett.", "tokens": ["Dringt", "durch", ".", "\u2013", "Der", "Ma\u00b7ler", "kriecht", "ins", "Bett", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt kommt vermittelst einer Pfeife", "tokens": ["Jetzt", "kommt", "ver\u00b7mit\u00b7telst", "ei\u00b7ner", "Pfei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Malers Racheplan zur Reife.", "tokens": ["Des", "Ma\u00b7lers", "Ra\u00b7che\u00b7plan", "zur", "Rei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Wasser rinnt ins Instrument;", "tokens": ["Das", "Was\u00b7ser", "rinnt", "ins", "Inst\u00b7ru\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Musikus schreit Zapperment!", "tokens": ["Der", "Mu\u00b7si\u00b7kus", "schreit", "Zap\u00b7per\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er kommt, von Rachedurst durchdrungen,", "tokens": ["Er", "kommt", ",", "von", "Ra\u00b7che\u00b7durst", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Atelier hereingesprungen;", "tokens": ["Ins", "A\u00b7te\u00b7lier", "her\u00b7ein\u00b7ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und packt \u2013 ritsch, ratsch! mit k\u00fchner Hand", "tokens": ["Und", "packt", "\u2013", "ritsch", ",", "ratsch", "!", "mit", "k\u00fch\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "ADJD", "$,", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Maler durch die Leinewand.", "tokens": ["Den", "Ma\u00b7ler", "durch", "die", "Lei\u00b7ne\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun geht es los! \u2013 Der Pudel naht", "tokens": ["Nun", "geht", "es", "los", "!", "\u2013", "Der", "Pu\u00b7del", "naht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mischt sich in das Attentat.", "tokens": ["Und", "mischt", "sich", "in", "das", "At\u00b7ten\u00b7tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Musikus k\u00e4mpft unverdrossen", "tokens": ["Der", "Mu\u00b7si\u00b7kus", "k\u00e4mpft", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird mit Sikkativ begossen. \u2013", "tokens": ["Und", "wird", "mit", "Sik\u00b7ka\u00b7tiv", "be\u00b7gos\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Am Ende l\u00e4\u00dft man ab vom Streite;", "tokens": ["Am", "En\u00b7de", "l\u00e4\u00dft", "man", "ab", "vom", "Strei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Pudel freut sich seiner Beute.", "tokens": ["Der", "Pu\u00b7del", "freut", "sich", "sei\u00b7ner", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Verruiniert stehn beide da. \u2013", "tokens": ["Ver\u00b7ru\u00b7i\u00b7niert", "stehn", "bei\u00b7de", "da", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VVFIN", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das tatest du, Frau Musika!", "tokens": ["Das", "ta\u00b7test", "du", ",", "Frau", "Mu\u00b7si\u00b7ka", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}