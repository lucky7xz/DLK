{"textgrid.poem.52204": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auf seine Liebste, bey Uberreichung eines Geschenckes", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, di\u00df schlechte Gold, da\u00df die verme\u00dfne Hand,", "tokens": ["Mein", "Kind", ",", "di\u00df", "schlech\u00b7te", "Gold", ",", "da\u00df", "die", "ver\u00b7me\u00df\u00b7ne", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unverdiente Huld zur K\u00fchnheit angetrieben,", "tokens": ["Die", "un\u00b7ver\u00b7dien\u00b7te", "Huld", "zur", "K\u00fchn\u00b7heit", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor ihre F\u00fcsse legt, zeigt mein best\u00e4ndig Lieben", "tokens": ["Vor", "ih\u00b7re", "F\u00fcs\u00b7se", "legt", ",", "zeigt", "mein", "be\u00b7st\u00e4n\u00b7dig", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und keusche Flammen an. Mir ist kein falscher Brand,", "tokens": ["Und", "keu\u00b7sche", "Flam\u00b7men", "an", ".", "Mir", "ist", "kein", "fal\u00b7scher", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein' aufgew\u00e4rmte Tracht der Schmeucheley bekand.", "tokens": ["Kein'", "auf\u00b7ge\u00b7w\u00e4rm\u00b7te", "Tracht", "der", "Schmeu\u00b7che\u00b7ley", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So rein als dieses Gold selbst in der Glutt geblieben,", "tokens": ["So", "rein", "als", "die\u00b7ses", "Gold", "selbst", "in", "der", "Glutt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PDAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So fest ist ihre Lieb' in meine Brust geschrieben,", "tokens": ["So", "fest", "ist", "ih\u00b7re", "Lieb'", "in", "mei\u00b7ne", "Brust", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Liebe, meiner Ruh gewisses Unter-Pfand.", "tokens": ["Die", "Lie\u00b7be", ",", "mei\u00b7ner", "Ruh", "ge\u00b7wis\u00b7ses", "Un\u00b7ter\u00b7Pfand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie wundre sich nur nicht, o Sonne meines Lebens,", "tokens": ["Sie", "wund\u00b7re", "sich", "nur", "nicht", ",", "o", "Son\u00b7ne", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich di\u00df Sonnen-Kind ihr \u00fcberreichen darf,", "tokens": ["Da\u00df", "ich", "di\u00df", "Son\u00b7nen\u00b7Kind", "ihr", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Strahl, den ihre Huld in meine Seele warf,", "tokens": ["Der", "Strahl", ",", "den", "ih\u00b7re", "Huld", "in", "mei\u00b7ne", "See\u00b7le", "warf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erfordert di\u00df von mir: Sonst w\u00e4r' es nur vergebens", "tokens": ["Er\u00b7for\u00b7dert", "di\u00df", "von", "mir", ":", "Sonst", "w\u00e4r'", "es", "nur", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "APPR", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Krantz, wo Tugenden und Anmutt v\u00f6llig strahlen,", "tokens": ["Den", "Krantz", ",", "wo", "Tu\u00b7gen\u00b7den", "und", "An\u00b7mutt", "v\u00f6l\u00b7lig", "strah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Sch\u00e4tzen Indiens, ja aller Welt, zu zahlen.", "tokens": ["Mit", "Sch\u00e4t\u00b7zen", "In\u00b7di\u00b7ens", ",", "ja", "al\u00b7ler", "Welt", ",", "zu", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$,", "ADV", "PIAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}