{"dta.poem.15870": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich s\u00e4he, wie du dort mir in bekannter", "tokens": ["O", "da\u00df", "ich", "s\u00e4\u00b7he", ",", "wie", "du", "dort", "mir", "in", "be\u00b7kann\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestalt entgegen k\u00e4mst, zu fr\u00fch von hier verbannter,", "tokens": ["Ge\u00b7stalt", "ent\u00b7ge\u00b7gen", "k\u00e4mst", ",", "zu", "fr\u00fch", "von", "hier", "ver\u00b7bann\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVFIN", "$,", "PTKA", "ADJD", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mir unter Klagen heim, zum Himmel heim gesandter!", "tokens": ["Mir", "un\u00b7ter", "Kla\u00b7gen", "heim", ",", "zum", "Him\u00b7mel", "heim", "ge\u00b7sand\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PTKVZ", "$,", "APPRART", "NN", "PTKVZ", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein liebster, sch\u00f6nster Sohn! die Lust empfind' ich schon,", "tokens": ["Mein", "liebs\u00b7ter", ",", "sch\u00f6ns\u00b7ter", "Sohn", "!", "die", "Lust", "emp\u00b7find'", "ich", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "w