{"textgrid.poem.65210": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Ohrring", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Bursch einen Ring in's Ohr mir hing,", "tokens": ["Mein", "Bursch", "ei\u00b7nen", "Ring", "in's", "Ohr", "mir", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als nach der b\u00f6sen Stadt er ging \u2013", "tokens": ["Als", "nach", "der", "b\u00f6\u00b7sen", "Stadt", "er", "ging", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, w\u00e4ren's zwei gewesen!", "tokens": ["Ach", ",", "w\u00e4\u00b7ren's", "zwei", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "CARD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sprach: Du sollst ein Schl\u00f6\u00dfchen sein,", "tokens": ["Er", "sprach", ":", "Du", "sollst", "ein", "Schl\u00f6\u00df\u00b7chen", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df mir kein Schmeichelwort hinein!", "tokens": ["La\u00df", "mir", "kein", "Schmei\u00b7chel\u00b7wort", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, w\u00e4ren's zwei gewesen!", "tokens": ["Ach", ",", "w\u00e4\u00b7ren's", "zwei", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "CARD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Schmeichler gehn zum offnen Ohr,", "tokens": ["Die", "Schmeich\u00b7ler", "gehn", "zum", "off\u00b7nen", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und reden ihm viel S\u00fc\u00dfes vor \u2013", "tokens": ["Und", "re\u00b7den", "ihm", "viel", "S\u00fc\u00b7\u00dfes", "vor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, h\u00e4tt' ich nur zwei Schl\u00f6sser!", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "nur", "zwei", "Schl\u00f6s\u00b7ser", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Bittres auch noch hinterher,", "tokens": ["Und", "Bitt\u00b7res", "auch", "noch", "hin\u00b7ter\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das macht das Herz mir zentnerschwer \u2013", "tokens": ["Das", "macht", "das", "Herz", "mir", "zent\u00b7ner\u00b7schwer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, h\u00e4tt' ich nur zwei Schl\u00f6sser!", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "nur", "zwei", "Schl\u00f6s\u00b7ser", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sagen mir: mein Liebster sei", "tokens": ["Sie", "sa\u00b7gen", "mir", ":", "mein", "Liebs\u00b7ter", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wie ein Schmetterling getreu \u2013", "tokens": ["Mir", "wie", "ein", "Schmet\u00b7ter\u00b7ling", "ge\u00b7treu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, h\u00e4tt' ich gar kein Schl\u00f6\u00dfchen!", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "gar", "kein", "Schl\u00f6\u00df\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann fl\u00f6g's herein zu einem Ohr,", "tokens": ["Dann", "fl\u00f6g's", "her\u00b7ein", "zu", "ei\u00b7nem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gleich hinaus zum andern Thor \u2013", "tokens": ["Und", "gleich", "hin\u00b7aus", "zum", "an\u00b7dern", "Thor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APZR", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, h\u00e4tt' ich gar kein Schl\u00f6\u00dfchen!", "tokens": ["Ach", ",", "h\u00e4tt'", "ich", "gar", "kein", "Schl\u00f6\u00df\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}