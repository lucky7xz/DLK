{"textgrid.poem.37342": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fing man vorzeiten einen Dieb,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fing man vorzeiten einen Dieb,", "tokens": ["Fing", "man", "vor\u00b7zei\u00b7ten", "ei\u00b7nen", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hing man ihn auf mit Schnellbetrieb,", "tokens": ["Hing", "man", "ihn", "auf", "mit", "Schnell\u00b7be\u00b7trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meinte man, er sei verschieden,", "tokens": ["Und", "mein\u00b7te", "man", ",", "er", "sei", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ging man nach Haus und war zufrieden.", "tokens": ["Ging", "man", "nach", "Haus", "und", "war", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Wandrer von der weichen Sorte", "tokens": ["Ein", "Wand\u00b7rer", "von", "der", "wei\u00b7chen", "Sor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kam einst zu solchem Galgenorte", "tokens": ["Kam", "einst", "zu", "sol\u00b7chem", "Gal\u00b7ge\u00b7nor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sah, da\u00df oben einer h\u00e4ngt,", "tokens": ["Und", "sah", ",", "da\u00df", "o\u00b7ben", "ei\u00b7ner", "h\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem k\u00fcrzlich man den Hals verl\u00e4ngt.", "tokens": ["Dem", "k\u00fcrz\u00b7lich", "man", "den", "Hals", "ver\u00b7l\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sogleich, als er ihn baumeln sieht,", "tokens": ["Sog\u00b7leich", ",", "als", "er", "ihn", "bau\u00b7meln", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zerflie\u00dft in Tr\u00e4nen sein Gem\u00fct.", "tokens": ["Zer\u00b7flie\u00dft", "in", "Tr\u00e4\u00b7nen", "sein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich will den armen Schelm begraben,", "tokens": ["Ich", "will", "den", "ar\u00b7men", "Schelm", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denkt er, sonst fressen ihn die Raben.", "tokens": ["Denkt", "er", ",", "sonst", "fres\u00b7sen", "ihn", "die", "Ra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht ohne M\u00fch, doch mit Geschick,", "tokens": ["Nicht", "oh\u00b7ne", "M\u00fch", ",", "doch", "mit", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Klimmt er hinauf und l\u00f6st den Strick;", "tokens": ["Klimmt", "er", "hin\u00b7auf", "und", "l\u00f6st", "den", "Strick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und jener, der im Wind geschwebt,", "tokens": ["Und", "je\u00b7ner", ",", "der", "im", "Wind", "ge\u00b7schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Liegt unten, scheinbar unbelebt.", "tokens": ["Liegt", "un\u00b7ten", ",", "schein\u00b7bar", "un\u00b7be\u00b7lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sieh da, nach \u00c4nderung der Lage", "tokens": ["Sieh", "da", ",", "nach", "\u00c4n\u00b7de\u00b7rung", "der", "La\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Tritt neu die Lebenskraft zutage,", "tokens": ["Tritt", "neu", "die", "Le\u00b7bens\u00b7kraft", "zu\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So da\u00df der gute Delinquent", "tokens": ["So", "da\u00df", "der", "gu\u00b7te", "De\u00b7lin\u00b7quent"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Die Welt ganz deutlich wiederkennt.", "tokens": ["Die", "Welt", "ganz", "deut\u00b7lich", "wie\u00b7der\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Z\u00e4rtlich, als w\u00e4r's der eigne Vetter,", "tokens": ["Z\u00e4rt\u00b7lich", ",", "als", "w\u00e4r's", "der", "eig\u00b7ne", "Vet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOKOM", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Umarmt er seinen Lebensretter", "tokens": ["Um\u00b7armt", "er", "sei\u00b7nen", "Le\u00b7bens\u00b7ret\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nicht einmal, sondern nocheinmal,", "tokens": ["Nicht", "ein\u00b7mal", ",", "son\u00b7dern", "no\u00b7ch\u00b7ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Vor Freude nach so gro\u00dfer Qual.", "tokens": ["Vor", "Freu\u00b7de", "nach", "so", "gro\u00b7\u00dfer", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Mein lieber Mitmensch, sprach der Wandrer,", "tokens": ["Mein", "lie\u00b7ber", "Mit\u00b7mensch", ",", "sprach", "der", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Geh in dich, sei hinf\u00fcr ein andrer,", "tokens": ["Geh", "in", "dich", ",", "sei", "hin\u00b7f\u00fcr", "ein", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "VAFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Zum Anfang f\u00fcr dein neues Leben", "tokens": ["Zum", "An\u00b7fang", "f\u00fcr", "dein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Werd' ich dir jetzt zwei Gulden geben.", "tokens": ["Werd'", "ich", "dir", "jetzt", "zwei", "Gul\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Das Geben tat ihm immer wohl.", "tokens": ["Das", "Ge\u00b7ben", "tat", "ihm", "im\u00b7mer", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Rasch griff er in sein Kamisol,", "tokens": ["Rasch", "griff", "er", "in", "sein", "Ka\u00b7mi\u00b7sol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wo er zur langen Pilgerfahrt", "tokens": ["Wo", "er", "zur", "lan\u00b7gen", "Pil\u00b7ger\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Den vollen S\u00e4ckel aufbewahrt.", "tokens": ["Den", "vol\u00b7len", "S\u00e4\u00b7ckel", "auf\u00b7be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Er sucht und sucht und fand ihn nicht,", "tokens": ["Er", "sucht", "und", "sucht", "und", "fand", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und l\u00e4nger wurde sein Gesicht.", "tokens": ["Und", "l\u00e4n\u00b7ger", "wur\u00b7de", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Er sucht und suchte wie ein Narr,", "tokens": ["Er", "sucht", "und", "such\u00b7te", "wie", "ein", "Narr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Weit wird der Mund, das Auge starr,", "tokens": ["Weit", "wird", "der", "Mund", ",", "das", "Au\u00b7ge", "starr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Bald ist ihm hei\u00df, bald ist ihm kalt.", "tokens": ["Bald", "ist", "ihm", "hei\u00df", ",", "bald", "ist", "ihm", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Der Dieb verschwand in Tannenwald.", "tokens": ["Der", "Dieb", "ver\u00b7schwand", "in", "Tan\u00b7nen\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}