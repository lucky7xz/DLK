{"textgrid.poem.43502": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun s\u00e4useln linde", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun s\u00e4useln linde", "tokens": ["Nun", "s\u00e4u\u00b7seln", "lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Aus Westen die Winde.", "tokens": ["Aus", "Wes\u00b7ten", "die", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schon rieseln die Quellen", "tokens": ["Schon", "rie\u00b7seln", "die", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ins Thal hernieder.", "tokens": ["Ins", "Thal", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Knospen schwellen.", "tokens": ["Die", "Knos\u00b7pen", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der V\u00f6gel Lieder", "tokens": ["Der", "V\u00f6\u00b7gel", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Erschallen wieder.", "tokens": ["Er\u00b7schal\u00b7len", "wie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schneegl\u00f6ckchen l\u00e4uten fern und nah:", "tokens": ["Schnee\u00b7gl\u00f6ck\u00b7chen", "l\u00e4u\u00b7ten", "fern", "und", "nah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Fr\u00fchling ist da, der Fr\u00fchling ist da!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "da", ",", "der", "Fr\u00fch\u00b7ling", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O seht, wie der Fr\u00fchling schaltet und waltet,", "tokens": ["O", "seht", ",", "wie", "der", "Fr\u00fch\u00b7ling", "schal\u00b7tet", "und", "wal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und neues Leben enth\u00fcllt und entfaltet,", "tokens": ["Und", "neu\u00b7es", "Le\u00b7ben", "ent\u00b7h\u00fcllt", "und", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sch\u00f6nes Leben ersinnt und gestaltet!", "tokens": ["Und", "sch\u00f6\u00b7nes", "Le\u00b7ben", "er\u00b7sinnt", "und", "ge\u00b7stal\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit Duft und Farben erquickt und belebt,", "tokens": ["Mit", "Duft", "und", "Far\u00b7ben", "er\u00b7quickt", "und", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Sang und Klang entz\u00fcckt und erhebt,", "tokens": ["Mit", "Sang", "und", "Klang", "ent\u00b7z\u00fcckt", "und", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und segnend \u00fcber Allem schwebt!", "tokens": ["Und", "seg\u00b7nend", "\u00fc\u00b7ber", "Al\u00b7lem", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun la\u00dft uns nicht l\u00e4nger bleiben zu Haus!", "tokens": ["Nun", "la\u00dft", "uns", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir wollen hinaus, ins Freie hinaus!", "tokens": ["Wir", "wol\u00b7len", "hin\u00b7aus", ",", "ins", "Frei\u00b7e", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APZR", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}