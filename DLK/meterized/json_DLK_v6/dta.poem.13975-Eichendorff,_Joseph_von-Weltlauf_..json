{"dta.poem.13975": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Weltlauf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und der Herr hat nichts vergessen,", "tokens": ["Und", "der", "Herr", "hat", "nichts", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was geschehen wird er messen", "tokens": ["Was", "ge\u00b7sche\u00b7hen", "wird", "er", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVPP", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Maa\u00df der Ewigkeit \u2014", "tokens": ["Nach", "dem", "Maa\u00df", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie klein ist doch die Zeit!", "tokens": ["O", "wie", "klein", "ist", "doch", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}