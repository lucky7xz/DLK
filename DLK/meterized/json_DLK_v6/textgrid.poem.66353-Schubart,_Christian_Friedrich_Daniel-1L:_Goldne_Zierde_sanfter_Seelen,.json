{"textgrid.poem.66353": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Goldne Zierde sanfter Seelen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldne Zierde sanfter Seelen,", "tokens": ["Gold\u00b7ne", "Zier\u00b7de", "sanf\u00b7ter", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelsgrazie, mit dir", "tokens": ["Him\u00b7mels\u00b7gra\u00b7zie", ",", "mit", "dir"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Will ich ewig mich verm\u00e4hlen;", "tokens": ["Will", "ich", "e\u00b7wig", "mich", "ver\u00b7m\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O, wie lieblich strahlst du mir", "tokens": ["O", ",", "wie", "lieb\u00b7lich", "strahlst", "du", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus Luisens sanftem Blick", "tokens": ["Aus", "Lu\u00b7i\u00b7sens", "sanf\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Herrlichkeit zur\u00fcck.", "tokens": ["Dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einfalt mit dem Silberschleier,", "tokens": ["Ein\u00b7falt", "mit", "dem", "Sil\u00b7ber\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Unschuld mit dem Rosenflor", "tokens": ["Un\u00b7schuld", "mit", "dem", "Ro\u00b7sen\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandern dir in stiller Feier", "tokens": ["Wan\u00b7dern", "dir", "in", "stil\u00b7ler", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als zwei liebe Schwestern vor.", "tokens": ["Als", "zwei", "lie\u00b7be", "Schwes\u00b7tern", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Engel Gottes freuen sich", "tokens": ["En\u00b7gel", "Got\u00b7tes", "freu\u00b7en", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber dir, und k\u00fcssen dich.", "tokens": ["Ue\u00b7ber", "dir", ",", "und", "k\u00fcs\u00b7sen", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als die Sch\u00f6nheit und die G\u00fcte", "tokens": ["Als", "die", "Sch\u00f6n\u00b7heit", "und", "die", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst im Garten Gottes stand,", "tokens": ["Einst", "im", "Gar\u00b7ten", "Got\u00b7tes", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der erste Vater gl\u00fchte,", "tokens": ["Und", "der", "ers\u00b7te", "Va\u00b7ter", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da sich Eva ihm entwand;", "tokens": ["Da", "sich", "E\u00b7va", "ihm", "ent\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "PPER", "VVFIN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Blicktest du das erstemal", "tokens": ["Blick\u00b7test", "du", "das", "ers\u00b7te\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "As des Weibes Augenstrahl.", "tokens": ["As", "des", "Wei\u00b7bes", "Au\u00b7gen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "G\u00f6ttin \u2013 doch, so sch\u00f6n und milde", "tokens": ["G\u00f6t\u00b7tin", "\u2013", "doch", ",", "so", "sch\u00f6n", "und", "mil\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADV", "$,", "ADV", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dich nie ein Aug' erkannt,", "tokens": ["Hat", "dich", "nie", "ein", "Aug'", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich in Luisens Bilde", "tokens": ["Als", "ich", "in", "Lu\u00b7i\u00b7sens", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich zum erstenmal empfand.", "tokens": ["Dich", "zum", "ers\u00b7ten\u00b7mal", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Still und gro\u00df und himmlisch mild", "tokens": ["Still", "und", "gro\u00df", "und", "himm\u00b7lisch", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Warst du in Luisens Bild.", "tokens": ["Warst", "du", "in", "Lu\u00b7i\u00b7sens", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihrer Augen Z\u00e4hrenhelle,", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Z\u00e4h\u00b7ren\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Wangen Purpurschein,", "tokens": ["Ih\u00b7rer", "Wan\u00b7gen", "Pur\u00b7pur\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, in Edens lichtem Quelle", "tokens": ["Ach", ",", "in", "E\u00b7dens", "lich\u00b7tem", "Quel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wuschen sie die Engel rein.", "tokens": ["Wu\u00b7schen", "sie", "die", "En\u00b7gel", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihrer Stimme s\u00fc\u00dfer Ton,", "tokens": ["Ih\u00b7rer", "Stim\u00b7me", "s\u00fc\u00b7\u00dfer", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Himmelspantalon;", "tokens": ["Wie", "ein", "Him\u00b7mel\u00b7span\u00b7ta\u00b7lon", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blitzt' und drang in meine Seele,", "tokens": ["Blitzt'", "und", "drang", "in", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz und Busen wurden weit,", "tokens": ["Herz", "und", "Bu\u00b7sen", "wur\u00b7den", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus meiner Augenh\u00f6hle", "tokens": ["Und", "aus", "mei\u00b7ner", "Au\u00b7gen\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmerte die Z\u00e4rtlichkeit.", "tokens": ["Schim\u00b7mer\u00b7te", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebeschauernd schlug mein Herz,", "tokens": ["Lie\u00b7be\u00b7schau\u00b7ernd", "schlug", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald vor Wonne, bald vor Schmerz.", "tokens": ["Bald", "vor", "Won\u00b7ne", ",", "bald", "vor", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sterben m\u00f6cht' ich nun vor Liebe,", "tokens": ["Ster\u00b7ben", "m\u00f6cht'", "ich", "nun", "vor", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich diese Zauberin;", "tokens": ["Seh'", "ich", "die\u00b7se", "Zau\u00b7be\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wird ihr Auge tr\u00fcbe,", "tokens": ["A\u00b7ber", "wird", "ihr", "Au\u00b7ge", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O, wie tr\u00fcbt sich dann mein Sinn!", "tokens": ["O", ",", "wie", "tr\u00fcbt", "sich", "dann", "mein", "Sinn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeden Zug der Sympathie", "tokens": ["Je\u00b7den", "Zug", "der", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlt mein armes Herz durch sie.", "tokens": ["F\u00fchlt", "mein", "ar\u00b7mes", "Herz", "durch", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bruderliebe zu den Br\u00fcdern,", "tokens": ["Bru\u00b7der\u00b7lie\u00b7be", "zu", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitgef\u00fchl bei jeder Noth;", "tokens": ["Mit\u00b7ge\u00b7f\u00fchl", "bei", "je\u00b7der", "Noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes L\u00e4cheln zu erwiedern;", "tokens": ["Je\u00b7des", "L\u00e4\u00b7cheln", "zu", "er\u00b7wie\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Angst bei fremdem Tod;", "tokens": ["Je\u00b7de", "Angst", "bei", "frem\u00b7dem", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Demuth, Kinderfreundlichkeit", "tokens": ["De\u00b7muth", ",", "Kin\u00b7der\u00b7freund\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lehrte mich die Z\u00e4rtlichkeit.", "tokens": ["Lehr\u00b7te", "mich", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber nur aus deinen Blicken,", "tokens": ["A\u00b7ber", "nur", "aus", "dei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O Luise! lernt' ich sie;", "tokens": ["O", "Lu\u00b7i\u00b7se", "!", "lernt'", "ich", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ewig soll mich nun entz\u00fccken", "tokens": ["E\u00b7wig", "soll", "mich", "nun", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Seelensympathie;", "tokens": ["Die\u00b7se", "See\u00b7len\u00b7sym\u00b7pa\u00b7thie", ";"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese s\u00fc\u00dfe Z\u00e4rtlichkeit,", "tokens": ["Die\u00b7se", "s\u00fc\u00b7\u00dfe", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns Cherubsschwingen leiht.", "tokens": ["Die", "uns", "Che\u00b7rubs\u00b7schwin\u00b7gen", "leiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn ich rede, wenn ich schweige;", "tokens": ["Wenn", "ich", "re\u00b7de", ",", "wenn", "ich", "schwei\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn, in deinen Reiz verschwemmt,", "tokens": ["Wenn", ",", "in", "dei\u00b7nen", "Reiz", "ver\u00b7schwemmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche Thr\u00e4n', der Liebe Zeuge,", "tokens": ["Man\u00b7che", "Thr\u00e4n'", ",", "der", "Lie\u00b7be", "Zeu\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die s\u00fc\u00dfe Rede hemmt;", "tokens": ["Mir", "die", "s\u00fc\u00b7\u00dfe", "Re\u00b7de", "hemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O so denke: tief, wie dich,", "tokens": ["O", "so", "den\u00b7ke", ":", "tief", ",", "wie", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$.", "ADJD", "$,", "PWAV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fchrt die Z\u00e4rtlichkeit auch mich!", "tokens": ["R\u00fchrt", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "auch", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}