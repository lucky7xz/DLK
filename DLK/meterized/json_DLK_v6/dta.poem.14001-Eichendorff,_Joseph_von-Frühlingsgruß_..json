{"dta.poem.14001": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgru\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und auf dem h\u00f6chsten Wipfel", "tokens": ["Und", "auf", "dem", "h\u00f6chs\u00b7ten", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steh ich und schau vom Baum,", "tokens": ["Steh", "ich", "und", "schau", "vom", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Welt, Du sch\u00f6ne Welt Du,", "tokens": ["O", "Welt", ",", "Du", "sch\u00f6\u00b7ne", "Welt", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sieht Dich vor Bl\u00fcten kaum!", "tokens": ["Man", "sieht", "Dich", "vor", "Bl\u00fc\u00b7ten", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}