{"textgrid.poem.61214": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sprach der Herr zu uns in Krieges Lohen:", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sprach der Herr zu uns in Krieges Lohen:", "tokens": ["Es", "sprach", "der", "Herr", "zu", "uns", "in", "Krie\u00b7ges", "Lo\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seid einig, und wir waren's eine Stunde,", "tokens": ["Seid", "ei\u00b7nig", ",", "und", "wir", "wa\u00b7ren's", "ei\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch lachten wir des Worts aus seinem Munde,", "tokens": ["Doch", "lach\u00b7ten", "wir", "des", "Worts", "aus", "sei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da am Gew\u00f6lk der Glutschein kaum entflohen.", "tokens": ["Da", "am", "Ge\u00b7w\u00f6lk", "der", "Glut\u00b7schein", "kaum", "ent\u00b7flo\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun l\u00e4\u00dft er wieder seine Stimme drohen", "tokens": ["Nun", "l\u00e4\u00dft", "er", "wie\u00b7der", "sei\u00b7ne", "Stim\u00b7me", "dro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mahnt uns, festzustehn im guten Bunde.", "tokens": ["Und", "mahnt", "uns", ",", "fest\u00b7zu\u00b7stehn", "im", "gu\u00b7ten", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O h\u00f6rt den Ruf, ihr Niedern in der Runde,", "tokens": ["O", "h\u00f6rt", "den", "Ruf", ",", "ihr", "Nie\u00b7dern", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und beugt euch ihm auf eurem Thron, ihr Hohen!", "tokens": ["Und", "beugt", "euch", "ihm", "auf", "eu\u00b7rem", "Thron", ",", "ihr", "Ho\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn also spricht er: \u00bbHabet ihr danieden", "tokens": ["Denn", "al\u00b7so", "spricht", "er", ":", "\u00bb", "Ha\u00b7bet", "ihr", "da\u00b7nie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "$(", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergessen schon der Tr\u00fcbsal eurer Herzen,", "tokens": ["Ver\u00b7ges\u00b7sen", "schon", "der", "Tr\u00fcb\u00b7sal", "eu\u00b7rer", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die auf euch kam, da ihr euch j\u00fcngst geschieden?", "tokens": ["Die", "auf", "euch", "kam", ",", "da", "ihr", "euch", "j\u00fcngst", "ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seid ", "tokens": ["Seid"], "token_info": ["word"], "pos": ["VAIMP"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Zerbrechen oder neu zusammenschmieden", "tokens": ["Zer\u00b7bre\u00b7chen", "o\u00b7der", "neu", "zu\u00b7sam\u00b7men\u00b7schmie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Feuer meines Zorns und eurer Schmerzen.\u00ab", "tokens": ["Im", "Feu\u00b7er", "mei\u00b7nes", "Zorns", "und", "eu\u00b7rer", "Schmer\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}