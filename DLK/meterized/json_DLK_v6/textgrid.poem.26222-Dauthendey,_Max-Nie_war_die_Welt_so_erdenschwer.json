{"textgrid.poem.26222": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nie war die Welt so erdenschwer", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilf Gott, wie ich mich qu\u00e4le!", "tokens": ["Hilf", "Gott", ",", "wie", "ich", "mich", "qu\u00e4\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Todstill wird meine Seele.", "tokens": ["Tod\u00b7still", "wird", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich wei\u00df schon l\u00e4ngst von Lust nichts mehr,", "tokens": ["Ich", "wei\u00df", "schon", "l\u00e4ngst", "von", "Lust", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie war die Welt so erdenschwer.", "tokens": ["Nie", "war", "die", "Welt", "so", "er\u00b7den\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Herz zuckt schmerzend Stund' um Stund',", "tokens": ["Mein", "Herz", "zuckt", "schmer\u00b7zend", "Stund'", "um", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Feinde, Feinde wandern rund.", "tokens": ["Nur", "Fein\u00b7de", ",", "Fein\u00b7de", "wan\u00b7dern", "rund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}