{"textgrid.poem.41124": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Vom morschen Baume", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter und gebrochner Baum,", "tokens": ["Al\u00b7ter", "und", "ge\u00b7broch\u00b7ner", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst dich bald zur Erde neigen!", "tokens": ["Wirst", "dich", "bald", "zur", "Er\u00b7de", "nei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! es ist mir wie ein Traum,", "tokens": ["Ach", "!", "es", "ist", "mir", "wie", "ein", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du standest reich an Zweigen,", "tokens": ["Da\u00df", "du", "stan\u00b7dest", "reich", "an", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df ert\u00f6nte manches Lied", "tokens": ["Da\u00df", "er\u00b7t\u00f6n\u00b7te", "man\u00b7ches", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch aus deinen Laubgewinden", "tokens": ["Noch", "aus", "dei\u00b7nen", "Laub\u00b7ge\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Wandrer wegem\u00fcd", "tokens": ["Da\u00df", "der", "Wand\u00b7rer", "we\u00b7ge\u00b7m\u00fcd"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei dir suchte Ruh' zu finden.", "tokens": ["Bei", "dir", "such\u00b7te", "Ruh'", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja! da\u00df manche s\u00fc\u00dfe Frucht", "tokens": ["Ja", "!", "da\u00df", "man\u00b7che", "s\u00fc\u00b7\u00dfe", "Frucht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fiel in deinen Schatten nieder,", "tokens": ["Fiel", "in", "dei\u00b7nen", "Schat\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, wer einmal sie versucht,", "tokens": ["Die", ",", "wer", "ein\u00b7mal", "sie", "ver\u00b7sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "ADV", "PPER", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Gern zu kosten kehrte wieder.", "tokens": ["Gern", "zu", "kos\u00b7ten", "kehr\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Armer Baum! wie \u00e4stelos", "tokens": ["Ar\u00b7mer", "Baum", "!", "wie", "\u00e4s\u00b7te\u00b7los"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehst du nun, kannst kaum dich halten!", "tokens": ["Stehst", "du", "nun", ",", "kannst", "kaum", "dich", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Steine legten schwer und gro\u00df", "tokens": ["Stei\u00b7ne", "leg\u00b7ten", "schwer", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dein Herz sie, das gespalten.", "tokens": ["In", "dein", "Herz", "sie", ",", "das", "ge\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "T\u00f6ne wie ein Sterbelied", "tokens": ["T\u00f6\u00b7ne", "wie", "ein", "Ster\u00b7be\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt man, wenn dich Sturm zerschellet,", "tokens": ["H\u00f6rt", "man", ",", "wenn", "dich", "Sturm", "zer\u00b7schel\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Vogel von dir flieht,", "tokens": ["Und", "der", "Vo\u00b7gel", "von", "dir", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und der Wandrer spricht: ", "tokens": ["Und", "der", "Wand\u00b7rer", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Sinke sanft in Gras und Moos!", "tokens": ["Sin\u00b7ke", "sanft", "in", "Gras", "und", "Moos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dir deinen Tod gefallen!", "tokens": ["La\u00df", "dir", "dei\u00b7nen", "Tod", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! es ist der Erde Los:", "tokens": ["Ach", "!", "es", "ist", "der", "Er\u00b7de", "Los", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchen, tragen und zerfallen!", "tokens": ["Bl\u00fc\u00b7hen", ",", "tra\u00b7gen", "und", "zer\u00b7fal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer sang dieses Lied vom Baum?", "tokens": ["Wer", "sang", "die\u00b7ses", "Lied", "vom", "Baum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tief ist mir's ins Herz gedrungen.", "tokens": ["Tief", "ist", "mir's", "ins", "Herz", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ich sang es, \u2013 mir im Traum", "tokens": ["Nicht", "ich", "sang", "es", ",", "\u2013", "mir", "im", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "VVFIN", "PPER", "$,", "$(", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat's ein Vogel vorgesungen.", "tokens": ["Hat's", "ein", "Vo\u00b7gel", "vor\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}