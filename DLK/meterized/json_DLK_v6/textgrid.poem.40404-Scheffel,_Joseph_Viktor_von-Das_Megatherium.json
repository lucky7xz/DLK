{"textgrid.poem.40404": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Das Megatherium", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hangt denn dort bewegungslos", "tokens": ["Was", "hangt", "denn", "dort", "be\u00b7we\u00b7gungs\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Knaul zusammgeballt", "tokens": ["Zum", "Knaul", "zu\u00b7samm\u00b7ge\u00b7ballt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So riesenfaul und riesengro\u00df", "tokens": ["So", "rie\u00b7sen\u00b7faul", "und", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Ururururwald?", "tokens": ["Im", "Ur\u00b7ur\u00b7u\u00b7rur\u00b7wald", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dreifach so wuchtig als ein Stier,", "tokens": ["Drei\u00b7fach", "so", "wuch\u00b7tig", "als", "ein", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dreifach so schwer und dumm \u2013", "tokens": ["Drei\u00b7fach", "so", "schwer", "und", "dumm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Klettertier, ein Krallentier:", "tokens": ["Ein", "Klet\u00b7ter\u00b7tier", ",", "ein", "Kral\u00b7len\u00b7tier", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Megatherium!", "tokens": ["Das", "Me\u00b7ga\u00b7the\u00b7ri\u00b7um", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Tr\u00e4g glotzt es in die Welt hinein", "tokens": ["Tr\u00e4g", "glotzt", "es", "in", "die", "Welt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00e4hnt als wie im Traum,", "tokens": ["Und", "g\u00e4hnt", "als", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und krallt die scharfen Krallen ein", "tokens": ["Und", "krallt", "die", "schar\u00b7fen", "Kral\u00b7len", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Embahubabaum.", "tokens": ["Am", "Em\u00b7ba\u00b7hu\u00b7ba\u00b7baum", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Fr\u00fcchte und das saftige Blatt", "tokens": ["Die", "Fr\u00fcch\u00b7te", "und", "das", "saf\u00b7ti\u00b7ge", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Verzehrt es und sagt: \u00bbAi!\u00ab", "tokens": ["Ver\u00b7zehrt", "es", "und", "sagt", ":", "\u00bb", "Ai", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$.", "$(", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn's ihn leer gefressen hat,", "tokens": ["Und", "wenn's", "ihn", "leer", "ge\u00b7fres\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sagt's auch zuweilen: \u00bb", "tokens": ["Sagt's", "auch", "zu\u00b7wei\u00b7len", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann aber steigt es nicht herab,", "tokens": ["Dann", "a\u00b7ber", "steigt", "es", "nicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kennt den k\u00fcrzern Weg:", "tokens": ["Es", "kennt", "den", "k\u00fcr\u00b7zern", "Weg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleich einem K\u00fcrbis f\u00e4llt es ab", "tokens": ["Gleich", "ei\u00b7nem", "K\u00fcr\u00b7bis", "f\u00e4llt", "es", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00fchrt sich nicht vom Fleck.", "tokens": ["Und", "r\u00fchrt", "sich", "nicht", "vom", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit rundem Eulenangesicht", "tokens": ["Mit", "run\u00b7dem", "Eu\u00b7le\u00b7nan\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nickt's sanft und l\u00e4chelt brav:", "tokens": ["Nickt's", "sanft", "und", "l\u00e4\u00b7chelt", "brav", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn nach gelungener F\u00fctterung kommt", "tokens": ["Denn", "nach", "ge\u00b7lun\u00b7ge\u00b7ner", "F\u00fct\u00b7te\u00b7rung", "kommt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als Hauptarbeit der Schlaf.", "tokens": ["Als", "Haupt\u00b7ar\u00b7beit", "der", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "... O Mensch, dem solch ein Riesentier", "tokens": ["...", "O", "Mensch", ",", "dem", "solch", "ein", "Rie\u00b7sen\u00b7tier"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "NN", "$,", "PRELS", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht glaublich scheinen will,", "tokens": ["Nicht", "glaub\u00b7lich", "schei\u00b7nen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh nach Madrid! dort zeigt man dir", "tokens": ["Geh", "nach", "Mad\u00b7rid", "!", "dort", "zeigt", "man", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "$.", "ADV", "VVFIN", "PIS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein ganz Skelett fossil.", "tokens": ["Sein", "ganz", "Ske\u00b7lett", "fos\u00b7sil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch bist du staunend ihm genaht,", "tokens": ["Doch", "bist", "du", "stau\u00b7nend", "ihm", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verliere nicht den Mut:", "tokens": ["Ver\u00b7lie\u00b7re", "nicht", "den", "Mut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So ungeheure Faulheit tat", "tokens": ["So", "un\u00b7ge\u00b7heu\u00b7re", "Faul\u00b7heit", "tat"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Du bist kein Megatherium,", "tokens": ["Du", "bist", "kein", "Me\u00b7ga\u00b7the\u00b7ri\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dein Geist kennt h\u00f6here Pflicht,", "tokens": ["Dein", "Geist", "kennt", "h\u00f6\u00b7he\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Drum schw\u00e4nze kein Kollegium", "tokens": ["Drum", "schw\u00e4n\u00b7ze", "kein", "Kol\u00b7le\u00b7gi\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und \u00fcberfri\u00df dich nicht.", "tokens": ["Und", "\u00fc\u00b7berf\u00b7ri\u00df", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "N\u00fctz' deine Zeit, sie gilt statt Gelds,", "tokens": ["N\u00fctz'", "dei\u00b7ne", "Zeit", ",", "sie", "gilt", "statt", "Gelds", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei flei\u00dfig bis zum Grab,", "tokens": ["Sei", "flei\u00b7\u00dfig", "bis", "zum", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und steckst du doch im faulen Pelz,", "tokens": ["Und", "steckst", "du", "doch", "im", "fau\u00b7len", "Pelz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So fall' mit Vorsicht ab!", "tokens": ["So", "fall'", "mit", "Vor\u00b7sicht", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}