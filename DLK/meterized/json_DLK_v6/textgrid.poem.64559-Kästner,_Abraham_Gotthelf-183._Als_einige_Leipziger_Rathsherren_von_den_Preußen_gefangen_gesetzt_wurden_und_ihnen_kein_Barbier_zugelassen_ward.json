{"textgrid.poem.64559": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "183. Als einige Leipziger Rathsherren von den Preu\u00dfen gefangen gesetzt wurden und ihnen kein Barbier zugelassen ward", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Leipzig, dir wird einst, o m\u00f6cht' es bald geschehn!", "tokens": ["Mein", "Leip\u00b7zig", ",", "dir", "wird", "einst", ",", "o", "m\u00f6cht'", "es", "bald", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PPER", "VAFIN", "ADV", "$,", "FM", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von \u00fcberstandner Noth das Bild noch \u00fcbrig bleiben.", "tokens": ["Von", "\u00fc\u00b7bers\u00b7tand\u00b7ner", "Noth", "das", "Bild", "noch", "\u00fcb\u00b7rig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ewigem Latein soll ", "tokens": ["In", "e\u00b7wi\u00b7gem", "La\u00b7tein", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}}}}