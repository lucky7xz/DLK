{"dta.poem.16551": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "212.  Ich wie GOtt/ GOtt wie ich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist das was Er ist: Ich was ich durch ihn bin:", "tokens": ["Gott", "ist", "das", "was", "Er", "ist", ":", "Ich", "was", "ich", "durch", "ihn", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PDS", "PRELS", "PPER", "VAFIN", "$.", "PPER", "PWS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kennstu einen wol/ so kenstu mich und Jhn.", "tokens": ["Doch", "kenns\u00b7tu", "ei\u00b7nen", "wol", "/", "so", "kens\u00b7tu", "mich", "und", "Jhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "$(", "ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}