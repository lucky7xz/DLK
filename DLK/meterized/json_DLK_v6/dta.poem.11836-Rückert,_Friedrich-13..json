{"dta.poem.11836": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Silbers reiner Glanz l\u00e4\u00dft Flecken am Gewand,", "tokens": ["Des", "Sil\u00b7bers", "rei\u00b7ner", "Glanz", "l\u00e4\u00dft", "Fle\u00b7cken", "am", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das es bestreift, und Schmutz an der ber\u00fchrten Hand.", "tokens": ["Das", "es", "be\u00b7streift", ",", "und", "Schmutz", "an", "der", "be\u00b7r\u00fchr\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht das gemeine nur, das Geld, das im Gedr\u00e4nge", "tokens": ["Nicht", "das", "ge\u00b7mei\u00b7ne", "nur", ",", "das", "Geld", ",", "das", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "ADV", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der M\u00e4rkte kreist, es thut's auch edles Kunstgesp\u00e4nge.", "tokens": ["Der", "M\u00e4rk\u00b7te", "kreist", ",", "es", "thut's", "auch", "ed\u00b7les", "Kunst\u00b7ge\u00b7sp\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Zeichen sei es dir, da\u00df du nie kannst benutzen", "tokens": ["Ein", "Zei\u00b7chen", "sei", "es", "dir", ",", "da\u00df", "du", "nie", "kannst", "be\u00b7nut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weltg\u00fcter, ohne dich mit ihnen zu beschmutzen.", "tokens": ["Welt\u00b7g\u00fc\u00b7ter", ",", "oh\u00b7ne", "dich", "mit", "ih\u00b7nen", "zu", "be\u00b7schmut\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}