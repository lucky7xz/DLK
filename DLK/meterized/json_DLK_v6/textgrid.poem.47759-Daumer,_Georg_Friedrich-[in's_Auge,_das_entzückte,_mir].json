{"textgrid.poem.47759": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in's Auge, das entz\u00fcckte, mir]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In's Auge, das entz\u00fcckte, mir", "tokens": ["In's", "Au\u00b7ge", ",", "das", "ent\u00b7z\u00fcck\u00b7te", ",", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "PDS", "VVFIN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam ein Besuch, ein f\u00fcrstlicher,", "tokens": ["Kam", "ein", "Be\u00b7such", ",", "ein", "f\u00fcrst\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Dein wunderherrlich Bild, o Lieb!", "tokens": ["Dein", "wun\u00b7der\u00b7herr\u00b7lich", "Bild", ",", "o", "Lieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich zog davor, zu ehren es,", "tokens": ["Ich", "zog", "da\u00b7vor", ",", "zu", "eh\u00b7ren", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PTKZU", "VVINF", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So viel ich immer kann, bestrebt,", "tokens": ["So", "viel", "ich", "im\u00b7mer", "kann", ",", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Purpurener Gardine Zier,", "tokens": ["Pur\u00b7pu\u00b7re\u00b7ner", "Gar\u00b7di\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Aus blutiger Thr\u00e4ne Stoff gewebt.", "tokens": ["Aus", "blu\u00b7ti\u00b7ger", "Thr\u00e4\u00b7ne", "Stoff", "ge\u00b7webt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}