{"dta.poem.802": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.  \n Auff einen vngeschickten R\u00f6mer.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Kan Rom dich nicht gelehrt/ vnd nicht geschickter machen/ ", "tokens": ["Kan", "Rom", "dich", "nicht", "ge\u00b7lehrt", "/", "vnd", "nicht", "ge\u00b7schick\u00b7ter", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPER", "PTKNEG", "VVPP", "$(", "KON", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo es an keiner kunst/ an keiner wei\u00dfheit fehlt/", "tokens": ["Wo", "es", "an", "kei\u00b7ner", "kunst", "/", "an", "kei\u00b7ner", "wei\u00df\u00b7heit", "fehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIS", "PTKVZ", "$(", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo die Scharffsinnigkeit selbst jhren sitz erwehlt.", "tokens": ["Wo", "die", "Scharff\u00b7sin\u00b7nig\u00b7keit", "selbst", "jhren", "sitz", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+---+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wo die vernunfft durchsucht der F\u00fcrsten h\u00f6chste sachen?", "tokens": ["Wo", "die", "ver\u00b7nunfft", "durch\u00b7sucht", "der", "F\u00fcrs\u00b7ten", "h\u00f6chs\u00b7te", "sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was t\u00e4glich w\u00e4chst/ was steht/ was sich dem grimmen Ra-", "tokens": ["Was", "t\u00e4g\u00b7lich", "w\u00e4chst", "/", "was", "steht", "/", "was", "sich", "dem", "grim\u00b7men", "Ra"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VVFIN", "$(", "PWS", "VVFIN", "$(", "PWS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "chen ", "tokens": ["chen"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der zeit die alles frist entzeucht; was sich verm\u00e4hlt", "tokens": ["Der", "zeit", "die", "al\u00b7les", "frist", "ent\u00b7zeucht", ";", "was", "sich", "ver\u00b7m\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PIS", "ADJD", "VVPP", "$.", "PWS", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der greisen Ewigkeit/ was lange Jahre zehlt/", "tokens": ["Der", "grei\u00b7sen", "E\u00b7wig\u00b7keit", "/", "was", "lan\u00b7ge", "Jah\u00b7re", "zehlt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sihst du; doch wer dich siht/ mu\u00df deiner grobheit lachen.", "tokens": ["Sihst", "du", ";", "doch", "wer", "dich", "siht", "/", "mu\u00df", "dei\u00b7ner", "grob\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KON", "PWS", "PPER", "VVFIN", "$(", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier haw\u2019t man Marmel aus/ hier schleifft man Dia-", "tokens": ["Hier", "ha\u00b7w't", "man", "Mar\u00b7mel", "aus", "/", "hier", "schleifft", "man", "Dia"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "PTKVZ", "$(", "ADV", "VVFIN", "PIS", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "mand/ ", "tokens": ["mand", "/"], "token_info": ["word", "punct"], "pos": ["PIS", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Porfirr/ (wie hart er auch) weicht der gelehrten hand", "tokens": ["Por\u00b7firr", "/", "(", "wie", "hart", "er", "auch", ")", "weicht", "der", "ge\u00b7lehr\u00b7ten", "hand"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "$(", "PWAV", "ADJD", "PPER", "ADV", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von welchem Steinri\u00df hat man dein Gehirn genommen?", "tokens": ["Von", "wel\u00b7chem", "Stein\u00b7ri\u00df", "hat", "man", "dein", "Ge\u00b7hirn", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man glaubt/ da\u00df diese Lufft den frembden sch\u00e4dlich sey:", "tokens": ["Man", "glaubt", "/", "da\u00df", "die\u00b7se", "Lufft", "den", "fremb\u00b7den", "sch\u00e4d\u00b7lich", "sey", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "PDAT", "NN", "ART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schaw das wiederspiel/ Ich bin von Seuchen frey", "tokens": ["Ich", "schaw", "das", "wie\u00b7der\u00b7spiel", "/", "Ich", "bin", "von", "Seu\u00b7chen", "frey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "$(", "PPER", "VAFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir ist die R\u00f6m\u2019sche Lufft in warheit nicht bekommen.", "tokens": ["Dir", "ist", "die", "R\u00f6m'\u00b7sche", "Lufft", "in", "war\u00b7heit", "nicht", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}