{"textgrid.poem.62287": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[kalt ist die Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt ist die Welt,", "tokens": ["Kalt", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hart ist das Geld,", "tokens": ["Hart", "ist", "das", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch h\u00e4rter und k\u00e4lter und d\u00fcmmer", "tokens": ["Doch", "h\u00e4r\u00b7ter", "und", "k\u00e4l\u00b7ter", "und", "d\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und tausendmal wohl schlimmer", "tokens": ["Und", "tau\u00b7send\u00b7mal", "wohl", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als alle Gifte auf deutschen Auen,", "tokens": ["Als", "al\u00b7le", "Gif\u00b7te", "auf", "deut\u00b7schen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ist Grausamkeit und Heuchelei zu schauen.", "tokens": ["Ist", "Grau\u00b7sam\u00b7keit", "und", "Heu\u00b7che\u00b7lei", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}