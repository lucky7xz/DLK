{"textgrid.poem.65678": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Missolunghi", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch, ihr Br\u00fcder! Durch, ihr Br\u00fcder! Durch! Die Stunde hat geschlagen!", "tokens": ["Durch", ",", "ihr", "Br\u00fc\u00b7der", "!", "Durch", ",", "ihr", "Br\u00fc\u00b7der", "!", "Durch", "!", "Die", "Stun\u00b7de", "hat", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "APPR", "$,", "PPOSAT", "NN", "$.", "APPR", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch! Aus Missolunghi's Thoren la\u00dft uns Missolunghi tragen!", "tokens": ["Durch", "!", "Aus", "Miss\u00b7o\u00b7lung\u00b7hi's", "Tho\u00b7ren", "la\u00dft", "uns", "Mis\u00b7so\u00b7lung\u00b7hi", "tra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "NE", "NN", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Von den freien Bergesh\u00f6hen winken schon die Feuerzeichen,", "tokens": ["Von", "den", "frei\u00b7en", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", "win\u00b7ken", "schon", "die", "Feu\u00b7er\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die uns durch die weiten L\u00fcfte ihre Flammenh\u00e4nde reichen,", "tokens": ["Die", "uns", "durch", "die", "wei\u00b7ten", "L\u00fcf\u00b7te", "ih\u00b7re", "Flam\u00b7men\u00b7h\u00e4n\u00b7de", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Uns zu sich empor zu ziehen in die Burg, die Gott erbauet,", "tokens": ["Uns", "zu", "sich", "em\u00b7por", "zu", "zie\u00b7hen", "in", "die", "Burg", ",", "die", "Gott", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "PTKVZ", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "In das neue Missolunghi, das er unsrer Wehr vertrauet.", "tokens": ["In", "das", "neu\u00b7e", "Mis\u00b7so\u00b7lung\u00b7hi", ",", "das", "er", "uns\u00b7rer", "Wehr", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Durch! Aus Missolunghi's Thoren la\u00dft uns Missolunghi tragen,", "tokens": ["Durch", "!", "Aus", "Miss\u00b7o\u00b7lung\u00b7hi's", "Tho\u00b7ren", "la\u00dft", "uns", "Mis\u00b7so\u00b7lung\u00b7hi", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "NE", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und mit unsrer heil'gen Veste durch den Heidenschwarm uns schlagen!", "tokens": ["Und", "mit", "uns\u00b7rer", "heil'\u00b7gen", "Ves\u00b7te", "durch", "den", "Hei\u00b7den\u00b7schwarm", "uns", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Missolunghi in den Waffen, in den Armen, in den Herzen,", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", "in", "den", "Waf\u00b7fen", ",", "in", "den", "Ar\u00b7men", ",", "in", "den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Missolunghi in dem Sturme unsrer rachefrohen Schmerzen,", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", "in", "dem", "Stur\u00b7me", "uns\u00b7rer", "ra\u00b7che\u00b7fro\u00b7hen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Unsre Herzen deine Kirchen, deine Zinnen unsre Lanzen,", "tokens": ["Uns\u00b7re", "Her\u00b7zen", "dei\u00b7ne", "Kir\u00b7chen", ",", "dei\u00b7ne", "Zin\u00b7nen", "uns\u00b7re", "Lan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Unsre Arme deine Mauern, unsre Br\u00fcste deine Schanzen! \u2013", "tokens": ["Uns\u00b7re", "Ar\u00b7me", "dei\u00b7ne", "Mau\u00b7ern", ",", "uns\u00b7re", "Br\u00fcs\u00b7te", "dei\u00b7ne", "Schan\u00b7zen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Ach, und um uns her gezogen ist ein tiefer rother Graben,", "tokens": ["Ach", ",", "und", "um", "uns", "her", "ge\u00b7zo\u00b7gen", "ist", "ein", "tie\u00b7fer", "ro\u00b7ther", "Gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "KOUI", "PRF", "APZR", "VVPP", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Blut der Weiber und der Kinder, die sie uns geschlachtet haben.", "tokens": ["Blut", "der", "Wei\u00b7ber", "und", "der", "Kin\u00b7der", ",", "die", "sie", "uns", "ge\u00b7schlach\u00b7tet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}