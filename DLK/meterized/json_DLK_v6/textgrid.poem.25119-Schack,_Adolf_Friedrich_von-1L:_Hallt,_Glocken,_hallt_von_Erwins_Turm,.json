{"textgrid.poem.25119": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hallt, Glocken, hallt von Erwins Turm,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hallt, Glocken, hallt von Erwins Turm,", "tokens": ["Hallt", ",", "Glo\u00b7cken", ",", "hallt", "von", "Er\u00b7wins", "Turm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und brausen mag der Jubelsturm", "tokens": ["Und", "brau\u00b7sen", "mag", "der", "Ju\u00b7bel\u00b7sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Berg zu Berg, von Strom zu Strome!", "tokens": ["Von", "Berg", "zu", "Berg", ",", "von", "Strom", "zu", "Stro\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An jedes Ohr die Botschaft tragt:", "tokens": ["An", "je\u00b7des", "Ohr", "die", "Bot\u00b7schaft", "tragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In deutsche Luft nun wieder ragt", "tokens": ["In", "deut\u00b7sche", "Luft", "nun", "wie\u00b7der", "ragt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der herrlichste der deutschen Dome!", "tokens": ["Der", "herr\u00b7lichs\u00b7te", "der", "deut\u00b7schen", "Do\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der alte Frevel ist ger\u00e4cht,", "tokens": ["Der", "al\u00b7te", "Fre\u00b7vel", "ist", "ge\u00b7r\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der von Geschlechte zu Geschlecht", "tokens": ["Der", "von", "Ge\u00b7schlech\u00b7te", "zu", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns bittre Schmach vererbt und Schande:", "tokens": ["Uns", "bitt\u00b7re", "Schmach", "ver\u00b7erbt", "und", "Schan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hallt, Glocken! Von des Nordens Meer", "tokens": ["Hallt", ",", "Glo\u00b7cken", "!", "Von", "des", "Nor\u00b7dens", "Meer"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis zu den Alpen ruft sie her,", "tokens": ["Bis", "zu", "den", "Al\u00b7pen", "ruft", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die S\u00f6hne aller deutschen Lande!", "tokens": ["Die", "S\u00f6h\u00b7ne", "al\u00b7ler", "deut\u00b7schen", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja freier, wie gel\u00f6st vom Bann,", "tokens": ["Ja", "frei\u00b7er", ",", "wie", "ge\u00b7l\u00f6st", "vom", "Bann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,", "PWAV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufatmet aller Brust; heran", "tokens": ["Auf\u00b7at\u00b7met", "al\u00b7ler", "Brust", ";", "he\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PIAT", "NN", "$.", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs M\u00fcnsterthor seh' ich sie wogen,", "tokens": ["Durchs", "M\u00fcns\u00b7ter\u00b7thor", "seh'", "ich", "sie", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie ein himmlischer Orkan", "tokens": ["Und", "wie", "ein", "himm\u00b7li\u00b7scher", "Or\u00b7kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Braust Orgelschall, indes sie nahn,", "tokens": ["Braust", "Or\u00b7gel\u00b7schall", ",", "in\u00b7des", "sie", "nahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Gurten hin und Strebebogen.", "tokens": ["An", "Gur\u00b7ten", "hin", "und", "Stre\u00b7be\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und durch die Fensterrose bricht", "tokens": ["Und", "durch", "die", "Fens\u00b7ter\u00b7ro\u00b7se", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Farbenglanz herein, wie Licht", "tokens": ["Ein", "Far\u00b7ben\u00b7glanz", "her\u00b7ein", ",", "wie", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Regenbogens nach Gewittern;", "tokens": ["Des", "Re\u00b7gen\u00b7bo\u00b7gens", "nach", "Ge\u00b7wit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allhin bewegt sich's wunderbar,", "tokens": ["A\u00b7llhin", "be\u00b7wegt", "sich's", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wie von Altare zu Altar", "tokens": ["Wie", "von", "Al\u00b7ta\u00b7re", "zu", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Strahlen durch den Tempel zittern.", "tokens": ["Die", "Strah\u00b7len", "durch", "den", "Tem\u00b7pel", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vom Mund der Cherubim von Stein,", "tokens": ["Vom", "Mund", "der", "Che\u00b7ru\u00b7bim", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die oben l\u00e4ngs der Pfeilerreihn", "tokens": ["Die", "o\u00b7ben", "l\u00e4ngs", "der", "Pfei\u00b7ler\u00b7reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an den Marmorbecken h\u00e4ngen,", "tokens": ["Und", "an", "den", "Mar\u00b7mor\u00b7be\u00b7cken", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt schmetternder Drommetensto\u00df,", "tokens": ["T\u00f6nt", "schmet\u00b7tern\u00b7der", "Drom\u00b7me\u00b7ten\u00b7sto\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wollt' im tiefsten Erdenscho\u00df", "tokens": ["Als", "wollt'", "im", "tiefs\u00b7ten", "Er\u00b7den\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Klang die Grabesriegel sprengen.", "tokens": ["Der", "Klang", "die", "Gra\u00b7be\u00b7srie\u00b7gel", "spren\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Beter jeder sinkt aufs Knie;", "tokens": ["Der", "Be\u00b7ter", "je\u00b7der", "sinkt", "aufs", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch der andern Reihen, sieh!", "tokens": ["Und", "durch", "der", "an\u00b7dern", "Rei\u00b7hen", ",", "sieh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umklungen von den Dankchor\u00e4len,", "tokens": ["Um\u00b7klun\u00b7gen", "von", "den", "Dank\u00b7cho\u00b7r\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nahn sich Gestalten schattengleich;", "tokens": ["Nahn", "sich", "Ge\u00b7stal\u00b7ten", "schat\u00b7ten\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die sind nicht aus des Lebens Reich,", "tokens": ["Die", "sind", "nicht", "aus", "des", "Le\u00b7bens", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie kommen aus dem Land der Seelen.", "tokens": ["Sie", "kom\u00b7men", "aus", "dem", "Land", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Voran, die Locken silberwei\u00df,", "tokens": ["Vo\u00b7ran", ",", "die", "Lo\u00b7cken", "sil\u00b7ber\u00b7wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Freudenthr\u00e4nen tritt ein Greis;", "tokens": ["In", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "tritt", "ein", "Greis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um ihn erschallt von tausend Zungen \u2013", "tokens": ["Um", "ihn", "er\u00b7schallt", "von", "tau\u00b7send", "Zun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn alle haben ihn erkannt \u2013", "tokens": ["Denn", "al\u00b7le", "ha\u00b7ben", "ihn", "er\u00b7kannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Lied vom deutschen Vaterland;", "tokens": ["Dein", "Lied", "vom", "deut\u00b7schen", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun ward erf\u00fcllt, was er gesungen.", "tokens": ["Nun", "ward", "er\u00b7f\u00fcllt", ",", "was", "er", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und rings knien sie, die opferfroh", "tokens": ["Und", "rings", "kni\u00b7en", "sie", ",", "die", "op\u00b7fer\u00b7froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Leipzigs Feld, bei Waterloo", "tokens": ["Auf", "Leip\u00b7zigs", "Feld", ",", "bei", "Wa\u00b7ter\u00b7loo"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Tod f\u00fcrs Vaterland geworben;", "tokens": ["Um", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", "ge\u00b7wor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lang wurde dr\u00fcben in der Welt", "tokens": ["Lang", "wur\u00b7de", "dr\u00fc\u00b7ben", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Seligen ihr Gl\u00fcck verg\u00e4llt", "tokens": ["Der", "Se\u00b7li\u00b7gen", "ihr", "Gl\u00fcck", "ver\u00b7g\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Gram, da\u00df sie umsonst gestorben.", "tokens": ["Vom", "Gram", ",", "da\u00df", "sie", "um\u00b7sonst", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch nun, verkl\u00e4rt im Morgenglanz,", "tokens": ["Doch", "nun", ",", "ver\u00b7kl\u00e4rt", "im", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschm\u00fcckt mit ihrem Siegeskranz", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "ih\u00b7rem", "Sie\u00b7ges\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit der Wunden blut'gen Malen,", "tokens": ["Und", "mit", "der", "Wun\u00b7den", "blut'\u00b7gen", "Ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begr\u00fc\u00dfen sie den hehren Tag", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "sie", "den", "heh\u00b7ren", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach langen N\u00e4chten dunkler Schmach", "tokens": ["Nach", "lan\u00b7gen", "N\u00e4ch\u00b7ten", "dunk\u00b7ler", "Schmach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und sonnen sich in seinen Strahlen.", "tokens": ["Und", "son\u00b7nen", "sich", "in", "sei\u00b7nen", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und hochher vom Gew\u00f6lb herab,", "tokens": ["Und", "hoch\u00b7her", "vom", "Ge\u00b7w\u00f6lb", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie von den Engeln, die das Grab", "tokens": ["Wie", "von", "den", "En\u00b7geln", ",", "die", "das", "Grab"], "token_info": ["word", "word", "word", "word", "punct