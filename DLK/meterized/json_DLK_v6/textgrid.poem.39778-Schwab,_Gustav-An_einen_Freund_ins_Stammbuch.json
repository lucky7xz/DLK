{"textgrid.poem.39778": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund ins Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Schwabens treu vereinigende Weine,", "tokens": ["An", "Schwa\u00b7bens", "treu", "ver\u00b7ei\u00b7ni\u00b7gen\u00b7de", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei denen wir manch frohes Fest begangen:", "tokens": ["Bei", "de\u00b7nen", "wir", "manch", "fro\u00b7hes", "Fest", "be\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Schwabens M\u00e4dchen, und vielleicht an Eine,", "tokens": ["An", "Schwa\u00b7bens", "M\u00e4d\u00b7chen", ",", "und", "viel\u00b7leicht", "an", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "KON", "ADV", "APPR", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unter schw\u00e4b'schen T\u00e4nzen dich befangen:", "tokens": ["Die", "un\u00b7ter", "schw\u00e4b'\u00b7schen", "T\u00e4n\u00b7zen", "dich", "be\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "An Schwabens Freunde, die getreu, wie keine,", "tokens": ["An", "Schwa\u00b7bens", "Freun\u00b7de", ",", "die", "ge\u00b7treu", ",", "wie", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "ADJD", "$,", "PWAV", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An Br\u00fcdern fest, wie an Geliebten, hangen:", "tokens": ["An", "Br\u00fc\u00b7dern", "fest", ",", "wie", "an", "Ge\u00b7lieb\u00b7ten", ",", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PWAV", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "An diese drei soll dich mit s\u00fc\u00dfem Ahnen", "tokens": ["An", "die\u00b7se", "drei", "soll", "dich", "mit", "s\u00fc\u00b7\u00dfem", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "CARD", "VMFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein vaterl\u00e4nd'scher Name stets gemahnen.", "tokens": ["Mein", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7scher", "Na\u00b7me", "stets", "ge\u00b7mah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wirst du de\u00df in nord'scher Heimat inne,", "tokens": ["Und", "wirst", "du", "de\u00df", "in", "nord'\u00b7scher", "Hei\u00b7mat", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird dann ein sanft Gef\u00fchl zur\u00fcck dich ziehen;", "tokens": ["Wird", "dann", "ein", "sanft", "Ge\u00b7f\u00fchl", "zu\u00b7r\u00fcck", "dich", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kommt dir der liebe Fr\u00fchling vor die Sinne,", "tokens": ["Kommt", "dir", "der", "lie\u00b7be", "Fr\u00fch\u00b7ling", "vor", "die", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie der im fernen Schwabenland mag bl\u00fchen;", "tokens": ["Wie", "der", "im", "fer\u00b7nen", "Schwa\u00b7ben\u00b7land", "mag", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPRART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denkst du an schw\u00e4b'schen Wein und schw\u00e4b'sche Minne,", "tokens": ["Denkst", "du", "an", "schw\u00e4b'\u00b7schen", "Wein", "und", "schw\u00e4b'\u00b7sche", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie f\u00fcr dich dort alte Freunde gl\u00fchen;", "tokens": ["Und", "wie", "f\u00fcr", "dich", "dort", "al\u00b7te", "Freun\u00b7de", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So denk' auch mein, und bei viel theuren Bildern", "tokens": ["So", "denk'", "auch", "mein", ",", "und", "bei", "viel", "theu\u00b7ren", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "$,", "KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "La\u00df mein Bild auch dir deine Sehnsucht mildern.", "tokens": ["La\u00df", "mein", "Bild", "auch", "dir", "dei\u00b7ne", "Sehn\u00b7sucht", "mil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}