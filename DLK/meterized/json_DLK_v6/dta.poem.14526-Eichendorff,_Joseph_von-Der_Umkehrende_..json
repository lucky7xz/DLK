{"dta.poem.14526": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich wollte, liegt zerschlagen,", "tokens": ["Was", "ich", "woll\u00b7te", ",", "liegt", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, ich lasse ja das Klagen,", "tokens": ["Herr", ",", "ich", "las\u00b7se", "ja", "das", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Herz ist still.", "tokens": ["Und", "das", "Herz", "ist", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nun aber gieb auch Kraft, zu tragen,", "tokens": ["Nun", "a\u00b7ber", "gieb", "auch", "Kraft", ",", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "ADV", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was ich ", "tokens": ["Was", "ich"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "-+", "measure": "iambic.single"}}}}}