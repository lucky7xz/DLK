{"textgrid.poem.39725": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Wolke am Sternenhimmel", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch eine Saat von goldnen Aehren", "tokens": ["Welch", "ei\u00b7ne", "Saat", "von", "gold\u00b7nen", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Durchwandl' ich dunkle Nachtgestalt,", "tokens": ["Durch\u00b7wandl'", "ich", "dunk\u00b7le", "Nacht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schaudernd ihre H\u00e4upter kehren", "tokens": ["Die", "schau\u00b7dernd", "ih\u00b7re", "H\u00e4up\u00b7ter", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor meinem Athem rauh und kalt?", "tokens": ["Vor", "mei\u00b7nem", "A\u00b7them", "rauh", "und", "kalt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin so fremd auf diesen Auen", "tokens": ["Ich", "bin", "so", "fremd", "auf", "die\u00b7sen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wohl aus einem andern Land,", "tokens": ["Und", "wohl", "aus", "ei\u00b7nem", "an\u00b7dern", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und m\u00f6chte da mich helle schauen,", "tokens": ["Und", "m\u00f6ch\u00b7te", "da", "mich", "hel\u00b7le", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch bleib' ich mir so unbekannt.", "tokens": ["Doch", "bleib'", "ich", "mir", "so", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Tr\u00fcb gl\u00e4nzt von meinem grauen Kleide", "tokens": ["Tr\u00fcb", "gl\u00e4nzt", "von", "mei\u00b7nem", "grau\u00b7en", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Saum in dieser Fl\u00e4mmlein Schein;", "tokens": ["Der", "Saum", "in", "die\u00b7ser", "Fl\u00e4mm\u00b7lein", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie feiern ruhig ew'ge Freude,", "tokens": ["Sie", "fei\u00b7ern", "ru\u00b7hig", "ew'\u00b7ge", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da zieh' ich st\u00f6rend mitten ein.", "tokens": ["Da", "zieh'", "ich", "st\u00f6\u00b7rend", "mit\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich darf nicht frei und sicher gehen,", "tokens": ["Ich", "darf", "nicht", "frei", "und", "si\u00b7cher", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Bald f\u00fchrt mich eine leise Hand,", "tokens": ["Bald", "f\u00fchrt", "mich", "ei\u00b7ne", "lei\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bald rei\u00dft es mich mit Sturmeswehen,", "tokens": ["Bald", "rei\u00dft", "es", "mich", "mit", "Stur\u00b7mes\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und fa\u00dft mein flatterndes Gewand.", "tokens": ["Und", "fa\u00dft", "mein", "flat\u00b7tern\u00b7des", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und mir begegnen dunkle Br\u00fcder,", "tokens": ["Und", "mir", "be\u00b7geg\u00b7nen", "dunk\u00b7le", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Stumm, grau und willenlos wie ich,", "tokens": ["Stumm", ",", "grau", "und", "wil\u00b7len\u00b7los", "wie", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie schlagen fremd die Wimpern nieder,", "tokens": ["Sie", "schla\u00b7gen", "fremd", "die", "Wim\u00b7pern", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und ziehen hin, als fl\u00f6hn sie mich.", "tokens": ["Und", "zie\u00b7hen", "hin", ",", "als", "fl\u00f6hn", "sie", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wenn sch\u00fcchtern dann mein Blick sich hebet,", "tokens": ["Wenn", "sch\u00fcch\u00b7tern", "dann", "mein", "Blick", "sich", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So fahren Flammen wild heraus,", "tokens": ["So", "fah\u00b7ren", "Flam\u00b7men", "wild", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und will ich sprechen, so erbebet", "tokens": ["Und", "will", "ich", "spre\u00b7chen", ",", "so", "er\u00b7be\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Vor meinem Ton das fremde Haus.", "tokens": ["Vor", "mei\u00b7nem", "Ton", "das", "frem\u00b7de", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wo bin ich Arme denn geboren,", "tokens": ["Wo", "bin", "ich", "Ar\u00b7me", "denn", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wo wird man liebend mich empfahn?", "tokens": ["Wo", "wird", "man", "lie\u00b7bend", "mich", "em\u00b7pfahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich blick', in ihr Gebiet verloren,", "tokens": ["Ich", "blick'", ",", "in", "ihr", "Ge\u00b7biet", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Fremd diese hohe Sch\u00f6nheit an.", "tokens": ["Fremd", "die\u00b7se", "ho\u00b7he", "Sch\u00f6n\u00b7heit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Doch winkt aus wunderbarer Tiefe", "tokens": ["Doch", "winkt", "aus", "wun\u00b7der\u00b7ba\u00b7rer", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Mir nicht ein mild Erbarmen zu,", "tokens": ["Mir", "nicht", "ein", "mild", "Er\u00b7bar\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Als ob mir eine Mutter riefe,", "tokens": ["Als", "ob", "mir", "ei\u00b7ne", "Mut\u00b7ter", "rie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mich l\u00fcd' an ihre Brust zur Ruh?", "tokens": ["Mich", "l\u00fcd'", "an", "ih\u00b7re", "Brust", "zur", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wie ist mir? Wehmut l\u00f6st in Thr\u00e4nen", "tokens": ["Wie", "ist", "mir", "?", "Weh\u00b7mut", "l\u00f6st", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Hell meine graue Nachtgestalt,", "tokens": ["Hell", "mei\u00b7ne", "grau\u00b7e", "Nacht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Hinab, hinab zieht all mein Sehnen", "tokens": ["Hin\u00b7ab", ",", "hin\u00b7ab", "zieht", "all", "mein", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Vers\u00f6hnend heilige Gewalt.\u00ab \u2013", "tokens": ["Ver\u00b7s\u00f6h\u00b7nend", "hei\u00b7li\u00b7ge", "Ge\u00b7walt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und liebend rauscht's der Erd' entgegen,", "tokens": ["Und", "lie\u00b7bend", "rauscht's", "der", "Erd'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Morgen kommt mit neuer Lust:", "tokens": ["Der", "Mor\u00b7gen", "kommt", "mit", "neu\u00b7er", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blau ist die Luft, ein s\u00fc\u00dfer Regen", "tokens": ["Blau", "ist", "die", "Luft", ",", "ein", "s\u00fc\u00b7\u00dfer", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Liegt an der Mutter Erde Brust.", "tokens": ["Liegt", "an", "der", "Mut\u00b7ter", "Er\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}