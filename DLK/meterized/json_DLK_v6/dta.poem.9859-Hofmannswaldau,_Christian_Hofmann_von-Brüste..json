{"dta.poem.9859": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dj\u00df schwesterliche paar/ das voll von flammen hencket/", "tokens": ["Dj\u00df", "schwes\u00b7ter\u00b7li\u00b7che", "paar", "/", "das", "voll", "von", "flam\u00b7men", "hen\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "ADJD", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von aussen vieler hertz mit liebes-\u00f6le tr\u00e4ncket/", "tokens": ["Von", "aus\u00b7sen", "vie\u00b7ler", "hertz", "mit", "lie\u00b7bes\u00b7\u00f6le", "tr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Inwendig aber feur als wie ein Aetna schencket/", "tokens": ["In\u00b7wen\u00b7dig", "a\u00b7ber", "feur", "als", "wie", "ein", "A\u00b7et\u00b7na", "schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "KOKOM", "KOKOM", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Da doch das schnee-geb\u00fcrg sich von dem athem schwencket/", "tokens": ["Da", "doch", "das", "schnee\u00b7ge\u00b7b\u00fcrg", "sich", "von", "dem", "at\u00b7hem", "schwen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wieder von dem west der seuftzer nieder sencket/", "tokens": ["Und", "wie\u00b7der", "von", "dem", "west", "der", "seuft\u00b7zer", "nie\u00b7der", "sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "VVFIN", "ART", "ADJA", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "H\u00e4lt alle lust und lieb in seinem platz verschrencket.", "tokens": ["H\u00e4lt", "al\u00b7le", "lust", "und", "lieb", "in", "sei\u00b7nem", "platz", "ver\u00b7schren\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}