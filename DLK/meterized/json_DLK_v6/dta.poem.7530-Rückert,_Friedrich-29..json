{"dta.poem.7530": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freust du auf K\u00fcnft'ges dich, so sieh doch zu, weswegen?", "tokens": ["Freust", "du", "auf", "K\u00fcnft'\u00b7ges", "dich", ",", "so", "sieh", "doch", "zu", ",", "wes\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "$,", "ADV", "VVIMP", "ADV", "PTKVZ", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob du nur hier dich weg, ob dort dich freust entgegen?", "tokens": ["Ob", "du", "nur", "hier", "dich", "weg", ",", "ob", "dort", "dich", "freust", "ent\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "PTKVZ", "$,", "KOUS", "ADV", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Entgegen soll man sich dem Tode selber freun,", "tokens": ["Ent\u00b7ge\u00b7gen", "soll", "man", "sich", "dem", "To\u00b7de", "sel\u00b7ber", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch \u00fcbers Leben sich hinwegzuw\u00fcnschen scheun.", "tokens": ["Doch", "\u00fc\u00b7bers", "Le\u00b7ben", "sich", "hin\u00b7weg\u00b7zu\u00b7w\u00fcn\u00b7schen", "scheun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie n\u00fcchtern, freudenleer, wie \u00f6d' ein Tag, wor\u00fcber", "tokens": ["Wie", "n\u00fcch\u00b7tern", ",", "freu\u00b7den\u00b7leer", ",", "wie", "\u00f6d'", "ein", "Tag", ",", "wo\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "$,", "PWAV", "ADJD", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du nichts zu denken hast, als: W\u00e4r' auch er vor\u00fcber!", "tokens": ["Du", "nichts", "zu", "den\u00b7ken", "hast", ",", "als", ":", "W\u00e4r'", "auch", "er", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKZU", "VVINF", "VAFIN", "$,", "KOUS", "$.", "VAFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}