{"textgrid.poem.47039": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft schon bin ich \u00fcber deine Schwellen", "tokens": ["So", "oft", "schon", "bin", "ich", "\u00fc\u00b7ber", "dei\u00b7ne", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschritten und geschlichen spat und fruhe,", "tokens": ["Ge\u00b7schrit\u00b7ten", "und", "ge\u00b7schli\u00b7chen", "spat", "und", "fru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df es der Hund, ihr H\u00fcter, sieht in Ruhe,", "tokens": ["Da\u00df", "es", "der", "Hund", ",", "ihr", "H\u00fc\u00b7ter", ",", "sieht", "in", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht der M\u00fch' es wert h\u00e4lt, noch zu bellen.", "tokens": ["Und", "nicht", "der", "M\u00fch'", "es", "wert", "h\u00e4lt", ",", "noch", "zu", "bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl hab' ich auch in Kammern und in Zellen", "tokens": ["Wohl", "hab'", "ich", "auch", "in", "Kam\u00b7mern", "und", "in", "Zel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erforscht schon jeden Schrank und jede Truhe,", "tokens": ["Er\u00b7forscht", "schon", "je\u00b7den", "Schrank", "und", "je\u00b7de", "Tru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo deine Hauben und wo deine Schuhe,", "tokens": ["Wo", "dei\u00b7ne", "Hau\u00b7ben", "und", "wo", "dei\u00b7ne", "Schu\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo deine dunklen T\u00fccher, deine hellen.", "tokens": ["Wo", "dei\u00b7ne", "dunk\u00b7len", "T\u00fc\u00b7cher", ",", "dei\u00b7ne", "hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur eines hab' ich noch nicht k\u00f6nnen leider", "tokens": ["Nur", "ei\u00b7nes", "hab'", "ich", "noch", "nicht", "k\u00f6n\u00b7nen", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADV", "PTKNEG", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir auskundschaften, wo im Schrein verborgen", "tokens": ["Mir", "aus\u00b7kund\u00b7schaf\u00b7ten", ",", "wo", "im", "Schrein", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du aufbewahrst den Vorrat deiner Launen,", "tokens": ["Du", "auf\u00b7be\u00b7wahrst", "den", "Vor\u00b7rat", "dei\u00b7ner", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die du viel \u00f6fter wechselst als die Kleider,", "tokens": ["Die", "du", "viel", "\u00f6f\u00b7ter", "wech\u00b7selst", "als", "die", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich dich oft schon zwischen heut und morgen", "tokens": ["Da", "ich", "dich", "oft", "schon", "zwi\u00b7schen", "heut", "und", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald in der ros'gen sah, bald in der braunen.", "tokens": ["Bald", "in", "der", "ros'\u00b7gen", "sah", ",", "bald", "in", "der", "brau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVFIN", "$,", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}