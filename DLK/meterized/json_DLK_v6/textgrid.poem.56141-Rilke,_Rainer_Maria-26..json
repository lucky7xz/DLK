{"textgrid.poem.56141": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du aber, G\u00f6ttlicher, du, bis zuletzt noch Ert\u00f6ner,", "tokens": ["Du", "a\u00b7ber", ",", "G\u00f6tt\u00b7li\u00b7cher", ",", "du", ",", "bis", "zu\u00b7letzt", "noch", "Er\u00b7t\u00f6\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PPER", "$,", "KOUS", "ADV", "ADV", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "da ihn der Schwarm der verschm\u00e4hten M\u00e4naden befiel,", "tokens": ["da", "ihn", "der", "Schwarm", "der", "ver\u00b7schm\u00e4h\u00b7ten", "M\u00e4\u00b7na\u00b7den", "be\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "hast ihr Geschrei \u00fcbert\u00f6nt mit Ordnung, du Sch\u00f6ner,", "tokens": ["hast", "ihr", "Ge\u00b7schrei", "\u00fc\u00b7ber\u00b7t\u00f6nt", "mit", "Ord\u00b7nung", ",", "du", "Sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "PPER", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "aus den Zerst\u00f6renden stieg dein erbauendes Spiel.", "tokens": ["aus", "den", "Zer\u00b7st\u00f6\u00b7ren\u00b7den", "stieg", "dein", "er\u00b7bau\u00b7en\u00b7des", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Keine war da, da\u00df sie Haupt dir und Leier zerst\u00f6r.", "tokens": ["Kei\u00b7ne", "war", "da", ",", "da\u00df", "sie", "Haupt", "dir", "und", "Lei\u00b7er", "zer\u00b7st\u00f6r", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "$,", "KOUS", "PPER", "NN", "PPER", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wie sie auch rangen und rasten, und alle die scharfen", "tokens": ["Wie", "sie", "auch", "ran\u00b7gen", "und", "ras\u00b7ten", ",", "und", "al\u00b7le", "die", "schar\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "KON", "PIS", "ART", "ADJA"], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Steine, die sie nach deinem Herzen warfen,", "tokens": ["Stei\u00b7ne", ",", "die", "sie", "nach", "dei\u00b7nem", "Her\u00b7zen", "war\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "wurden zu Sanftem an dir und begabt mit Geh\u00f6r.", "tokens": ["wur\u00b7den", "zu", "Sanf\u00b7tem", "an", "dir", "und", "be\u00b7gabt", "mit", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPR", "PPER", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Schlie\u00dflich zerschlugen sie dich, von der Rache gehetzt,", "tokens": ["Schlie\u00df\u00b7lich", "zer\u00b7schlu\u00b7gen", "sie", "dich", ",", "von", "der", "Ra\u00b7che", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "w\u00e4hrend dein Klang noch in L\u00f6wen und Felsen verweilte", "tokens": ["w\u00e4h\u00b7rend", "dein", "Klang", "noch", "in", "L\u00f6\u00b7wen", "und", "Fel\u00b7sen", "ver\u00b7weil\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "und in den B\u00e4umen und V\u00f6geln. Dort singst du noch jetzt.", "tokens": ["und", "in", "den", "B\u00e4u\u00b7men", "und", "V\u00f6\u00b7geln", ".", "Dort", "singst", "du", "noch", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-++--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "O du verlorener Gott! Du unendliche Spur!", "tokens": ["O", "du", "ver\u00b7lo\u00b7re\u00b7ner", "Gott", "!", "Du", "un\u00b7end\u00b7li\u00b7che", "Spur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur weil dich rei\u00dfend zuletzt die Feindschaft verteilte,", "tokens": ["Nur", "weil", "dich", "rei\u00b7\u00dfend", "zu\u00b7letzt", "die", "Feind\u00b7schaft", "ver\u00b7teil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "sind wir die H\u00f6renden jetzt und ein Mund der Natur.", "tokens": ["sind", "wir", "die", "H\u00f6\u00b7ren\u00b7den", "jetzt", "und", "ein", "Mund", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}