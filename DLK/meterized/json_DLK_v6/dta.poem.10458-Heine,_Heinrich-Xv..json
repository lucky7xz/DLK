{"dta.poem.10458": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Da droben auf jenem Berge,", "tokens": ["Da", "dro\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da steht ein feines Schlo\u00df,", "tokens": ["Da", "steht", "ein", "fei\u00b7nes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wohnen drei sch\u00f6ne Fr\u00e4ulein,", "tokens": ["Da", "woh\u00b7nen", "drei", "sch\u00f6\u00b7ne", "Fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Von denen ich Liebe geno\u00df.", "tokens": ["Von", "de\u00b7nen", "ich", "Lie\u00b7be", "ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sonnabend k\u00fc\u00dfte mich Jette,", "tokens": ["Sonn\u00b7a\u00b7bend", "k\u00fc\u00df\u00b7te", "mich", "Jet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und Sonntag die Julia,", "tokens": ["Und", "Sonn\u00b7tag", "die", "Ju\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Montag die Kunigunde,", "tokens": ["Und", "Mon\u00b7tag", "die", "Ku\u00b7ni\u00b7gun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die hat mich erdr\u00fcckt beinah.", "tokens": ["Die", "hat", "mich", "er\u00b7dr\u00fcckt", "bei\u00b7nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch Diensttag war eine Fete", "tokens": ["Doch", "Dienst\u00b7tag", "war", "ei\u00b7ne", "Fe\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Bei meinen drei Fr\u00e4ulein im Schlo\u00df;", "tokens": ["Bei", "mei\u00b7nen", "drei", "Fr\u00e4u\u00b7lein", "im", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachbarschafts-Herren und Damen,", "tokens": ["Die", "Nach\u00b7bar\u00b7schafts\u00b7Her\u00b7ren", "und", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die kamen zu Wagen und Ro\u00df.", "tokens": ["Die", "ka\u00b7men", "zu", "Wa\u00b7gen", "und", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ich aber war nicht geladen,", "tokens": ["Ich", "a\u00b7ber", "war", "nicht", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und das habt ihr dumm gemacht!", "tokens": ["Und", "das", "habt", "ihr", "dumm", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die zischelnden Muhmen und Basen,", "tokens": ["Die", "zi\u00b7scheln\u00b7den", "Muh\u00b7men", "und", "Ba\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die merkten's und haben gelacht.", "tokens": ["Die", "merk\u00b7ten's", "und", "ha\u00b7ben", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VAFIN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}