{"textgrid.poem.35260": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift der M\u00e4\u00dfigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gebrauch im deutschen Land,", "tokens": ["Der", "Ge\u00b7brauch", "im", "deut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwelgen und Gesundheit trinken,", "tokens": ["Schwel\u00b7gen", "und", "Ge\u00b7sund\u00b7heit", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat in's Kloster mich gebannt,", "tokens": ["Hat", "in's", "Klos\u00b7ter", "mich", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da l\u00e4\u00dft mich der M\u00f6nch auch sinken,", "tokens": ["Da", "l\u00e4\u00dft", "mich", "der", "M\u00f6nch", "auch", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df ich, aller Tugend Zier,", "tokens": ["Da\u00df", "ich", ",", "al\u00b7ler", "Tu\u00b7gend", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liege l\u00e4ngst begraben hier.", "tokens": ["Lie\u00b7ge", "l\u00e4ngst", "be\u00b7gra\u00b7ben", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}