{"textgrid.poem.35499": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Nachtwache", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schweren Tr\u00e4umen hob ich mich. \u2013 Noch lag gebreitet schweigend, gro\u00df,", "tokens": ["Aus", "schwe\u00b7ren", "Tr\u00e4u\u00b7men", "hob", "ich", "mich", ".", "\u2013", "Noch", "lag", "ge\u00b7brei\u00b7tet", "schwei\u00b7gend", ",", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$.", "$(", "ADV", "VVFIN", "VVPP", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Das schwarze Bahrgewand der Nacht, ein r\u00e4tselschwangrer Riesenscho\u00df \u2013", "tokens": ["Das", "schwar\u00b7ze", "Bahr\u00b7ge\u00b7wand", "der", "Nacht", ",", "ein", "r\u00e4t\u00b7sel\u00b7schwang\u00b7rer", "Rie\u00b7sen\u00b7scho\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Noch k\u00fcndete mit falbem Schein sich nicht der junge Morgen an \u2013", "tokens": ["Noch", "k\u00fcn\u00b7de\u00b7te", "mit", "fal\u00b7bem", "Schein", "sich", "nicht", "der", "jun\u00b7ge", "Mor\u00b7gen", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PRF", "PTKNEG", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ich aber schrie zu Gott empor: Erl\u00f6se mich von diesem Bann!", "tokens": ["Ich", "a\u00b7ber", "schrie", "zu", "Gott", "em\u00b7por", ":", "Er\u00b7l\u00f6\u00b7se", "mich", "von", "die\u00b7sem", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Erl\u00f6se mich von diesem Fluch, der auf mir liegt wie Panzererz \u2013", "tokens": ["Er\u00b7l\u00f6\u00b7se", "mich", "von", "die\u00b7sem", "Fluch", ",", "der", "auf", "mir", "liegt", "wie", "Pan\u00b7ze\u00b7rerz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Hinsiecht mein Leib, mein Geist verstumpft, verdorrt ist dein lebend'ges Herz \u2013", "tokens": ["Hin\u00b7siecht", "mein", "Leib", ",", "mein", "Geist", "ver\u00b7stumpft", ",", "ver\u00b7dorrt", "ist", "dein", "le\u00b7ben\u00b7d'\u00b7ges", "Herz", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,", "VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+----+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Mein Herz, das sonst mit reicher Kraft die Welt in Weh und Lust verstand:", "tokens": ["Mein", "Herz", ",", "das", "sonst", "mit", "rei\u00b7cher", "Kraft", "die", "Welt", "in", "Weh", "und", "Lust", "ver\u00b7stand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "ADJD", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Verbl\u00f6det liegt's und sucht und sucht und findet nimmer heil'ges Land ...", "tokens": ["Ver\u00b7bl\u00f6\u00b7det", "liegt's", "und", "sucht", "und", "sucht", "und", "fin\u00b7det", "nim\u00b7mer", "heil'\u00b7ges", "Land", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Und findet nirgends eine Statt, da es von neuem wurzeln mag \u2013", "tokens": ["Und", "fin\u00b7det", "nir\u00b7gends", "ei\u00b7ne", "Statt", ",", "da", "es", "von", "neu\u00b7em", "wur\u00b7zeln", "mag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Nacht ist schwarz ihm, sternenleer und sonnenlos der lange Tag \u2013", "tokens": ["Die", "Nacht", "ist", "schwarz", "ihm", ",", "ster\u00b7nen\u00b7leer", "und", "son\u00b7nen\u00b7los", "der", "lan\u00b7ge", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PPER", "$,", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und alles, dem es sonst geglaubt, dem es in Glut entgegenschlug,", "tokens": ["Und", "al\u00b7les", ",", "dem", "es", "sonst", "ge\u00b7glaubt", ",", "dem", "es", "in", "Glut", "ent\u00b7ge\u00b7gen\u00b7schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Verlor die Farbe, lie\u00df den Wert und wandelt' sich in Dunst und Trug ...", "tokens": ["Ver\u00b7lor", "die", "Far\u00b7be", ",", "lie\u00df", "den", "Wert", "und", "wan\u00b7delt'", "sich", "in", "Dunst", "und", "Trug", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Sank auf die Lider mitleidsvoll der Schlaf geheimen Flugs herab \u2013", "tokens": ["Sank", "auf", "die", "Li\u00b7der", "mit\u00b7le\u00b7ids\u00b7voll", "der", "Schlaf", "ge\u00b7hei\u00b7men", "Flugs", "her\u00b7ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Verga\u00df es, da\u00df die Welt doch nur ein bl\u00fcten\u00fcberwuchert Grab \u2013", "tokens": ["Ver\u00b7ga\u00df", "es", ",", "da\u00df", "die", "Welt", "doch", "nur", "ein", "bl\u00fc\u00b7ten\u00b7\u00fc\u00b7berw\u00b7uc\u00b7hert", "Grab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "ART", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Verga\u00df es, da\u00df ein jeder Schlag, den es nach ehernem Gebot \u2013", "tokens": ["Ver\u00b7ga\u00df", "es", ",", "da\u00df", "ein", "je\u00b7der", "Schlag", ",", "den", "es", "nach", "e\u00b7her\u00b7nem", "Ge\u00b7bot", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Hinzucken mu\u00df, ein Opferst\u00fcck der Kreatur dem Herrscher Tod:", "tokens": ["Hin\u00b7zu\u00b7cken", "mu\u00df", ",", "ein", "Op\u00b7fer\u00b7st\u00fcck", "der", "Kre\u00b7a\u00b7tur", "dem", "Herr\u00b7scher", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Dann tritt, als h\u00e4tte ihn gesandt der Furien beutedurst'ger Schwarm,", "tokens": ["Dann", "tritt", ",", "als", "h\u00e4t\u00b7te", "ihn", "ge\u00b7sandt", "der", "Fu\u00b7ri\u00b7en", "beu\u00b7te\u00b7dur\u00b7st'\u00b7ger", "Schwarm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zu meinem Lager hin ein Geist und r\u00fcttelt mich mit Geisterarm", "tokens": ["Zu", "mei\u00b7nem", "La\u00b7ger", "hin", "ein", "Geist", "und", "r\u00fct\u00b7telt", "mich", "mit", "Geis\u00b7ter\u00b7arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und st\u00f6\u00dft mich aus dem Paradies, da ich vergessen, was ich litt,", "tokens": ["Und", "st\u00f6\u00dft", "mich", "aus", "dem", "Pa\u00b7ra\u00b7dies", ",", "da", "ich", "ver\u00b7ges\u00b7sen", ",", "was", "ich", "litt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und fl\u00f6\u00dft in meine Tr\u00e4ume Blut und zerrt des Wahnsinns Schatten mit! ...", "tokens": ["Und", "fl\u00f6\u00dft", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "Blut", "und", "zerrt", "des", "Wahn\u00b7sinns", "Schat\u00b7ten", "mit", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "O Herr! O Herr! Als tr\u00fcge ich die S\u00fcndenlast der ganzen Welt,", "tokens": ["O", "Herr", "!", "O", "Herr", "!", "Als", "tr\u00fc\u00b7ge", "ich", "die", "S\u00fcn\u00b7den\u00b7last", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$.", "KOUS", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "So schwer liegt's auf mir \u2013 und ich bin doch nur ein fadenschein'ger Held!", "tokens": ["So", "schwer", "liegt's", "auf", "mir", "\u2013", "und", "ich", "bin", "doch", "nur", "ein", "fa\u00b7den\u00b7schein'\u00b7ger", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPER", "$(", "KON", "PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie einst vor deines Mundes Hauch zerbrochen Babels Riesendom,", "tokens": ["Wie", "einst", "vor", "dei\u00b7nes", "Mun\u00b7des", "Hauch", "zer\u00b7bro\u00b7chen", "Ba\u00b7bels", "Rie\u00b7sen\u00b7dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVPP", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So brach mein Herz, das einst so stolz, zermalmt von dieser L\u00fcste Strom!", "tokens": ["So", "brach", "mein", "Herz", ",", "das", "einst", "so", "stolz", ",", "zer\u00b7malmt", "von", "die\u00b7ser", "L\u00fcs\u00b7te", "Strom", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,", "VVPP", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Von dieser L\u00fcste Flutenschwall, der siedend alle Welt durchkreist", "tokens": ["Von", "die\u00b7ser", "L\u00fcs\u00b7te", "Flu\u00b7ten\u00b7schwall", ",", "der", "sie\u00b7dend", "al\u00b7le", "Welt", "durch\u00b7kreist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und jedes Herz dir abgewandt und sich geknechtet jeden Geist \u2013", "tokens": ["Und", "je\u00b7des", "Herz", "dir", "ab\u00b7ge\u00b7wandt", "und", "sich", "ge\u00b7knech\u00b7tet", "je\u00b7den", "Geist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVPP", "KON", "PRF", "VVPP", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "So brach es, das verblendet, j\u00e4h der wahren Freiheit ganz verga\u00df", "tokens": ["So", "brach", "es", ",", "das", "ver\u00b7blen\u00b7det", ",", "j\u00e4h", "der", "wah\u00b7ren", "Frei\u00b7heit", "ganz", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "VVPP", "$,", "ADJD", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und sich, ein gieriges Gew\u00fcrm, ins \u00fcpp'ge Fleisch der S\u00fcnde fra\u00df! ...", "tokens": ["Und", "sich", ",", "ein", "gie\u00b7ri\u00b7ges", "Ge\u00b7w\u00fcrm", ",", "ins", "\u00fcpp'\u00b7ge", "Fleisch", "der", "S\u00fcn\u00b7de", "fra\u00df", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PRF", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "So brach es, als es aufgeschreckt entsetzt erkannt, was es getan \u2013", "tokens": ["So", "brach", "es", ",", "als", "es", "auf\u00b7ge\u00b7schreckt", "ent\u00b7setzt", "er\u00b7kannt", ",", "was", "es", "ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "VVPP", "$,", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zertr\u00fcmmert liegt nun all sein Gl\u00fcck, versch\u00fcttet seines Heiles Bahn ...", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "liegt", "nun", "all", "sein", "Gl\u00fcck", ",", "ver\u00b7sch\u00fct\u00b7tet", "sei\u00b7nes", "Hei\u00b7les", "Bahn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PIAT", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wohl zuckt es noch in irrem Schlag, doch l\u00e4ngst entfloh die alte Kraft \u2013", "tokens": ["Wohl", "zuckt", "es", "noch", "in", "ir\u00b7rem", "Schlag", ",", "doch", "l\u00e4ngst", "ent\u00b7floh", "die", "al\u00b7te", "Kraft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Doch l\u00e4ngst entwich der alte Mut und die Prophetenleidenschaft!", "tokens": ["Doch", "l\u00e4ngst", "ent\u00b7wich", "der", "al\u00b7te", "Mut", "und", "die", "Pro\u00b7phe\u00b7ten\u00b7lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Zerbrochen liegt mein Saitenspiel \u2013 nur manchmal schrillt zerspalten, hart,", "tokens": ["Zer\u00b7bro\u00b7chen", "liegt", "mein", "Sai\u00b7ten\u00b7spiel", "\u2013", "nur", "manch\u00b7mal", "schrillt", "zer\u00b7spal\u00b7ten", ",", "hart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "ADV", "VVFIN", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ein Ton noch nach und schreit mir zu die nachtumflorte Gegenwart \u2013", "tokens": ["Ein", "Ton", "noch", "nach", "und", "schreit", "mir", "zu", "die", "nach\u00b7tum\u00b7flor\u00b7te", "Ge\u00b7gen\u00b7wart", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Geweckt von eines D\u00e4mons Hand, dem ich verfallen regungslos \u2013", "tokens": ["Ge\u00b7weckt", "von", "ei\u00b7nes", "D\u00e4\u00b7mons", "Hand", ",", "dem", "ich", "ver\u00b7fal\u00b7len", "re\u00b7gungs\u00b7los", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "VVINF", "ADJD", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "O Herr! O Herr! Erl\u00f6se mich! Die Qual ist", "tokens": ["O", "Herr", "!", "O", "Herr", "!", "Er\u00b7l\u00f6\u00b7se", "mich", "!", "Die", "Qual", "ist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "NE", "NN", "$.", "NN", "PPER", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Zu einem Riesenleibe hat die Menschheit sich verdichtet rings \u2013", "tokens": ["Zu", "ei\u00b7nem", "Rie\u00b7sen\u00b7lei\u00b7be", "hat", "die", "Menschheit", "sich", "ver\u00b7dich\u00b7tet", "rings", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "PRF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und dieser Leib ist schw\u00e4rensiech \u2013 aus seinen Augen grinst die Sphinx \u2013", "tokens": ["Und", "die\u00b7ser", "Leib", "ist", "schw\u00e4\u00b7ren\u00b7siech", "\u2013", "aus", "sei\u00b7nen", "Au\u00b7gen", "grinst", "die", "Sphinx", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADJD", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie Pestgestank durchst\u00e4ubt's die Luft, der \u00fcber die Gefilde streift,", "tokens": ["Wie", "Pest\u00b7ge\u00b7stank", "durch\u00b7st\u00e4ubt's", "die", "Luft", ",", "der", "\u00fc\u00b7ber", "die", "Ge\u00b7fil\u00b7de", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und alles stirbt und alles dorrt, was seiner Arme Klammer greift!", "tokens": ["Und", "al\u00b7les", "stirbt", "und", "al\u00b7les", "dorrt", ",", "was", "sei\u00b7ner", "Ar\u00b7me", "Klam\u00b7mer", "greift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.11": {"line.1": {"text": "In meine Tr\u00e4ume str\u00f6mt es wild \u2013 verzerrt zur Fratze mein Gesicht \u2013", "tokens": ["In", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "str\u00f6mt", "es", "wild", "\u2013", "ver\u00b7zerrt", "zur", "Frat\u00b7ze", "mein", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$(", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Jach schreck' ich auf, als riefe mich Posaunenschrei zum Weltgericht \u2013", "tokens": ["Jach", "schreck'", "ich", "auf", ",", "als", "rie\u00b7fe", "mich", "Po\u00b7sau\u00b7nen\u00b7schrei", "zum", "Welt\u00b7ge\u00b7richt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Mir ist's, als w\u00fcchse geisterbleich aus Nebeln eine wei\u00dfe Hand \u2013", "tokens": ["Mir", "ist's", ",", "als", "w\u00fcch\u00b7se", "geis\u00b7ter\u00b7bleich", "aus", "Ne\u00b7beln", "ei\u00b7ne", "wei\u00b7\u00dfe", "Hand", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN", "ADJD", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Schrieb ", "tokens": ["Schrieb"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.12": {"line.1": {"text": "Ja! ", "tokens": ["Ja", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Blutrot durchzittert es die Luft, f\u00e4hrt blendend hin wie Wetterschein \u2013", "tokens": ["Blut\u00b7rot", "durch\u00b7zit\u00b7tert", "es", "die", "Luft", ",", "f\u00e4hrt", "blen\u00b7dend", "hin", "wie", "Wet\u00b7ter\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie Trommelwirbel gellt es schrill, wie unterirdisches Gegroll \u2013", "tokens": ["Wie", "Trom\u00b7mel\u00b7wir\u00b7bel", "gellt", "es", "schrill", ",", "wie", "un\u00b7ter\u00b7ir\u00b7di\u00b7sches", "Ge\u00b7groll", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "O Herr! O Herr! Erbarme dich! Nimm meiner reinen Bu\u00dfe Zoll! ...", "tokens": ["O", "Herr", "!", "O", "Herr", "!", "Er\u00b7bar\u00b7me", "dich", "!", "Nimm", "mei\u00b7ner", "rei\u00b7nen", "Bu\u00b7\u00dfe", "Zoll", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$.", "NN", "PPER", "$.", "NE", "PPOSAT", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Nimm von mir dieses Angstgesicht \u2013 o f\u00fchr herauf das goldne Licht! ...", "tokens": ["Nimm", "von", "mir", "die\u00b7ses", "Angst\u00b7ge\u00b7sicht", "\u2013", "o", "f\u00fchr", "her\u00b7auf", "das", "gold\u00b7ne", "Licht", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PPER", "PDAT", "NN", "$(", "FM", "VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Dann flattert's hin im Morgenwind, der zu den Kreaturen spricht", "tokens": ["Dann", "flat\u00b7tert's", "hin", "im", "Mor\u00b7gen\u00b7wind", ",", "der", "zu", "den", "Kre\u00b7a\u00b7tu\u00b7ren", "spricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "In seines S\u00e4uselns Gnadenton: \u00bbder Herr die Finsternis zerschlug!", "tokens": ["In", "sei\u00b7nes", "S\u00e4u\u00b7selns", "Gna\u00b7den\u00b7ton", ":", "\u00bb", "der", "Herr", "die", "Fins\u00b7ter\u00b7nis", "zer\u00b7schlug", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$.", "$(", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Noch einmal flammt die Sonne auf \u2013 noch einmal t\u00f6tet er den Fluch! ...\u00ab", "tokens": ["Noch", "ein\u00b7mal", "flammt", "die", "Son\u00b7ne", "auf", "\u2013", "noch", "ein\u00b7mal", "t\u00f6\u00b7tet", "er", "den", "Fluch", "!", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "$(", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Noch einmal, Herr, erbarme dich \u2013 auch mich schlie\u00df' deine Gnade ein! ...", "tokens": ["Noch", "ein\u00b7mal", ",", "Herr", ",", "er\u00b7bar\u00b7me", "dich", "\u2013", "auch", "mich", "schlie\u00df'", "dei\u00b7ne", "Gna\u00b7de", "ein", "!", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$(", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Dann weitet meine Seele sich \u2013 will sich verj\u00fcngter Kraft dir weihn \u2013", "tokens": ["Dann", "wei\u00b7tet", "mei\u00b7ne", "See\u00b7le", "sich", "\u2013", "will", "sich", "ver\u00b7j\u00fcng\u00b7ter", "Kraft", "dir", "weihn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "$(", "VMFIN", "PRF", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und ob die Brandung mich umdr\u00f6hnt, mit S\u00fcndenfingern mich belegt:", "tokens": ["Und", "ob", "die", "Bran\u00b7dung", "mich", "um\u00b7dr\u00f6hnt", ",", "mit", "S\u00fcn\u00b7den\u00b7fin\u00b7gern", "mich", "be\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Du segnest mich von neuem, Herr, da\u00df jedes Herz mein Lied bewegt! ...", "tokens": ["Du", "seg\u00b7nest", "mich", "von", "neu\u00b7em", ",", "Herr", ",", "da\u00df", "je\u00b7des", "Herz", "mein", "Lied", "be\u00b7wegt", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "$,", "NN", "$,", "KOUS", "PIAT", "NN", "PPOSAT