{"textgrid.poem.36435": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00f6pschen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebes kleines M\u00f6pschen,", "tokens": ["Du", "lie\u00b7bes", "klei\u00b7nes", "M\u00f6p\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie hast du mich gefunden?", "tokens": ["Wie", "hast", "du", "mich", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm her! auf meinem Schosse", "tokens": ["Komm", "her", "!", "auf", "mei\u00b7nem", "Schos\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will ich dich sanfte streicheln,", "tokens": ["Will", "ich", "dich", "sanf\u00b7te", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und du sollt mir erz\u00e4len,", "tokens": ["Und", "du", "sollt", "mir", "er\u00b7z\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Warum du mich besuchest.", "tokens": ["Wa\u00b7rum", "du", "mich", "be\u00b7su\u00b7chest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herr hat mir dis Zimmer", "tokens": ["Mein", "Herr", "hat", "mir", "dis", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PDS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dieses Haus gewiesen,", "tokens": ["Und", "die\u00b7ses", "Haus", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schikkt mich her zum W\u00e4chter.", "tokens": ["Und", "schikkt", "mich", "her", "zum", "W\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APZR", "APPRART", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was sollt du denn bewachen?", "tokens": ["Was", "sollt", "du", "denn", "be\u00b7wa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch selber, sch\u00f6ne Nimfe,", "tokens": ["Euch", "sel\u00b7ber", ",", "sch\u00f6\u00b7ne", "Nim\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sollt mit keinem andern", "tokens": ["Ihr", "sollt", "mit", "kei\u00b7nem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als mit Filemon sprechen,", "tokens": ["Als", "mit", "Fi\u00b7le\u00b7mon", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mit keinem andern scherzen,", "tokens": ["Mit", "kei\u00b7nem", "an\u00b7dern", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit keinem andern spielen;", "tokens": ["Mit", "kei\u00b7nem", "an\u00b7dern", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn ihrs etwa th\u00e4tet:", "tokens": ["Und", "wenn", "ihrs", "et\u00b7wa", "th\u00e4\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So soll ich um mich beissen.", "tokens": ["So", "soll", "ich", "um", "mich", "beis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich bin ein treuer Diener,", "tokens": ["Ich", "bin", "ein", "treu\u00b7er", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Drum h\u00fctet euch vor Bisse.", "tokens": ["Drum", "h\u00fc\u00b7tet", "euch", "vor", "Bis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ich leide keinen Fremden,", "tokens": ["Ich", "lei\u00b7de", "kei\u00b7nen", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der euch die Bakken streichelt,", "tokens": ["Der", "euch", "die", "Bak\u00b7ken", "strei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der sich mit seinen Lippen", "tokens": ["Der", "sich", "mit", "sei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Auf eure Lippen dr\u00fckket,", "tokens": ["Auf", "eu\u00b7re", "Lip\u00b7pen", "dr\u00fck\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und dann zur\u00fckke ziehet", "tokens": ["Und", "dann", "zu\u00b7r\u00fck\u00b7ke", "zie\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und eure H\u00e4nde dr\u00fckket.", "tokens": ["Und", "eu\u00b7re", "H\u00e4n\u00b7de", "dr\u00fck\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn aber eine Freundinn", "tokens": ["Wenn", "a\u00b7ber", "ei\u00b7ne", "Freun\u00b7dinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einem langen Kleide", "tokens": ["In", "ei\u00b7nem", "lan\u00b7gen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schlafgemach besuchet,", "tokens": ["Mein", "Schlaf\u00b7ge\u00b7mach", "be\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirst du es auch nicht leiden,", "tokens": ["Wirst", "du", "es", "auch", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich ihr was verstatte?", "tokens": ["Wenn", "ich", "ihr", "was", "ver\u00b7stat\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Davon hat mich Filemon", "tokens": ["Da\u00b7von", "hat", "mich", "Fi\u00b7le\u00b7mon"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "NE"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nicht v\u00f6llig unterrichtet.", "tokens": ["Nicht", "v\u00f6l\u00b7lig", "un\u00b7ter\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geschwinde la\u00dft mich laufen,", "tokens": ["Ge\u00b7schwin\u00b7de", "la\u00dft", "mich", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich will ihn drum befragen.", "tokens": ["Ich", "will", "ihn", "drum", "be\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}