{"textgrid.poem.42065": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson tanzt", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut aber gr\u00fc\u00df ich dich, o Gott, im Brand", "tokens": ["Heut", "a\u00b7ber", "gr\u00fc\u00df", "ich", "dich", ",", "o", "Gott", ",", "im", "Brand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des fr\u00fchen Morgens, da ich mich befreit", "tokens": ["Des", "fr\u00fc\u00b7hen", "Mor\u00b7gens", ",", "da", "ich", "mich", "be\u00b7freit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$,", "KOUS", "PPER", "PRF", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aufschwang im Tanz wie eine Welt, verwandt", "tokens": ["Auf\u00b7schwang", "im", "Tanz", "wie", "ei\u00b7ne", "Welt", ",", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPRART", "NN", "KOKOM", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir Gleichgewicht; da, ein vermorschtes Kleid,", "tokens": ["Dir", "Gleich\u00b7ge\u00b7wicht", ";", "da", ",", "ein", "ver\u00b7morschtes", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Hinsanken Trauer, Lust. Nichts war, das blieb", "tokens": ["Hin\u00b7san\u00b7ken", "Trau\u00b7er", ",", "Lust", ".", "Nichts", "war", ",", "das", "blieb"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "$.", "PIS", "VAFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus dem gebundnen Sein; wie fiel Gewicht", "tokens": ["Aus", "dem", "ge\u00b7bund\u00b7nen", "Sein", ";", "wie", "fiel", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PWAV", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von Arm und Fu\u00df: ein leerer Raum, so trieb", "tokens": ["Von", "Arm", "und", "Fu\u00df", ":", "ein", "lee\u00b7rer", "Raum", ",", "so", "trieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich hin im Raum, ins Leere, es durchbricht", "tokens": ["Ich", "hin", "im", "Raum", ",", "ins", "Lee\u00b7re", ",", "es", "durch\u00b7bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mich Luft, mich Licht, mich ungeheurer Glanz,", "tokens": ["Mich", "Luft", ",", "mich", "Licht", ",", "mich", "un\u00b7ge\u00b7heu\u00b7rer", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In den ich barst, Gott; bin ich, bin ich nicht?!", "tokens": ["In", "den", "ich", "barst", ",", "Gott", ";", "bin", "ich", ",", "bin", "ich", "nicht", "?!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADV", "$,", "NN", "$.", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich tanze dir den grenzenlosen Tanz.", "tokens": ["Ich", "tan\u00b7ze", "dir", "den", "gren\u00b7zen\u00b7lo\u00b7sen", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}