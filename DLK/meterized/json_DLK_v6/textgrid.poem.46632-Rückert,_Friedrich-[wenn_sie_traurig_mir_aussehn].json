{"textgrid.poem.46632": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn sie traurig mir aussehn]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sie traurig mir aussehn,", "tokens": ["Wenn", "sie", "trau\u00b7rig", "mir", "aus\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich gar sie nicht ausstehn.", "tokens": ["Kann", "ich", "gar", "sie", "nicht", "aus\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und noch weniger mag ich's,", "tokens": ["Und", "noch", "we\u00b7ni\u00b7ger", "mag", "ich's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VMFIN", "PIS", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenn sie froh wie zum Schmaus gehn", "tokens": ["Wenn", "sie", "froh", "wie", "zum", "Schmaus", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KOKOM", "APPRART", "NN", "VVINF"], "meter": "--++-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Darum halt' ich f\u00fcrs Beste,", "tokens": ["Da\u00b7rum", "halt'", "ich", "f\u00fcrs", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gar nicht mehr aus dem Haus gehn,", "tokens": ["Gar", "nicht", "mehr", "aus", "dem", "Haus", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und auf Alle von oben", "tokens": ["Und", "auf", "Al\u00b7le", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "APPR", "ADV"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aus dem Fenster heraussehn,", "tokens": ["Aus", "dem", "Fens\u00b7ter", "her\u00b7aus\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Unneugierig, woher sie", "tokens": ["Un\u00b7neu\u00b7gie\u00b7rig", ",", "wo\u00b7her", "sie"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "PPER"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Kommen, und wohinaus gehn,", "tokens": ["Kom\u00b7men", ",", "und", "wo\u00b7hi\u00b7naus", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ob unter den H\u00fcten", "tokens": ["Und", "ob", "un\u00b7ter", "den", "H\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sie vergn\u00fcgt oder kraus sehn.", "tokens": ["Sie", "ver\u00b7gn\u00fcgt", "o\u00b7der", "kraus", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}