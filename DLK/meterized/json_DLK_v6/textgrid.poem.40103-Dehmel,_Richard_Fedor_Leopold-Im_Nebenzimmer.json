{"textgrid.poem.40103": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Im Nebenzimmer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigen auf Einmal", "tokens": ["Schwei\u00b7gen", "auf", "Ein\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "drinnen? \u2013 Horch!", "tokens": ["drin\u00b7nen", "?", "\u2013", "Horch", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["ADV", "$.", "$(", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Quellende Laute,", "tokens": ["Quel\u00b7len\u00b7de", "Lau\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "schmeichelnd raunende,", "tokens": ["schmei\u00b7chelnd", "rau\u00b7nen\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "fl\u00fcsternd ber\u00fcckende,", "tokens": ["fl\u00fcs\u00b7ternd", "be\u00b7r\u00fc\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "perlen daher;", "tokens": ["per\u00b7len", "da\u00b7her", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PAV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "quellende Kl\u00e4nge,", "tokens": ["quel\u00b7len\u00b7de", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "jauchzend wallende,", "tokens": ["jauch\u00b7zend", "wal\u00b7len\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "rauschend st\u00fcrmende,", "tokens": ["rau\u00b7schend", "st\u00fcr\u00b7men\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "dr\u00e4ngen, umwogen, erw\u00fchlen mich.", "tokens": ["dr\u00e4n\u00b7gen", ",", "um\u00b7wo\u00b7gen", ",", "er\u00b7w\u00fch\u00b7len", "mich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Weib, dich schauen!", "tokens": ["Weib", ",", "dich", "schau\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Gl\u00fchender Lippen schwellende Knospe,", "tokens": ["Gl\u00fc\u00b7hen\u00b7der", "Lip\u00b7pen", "schwel\u00b7len\u00b7de", "Knos\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "schwellender gl\u00fcht sie, erbl\u00fcht zum Lied.", "tokens": ["schwel\u00b7len\u00b7der", "gl\u00fcht", "sie", ",", "er\u00b7bl\u00fcht", "zum", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Schauen \u2013! lauschen \u2013!", "tokens": ["Schau\u00b7en", "\u2013", "!", "lau\u00b7schen", "\u2013", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "punct"], "pos": ["NN", "$(", "$.", "VVINF", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Weib, die ", "tokens": ["Weib", ",", "die"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PRELS"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Weib, die Laute:", "tokens": ["Weib", ",", "die", "Lau\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "f\u00fchlt sie dein Herz?!", "tokens": ["f\u00fchlt", "sie", "dein", "Herz", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}