ord", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Entgegen dr\u00e4ngten aus der todten Zacke,", "tokens": ["Ent\u00b7ge\u00b7gen", "dr\u00e4ng\u00b7ten", "aus", "der", "tod\u00b7ten", "Za\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Wenn diese, dumpf und klein, zu ew'gem Frieden", "tokens": ["Wenn", "die\u00b7se", ",", "dumpf", "und", "klein", ",", "zu", "ew'\u00b7gem", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sich gern verschlossen h\u00e4tten in die Schlacke,", "tokens": ["Sich", "gern", "ver\u00b7schlos\u00b7sen", "h\u00e4t\u00b7ten", "in", "die", "Schla\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Damit er, den sie nur mit Schaudern ahnten,", "tokens": ["Da\u00b7mit", "er", ",", "den", "sie", "nur", "mit", "Schau\u00b7dern", "ahn\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sie nicht, vor\u00fcber wandelnd, pl\u00f6tzlich packe!", "tokens": ["Sie", "nicht", ",", "vor\u00b7\u00fc\u00b7ber", "wan\u00b7delnd", ",", "pl\u00f6tz\u00b7lich", "pa\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "ADV", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "O da\u00df sich, die noch leben, hieran mahnten,", "tokens": ["O", "da\u00df", "sich", ",", "die", "noch", "le\u00b7ben", ",", "hie\u00b7ran", "mahn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOUS", "PRF", "$,", "PRELS", "ADV", "VVINF", "$,", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und so, durch eig'ne Kraft heraus sich sch\u00e4lend,", "tokens": ["Und", "so", ",", "durch", "eig'\u00b7ne", "Kraft", "he\u00b7raus", "sich", "sch\u00e4\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN", "APZR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Den Weg zur Welt- und Selbst-Erl\u00f6sung bahnten!", "tokens": ["Den", "Weg", "zur", "Welt", "und", "Selbst\u00b7Er\u00b7l\u00f6\u00b7sung", "bahn\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Denn, auf den Letzten, wie den Ersten, z\u00e4hlend,", "tokens": ["Denn", ",", "auf", "den", "Letz\u00b7ten", ",", "wie", "den", "Ers\u00b7ten", ",", "z\u00e4h\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Kann Gott das Liebeswerk erst dann vollbringen,", "tokens": ["Kann", "Gott", "das", "Lie\u00b7bes\u00b7werk", "erst", "dann", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wenn dieser auch, sich m\u00fchsam aufw\u00e4rts qu\u00e4lend,", "tokens": ["Wenn", "die\u00b7ser", "auch", ",", "sich", "m\u00fch\u00b7sam", "auf\u00b7w\u00e4rts", "qu\u00e4\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "$,", "PRF", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Gekr\u00e4ftigt ist, mit uns empor zu dringen.", "tokens": ["Ge\u00b7kr\u00e4f\u00b7tigt", "ist", ",", "mit", "uns", "em\u00b7por", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So lange aber m\u00fcssen wir's entbehren,", "tokens": ["So", "lan\u00b7ge", "a\u00b7ber", "m\u00fcs\u00b7sen", "wir's", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und ob Aeonen noch darob vergingen.", "tokens": ["Und", "ob", "A\u00b7e\u00b7o\u00b7nen", "noch", "da\u00b7rob", "ver\u00b7gin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "Auch wird uns erst der Uebergang erkl\u00e4ren,", "tokens": ["Auch", "wird", "uns", "erst", "der", "Ue\u00b7ber\u00b7gang", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wozu im Ewig-Einen dies Zersplittern;", "tokens": ["Wo\u00b7zu", "im", "E\u00b7wig\u00b7Ei\u00b7nen", "dies", "Zer\u00b7split\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ob einzig, um das B\u00f6se zu verzehren,", "tokens": ["Ob", "ein\u00b7zig", ",", "um", "das", "B\u00f6\u00b7se", "zu", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Das, wenn es sich in tausend Ungewittern", "tokens": ["Das", ",", "wenn", "es", "sich", "in", "tau\u00b7send", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "PPER", "PRF", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Entlud, vor seiner eig'nen Ohnmacht endlich", "tokens": ["Ent\u00b7lud", ",", "vor", "sei\u00b7ner", "eig'\u00b7nen", "Ohn\u00b7macht", "end\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Erschrecken wird und still in sich zerzittern;", "tokens": ["Er\u00b7schre\u00b7cken", "wird", "und", "still", "in", "sich", "zer\u00b7zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Ob mit, weil Gott, sich selber unverst\u00e4ndlich,", "tokens": ["Ob", "mit", ",", "weil", "Gott", ",", "sich", "sel\u00b7ber", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "$,", "KOUS", "NN", "$,", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wie unser Geist in Worte, in Figuren", "tokens": ["Wie", "un\u00b7ser", "Geist", "in", "Wor\u00b7te", ",", "in", "Fi\u00b7gu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Zerflie\u00dfen mu\u00dfte, um sich dadurch kenntlich", "tokens": ["Zer\u00b7flie\u00b7\u00dfen", "mu\u00df\u00b7te", ",", "um", "sich", "da\u00b7durch", "kennt\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "KOUI", "PRF", "PAV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Zu werden, und aus allen Signaturen", "tokens": ["Zu", "wer\u00b7den", ",", "und", "aus", "al\u00b7len", "Sig\u00b7na\u00b7tu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VAINF", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Die eigene zusammen sich zu stellen,", "tokens": ["Die", "ei\u00b7ge\u00b7ne", "zu\u00b7sam\u00b7men", "sich", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "So da\u00df die Welt, trotz ihrer finstern Spuren,", "tokens": ["So", "da\u00df", "die", "Welt", ",", "trotz", "ih\u00b7rer", "fins\u00b7tern", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Ihm Fackel war, sein Inn'res aufzuhellen,", "tokens": ["Ihm", "Fa\u00b7ckel", "war", ",", "sein", "Inn'\u00b7res", "auf\u00b7zu\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "$,", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Und da\u00df nicht uns're Schuld, nur sein Bed\u00fcrfen", "tokens": ["Und", "da\u00df", "nicht", "un\u00b7s'\u00b7re", "Schuld", ",", "nur", "sein", "Be\u00b7d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Den Gegensatz, dem Trotz und Ha\u00df entquellen,", "tokens": ["Den", "Ge\u00b7gen\u00b7satz", ",", "dem", "Trotz", "und", "Ha\u00df", "ent\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Hervor rief, der nach mystischen Entw\u00fcrfen", "tokens": ["Her\u00b7vor", "rief", ",", "der", "nach", "mys\u00b7ti\u00b7schen", "Ent\u00b7w\u00fcr\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.47": {"text": "Uns, die wir leiden, qu\u00e4lt, als ob wir th\u00e4ten,", "tokens": ["Uns", ",", "die", "wir", "lei\u00b7den", ",", "qu\u00e4lt", ",", "als", "ob", "wir", "th\u00e4\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Um so, indem wir all sein Bitt'res schl\u00fcrfen,", "tokens": ["Um", "so", ",", "in\u00b7dem", "wir", "all", "sein", "Bitt'\u00b7res", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "$,", "KOUS", "PPER", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "In uns ihn, bis zur Wurzel auszuj\u00e4ten", "tokens": ["In", "uns", "ihn", ",", "bis", "zur", "Wur\u00b7zel", "aus\u00b7zu\u00b7j\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPER", "$,", "KOUS", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Und das Geheimni\u00df erst zu offenbaren,", "tokens": ["Und", "das", "Ge\u00b7heim\u00b7ni\u00df", "erst", "zu", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Wenn wir zur\u00fcck in ihn, den Urgrund, treten", "tokens": ["Wenn", "wir", "zu\u00b7r\u00fcck", "in", "ihn", ",", "den", "Ur\u00b7grund", ",", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PTKVZ", "APPR", "PPER", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Und wieder werden, was wir einst schon waren,", "tokens": ["Und", "wie\u00b7der", "wer\u00b7den", ",", "was", "wir", "einst", "schon", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAINF", "$,", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Den Tropfen gleich, die, in sich abgeschlossen,", "tokens": ["Den", "Trop\u00b7fen", "gleich", ",", "die", ",", "in", "sich", "ab\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "$,", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Doch in der Welle rollen, in der klaren,", "tokens": ["Doch", "in", "der", "Wel\u00b7le", "rol\u00b7len", ",", "in", "der", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "So rund f\u00fcr sich, als ganz mit ihr verflossen.", "tokens": ["So", "rund", "f\u00fcr", "sich", ",", "als", "ganz", "mit", "ihr", "ver\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PRF", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}