{"textgrid.poem.33984": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das abgeschiedene Kind an seine Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, meine Mutter, schwer war unser Scheiden,", "tokens": ["O", ",", "mei\u00b7ne", "Mut\u00b7ter", ",", "schwer", "war", "un\u00b7ser", "Schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum mu\u00df ich mich noch einmal zu dir wenden,", "tokens": ["Drum", "mu\u00df", "ich", "mich", "noch", "ein\u00b7mal", "zu", "dir", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich zu beschwichtigen in deinem Leiden!", "tokens": ["Dich", "zu", "be\u00b7schwich\u00b7ti\u00b7gen", "in", "dei\u00b7nem", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ob mich auch die tausend Sonnen blenden,", "tokens": ["Und", "ob", "mich", "auch", "die", "tau\u00b7send", "Son\u00b7nen", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die still und gro\u00df an mir vor\u00fcber wallen,", "tokens": ["Die", "still", "und", "gro\u00df", "an", "mir", "vor\u00b7\u00fc\u00b7ber", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch find' ich sie, der sie die Stralen senden, \u2013", "tokens": ["Doch", "find'", "ich", "sie", ",", "der", "sie", "die", "Stra\u00b7len", "sen\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Erde noch heraus, die d\u00e4mmernd-kleine,", "tokens": ["Die", "Er\u00b7de", "noch", "he\u00b7raus", ",", "die", "d\u00e4m\u00b7mern\u00b7dklei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die, sonst verschwimmend in den blauen Hallen,", "tokens": ["Die", ",", "sonst", "ver\u00b7schwim\u00b7mend", "in", "den", "blau\u00b7en", "Hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Jetzt heller aufgl\u00e4nzt, wie im eig'nen Scheine,", "tokens": ["Jetzt", "hel\u00b7ler", "auf\u00b7gl\u00e4nzt", ",", "wie", "im", "eig'\u00b7nen", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Denn fr\u00f6hlich sind der Menschen Angesichter,", "tokens": ["Denn", "fr\u00f6h\u00b7lich", "sind", "der", "Men\u00b7schen", "An\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und keines ist verd\u00fcstert, als das deine!", "tokens": ["Und", "kei\u00b7nes", "ist", "ver\u00b7d\u00fcs\u00b7tert", ",", "als", "das", "dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,", "KOUS", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Kinder h\u00fcpfen um die Weihnachtslichter,", "tokens": ["Die", "Kin\u00b7der", "h\u00fcp\u00b7fen", "um", "die", "Weih\u00b7nachts\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die ihre M\u00fctter ihnen angez\u00fcndet,", "tokens": ["Die", "ih\u00b7re", "M\u00fct\u00b7ter", "ih\u00b7nen", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Du siehst es und verh\u00fcllst dich dicht und dichter.", "tokens": ["Du", "siehst", "es", "und", "ver\u00b7h\u00fcllst", "dich", "dicht", "und", "dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich aber will, geheimni\u00dfvoll verb\u00fcndet", "tokens": ["Ich", "a\u00b7ber", "will", ",", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mit meines Vaters Geist, nicht von dir lassen,", "tokens": ["Mit", "mei\u00b7nes", "Va\u00b7ters", "Geist", ",", "nicht", "von", "dir", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Bis ich das Wort der Worte dir verk\u00fcndet,", "tokens": ["Bis", "ich", "das", "Wort", "der", "Wor\u00b7te", "dir", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Das, kannst du's auch nicht ungestorben fassen,", "tokens": ["Das", ",", "kannst", "du's", "auch", "nicht", "un\u00b7ge\u00b7stor\u00b7ben", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VMFIN", "PIS", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Doch all dein Sinnen fesselt und dein Denken,", "tokens": ["Doch", "all", "dein", "Sin\u00b7nen", "fes\u00b7selt", "und", "dein", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Bis es sich ganz dir aufschlie\u00dft im Erblassen.", "tokens": ["Bis", "es", "sich", "ganz", "dir", "auf\u00b7schlie\u00dft", "im", "Er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.21": {"text": "Ich will in meinen Vater mich versenken,", "tokens": ["Ich", "will", "in", "mei\u00b7nen", "Va\u00b7ter", "mich", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich will mein tiefstes Ahnen ihm entdecken,", "tokens": ["Ich", "will", "mein", "tiefs\u00b7tes", "Ah\u00b7nen", "ihm", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ich will ihm Bilder und Gedanken schenken,", "tokens": ["Ich", "will", "ihm", "Bil\u00b7der", "und", "Ge\u00b7dan\u00b7ken", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die selbst vor einem Dichter sich verstecken.", "tokens": ["Die", "selbst", "vor", "ei\u00b7nem", "Dich\u00b7ter", "sich", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und fa\u00dft er sie so wenig, wie die Harfe", "tokens": ["Und", "fa\u00dft", "er", "sie", "so", "we\u00b7nig", ",", "wie", "die", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "PIS", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Den Ton, den Abendlispel in ihr wecken,", "tokens": ["Den", "Ton", ",", "den", "A\u00b7bend\u00b7lis\u00b7pel", "in", "ihr", "we\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "So wird er doch nach innerstem Bedarfe", "tokens": ["So", "wird", "er", "doch", "nach", "in\u00b7ners\u00b7tem", "Be\u00b7dar\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+----+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Sie fromm in deine Brust hin\u00fcber leiten,", "tokens": ["Sie", "fromm", "in", "dei\u00b7ne", "Brust", "hin\u00b7\u00fc\u00b7ber", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dann l\u00f6s't in ihr der Mi\u00dflaut sich, der scharfe,", "tokens": ["Dann", "l\u00f6s't", "in", "ihr", "der", "Mi\u00df\u00b7laut", "sich", ",", "der", "schar\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ART", "NN", "PRF", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Da ew'ge Harmonieen ihn bestreiten.", "tokens": ["Da", "ew'\u00b7ge", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", "ihn", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+----+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "O, had're nimmer mit den Urgewalten,", "tokens": ["O", ",", "ha\u00b7d'\u00b7re", "nim\u00b7mer", "mit", "den", "Ur\u00b7ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die, ruhig thronend \u00fcber alle Zeiten,", "tokens": ["Die", ",", "ru\u00b7hig", "thro\u00b7nend", "\u00fc\u00b7ber", "al\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In festen H\u00e4nden jeglich Schicksal halten!", "tokens": ["In", "fes\u00b7ten", "H\u00e4n\u00b7den", "jeg\u00b7lich", "Schick\u00b7sal", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens Sch\u00f6nheit wollt' ich dir erschlie\u00dfen,", "tokens": ["Des", "Le\u00b7bens", "Sch\u00f6n\u00b7heit", "wollt'", "ich", "dir", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Todes Schrecken mu\u00dft' ich dir entfalten,", "tokens": ["Des", "To\u00b7des", "Schre\u00b7cken", "mu\u00dft'", "ich", "dir", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ird'schen Wonnen brannt' ich, zu genie\u00dfen,", "tokens": ["Die", "ird'\u00b7schen", "Won\u00b7nen", "brannt'", "ich", ",", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch zu den h\u00f6hern ward ich abgerufen.", "tokens": ["Doch", "zu", "den", "h\u00f6\u00b7hern", "ward", "ich", "ab\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir war, als s\u00e4hst du mich in Nichts zerflie\u00dfen,", "tokens": ["Dir", "war", ",", "als", "s\u00e4hst", "du", "mich", "in", "Nichts", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PRF", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als mich's erhob zur letzten aller Stufen,", "tokens": ["Als", "mich's", "er\u00b7hob", "zur", "letz\u00b7ten", "al\u00b7ler", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ich selber str\u00e4ubte mich, obgleich mein Beben", "tokens": ["Ich", "sel\u00b7ber", "str\u00e4ub\u00b7te", "mich", ",", "ob\u00b7gleich", "mein", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und S\u00e4umen einzig so viel Qual mir schufen.", "tokens": ["Und", "S\u00e4u\u00b7men", "ein\u00b7zig", "so", "viel", "Qual", "mir", "schu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADV", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ich glich in meinem eitlen Widerstreben", "tokens": ["Ich", "glich", "in", "mei\u00b7nem", "eit\u00b7len", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dem Eingekerkerten, der das Gef\u00e4ngni\u00df,", "tokens": ["Dem", "Ein\u00b7ge\u00b7ker\u00b7ker\u00b7ten", ",", "der", "das", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Wenn es zusammenst\u00fcrzt in Windes Weben,", "tokens": ["Wenn", "es", "zu\u00b7sam\u00b7men\u00b7st\u00fcrzt", "in", "Win\u00b7des", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nicht lassen will in seines Herzens B\u00e4ngni\u00df,", "tokens": ["Nicht", "las\u00b7sen", "will", "in", "sei\u00b7nes", "Her\u00b7zens", "B\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Es f\u00e4llt kein Stein, der ihm nicht Wunden schl\u00fcge,", "tokens": ["Es", "f\u00e4llt", "kein", "Stein", ",", "der", "ihm", "nicht", "Wun\u00b7den", "schl\u00fc\u00b7ge", ","], "token_info