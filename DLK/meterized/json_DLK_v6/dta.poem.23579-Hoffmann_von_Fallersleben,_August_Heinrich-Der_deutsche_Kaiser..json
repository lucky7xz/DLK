{"dta.poem.23579": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der deutsche Kaiser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hin ist des deutschen Reichs uralte Herrlichkeit,", "tokens": ["Hin", "ist", "des", "deut\u00b7schen", "Reichs", "ur\u00b7al\u00b7te", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einer Sage ward's in dieser jungen Zeit,", "tokens": ["Zu", "ei\u00b7ner", "Sa\u00b7ge", "ward's", "in", "die\u00b7ser", "jun\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch h\u00e4lt das Volk noch fest an seinem alten Herrn,", "tokens": ["Doch", "h\u00e4lt", "das", "Volk", "noch", "fest", "an", "sei\u00b7nem", "al\u00b7ten", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu seinem Banner eilt's noch hin von nah und fern.", "tokens": ["Zu", "sei\u00b7nem", "Ban\u00b7ner", "eilt's", "noch", "hin", "von", "nah", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "APPR", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was lockt das Volk wohl hin? Nicht Kriegslust, Sold und Ruhm,", "tokens": ["Was", "lockt", "das", "Volk", "wohl", "hin", "?", "Nicht", "Kriegs\u00b7lust", ",", "Sold", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$.", "PTKNEG", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht mehr Begeisterung f\u00fcrs alte Kaiserthum.", "tokens": ["Nicht", "mehr", "Be\u00b7geis\u00b7te\u00b7rung", "f\u00fcrs", "al\u00b7te", "Kai\u00b7ser\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Volk sucht Obdach nur, es will nur Ruh' und Rast,", "tokens": ["Das", "Volk", "sucht", "Ob\u00b7dach", "nur", ",", "es", "will", "nur", "Ruh'", "und", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "$,", "PPER", "VMFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begehrt Erquickung nur f\u00fcr manche M\u00fch' und Last.", "tokens": ["Be\u00b7gehrt", "Er\u00b7quic\u00b7kung", "nur", "f\u00fcr", "man\u00b7che", "M\u00fch'", "und", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum deutschen Kaiser bin auch ich wohl eingekehrt,", "tokens": ["Zum", "deut\u00b7schen", "Kai\u00b7ser", "bin", "auch", "ich", "wohl", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Denn dieser Kaiser war ein deutsches Wirthshaus nur,", "tokens": ["Denn", "die\u00b7ser", "Kai\u00b7ser", "war", "ein", "deut\u00b7sches", "Wirths\u00b7haus", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom heil'gen r\u00f6m'schen Reich die allerletzte Spur.", "tokens": ["Vom", "heil'\u00b7gen", "r\u00f6m'\u00b7schen", "Reich", "die", "al\u00b7ler\u00b7letz\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}