{"textgrid.poem.25536": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von allem was Natur und Sch\u00f6nheit ist,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allem was Natur und Sch\u00f6nheit ist,", "tokens": ["Von", "al\u00b7lem", "was", "Na\u00b7tur", "und", "Sch\u00f6n\u00b7heit", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rei\u00dft nichts so sehr mich zum Entz\u00fccken,", "tokens": ["Rei\u00dft", "nichts", "so", "sehr", "mich", "zum", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Thr\u00e4nen hin, Ruhm! Wollust! euch geweint!", "tokens": ["Zu", "Thr\u00e4\u00b7nen", "hin", ",", "Ruhm", "!", "Wol\u00b7lust", "!", "euch", "ge\u00b7weint", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "NN", "$.", "NN", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als wenn dein Freund im Fingal liest.", "tokens": ["Als", "wenn", "dein", "Freund", "im", "Fin\u00b7gal", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch heute trug die\u00df Buch in seinem R\u00fccken,", "tokens": ["Doch", "heu\u00b7te", "trug", "die\u00df", "Buch", "in", "sei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Lied, \u2013 sey noch einmal gek\u00fc\u00dft! \u2013", "tokens": ["Ein", "Lied", ",", "\u2013", "sey", "noch", "ein\u00b7mal", "ge\u00b7k\u00fc\u00dft", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "$(", "VAFIN", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Lied, wogegen die\u00df Entz\u00fccken,", "tokens": ["Ein", "Lied", ",", "wo\u00b7ge\u00b7gen", "die\u00df", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Ruhm des Barden \u2013 nichts mir ist!", "tokens": ["Und", "Ruhm", "des", "Bar\u00b7den", "\u2013", "nichts", "mir", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$(", "PIS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}