{"textgrid.poem.37647": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel schw\u00e4cher ich mich f\u00fchle,", "tokens": ["Viel", "schw\u00e4\u00b7cher", "ich", "mich", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da mir so nah die Freud,", "tokens": ["Da", "mir", "so", "nah", "die", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als da ich fern dem Ziele", "tokens": ["Als", "da", "ich", "fern", "dem", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Leid und Bitterkeit;", "tokens": ["In", "Leid", "und", "Bit\u00b7ter\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nacht der N\u00e4chte, s\u00fc\u00df und bittre Zeiten,", "tokens": ["Nacht", "der", "N\u00e4ch\u00b7te", ",", "s\u00fc\u00df", "und", "bitt\u00b7re", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bald wird seinen Arm der Liebste um mich breiten,", "tokens": ["Bald", "wird", "sei\u00b7nen", "Arm", "der", "Liebs\u00b7te", "um", "mich", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "ADJA", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Jungfrau vergehet,", "tokens": ["Die", "Jung\u00b7frau", "ver\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Frau dann erstehet,", "tokens": ["Die", "Frau", "dann", "er\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der Name des Herrn sei gelobt!", "tokens": ["Der", "Na\u00b7me", "des", "Herrn", "sei", "ge\u00b7lobt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Myrthenkranz so lose", "tokens": ["Der", "Myr\u00b7then\u00b7kranz", "so", "lo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir schon im Haare spielt,", "tokens": ["Mir", "schon", "im", "Haa\u00b7re", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Liebesbecher, Rose,", "tokens": ["O", "Lie\u00b7bes\u00b7be\u00b7cher", ",", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie mich dein Duft hier k\u00fchlt;", "tokens": ["Wie", "mich", "dein", "Duft", "hier", "k\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lieb' ist st\u00e4rker als der Tod erfunden,", "tokens": ["Lieb'", "ist", "st\u00e4r\u00b7ker", "als", "der", "Tod", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie ein Lamm zum Opfer bin ich bunden.", "tokens": ["Wie", "ein", "Lamm", "zum", "Op\u00b7fer", "bin", "ich", "bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Mein Hemdlein spielt im Winde,", "tokens": ["Mein", "Hemd\u00b7lein", "spielt", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er ruft mir: Kind, geschwinde,", "tokens": ["Er", "ruft", "mir", ":", "Kind", ",", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Name des Herrn sei gelobt.", "tokens": ["Der", "Na\u00b7me", "des", "Herrn", "sei", "ge\u00b7lobt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel schw\u00e4cher ich mich f\u00fchle,", "tokens": ["Viel", "schw\u00e4\u00b7cher", "ich", "mich", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da mir so nah die Lust,", "tokens": ["Da", "mir", "so", "nah", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als da ich fern dem Ziele", "tokens": ["Als", "da", "ich", "fern", "dem", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An's Sterben denken mu\u00dft:", "tokens": ["An's", "Ster\u00b7ben", "den\u00b7ken", "mu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nackt bin ich in diese Welt gekommen,", "tokens": ["Nackt", "bin", "ich", "in", "die\u00b7se", "Welt", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nackt werd' ich auch wieder aufgenommen.", "tokens": ["Nackt", "werd'", "ich", "auch", "wie\u00b7der", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Der Herr hat's gegeben,", "tokens": ["Der", "Herr", "hat's", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Herr hat's genommen,", "tokens": ["Der", "Herr", "hat's", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der Name des Herrn sei gelobt. Amen.", "tokens": ["Der", "Na\u00b7me", "des", "Herrn", "sei", "ge\u00b7lobt", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$.", "NN", "$."], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}}}}}