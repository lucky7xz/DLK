{"textgrid.poem.66521": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Uns ward aus alten Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns ward aus alten Tagen", "tokens": ["Uns", "ward", "aus", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Spr\u00fcchlein zugetragen:", "tokens": ["Ein", "Spr\u00fcch\u00b7lein", "zu\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer will nach Reben fragen,", "tokens": ["Wer", "will", "nach", "Re\u00b7ben", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der halte sich zu Drei'n.", "tokens": ["Der", "hal\u00b7te", "sich", "zu", "Drei'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbzu Hochheim an dem Maine,", "tokens": ["\u00bb", "zu", "Hoch\u00b7heim", "an", "dem", "Mai\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zu W\u00fcrzburg auf dem Steine,", "tokens": ["Zu", "W\u00fcrz\u00b7burg", "auf", "dem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu Bacharach am Rheine,", "tokens": ["Zu", "Bac\u00b7ha\u00b7rach", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da w\u00e4chst der beste Wein.\u00ab", "tokens": ["Da", "w\u00e4chst", "der", "bes\u00b7te", "Wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Zeiten sind vergangen,", "tokens": ["Die", "Zei\u00b7ten", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und andre Lieder klangen,", "tokens": ["Und", "and\u00b7re", "Lie\u00b7der", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt hat mehr empfangen,", "tokens": ["Die", "Welt", "hat", "mehr", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Spr\u00fcchlein gilt nicht mehr.", "tokens": ["Das", "Spr\u00fcch\u00b7lein", "gilt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von allen Bergen sprangen", "tokens": ["Von", "al\u00b7len", "Ber\u00b7gen", "spran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Rhein mit frischen Wangen", "tokens": ["Am", "Rhein", "mit", "fri\u00b7schen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Streiter voll Verlangen,", "tokens": ["Die", "Strei\u00b7ter", "voll", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein goldger\u00fcstet Heer.", "tokens": ["Ein", "gold\u00b7ge\u00b7r\u00fcs\u00b7tet", "Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie stehn auf allen Warten,", "tokens": ["Sie", "stehn", "auf", "al\u00b7len", "War\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In jedem Rebengarten,", "tokens": ["In", "je\u00b7dem", "Re\u00b7beng\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei flatternden Standarten,", "tokens": ["Bei", "flat\u00b7tern\u00b7den", "Stand\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In stolzer Herrlichkeit.", "tokens": ["In", "stol\u00b7zer", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und hast im Spiel der Thoren", "tokens": ["Und", "hast", "im", "Spiel", "der", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Hader sie beschworen,", "tokens": ["Zum", "Ha\u00b7der", "sie", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gieb deinen Kopf verloren,", "tokens": ["Gieb", "dei\u00b7nen", "Kopf", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verloren in dem Streit!", "tokens": ["Ver\u00b7lo\u00b7ren", "in", "dem", "Streit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wenn dir im Gem\u00fcthe", "tokens": ["Doch", "wenn", "dir", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gerechte Tugend bl\u00fchte,", "tokens": ["Ge\u00b7rech\u00b7te", "Tu\u00b7gend", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du findest sie voll G\u00fcte,", "tokens": ["Du", "fin\u00b7dest", "sie", "voll", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bist ein lieber Gast!", "tokens": ["Du", "bist", "ein", "lie\u00b7ber", "Gast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Poch an in jedem Orte,", "tokens": ["Poch", "an", "in", "je\u00b7dem", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei jeder Rebenpforte,", "tokens": ["Bei", "je\u00b7der", "Re\u00b7ben\u00b7pfor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du bist auch ohne Worte", "tokens": ["Du", "bist", "auch", "oh\u00b7ne", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Liebesarm umfa\u00dft!", "tokens": ["Von", "Lie\u00b7be\u00b7sarm", "um\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit Himmelsblick der Eine,", "tokens": ["Mit", "Him\u00b7mels\u00b7blick", "der", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zweit' im Purpurscheine,", "tokens": ["Der", "Zweit'", "im", "Pur\u00b7pur\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und noch der Hundertst-Eine", "tokens": ["Und", "noch", "der", "Hun\u00b7dert\u00b7stEi\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleichwie der Erst' ein Held!", "tokens": ["Gleich\u00b7wie", "der", "Er\u00b7st'", "ein", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr Helden all vom Rheine,", "tokens": ["Ihr", "Hel\u00b7den", "all", "vom", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr k\u00f6niglichen Weine,", "tokens": ["Ihr", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Ihr nehmt es auf alleine", "tokens": ["Ihr", "nehmt", "es", "auf", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wohl mit der ganzen Welt!", "tokens": ["Wohl", "mit", "der", "gan\u00b7zen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}