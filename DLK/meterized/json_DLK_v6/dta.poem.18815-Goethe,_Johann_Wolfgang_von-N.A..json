{"dta.poem.18815": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, in der Schenke hab\u2019 ich auch gesessen,", "tokens": ["Ja", ",", "in", "der", "Schen\u00b7ke", "hab'", "ich", "auch", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ward wie andern zugemessen,", "tokens": ["Mir", "ward", "wie", "an\u00b7dern", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schwatzten, schrieen, h\u00e4ndelten von heut,", "tokens": ["Sie", "schwatz\u00b7ten", ",", "schri\u00b7een", ",", "h\u00e4n\u00b7del\u00b7ten", "von", "heut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So froh und traurig wie\u2019s der Tag gebeut;", "tokens": ["So", "froh", "und", "trau\u00b7rig", "wie's", "der", "Tag", "ge\u00b7beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich aber sass, im Innersten erfreut,", "tokens": ["Ich", "a\u00b7ber", "sass", ",", "im", "In\u00b7ners\u00b7ten", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An meine Liebste dacht\u2019 ich \u2014 wie sie liebt?", "tokens": ["An", "mei\u00b7ne", "Liebs\u00b7te", "dacht'", "ich", "wie", "sie", "liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das weiss ich nicht; was aber mich bedr\u00e4ngt!", "tokens": ["Das", "weiss", "ich", "nicht", ";", "was", "a\u00b7ber", "mich", "be\u00b7dr\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$.", "PWS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich liebe sie wie es ein Busen giebt", "tokens": ["Ich", "lie\u00b7be", "sie", "wie", "es", "ein", "Bu\u00b7sen", "giebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.9": {"text": "Der treu sich Einer gab und knechtisch h\u00e4ngt.", "tokens": ["Der", "treu", "sich", "Ei\u00b7ner", "gab", "und", "knech\u00b7tisch", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "PIS", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wo war das Pergament, der Griffel wo?", "tokens": ["Wo", "war", "das", "Per\u00b7ga\u00b7ment", ",", "der", "Grif\u00b7fel", "wo", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die alles fassten! \u2014 doch so wars! ja so!", "tokens": ["Die", "al\u00b7les", "fass\u00b7ten", "!", "doch", "so", "wars", "!", "ja", "so", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$.", "$(", "ADV", "ADV", "VAFIN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}