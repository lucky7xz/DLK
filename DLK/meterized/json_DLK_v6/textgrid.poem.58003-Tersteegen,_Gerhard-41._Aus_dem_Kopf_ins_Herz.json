{"textgrid.poem.58003": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "41. Aus dem Kopf ins Herz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein Herzens-Gott; drum, wenn du ihn willst finden,", "tokens": ["Gott", "ist", "ein", "Her\u00b7zens\u00b7Gott", ";", "drum", ",", "wenn", "du", "ihn", "willst", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "PAV", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So blende die Vernunft, sie wird ihn nie ergr\u00fcnden!", "tokens": ["So", "blen\u00b7de", "die", "Ver\u00b7nunft", ",", "sie", "wird", "ihn", "nie", "er\u00b7gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Senk dich aus deinem Kopf in 's Herzens Grund hinein", "tokens": ["Senk", "dich", "aus", "dei\u00b7nem", "Kopf", "in", "'s", "Her\u00b7zens", "Grund", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPR", "PPER", "NN", "NN", "APZR"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sanft, liebreich, wie ein Kind, so wird er dir gemein!", "tokens": ["Sanft", ",", "lieb\u00b7reich", ",", "wie", "ein", "Kind", ",", "so", "wird", "er", "dir", "ge\u00b7mein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}