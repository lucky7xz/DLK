{"textgrid.poem.65277": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich erst kaufen", "tokens": ["Was", "soll", "ich", "erst", "kau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eine Feder und Tint'?", "tokens": ["Ei\u00b7ne", "Fe\u00b7der", "und", "Tint'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Buchstabiren und Schreiben", "tokens": ["Buch\u00b7sta\u00b7bi\u00b7ren", "und", "Schrei\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Geht auch nicht geschwind.", "tokens": ["Geht", "auch", "nicht", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Will selber hinlaufen", "tokens": ["Will", "sel\u00b7ber", "hin\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zu der Nannerl in's Haus,", "tokens": ["Zu", "der", "Nan\u00b7nerl", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--++-+", "measure": "anapaest.init"}, "line.7": {"text": "Will's m\u00fcndlich ihr sagen:", "tokens": ["Will's", "m\u00fcnd\u00b7lich", "ihr", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Unsre Liebschaft ist aus!", "tokens": ["Uns\u00b7re", "Lieb\u00b7schaft", "ist", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Unsre Liebschaft ist zerrissen,", "tokens": ["Uns\u00b7re", "Lieb\u00b7schaft", "ist", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird nimmermehr ganz;", "tokens": ["Wird", "nim\u00b7mer\u00b7mehr", "ganz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und morgen da f\u00fchr' ich", "tokens": ["Und", "mor\u00b7gen", "da", "f\u00fchr'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ein' Andre zum Tanz.", "tokens": ["Ein'", "And\u00b7re", "zum", "Tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es springen viel Dirnen", "tokens": ["Es", "sprin\u00b7gen", "viel", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und singen dazu,", "tokens": ["Und", "sin\u00b7gen", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ach Nannerl, ach Nannerl,", "tokens": ["Ach", "Nan\u00b7nerl", ",", "ach", "Nan\u00b7nerl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "XY", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Doch Keine wie du!", "tokens": ["Doch", "Kei\u00b7ne", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "KOKOM", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Unsre Liebschaft ist zerrissen,", "tokens": ["Uns\u00b7re", "Lieb\u00b7schaft", "ist", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Liebschaft ist aus!", "tokens": ["Uns\u00b7re", "Lieb\u00b7schaft", "ist", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich klopfe nicht wieder", "tokens": ["Ich", "klop\u00b7fe", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "An der Nannerl ihr Haus.", "tokens": ["An", "der", "Nan\u00b7nerl", "ihr", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der H\u00e4user giebt's viele", "tokens": ["Der", "H\u00e4u\u00b7ser", "giebt's", "vie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit Fenstern darein;", "tokens": ["Mit", "Fens\u00b7tern", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch's klinget kein Fenster", "tokens": ["Doch's", "klin\u00b7get", "kein", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wie deines so fein!", "tokens": ["Wie", "dei\u00b7nes", "so", "fein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Unsre Liebschaft ist zerrissen \u2013", "tokens": ["Uns\u00b7re", "Lieb\u00b7schaft", "ist", "zer\u00b7ris\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leb' wohl denn, mein Kind!", "tokens": ["Leb'", "wohl", "denn", ",", "mein", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was ist's, da\u00df so bei\u00dfend", "tokens": ["Was", "ist's", ",", "da\u00df", "so", "bei\u00b7\u00dfend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Aus den Augen mir rinnt?", "tokens": ["Aus", "den", "Au\u00b7gen", "mir", "rinnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Es weinen viel Bursche", "tokens": ["Es", "wei\u00b7nen", "viel", "Bur\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und jammern dabei \u2013", "tokens": ["Und", "jam\u00b7mern", "da\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch, Nannerl, 's k\u00f6mmt Keinem", "tokens": ["Doch", ",", "Nan\u00b7nerl", ",", "'s", "k\u00f6mmt", "Kei\u00b7nem"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NE", "$,", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom Herzen so treu!", "tokens": ["Vom", "Her\u00b7zen", "so", "treu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Unsre Liebschaft ist zerrissen,", "tokens": ["Uns\u00b7re", "Lieb\u00b7schaft", "ist", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Herze dazu \u2013", "tokens": ["Mein", "Her\u00b7ze", "da\u00b7zu", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PAV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ach Nannerl, mein Nannerl,", "tokens": ["Ach", "Nan\u00b7nerl", ",", "mein", "Nan\u00b7nerl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was meinest denn du?", "tokens": ["Was", "mei\u00b7nest", "denn", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und m\u00fcssen wir scheiden", "tokens": ["Und", "m\u00fcs\u00b7sen", "wir", "schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In jetziger Zeit,", "tokens": ["In", "jet\u00b7zi\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "F\u00fchr' Gott uns zusammen", "tokens": ["F\u00fchr'", "Gott", "uns", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "In die ewige Freud'!", "tokens": ["In", "die", "e\u00b7wi\u00b7ge", "Freud'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}