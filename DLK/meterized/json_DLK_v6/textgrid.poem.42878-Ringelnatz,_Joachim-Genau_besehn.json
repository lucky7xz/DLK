{"textgrid.poem.42878": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Genau besehn", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man das zierlichste N\u00e4schen", "tokens": ["Wenn", "man", "das", "zier\u00b7lichs\u00b7te", "N\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von seiner liebsten Braut", "tokens": ["Von", "sei\u00b7ner", "liebs\u00b7ten", "Braut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch ein Vergr\u00f6\u00dferungsgl\u00e4schen", "tokens": ["Durch", "ein", "Ver\u00b7gr\u00f6\u00b7\u00dfe\u00b7rungs\u00b7gl\u00e4\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "N\u00e4her beschaut,", "tokens": ["N\u00e4\u00b7her", "be\u00b7schaut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dann zeigen sich haarige Berge,", "tokens": ["Dann", "zei\u00b7gen", "sich", "haa\u00b7ri\u00b7ge", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Da\u00df einem graut.", "tokens": ["Da\u00df", "ei\u00b7nem", "graut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}