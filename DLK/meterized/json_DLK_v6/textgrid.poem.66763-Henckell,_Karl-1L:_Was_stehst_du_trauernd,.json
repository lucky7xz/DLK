{"textgrid.poem.66763": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was stehst du trauernd,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was stehst du trauernd,", "tokens": ["Was", "stehst", "du", "trau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ewiger Sehnsucht Freund,", "tokens": ["E\u00b7wi\u00b7ger", "Sehn\u00b7sucht", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Am Grab des Liebsten,", "tokens": ["Am", "Grab", "des", "Liebs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Welchen der Tod verschlang?", "tokens": ["Wel\u00b7chen", "der", "Tod", "ver\u00b7schlang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Was birgst dein Haupt du,", "tokens": ["Was", "birgst", "dein", "Haupt", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Schmerzbeschattet,", "tokens": ["Schmerz\u00b7be\u00b7schat\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und suchst des Menschen", "tokens": ["Und", "suchst", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "G\u00f6ttlich Antlitz,", "tokens": ["G\u00f6tt\u00b7lich", "Ant\u00b7litz", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ach, vergebens?", "tokens": ["Ach", ",", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der selbst sein Kreuz trug,", "tokens": ["Der", "selbst", "sein", "Kreuz", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dornengekr\u00f6nter Held,", "tokens": ["Dor\u00b7nen\u00b7ge\u00b7kr\u00f6n\u00b7ter", "Held", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Gepeitscht mit Ruten,", "tokens": ["Ge\u00b7peitscht", "mit", "Ru\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Weil in der Wahrheit Wehr", "tokens": ["Weil", "in", "der", "Wahr\u00b7heit", "Wehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er zeugen mu\u00dfte", "tokens": ["Er", "zeu\u00b7gen", "mu\u00df\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wider Weltwahn", "tokens": ["Wi\u00b7der", "Welt\u00b7wahn"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Vom innern Himmel-", "tokens": ["Vom", "in\u00b7nern", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "TRUNC"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Reich der Liebe,", "tokens": ["Reich", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "F\u00fcrst des Lebens.", "tokens": ["F\u00fcrst", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Der auch der Sch\u00f6nheit", "tokens": ["Der", "auch", "der", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Rose gesegnet \u2013 sieh!", "tokens": ["Ro\u00b7se", "ge\u00b7seg\u00b7net", "\u2013", "sieh", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "VVPP", "$(", "VVIMP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Schwester brachte", "tokens": ["Die", "Schwes\u00b7ter", "brach\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bl\u00fchenden Abschiedsgru\u00df", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Ab\u00b7schieds\u00b7gru\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dem sonnenmilden", "tokens": ["Dem", "son\u00b7nen\u00b7mil\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Herzerl\u00f6ser.", "tokens": ["Her\u00b7zer\u00b7l\u00f6\u00b7ser", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Betaut von Tr\u00e4nen", "tokens": ["Be\u00b7taut", "von", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Irrt Maria", "tokens": ["Irrt", "Ma\u00b7ria"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Bleich im Garten ...", "tokens": ["Bleich", "im", "Gar\u00b7ten", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Auf Sch\u00f6pferschwingen", "tokens": ["Auf", "Sch\u00f6p\u00b7fer\u00b7schwin\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Freudegefilden zu,", "tokens": ["Freu\u00b7de\u00b7ge\u00b7fil\u00b7den", "zu", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du gramgebeugter", "tokens": ["Du", "gram\u00b7ge\u00b7beug\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freund des Erhabenen,", "tokens": ["Freund", "des", "Er\u00b7ha\u00b7be\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.5": {"text": "Schwebt der geschm\u00e4hte", "tokens": ["Schwebt", "der", "ge\u00b7schm\u00e4h\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Menschen-Meister", "tokens": ["Men\u00b7schen\u00b7Meis\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und thront zur Rechten", "tokens": ["Und", "thront", "zur", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Gottes, wo die", "tokens": ["Got\u00b7tes", ",", "wo", "die"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Strahlend-Unsterblichen warten.", "tokens": ["Strah\u00b7len\u00b7dUn\u00b7sterb\u00b7li\u00b7chen", "war\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}}}}