{"textgrid.poem.56675": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Gott", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Furcht geboren und vom Wunsch versch\u00f6nt,", "tokens": ["Aus", "Furcht", "ge\u00b7bo\u00b7ren", "und", "vom", "Wunsch", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein Bild unsrer Vollkommenheit zu malen,", "tokens": ["ein", "Bild", "uns\u00b7rer", "Voll\u00b7kom\u00b7men\u00b7heit", "zu", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wurdest du Jude und zum Kannibalen,", "tokens": ["wur\u00b7dest", "du", "Ju\u00b7de", "und", "zum", "Kan\u00b7ni\u00b7ba\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "der eifervoll dem Bruderfra\u00dfe fr\u00f6nt;", "tokens": ["der", "ei\u00b7fer\u00b7voll", "dem", "Bru\u00b7der\u00b7fra\u00b7\u00dfe", "fr\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "dann n\u00e4hrtest du dich, opferblutgew\u00f6hnt,", "tokens": ["dann", "n\u00e4hr\u00b7test", "du", "dich", ",", "op\u00b7fer\u00b7blut\u00b7ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von unsrer Selbstzerfleischung Folterqualen,", "tokens": ["von", "uns\u00b7rer", "Selbst\u00b7zer\u00b7flei\u00b7schung", "Fol\u00b7ter\u00b7qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bis deine Wut verd\u00e4mmerte zum fahlen", "tokens": ["bis", "dei\u00b7ne", "Wut", "ver\u00b7d\u00e4m\u00b7mer\u00b7te", "zum", "fah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gespenst, das hohl und wimmernd uns umst\u00f6hnt:", "tokens": ["Ge\u00b7spenst", ",", "das", "hohl", "und", "wim\u00b7mernd", "uns", "um\u00b7st\u00f6hnt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oh Ding an sich! Oh Wahrheit! Letzter Grund!", "tokens": ["Oh", "Ding", "an", "sich", "!", "Oh", "Wahr\u00b7heit", "!", "Letz\u00b7ter", "Grund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["XY", "NN", "APPR", "PRF", "$.", "XY", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun stirbst du \u2013 \u2013 dennoch fachte dieses Wort", "tokens": ["Nun", "stirbst", "du", "\u2013", "\u2013", "den\u00b7noch", "fach\u00b7te", "die\u00b7ses", "Wort"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "$(", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "all unsrer Sehnsucht Narrenschmerzen und", "tokens": ["all", "uns\u00b7rer", "Sehn\u00b7sucht", "Nar\u00b7ren\u00b7schmer\u00b7zen", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gel\u00fcste an und unsre Welt verdorrt", "tokens": ["Ge\u00b7l\u00fcs\u00b7te", "an", "und", "uns\u00b7re", "Welt", "ver\u00b7dorrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "noch in den D\u00fcnsten, die dein toter Mund", "tokens": ["noch", "in", "den", "D\u00fcns\u00b7ten", ",", "die", "dein", "to\u00b7ter", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "aushaucht, zu einem runden Narrenort.", "tokens": ["aus\u00b7haucht", ",", "zu", "ei\u00b7nem", "run\u00b7den", "Nar\u00b7ren\u00b7ort", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}