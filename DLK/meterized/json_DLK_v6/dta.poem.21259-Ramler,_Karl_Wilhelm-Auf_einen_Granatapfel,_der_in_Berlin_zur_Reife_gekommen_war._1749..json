{"dta.poem.21259": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Granatapfel,  \n der in Berlin zur Reife gekommen war.  \n 1749.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Find' ich dich hier in deiner gr&#252;nen\nKrone!", "tokens": ["Find'", "ich", "dich", "hier", "in", "dei\u00b7ner", "gr", "&#252;", "nen", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerspaltest du die purpurrothe Brust", "tokens": ["Zer\u00b7spal\u00b7test", "du", "die", "pur\u00b7pur\u00b7ro\u00b7the", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An dieser Sonn\u2019! o Liebling der Pomone,", "tokens": ["An", "die\u00b7ser", "Sonn'", "!", "o", "Lieb\u00b7ling", "der", "Po\u00b7mo\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Proserpinens Apfel, die mit Lust", "tokens": ["O", "Pro\u00b7ser\u00b7pi\u00b7nens", "Ap\u00b7fel", ",", "die", "mit", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Wollust deine goldnen K\u00f6rner", "tokens": ["Und", "Wol\u00b7lust", "dei\u00b7ne", "gold\u00b7nen", "K\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Reich des H\u00f6llengottes ass,", "tokens": ["Im", "Reich", "des", "H\u00f6l\u00b7len\u00b7got\u00b7tes", "ass", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und allen Nektar ferner", "tokens": ["Und", "al\u00b7len", "Nek\u00b7tar", "fer\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und den Olymp vergass.", "tokens": ["Und", "den", "O\u00b7lymp", "ver\u00b7gass", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Der Erdball \u00e4ndert sich! Das Meer", "tokens": ["Der", "Erd\u00b7ball", "\u00e4n\u00b7dert", "sich", "!", "Das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht dem Pfluge Raum; der Fels", "tokens": ["Und", "macht", "dem", "Pflu\u00b7ge", "Raum", ";", "der", "Fels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, o Berlin! dein d\u00fcrrer Boden bl\u00fchet:", "tokens": ["Und", ",", "o", "Ber\u00b7lin", "!", "dein", "d\u00fcr\u00b7rer", "Bo\u00b7den", "bl\u00fc\u00b7het", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NE", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pomona f\u00fcllt ihr Horn in dir allein;", "tokens": ["Po\u00b7mo\u00b7na", "f\u00fcllt", "ihr", "Horn", "in", "dir", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In dir kann Flora, nach Begehren,", "tokens": ["In", "dir", "kann", "Flo\u00b7ra", ",", "nach", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "NE", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich tausendfache Kr\u00e4nze drehn,", "tokens": ["Sich", "tau\u00b7send\u00b7fa\u00b7che", "Kr\u00e4n\u00b7ze", "drehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ganz verdeckt in Aehren", "tokens": ["Und", "ganz", "ver\u00b7deckt", "in", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die blonde Ceres gehn.", "tokens": ["Die", "blon\u00b7de", "Ce\u00b7res", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und fremde B\u00e4um\u2019, ihr junges Haupt", "tokens": ["Und", "frem\u00b7de", "B\u00e4um'", ",", "ihr", "jun\u00b7ges", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt dir Sylvan, und zieht ein La-", "tokens": ["Bringt", "dir", "Syl\u00b7van", ",", "und", "zieht", "ein", "La"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "$,", "KON", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von B\u00fcschen auf vor deinen offnen Thoren,", "tokens": ["Von", "B\u00fc\u00b7schen", "auf", "vor", "dei\u00b7nen", "off\u00b7nen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mir und diesen K\u00fcnsten offen sind,", "tokens": ["Die", "mir", "und", "die\u00b7sen", "K\u00fcns\u00b7ten", "of\u00b7fen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PDAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die jetzt auf Fl\u00fcgeln D\u00e4dals eilen,", "tokens": ["Die", "jetzt", "auf", "Fl\u00fc\u00b7geln", "D\u00e4\u00b7dals", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch \u00fcber Meer und \u00fcber Land,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Meer", "und", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bleymasse, Meissel, Feilen", "tokens": ["Bley\u00b7mas\u00b7se", ",", "Meis\u00b7sel", ",", "Fei\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ihrer harten Hand.", "tokens": ["In", "ih\u00b7rer", "har\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Urpl\u00f6tzlich sind der Felsen graue R\u00fccken", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "sind", "der", "Fel\u00b7sen", "grau\u00b7e", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Tempeln und Pal\u00e4sten ausgeh\u00f6hlt,", "tokens": ["Zu", "Tem\u00b7peln", "und", "Pa\u00b7l\u00e4s\u00b7ten", "aus\u00b7ge\u00b7h\u00f6hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die rund umher der Pyrrha Kinder", "tokens": ["Die", "rund", "um\u00b7her", "der", "Pyrr\u00b7ha", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PTKVZ", "ART", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch halb den Steinen gleich, und halb", "tokens": ["Noch", "halb", "den", "Stei\u00b7nen", "gleich", ",", "und", "halb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr G\u00f6tter! pr\u00e4chtig aus Ruinen", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "pr\u00e4ch\u00b7tig", "aus", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erhebt sich euer Pantheon:", "tokens": ["Er\u00b7hebt", "sich", "eu\u00b7er", "Pan\u00b7the\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Weisen alle dienen,", "tokens": ["Die", "Wei\u00b7sen", "al\u00b7le", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die V\u00f6lker lernen schon.", "tokens": ["Die", "V\u00f6l\u00b7ker", "ler\u00b7nen", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sagt, Sterbliche, den Sph\u00e4ren ihre", "tokens": ["Sagt", ",", "Sterb\u00b7li\u00b7che", ",", "den", "Sph\u00e4\u00b7ren", "ih\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lehrt dem wilden Winde seinen Lauf,", "tokens": ["Und", "lehrt", "dem", "wil\u00b7den", "Win\u00b7de", "sei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4gt den Mond, und spaltet Sonnen-", "tokens": ["Und", "w\u00e4gt", "den", "Mond", ",", "und", "spal\u00b7tet", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deckt die Geburt des alten Goldes auf,", "tokens": ["Deckt", "die", "Ge\u00b7burt", "des", "al\u00b7ten", "Gol\u00b7des", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und steiget an der Wesen Kette", "tokens": ["Und", "stei\u00b7get", "an", "der", "We\u00b7sen", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis dahin, wo den h\u00f6chsten Ring", "tokens": ["Bis", "da\u00b7hin", ",", "wo", "den", "h\u00f6chs\u00b7ten", "Ring"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Zevs an sein Ruhebette", "tokens": ["Zevs", "an", "sein", "Ru\u00b7he\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu seinen F\u00fcssen hieng.", "tokens": ["Zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "hieng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl dir, o du, durch meinen Freund", "tokens": ["Wohl", "dir", ",", "o", "du", ",", "durch", "mei\u00b7nen", "Freund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "FM", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Athen an Geist, voll Muth, wie Sparta", "tokens": ["A\u00b7then", "an", "Geist", ",", "voll", "Muth", ",", "wie", "Spar\u00b7ta"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "NN", "$,", "ADJD", "NN", "$,", "PWAV", "NE"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es zog, von Kastors Liede gern verf\u00fchret,", "tokens": ["Es", "zog", ",", "von", "Kas\u00b7tors", "Lie\u00b7de", "gern", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NE", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Kampf hinaus mit aufgebundnem", "tokens": ["Zum", "Kampf", "hin\u00b7aus", "mit", "auf\u00b7ge\u00b7bund\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APZR", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Feinde, die den Kampf verloren,", "tokens": ["Die", "Fein\u00b7de", ",", "die", "den", "Kampf", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erwiederten, (nicht ohne Neid!)", "tokens": ["Er\u00b7wie\u00b7der\u00b7ten", ",", "(", "nicht", "oh\u00b7ne", "Neid", "!", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "$(", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Die Stadt sey nur geboren", "tokens": ["Die", "Stadt", "sey", "nur", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Waffen und zum Streit. \u2014", "tokens": ["Zu", "Waf\u00b7fen", "und", "zum", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sang Kalliope, die, voll Entz\u00fccken,", "tokens": ["So", "sang", "Kal\u00b7li\u00b7o\u00b7pe", ",", "die", ",", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "PRELS", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihrer kriegerischen Tuba kam,", "tokens": ["Mit", "ih\u00b7rer", "krie\u00b7ge\u00b7ri\u00b7schen", "Tu\u00b7ba", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, nicht gesehn von ungeweihten", "tokens": ["Und", ",", "nicht", "ge\u00b7sehn", "von", "un\u00b7ge\u00b7weih\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PTKNEG", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg zum Tempel des Apollo nahm,", "tokens": ["Den", "Weg", "zum", "Tem\u00b7pel", "des", "A\u00b7pol\u00b7lo", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.5": {"text": "Wo schon mit Lauten und mit Fl\u00f6ten,", "tokens": ["Wo", "schon", "mit", "Lau\u00b7ten", "und", "mit", "Fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verlarvt und im Zypressenkranz", "tokens": ["Ver\u00b7larvt", "und", "im", "Zyp\u00b7res\u00b7sen\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Sich ihre Schwestern drehten", "tokens": ["Sich", "ih\u00b7re", "Schwes\u00b7tern", "dreh\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im sch\u00f6nsten Reihentanz.", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Rei\u00b7hen\u00b7tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}