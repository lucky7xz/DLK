{"textgrid.poem.49162": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer bin ich, liebster Gott, vor dir,", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bin ich, liebster Gott, vor dir,", "tokens": ["Wer", "bin", "ich", ",", "liebs\u00b7ter", "Gott", ",", "vor", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du mich hast gebracht bis hier?", "tokens": ["Da\u00df", "du", "mich", "hast", "ge\u00b7bracht", "bis", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, es versinket mein Gem\u00fcthe", "tokens": ["Ach", ",", "es", "ver\u00b7sin\u00b7ket", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Meere deiner Lieb' und G\u00fcte,", "tokens": ["Im", "Mee\u00b7re", "dei\u00b7ner", "Lieb'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wei\u00df vor grossem Unwerth nicht,", "tokens": ["Und", "wei\u00df", "vor", "gros\u00b7sem", "Un\u00b7werth", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Woher mir Alles dies geschicht.", "tokens": ["Wo\u00b7her", "mir", "Al\u00b7les", "dies", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "PDS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist es schon gewohnt mit mir,", "tokens": ["Du", "bist", "es", "schon", "ge\u00b7wohnt", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich mich gern nachdenk' in dir;", "tokens": ["Da\u00df", "ich", "mich", "gern", "nach\u00b7denk'", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich halt' ohn' dich mir Nichts geschehen,", "tokens": ["Ich", "halt'", "ohn'", "dich", "mir", "Nichts", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch wen'ger mag ich was angehen,", "tokens": ["Noch", "wen'\u00b7ger", "mag", "ich", "was", "an\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es sei denn, da\u00df mein Herze Rath", "tokens": ["Es", "sei", "denn", ",", "da\u00df", "mein", "Her\u00b7ze", "Rath"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vorher mit dir gehalten hat.", "tokens": ["Vor\u00b7her", "mit", "dir", "ge\u00b7hal\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du, so geht es gl\u00fccklich fort", "tokens": ["Willst", "du", ",", "so", "geht", "es", "gl\u00fcck\u00b7lich", "fort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fordert oft es kaum ein Wort;", "tokens": ["Und", "for\u00b7dert", "oft", "es", "kaum", "ein", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will aber nicht mein Wunsch bestehen,", "tokens": ["Will", "a\u00b7ber", "nicht", "mein", "Wunsch", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fang' ich fort in mich zu gehen", "tokens": ["So", "fang'", "ich", "fort", "in", "mich", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und seh', ob ich, Herr, oder du", "tokens": ["Und", "seh'", ",", "ob", "ich", ",", "Herr", ",", "o\u00b7der", "du"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "$,", "NN", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Fug und Ursach geb' hierzu.", "tokens": ["Wo", "Fug", "und", "Ur\u00b7sach", "geb'", "hier\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft find' ich keinen Weg noch Steg", "tokens": ["Oft", "find'", "ich", "kei\u00b7nen", "Weg", "noch", "Steg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00df so nicht um deinen Zweck,", "tokens": ["Und", "wei\u00df", "so", "nicht", "um", "dei\u00b7nen", "Zweck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch gleichwol bleib' ich unbetr\u00fcbet;", "tokens": ["Noch", "gleich\u00b7wol", "bleib'", "ich", "un\u00b7be\u00b7tr\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast mich schon so lang ge\u00fcbet,", "tokens": ["Du", "hast", "mich", "schon", "so", "lang", "ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich von deinem treuen Sinn", "tokens": ["Da\u00df", "ich", "von", "dei\u00b7nem", "treu\u00b7en", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr als zu viel versichert bin.", "tokens": ["Mehr", "als", "zu", "viel", "ver\u00b7si\u00b7chert", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPR", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bald legt sich deiner Liebe Nacht,", "tokens": ["Bald", "legt", "sich", "dei\u00b7ner", "Lie\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann sehe ich, was du gemacht.", "tokens": ["Dann", "se\u00b7he", "ich", ",", "was", "du", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie einer, der im Schiffe lieget", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "im", "Schif\u00b7fe", "lie\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in dem Schlaf die See durchpfl\u00fcget,", "tokens": ["Und", "in", "dem", "Schlaf", "die", "See", "durch\u00b7pfl\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So find' ich ausser meiner Acht", "tokens": ["So", "find'", "ich", "aus\u00b7ser", "mei\u00b7ner", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich weiter fort als ich gedacht.", "tokens": ["Mich", "wei\u00b7ter", "fort", "als", "ich", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Berg, die Klippen und das Meer,", "tokens": ["Den", "Berg", ",", "die", "Klip\u00b7pen", "und", "das", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich beschlossen ganz umher", "tokens": ["Die", "mich", "be\u00b7schlos\u00b7sen", "ganz", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meiner Seelen Sorge waren,", "tokens": ["Und", "mei\u00b7ner", "See\u00b7len", "Sor\u00b7ge", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die bist du weislich umgefahren", "tokens": ["Die", "bist", "du", "weis\u00b7lich", "um\u00b7ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und hast mir einen Weg gebahnt,", "tokens": ["Und", "hast", "mir", "ei\u00b7nen", "Weg", "ge\u00b7bahnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Davon mir kaum nicht hat geahnt.", "tokens": ["Da\u00b7von", "mir", "kaum", "nicht", "hat", "ge\u00b7ahnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "F\u00fchr', o mein Sch\u00f6pfer, stets mich hier,", "tokens": ["F\u00fchr'", ",", "o", "mein", "Sch\u00f6p\u00b7fer", ",", "stets", "mich", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dir es d\u00fcnkt und seelig mir;", "tokens": ["Wie", "dir", "es", "d\u00fcnkt", "und", "see\u00b7lig", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "KON", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es mag so rauh es will hergehen,", "tokens": ["Es", "mag", "so", "rauh", "es", "will", "her\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mag nicht um noch vor mich sehen,", "tokens": ["Ich", "mag", "nicht", "um", "noch", "vor", "mich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist doch mit mir auf der Bahn", "tokens": ["Du", "bist", "doch", "mit", "mir", "auf", "der", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nimmst mich letzt mit Ehren an.", "tokens": ["Und", "nimmst", "mich", "letzt", "mit", "Eh\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach ja, mit Ehren hin zu dir,", "tokens": ["Ach", "ja", ",", "mit", "Eh\u00b7ren", "hin", "zu", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "APPR", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich dein Heilskleid, meine Zier,", "tokens": ["Da", "ich", "dein", "Heils\u00b7kleid", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In h\u00f6chster W\u00fcrde werde tragen", "tokens": ["In", "h\u00f6chs\u00b7ter", "W\u00fcr\u00b7de", "wer\u00b7de", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ewig meinen Dank dir sagen.", "tokens": ["Und", "e\u00b7wig", "mei\u00b7nen", "Dank", "dir", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Acht', Herz, nicht, was die Welt dir thut,", "tokens": ["Acht'", ",", "Herz", ",", "nicht", ",", "was", "die", "Welt", "dir", "thut", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PTKNEG", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Himmel, schau', wird Alles gut.", "tokens": ["Im", "Him\u00b7mel", ",", "schau'", ",", "wird", "Al\u00b7les", "gut", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}