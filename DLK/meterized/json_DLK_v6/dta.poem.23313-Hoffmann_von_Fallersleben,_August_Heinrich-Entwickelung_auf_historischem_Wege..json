{"dta.poem.23313": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwickelung auf historischem Wege.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O lasset doch den Geist der Zeiten!", "tokens": ["O", "las\u00b7set", "doch", "den", "Geist", "der", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn hemmt kein Wehr, kein Damm, kein Band;", "tokens": ["Ihn", "hemmt", "kein", "Wehr", ",", "kein", "Damm", ",", "kein", "Band", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird tagt\u00e4glich vorw\u00e4rts schreiten", "tokens": ["Er", "wird", "tag\u00b7t\u00e4g\u00b7lich", "vor\u00b7w\u00e4rts", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frei wie der Flu\u00df durch's ganze Land.", "tokens": ["Frei", "wie", "der", "Flu\u00df", "durch's", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Er str\u00f6met nicht aus Einer Quelle,", "tokens": ["Er", "str\u00f6\u00b7met", "nicht", "aus", "Ei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Einer Lebensader nur;", "tokens": ["Aus", "Ei\u00b7ner", "Le\u00b7ben\u00b7sa\u00b7der", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn n\u00e4hrt und speist an jeder Stelle", "tokens": ["Ihn", "n\u00e4hrt", "und", "speist", "an", "je\u00b7der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze lebende Natur.", "tokens": ["Die", "gan\u00b7ze", "le\u00b7ben\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ihr seht nur Eine Quelle springen,", "tokens": ["Ihr", "seht", "nur", "Ei\u00b7ne", "Quel\u00b7le", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diese stopft ihr zu im Nu", "tokens": ["Und", "die\u00b7se", "stopft", "ihr", "zu", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denkt, es wird uns jetzt gelingen,", "tokens": ["Und", "denkt", ",", "es", "wird", "uns", "jetzt", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir stopften ja die Quelle zu.", "tokens": ["Wir", "stopf\u00b7ten", "ja", "die", "Quel\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr hohen Herrn und Herrendiener!", "tokens": ["Ihr", "ho\u00b7hen", "Herrn", "und", "Her\u00b7ren\u00b7die\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr macht's ja grade wie der Wiener,", "tokens": ["Ihr", "macht's", "ja", "gra\u00b7de", "wie", "der", "Wie\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der auf die Donauquelle trat.", "tokens": ["Der", "auf", "die", "Do\u00b7nau\u00b7quel\u00b7le", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er sprach mit stillem Wohlbehagen:", "tokens": ["Er", "sprach", "mit", "stil\u00b7lem", "Wohl\u00b7be\u00b7ha\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Quelle hab' ich nun bekleibt!", "tokens": ["Die", "Quel\u00b7le", "hab'", "ich", "nun", "be\u00b7kleibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was werden wohl die Wiener sagen,", "tokens": ["Was", "wer\u00b7den", "wohl", "die", "Wie\u00b7ner", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn jetzt die Donau au\u00dfen bleibt? \u2014", "tokens": ["Wenn", "jetzt", "die", "Do\u00b7nau", "au\u00b7\u00dfen", "bleibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum lasset doch den Geist der Zeiten!", "tokens": ["Drum", "las\u00b7set", "doch", "den", "Geist", "der", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn hemmt kein Wehr, kein Damm, kein Band;", "tokens": ["Ihn", "hemmt", "kein", "Wehr", ",", "kein", "Damm", ",", "kein", "Band", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird tagt\u00e4glich vorw\u00e4rts schreiten", "tokens": ["Er", "wird", "tag\u00b7t\u00e4g\u00b7lich", "vor\u00b7w\u00e4rts", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frei wie der Flu\u00df durch's ganze Land.", "tokens": ["Frei", "wie", "der", "Flu\u00df", "durch's", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "ADJA", 