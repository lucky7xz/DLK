{"dta.poem.19429": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "162.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich den nahen Tod dem Todesnahen zeigen?", "tokens": ["Soll", "ich", "den", "na\u00b7hen", "Tod", "dem", "To\u00b7des\u00b7na\u00b7hen", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll ich dem Sterbenden von seinem Sterben schweigen? \u2014", "tokens": ["Soll", "ich", "dem", "Ster\u00b7ben\u00b7den", "von", "sei\u00b7nem", "Ster\u00b7ben", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vor Augen hatt' er stets in diesem jenes Leben,", "tokens": ["Vor", "Au\u00b7gen", "hatt'", "er", "stets", "in", "die\u00b7sem", "je\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewaltsam brauchst du nicht den Vorhang ihm zu heben.", "tokens": ["Ge\u00b7walt\u00b7sam", "brauchst", "du", "nicht", "den", "Vor\u00b7hang", "ihm", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch w\u00fcrd' er auch dem Tod mit unbefangnem Blick", "tokens": ["Doch", "w\u00fcrd'", "er", "auch", "dem", "Tod", "mit", "un\u00b7be\u00b7fang\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Antlitz schaun, wie sonst manch anderem Geschick.", "tokens": ["Ins", "Ant\u00b7litz", "schaun", ",", "wie", "sonst", "manch", "an\u00b7de\u00b7rem", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "PWAV", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob du den Tod ihm magst verdecken, ob entdecken,", "tokens": ["Ob", "du", "den", "Tod", "ihm", "magst", "ver\u00b7de\u00b7cken", ",", "ob", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,", "KOUS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gef\u00e4rden wirst du dort ihn nicht, hier nicht erschrecken.", "tokens": ["Ge\u00b7f\u00e4r\u00b7den", "wirst", "du", "dort", "ihn", "nicht", ",", "hier", "nicht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "PPER", "PTKNEG", "$,", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch ists ein wicht'ger Schritt, von hier hin\u00fcbertreten", "tokens": ["Doch", "ists", "ein", "wicht'\u00b7ger", "Schritt", ",", "von", "hier", "hin\u00b7\u00fc\u00b7bert\u00b7re\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins unbekannte dort, bei dem es ziemt zu beten.", "tokens": ["Ins", "un\u00b7be\u00b7kann\u00b7te", "dort", ",", "bei", "dem", "es", "ziemt", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "$,", "APPR", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du bet', und frage nicht, ob er auch bete mit;", "tokens": ["Du", "bet'", ",", "und", "fra\u00b7ge", "nicht", ",", "ob", "er", "auch", "be\u00b7te", "mit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bete f\u00fcr dich und ihn, wie er hin\u00fcber tritt.", "tokens": ["Be\u00b7te", "f\u00fcr", "dich", "und", "ihn", ",", "wie", "er", "hin\u00b7\u00fc\u00b7ber", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}