{"textgrid.poem.37939": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger Wohlgemuth", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es jagt ein J\u00e4ger wohlgemuth,", "tokens": ["Es", "jagt", "ein", "J\u00e4\u00b7ger", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er jagt aus frischem freien Muth", "tokens": ["Er", "jagt", "aus", "fri\u00b7schem", "frei\u00b7en", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl unter gr\u00fcnen Linden,", "tokens": ["Wohl", "un\u00b7ter", "gr\u00fc\u00b7nen", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er jagt derselben Thierlein viel,", "tokens": ["Er", "jagt", "der\u00b7sel\u00b7ben", "Thier\u00b7lein", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinen schnellen Winden.", "tokens": ["Mit", "sei\u00b7nen", "schnel\u00b7len", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er jagt \u00fcber Berg und tiefe Thal,", "tokens": ["Er", "jagt", "\u00fc\u00b7ber", "Berg", "und", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unter den Stauden \u00fcberall.", "tokens": ["Un\u00b7ter", "den", "Stau\u00b7den", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sein H\u00f6rnlein th\u00e4t er blasen,", "tokens": ["Sein", "H\u00f6rn\u00b7lein", "th\u00e4t", "er", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Lieb wohl auf den J\u00e4ger harrt,", "tokens": ["Sein", "Lieb", "wohl", "auf", "den", "J\u00e4\u00b7ger", "harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort auf der gr\u00fcnen Stra\u00dfen.", "tokens": ["Dort", "auf", "der", "gr\u00fc\u00b7nen", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er spreit den Mantel in das Gras,", "tokens": ["Er", "spreit", "den", "Man\u00b7tel", "in", "das", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bat, da\u00df sie zu ihm nieder sa\u00df,", "tokens": ["Bat", ",", "da\u00df", "sie", "zu", "ihm", "nie\u00b7der", "sa\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit weissem Arm umfangen:", "tokens": ["Mit", "weis\u00b7sem", "Arm", "um\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbgehab dich wohl mein Tr\u00f6sterin,", "tokens": ["\u00bb", "ge\u00b7hab", "dich", "wohl", "mein", "Tr\u00f6s\u00b7te\u00b7rin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach dir steht mein Verlangen.", "tokens": ["Nach", "dir", "steht", "mein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Uns nezt kein Reif, uns k\u00fchlt kein Schnee,", "tokens": ["Uns", "nezt", "kein", "Reif", ",", "uns", "k\u00fchlt", "kein", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es brennen noch im gr\u00fcnen Klee,", "tokens": ["Es", "bren\u00b7nen", "noch", "im", "gr\u00fc\u00b7nen", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwei R\u00f6slein auf der Heiden,", "tokens": ["Zwei", "R\u00f6s\u00b7lein", "auf", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Liebesschein, in Sonnenschein,", "tokens": ["In", "Lie\u00b7bes\u00b7schein", ",", "in", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die zwei soll man nicht scheiden.\u00ab", "tokens": ["Die", "zwei", "soll", "man", "nicht", "schei\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "CARD", "VMFIN", "PIS", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}