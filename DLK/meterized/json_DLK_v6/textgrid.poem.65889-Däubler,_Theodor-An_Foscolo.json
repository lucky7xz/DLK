{"textgrid.poem.65889": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Foscolo", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Morgenlandes maibetaute Blume,", "tokens": ["Des", "Mor\u00b7gen\u00b7lan\u00b7des", "mai\u00b7be\u00b7tau\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Eiland Z\u00e1kynthos auf stolzem Meer,", "tokens": ["Das", "Ei\u00b7land", "Z\u00e1ky\u00b7nthos", "auf", "stol\u00b7zem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gebar den Leib f\u00fcr deinen Lichtbegehr:", "tokens": ["Ge\u00b7bar", "den", "Leib", "f\u00fcr", "dei\u00b7nen", "Licht\u00b7be\u00b7gehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Wunsch blieb still auf duftbewehter Krume.", "tokens": ["Der", "Wunsch", "blieb", "still", "auf", "duft\u00b7be\u00b7weh\u00b7ter", "Kru\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verliebter Lieder Lilie wuchs zum Ruhme", "tokens": ["Ver\u00b7lieb\u00b7ter", "Lie\u00b7der", "Li\u00b7lie", "wuchs", "zum", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Griechen, spro\u00df als ihrer Helden Wehr,", "tokens": ["Der", "Grie\u00b7chen", ",", "spro\u00df", "als", "ih\u00b7rer", "Hel\u00b7den", "Wehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KOKOM", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn Reinheit ist das Erz f\u00fcr hellen Speer", "tokens": ["Denn", "Rein\u00b7heit", "ist", "das", "Erz", "f\u00fcr", "hel\u00b7len", "Speer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hilft dem Volk bei altem Eigentume.", "tokens": ["Und", "hilft", "dem", "Volk", "bei", "al\u00b7tem", "Ei\u00b7gen\u00b7tu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ersungne B\u00e4ume setztest du den Toten,", "tokens": ["Er\u00b7sung\u00b7ne", "B\u00e4u\u00b7me", "setz\u00b7test", "du", "den", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zu Marmorurnen, wo ein Name glomm,", "tokens": ["Zu", "Mar\u00b7mo\u00b7rur\u00b7nen", ",", "wo", "ein", "Na\u00b7me", "glomm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gewitter keine Inschriften bedrohten.", "tokens": ["Ge\u00b7wit\u00b7ter", "kei\u00b7ne", "In\u00b7schrif\u00b7ten", "be\u00b7droh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie ging dein Schritt zu Gr\u00e4bern schlicht und fromm,", "tokens": ["Wie", "ging", "dein", "Schritt", "zu", "Gr\u00e4\u00b7bern", "schlicht", "und", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis, dort zu knien, Verbote dir entlohten,", "tokens": ["Bis", ",", "dort", "zu", "kni\u00b7en", ",", "Ver\u00b7bo\u00b7te", "dir", "ent\u00b7loh\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "PTKZU", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn blo\u00df zum freien Menschen riefst du: Komm!", "tokens": ["Denn", "blo\u00df", "zum", "frei\u00b7en", "Men\u00b7schen", "riefst", "du", ":", "Komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}