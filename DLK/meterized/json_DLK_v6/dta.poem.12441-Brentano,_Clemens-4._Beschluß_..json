{"dta.poem.12441": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "4.  Beschlu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt das M\u00e4dchen fr\u00fch aufstehn                 ", "tokens": ["Es", "wollt", "das", "M\u00e4d\u00b7chen", "fr\u00fch", "auf\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in den gr\u00fcnen Wald spazieren gehn.", "tokens": ["Und", "in", "den", "gr\u00fc\u00b7nen", "Wald", "spa\u00b7zie\u00b7ren", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als sie nun in den gr\u00fcnen Wald kam,", "tokens": ["Und", "als", "sie", "nun", "in", "den", "gr\u00fc\u00b7nen", "Wald", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da fand sie einen verwundeten Knabn.", "tokens": ["da", "fand", "sie", "ei\u00b7nen", "ver\u00b7wun\u00b7de\u00b7ten", "Knabn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Knab der war von Blut so roth,", "tokens": ["Der", "Knab", "der", "war", "von", "Blut", "so", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und als sie sich verwand, war er schon todt.", "tokens": ["Und", "als", "sie", "sich", "ver\u00b7wand", ",", "war", "er", "schon", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u201ewo krieg ich nun zwey Leidfr\u00e4ulein,", "tokens": ["\u201e", "wo", "krieg", "ich", "nun", "zwey", "Leid\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edie mein fein Knaben zu Grabe weinn?", "tokens": ["\u201e", "die", "mein", "fein", "Kna\u00b7ben", "zu", "Gra\u00b7be", "weinn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "ADJD", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201ewo krieg ich nun sechs Reuterknabn,", "tokens": ["\u201e", "wo", "krieg", "ich", "nun", "sechs", "Reu\u00b7ter\u00b7knabn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edie mein fein Knaben zu Grabe tragn?", "tokens": ["\u201e", "die", "mein", "fein", "Kna\u00b7ben", "zu", "Gra\u00b7be", "tragn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "ADJD", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201ewie lang soll ich denn trauren gehn?", "tokens": ["\u201e", "wie", "lang", "soll", "ich", "denn", "trau\u00b7ren", "gehn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ebis alle Wasser zusammen gehn!", "tokens": ["\u201e", "bis", "al\u00b7le", "Was\u00b7ser", "zu\u00b7sam\u00b7men", "gehn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201eja alle Wasser gehn nicht zusammn,", "tokens": ["\u201e", "ja", "al\u00b7le", "Was\u00b7ser", "gehn", "nicht", "zu\u00b7sammn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201eso wird mein Trauren kein Ende han.\u201c", "tokens": ["\u201e", "so", "wird", "mein", "Trau\u00b7ren", "kein", "En\u00b7de", "han", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN", "PIAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}