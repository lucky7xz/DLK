{"textgrid.poem.52746": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. Aus dem FF.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steigt aus euren blut'gen Gr\u00fcften,", "tokens": ["Steigt", "aus", "eu\u00b7ren", "blut'\u00b7gen", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr trugt ein Heldenherz,", "tokens": ["Die", "ihr", "trugt", "ein", "Hel\u00b7den\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich wittr' es in den L\u00fcften,", "tokens": ["Denn", "ich", "wit\u00b7tr'", "es", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie da g\u00e4hrt ein neuer M\u00e4rz.", "tokens": ["Wie", "da", "g\u00e4hrt", "ein", "neu\u00b7er", "M\u00e4rz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Rauschen dieser St\u00fcrme", "tokens": ["Aus", "dem", "Rau\u00b7schen", "die\u00b7ser", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6r' ich zu der Menschheit Heile", "tokens": ["H\u00f6r'", "ich", "zu", "der", "Menschheit", "Hei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Einen Zug von Guillotinen", "tokens": ["Ei\u00b7nen", "Zug", "von", "Guil\u00b7lo\u00b7ti\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Schwirren heil'ger Beile.", "tokens": ["Und", "das", "Schwir\u00b7ren", "heil'\u00b7ger", "Bei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur im Blute wohnt die neue", "tokens": ["Nur", "im", "Blu\u00b7te", "wohnt", "die", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit, nur im rothen Blut!", "tokens": ["Frei\u00b7heit", ",", "nur", "im", "ro\u00b7then", "Blut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommen wird der Tag der Reue,", "tokens": ["Kom\u00b7men", "wird", "der", "Tag", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Stahl noch l\u00e4nger ruht;", "tokens": ["Wenn", "der", "Stahl", "noch", "l\u00e4n\u00b7ger", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn nicht bald an jedem Baume", "tokens": ["Wenn", "nicht", "bald", "an", "je\u00b7dem", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns ein Volksverr\u00e4ther baumelt,", "tokens": ["Uns", "ein", "Volks\u00b7ver\u00b7r\u00e4\u00b7ther", "bau\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn nicht bald von jeder Klinge", "tokens": ["Wenn", "nicht", "bald", "von", "je\u00b7der", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns ein Schergenhaupt enttaumelt.", "tokens": ["Uns", "ein", "Scher\u00b7gen\u00b7haupt", "ent\u00b7tau\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der Scheich aus M\u00e4dchenarmen", "tokens": ["Wie", "der", "Scheich", "aus", "M\u00e4d\u00b7chen\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00dfet euch vom Mitleid los,", "tokens": ["Rei\u00b7\u00dfet", "euch", "vom", "Mit\u00b7leid", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Rothhaut st\u00fcrzt auf Farmen", "tokens": ["Wie", "die", "Roth\u00b7haut", "st\u00fcrzt", "auf", "Far\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "St\u00fcrzt euch in der Mordlust Schoo\u00df!", "tokens": ["St\u00fcrzt", "euch", "in", "der", "Mord\u00b7lust", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schrecket so die Camarilla,", "tokens": ["Schre\u00b7cket", "so", "die", "Ca\u00b7ma\u00b7ril\u00b7la", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie das Wigwam wird geschrecket,", "tokens": ["Wie", "das", "Wig\u00b7wam", "wird", "ge\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn den Leu von Madagaskar", "tokens": ["Wenn", "den", "Leu", "von", "Ma\u00b7da\u00b7gas\u00b7kar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.8": {"text": "Frevelnd der Malaie necket!", "tokens": ["Fre\u00b7velnd", "der", "Ma\u00b7lai\u00b7e", "ne\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Seht! wie sie die Z\u00e4hne fletschen,", "tokens": ["Seht", "!", "wie", "sie", "die", "Z\u00e4h\u00b7ne", "flet\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die von dem Hy\u00e4nenbund,", "tokens": ["Die", "von", "dem", "Hy\u00b7\u00e4\u00b7nen\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mit den eigenen Kart\u00e4tschen", "tokens": ["Mit", "den", "ei\u00b7ge\u00b7nen", "Kar\u00b7t\u00e4t\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stopfet ihren heischern Schlund!", "tokens": ["Stop\u00b7fet", "ih\u00b7ren", "hei\u00b7schern", "Schlund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur wenn jedes Herz mu\u00df zittern", "tokens": ["Nur", "wenn", "je\u00b7des", "Herz", "mu\u00df", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem Schwurgericht der Rache,", "tokens": ["Vor", "dem", "Schwur\u00b7ge\u00b7richt", "der", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird das Volk der Freiheit inne,", "tokens": ["Wird", "das", "Volk", "der", "Frei\u00b7heit", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Triumphirt die gute Sache.", "tokens": ["Tri\u00b7um\u00b7phirt", "die", "gu\u00b7te", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht allein die F\u00fcrstenthrone,", "tokens": ["Nicht", "al\u00b7lein", "die", "F\u00fcrs\u00b7ten\u00b7thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und die Pfaffenst\u00fchle nur,", "tokens": ["Und", "die", "Pfaf\u00b7fen\u00b7st\u00fch\u00b7le", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lodern auf im Flammenhohne,", "tokens": ["Lo\u00b7dern", "auf", "im", "Flam\u00b7men\u00b7hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fallen dem Vernichtungsschwur \u2013", "tokens": ["Fal\u00b7len", "dem", "Ver\u00b7nich\u00b7tungs\u00b7schwur", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, es sorgt der Hahn, der rothe,", "tokens": ["Nein", ",", "es", "sorgt", "der", "Hahn", ",", "der", "ro\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er ", "tokens": ["Da\u00df", "er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der nicht unter sich beherbergt", "tokens": ["Der", "nicht", "un\u00b7ter", "sich", "be\u00b7her\u00b7bergt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "PRF", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Eine Proletarierseele.", "tokens": ["Ei\u00b7ne", "Pro\u00b7le\u00b7ta\u00b7rier\u00b7see\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur wenn ", "tokens": ["Nur", "wenn"], "token_info": ["word", "word"], "pos": ["ADV", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Ueber alle Welt gestreut,", "tokens": ["Ue\u00b7ber", "al\u00b7le", "Welt", "ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dereinst auch wiederkehren", "tokens": ["Wird", "de\u00b7reinst", "auch", "wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verhei\u00df'ne gold'ne Zeit.", "tokens": ["Die", "ver\u00b7hei\u00df'\u00b7ne", "gold'\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo kein W\u00fcrger mehr zu schauen,", "tokens": ["Wo", "kein", "W\u00fcr\u00b7ger", "mehr", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kein Aristokratentiger,", "tokens": ["Kein", "A\u00b7ris\u00b7to\u00b7kra\u00b7ten\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kein Verr\u00e4ther und kein Sclave,", "tokens": ["Kein", "Ver\u00b7r\u00e4\u00b7ther", "und", "kein", "Scla\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schurke nicht und nicht Betr\u00fcger.", "tokens": ["Schur\u00b7ke", "nicht", "und", "nicht", "Be\u00b7tr\u00fc\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "D'rum wohlauf, ihr rothen Br\u00fcder,", "tokens": ["D'\u00b7rum", "wohl\u00b7auf", ",", "ihr", "ro\u00b7then", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geht an euer Tagewerk!", "tokens": ["Geht", "an", "eu\u00b7er", "Ta\u00b7ge\u00b7werk", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Lenze tagen wieder", "tokens": ["Dei\u00b7ne", "Len\u00b7ze", "ta\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Edle M\u00e4nnerschaft vom Berg.", "tokens": ["Ed\u00b7le", "M\u00e4n\u00b7ner\u00b7schaft", "vom", "Berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Knirschet ", "tokens": ["Knir\u00b7schet"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Kein Erbarmen und kein Schonen \u2013", "tokens": ["Kein", "Er\u00b7bar\u00b7men", "und", "kein", "Scho\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So zertritt das Volk die Hyder", "tokens": ["So", "zer\u00b7tritt", "das", "Volk", "die", "Hy\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller k\u00fcnft'gen Reactionen.", "tokens": ["Al\u00b7ler", "k\u00fcnft'\u00b7gen", "Re\u00b7ac\u00b7ti\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}