{"textgrid.poem.36911": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mutter Erde! Deutsche Erde!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter Erde! Deutsche Erde!", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "!", "Deut\u00b7sche", "Er\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du nicht? Der Fr\u00fchling kam!", "tokens": ["H\u00f6rst", "du", "nicht", "?", "Der", "Fr\u00fch\u00b7ling", "kam", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Bote rief das Werde!", "tokens": ["Got\u00b7tes", "Bo\u00b7te", "rief", "das", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PDS", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttle ab den Wintergram!", "tokens": ["Sch\u00fctt\u00b7le", "ab", "den", "Win\u00b7ter\u00b7gram", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine See'n, sie rauschen m\u00e4chtig,", "tokens": ["Dei\u00b7ne", "See'n", ",", "sie", "rau\u00b7schen", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Str\u00f6me ziehn geschwind,", "tokens": ["Dei\u00b7ne", "Str\u00f6\u00b7me", "ziehn", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine W\u00e4lder gr\u00fcnen pr\u00e4chtig,", "tokens": ["Dei\u00b7ne", "W\u00e4l\u00b7der", "gr\u00fc\u00b7nen", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Luft ist s\u00fc\u00df und lind!", "tokens": ["Und", "die", "Luft", "ist", "s\u00fc\u00df", "und", "lind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Mutter Erde! Deutsche Erde!", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "!", "Deut\u00b7sche", "Er\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob dein Ohr es nicht vernahm'?", "tokens": ["Ob", "dein", "Ohr", "es", "nicht", "ver\u00b7nahm'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Bote rief das Werde!", "tokens": ["Got\u00b7tes", "Bo\u00b7te", "rief", "das", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PDS", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttle ab den Wintergram!", "tokens": ["Sch\u00fctt\u00b7le", "ab", "den", "Win\u00b7ter\u00b7gram", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine V\u00f6gel singen Lieder,", "tokens": ["Dei\u00b7ne", "V\u00f6\u00b7gel", "sin\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und all\u00fcberall ist's Mai!", "tokens": ["Und", "al\u00b7l\u00fc\u00b7be\u00b7rall", "ist's", "Mai", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Deine Rosen bl\u00fchen wieder,", "tokens": ["Dei\u00b7ne", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Schmerzen sind vorbei!", "tokens": ["Dei\u00b7ne", "Schmer\u00b7zen", "sind", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mutter Erde, deutsche Erde,", "tokens": ["Mut\u00b7ter", "Er\u00b7de", ",", "deut\u00b7sche", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz der holde Fr\u00fchling kam,", "tokens": ["Trotz", "der", "hol\u00b7de", "Fr\u00fch\u00b7ling", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt aus deinen Felsenrunzeln", "tokens": ["Blickt", "aus", "dei\u00b7nen", "Fel\u00b7sen\u00b7run\u00b7zeln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch der alte Wintergram.", "tokens": ["Noch", "der", "al\u00b7te", "Win\u00b7ter\u00b7gram", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Deine S\u00f6hne sind so flei\u00dfig,", "tokens": ["Dei\u00b7ne", "S\u00f6h\u00b7ne", "sind", "so", "flei\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So gehorsam und so treu!", "tokens": ["So", "ge\u00b7hor\u00b7sam", "und", "so", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle, alle Neununddrei\u00dfig", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "Neun\u00b7und\u00b7drei\u00b7\u00dfig"], "token_info": ["word", "punct", "word", "word"], "pos": ["PIS", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dichten Lieder auf den Mai.", "tokens": ["Dich\u00b7ten", "Lie\u00b7der", "auf", "den", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Liebe, gute Mutter Erde", "tokens": ["Lie\u00b7be", ",", "gu\u00b7te", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du nicht? der Fr\u00fchling kam!", "tokens": ["H\u00f6rst", "du", "nicht", "?", "der", "Fr\u00fch\u00b7ling", "kam", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein! trotz Gottes neuem Werde", "tokens": ["Nein", "!", "trotz", "Got\u00b7tes", "neu\u00b7em", "Wer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt im Antlitz ihr der Gram.", "tokens": ["Bleibt", "im", "Ant\u00b7litz", "ihr", "der", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}