{"textgrid.poem.62914": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der gro\u00dfe Heinrich kroch auf allen Vieren", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gro\u00dfe Heinrich kroch auf allen Vieren", "tokens": ["Der", "gro\u00b7\u00dfe", "Hein\u00b7rich", "kroch", "auf", "al\u00b7len", "Vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinem Sohn, der auf ihm ritt", "tokens": ["Mit", "sei\u00b7nem", "Sohn", ",", "der", "auf", "ihm", "ritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem Saal umher. Schnell \u00f6fnen sich die Th\u00fcren:", "tokens": ["In", "ei\u00b7nem", "Saal", "um\u00b7her", ".", "Schnell", "\u00f6f\u00b7nen", "sich", "die", "Th\u00fc\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Abgesandte von Madrid", "tokens": ["Der", "Ab\u00b7ge\u00b7sand\u00b7te", "von", "Mad\u00b7rid"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Trat ins Gemach und sah ihn galopieren.", "tokens": ["Trat", "ins", "Ge\u00b7mach", "und", "sah", "ihn", "ga\u00b7lo\u00b7pie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Herr, sind Sie Vater? rief der Held mit heiterm Muth,", "tokens": ["Herr", ",", "sind", "Sie", "Va\u00b7ter", "?", "rief", "der", "Held", "mit", "hei\u00b7term", "Muth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "$.", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und lag noch immer auf den H\u00e4nden.", "tokens": ["Und", "lag", "noch", "im\u00b7mer", "auf", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja, Sir, versetzt der Don. Gut, sagte Heinrich, gut,", "tokens": ["Ja", ",", "Sir", ",", "ver\u00b7setzt", "der", "Don", ".", "Gut", ",", "sag\u00b7te", "Hein\u00b7rich", ",", "gut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$.", "ADJD", "$,", "VVFIN", "NE", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So kann ich meinen Zug vollenden.", "tokens": ["So", "kann", "ich", "mei\u00b7nen", "Zug", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}