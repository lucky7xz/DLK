{"dta.poem.2930": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "129.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich preise laut die Stadt, die nicht zwar mich geboren,", "tokens": ["Ich", "prei\u00b7se", "laut", "die", "Stadt", ",", "die", "nicht", "zwar", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch zum B\u00fcrger hat in Ehren mich erkoren,", "tokens": ["Und", "doch", "zum", "B\u00fcr\u00b7ger", "hat", "in", "Eh\u00b7ren", "mich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht weil ich irgend mich verdient gemacht um sie", "tokens": ["Nicht", "weil", "ich", "ir\u00b7gend", "mich", "ver\u00b7dient", "ge\u00b7macht", "um", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "VVPP", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch etwas anders als durch meine Poesie.", "tokens": ["Durch", "et\u00b7was", "an\u00b7ders", "als", "durch", "mei\u00b7ne", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Durch meine Poesie war mirs zuvor gelungen,", "tokens": ["Durch", "mei\u00b7ne", "Poe\u00b7sie", "war", "mirs", "zu\u00b7vor", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Da\u00df in derselben Stadt ich mir ein Weib errungen.", "tokens": ["Da\u00df", "in", "der\u00b7sel\u00b7ben", "Stadt", "ich", "mir", "ein", "Weib", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Himmelspoesie hat eine ird'sche Kraft,", "tokens": ["Die", "Him\u00b7mel\u00b7spo\u00b7e\u00b7sie", "hat", "ei\u00b7ne", "ird'\u00b7sche", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zu Hauswirthschaft mir verhalf und B\u00fcrgerschaft.", "tokens": ["Die", "zu", "Haus\u00b7wirth\u00b7schaft", "mir", "ver\u00b7half", "und", "B\u00fcr\u00b7ger\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}}}}}