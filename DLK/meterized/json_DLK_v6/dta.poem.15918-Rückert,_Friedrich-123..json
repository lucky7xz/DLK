{"dta.poem.15918": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "123.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seherinnen, die statt Augen andre Glieder", "tokens": ["Die", "Se\u00b7he\u00b7rin\u00b7nen", ",", "die", "statt", "Au\u00b7gen", "and\u00b7re", "Glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Sehn gebrauchen, sind von Herzen mir zuwider.", "tokens": ["Zum", "Sehn", "ge\u00b7brau\u00b7chen", ",", "sind", "von", "Her\u00b7zen", "mir", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "VAFIN", "APPR", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch eine sah ein Bild in ihres Herzens Spiegel,", "tokens": ["Doch", "ei\u00b7ne", "sah", "ein", "Bild", "in", "ih\u00b7res", "Her\u00b7zens", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem gerne sich erschlie\u00dft des meinen Schlo\u00df und Riegel.", "tokens": ["Dem", "ger\u00b7ne", "sich", "er\u00b7schlie\u00dft", "des", "mei\u00b7nen", "Schlo\u00df", "und", "Rie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PRF", "VVFIN", "ART", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein freundlich ernster Greis, mit offenstehnder Brust;", "tokens": ["Ein", "freund\u00b7lich", "erns\u00b7ter", "Greis", ",", "mit", "of\u00b7fen\u00b7stehn\u00b7der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Kindlein, l\u00e4chelnd, blickt daraus hervor mit Lust.", "tokens": ["Ein", "Kin\u00b7dlein", ",", "l\u00e4\u00b7chelnd", ",", "blickt", "da\u00b7raus", "her\u00b7vor", "mit", "Lust", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "PAV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So machen m\u00f6cht' ich selbst mein Herz zu einer Wiege,", "tokens": ["So", "ma\u00b7chen", "m\u00f6cht'", "ich", "selbst", "mein", "Herz", "zu", "ei\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df ich selber so als wie ein Kindlein liege!", "tokens": ["Und", "da\u00df", "ich", "sel\u00b7ber", "so", "als", "wie", "ein", "Kin\u00b7dlein", "lie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da ist das Innerste des Menschen sch\u00f6nbeschickt,", "tokens": ["Da", "ist", "das", "In\u00b7ners\u00b7te", "des", "Men\u00b7schen", "sch\u00f6n\u00b7be\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo aus des Greisen Brust das Kindlein l\u00e4chelnd blickt.", "tokens": ["Wo", "aus", "des", "Grei\u00b7sen", "Brust", "das", "Kin\u00b7dlein", "l\u00e4\u00b7chelnd", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}