{"dta.poem.9347": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV  \n Sexain/ Sechszeiliges gesetze/  \n von gemeinen Versen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein wunder ists/ da\u00df deine rosen bl\u00fchen/", "tokens": ["Kein", "wun\u00b7der", "ists", "/", "da\u00df", "dei\u00b7ne", "ro\u00b7sen", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o sch\u00f6nes Bild/ bey rauher winterszeit/", "tokens": ["o", "sch\u00f6\u00b7nes", "Bild", "/", "bey", "rau\u00b7her", "win\u00b7ter\u00b7szeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "weil sie vor frost dein Athem gantz befreyht/", "tokens": ["weil", "sie", "vor", "frost", "dein", "A\u00b7them", "gantz", "be\u00b7freyht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die warme Lufft kann sie so sch\u00f6n erzihen;", "tokens": ["die", "war\u00b7me", "Lufft", "kann", "sie", "so", "sch\u00f6n", "er\u00b7zi\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "dein Augen-glantz ist jhnen Sonnenschein;", "tokens": ["dein", "Au\u00b7gen\u00b7glantz", "ist", "jh\u00b7nen", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "wann gleich der frost von au\u00dfen bricht herem.", "tokens": ["wann", "gleich", "der", "frost", "von", "au\u00b7\u00dfen", "bricht", "he\u00b7rem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}