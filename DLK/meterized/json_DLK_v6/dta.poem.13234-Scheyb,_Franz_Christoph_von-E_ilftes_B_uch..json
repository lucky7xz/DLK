{"dta.poem.13234": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eoft hat der Feinde Wiz der Freunde Sinn entzweyt,", "tokens": ["\u201e", "oft", "hat", "der", "Fein\u00b7de", "Wiz", "der", "Freun\u00b7de", "Sinn", "ent\u00b7zweyt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso da\u00df sie lieber sich dem Feind, als uns geweiht;", "tokens": ["\u201e", "so", "da\u00df", "sie", "lie\u00b7ber", "sich", "dem", "Feind", ",", "als", "uns", "ge\u00b7weiht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "ADV", "PRF", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eja gar das Krieges-Schwert mit Freundes Blut beflecket,", "tokens": ["\u201e", "ja", "gar", "das", "Krie\u00b7ge\u00b7sSchwert", "mit", "Freun\u00b7des", "Blut", "be\u00b7fle\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "80\u201dMit eignen Leichen selbst das Vaterland bedecket.", "tokens": ["\"", "Mit", "eig\u00b7nen", "Lei\u00b7chen", "selbst", "das", "Va\u00b7ter\u00b7land", "be\u00b7de\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edie List verblendte sie; sie f\u00fchrten manchen Streich,", "tokens": ["\u201e", "die", "List", "ver\u00b7blend\u00b7te", "sie", ";", "sie", "f\u00fchr\u00b7ten", "man\u00b7chen", "Streich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund wurden nicht gewahr, da\u00df sie das eigne Reich", "tokens": ["\u201e", "und", "wur\u00b7den", "nicht", "ge\u00b7wahr", ",", "da\u00df", "sie", "das", "eig\u00b7ne", "Reich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201emehr als des Feindes Land durch solche That getroffen;", "tokens": ["\u201e", "mehr", "als", "des", "Fein\u00b7des", "Land", "durch", "sol\u00b7che", "That", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "KOKOM", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df ihnen nichts daf\u00fcr, als eigner Fall zu hoffen.", "tokens": ["\u201e", "da\u00df", "ih\u00b7nen", "nichts", "da\u00b7f\u00fcr", ",", "als", "eig\u00b7ner", "Fall", "zu", "hof\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIS", "PAV", "$,", "KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "85\u201dSie sahen nicht da\u00df es der Feinde Vortheil sey,", "tokens": ["\"", "Sie", "sa\u00b7hen", "nicht", "da\u00df", "es", "der", "Fein\u00b7de", "Vor\u00b7theil", "sey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewann die Vereinigung des Vaterlands entzwey;", "tokens": ["\u201e", "wann", "die", "Ver\u00b7ei\u00b7ni\u00b7gung", "des", "Va\u00b7ter\u00b7lands", "ent\u00b7zwey", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eviel weniger, wie sie Ruhm, Ehr und Kr\u00e4fte schw\u00e4chen,", "tokens": ["\u201e", "viel", "we\u00b7ni\u00b7ger", ",", "wie", "sie", "Ruhm", ",", "Ehr", "und", "Kr\u00e4f\u00b7te", "schw\u00e4\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "PWAV", "PPER", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+---+-++-+-+-", "measure": "dactylic.init"}, "line.12": {"text": "\u201eund zum Behuf des Feinds der Lanze Spize brechen,", "tokens": ["\u201e", "und", "zum", "Be\u00b7huf", "des", "Feinds", "der", "Lan\u00b7ze", "Spi\u00b7ze", "bre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPRART", "NN", "ART", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewann zwischen ihnen nichts als Zwist und Trennung glim\u0303t:", "tokens": ["\u201e", "wann", "zwi\u00b7schen", "ih\u00b7nen", "nichts", "als", "Zwist", "und", "Tren\u00b7nung", "glim\u0303t", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "APPR", "PPER", "PIS", "KOKOM", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "90\u201dDer mit des Feindes Rath, der mit den Freunden stimmt.", "tokens": ["\"", "Der", "mit", "des", "Fein\u00b7des", "Rath", ",", "der", "mit", "den", "Freun\u00b7den", "stimmt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eja da\u00df sie mit Gewalt nur nach den Ketten streben,", "tokens": ["\u201e", "ja", "da\u00df", "sie", "mit", "Ge\u00b7walt", "nur", "nach", "den", "Ket\u00b7ten", "stre\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "APPR", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201edie manchem schon so lang an H\u00e4nd und F\u00fcssen kleben.", "tokens": ["\u201e", "die", "man\u00b7chem", "schon", "so", "lang", "an", "H\u00e4nd", "und", "F\u00fcs\u00b7sen", "kle\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "ADV", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}