{"textgrid.poem.59447": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00e4mpe", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach drei Jahren kam gefahren", "tokens": ["Nach", "drei", "Jah\u00b7ren", "kam", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam auf dem Rhein ein Schiff,", "tokens": ["Ein\u00b7sam", "auf", "dem", "Rhein", "ein", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin gebunden und voll Wunden", "tokens": ["Drin", "ge\u00b7bun\u00b7den", "und", "voll", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lag ein Rittersmann und rief:", "tokens": ["Lag", "ein", "Rit\u00b7ters\u00b7mann", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbstill den Garten sch\u00f6n tust warten", "tokens": ["\u00bb", "still", "den", "Gar\u00b7ten", "sch\u00f6n", "tust", "war\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "ART", "NN", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibst am Fenster ofte stehn,", "tokens": ["Bleibst", "am", "Fens\u00b7ter", "of\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhig scheinst du, heimlich weinst du,", "tokens": ["Ru\u00b7hig", "scheinst", "du", ",", "heim\u00b7lich", "weinst", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie die Schiffe unten gehn.", "tokens": ["Wie", "die", "Schif\u00b7fe", "un\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was vertraust du, warum baust du", "tokens": ["Was", "ver\u00b7traust", "du", ",", "wa\u00b7rum", "baust", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf der M\u00e4nner wilde Brust,", "tokens": ["Auf", "der", "M\u00e4n\u00b7ner", "wil\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das Blut ziert und der Streit r\u00fchrt", "tokens": ["Die", "das", "Blut", "ziert", "und", "der", "Streit", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und die sch\u00f6ne Todeslust!\u00ab", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "To\u00b7des\u00b7lust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oben spinnend, sa\u00df sie sinnend \u2013", "tokens": ["O\u00b7ben", "spin\u00b7nend", ",", "sa\u00df", "sie", "sin\u00b7nend", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwanden Schiff und Tageslicht,", "tokens": ["Schwan\u00b7den", "Schiff", "und", "Ta\u00b7ges\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was er sunge, war verklungen,", "tokens": ["Was", "er", "sun\u00b7ge", ",", "war", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie erkannt den Liebsten nicht.", "tokens": ["Sie", "er\u00b7kannt", "den", "Liebs\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}