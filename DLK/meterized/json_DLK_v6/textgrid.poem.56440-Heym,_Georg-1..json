{"textgrid.poem.56440": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nebel graun, die keinem Winde weichen.", "tokens": ["Die", "Ne\u00b7bel", "graun", ",", "die", "kei\u00b7nem", "Win\u00b7de", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die giftigen D\u00fcnste schw\u00e4ngern weit das Tal.", "tokens": ["Die", "gif\u00b7ti\u00b7gen", "D\u00fcns\u00b7te", "schw\u00e4n\u00b7gern", "weit", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein blasses Licht scheint in der Toten Reichen,", "tokens": ["Ein", "blas\u00b7ses", "Licht", "scheint", "in", "der", "To\u00b7ten", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eines Totenkopfes Auge fahl.", "tokens": ["Wie", "ei\u00b7nes", "To\u00b7ten\u00b7kop\u00b7fes", "Au\u00b7ge", "fahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entsetzlich w\u00e4lzt sich hin der Phlegeton.", "tokens": ["Ent\u00b7setz\u00b7lich", "w\u00e4lzt", "sich", "hin", "der", "Phle\u00b7ge\u00b7ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie tausend Niagaras hallt sein Br\u00fcllen.", "tokens": ["Wie", "tau\u00b7send", "Nia\u00b7ga\u00b7ras", "hallt", "sein", "Br\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Kl\u00fcfte wanken von den Schreien schon,", "tokens": ["Die", "Kl\u00fcf\u00b7te", "wan\u00b7ken", "von", "den", "Schrei\u00b7en", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die im Orkan die Feuerfluten f\u00fcllen.", "tokens": ["Die", "im", "Or\u00b7kan", "die", "Feu\u00b7er\u00b7flu\u00b7ten", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sie gl\u00fchn von Qualen wei\u00df. Wie Steine rollen", "tokens": ["Sie", "gl\u00fchn", "von", "Qua\u00b7len", "wei\u00df", ".", "Wie", "Stei\u00b7ne", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVFIN", "$.", "PWAV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Flu\u00df herab sie in der tr\u00fcben Glut,", "tokens": ["Den", "Flu\u00df", "her\u00b7ab", "sie", "in", "der", "tr\u00fc\u00b7ben", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie des geborstenen Eises Riesenschollen", "tokens": ["Wie", "des", "ge\u00b7bors\u00b7te\u00b7nen", "Ei\u00b7ses", "Rie\u00b7sen\u00b7schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So schmettert ihre Leiber hin die Flut.", "tokens": ["So", "schmet\u00b7tert", "ih\u00b7re", "Lei\u00b7ber", "hin", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie reiten aufeinander nackt und wild,", "tokens": ["Sie", "rei\u00b7ten", "auf\u00b7ein\u00b7an\u00b7der", "nackt", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Zorn und Wollust aufgebl\u00e4ht wie Schw\u00e4mme.", "tokens": ["Von", "Zorn", "und", "Wol\u00b7lust", "auf\u00b7ge\u00b7bl\u00e4ht", "wie", "Schw\u00e4m\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein h\u00f6llischer Choral im Takte schwillt", "tokens": ["Ein", "h\u00f6l\u00b7li\u00b7scher", "Cho\u00b7ral", "im", "Tak\u00b7te", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Grunde auf bis zu dem Kamm der D\u00e4mme.", "tokens": ["Vom", "Grun\u00b7de", "auf", "bis", "zu", "dem", "Kamm", "der", "D\u00e4m\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auf einem fetten Greise rittlings reitet", "tokens": ["Auf", "ei\u00b7nem", "fet\u00b7ten", "Grei\u00b7se", "ritt\u00b7lings", "rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein nacktes Weib mit schwarzem Flatterhaar.", "tokens": ["Ein", "nack\u00b7tes", "Weib", "mit", "schwar\u00b7zem", "Flat\u00b7ter\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ihren Scho\u00df und ihre Br\u00fcste breitet", "tokens": ["Und", "ih\u00b7ren", "Scho\u00df", "und", "ih\u00b7re", "Br\u00fcs\u00b7te", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie l\u00fcstern aus vor der Verdammten Schar.", "tokens": ["Sie", "l\u00fcs\u00b7tern", "aus", "vor", "der", "Ver\u00b7damm\u00b7ten", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Da br\u00fcllt der Chor in aufgepeitschter Lust.", "tokens": ["Da", "br\u00fcllt", "der", "Chor", "in", "auf\u00b7ge\u00b7peitschter", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Echo rollt im roten Katarakt.", "tokens": ["Das", "E\u00b7cho", "rollt", "im", "ro\u00b7ten", "Ka\u00b7ta\u00b7rakt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein riesiger Neger steigt herauf und packt", "tokens": ["Ein", "rie\u00b7si\u00b7ger", "Ne\u00b7ger", "steigt", "her\u00b7auf", "und", "packt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den wei\u00dfen Leib an seine schwarze Brust.", "tokens": ["Den", "wei\u00b7\u00dfen", "Leib", "an", "sei\u00b7ne", "schwar\u00b7ze", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Unz\u00e4hlige Augen sehn den Kampf und trinken", "tokens": ["Un\u00b7z\u00e4h\u00b7li\u00b7ge", "Au\u00b7gen", "sehn", "den", "Kampf", "und", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVINF"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Den Rausch der Gier. Er braust durch das Gew\u00fchl,", "tokens": ["Den", "Rausch", "der", "Gier", ".", "Er", "braust", "durch", "das", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da in dem Strom die Liebenden versinken,", "tokens": ["Da", "in", "dem", "Strom", "die", "Lie\u00b7ben\u00b7den", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den G\u00f6ttern gleich im hei\u00dfen Purpurpf\u00fchl.", "tokens": ["Den", "G\u00f6t\u00b7tern", "gleich", "im", "hei\u00b7\u00dfen", "Pur\u00b7pur\u00b7pf\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}