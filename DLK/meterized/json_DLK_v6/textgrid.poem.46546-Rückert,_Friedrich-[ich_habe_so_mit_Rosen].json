{"textgrid.poem.46546": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich habe so mit Rosen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe so mit Rosen", "tokens": ["Ich", "ha\u00b7be", "so", "mit", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich zugesteckt,", "tokens": ["Dich", "zu\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es blieb, da\u00df du gestorben,", "tokens": ["Es", "blieb", ",", "da\u00df", "du", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir unentdeckt.", "tokens": ["Mir", "un\u00b7ent\u00b7deckt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bald dacht' ich, da\u00df du lebend", "tokens": ["Bald", "dacht'", "ich", ",", "da\u00df", "du", "le\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch seiest mein,", "tokens": ["Noch", "sei\u00b7est", "mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bald, da\u00df du mir gewesen", "tokens": ["Bald", ",", "da\u00df", "du", "mir", "ge\u00b7we\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur stets ein Schein.", "tokens": ["Nur", "stets", "ein", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch wenn der Wind die Decke", "tokens": ["Doch", "wenn", "der", "Wind", "die", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rosen hebt,", "tokens": ["Der", "Ro\u00b7sen", "hebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Entdeck' ich, und erschrecke,", "tokens": ["Ent\u00b7deck'", "ich", ",", "und", "er\u00b7schre\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du gelebt.", "tokens": ["Da\u00df", "du", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}