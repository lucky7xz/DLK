{"dta.poem.19563": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schneewittchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Schneewittchen hast im Scherz du dich genannt,", "tokens": ["Schnee\u00b7witt\u00b7chen", "hast", "im", "Scherz", "du", "dich", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "PPER", "PRF", "VVPP", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Da plaudernd einst zusammen wir gesessen,", "tokens": ["Da", "plau\u00b7dernd", "einst", "zu\u00b7sam\u00b7men", "wir", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Augen tiefes Blau, die Elfenhand,", "tokens": ["Der", "Au\u00b7gen", "tie\u00b7fes", "Blau", ",", "die", "El\u00b7fen\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Nackens Blondgekraus, wer kann's vergessen?", "tokens": ["Des", "Na\u00b7ckens", "Blond\u00b7ge\u00b7kraus", ",", "wer", "kann's", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch j\u00fcngst \u2014 ich schritt ein hohes Thal entlang,", "tokens": ["Noch", "j\u00fcngst", "ich", "schritt", "ein", "ho\u00b7hes", "Thal", "ent\u00b7lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es war gekr\u00f6nt mit sieben Silberspitzen,", "tokens": ["Es", "war", "ge\u00b7kr\u00f6nt", "mit", "sie\u00b7ben", "Sil\u00b7ber\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die von dem himmelnahen Felsenhang", "tokens": ["Die", "von", "dem", "him\u00b7mel\u00b7na\u00b7hen", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Herunter auf die gr\u00fcnen Pfade blitzen \u2014", "tokens": ["Her\u00b7un\u00b7ter", "auf", "die", "gr\u00fc\u00b7nen", "Pfa\u00b7de", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201eschneewittchen!\u201c rief ich laut und unbewu\u00dft,", "tokens": ["\u201e", "schnee\u00b7witt\u00b7chen", "!", "\u201c", "rief", "ich", "laut", "und", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "$(", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201eschneewittchen! hinter deinen sieben Bergen", "tokens": ["\u201e", "schnee\u00b7witt\u00b7chen", "!", "hin\u00b7ter", "dei\u00b7nen", "sie\u00b7ben", "Ber\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVINF", "$.", "APPR", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchrst droben p\u00fcnktlich du mit k\u00fchler Brust", "tokens": ["F\u00fchrst", "dro\u00b7ben", "p\u00fcnkt\u00b7lich", "du", "mit", "k\u00fch\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den kleinen Haushalt deinen sieben Zwergen?\u201c", "tokens": ["Den", "klei\u00b7nen", "Haus\u00b7halt", "dei\u00b7nen", "sie\u00b7ben", "Zwer\u00b7gen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein spottend Echo nur antwortet' mir,", "tokens": ["Ein", "spot\u00b7tend", "E\u00b7cho", "nur", "ant\u00b7wor\u00b7tet'", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Felsstirn r\u00fcmpfte lachend ihre Falten", "tokens": ["Die", "Felss\u00b7tirn", "r\u00fcmpf\u00b7te", "la\u00b7chend", "ih\u00b7re", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch, und doch, mir war's, ich h\u00e4tt' von dir,", "tokens": ["Und", "doch", ",", "und", "doch", ",", "mir", "wa\u00b7r's", ",", "ich", "h\u00e4tt'", "von", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$,", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Schneewittchen! einen lieben Gru\u00df erhalten.", "tokens": ["Schnee\u00b7witt\u00b7chen", "!", "ei\u00b7nen", "lie\u00b7ben", "Gru\u00df", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}