{"dta.poem.23661": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "herzenerhebenden,", "tokens": ["her\u00b7ze\u00b7ner\u00b7he\u00b7ben\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "D\u00fcstres und Klares, Sch\u00f6nes und Wahres", "tokens": ["D\u00fcst\u00b7res", "und", "Kla\u00b7res", ",", "Sch\u00f6\u00b7nes", "und", "Wah\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "ADJA", "KON", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "sinnig verwebenden,", "tokens": ["sin\u00b7nig", "ver\u00b7we\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Erd' und Himmel minnig umschwebenden,", "tokens": ["Erd'", "und", "Him\u00b7mel", "min\u00b7nig", "um\u00b7schwe\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "die da trachten und dichten,", "tokens": ["die", "da", "trach\u00b7ten", "und", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "KON", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "das Dunkle zu lichten,", "tokens": ["das", "Dunk\u00b7le", "zu", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "das Gebeugte zu richten,", "tokens": ["das", "Ge\u00b7beug\u00b7te", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "das Verworrne zu schlichten;", "tokens": ["das", "Ver\u00b7worr\u00b7ne", "zu", "schlich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Die aus der Erde Banden und Schlingen", "tokens": ["Die", "aus", "der", "Er\u00b7de", "Ban\u00b7den", "und", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sich frei mit der Lerche gen Himmel schwingen,", "tokens": ["Sich", "frei", "mit", "der", "Ler\u00b7che", "gen", "Him\u00b7mel", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und unbek\u00fcmmert um diesen und jenen", "tokens": ["Und", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "um", "die\u00b7sen", "und", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PDAT", "KON", "PDS"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Fr\u00f6hlich singen ihr Lieben und Sehnen,", "tokens": ["Fr\u00f6h\u00b7lich", "sin\u00b7gen", "ihr", "Lie\u00b7ben", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Und nicht aus Pf\u00fctzen und Lachen schl\u00fcrfen,", "tokens": ["Und", "nicht", "aus", "Pf\u00fct\u00b7zen", "und", "La\u00b7chen", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und keiner undeutschen Quelle bed\u00fcrfen,", "tokens": ["Und", "kei\u00b7ner", "un\u00b7deut\u00b7schen", "Quel\u00b7le", "be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und nach keinen fremden G\u00e4ngen schl\u00fcrfen \u2014", "tokens": ["Und", "nach", "kei\u00b7nen", "frem\u00b7den", "G\u00e4n\u00b7gen", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Sondern am heimischen Born sich laben", "tokens": ["Son\u00b7dern", "am", "hei\u00b7mi\u00b7schen", "Born", "sich", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "PRF", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Und in ihrem eigenen Herzen graben,", "tokens": ["Und", "in", "ih\u00b7rem", "ei\u00b7ge\u00b7nen", "Her\u00b7zen", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Weil sie selbst den Schacht im Herzen haben;", "tokens": ["Weil", "sie", "selbst", "den", "Schacht", "im", "Her\u00b7zen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Die wie der Fr\u00fchling Bl\u00fcthen entfalten", "tokens": ["Die", "wie", "der", "Fr\u00fch\u00b7ling", "Bl\u00fc\u00b7then", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und wie der Fr\u00fchling niemals alten,", "tokens": ["Und", "wie", "der", "Fr\u00fch\u00b7ling", "nie\u00b7mals", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und auf die ganze Welt verzichten,", "tokens": ["Und", "auf", "die", "gan\u00b7ze", "Welt", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Weil sie nicht um Ruhm und Geld dichten.", "tokens": ["Weil", "sie", "nicht", "um", "Ruhm", "und", "Geld", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}