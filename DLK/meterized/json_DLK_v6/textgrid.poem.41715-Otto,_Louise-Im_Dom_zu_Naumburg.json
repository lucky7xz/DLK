{"textgrid.poem.41715": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Im Dom zu Naumburg", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbden obdachlosen Ritter, den armen mag ich nicht,", "tokens": ["\u00bb", "den", "ob\u00b7dach\u00b7lo\u00b7sen", "Rit\u00b7ter", ",", "den", "ar\u00b7men", "mag", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ich mag nicht Minnedienste, mich bindet h\u00f6hre Pflicht", "tokens": ["Ich", "mag", "nicht", "Min\u00b7ne\u00b7diens\u00b7te", ",", "mich", "bin\u00b7det", "h\u00f6h\u00b7re", "Pflicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr, ohne Ruhm und Sch\u00e4tze wagt doch um mich zu freien?", "tokens": ["Ihr", ",", "oh\u00b7ne", "Ruhm", "und", "Sch\u00e4t\u00b7ze", "wagt", "doch", "um", "mich", "zu", "frei\u00b7en", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "NN", "KON", "NN", "VVFIN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nehmt das zur\u00fcck, sonst m\u00f6cht ich Euch noch des Irrsinns zeihen!\u00ab", "tokens": ["Nehmt", "das", "zu\u00b7r\u00fcck", ",", "sonst", "m\u00f6cht", "ich", "Euch", "noch", "des", "Irr\u00b7sinns", "zei\u00b7hen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PDS", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "So sprach mit Hohn, das Fr\u00e4ulein zum armen Rittersmann,", "tokens": ["So", "sprach", "mit", "Hohn", ",", "das", "Fr\u00e4u\u00b7lein", "zum", "ar\u00b7men", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df nur mit M\u00fch er Fassung bei solchem Wort gewann,", "tokens": ["Da\u00df", "nur", "mit", "M\u00fch", "er", "Fas\u00b7sung", "bei", "sol\u00b7chem", "Wort", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PPER", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er neigt sich vor der Kalten, die er so hei\u00df verehrt", "tokens": ["Er", "neigt", "sich", "vor", "der", "Kal\u00b7ten", ",", "die", "er", "so", "hei\u00df", "ver\u00b7ehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und die mit schn\u00f6dem Abschied sich also von ihm kehrt.", "tokens": ["Und", "die", "mit", "schn\u00f6\u00b7dem", "Ab\u00b7schied", "sich", "al\u00b7so", "von", "ihm", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf ihres Schlosses Zinne das stolze Fr\u00e4ulein stand,", "tokens": ["Auf", "ih\u00b7res", "Schlos\u00b7ses", "Zin\u00b7ne", "das", "stol\u00b7ze", "Fr\u00e4u\u00b7lein", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hielt einen Specht gefangen in ihrer kleinen Hand.", "tokens": ["Hielt", "ei\u00b7nen", "Specht", "ge\u00b7fan\u00b7gen", "in", "ih\u00b7rer", "klei\u00b7nen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbdu, V\u00f6glein, sollst es mir sagen, Dir will ich's anvertrauen,", "tokens": ["\u00bb", "du", ",", "V\u00f6\u00b7glein", ",", "sollst", "es", "mir", "sa\u00b7gen", ",", "Dir", "will", "ich's", "an\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo ich von meinen Sch\u00e4tzen den Tempel des Herrn soll bauen?\u00ab", "tokens": ["Wo", "ich", "von", "mei\u00b7nen", "Sch\u00e4t\u00b7zen", "den", "Tem\u00b7pel", "des", "Herrn", "soll", "bau\u00b7en", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Specht begann zu kreisen bis da\u00df zur Stell'er kam,", "tokens": ["Der", "Specht", "be\u00b7gann", "zu", "krei\u00b7sen", "bis", "da\u00df", "zur", "Stell'\u00b7er", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo zwischen hohen Linden er seinen Platz sich nahm.\u2013", "tokens": ["Wo", "zwi\u00b7schen", "ho\u00b7hen", "Lin\u00b7den", "er", "sei\u00b7nen", "Platz", "sich", "nahm", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NE", "PPER", "PPOSAT", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drauf sah man dort die Bauleut gesch\u00e4ftig von fr\u00fch bis n\u00e4chten,", "tokens": ["Drauf", "sah", "man", "dort", "die", "Bau\u00b7leut", "ge\u00b7sch\u00e4f\u00b7tig", "von", "fr\u00fch", "bis", "n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "ART", "NN", "ADJD", "APPR", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Damit sie bald das Bauwerk, des herrlichen Doms vollbr\u00e4chten.", "tokens": ["Da\u00b7mit", "sie", "bald", "das", "Bau\u00b7werk", ",", "des", "herr\u00b7li\u00b7chen", "Doms", "voll\u00b7br\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Hei, wie die Gruftgew\u00f6lbe erstanden meisterlich,", "tokens": ["Hei", ",", "wie", "die", "Gruft\u00b7ge\u00b7w\u00f6l\u00b7be", "er\u00b7stan\u00b7den", "meis\u00b7ter\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie k\u00fchne Strebepfeiler zur H\u00f6he schwingten sich,", "tokens": ["Wie", "k\u00fch\u00b7ne", "Stre\u00b7be\u00b7pfei\u00b7ler", "zur", "H\u00f6\u00b7he", "schwing\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie sch\u00f6n die Marmorplatten und dreingehaune Bilder,", "tokens": ["Wie", "sch\u00f6n", "die", "Mar\u00b7mor\u00b7plat\u00b7ten", "und", "drein\u00b7ge\u00b7hau\u00b7ne", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie prangten die Bogenfenster und buntgemalte Schilder! \u2013", "tokens": ["Wie", "prang\u00b7ten", "die", "Bo\u00b7gen\u00b7fens\u00b7ter", "und", "bunt\u00b7ge\u00b7mal\u00b7te", "Schil\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch war des Fr\u00e4uleins Reichtum gar schnell zu Ende nun,", "tokens": ["Doch", "war", "des", "Fr\u00e4u\u00b7leins", "Reich\u00b7tum", "gar", "schnell", "zu", "En\u00b7de", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NE", "ADV", "ADJD", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und die gesch\u00e4ft'gen H\u00e4nde, sie mu\u00dften alle ruhn.", "tokens": ["Und", "die", "ge\u00b7sch\u00e4ft'\u00b7gen", "H\u00e4n\u00b7de", ",", "sie", "mu\u00df\u00b7ten", "al\u00b7le", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie war der Stolzen Sch\u00f6nheit durch drei\u00dfig Jahr geschwunden \u2013", "tokens": ["Wie", "war", "der", "Stol\u00b7zen", "Sch\u00f6n\u00b7heit", "durch", "drei\u00b7\u00dfig", "Jahr", "ge\u00b7schwun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "APPR", "CARD", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbich kann den Dom nicht vollenden!\u00ab seufzt sie in bangen Stunden.", "tokens": ["\u00bb", "ich", "kann", "den", "Dom", "nicht", "voll\u00b7en\u00b7den", "!", "\u00ab", "seufzt", "sie", "in", "ban\u00b7gen", "Stun\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$.", "$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Da trat ein fremder Pilger urpl\u00f6tzlich vor sie hin,", "tokens": ["Da", "trat", "ein", "frem\u00b7der", "Pil\u00b7ger", "ur\u00b7pl\u00f6tz\u00b7lich", "vor", "sie", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er kam vom heiligen Grabe und kam mit gl\u00e4ubigem Sinn.", "tokens": ["Er", "kam", "vom", "hei\u00b7li\u00b7gen", "Gra\u00b7be", "und", "kam", "mit", "gl\u00e4u\u00b7bi\u00b7gem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das war der erst verh\u00f6hnte der armgescholt'ne Freier,", "tokens": ["Das", "war", "der", "erst", "ver\u00b7h\u00f6hn\u00b7te", "der", "arm\u00b7ge\u00b7scholt'\u00b7ne", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der sprach: \u00bbReich komm ich wieder zu Eurer Tempelfeier.\u00ab", "tokens": ["Der", "sprach", ":", "\u00bb", "Reich", "komm", "ich", "wie\u00b7der", "zu", "Eu\u00b7rer", "Tem\u00b7pel\u00b7fei\u00b7er", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbin dem gelobten Lande fand Ruhm und Sch\u00e4tze ich viel,", "tokens": ["\u00bb", "in", "dem", "ge\u00b7lob\u00b7ten", "Lan\u00b7de", "fand", "Ruhm", "und", "Sch\u00e4t\u00b7ze", "ich", "viel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So will denn ich vollenden, was Euer stolzes Ziel,", "tokens": ["So", "will", "denn", "ich", "voll\u00b7en\u00b7den", ",", "was", "Eu\u00b7er", "stol\u00b7zes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KON", "PPER", "VVINF", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So wachsen diese Hallen uns zum Vers\u00f6hnungszeichen!\u00ab", "tokens": ["So", "wach\u00b7sen", "die\u00b7se", "Hal\u00b7len", "uns", "zum", "Ver\u00b7s\u00f6h\u00b7nungs\u00b7zei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Besch\u00e4mung auf dem Antlitz th\u00e4t sie die Hand ihm reichen. \u2013", "tokens": ["Be\u00b7sch\u00e4\u00b7mung", "auf", "dem", "Ant\u00b7litz", "th\u00e4t", "sie", "die", "Hand", "ihm", "rei\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Das ist die alte M\u00e4re, die man vom Dome sagt", "tokens": ["Das", "ist", "die", "al\u00b7te", "M\u00e4\u00b7re", ",", "die", "man", "vom", "Do\u00b7me", "sagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo Trotz und Stolz gewaltet und Liebe nicht verzagt.", "tokens": ["Wo", "Trotz", "und", "Stolz", "ge\u00b7wal\u00b7tet", "und", "Lie\u00b7be", "nicht", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVPP", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob wohl auch jetzt da drinnen manch frevler Stolz noch waltet", "tokens": ["Ob", "wohl", "auch", "jetzt", "da", "drin\u00b7nen", "manch", "frev\u00b7ler", "Stolz", "noch", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ADV", "ADV", "PIAT", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Liebe ihre Schwingen noch also treu entfaltet?", "tokens": ["Und", "Lie\u00b7be", "ih\u00b7re", "Schwin\u00b7gen", "noch", "al\u00b7so", "treu", "ent\u00b7fal\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Es stehn in Stein gehauen Fr\u00e4ulein und Ritter da,", "tokens": ["Es", "stehn", "in", "Stein", "ge\u00b7hau\u00b7en", "Fr\u00e4u\u00b7lein", "und", "Rit\u00b7ter", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es sitzt der Specht 