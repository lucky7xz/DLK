{"textgrid.poem.43265": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gelbes Kleid! und Edelsteine!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gelbes Kleid! und Edelsteine!", "tokens": ["Ein", "gel\u00b7bes", "Kleid", "!", "und", "E\u00b7del\u00b7stei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ei, die ist spa\u00dfhaft hergeputzt!", "tokens": ["Ei", ",", "die", "ist", "spa\u00df\u00b7haft", "her\u00b7ge\u00b7putzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch Augen hat sie wie nur Eine,", "tokens": ["Doch", "Au\u00b7gen", "hat", "sie", "wie", "nur", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "KOKOM", "ADV", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcbsch wenn sie lacht, h\u00fcbsch wenn sie trutzt.", "tokens": ["H\u00fcbsch", "wenn", "sie", "lacht", ",", "h\u00fcbsch", "wenn", "sie", "trutzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Federn strotzt ihr Hut ihr feiner,", "tokens": ["Von", "Fe\u00b7dern", "strotzt", "ihr", "Hut", "ihr", "fei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lorgnon und F\u00e4cher tr\u00e4gt sie gar,", "tokens": ["Lorg\u00b7non", "und", "F\u00e4\u00b7cher", "tr\u00e4gt", "sie", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Handschuh macht die H\u00e4nde kleiner", "tokens": ["Kein", "Hand\u00b7schuh", "macht", "die", "H\u00e4n\u00b7de", "klei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kuhmagd, die sie fr\u00fcher war.", "tokens": ["Der", "Kuh\u00b7magd", ",", "die", "sie", "fr\u00fc\u00b7her", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch lustig ist das Ding geblieben,", "tokens": ["Auch", "lus\u00b7tig", "ist", "das", "Ding", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So kindisch-eitel, schwatzhaft-froh,", "tokens": ["So", "kin\u00b7dischei\u00b7tel", ",", "schwatz\u00b7haft\u00b7froh", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Trotzdem es sich herumgetrieben", "tokens": ["Trotz\u00b7dem", "es", "sich", "her\u00b7um\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenlos und herzensroh", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "und", "her\u00b7zens\u00b7roh"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch Eines hat sie gut begriffen", "tokens": ["Doch", "Ei\u00b7nes", "hat", "sie", "gut", "be\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und es den St\u00e4dtern nachgethan:", "tokens": ["Und", "es", "den", "St\u00e4d\u00b7tern", "nach\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fing mit dummen Weiberkniffen", "tokens": ["Sie", "fing", "mit", "dum\u00b7men", "Wei\u00b7berk\u00b7nif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich einen klugen, reichen Mann.", "tokens": ["Sich", "ei\u00b7nen", "klu\u00b7gen", ",", "rei\u00b7chen", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}