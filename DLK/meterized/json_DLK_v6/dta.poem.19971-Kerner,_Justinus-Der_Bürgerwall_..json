{"dta.poem.19971": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der B\u00fcrgerwall .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ritterthum kann nimmer hei\u00dfen", "tokens": ["Rit\u00b7ter\u00b7thum", "kann", "nim\u00b7mer", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sichrer Wall um's K\u00f6nigshaus,", "tokens": ["Sich\u00b7rer", "Wall", "um's", "K\u00f6\u00b7nigs\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit ihr Kleid von Stahl und Eisen", "tokens": ["Seit", "ihr", "Kleid", "von", "Stahl", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zogen alle Ritter aus.", "tokens": ["Zo\u00b7gen", "al\u00b7le", "Rit\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seit sie tragen mit Behagen", "tokens": ["Seit", "sie", "tra\u00b7gen", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00fcssel an der Schwerterstatt,", "tokens": ["Schl\u00fcs\u00b7sel", "an", "der", "Schwer\u00b7ter\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit sie mit der Feder wagen", "tokens": ["Seit", "sie", "mit", "der", "Fe\u00b7der", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in's Feld, in's Zeitungsblatt.", "tokens": ["Sich", "in's", "Feld", ",", "in's", "Zei\u00b7tungs\u00b7blatt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seit statt fester Burgeshallen,", "tokens": ["Seit", "statt", "fes\u00b7ter", "Bur\u00b7ges\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6lzern steht im Thal ihr Haus,", "tokens": ["H\u00f6l\u00b7zern", "steht", "im", "Thal", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit sie leicht und luftig wallen,", "tokens": ["Seit", "sie", "leicht", "und", "luf\u00b7tig", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es mit den Rittern aus.", "tokens": ["Ist", "es", "mit", "den", "Rit\u00b7tern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was noch scheint, ist Gl\u00fchwurms Schimmer", "tokens": ["Was", "noch", "scheint", ",", "ist", "Gl\u00fch\u00b7wurms", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In verwittert' Stein und Moos.", "tokens": ["In", "ver\u00b7wit\u00b7tert'", "Stein", "und", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Wall, der liegt in Tr\u00fcmmer,", "tokens": ["Je\u00b7ner", "Wall", ",", "der", "liegt", "in", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ein andrer w\u00f6lbt sich gro\u00df.", "tokens": ["Doch", "ein", "an\u00b7drer", "w\u00f6lbt", "sich", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "B\u00fcrgerthum ist der gehei\u00dfen,", "tokens": ["B\u00fcr\u00b7ger\u00b7thum", "ist", "der", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft sich fest um's K\u00f6nigshaus. \u2014", "tokens": ["Schlie\u00dft", "sich", "fest", "um's", "K\u00f6\u00b7nigs\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heil! in solchem Wall von Eisen", "tokens": ["Heil", "!", "in", "sol\u00b7chem", "Wall", "von", "Ei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt es jeden Donner aus.", "tokens": ["H\u00e4lt", "es", "je\u00b7den", "Don\u00b7ner", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}