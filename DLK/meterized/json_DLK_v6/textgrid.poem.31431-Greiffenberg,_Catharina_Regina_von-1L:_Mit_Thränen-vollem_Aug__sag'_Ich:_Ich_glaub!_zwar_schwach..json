{"textgrid.poem.31431": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Thr\u00e4nen-vollem Aug / sag' Ich: Ich glaub! zwar schwach.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Thr\u00e4nen-vollem Aug / sag' Ich: Ich glaub! zwar schwach.", "tokens": ["Mit", "Thr\u00e4\u00b7nen\u00b7vol\u00b7lem", "Aug", "/", "sag'", "Ich", ":", "Ich", "glaub", "!", "zwar", "schwach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach hilff mir starker Gott! dein' Ehr wird mehr erscheinen /", "tokens": ["Ach", "hilff", "mir", "star\u00b7ker", "Gott", "!", "dein'", "Ehr", "wird", "mehr", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wann du mit deiner Krafft beherrlichest die kleinen.", "tokens": ["wann", "du", "mit", "dei\u00b7ner", "Krafft", "be\u00b7herr\u00b7li\u00b7chest", "die", "klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch schwache / wunder thun / ist eine Gottes Sach.", "tokens": ["Durch", "schwa\u00b7che", "/", "wun\u00b7der", "thun", "/", "ist", "ei\u00b7ne", "Got\u00b7tes", "Sach", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$(", "NN", "VVINF", "$(", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den hellen blitzen folgt / ein starker Donner / nach:", "tokens": ["Den", "hel\u00b7len", "blit\u00b7zen", "folgt", "/", "ein", "star\u00b7ker", "Don\u00b7ner", "/", "nach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VVFIN", "$(", "ART", "ADJA", "NN", "$(", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "von sanfften L\u00fcfftlein kan man kein gewalt vermeinen.", "tokens": ["von", "sanff\u00b7ten", "L\u00fcff\u00b7tlein", "kan", "man", "kein", "ge\u00b7walt", "ver\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dich einig r\u00fchmend / mu\u00df man Menschen Krafft verneinen /", "tokens": ["Dich", "ei\u00b7nig", "r\u00fch\u00b7mend", "/", "mu\u00df", "man", "Men\u00b7schen", "Krafft", "ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$(", "VMFIN", "PIS", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "bekennen auch da\u00df flie\u00df vom ursprung dieser Bach.", "tokens": ["be\u00b7ken\u00b7nen", "auch", "da\u00df", "flie\u00df", "vom", "ur\u00b7sprung", "die\u00b7ser", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "VVFIN", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach H\u00f6chster! deine Krafft wird ja so klar sich zeigen", "tokens": ["Ach", "H\u00f6chs\u00b7ter", "!", "dei\u00b7ne", "Krafft", "wird", "ja", "so", "klar", "sich", "zei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "in meiner nichtigkeit / wie ein sch\u00f6n Angesicht", "tokens": ["in", "mei\u00b7ner", "nich\u00b7tig\u00b7keit", "/", "wie", "ein", "sch\u00f6n", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "eim klaren brunnen weist sein Ros-und Lilgen-Liecht.", "tokens": ["eim", "kla\u00b7ren", "brun\u00b7nen", "weist", "sein", "Ros\u00b7\u00b7und", "Lil\u00b7gen\u00b7Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es ist ja nicht die quell / der solche sch\u00f6nheit eigen!", "tokens": ["Es", "ist", "ja", "nicht", "die", "quell", "/", "der", "sol\u00b7che", "sch\u00f6n\u00b7heit", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "$(", "ART", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "sie w\u00fcrcket nichts / als da\u00df sie rein und stille bleibt.", "tokens": ["sie", "w\u00fcr\u00b7cket", "nichts", "/", "als", "da\u00df", "sie", "rein", "und", "stil\u00b7le", "bleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "KOKOM", "KOUS", "PPER", "ADJD", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Also mein Glaub / wann Gott sein werk in solchem treibt.", "tokens": ["Al\u00b7so", "mein", "Glaub", "/", "wann", "Gott", "sein", "werk", "in", "sol\u00b7chem", "treibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "PWAV", "NN", "PPOSAT", "NN", "APPR", "PIAT", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}