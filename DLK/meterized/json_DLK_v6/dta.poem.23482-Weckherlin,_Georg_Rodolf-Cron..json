{"dta.poem.23482": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dvrch meiner stim vnverg\u00e4nglichen", "tokens": ["Dvrch", "mei\u00b7ner", "stim", "vn\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "pracht", "tokens": ["pracht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mu\u00df ich seine M\u00e4\u00dfigkeit preisen/", "tokens": ["Mu\u00df", "ich", "sei\u00b7ne", "M\u00e4\u00b7\u00dfig\u00b7keit", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die als sein Raht vn\u0303 Doctor tag vn\u0303 nache", "tokens": ["Die", "als", "sein", "Raht", "v\u00f1", "Doc\u00b7tor", "tag", "v\u00f1", "na\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "PPOSAT", "NN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Th\u00e4t seine begird vnderweisen.", "tokens": ["Th\u00e4t", "sei\u00b7ne", "be\u00b7gird", "vn\u00b7der\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Sie h\u00e4lt den zaum mit so gewi\u00dfer hand", "tokens": ["Sie", "h\u00e4lt", "den", "zaum", "mit", "so", "ge\u00b7wi\u00b7\u00dfer", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In seiner sehl/ das in allem zustand", "tokens": ["In", "sei\u00b7ner", "sehl", "/", "das", "in", "al\u00b7lem", "zu\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "APPR", "PIS", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sein gaist/ geberden vnd gedancken", "tokens": ["Sein", "gaist", "/", "ge\u00b7ber\u00b7den", "vnd", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVPP", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Niemahl vnder einander schwancken:", "tokens": ["Nie\u00b7mahl", "vn\u00b7der", "ein\u00b7an\u00b7der", "schwan\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Sielasst keine vnordnung zu/", "tokens": ["Sie\u00b7lasst", "kei\u00b7ne", "vnord\u00b7nung", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Zu-wenig vnd Zu-vil in jhm nicht krieget/", "tokens": ["Zu\u00b7we\u00b7nig", "vnd", "Zu\u00b7vil", "in", "jhm", "nicht", "krie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie macht in Jhm ein solche ruh/", "tokens": ["Sie", "macht", "in", "Jhm", "ein", "sol\u00b7che", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das die Vernunft allen willen vern\u00fceget:", "tokens": ["Das", "die", "Ver\u00b7nunft", "al\u00b7len", "wil\u00b7len", "ver\u00b7n\u00fce\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Sie macht das seiner thaten grund", "tokens": ["Sie", "macht", "das", "sei\u00b7ner", "tha\u00b7ten", "grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht auf des Volcks meinung vnd lob be-", "tokens": ["Nicht", "auf", "des", "Volcks", "mei\u00b7nung", "vnd", "lob", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "KON", "NN", "TRUNC"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "stehet;", "tokens": ["ste\u00b7het", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Vnd das sein red vnd thun gleich gehet/", "tokens": ["Vnd", "das", "sein", "red", "vnd", "thun", "gleich", "ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPOSAT", "VVFIN", "KON", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Damit steif bleib seiner Tugenten bund.", "tokens": ["Da\u00b7mit", "steif", "bleib", "sei\u00b7ner", "Tu\u00b7gen\u00b7ten", "bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}}}}