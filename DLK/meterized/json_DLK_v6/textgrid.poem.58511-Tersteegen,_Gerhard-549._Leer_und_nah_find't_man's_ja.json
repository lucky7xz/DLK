{"textgrid.poem.58511": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "549. Leer und nah find't man's ja", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, kannst du ansehn ohne Weinen", "tokens": ["Mensch", ",", "kannst", "du", "an\u00b7sehn", "oh\u00b7ne", "Wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PPER", "VVINF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein's Geistes Sterben, dieses Einen?", "tokens": ["Dein's", "Geis\u00b7tes", "Ster\u00b7ben", ",", "die\u00b7ses", "Ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Glaub's, Gottes Brunn hat Wassers g'nug!", "tokens": ["Glaub's", ",", "Got\u00b7tes", "Brunn", "hat", "Was\u00b7sers", "g'\u00b7nug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Geschaffner Trost ist bald zerronnen;", "tokens": ["Ge\u00b7schaff\u00b7ner", "Trost", "ist", "bald", "zer\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als Hagar nichts mehr fand im Krug,", "tokens": ["Als", "Ha\u00b7gar", "nichts", "mehr", "fand", "im", "Krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da fand sie erst den nahen Brunnen.", "tokens": ["Da", "fand", "sie", "erst", "den", "na\u00b7hen", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}