{"textgrid.poem.36813": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Tage, da St. Petrus einst in Rom", "tokens": ["Am", "Ta\u00b7ge", ",", "da", "St.", "Pet\u00b7rus", "einst", "in", "Rom"], "token_info": ["word", "word", "punct", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NE", "NE", "ADV", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den heil'gen Stuhl der Christenheit bestiegen,", "tokens": ["Den", "heil'\u00b7gen", "Stuhl", "der", "Chris\u00b7ten\u00b7heit", "be\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht man das Volk in seinem Riesendom", "tokens": ["Sieht", "man", "das", "Volk", "in", "sei\u00b7nem", "Rie\u00b7sen\u00b7dom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vorm heil'gen Vater auf den Knieen liegen.", "tokens": ["Vorm", "heil'\u00b7gen", "Va\u00b7ter", "auf", "den", "Kni\u00b7e\u00b7en", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und wie sie alle gl\u00e4ubig oder nicht", "tokens": ["Und", "wie", "sie", "al\u00b7le", "gl\u00e4u\u00b7big", "o\u00b7der", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PIS", "ADJD", "KON", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von allen Enden zu dem Fest erschienen,", "tokens": ["Von", "al\u00b7len", "En\u00b7den", "zu", "dem", "Fest", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da als der Glocke m\u00e4chtiges Gewicht", "tokens": ["Da", "als", "der", "Glo\u00b7cke", "m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vom Schlag erklang, so kam auch ich mit ihnen \u2013", "tokens": ["Vom", "Schlag", "er\u00b7klang", ",", "so", "kam", "auch", "ich", "mit", "ih\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und als die Feier nun vollendet war,", "tokens": ["Und", "als", "die", "Fei\u00b7er", "nun", "voll\u00b7en\u00b7det", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sa\u00df ich noch lange stumm an einer S\u00e4ule,", "tokens": ["Sa\u00df", "ich", "noch", "lan\u00b7ge", "stumm", "an", "ei\u00b7ner", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich dachte manches mir, und wunderbar", "tokens": ["Ich", "dach\u00b7te", "man\u00b7ches", "mir", ",", "und", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PPER", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auch die Vergangenheit in stiller Weile.", "tokens": ["Auch", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "in", "stil\u00b7ler", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn hinter deinen stolzen Pinienhain", "tokens": ["Wenn", "hin\u00b7ter", "dei\u00b7nen", "stol\u00b7zen", "Pi\u00b7ni\u00b7en\u00b7hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Die Sonne sinkt in ihren s\u00fc\u00dfen Gluthen,", "tokens": ["Die", "Son\u00b7ne", "sinkt", "in", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Gianicolo, wie da im Abendschein", "tokens": ["Gi\u00b7a\u00b7ni\u00b7co\u00b7lo", ",", "wie", "da", "im", "A\u00b7bend\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Die Wolken trunken sind von goldnen Fluthen,", "tokens": ["Die", "Wol\u00b7ken", "trun\u00b7ken", "sind", "von", "gold\u00b7nen", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ja, wie das Meer, wenn's auch die Klipp' umsch\u00e4umt,", "tokens": ["Ja", ",", "wie", "das", "Meer", ",", "wenn's", "auch", "die", "Klipp'", "um\u00b7sch\u00e4umt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die Fl\u00e4che hin voll immer z\u00e4rt'rer T\u00f6ne,", "tokens": ["Die", "Fl\u00e4\u00b7che", "hin", "voll", "im\u00b7mer", "z\u00e4rt'\u00b7rer", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Von dieses Himmels reinem Licht bes\u00e4umt,", "tokens": ["Von", "die\u00b7ses", "Him\u00b7mels", "rei\u00b7nem", "Licht", "be\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Doch gl\u00e4nzt in unaussprechlich hoher Sch\u00f6ne,", "tokens": ["Doch", "gl\u00e4nzt", "in", "un\u00b7aus\u00b7sprech\u00b7lich", "ho\u00b7her", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "So sanft im Sonnenschein des Augenblicks", "tokens": ["So", "sanft", "im", "Son\u00b7nen\u00b7schein", "des", "Au\u00b7gen\u00b7blicks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ergl\u00fchten alle Schatten meines Lebens,", "tokens": ["Er\u00b7gl\u00fch\u00b7ten", "al\u00b7le", "Schat\u00b7ten", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und selbst dem dunkeln Abgrund des Geschicks", "tokens": ["Und", "selbst", "dem", "dun\u00b7keln", "Ab\u00b7grund", "des", "Ge\u00b7schicks"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Entdrohten alle Str\u00f6mungen vergebens.", "tokens": ["Ent\u00b7droh\u00b7ten", "al\u00b7le", "Str\u00f6\u00b7mun\u00b7gen", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Dem Tantalus glich einst die Herzensqual,", "tokens": ["Dem", "Tan\u00b7ta\u00b7lus", "glich", "einst", "die", "Her\u00b7zens\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Die mir die Tage nahm, die N\u00e4chte raubte,", "tokens": ["Die", "mir", "die", "Ta\u00b7ge", "nahm", ",", "die", "N\u00e4ch\u00b7te", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dem alten Halbgott, der das Feuer stahl,", "tokens": ["Dem", "al\u00b7ten", "Halb\u00b7gott", ",", "der", "das", "Feu\u00b7er", "stahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und das Geschlecht nur zu begl\u00fccken glaubte.", "tokens": ["Und", "das", "Ge\u00b7schlecht", "nur", "zu", "be\u00b7gl\u00fc\u00b7cken", "glaub\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Fern vom Lebend'gen, in der Schattenwelt", "tokens": ["Fern", "vom", "Le\u00b7ben\u00b7d'\u00b7gen", ",", "in", "der", "Schat\u00b7ten\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "Stand ich verwaist in grenzenloser Leere,", "tokens": ["Stand", "ich", "ver\u00b7waist", "in", "gren\u00b7zen\u00b7lo\u00b7ser", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Die Brust vom hei\u00dfen Wissensdurst geschwellt,", "tokens": ["Die", "Brust", "vom", "hei\u00b7\u00dfen", "Wis\u00b7sens\u00b7durst", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Von Sehnsucht nach Verdienst und Ruhm und Ehre.", "tokens": ["Von", "Sehn\u00b7sucht", "nach", "Ver\u00b7dienst", "und", "Ruhm", "und", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Es winkte mir des Lebens goldne Frucht,", "tokens": ["Es", "wink\u00b7te", "mir", "des", "Le\u00b7bens", "gold\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und doch entschwang der Zweig sich meinen Lippen,", "tokens": ["Und", "doch", "ent\u00b7schwang", "der", "Zweig", "sich", "mei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und mitten in der Fluth war ich verflucht,", "tokens": ["Und", "mit\u00b7ten", "in", "der", "Fluth", "war", "ich", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "In Tropfen nur den k\u00fchlen Trunk zu nippen.", "tokens": ["In", "Trop\u00b7fen", "nur", "den", "k\u00fch\u00b7len", "Trunk", "zu", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und meine Schuld? Ach da\u00df in k\u00fchnerm Drang", "tokens": ["Und", "mei\u00b7ne", "Schuld", "?", "Ach", "da\u00df", "in", "k\u00fch\u00b7nerm", "Drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$.", "NN", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Nach h\u00f6hern Dingen und nach gr\u00f6\u00dfern Thaten", "tokens": ["Nach", "h\u00f6\u00b7hern", "Din\u00b7gen", "und", "nach", "gr\u00f6\u00b7\u00dfern", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Mein Mund oft im begeisterten Gesang", "tokens": ["Mein", "Mund", "oft", "im", "be\u00b7geis\u00b7ter\u00b7ten", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Aus dem Olymp Geheimnisse verrathen.", "tokens": ["Aus", "dem", "O\u00b7lymp", "Ge\u00b7heim\u00b7nis\u00b7se", "ver\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Und als in reichem Fr\u00fchling mein Gem\u00fcth", "tokens": ["Und", "als", "in", "rei\u00b7chem", "Fr\u00fch\u00b7ling", "mein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Die jungen frischen Augen aufgeschlossen,", "tokens": ["Die", "jun\u00b7gen", "fri\u00b7schen", "Au\u00b7gen", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "In ungeme\u00dfner Liebe nun erbl\u00fcht,", "tokens": ["In", "un\u00b7ge\u00b7me\u00df\u00b7ner", "Lie\u00b7be", "nun", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Den h\u00f6chsten Schmerz, die h\u00f6chste Lust genossen,", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Schmerz", ",", "die", "h\u00f6chs\u00b7te", "Lust", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Da kn\u00fcpft' ich th\u00f6richt an der Bl\u00fcthe Saft", "tokens": ["Da", "kn\u00fcpft'", "ich", "th\u00f6\u00b7richt", "an", "der", "Bl\u00fc\u00b7the", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Die sel'ge Hoffnung eines ew'gen Segens,", "tokens": ["Die", "sel'\u00b7ge", "Hoff\u00b7nung", "ei\u00b7nes", "ew'\u00b7gen", "Se\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Bald starb die sch\u00f6ne Wirkung mit der Kraft,", "tokens": ["Bald", "starb", "die", "sch\u00f6\u00b7ne", "Wir\u00b7kung", "mit", "der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Die Blume mit dem Keim des frohen Regens.", "tokens": ["Die", "Blu\u00b7me", "mit", "dem", "Keim", "des", "fro\u00b7hen", "Re\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Der Schlange glich ich nun, die halb zerst\u00fcc