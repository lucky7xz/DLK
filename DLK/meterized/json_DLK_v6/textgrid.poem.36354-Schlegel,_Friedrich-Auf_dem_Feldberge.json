{"textgrid.poem.36354": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Feldberge", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie still ist es hier oben,", "tokens": ["Wie", "still", "ist", "es", "hier", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo kein Geb\u00fcsch mehr bl\u00fcht,", "tokens": ["Wo", "kein", "Ge\u00b7b\u00fcsch", "mehr", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00dcber die Berg' erhoben,", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7g'", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wo nie der Sommer gl\u00fcht;", "tokens": ["Wo", "nie", "der", "Som\u00b7mer", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo selbst der Schall verklungen,", "tokens": ["Wo", "selbst", "der", "Schall", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Vogel je gesungen", "tokens": ["Kein", "Vo\u00b7gel", "je", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein froh gesellig Lied.", "tokens": ["Sein", "froh", "ge\u00b7sel\u00b7lig", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zum Teppich Moos gebreitet,", "tokens": ["Zum", "Tep\u00b7pich", "Moos", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf w\u00fcstem Heidefeld", "tokens": ["Auf", "w\u00fcs\u00b7tem", "Hei\u00b7de\u00b7feld"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Felsen weich umkleidet;", "tokens": ["Die", "Fel\u00b7sen", "weich", "um\u00b7klei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohin das Auge f\u00e4llt,", "tokens": ["Wo\u00b7hin", "das", "Au\u00b7ge", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Berg, Tal, Schlo\u00df und W\u00e4ldern,", "tokens": ["Von", "Berg", ",", "Tal", ",", "Schlo\u00df", "und", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "St\u00e4dtebes\u00e4ten Feldern,", "tokens": ["St\u00e4d\u00b7te\u00b7be\u00b7s\u00e4\u00b7ten", "Fel\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein' unerme\u00dfne Welt.", "tokens": ["Ein'", "un\u00b7er\u00b7me\u00df\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Wanderern zusammen", "tokens": ["Den", "Wan\u00b7de\u00b7rern", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lodern einsame Flammen", "tokens": ["Lo\u00b7dern", "ein\u00b7sa\u00b7me", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Am Felsenbette auf;", "tokens": ["Am", "Fel\u00b7sen\u00b7bet\u00b7te", "auf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00d6de den Pfad herauf,", "tokens": ["\u00d6\u00b7de", "den", "Pfad", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ein schaurig Tal zur Seite,", "tokens": ["Ein", "schau\u00b7rig", "Tal", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In nebelferner Weite", "tokens": ["In", "ne\u00b7bel\u00b7fer\u00b7ner", "Wei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Schimmernd der Str\u00f6me Lauf.", "tokens": ["Schim\u00b7mernd", "der", "Str\u00f6\u00b7me", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und wie ich nun betrachte,", "tokens": ["Und", "wie", "ich", "nun", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir das Herz erfreut,", "tokens": ["Was", "mir", "das", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "All sorgsam das beachte,", "tokens": ["All", "sorg\u00b7sam", "das", "be\u00b7ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wird es klar so weit;", "tokens": ["Da", "wird", "es", "klar", "so", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sehe sich entfalten", "tokens": ["Ich", "se\u00b7he", "sich", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor meinem Blick die alten", "tokens": ["Vor", "mei\u00b7nem", "Blick", "die", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kunden der grauen Zeit.", "tokens": ["Kun\u00b7den", "der", "grau\u00b7en", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Nach Kriegerweise handeln,", "tokens": ["Nach", "Krie\u00b7ger\u00b7wei\u00b7se", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "German'scher M\u00e4nner Schar,", "tokens": ["Ger\u00b7man'\u00b7scher", "M\u00e4n\u00b7ner", "Schar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seh ich sie da und wandeln;", "tokens": ["Seh", "ich", "sie", "da", "und", "wan\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo einst ihr Lager war,", "tokens": ["Wo", "einst", "ihr", "La\u00b7ger", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf jenes Berges H\u00f6hen,", "tokens": ["Auf", "je\u00b7nes", "Ber\u00b7ges", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "D\u00fcnkt mich sie noch zu sehen,", "tokens": ["D\u00fcnkt", "mich", "sie", "noch", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den K\u00f6nig auch f\u00fcrwahr.", "tokens": ["Den", "K\u00f6\u00b7nig", "auch", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Aus diesen selben Zeiten,", "tokens": ["Aus", "die\u00b7sen", "sel\u00b7ben", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Scheinend im Nebelrauch,", "tokens": ["Schei\u00b7nend", "im", "Ne\u00b7bel\u00b7rauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind wohl, die dort sich breiten,", "tokens": ["Sind", "wohl", ",", "die", "dort", "sich", "brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PRELS", "ADV", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die H\u00fcnengr\u00e4ber auch;", "tokens": ["Die", "H\u00fc\u00b7nen\u00b7gr\u00e4\u00b7ber", "auch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich wehrend der Gewalten,", "tokens": ["Sich", "weh\u00b7rend", "der", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lebten da frei im Walde", "tokens": ["Leb\u00b7ten", "da", "frei", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sie treu dem alten Brauch.", "tokens": ["Sie", "treu", "dem", "al\u00b7ten", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dann durch die Schranken dringen,", "tokens": ["Dann", "durch", "die", "Schran\u00b7ken", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im hohen Siegergl\u00fcck,", "tokens": ["Im", "ho\u00b7hen", "Sie\u00b7ger\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Helden vor und bringen", "tokens": ["Die", "Hel\u00b7den", "vor", "und", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Freiheit der Welt zur\u00fcck.", "tokens": ["Frei\u00b7heit", "der", "Welt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Es str\u00f6mt die Flut der Schmerzen", "tokens": ["Es", "str\u00f6mt", "die", "Flut", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir fragend hin zum Herzen;", "tokens": ["Mir", "fra\u00b7gend", "hin", "zum", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer bringt sie uns zur\u00fcck?", "tokens": ["Wer", "bringt", "sie", "uns", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}