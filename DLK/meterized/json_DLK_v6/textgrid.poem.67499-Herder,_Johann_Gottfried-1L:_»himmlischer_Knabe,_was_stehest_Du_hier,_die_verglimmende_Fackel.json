{"textgrid.poem.67499": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbhimmlischer Knabe, was stehest Du hier, die verglimmende Fackel", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhimmlischer Knabe, was stehest Du hier, die verglimmende Fackel", "tokens": ["\u00bb", "himm\u00b7li\u00b7scher", "Kna\u00b7be", ",", "was", "ste\u00b7hest", "Du", "hier", ",", "die", "ver\u00b7glim\u00b7men\u00b7de", "Fa\u00b7ckel"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nieder zur Erde gesenkt, aber die andere flammt", "tokens": ["Nie\u00b7der", "zur", "Er\u00b7de", "ge\u00b7senkt", ",", "a\u00b7ber", "die", "an\u00b7de\u00b7re", "flammt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,", "ADV", "ART", "ADJA", "VVFIN"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Dir auf Deiner ambrosischen Schulter im Lichte so herrlich!", "tokens": ["Dir", "auf", "Dei\u00b7ner", "am\u00b7bro\u00b7si\u00b7schen", "Schul\u00b7ter", "im", "Lich\u00b7te", "so", "herr\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sch\u00f6neren Purpurglanz sah ja mein Auge nie!", "tokens": ["Sch\u00f6\u00b7ne\u00b7ren", "Pur\u00b7pur\u00b7glanz", "sah", "ja", "mein", "Au\u00b7ge", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Bist Du Amor?\u00ab \u00bbIch bin's! doch unter dieser Umh\u00fcllung,", "tokens": ["Bist", "Du", "A\u00b7mor", "?", "\u00ab", "\u00bb", "Ich", "bin's", "!", "doch", "un\u00b7ter", "die\u00b7ser", "Um\u00b7h\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$.", "$(", "$(", "PPER", "VAFIN", "$.", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ob ich gleich Amor bin, hei\u00df' ich den Sterblichen", "tokens": ["Ob", "ich", "gleich", "A\u00b7mor", "bin", ",", "hei\u00df'", "ich", "den", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NE", "VAFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Unter allen Genien sahn die g\u00fctigen G\u00f6tter", "tokens": ["Un\u00b7ter", "al\u00b7len", "Ge\u00b7ni\u00b7en", "sahn", "die", "g\u00fc\u00b7ti\u00b7gen", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Keinen, der sanft wie ich l\u00f6se das menschliche Herz;", "tokens": ["Kei\u00b7nen", ",", "der", "sanft", "wie", "ich", "l\u00f6\u00b7se", "das", "menschli\u00b7che", "Herz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "ADJD", "KOKOM", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Und sie tauchten die Pfeile, womit ich die Armen erl\u00f6se,", "tokens": ["Und", "sie", "tauch\u00b7ten", "die", "Pfei\u00b7le", ",", "wo\u00b7mit", "ich", "die", "Ar\u00b7men", "er\u00b7l\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-++--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ihnen ein bitter Gescho\u00df, selbst in den Becher der Lust.", "tokens": ["Ih\u00b7nen", "ein", "bit\u00b7ter", "Ge\u00b7scho\u00df", ",", "selbst", "in", "den", "Be\u00b7cher", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Dann geleit' ich im lieblichen Ku\u00df die scheidende Seele", "tokens": ["Dann", "ge\u00b7leit'", "ich", "im", "lieb\u00b7li\u00b7chen", "Ku\u00df", "die", "schei\u00b7den\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Auf zum wahren Genu\u00df br\u00e4utlicher Freuden hinauf.\u00ab", "tokens": ["Auf", "zum", "wah\u00b7ren", "Ge\u00b7nu\u00df", "br\u00e4ut\u00b7li\u00b7cher", "Freu\u00b7den", "hin\u00b7auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "\u00bbaber wo ist Dein Bogen und Pfeil?\u00ab \u00bbDem tapferen Weisen,", "tokens": ["\u00bb", "a\u00b7ber", "wo", "ist", "Dein", "Bo\u00b7gen", "und", "Pfeil", "?", "\u00ab", "\u00bb", "Dem", "tap\u00b7fe\u00b7ren", "Wei\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$.", "$(", "$(", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Der sich selber den Geist l\u00e4ngst von der H\u00fclle getrennt,", "tokens": ["Der", "sich", "sel\u00b7ber", "den", "Geist", "l\u00e4ngst", "von", "der", "H\u00fcl\u00b7le", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Brauch' ich keiner Pfeile. Ich l\u00f6sche die gl\u00e4nzende Fackel", "tokens": ["Brauch'", "ich", "kei\u00b7ner", "Pfei\u00b7le", ".", "Ich", "l\u00f6\u00b7sche", "die", "gl\u00e4n\u00b7zen\u00b7de", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Sanft ihm aus; da erglimmt eilig vom purpurnen Licht", "tokens": ["Sanft", "ihm", "aus", ";", "da", "er\u00b7glimmt", "ei\u00b7lig", "vom", "pur\u00b7pur\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Diese andre. Des Schlafes Bruder, gie\u00df' ich ihm Schlummer", "tokens": ["Die\u00b7se", "and\u00b7re", ".", "Des", "Schla\u00b7fes", "Bru\u00b7der", ",", "gie\u00df'", "ich", "ihm", "Schlum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "$.", "ART", "NN", "NN", "$,", "VVIMP", "PPER", "PPER", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Um den ruhigen Blick, bis er dort oben erwacht.\u00ab", "tokens": ["Um", "den", "ru\u00b7hi\u00b7gen", "Blick", ",", "bis", "er", "dort", "o\u00b7ben", "er\u00b7wacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "\u00bbund wer ist der Weise, dem Du die Fackel der Erde", "tokens": ["\u00bb", "und", "wer", "ist", "der", "Wei\u00b7se", ",", "dem", "Du", "die", "Fa\u00b7ckel", "der", "Er\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Hier gel\u00f6schet, und dem jetzo die sch\u00f6nere flammt?\u00ab", "tokens": ["Hier", "ge\u00b7l\u00f6\u00b7schet", ",", "und", "dem", "jet\u00b7zo", "die", "sch\u00f6\u00b7ne\u00b7re", "flammt", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "$,", "KON", "ART", "ADV", "ART", "ADJA", "VVFIN", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "\u00bbder ist's, dem Athene, wie dort dem tapfern Tydides,", "tokens": ["\u00bb", "der", "ist's", ",", "dem", "A\u00b7the\u00b7ne", ",", "wie", "dort", "dem", "tap\u00b7fern", "Ty\u00b7di\u00b7des", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "$,", "ART", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Selber sch\u00e4rfte den Blick, da\u00df er die G\u00f6tter ersah.", "tokens": ["Sel\u00b7ber", "sch\u00e4rf\u00b7te", "den", "Blick", ",", "da\u00df", "er", "die", "G\u00f6t\u00b7ter", "er\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Mich erkannte Lessing an meiner sinkenden Fackel,", "tokens": ["Mich", "er\u00b7kann\u00b7te", "Les\u00b7sing", "an", "mei\u00b7ner", "sin\u00b7ken\u00b7den", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Und bald z\u00fcndet' ich ihm gl\u00e4nzend die andere an.\u00ab", "tokens": ["Und", "bald", "z\u00fcn\u00b7det'", "ich", "ihm", "gl\u00e4n\u00b7zend", "die", "an\u00b7de\u00b7re", "an", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "ART", "PIS", "PTKVZ", "$.", "$("], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}