{"dta.poem.677": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.71", "af:0.28"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele der du hast ", "tokens": ["Die", "See\u00b7le", "der", "du", "hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Offt beystandt zugesagt/", "tokens": ["Offt", "beys\u00b7tandt", "zu\u00b7ge\u00b7sagt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In die du dich verliebet", "tokens": ["In", "die", "du", "dich", "ver\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die tr\u00e4get diese last/", "tokens": ["Die", "tr\u00e4\u00b7get", "die\u00b7se", "last", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ist die so betr\u00fcbet", "tokens": ["Die", "ist", "die", "so", "be\u00b7tr\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir jhre banden klagt", "tokens": ["Dir", "jhre", "ban\u00b7den", "klagt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Es ist dein trawtes Kindt/ ", "tokens": ["Es", "ist", "dein", "traw\u00b7tes", "Kindt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "chet/ ", "tokens": ["chet", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Das seine freyheit sucht. ", "tokens": ["Das", "sei\u00b7ne", "frey\u00b7heit", "sucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Es ist dein ", "tokens": ["Es", "ist", "dein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Dem man so h\u00fcndisch flucht. ", "tokens": ["Dem", "man", "so", "h\u00fcn\u00b7disch", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dein Kindt hertzlibster GOTT!", "tokens": ["Dein", "Kindt", "hertz\u00b7libs\u00b7ter", "GoTT", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ist frembder spiel vnd spott.", "tokens": ["Ist", "fremb\u00b7der", "spiel", "vnd", "spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}