{"textgrid.poem.38821": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Weihung", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies soll den Schwestern meine Grusse schicken,", "tokens": ["Dies", "soll", "den", "Schwes\u00b7tern", "mei\u00b7ne", "Grus\u00b7se", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in Gesang des Herzens Blum' entbunden,", "tokens": ["Die", "in", "Ge\u00b7sang", "des", "Her\u00b7zens", "Blum'", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mir in Nacht schon war hinweggeschwunden,", "tokens": ["Die", "mir", "in", "Nacht", "schon", "war", "hin\u00b7weg\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun f\u00fchl' ich wiederum ihr goldnes Blicken.", "tokens": ["Nun", "f\u00fchl'", "ich", "wie\u00b7de\u00b7rum", "ihr", "gold\u00b7nes", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dich h\u00f6r' ich T\u00f6ne so wie Blumen pfl\u00fccken", "tokens": ["Dich", "h\u00f6r'", "ich", "T\u00f6\u00b7ne", "so", "wie", "Blu\u00b7men", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADV", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Hulda, und es mu\u00df das Herz gesunden,", "tokens": ["O", "Hul\u00b7da", ",", "und", "es", "mu\u00df", "das", "Herz", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "PPER", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Glaub' und Kraft' ist Una fest verbunden,", "tokens": ["Mit", "Glaub'", "und", "Kraft'", "ist", "U\u00b7na", "fest", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Clara blickt und spricht und singt Entz\u00fccken.", "tokens": ["Und", "Cla\u00b7ra", "blickt", "und", "spricht", "und", "singt", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir haben Glaub', Entz\u00fccken beigestanden,", "tokens": ["Mir", "ha\u00b7ben", "Glaub'", ",", "Ent\u00b7z\u00fc\u00b7cken", "bei\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Holdseeligkeit mit ihrer stillen G\u00fcte,", "tokens": ["Hold\u00b7see\u00b7lig\u00b7keit", "mit", "ih\u00b7rer", "stil\u00b7len", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets will ich mich in dies Erinnern senken:", "tokens": ["Stets", "will", "ich", "mich", "in", "dies", "E\u00b7rin\u00b7nern", "sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nehmt, wie unw\u00fcrdig auch, von meinen H\u00e4nden,", "tokens": ["Nehmt", ",", "wie", "un\u00b7w\u00fcr\u00b7dig", "auch", ",", "von", "mei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ich der Mild', die gern verzeihet, biete,", "tokens": ["Was", "ich", "der", "Mild'", ",", "die", "gern", "ver\u00b7zei\u00b7het", ",", "bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dies schwache Lied zu meinem Angedenken.", "tokens": ["Dies", "schwa\u00b7che", "Lied", "zu", "mei\u00b7nem", "An\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}