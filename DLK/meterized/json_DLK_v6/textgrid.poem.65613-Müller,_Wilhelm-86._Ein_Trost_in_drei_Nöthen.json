{"textgrid.poem.65613": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "86. Ein Trost in drei N\u00f6then", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn morsche B\u00e4ume anfangen zu brennen,", "tokens": ["Wenn", "mor\u00b7sche", "B\u00e4u\u00b7me", "an\u00b7fan\u00b7gen", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn faule Pferde anfangen zu rennen,", "tokens": ["Wenn", "fau\u00b7le", "Pfer\u00b7de", "an\u00b7fan\u00b7gen", "zu", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "PTKZU", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn alte Weiber anfangen zu lieben,", "tokens": ["Wenn", "al\u00b7te", "Wei\u00b7ber", "an\u00b7fan\u00b7gen", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott Lob, noch Keines hat's lange getrieben!", "tokens": ["Gott", "Lob", ",", "noch", "Kei\u00b7nes", "hat's", "lan\u00b7ge", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "PIS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}