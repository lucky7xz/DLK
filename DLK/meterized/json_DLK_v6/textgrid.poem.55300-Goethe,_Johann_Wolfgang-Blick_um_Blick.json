{"textgrid.poem.55300": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Blick um Blick", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du dich im Spiegel besiehst,", "tokens": ["Wenn", "du", "dich", "im", "Spie\u00b7gel", "be\u00b7siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Denke, da\u00df ich diese Augen k\u00fc\u00dfte", "tokens": ["Den\u00b7ke", ",", "da\u00df", "ich", "die\u00b7se", "Au\u00b7gen", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mich mit mir selbst entzweien m\u00fc\u00dfte,", "tokens": ["Und", "mich", "mit", "mir", "selbst", "ent\u00b7zwei\u00b7en", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sobalde du mich fliehst:", "tokens": ["So\u00b7bal\u00b7de", "du", "mich", "fliehst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn da ich nur in diesen Augen lebe,", "tokens": ["Denn", "da", "ich", "nur", "in", "die\u00b7sen", "Au\u00b7gen", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du mir gibst, was ich gebe,", "tokens": ["Du", "mir", "gibst", ",", "was", "ich", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "So w\u00e4r ich ganz verloren;", "tokens": ["So", "w\u00e4r", "ich", "ganz", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jetzt bin ich immer wie neu geboren.", "tokens": ["Jetzt", "bin", "ich", "im\u00b7mer", "wie", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}