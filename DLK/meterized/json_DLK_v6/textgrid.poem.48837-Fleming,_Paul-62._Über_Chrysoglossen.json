{"textgrid.poem.48837": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "62. \u00dcber Chrysoglossen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sei! Ich habe Trost von ihrer Gunst gehabt,", "tokens": ["Es", "sei", "!", "Ich", "ha\u00b7be", "Trost", "von", "ih\u00b7rer", "Gunst", "ge\u00b7habt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der s\u00fc\u00dfen Lieblichkeit mit halber Lust geno\u00dfen;", "tokens": ["der", "s\u00fc\u00b7\u00dfen", "Lieb\u00b7lich\u00b7keit", "mit", "hal\u00b7ber", "Lust", "ge\u00b7no\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wer aber denkt auch das, was drunter war verschlo\u00dfen;", "tokens": ["wer", "a\u00b7ber", "denkt", "auch", "das", ",", "was", "drun\u00b7ter", "war", "ver\u00b7schlo\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "PDS", "$,", "PRELS", "PAV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie bald ihr, Schmerzen, doch die Freuden untergrabt,", "tokens": ["wie", "bald", "ihr", ",", "Schmer\u00b7zen", ",", "doch", "die", "Freu\u00b7den", "un\u00b7ter\u00b7grabt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "und wie ihr, wenn ihr kompt, so langsam wieder trabt?", "tokens": ["und", "wie", "ihr", ",", "wenn", "ihr", "kompt", ",", "so", "lang\u00b7sam", "wie\u00b7der", "trabt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer mi\u00dft die Tr\u00e4nen auch, die ich um sie vergo\u00dfen,", "tokens": ["Wer", "mi\u00dft", "die", "Tr\u00e4\u00b7nen", "auch", ",", "die", "ich", "um", "sie", "ver\u00b7go\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die zweien Str\u00f6men gleich von beiden Augen flo\u00dfen,", "tokens": ["die", "zwei\u00b7en", "Str\u00f6\u00b7men", "gleich", "von", "bei\u00b7den", "Au\u00b7gen", "flo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wie manchen Herzenssto\u00df, ihr Seufzer, ihr mir gabt?", "tokens": ["wie", "man\u00b7chen", "Her\u00b7zens\u00b7sto\u00df", ",", "ihr", "Seuf\u00b7zer", ",", "ihr", "mir", "gabt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Itzt seh' ich, da\u00df es sind Kupidos alte Possen.", "tokens": ["Itzt", "seh'", "ich", ",", "da\u00df", "es", "sind", "Ku\u00b7pi\u00b7dos", "al\u00b7te", "Pos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Pfeile hat er viel von anfangs her verscho\u00dfen,", "tokens": ["Der", "Pfei\u00b7le", "hat", "er", "viel", "von", "an\u00b7fangs", "her", "ver\u00b7scho\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADV", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die er ihm schneidet selbst, selbst fiedert, selbsten schabt.", "tokens": ["die", "er", "ihm", "schnei\u00b7det", "selbst", ",", "selbst", "fie\u00b7dert", ",", "selbs\u00b7ten", "schabt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er gehe nun auch hin und sage ", "tokens": ["Er", "ge\u00b7he", "nun", "auch", "hin", "und", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was mich vor so erfreut, macht itzt mich so verdro\u00dfen,", "tokens": ["was", "mich", "vor", "so", "er\u00b7freut", ",", "macht", "itzt", "mich", "so", "ver\u00b7dro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADV", "ADJD", "$,", "VVFIN", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "sie kr\u00e4nket mich so sehr, als sehr sie mich gelabt.", "tokens": ["sie", "kr\u00e4n\u00b7ket", "mich", "so", "sehr", ",", "als", "sehr", "sie", "mich", "ge\u00b7labt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ADV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}