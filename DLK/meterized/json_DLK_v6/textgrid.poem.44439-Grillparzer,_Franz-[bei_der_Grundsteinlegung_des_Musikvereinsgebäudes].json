{"textgrid.poem.44439": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[bei der Grundsteinlegung des Musikvereinsgeb\u00e4udes]", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die dieses Haus in Gottes Hut vertraut,", "tokens": ["Die", "die\u00b7ses", "Haus", "in", "Got\u00b7tes", "Hut", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus kleinen Spenden haben sammelnd sies erbaut,", "tokens": ["Aus", "klei\u00b7nen", "Spen\u00b7den", "ha\u00b7ben", "sam\u00b7melnd", "sies", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gespart, gesorgt, getrachtet jahrelang,", "tokens": ["Ge\u00b7spart", ",", "ge\u00b7sorgt", ",", "ge\u00b7trach\u00b7tet", "jah\u00b7re\u00b7lang", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Tonkunst es geweiht und dem Gesang,", "tokens": ["Der", "Ton\u00b7kunst", "es", "ge\u00b7weiht", "und", "dem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie dachten, als sie legten diesen Stein,", "tokens": ["Sie", "dach\u00b7ten", ",", "als", "sie", "leg\u00b7ten", "die\u00b7sen", "Stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So fest wie er m\u00f6g ihre Stiftung sein,", "tokens": ["So", "fest", "wie", "er", "m\u00f6g", "ih\u00b7re", "Stif\u00b7tung", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und selbst wenn einst zerfallen Stein und Mauern,", "tokens": ["Und", "selbst", "wenn", "einst", "zer\u00b7fal\u00b7len", "Stein", "und", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Noch gleicher Sinn in diesem Lande dauern.", "tokens": ["Noch", "glei\u00b7cher", "Sinn", "in", "die\u00b7sem", "Lan\u00b7de", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}