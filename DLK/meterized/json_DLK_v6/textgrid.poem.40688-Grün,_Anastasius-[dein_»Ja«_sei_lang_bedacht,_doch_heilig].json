{"textgrid.poem.40688": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[dein \u00bbJa\u00ab sei lang bedacht, doch heilig]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein \u00bbJa\u00ab sei lang bedacht, doch heilig,", "tokens": ["Dein", "\u00bb", "Ja", "\u00ab", "sei", "lang", "be\u00b7dacht", ",", "doch", "hei\u00b7lig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "PTKANT", "$(", "VAFIN", "ADJD", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein \u00bbNein\u00ab sprich mild, doch nicht zu eilig,", "tokens": ["Dein", "\u00bb", "Nein", "\u00ab", "sprich", "mild", ",", "doch", "nicht", "zu", "ei\u00b7lig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "PTKANT", "$(", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wird das Ja den Freund erfreuen,", "tokens": ["So", "wird", "das", "Ja", "den", "Freund", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Nein dich selber niemals reuen.", "tokens": ["Das", "Nein", "dich", "sel\u00b7ber", "nie\u00b7mals", "reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}