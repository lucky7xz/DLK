{"textgrid.poem.53916": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ersatz", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen richtigen K\u00f6nig? Wir haben keinen", "tokens": ["Ei\u00b7nen", "rich\u00b7ti\u00b7gen", "K\u00f6\u00b7nig", "?", "Wir", "ha\u00b7ben", "kei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PIAT"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und daher borgen wir uns einen.", "tokens": ["und", "da\u00b7her", "bor\u00b7gen", "wir", "uns", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "ART", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei gegr\u00fc\u00dft, du sch\u00f6ne Gelegenheit!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "sch\u00f6\u00b7ne", "Ge\u00b7le\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alles ist wie in alter Zeit:", "tokens": ["Al\u00b7les", "ist", "wie", "in", "al\u00b7ter", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Stra\u00dfenabsperrung und Schutzmannsg\u00e4ule,", "tokens": ["Stra\u00b7\u00dfen\u00b7ab\u00b7sper\u00b7rung", "und", "Schutz\u00b7manns\u00b7g\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neugier der Kleinb\u00fcrger, Hurra-Geheule,", "tokens": ["Neu\u00b7gier", "der", "Klein\u00b7b\u00fcr\u00b7ger", ",", "Hur\u00b7ra\u00b7Ge\u00b7heu\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Monokel-Kerle die Kreuz und die Quer", "tokens": ["Mo\u00b7no\u00b7kel\u00b7Ker\u00b7le", "die", "Kreuz", "und", "die", "Quer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und: Milit\u00e4r! Milit\u00e4r! Milit\u00e4r!", "tokens": ["und", ":", "Mi\u00b7li\u00b7t\u00e4r", "!", "Mi\u00b7li\u00b7t\u00e4r", "!", "Mi\u00b7li\u00b7t\u00e4r", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Endlich wissen die deutschen Knaben,", "tokens": ["End\u00b7lich", "wis\u00b7sen", "die", "deut\u00b7schen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "wozu sie eine Reichswehr haben!", "tokens": ["wo\u00b7zu", "sie", "ei\u00b7ne", "Reichs\u00b7wehr", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dazu.", "tokens": ["Da\u00b7zu", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Denn wenn Deutschland was feiert,", "tokens": ["Denn", "wenn", "Deutschland", "was", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "PWS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "kommt immer die Reichswehr angemeiert,", "tokens": ["kommt", "im\u00b7mer", "die", "Reichs\u00b7wehr", "an\u00b7ge\u00b7mei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "als der vollendete Ausdruck des Landes", "tokens": ["als", "der", "voll\u00b7en\u00b7de\u00b7te", "Aus\u00b7druck", "des", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "und zur Erfrischung des B\u00fcrgerverstandes.", "tokens": ["und", "zur", "Er\u00b7fri\u00b7schung", "des", "B\u00fcr\u00b7ger\u00b7ver\u00b7stan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Im Spalier aber steht bei Aman Ullah-Chan", "tokens": ["Im", "Spa\u00b7lier", "a\u00b7ber", "steht", "bei", "A\u00b7man", "Ul\u00b7lah\u00b7Chan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er: der deutsche Untertan.", "tokens": ["Er", ":", "der", "deut\u00b7sche", "Un\u00b7ter\u00b7tan", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo wird denn der fremde K\u00f6nig wohnen?", "tokens": ["Wo", "wird", "denn", "der", "frem\u00b7de", "K\u00f6\u00b7nig", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er kann doch nicht auf dem Bahnhof thronen . . .", "tokens": ["Er", "kann", "doch", "nicht", "auf", "dem", "Bahn\u00b7hof", "thro\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Palais? Ja, es tut uns furchtbar leid:", "tokens": ["Ein", "Pa\u00b7lais", "?", "Ja", ",", "es", "tut", "uns", "furcht\u00b7bar", "leid", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "aber die Palais geh\u00f6ren zur Zeit", "tokens": ["a\u00b7ber", "die", "Pa\u00b7lais", "ge\u00b7h\u00f6\u00b7ren", "zur", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "der republikanischen F\u00fcrstlichkeit.", "tokens": ["der", "re\u00b7pub\u00b7li\u00b7ka\u00b7ni\u00b7schen", "F\u00fcrst\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da mieten wir schon \u2013 la\u00df die Arbeiter kollern! \u2013", "tokens": ["Da", "mie\u00b7ten", "wir", "schon", "\u2013", "la\u00df", "die", "Ar\u00b7bei\u00b7ter", "kol\u00b7lern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "VVIMP", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "bescheiden ein Haus von den Hohenzollern.", "tokens": ["be\u00b7schei\u00b7den", "ein", "Haus", "von", "den", "Ho\u00b7hen\u00b7zol\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "(von deinen Steuern.)", "tokens": ["(", "von", "dei\u00b7nen", "Steu\u00b7ern", ".", ")"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und mit m\u00e4chtigem Getos", "tokens": ["Und", "mit", "m\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7tos"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "gehts los:", "tokens": ["gehts", "los", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Generale und Admirale.", "tokens": ["Ge\u00b7ne\u00b7ra\u00b7le", "und", "Ad\u00b7mi\u00b7ra\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "B\u00fcrgermeister und Ehrenpokale \u2013", "tokens": ["B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "und", "Eh\u00b7ren\u00b7po\u00b7ka\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "oben, auf dem Brandenburger Tor,", "tokens": ["o\u00b7ben", ",", "auf", "dem", "Bran\u00b7den\u00b7bur\u00b7ger", "Tor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "lugt eine richtige Feldwache vor", "tokens": ["lugt", "ei\u00b7ne", "rich\u00b7ti\u00b7ge", "Feld\u00b7wa\u00b7che", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u2013 sie spielen Krieg \u2013 als ob sie drauf lauern,", "tokens": ["\u2013", "sie", "spie\u00b7len", "Krieg", "\u2013", "als", "ob", "sie", "drauf", "lau\u00b7ern", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$(", "KOKOM", "KOUS", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "vor der Macht eines K\u00f6nigs zu erschauern.", "tokens": ["vor", "der", "Macht", "ei\u00b7nes", "K\u00f6\u00b7nigs", "zu", "er\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und in allen Augen ein Glanz:", "tokens": ["Und", "in", "al\u00b7len", "Au\u00b7gen", "ein", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Heil Aman Ullah im Siegerkranz!", "tokens": ["Heil", "A\u00b7man", "Ul\u00b7lah", "im", "Sie\u00b7ger\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "APPRART", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.9": {"text": "(unsrer ist leider \u2013 Gott seis gepfiffen \u2013", "tokens": ["(", "uns\u00b7rer", "ist", "lei\u00b7der", "\u2013", "Gott", "seis", "ge\u00b7pfif\u00b7fen", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ADV", "$(", "NN", "VAFIN", "VVPP", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "leise weinend ausgekniffen.)", "tokens": ["lei\u00b7se", "wei\u00b7nend", "aus\u00b7ge\u00b7knif\u00b7fen", ".", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "VVIZU", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wer tommt denn da \u2013?", "tokens": ["Und", "wer", "tommt", "denn", "da", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ADV", "$(", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der liebe gute republikanische Kronprinz ist auch noch da!", "tokens": ["Der", "lie\u00b7be", "gu\u00b7te", "re\u00b7pub\u00b7li\u00b7ka\u00b7ni\u00b7sche", "Kron\u00b7prinz", "ist", "auch", "noch", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+--+--+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Mit dem Geschmack von Papa", "tokens": ["Mit", "dem", "Ge\u00b7schmack", "von", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "und mit Tat\u00fc und Tata", "tokens": ["und", "mit", "Ta\u00b7t\u00fc", "und", "Ta\u00b7ta"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "f\u00e4hrt er im Auto durch die Linden,", "tokens": ["f\u00e4hrt", "er", "im", "Au\u00b7to", "durch", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "um in den Pferde\u00e4ppeln eine verlorene Krone zu finden.", "tokens": ["um", "in", "den", "Pfer\u00b7de\u00b7\u00e4p\u00b7peln", "ei\u00b7ne", "ver\u00b7lo\u00b7re\u00b7ne", "Kro\u00b7ne", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Das gute Kind \u2013! Wie die R\u00fccken sich beugen", "tokens": ["Das", "gu\u00b7te", "Kind", "\u2013", "!", "Wie", "die", "R\u00fc\u00b7cken", "sich", "beu\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "$.", "PWAV", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "wie die Fr\u00e4cke sich demutsvoll verneigen!", "tokens": ["wie", "die", "Fr\u00e4\u00b7cke", "sich", "de\u00b7muts\u00b7voll", "ver\u00b7nei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Uniformen blitzen ordensbesternt!", "tokens": ["U\u00b7nif\u00b7or\u00b7men", "blit\u00b7zen", "or\u00b7dens\u00b7bes\u00b7ternt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Das k\u00f6nnen sie. Das haben sie gelernt.", "tokens": ["Das", "k\u00f6n\u00b7nen", "sie", ".", "Das", "ha\u00b7ben", "sie", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$.", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Lacht da einer? Da lacht keiner dr\u00fcber.", "tokens": ["Lacht", "da", "ei\u00b7ner", "?", "Da", "lacht", "kei\u00b7ner", "dr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$.", "ADV", "VVFIN", "PIS", "PAV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Die Zeitungen schwappen vor Schwachsinn \u00fcber,", "tokens": ["Die", "Zei\u00b7tun\u00b7gen", "schwap\u00b7pen", "vor", "Schwach\u00b7sinn", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "belichten vom Pr\u00e4sidenten-Salon,", "tokens": ["be\u00b7lich\u00b7ten", "vom", "Pr\u00e4\u00b7si\u00b7den\u00b7ten\u00b7Sa\u00b7lon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "von Gala-Oper und H\u00fchnerbouillon,", "tokens": ["von", "Ga\u00b7la\u00b7O\u00b7per", "und", "H\u00fch\u00b7ner\u00b7bou\u00b7il\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der braune K\u00f6nig wird Ehrendoktor . . .", "tokens": ["Der", "brau\u00b7ne", "K\u00f6\u00b7nig", "wird", "Eh\u00b7ren\u00b7dok\u00b7tor", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"