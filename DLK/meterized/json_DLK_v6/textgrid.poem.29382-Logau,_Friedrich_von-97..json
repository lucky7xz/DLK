{"textgrid.poem.29382": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herodes weist die Weisen,", "tokens": ["He\u00b7ro\u00b7des", "weist", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo sie zu Christus reisen,", "tokens": ["Wo", "sie", "zu", "Chris\u00b7tus", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kummt aber selbsten nicht", "tokens": ["Kummt", "a\u00b7ber", "selbs\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und bringt ihm seine Pflicht;", "tokens": ["Und", "bringt", "ihm", "sei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wei\u00df, was die wol glauben,", "tokens": ["Wer", "wei\u00df", ",", "was", "die", "wol", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die uns zum Glauben schrauben?", "tokens": ["Die", "uns", "zum", "Glau\u00b7ben", "schrau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}