{"textgrid.poem.63844": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Ermutigung", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nur getrost! Was auch geschieht,", "tokens": ["Sei", "nur", "ge\u00b7trost", "!", "Was", "auch", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Werd' an dir selbst nicht irre, mein Herz!", "tokens": ["Werd'", "an", "dir", "selbst", "nicht", "ir\u00b7re", ",", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Sieh, auf schwankender Lebensfahrt,", "tokens": ["Sieh", ",", "auf", "schwan\u00b7ken\u00b7der", "Le\u00b7bens\u00b7fahrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Der st\u00fcrmevollen,", "tokens": ["Der", "st\u00fcr\u00b7me\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bleibt kein anderes Heil,", "tokens": ["Bleibt", "kein", "an\u00b7de\u00b7res", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Als zu vertrauen dem D\u00e4mon,", "tokens": ["Als", "zu", "ver\u00b7trau\u00b7en", "dem", "D\u00e4\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Der deines Schiffleins Steuer", "tokens": ["Der", "dei\u00b7nes", "Schif\u00b7fleins", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lenkt mit eigenwilliger Hand.", "tokens": ["Lenkt", "mit", "ei\u00b7gen\u00b7wil\u00b7li\u00b7ger", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ob er es f\u00fchrt zum sichern Port,", "tokens": ["Ob", "er", "es", "f\u00fchrt", "zum", "si\u00b7chern", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob scheitern l\u00e4sset an Klippen,", "tokens": ["Ob", "schei\u00b7tern", "l\u00e4s\u00b7set", "an", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Du bist sein Meister nicht,", "tokens": ["Du", "bist", "sein", "Meis\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mit kl\u00fcgelnder Torheit", "tokens": ["Mit", "kl\u00fc\u00b7geln\u00b7der", "Tor\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "In den Arm ihm zu fallen.", "tokens": ["In", "den", "Arm", "ihm", "zu", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Denn er ist's, der dir von Anbeginn", "tokens": ["Denn", "er", "ist's", ",", "der", "dir", "von", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Ward zum Lenker gegeben,", "tokens": ["Ward", "zum", "Len\u00b7ker", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Da\u00df deine Bahn du vollendest,", "tokens": ["Da\u00df", "dei\u00b7ne", "Bahn", "du", "voll\u00b7en\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Nachtwandlerisch nach verh\u00fclltem Ziel", "tokens": ["Nacht\u00b7wand\u00b7le\u00b7risch", "nach", "ver\u00b7h\u00fcll\u00b7tem", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Und wissest nicht, was sie wollen mit dir,", "tokens": ["Und", "wis\u00b7sest", "nicht", ",", "was", "sie", "wol\u00b7len", "mit", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VMFIN", "APPR", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Die dunklen M\u00e4chte.", "tokens": ["Die", "dunk\u00b7len", "M\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Sie aber wissen's.", "tokens": ["Sie", "a\u00b7ber", "wis\u00b7sen'", "s."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPER", "ADV", "VVFIN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Denn was du blindlings, dem D\u00e4mon getreu,", "tokens": ["Denn", "was", "du", "blind\u00b7lings", ",", "dem", "D\u00e4\u00b7mon", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vollbringst an deinen sprossenden Tagen,", "tokens": ["Voll\u00b7bringst", "an", "dei\u00b7nen", "spros\u00b7sen\u00b7den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mehr ist's und fruchtet reicher,", "tokens": ["Mehr", "ist's", "und", "fruch\u00b7tet", "rei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NE", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als was du t\u00e4tst, auf den Zuruf horchend", "tokens": ["Als", "was", "du", "t\u00e4tst", ",", "auf", "den", "Zu\u00b7ruf", "hor\u00b7chend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der fremden schwatzenden Menge.", "tokens": ["Der", "frem\u00b7den", "schwat\u00b7zen\u00b7den", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und w\u00e4r' es herrlich, es w\u00e4r' nicht dein,", "tokens": ["Und", "w\u00e4r'", "es", "herr\u00b7lich", ",", "es", "w\u00e4r'", "nicht", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "W\u00e4r' nur ein Knechtswerk,", "tokens": ["W\u00e4r'", "nur", "ein", "Knechts\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Unwirksam, dich zu begl\u00fccken.", "tokens": ["Un\u00b7wirk\u00b7sam", ",", "dich", "zu", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Sei nur getrost! Was auch geschieht,", "tokens": ["Sei", "nur", "ge\u00b7trost", "!", "Was", "auch", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Werd' an dir selbst nicht irre, mein Herz!", "tokens": ["Werd'", "an", "dir", "selbst", "nicht", "ir\u00b7re", ",", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}