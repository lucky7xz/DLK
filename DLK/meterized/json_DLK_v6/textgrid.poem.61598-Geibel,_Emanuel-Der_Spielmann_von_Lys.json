{"textgrid.poem.61598": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Spielmann von Lys", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Forst von Lys am tiefen See", "tokens": ["Im", "Forst", "von", "Lys", "am", "tie\u00b7fen", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergl\u00fcht die Mittagsstunde,", "tokens": ["Er\u00b7gl\u00fcht", "die", "Mit\u00b7tags\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hundertj\u00e4hrigen Eichen stehn", "tokens": ["Die", "hun\u00b7dert\u00b7j\u00e4h\u00b7ri\u00b7gen", "Ei\u00b7chen", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verschlafen in der Runde.", "tokens": ["Ver\u00b7schla\u00b7fen", "in", "der", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein L\u00fcftchen geht, man h\u00f6rt von fern", "tokens": ["Kein", "L\u00fcft\u00b7chen", "geht", ",", "man", "h\u00f6rt", "von", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIS", "VVFIN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Specht in Waldesmitten,", "tokens": ["Den", "Specht", "in", "Wal\u00b7des\u00b7mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt der Spielmann durch den Busch,", "tokens": ["Da", "kommt", "der", "Spiel\u00b7mann", "durch", "den", "Busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der braune Geselle, geschritten.", "tokens": ["Der", "brau\u00b7ne", "Ge\u00b7sel\u00b7le", ",", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Er tr\u00e4gt ein Wams von Flicken bunt,", "tokens": ["Er", "tr\u00e4gt", "ein", "Wams", "von", "Fli\u00b7cken", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4gt Farnkrautbl\u00fct' am Hute,", "tokens": ["Tr\u00e4gt", "Farn\u00b7kraut\u00b7bl\u00fct'", "am", "Hu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein schwarzes Auge lacht und blitzt,", "tokens": ["Sein", "schwar\u00b7zes", "Au\u00b7ge", "lacht", "und", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er singt mit lachendem Mute:", "tokens": ["Er", "singt", "mit", "la\u00b7chen\u00b7dem", "Mu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbich bin des gr\u00fcnen Waldes Kind,", "tokens": ["\u00bb", "ich", "bin", "des", "gr\u00fc\u00b7nen", "Wal\u00b7des", "Kind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tierlein kennen mich alle;", "tokens": ["Die", "Tier\u00b7lein", "ken\u00b7nen", "mich", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Woher ich komme, das wei\u00df der Wind,", "tokens": ["Wo\u00b7her", "ich", "kom\u00b7me", ",", "das", "wei\u00df", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Wind, wohin ich walle.", "tokens": ["Der", "Wind", ",", "wo\u00b7hin", "ich", "wal\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Bauern lach' ich hinterm Pflug,", "tokens": ["Des", "Bau\u00b7ern", "lach'", "ich", "hin\u00b7term", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grafen hoch im Saale;", "tokens": ["Des", "Gra\u00b7fen", "hoch", "im", "Saa\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Truchse\u00df ist der Brombeerstrauch,", "tokens": ["Mein", "Truch\u00b7se\u00df", "ist", "der", "Brom\u00b7beer\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schenk der Quell im Tale.", "tokens": ["Mein", "Schenk", "der", "Quell", "im", "Ta\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbim Winter schlaf' ich bei dem Fuchs,", "tokens": ["\u00bb", "im", "Win\u00b7ter", "schlaf'", "ich", "bei", "dem", "Fuchs", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Lenz auf sonnigem Rasen,", "tokens": ["Im", "Lenz", "auf", "son\u00b7ni\u00b7gem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wird die Weile mir lang einmal,", "tokens": ["Und", "wird", "die", "Wei\u00b7le", "mir", "lang", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So heb' ich an zu blasen.\u00ab", "tokens": ["So", "heb'", "ich", "an", "zu", "bla\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er zieht hervor die Pfeif' aus Rohr,", "tokens": ["Er", "zieht", "her\u00b7vor", "die", "Pfeif'", "aus", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Klang versucht er leise;", "tokens": ["Den", "Klang", "ver\u00b7sucht", "er", "lei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fremdartig durch die stille Luft,", "tokens": ["Fremd\u00b7ar\u00b7tig", "durch", "die", "stil\u00b7le", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verlockend schwillt die Weise.", "tokens": ["Ver\u00b7lo\u00b7ckend", "schwillt", "die", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie jauchzt wie wirbelnder Lerchenschlag,", "tokens": ["Sie", "jauchzt", "wie", "wir\u00b7beln\u00b7der", "Ler\u00b7chen\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie klagt wie Unkengest\u00f6hne,", "tokens": ["Sie", "klagt", "wie", "Un\u00b7ken\u00b7ge\u00b7st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie Kinderjubel und Todesqual", "tokens": ["Wie", "Kin\u00b7der\u00b7ju\u00b7bel", "und", "To\u00b7des\u00b7qual"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lachen und weinen die T\u00f6ne.", "tokens": ["La\u00b7chen", "und", "wei\u00b7nen", "die", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Und wie er sanft und sanfter bl\u00e4st,", "tokens": ["Und", "wie", "er", "sanft", "und", "sanf\u00b7ter", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da regt sich's in den B\u00fcschen,", "tokens": ["Da", "regt", "sich's", "in", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt es geschl\u00fcpft durchs hohe Gras", "tokens": ["Da", "kommt", "es", "ge\u00b7schl\u00fcpft", "durchs", "ho\u00b7he", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit leisem Rieseln und Zischen;", "tokens": ["Mit", "lei\u00b7sem", "Rie\u00b7seln", "und", "Zi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Jetzt hebt sich vom Boden ein gr\u00fcnes Haupt", "tokens": ["Jetzt", "hebt", "sich", "vom", "Bo\u00b7den", "ein", "gr\u00fc\u00b7nes", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf gr\u00fcnem, glei\u00dfendem R\u00fccken,", "tokens": ["Auf", "gr\u00fc\u00b7nem", ",", "glei\u00b7\u00dfen\u00b7dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zwei Augen gl\u00fchn wie Edelgestein", "tokens": ["Zwei", "Au\u00b7gen", "gl\u00fchn", "wie", "E\u00b7del\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und funkeln vor Entz\u00fccken.", "tokens": ["Und", "fun\u00b7keln", "vor", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das ist die Schlangenk\u00f6nigin,", "tokens": ["Das", "ist", "die", "Schlan\u00b7gen\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kommt bezaubert vom Schalle,", "tokens": ["Sie", "kommt", "be\u00b7zau\u00b7bert", "vom", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hinter der Alten, wie Heeresgefolg',", "tokens": ["Und", "hin\u00b7ter", "der", "Al\u00b7ten", ",", "wie", "Hee\u00b7res\u00b7ge\u00b7fol\u00b7g'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Schlangen des Waldes alle.", "tokens": ["Die", "Schlan\u00b7gen", "des", "Wal\u00b7des", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Sie schlie\u00dfen den Kreis gleich wie zum Reihn,", "tokens": ["Sie", "schlie\u00b7\u00dfen", "den", "Kreis", "gleich", "wie", "zum", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ringeln und z\u00fcngeln vor Wonne,", "tokens": ["Sie", "rin\u00b7geln", "und", "z\u00fcn\u00b7geln", "vor", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Um ihre schillernden Leiber spielt", "tokens": ["Um", "ih\u00b7re", "schil\u00b7lern\u00b7den", "Lei\u00b7ber", "spielt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durchs Laub der Strahl der Sonne.", "tokens": ["Durchs", "Laub", "der", "Strahl", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und sieh, nun schl\u00fcpft um des Spielmanns Hals", "tokens": ["Und", "sieh", ",", "nun", "schl\u00fcpft", "um", "des", "Spiel\u00b7manns", "Hals"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die K\u00f6nigin z\u00e4rtlich und leise,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "z\u00e4rt\u00b7lich", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er kennt das Liebkosen der Freundin schon", "tokens": ["Er", "kennt", "das", "Lieb\u00b7ko\u00b7sen", "der", "Freun\u00b7din", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und bl\u00e4st die schmelzendste Weise.", "tokens": ["Und", "bl\u00e4st", "die", "schmel\u00b7zends\u00b7te", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Doch als des Schalls ihn d\u00fcnkt genug,", "tokens": ["Doch", "als", "des", "Schalls", "ihn", "d\u00fcnkt", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da setzt er vom Munde die Pfeife,", "tokens": ["Da", "setzt", "er", "vom", "Mun\u00b7de", "die", "Pfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Schlange, wonneges\u00e4ttigt, l\u00f6st", "tokens": ["Die", "Schlan\u00b7ge", ",", "won\u00b7ne\u00b7ge\u00b7s