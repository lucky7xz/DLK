{"textgrid.poem.48960": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von wannen k\u00f6mmt der s\u00fc\u00dfe Schall? \u2013", "tokens": ["Von", "wan\u00b7nen", "k\u00f6mmt", "der", "s\u00fc\u00b7\u00dfe", "Schall", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du es, Melodieenreicher,", "tokens": ["Bist", "du", "es", ",", "Me\u00b7lo\u00b7di\u00b7een\u00b7rei\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du Bardenvogel Nachtigall,", "tokens": ["Du", "Bar\u00b7den\u00b7vo\u00b7gel", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mich aus meinem Schlummer ruft? \u2013", "tokens": ["Der", "mich", "aus", "mei\u00b7nem", "Schlum\u00b7mer", "ruft", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn du es bist; dann ist der Schnee zerronnen,", "tokens": ["Wenn", "du", "es", "bist", ";", "dann", "ist", "der", "Schnee", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dann herrscht der Lenz mit seinen Wonnen", "tokens": ["Dann", "herrscht", "der", "Lenz", "mit", "sei\u00b7nen", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im Felde, Wald und Luft.", "tokens": ["Im", "Fel\u00b7de", ",", "Wald", "und", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heil mir! Er ist's! Die Seele", "tokens": ["Heil", "mir", "!", "Er", "ist's", "!", "Die", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "$.", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Des Jahrs ward ihrer Todesbanden los.", "tokens": ["Des", "Jahrs", "ward", "ih\u00b7rer", "To\u00b7des\u00b7ban\u00b7den", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heraus, heraus aus deiner H\u00f6le,", "tokens": ["He\u00b7raus", ",", "he\u00b7raus", "aus", "dei\u00b7ner", "H\u00f6\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Barde, wo der Winter dich verschlo\u00df!", "tokens": ["O", "Bar\u00b7de", ",", "wo", "der", "Win\u00b7ter", "dich", "ver\u00b7schlo\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Reib dir das Auge hell,", "tokens": ["Reib", "dir", "das", "Au\u00b7ge", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Befl\u00fcgle deinen Schritt.", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "dei\u00b7nen", "Schritt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Heraus, und nimm die Harfe mit!", "tokens": ["He\u00b7raus", ",", "und", "nimm", "die", "Har\u00b7fe", "mit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In meines Felsens H\u00f6le,", "tokens": ["In", "mei\u00b7nes", "Fel\u00b7sens", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Lebens satt, und kaum des Lebens werth,", "tokens": ["Des", "Le\u00b7bens", "satt", ",", "und", "kaum", "des", "Le\u00b7bens", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lag ich, mit tr\u00e4gentschlafner Seele,", "tokens": ["Lag", "ich", ",", "mit", "tr\u00e4\u00b7gent\u00b7schlaf\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf weichem Moos in meiner H\u00f6le;", "tokens": ["Auf", "wei\u00b7chem", "Moos", "in", "mei\u00b7ner", "H\u00f6\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich f\u00fctterte der Knecht, und w\u00e4rmte meinen Heerd.", "tokens": ["Mich", "f\u00fct\u00b7ter\u00b7te", "der", "Knecht", ",", "und", "w\u00e4rm\u00b7te", "mei\u00b7nen", "Heerd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Au\u00dfen war der Schmuck der Felder", "tokens": ["Au\u00b7\u00dfen", "war", "der", "Schmuck", "der", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Wintersturm hinweg geraubt;", "tokens": ["Vom", "Win\u00b7ter\u00b7sturm", "hin\u00b7weg", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jeder Ast der W\u00e4lder", "tokens": ["Je\u00b7der", "Ast", "der", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit Zapfeneis und Flockenschnee belaubt.", "tokens": ["Mit", "Zap\u00b7fen\u00b7eis", "und", "Flo\u00b7cken\u00b7schnee", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur selten blinkte durch die Nebeldecken", "tokens": ["Nur", "sel\u00b7ten", "blink\u00b7te", "durch", "die", "Ne\u00b7bel\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der sp\u00e4ten Sonne Blick:", "tokens": ["Der", "sp\u00e4\u00b7ten", "Son\u00b7ne", "Blick", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bald zog sie, wie voll Schrecken,", "tokens": ["Bald", "zog", "sie", ",", "wie", "voll", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich hinter ihr Geb\u00fcrge zur\u00fcck.", "tokens": ["Sich", "hin\u00b7ter", "ihr", "Ge\u00b7b\u00fcr\u00b7ge", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Da scholl durch die ein\u00f6de Haide", "tokens": ["Da", "scholl", "durch", "die", "ein\u00b7\u00f6\u00b7de", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hungriger W\u00f6lfe Gebr\u00fcll;", "tokens": ["Hung\u00b7ri\u00b7ger", "W\u00f6l\u00b7fe", "Ge\u00b7br\u00fcll", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Da schwieg das fr\u00fche Lied der Freude,", "tokens": ["Da", "schwieg", "das", "fr\u00fc\u00b7he", "Lied", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und alle Harfen waren still.", "tokens": ["Und", "al\u00b7le", "Har\u00b7fen", "wa\u00b7ren", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Einsamkeit und Trauer", "tokens": ["Ein\u00b7sam\u00b7keit", "und", "Trau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Machten um mich her ein Grab,", "tokens": ["Mach\u00b7ten", "um", "mich", "her", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APZR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und mir war, als k\u00e4m' der Schauer", "tokens": ["Und", "mir", "war", ",", "als", "k\u00e4m'", "der", "Schau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Des Todes \u00fcber mich herab.", "tokens": ["Des", "To\u00b7des", "\u00fc\u00b7ber", "mich", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber die Nachtigall ruft!", "tokens": ["A\u00b7ber", "die", "Nach\u00b7ti\u00b7gall", "ruft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Es keimt das Feld, es gl\u00e4nzt die Luft;", "tokens": ["Es", "keimt", "das", "Feld", ",", "es", "gl\u00e4nzt", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Milde Sonnenstrahlen schweben,", "tokens": ["Mil\u00b7de", "Son\u00b7nen\u00b7strah\u00b7len", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen knospen hervor;", "tokens": ["Blu\u00b7men", "knos\u00b7pen", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und mit freudigem Leben", "tokens": ["Und", "mit", "freu\u00b7di\u00b7gem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ringt sich mein Geist empor.", "tokens": ["Ringt", "sich", "mein", "Geist", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "O la\u00dft mich, la\u00dft mich ganz erquicken", "tokens": ["O", "la\u00dft", "mich", ",", "la\u00dft", "mich", "ganz", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der balsamirten L\u00fcfte Wehn!", "tokens": ["Der", "bal\u00b7sa\u00b7mir\u00b7ten", "L\u00fcf\u00b7te", "Wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft mich das erste Veilchen pfl\u00fccken,", "tokens": ["La\u00dft", "mich", "das", "ers\u00b7te", "Veil\u00b7chen", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mein' entnebelten Augen sehn!", "tokens": ["Das", "mein'", "ent\u00b7ne\u00b7bel\u00b7ten", "Au\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O da\u00df ich, wie mit Schwalbenfl\u00fcgeln,", "tokens": ["O", "da\u00df", "ich", ",", "wie", "mit", "Schwal\u00b7ben\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Nu, vom Thale zu den H\u00fcgeln,", "tokens": ["Im", "Nu", ",", "vom", "Tha\u00b7le", "zu", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von dar mich hoch gen Himmel d\u00fcrfte drehn,", "tokens": ["Von", "dar", "mich", "hoch", "gen", "Him\u00b7mel", "d\u00fcrf\u00b7te", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "PPER", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Um \u00fcberall die H\u00f6len heiter,", "tokens": ["Um", "\u00fc\u00b7be\u00b7rall", "die", "H\u00f6\u00b7len", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die keimenden W\u00e4lder, die Berge voll Kr\u00e4uter,", "tokens": ["Die", "kei\u00b7men\u00b7den", "W\u00e4l\u00b7der", ",", "die", "Ber\u00b7ge", "voll", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die silberrieselnden B\u00e4che zu sehn!", "tokens": ["Die", "sil\u00b7ber\u00b7rie\u00b7seln\u00b7den", "B\u00e4\u00b7che", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Hervor, hervor wie diese Sprossen,", "tokens": ["Her\u00b7vor", ",", "her\u00b7vor", "wie", "die\u00b7se", "Spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du jugendliche Schar!", "tokens": ["Du", "ju\u00b7gend\u00b7li\u00b7che", "Schar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb, J\u00fcngling, deinen gier'gen Rossen", "tokens": ["Gieb", ",", "J\u00fcng\u00b7ling", ",", "dei\u00b7nen", "gier'\u00b7gen", "Ros\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ersten zarten Blumensprossen,", "tokens": ["Die", "ers\u00b7ten", "zar\u00b7ten", "Blu\u00b7men\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sch\u00e4rfe deinen Spie\u00df, und kn\u00fcpfe dir das Haar.", "tokens": ["Und", "sch\u00e4r\u00b7fe", "dei\u00b7nen", "Spie\u00df", ",", "und", "kn\u00fcp\u00b7fe", "dir", "das", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann merke, wo die Ehre winkt,", "tokens": ["Dann", "mer\u00b7ke", ",", "wo", "die", "Eh\u00b7re", "winkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und horche, wo der Bogen klingt,", "tokens": ["Und", "hor\u00b7che", ",", "wo", "der", "Bo\u00b7gen", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sammle dir in deinem Lenze", "tokens": ["Und", "samm\u00b7le", "dir", "in", "dei\u00b7nem", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Ruhmes ewig frische Kr\u00e4nze.", "tokens": ["Des", "Ruh\u00b7mes", "e\u00b7wig", "fri\u00b7sche", "Kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Oder h\u00f6rest du die sch\u00f6nen", "tokens": ["O\u00b7der", "h\u00f6\u00b7rest", "du", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bardenlieder lieber t\u00f6nen?", "tokens": ["Bar\u00b7den\u00b7lie\u00b7der", "lie\u00b7ber", "t\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So mu\u00dft du dich", "tokens": ["So", "mu\u00dft", "du", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Weisheit heiligen,", "tokens": ["Der", "Weis\u00b7heit", "hei\u00b7li\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Eichenlaub dich kr\u00f6nen;", "tokens": ["Mit", "Ei\u00b7chen\u00b7laub", "dich", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dann tritt zum Barden hin und sprich:", "tokens": ["Dann", "tritt", "zum", "Bar\u00b7den", "hin", "und", "sprich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O weihe du mir eine Harfe", "tokens": ["O", "wei\u00b7he", "du", "mir", "ei\u00b7ne", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und unterweise mich darauf,", "tokens": ["Und", "un\u00b7ter\u00b7wei\u00b7se", "mich", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich, was reizend ist, besinge,", "tokens": ["Da\u00df", "ich", ",", "was", "rei\u00b7zend", "ist", ",", "be\u00b7sin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Lenze bis zur Tugend hinauf.", "tokens": ["Vom", "Len\u00b7ze", "bis", "zur", "Tu\u00b7gend", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Da\u00df ich nicht wie das dumme Thier", "tokens": ["Da\u00df", "ich", "nicht", "wie", "das", "dum\u00b7me", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf diesen bunten Auen spiele;", "tokens": ["Auf", "die\u00b7sen", "bun\u00b7ten", "Au\u00b7en", "spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht dieser Eiche Pracht, nicht dieser Blume Zier,", "tokens": ["Nicht", "die\u00b7ser", "Ei\u00b7che", "Pracht", ",", "nicht", "die\u00b7ser", "Blu\u00b7me", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "NN", "$,", "PTKNEG", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und keinen Dank, noch kein Entz\u00fccken f\u00fchle.", "tokens": ["Und", "kei\u00b7nen", "Dank", ",", "noch", "kein", "Ent\u00b7z\u00fc\u00b7cken", "f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Drum g\u00f6nne mir der Weisheit Lehre", "tokens": ["Drum", "g\u00f6n\u00b7ne", "mir", "der", "Weis\u00b7heit", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weil ich noch lernen kann;", "tokens": ["Weil", "ich", "noch", "ler\u00b7nen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Damit die Enkelwelt einst meine Lieder ehre,", "tokens": ["Da\u00b7mit", "die", "En\u00b7kel\u00b7welt", "einst", "mei\u00b7ne", "Lie\u00b7der", "eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch mehr, den guten greisen Mann.", "tokens": ["Noch", "mehr", ",", "den", "gu\u00b7ten", "grei\u00b7sen", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}