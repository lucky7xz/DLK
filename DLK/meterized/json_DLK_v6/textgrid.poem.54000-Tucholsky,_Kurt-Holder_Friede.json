{"textgrid.poem.54000": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Holder Friede", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun senkt sich auf die Fluren nieder", "tokens": ["Nun", "senkt", "sich", "auf", "die", "Flu\u00b7ren", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der s\u00fc\u00dfe Tran der Vorkriegszeit;", "tokens": ["der", "s\u00fc\u00b7\u00dfe", "Tran", "der", "Vor\u00b7kriegs\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es kehren Ruh und Stille wieder,", "tokens": ["es", "keh\u00b7ren", "Ruh", "und", "Stil\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "getretener Quark wird weich und breit.", "tokens": ["ge\u00b7tre\u00b7te\u00b7ner", "Quark", "wird", "weich", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und alle atmen auf hienieden:", "tokens": ["Und", "al\u00b7le", "at\u00b7men", "auf", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt haben wir Frieden.", "tokens": ["Jetzt", "ha\u00b7ben", "wir", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun ist es Herbst. Die Storchenpaare", "tokens": ["Nun", "ist", "es", "Herbst", ".", "Die", "Stor\u00b7chen\u00b7paa\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "stehn klappernd, und der Eichbaum schwankt.", "tokens": ["stehn", "klap\u00b7pernd", ",", "und", "der", "Eich\u00b7baum", "schwankt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das ist ja wohl die Zeit im Jahre,", "tokens": ["Das", "ist", "ja", "wohl", "die", "Zeit", "im", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wo Engel sich mit Brechten zankt.", "tokens": ["wo", "En\u00b7gel", "sich", "mit", "Brech\u00b7ten", "zankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ehe wird noch oft geschieden.", "tokens": ["Die", "E\u00b7he", "wird", "noch", "oft", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt haben wir Frieden.", "tokens": ["Jetzt", "ha\u00b7ben", "wir", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir wollen nur das eine wissen,", "tokens": ["Wir", "wol\u00b7len", "nur", "das", "ei\u00b7ne", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "weil uns das wirklich interessiert:", "tokens": ["weil", "uns", "das", "wirk\u00b7lich", "in\u00b7ter\u00b7es\u00b7siert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Premierenknatsch in den Kulissen \u2013", "tokens": ["Pre\u00b7mie\u00b7ren\u00b7knatsch", "in", "den", "Ku\u00b7lis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ob Kortner Je\u00dfner engagiert?", "tokens": ["ob", "Kort\u00b7ner", "Je\u00df\u00b7ner", "en\u00b7ga\u00b7giert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Baut Laemmle pappene Pyramiden?", "tokens": ["Baut", "Laemm\u00b7le", "pap\u00b7pe\u00b7ne", "Py\u00b7ra\u00b7mi\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jetzt haben wir Frieden.", "tokens": ["Jetzt", "ha\u00b7ben", "wir", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir geben einer m\u00fcden Masse", "tokens": ["Wir", "ge\u00b7ben", "ei\u00b7ner", "m\u00fc\u00b7den", "Mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zum Ansehn, was sie niemals hat.", "tokens": ["zum", "An\u00b7sehn", ",", "was", "sie", "nie\u00b7mals", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbin Schiffskabinen erster Klasse", "tokens": ["\u00bb", "in", "Schiffs\u00b7ka\u00b7bi\u00b7nen", "ers\u00b7ter", "Klas\u00b7se"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "gibt es jetzt Radio, Turnsaal, Bad . . . !\u00ab", "tokens": ["gibt", "es", "jetzt", "Ra\u00b7dio", ",", "Turn\u00b7saal", ",", "Bad", ".", ".", ".", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "$,", "NN", "$,", "NE", "$.", "$.", "$.", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Vergessen sind die Invaliden \u2013", "tokens": ["Ver\u00b7ges\u00b7sen", "sind", "die", "In\u00b7va\u00b7li\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "jetzt haben wir Frieden.", "tokens": ["jetzt", "ha\u00b7ben", "wir", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Verrauscht ist L\u00e4rm und Trommelfeuer,", "tokens": ["Ver\u00b7rauscht", "ist", "L\u00e4rm", "und", "Trom\u00b7mel\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "verweht das Leid der Inflation.", "tokens": ["ver\u00b7weht", "das", "Leid", "der", "In\u00b7fla\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir hassen jedes Abenteuer \u2013", "tokens": ["Wir", "has\u00b7sen", "je\u00b7des", "A\u00b7bent\u00b7eu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wir wollen nicht mehr. Wir haben schon.", "tokens": ["wir", "wol\u00b7len", "nicht", "mehr", ".", "Wir", "ha\u00b7ben", "schon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir pfeifen auf dem ersten Loche.", "tokens": ["Wir", "pfei\u00b7fen", "auf", "dem", "ers\u00b7ten", "Lo\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun liegt schon alles weit entfernt . . .", "tokens": ["Nun", "liegt", "schon", "al\u00b7les", "weit", "ent\u00b7fernt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "ADJD", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir spielen Metternich-Epoche", "tokens": ["Wir", "spie\u00b7len", "Met\u00b7ter\u00b7nich\u00b7E\u00b7po\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "und haben nichts dazugelernt.", "tokens": ["und", "ha\u00b7ben", "nichts", "da\u00b7zu\u00b7ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}