{"textgrid.poem.39813": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Fl\u00fcchtling", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wirst mir vor der Seele stehen,", "tokens": ["Du", "wirst", "mir", "vor", "der", "See\u00b7le", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lang mein Geist noch Bilder treibt,", "tokens": ["So", "lang", "mein", "Geist", "noch", "Bil\u00b7der", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang mein Blick, was er gesehen,", "tokens": ["So", "lang", "mein", "Blick", ",", "was", "er", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch vor sich in die L\u00fcfte schreibt;", "tokens": ["Noch", "vor", "sich", "in", "die", "L\u00fcf\u00b7te", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf feste Schultern hoch gegr\u00fcndet", "tokens": ["Auf", "fes\u00b7te", "Schul\u00b7tern", "hoch", "ge\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Haupt, vom Kummer nicht gebeugt,", "tokens": ["Ein", "Haupt", ",", "vom", "Kum\u00b7mer", "nicht", "ge\u00b7beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Finger straff zur Faust ger\u00fcndet,", "tokens": ["Die", "Fin\u00b7ger", "straff", "zur", "Faust", "ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Blick aus Licht und Nacht gezeugt.", "tokens": ["Der", "Blick", "aus", "Licht", "und", "Nacht", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Sinn gedr\u00e4ngt, schwoll dir die Stimme,", "tokens": ["Vom", "Sinn", "ge\u00b7dr\u00e4ngt", ",", "schwoll", "dir", "die", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie R\u00f6merwort her\u00fcberschallt;", "tokens": ["Wie", "R\u00f6\u00b7mer\u00b7wort", "her\u00b7\u00fc\u00b7bersc\u00b7hallt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, deine Rede gab vom Grimme", "tokens": ["Ja", ",", "dei\u00b7ne", "Re\u00b7de", "gab", "vom", "Grim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Schicksals uns den Vollgehalt.", "tokens": ["Des", "Schick\u00b7sals", "uns", "den", "Voll\u00b7ge\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Menschenwahn dazuges\u00fcndigt,", "tokens": ["Wie", "Men\u00b7schen\u00b7wahn", "da\u00b7zu\u00b7ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was Thorheit und Verblendung that,", "tokens": ["Was", "Thor\u00b7heit", "und", "Ver\u00b7blen\u00b7dung", "that", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ward ruhig klar von dir verk\u00fcndigt;", "tokens": ["Ward", "ru\u00b7hig", "klar", "von", "dir", "ver\u00b7k\u00fcn\u00b7digt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur donnernd sprachst du vom Verrat:", "tokens": ["Nur", "don\u00b7nernd", "sprachst", "du", "vom", "Ver\u00b7rat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von Einem, \u00bbder im Heimatgarten", "tokens": ["Von", "Ei\u00b7nem", ",", "\u00bb", "der", "im", "Hei\u00b7mat\u00b7gar\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "$(", "ART", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aufwuchs, ein unfruchtbarer Baum,", "tokens": ["Auf\u00b7wuchs", ",", "ein", "un\u00b7frucht\u00b7ba\u00b7rer", "Baum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bei Gelag' und schn\u00f6den Karten", "tokens": ["Der", "bei", "Ge\u00b7lag'", "und", "schn\u00f6\u00b7den", "Kar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verd\u00e4mmerte des Lebens Traum;", "tokens": ["Ver\u00b7d\u00e4m\u00b7mer\u00b7te", "des", "Le\u00b7bens", "Traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der in der Knechtschaft schwersten Tagen", "tokens": ["Der", "in", "der", "Knecht\u00b7schaft", "schwers\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Greis ein junges Weib gefreit,", "tokens": ["Als", "Greis", "ein", "jun\u00b7ges", "Weib", "ge\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, seinen Arm um sie geschlagen,", "tokens": ["Und", ",", "sei\u00b7nen", "Arm", "um", "sie", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durcht\u00e4ndelte die Jammerzeit.", "tokens": ["Durch\u00b7t\u00e4n\u00b7del\u00b7te", "die", "Jam\u00b7mer\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als nun im Fieber seine Ketten", "tokens": ["Als", "nun", "im", "Fie\u00b7ber", "sei\u00b7ne", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das kranke Vaterland zerbrach,", "tokens": ["Das", "kran\u00b7ke", "Va\u00b7ter\u00b7land", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rafft' er sich auf, als g\u00e4lt' es retten,", "tokens": ["Rafft'", "er", "sich", "auf", ",", "als", "g\u00e4lt'", "es", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sann, Freiheit jauchzend, er auf Schmach.", "tokens": ["Sann", ",", "Frei\u00b7heit", "jauch\u00b7zend", ",", "er", "auf", "Schmach", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVPP", "$,", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er war der Tyrannei Verwalter", "tokens": ["Er", "war", "der", "Ty\u00b7ran\u00b7nei", "Ver\u00b7wal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf ihrem umgest\u00fcrzten Thron,", "tokens": ["Auf", "ih\u00b7rem", "um\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und lie\u00df sein silberhaarig Alter", "tokens": ["Und", "lie\u00df", "sein", "sil\u00b7ber\u00b7haa\u00b7rig", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergolden sich mit Feindeslohn.\u00ab", "tokens": ["Ver\u00b7gol\u00b7den", "sich", "mit", "Fein\u00b7des\u00b7lohn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du riefst: \u00bbWeh diesem! der empfinde", "tokens": ["Du", "riefst", ":", "\u00bb", "Weh", "die\u00b7sem", "!", "der", "emp\u00b7fin\u00b7de"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "PDAT", "$.", "ART", "ADJA"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gott als des ", "tokens": ["Gott", "als", "des"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOKOM", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "In seinem sp\u00e4tgezeugten Kinde", "tokens": ["In", "sei\u00b7nem", "sp\u00e4t\u00b7ge\u00b7zeug\u00b7ten", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00fc\u00df' er den frech getriebnen Spott!", "tokens": ["B\u00fc\u00df'", "er", "den", "frech", "ge\u00b7trieb\u00b7nen", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Quell der Pein, der ihm nicht qu\u00f6lle,", "tokens": ["Kein", "Quell", "der", "Pein", ",", "der", "ihm", "nicht", "qu\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis ihn hinunterschlingt die Flut;", "tokens": ["Bis", "ihn", "hin\u00b7un\u00b7ter\u00b7schlingt", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und drunten eine eigne H\u00f6lle,", "tokens": ["Und", "drun\u00b7ten", "ei\u00b7ne", "eig\u00b7ne", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gemeine Qual ist viel zu gut!\u00ab", "tokens": ["Ge\u00b7mei\u00b7ne", "Qual", "ist", "viel", "zu", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ernstkr\u00e4ftig wiegtest du den wackern,", "tokens": ["Ernst\u00b7kr\u00e4f\u00b7tig", "wieg\u00b7test", "du", "den", "wa\u00b7ckern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den schwertgewohnten Heldenarm:", "tokens": ["Den", "schwert\u00b7ge\u00b7wohn\u00b7ten", "Hel\u00b7de\u00b7narm", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmu\u00df ich auf fremdem Boden ackern,\u00ab", "tokens": ["\u00bb", "mu\u00df", "ich", "auf", "frem\u00b7dem", "Bo\u00b7den", "ac\u00b7kern", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprachst du, \u00bbdas thu' ich ohne Harm!", "tokens": ["Sprachst", "du", ",", "\u00bb", "das", "thu'", "ich", "oh\u00b7ne", "Harm", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PDS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gern irr' ich, wie ein Misseth\u00e4ter,", "tokens": ["Gern", "irr'", "ich", ",", "wie", "ein", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Elends Stecken in der Hand,", "tokens": ["Des", "E\u00b7lends", "Ste\u00b7cken", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur weit, recht weit von dem Verr\u00e4ter,", "tokens": ["Nur", "weit", ",", "recht", "weit", "von", "dem", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vom unterjochten Vaterland!", "tokens": ["Vom", "un\u00b7ter\u00b7joch\u00b7ten", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O M\u00e4nner, die mit finstrem Sinnen", "tokens": ["O", "M\u00e4n\u00b7ner", ",", "die", "mit", "fins\u00b7trem", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr seht, wie unser W\u00fcrfel fiel,", "tokens": ["Ihr", "seht", ",", "wie", "un\u00b7ser", "W\u00fcr\u00b7fel", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glaubt's: w\u00e4re wieder zu beginnen,", "tokens": ["Glaubt's", ":", "w\u00e4\u00b7re", "wie\u00b7der", "zu", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder Untergang das Ziel:", "tokens": ["Und", "wie\u00b7der", "Un\u00b7ter\u00b7gang", "das", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir schaarten wieder uns zum Heere,", "tokens": ["Wir", "schaar\u00b7ten", "wie\u00b7der", "uns", "zum", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir spr\u00e4chen: Henker, g\u00fcrte dich!", "tokens": ["Wir", "spr\u00e4\u00b7chen", ":", "Hen\u00b7ker", ",", "g\u00fcr\u00b7te", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", 