{"textgrid.poem.60634": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu schwach ist keiner und zu klein \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu schwach ist keiner und zu klein \u2013", "tokens": ["Zu", "schwach", "ist", "kei\u00b7ner", "und", "zu", "klein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PIS", "KON", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kann dem andern dienlich sein.", "tokens": ["Er", "kann", "dem", "an\u00b7dern", "dien\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus einem klaren B\u00e4chlein trank", "tokens": ["Aus", "ei\u00b7nem", "kla\u00b7ren", "B\u00e4ch\u00b7lein", "trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein T\u00e4ubchen in dem Augenblick,", "tokens": ["Ein", "T\u00e4ub\u00b7chen", "in", "dem", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als eine Ameis niedersank", "tokens": ["Als", "ei\u00b7ne", "Am\u00b7eis", "nie\u00b7der\u00b7sank"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Wasser, das f\u00fcr sie ein Meer.", "tokens": ["Ins", "Was\u00b7ser", ",", "das", "f\u00fcr", "sie", "ein", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Sie h\u00e4tte nie ans Land zur\u00fcck", "tokens": ["Sie", "h\u00e4t\u00b7te", "nie", "ans", "Land", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich retten k\u00f6nnen, wenn ihr nicht", "tokens": ["Sich", "ret\u00b7ten", "k\u00f6n\u00b7nen", ",", "wenn", "ihr", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "VMINF", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Taube beigesprungen w\u00e4r.", "tokens": ["Die", "Tau\u00b7be", "bei\u00b7ge\u00b7sprun\u00b7gen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die f\u00fchlte des Erbarmens Pflicht", "tokens": ["Die", "f\u00fchl\u00b7te", "des", "Er\u00b7bar\u00b7mens", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und warf ein H\u00e4lmchen Gras hinab,", "tokens": ["Und", "warf", "ein", "H\u00e4lm\u00b7chen", "Gras", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das der Ertrinkenden die Br\u00fccke gab,", "tokens": ["Das", "der", "Er\u00b7trin\u00b7ken\u00b7den", "die", "Br\u00fc\u00b7cke", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auf der sie eilig lief zum Uferrand.", "tokens": ["Auf", "der", "sie", "ei\u00b7lig", "lief", "zum", "Uf\u00b7er\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als sie auf festem Boden sich befand", "tokens": ["Als", "sie", "auf", "fes\u00b7tem", "Bo\u00b7den", "sich", "be\u00b7fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und ihre Retterin zum Baum geflogen,", "tokens": ["Und", "ih\u00b7re", "Ret\u00b7te\u00b7rin", "zum", "Baum", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da kam ein wilder Mann des Wegs gezogen", "tokens": ["Da", "kam", "ein", "wil\u00b7der", "Mann", "des", "Wegs", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Verlumpt und barfu\u00df, der die Taube sah", "tokens": ["Ver\u00b7lumpt", "und", "bar\u00b7fu\u00df", ",", "der", "die", "Tau\u00b7be", "sah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und gierig griff zu Pfeil und Bogen.", "tokens": ["Und", "gie\u00b7rig", "griff", "zu", "Pfeil", "und", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Er meinte, da\u00df der Vogel da", "tokens": ["Er", "mein\u00b7te", ",", "da\u00df", "der", "Vo\u00b7gel", "da"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als guter Schmaus ihm sei gesandt,", "tokens": ["Als", "gu\u00b7ter", "Schmaus", "ihm", "sei", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und will ihn t\u00f6ten. Wie er spannt", "tokens": ["Und", "will", "ihn", "t\u00f6\u00b7ten", ".", "Wie", "er", "spannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$.", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Sehne und verborgen zielt,", "tokens": ["Die", "Seh\u00b7ne", "und", "ver\u00b7bor\u00b7gen", "zielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da bei\u00dft die Ameis ihn ins Bein,", "tokens": ["Da", "bei\u00dft", "die", "Am\u00b7eis", "ihn", "ins", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.22": {"text": "Wobei er sich nicht still verhielt,", "tokens": ["Wo\u00b7bei", "er", "sich", "nicht", "still", "ver\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Denn solch ein Bi\u00df bereitet Pein.", "tokens": ["Denn", "solch", "ein", "Bi\u00df", "be\u00b7rei\u00b7tet", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nun merkt die Taube, was ihr droht,", "tokens": ["Nun", "merkt", "die", "Tau\u00b7be", ",", "was", "ihr", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und schnell enteilet sie dem Tod.", "tokens": ["Und", "schnell", "ent\u00b7ei\u00b7let", "sie", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Lebwohl, du sch\u00f6nes Festgericht,", "tokens": ["Leb\u00b7wohl", ",", "du", "sch\u00f6\u00b7nes", "Fest\u00b7ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So billig war die Taube nicht!", "tokens": ["So", "bil\u00b7lig", "war", "die", "Tau\u00b7be", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}