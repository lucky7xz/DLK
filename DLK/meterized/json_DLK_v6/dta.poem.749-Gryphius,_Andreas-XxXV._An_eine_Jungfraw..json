{"dta.poem.749": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.  An eine Jungfraw.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ob zwar ewr eigen lob hier gar nicht ist zu schawen/ ", "tokens": ["Ob", "zwar", "ewr", "ei\u00b7gen", "lob", "hier", "gar", "nicht", "ist", "zu", "scha\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "NN", "ADV", "ADV", "PTKNEG", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "So nembt doch von mir an was dieser feder pflicht", "tokens": ["So", "nembt", "doch", "von", "mir", "an", "was", "die\u00b7ser", "fe\u00b7der", "pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PRELS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd meine Pieris hat andern auffgericht", "tokens": ["Vnd", "mei\u00b7ne", "Pie\u00b7ris", "hat", "an\u00b7dern", "auff\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIS", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die jhr nur vbertrefft/ O Crone der Jungfrawen/ ", "tokens": ["Die", "jhr", "nur", "vbert\u00b7refft", "/", "O", "Cro\u00b7ne", "der", "Jung\u00b7fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$(", "NE", "NE", "ART", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die schrancken sindt zu klein; dem heiligen vertrawen/", "tokens": ["Die", "schran\u00b7cken", "sindt", "zu", "klein", ";", "dem", "hei\u00b7li\u00b7gen", "ver\u00b7tra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VAFIN", "PTKA", "ADJD", "$.", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Demut/ der vernunfft/ der hellen Tugendt licht/", "tokens": ["Der", "De\u00b7mut", "/", "der", "ver\u00b7nunfft", "/", "der", "hel\u00b7len", "Tu\u00b7gendt", "licht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem keusch vnd sitsamb seyn/ dem him\u0303lischen gesicht ", "tokens": ["Dem", "keusch", "vnd", "sit\u00b7samb", "seyn", "/", "dem", "him\u0303\u00b7li\u00b7schen", "ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAINF", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan in so kurtzer schrifft ich kein Altgr auff bawen.", "tokens": ["Kan", "in", "so", "kurt\u00b7zer", "schrifft", "ich", "kein", "Alt\u00b7gr", "auff", "ba\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADV", "ADJA", "VVFIN", "PPER", "PIAT", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Vnd fing ich dennoch an/ wo bliebe der verstandt/", "tokens": ["Vnd", "fing", "ich", "den\u00b7noch", "an", "/", "wo", "blie\u00b7be", "der", "ver\u00b7standt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "PWAV", "VVFIN", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jugend/ das geschlecht/ des hohen gl\u00fcckes pfandt/ ", "tokens": ["Die", "ju\u00b7gend", "/", "das", "ge\u00b7schlecht", "/", "des", "ho\u00b7hen", "gl\u00fc\u00b7ckes", "pfandt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VVPP", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mutter freundligkeit/ des ", "tokens": ["Der", "Mut\u00b7ter", "freund\u00b7lig\u00b7keit", "/", "des"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vnd beider tewrer ruhm/ der gantz euch einverleibt/", "tokens": ["Vnd", "bei\u00b7der", "tew\u00b7rer", "ruhm", "/", "der", "gantz", "euch", "ein\u00b7ver\u00b7leibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$(", "ART", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd was noch mehr ein Geist wie frey er jmmer schreibt", "tokens": ["Vnd", "was", "noch", "mehr", "ein", "Geist", "wie", "frey", "er", "jm\u00b7mer", "schreibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADV", "ART", "NN", "KOKOM", "ADJD", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie hoch er jmmer geht/ nicht recht hat preisen k\u00f6nnen.", "tokens": ["Wie", "hoch", "er", "jm\u00b7mer", "geht", "/", "nicht", "recht", "hat", "prei\u00b7sen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$(", "PTKNEG", "ADJD", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}