{"textgrid.poem.54826": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "40. An Serenum", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drusilla hat uns nechst ein R\u00e4tzel vorgeschlagen/", "tokens": ["Dru\u00b7sil\u00b7la", "hat", "uns", "nechst", "ein", "R\u00e4t\u00b7zel", "vor\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dem der es erriht/ zu Danck sich angetragen:", "tokens": ["Und", "dem", "der", "es", "er\u00b7riht", "/", "zu", "Danck", "sich", "an\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRELS", "PPER", "VVFIN", "$(", "PTKZU", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schwig/ nicht umb da\u00df mir die Meinung nicht bewust;", "tokens": ["Ich", "schwig", "/", "nicht", "umb", "da\u00df", "mir", "die", "Mei\u00b7nung", "nicht", "be\u00b7wust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "PTKNEG", "APPR", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur/ weil ich zu dem Prei\u00df trug gar geringe Lust.", "tokens": ["Nur", "/", "weil", "ich", "zu", "dem", "Prei\u00df", "trug", "gar", "ge\u00b7rin\u00b7ge", "Lust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}