{"textgrid.poem.39453": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wes ist das Lied, das mit geweihten Zungen", "tokens": ["Wes", "ist", "das", "Lied", ",", "das", "mit", "ge\u00b7weih\u00b7ten", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weltalls H\u00f6hn und Tiefen ernst verk\u00fcndet;", "tokens": ["Des", "Welt\u00b7alls", "H\u00f6hn", "und", "Tie\u00b7fen", "ernst", "ver\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erst langsam durch des Abgrunds Nacht sich windet,", "tokens": ["Erst", "lang\u00b7sam", "durch", "des", "Ab\u00b7grunds", "Nacht", "sich", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Pr\u00fcfung Gipfel k\u00fchner schon errungen;", "tokens": ["Der", "Pr\u00fc\u00b7fung", "Gip\u00b7fel", "k\u00fch\u00b7ner", "schon", "er\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann, neu gekr\u00e4ftigt, himmelan gedrungen,", "tokens": ["Dann", ",", "neu", "ge\u00b7kr\u00e4f\u00b7tigt", ",", "him\u00b7me\u00b7lan", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Religion und Poesie verb\u00fcndet", "tokens": ["Da\u00df", "Re\u00b7li\u00b7gi\u00b7on", "und", "Poe\u00b7sie", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch nie so Cherubinen-gleich entz\u00fcndet", "tokens": ["Noch", "nie", "so", "Che\u00b7ru\u00b7bi\u00b7nen\u00b7gleich", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich mit den Sph\u00e4ren schwungen und erklungen?", "tokens": ["Sich", "mit", "den", "Sph\u00e4\u00b7ren", "schwun\u00b7gen", "und", "er\u00b7klun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zugleich der Tempel und des Baues Meister,", "tokens": ["Zu\u00b7gleich", "der", "Tem\u00b7pel", "und", "des", "Bau\u00b7es", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schuf die\u00df lebend'ge Grabmal seiner Liebe,", "tokens": ["Schuf", "die\u00df", "le\u00b7ben\u00b7d'\u00b7ge", "Grab\u00b7mal", "sei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die er, beseligt, Beatrice nannte,", "tokens": ["Die", "er", ",", "be\u00b7se\u00b7ligt", ",", "Be\u00b7at\u00b7ri\u00b7ce", "nann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVPP", "$,", "NE", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Verbannt hier, B\u00fcrger nur im Reich der Geister,", "tokens": ["Ver\u00b7bannt", "hier", ",", "B\u00fcr\u00b7ger", "nur", "im", "Reich", "der", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo in der Gottheit Schaun die Kraft dem Triebe", "tokens": ["Wo", "in", "der", "Got\u00b7theit", "Schaun", "die", "Kraft", "dem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mehr erliegen mu\u00df, der gro\u00dfe Dante.", "tokens": ["Nicht", "mehr", "er\u00b7lie\u00b7gen", "mu\u00df", ",", "der", "gro\u00b7\u00dfe", "Dan\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}