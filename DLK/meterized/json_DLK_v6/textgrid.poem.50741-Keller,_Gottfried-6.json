{"textgrid.poem.50741": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So werd ich manchmal irre an der Stunde,", "tokens": ["So", "werd", "ich", "manch\u00b7mal", "ir\u00b7re", "an", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Tag und Jahr, ach, an der ganzen Zeit!", "tokens": ["An", "Tag", "und", "Jahr", ",", "ach", ",", "an", "der", "gan\u00b7zen", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ITJ", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie g\u00e4rt, sie tost, doch mitten auf dem Grunde", "tokens": ["Sie", "g\u00e4rt", ",", "sie", "tost", ",", "doch", "mit\u00b7ten", "auf", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist es so still, so kalt und zugeschneit!", "tokens": ["Ist", "es", "so", "still", ",", "so", "kalt", "und", "zu\u00b7ge\u00b7schneit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Habt ihr euch auf ein neues Jahr gefreut,", "tokens": ["Habt", "ihr", "euch", "auf", "ein", "neu\u00b7es", "Jahr", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zukunft preisend mit beredtem Munde?", "tokens": ["Die", "Zu\u00b7kunft", "prei\u00b7send", "mit", "be\u00b7red\u00b7tem", "Mun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es rollt heran und schleudert weit, o weit!", "tokens": ["Es", "rollt", "he\u00b7ran", "und", "schleu\u00b7dert", "weit", ",", "o", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$,", "FM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur\u00fcck euch, ihr versinkt im alten Schlunde!", "tokens": ["Zu\u00b7r\u00fcck", "euch", ",", "ihr", "ver\u00b7sinkt", "im", "al\u00b7ten", "Schlun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O h\u00e4tt den Hammer ich des starken Thor,", "tokens": ["O", "h\u00e4tt", "den", "Ham\u00b7mer", "ich", "des", "star\u00b7ken", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf das Jahrhundert einen Schlag zu f\u00fchren,", "tokens": ["Auf", "das", "Jahr\u00b7hun\u00b7dert", "ei\u00b7nen", "Schlag", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich schl\u00fcg sein morsches Zeigerblatt zu Tr\u00fcmmern!", "tokens": ["Ich", "schl\u00fcg", "sein", "mor\u00b7sches", "Zei\u00b7ger\u00b7blatt", "zu", "Tr\u00fcm\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Tritt denn kein Uhrenmacher k\u00fchn hervor,", "tokens": ["Tritt", "denn", "kein", "Uh\u00b7ren\u00b7ma\u00b7cher", "k\u00fchn", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die irre Zeit mit Macht zu regulieren?", "tokens": ["Die", "ir\u00b7re", "Zeit", "mit", "Macht", "zu", "re\u00b7gu\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll sie denn ganz in Staub und Rost verk\u00fcmmern?", "tokens": ["Soll", "sie", "denn", "ganz", "in", "Staub", "und", "Rost", "ver\u00b7k\u00fcm\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}