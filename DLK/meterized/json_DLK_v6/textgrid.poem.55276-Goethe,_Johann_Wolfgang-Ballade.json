{"textgrid.poem.55276": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ballade", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbherein, o du Guter! du Alter, herein!", "tokens": ["\u00bb", "her\u00b7ein", ",", "o", "du", "Gu\u00b7ter", "!", "du", "Al\u00b7ter", ",", "her\u00b7ein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "FM", "PPER", "NN", "$.", "PPER", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hier unten im Saale, da sind wir allein,", "tokens": ["Hier", "un\u00b7ten", "im", "Saa\u00b7le", ",", "da", "sind", "wir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir wollen die Pforte verschlie\u00dfen.", "tokens": ["Wir", "wol\u00b7len", "die", "Pfor\u00b7te", "ver\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Mutter, sie betet, der Vater im Hain", "tokens": ["Die", "Mut\u00b7ter", ",", "sie", "be\u00b7tet", ",", "der", "Va\u00b7ter", "im", "Hain"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ist gangen, die W\u00f6lfe zu schie\u00dfen.", "tokens": ["Ist", "gan\u00b7gen", ",", "die", "W\u00f6l\u00b7fe", "zu", "schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "O sing uns ein M\u00e4rchen, o sing es uns oft,", "tokens": ["O", "sing", "uns", "ein", "M\u00e4r\u00b7chen", ",", "o", "sing", "es", "uns", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,", "FM", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Da\u00df ich und der Bruder es lerne,", "tokens": ["Da\u00df", "ich", "und", "der", "Bru\u00b7der", "es", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wir haben schon l\u00e4ngst einen S\u00e4nger gehofft \u2013", "tokens": ["Wir", "ha\u00b7ben", "schon", "l\u00e4ngst", "ei\u00b7nen", "S\u00e4n\u00b7ger", "ge\u00b7hofft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Die Kinder, sie h\u00f6ren es gerne.\u00ab", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "h\u00f6\u00b7ren", "es", "ger\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbim n\u00e4chtlichen Schrecken, im feindlichen Graus", "tokens": ["\u00bb", "im", "n\u00e4cht\u00b7li\u00b7chen", "Schre\u00b7cken", ",", "im", "feind\u00b7li\u00b7chen", "Graus"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verl\u00e4\u00dft er das hohe, das herrliche Haus,", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "das", "ho\u00b7he", ",", "das", "herr\u00b7li\u00b7che", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Sch\u00e4tze, die hat er vergraben.", "tokens": ["Die", "Sch\u00e4t\u00b7ze", ",", "die", "hat", "er", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Graf nun so eilig zum Pf\u00f6rtchen hinaus,", "tokens": ["Der", "Graf", "nun", "so", "ei\u00b7lig", "zum", "Pf\u00f6rt\u00b7chen", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Was mag er im Arme denn haben?", "tokens": ["Was", "mag", "er", "im", "Ar\u00b7me", "denn", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPRART", "NN", "KON", "VAFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Was birget er unter dem Mantel geschwind?", "tokens": ["Was", "bir\u00b7get", "er", "un\u00b7ter", "dem", "Man\u00b7tel", "ge\u00b7schwind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Was tr\u00e4gt er so rasch in die Ferne?", "tokens": ["Was", "tr\u00e4gt", "er", "so", "rasch", "in", "die", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ein T\u00f6chterlein ist es, da schl\u00e4ft nun das Kind.\u00ab \u2013", "tokens": ["Ein", "T\u00f6ch\u00b7ter\u00b7lein", "ist", "es", ",", "da", "schl\u00e4ft", "nun", "das", "Kind", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Kinder, sie h\u00f6ren es gerne.", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "h\u00f6\u00b7ren", "es", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbnun hellt sich der Morgen, die Welt ist so weit,", "tokens": ["\u00bb", "nun", "hellt", "sich", "der", "Mor\u00b7gen", ",", "die", "Welt", "ist", "so", "weit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In T\u00e4lern und W\u00e4ldern die Wohnung bereit,", "tokens": ["In", "T\u00e4\u00b7lern", "und", "W\u00e4l\u00b7dern", "die", "Woh\u00b7nung", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In D\u00f6rfern erquickt man den S\u00e4nger,", "tokens": ["In", "D\u00f6r\u00b7fern", "er\u00b7quickt", "man", "den", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So schreitet und heischt er undenkliche Zeit,", "tokens": ["So", "schrei\u00b7tet", "und", "heischt", "er", "un\u00b7denk\u00b7li\u00b7che", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Bart w\u00e4chst ihm l\u00e4nger und l\u00e4nger;", "tokens": ["Der", "Bart", "w\u00e4chst", "ihm", "l\u00e4n\u00b7ger", "und", "l\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Doch w\u00e4chst in dem Arme das liebliche Kind,", "tokens": ["Doch", "w\u00e4chst", "in", "dem", "Ar\u00b7me", "das", "lieb\u00b7li\u00b7che", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wie unter dem gl\u00fccklichsten Sterne,", "tokens": ["Wie", "un\u00b7ter", "dem", "gl\u00fcck\u00b7lichs\u00b7ten", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Gesch\u00fctzt in dem Mantel vor Regen und Wind.\u00ab \u2013", "tokens": ["Ge\u00b7sch\u00fctzt", "in", "dem", "Man\u00b7tel", "vor", "Re\u00b7gen", "und", "Wind", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Kinder, sie h\u00f6ren es gerne.", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "h\u00f6\u00b7ren", "es", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbund immer sind weiter die Jahre ger\u00fcckt,", "tokens": ["\u00bb", "und", "im\u00b7mer", "sind", "wei\u00b7ter", "die", "Jah\u00b7re", "ge\u00b7r\u00fcckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Mantel entf\u00e4rbt sich, der Mantel zerst\u00fcckt,", "tokens": ["Der", "Man\u00b7tel", "ent\u00b7f\u00e4rbt", "sich", ",", "der", "Man\u00b7tel", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er k\u00f6nnte sie l\u00e4nger nicht fassen.", "tokens": ["Er", "k\u00f6nn\u00b7te", "sie", "l\u00e4n\u00b7ger", "nicht", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Vater, er schaut sie, wie ist er begl\u00fcckt!", "tokens": ["Der", "Va\u00b7ter", ",", "er", "schaut", "sie", ",", "wie", "ist", "er", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Er kann sich f\u00fcr Freude nicht lassen;", "tokens": ["Er", "kann", "sich", "f\u00fcr", "Freu\u00b7de", "nicht", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So sch\u00f6n und so edel erscheint sie zugleich,", "tokens": ["So", "sch\u00f6n", "und", "so", "e\u00b7del", "er\u00b7scheint", "sie", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Entsprossen aus t\u00fcchtigem Kerne,", "tokens": ["Ent\u00b7spros\u00b7sen", "aus", "t\u00fcch\u00b7ti\u00b7gem", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wie macht sie den Vater, den teuren, so reich!\u00ab \u2013", "tokens": ["Wie", "macht", "sie", "den", "Va\u00b7ter", ",", "den", "teu\u00b7ren", ",", "so", "reich", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Kinder, sie h\u00f6ren es gerne.", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "h\u00f6\u00b7ren", "es", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbda reitet ein f\u00fcrstlicher Ritter heran,", "tokens": ["\u00bb", "da", "rei\u00b7tet", "ein", "f\u00fcrst\u00b7li\u00b7cher", "Rit\u00b7ter", "he\u00b7ran", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie recket die Hand aus, der Gabe zu nahn,", "tokens": ["Sie", "re\u00b7cket", "die", "Hand", "aus", ",", "der", "Ga\u00b7be", "zu", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Almosen will er nicht geben.", "tokens": ["Al\u00b7mo\u00b7sen", "will", "er", "nicht", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er fasset das H\u00e4ndchen so kr\u00e4ftiglich an:", "tokens": ["Er", "fas\u00b7set", "das", "H\u00e4nd\u00b7chen", "so", "kr\u00e4f\u00b7tig\u00b7lich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u203adie will ich\u2039, so ruft er, \u203aaufs Leben!\u2039", "tokens": ["\u203a", "die", "will", "ich", "\u2039", ",", "so", "ruft", "er", ",", "\u203a", "aufs", "Le\u00b7ben", "!", "\u2039"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "\u203aerkennst du\u2039, erwidert der Alte, \u203aden Schatz,", "tokens": ["\u203a", "er\u00b7kennst", "du", "\u2039", ",", "er\u00b7wi\u00b7dert", "der", "Al\u00b7te", ",", "\u203a", "den", "Schatz", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$(", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Erhebst du zur F\u00fcrstin sie gerne;", "tokens": ["Er\u00b7hebst", "du", "zur", "F\u00fcrs\u00b7tin", "sie", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Sie sei dir verlobet auf gr\u00fcnendem Platz.\u2039\u00ab \u2013", "tokens": ["Sie", "sei", "dir", "ver\u00b7lo\u00b7bet", "auf", "gr\u00fc\u00b7nen\u00b7dem", "Platz", ".", "\u2039", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Kinder, sie h\u00f6ren es gerne.", "tokens": ["Die", "Kin\u00b7der", ",", "sie", "h\u00f6\u00b7ren", "es", "ger\u00b7ne", 