{"textgrid.poem.62363": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4chle, l\u00e4chle lieber Mond", "tokens": ["L\u00e4ch\u00b7le", ",", "l\u00e4ch\u00b7le", "lie\u00b7ber", "Mond"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Zelle Nacht,", "tokens": ["In", "der", "Zel\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo die stille Liebe wohnt,", "tokens": ["Wo", "die", "stil\u00b7le", "Lie\u00b7be", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Sehnsucht wacht.", "tokens": ["Wo", "die", "Sehn\u00b7sucht", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meines Herzens ew'gen Drang", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "ew'\u00b7gen", "Drang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bring' ihn doch zur Ruh,", "tokens": ["Bring'", "ihn", "doch", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sing' ihm s\u00fc\u00dfen Wiegensang,", "tokens": ["Sing'", "ihm", "s\u00fc\u00b7\u00dfen", "Wie\u00b7gen\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6stungen ihm zu.", "tokens": ["Tr\u00f6s\u00b7tun\u00b7gen", "ihm", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "L\u00e4chle mit dem Himmelstrahl", "tokens": ["L\u00e4ch\u00b7le", "mit", "dem", "Him\u00b7mel\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trauter, lieber Mond", "tokens": ["Trau\u00b7ter", ",", "lie\u00b7ber", "Mond"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In das stille Friedensthal,", "tokens": ["In", "das", "stil\u00b7le", "Frie\u00b7denst\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Freundin wohnt.", "tokens": ["Wo", "die", "Freun\u00b7din", "wohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Str\u00f6me deinen Segensquell", "tokens": ["Str\u00f6\u00b7me", "dei\u00b7nen", "Se\u00b7gens\u00b7quell"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin auf ihr Gem\u00fcth,", "tokens": ["Hin", "auf", "ihr", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das so lieblich, rein und hell", "tokens": ["Das", "so", "lieb\u00b7lich", ",", "rein", "und", "hell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dein Antlitz bl\u00fcht.", "tokens": ["Wie", "dein", "Ant\u00b7litz", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Zeuge meiner Seligkeit,", "tokens": ["Zeu\u00b7ge", "mei\u00b7ner", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Freundin Freund,", "tokens": ["Mei\u00b7ner", "Freun\u00b7din", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der oft still und ohne Neid", "tokens": ["Der", "oft", "still", "und", "oh\u00b7ne", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich mit ihr vereint.", "tokens": ["Mich", "mit", "ihr", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ist der Tag nicht bald vollbracht,", "tokens": ["Ist", "der", "Tag", "nicht", "bald", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Holder Bundesstern?", "tokens": ["Hol\u00b7der", "Bun\u00b7dess\u00b7tern", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ach, ist die Verm\u00e4hlungsnacht", "tokens": ["Ach", ",", "ist", "die", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer noch so fern?", "tokens": ["Im\u00b7mer", "noch", "so", "fern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Mond, wann f\u00e4llt dein bleicher Strahl", "tokens": ["Mond", ",", "wann", "f\u00e4llt", "dein", "blei\u00b7cher", "Strahl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelnder herab", "tokens": ["L\u00e4\u00b7cheln\u00b7der", "her\u00b7ab"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf das ew'ge Friedensthal,", "tokens": ["Auf", "das", "ew'\u00b7ge", "Frie\u00b7denst\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf das stille Grab,", "tokens": ["Auf", "das", "stil\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wo die Sehnsucht schl\u00e4ft und ruht,", "tokens": ["Wo", "die", "Sehn\u00b7sucht", "schl\u00e4ft", "und", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wohin sie zieht,", "tokens": ["Ach", "wo\u00b7hin", "sie", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn mit ihrem h\u00f6chsten Gut", "tokens": ["Wenn", "mit", "ih\u00b7rem", "h\u00f6chs\u00b7ten", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie der Erd' entflieht?", "tokens": ["Sie", "der", "Erd'", "ent\u00b7flieht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Mond, mein Geist fliegt auf zu dir,", "tokens": ["Mond", ",", "mein", "Geist", "fliegt", "auf", "zu", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Ort zu weihn,", "tokens": ["Um", "den", "Ort", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo er eins mit Ihr, mit Ihr,", "tokens": ["Wo", "er", "eins", "mit", "Ihr", ",", "mit", "Ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "APPR", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seliger wird sein.", "tokens": ["Se\u00b7li\u00b7ger", "wird", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}