{"textgrid.poem.24972": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch S\u00e4useln in der Linde Bl\u00e4tterdach?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch S\u00e4useln in der Linde Bl\u00e4tterdach?", "tokens": ["Welch", "S\u00e4u\u00b7seln", "in", "der", "Lin\u00b7de", "Bl\u00e4t\u00b7ter\u00b7dach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was st\u00e4ubt zu mir herab wie Bl\u00fctenregen", "tokens": ["Was", "st\u00e4ubt", "zu", "mir", "her\u00b7ab", "wie", "Bl\u00fc\u00b7ten\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcllt mit Glanz und D\u00fcften mein Gemach", "tokens": ["Und", "f\u00fcllt", "mit", "Glanz", "und", "D\u00fcf\u00b7ten", "mein", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und treibt die Pulse mir zu schnellern Schl\u00e4gen,", "tokens": ["Und", "treibt", "die", "Pul\u00b7se", "mir", "zu", "schnel\u00b7lern", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als kehrte neu der ersten Liebe Gl\u00fcck", "tokens": ["Als", "kehr\u00b7te", "neu", "der", "ers\u00b7ten", "Lie\u00b7be", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADJD", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In dieses winter\u00f6de Herz zur\u00fcck?", "tokens": ["In", "die\u00b7ses", "win\u00b7ter\u00b7\u00f6\u00b7de", "Herz", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist's! Dich gr\u00fc\u00dft mit Freudenz\u00e4hrenschimmer", "tokens": ["Du", "bist's", "!", "Dich", "gr\u00fc\u00dft", "mit", "Freu\u00b7den\u00b7z\u00e4h\u00b7ren\u00b7schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Auge, lang der Thr\u00e4nen schon entw\u00f6hnt;", "tokens": ["Mein", "Au\u00b7ge", ",", "lang", "der", "Thr\u00e4\u00b7nen", "schon", "ent\u00b7w\u00f6hnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In meines Lebens tiefzerfallne Tr\u00fcmmer", "tokens": ["In", "mei\u00b7nes", "Le\u00b7bens", "tief\u00b7zer\u00b7fall\u00b7ne", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trittst du noch einmal l\u00e4chelnd und vers\u00f6hnt,", "tokens": ["Trittst", "du", "noch", "ein\u00b7mal", "l\u00e4\u00b7chelnd", "und", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du Einzige, die Treue mir bewahrt", "tokens": ["Du", "Ein\u00b7zi\u00b7ge", ",", "die", "Treu\u00b7e", "mir", "be\u00b7wahrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "$,", "ART", "NN", "PPER", "VVPP"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Auf dieser wechselvollen Erdenfahrt!", "tokens": ["Auf", "die\u00b7ser", "wech\u00b7sel\u00b7vol\u00b7len", "Er\u00b7den\u00b7fahrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie nenn' ich dich, die du die hohen Bahnen", "tokens": ["Wie", "nenn'", "ich", "dich", ",", "die", "du", "die", "ho\u00b7hen", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort oben neugebornen Sonnen zeigst", "tokens": ["Dort", "o\u00b7ben", "neu\u00b7ge\u00b7bor\u00b7nen", "Son\u00b7nen", "zeigst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in der Kinderseele stilles Ahnen", "tokens": ["Und", "in", "der", "Kin\u00b7der\u00b7see\u00b7le", "stil\u00b7les", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in des J\u00fcnglings Traum herniedersteigst?", "tokens": ["Und", "in", "des", "J\u00fcng\u00b7lings", "Traum", "her\u00b7nie\u00b7ders\u00b7teigst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fr\u00fch hab' ich dich gekannt, o Heilig-Gro\u00dfe!", "tokens": ["Fr\u00fch", "hab'", "ich", "dich", "ge\u00b7kannt", ",", "o", "Hei\u00b7lig\u00b7Gro\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,", "FM", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.6": {"text": "Und spielte, wie der Mutter, dir im Scho\u00dfe.", "tokens": ["Und", "spiel\u00b7te", ",", "wie", "der", "Mut\u00b7ter", ",", "dir", "im", "Scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mild sahst du mich an, so wundersam!", "tokens": ["So", "mild", "sahst", "du", "mich", "an", ",", "so", "wun\u00b7der\u00b7sam", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus deiner Augen himmlisch blauer Reine", "tokens": ["Aus", "dei\u00b7ner", "Au\u00b7gen", "himm\u00b7lisch", "blau\u00b7er", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umstrahlte noch mit morgenrotem Scheine", "tokens": ["Um\u00b7strahl\u00b7te", "noch", "mit", "mor\u00b7gen\u00b7ro\u00b7tem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich die Unendlichkeit, aus der ich kam,", "tokens": ["Mich", "die", "Un\u00b7end\u00b7lich\u00b7keit", ",", "aus", "der", "ich", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Himmelslieder sangst du mir \u2013 o nie", "tokens": ["Und", "Him\u00b7mels\u00b7lie\u00b7der", "sangst", "du", "mir", "\u2013", "o", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPER", "$(", "FM", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verklingen wird mir ihre Melodie!", "tokens": ["Ver\u00b7klin\u00b7gen", "wird", "mir", "ih\u00b7re", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Oft, wenn ich einsam klomm auf Bergesh\u00f6hn", "tokens": ["Oft", ",", "wenn", "ich", "ein\u00b7sam", "klomm", "auf", "Ber\u00b7ges\u00b7h\u00f6hn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mir vom Haupte troff des Fr\u00fchlings Regen,", "tokens": ["Und", "mir", "vom", "Haup\u00b7te", "troff", "des", "Fr\u00fch\u00b7lings", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Waldesstille tratst du mir entgegen", "tokens": ["In", "Wal\u00b7des\u00b7stil\u00b7le", "tratst", "du", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und neigtest mir dein Antlitz, g\u00f6ttlich sch\u00f6n,", "tokens": ["Und", "neig\u00b7test", "mir", "dein", "Ant\u00b7litz", ",", "g\u00f6tt\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in der Grotte auf das Moos gesunken", "tokens": ["Und", "in", "der", "Grot\u00b7te", "auf", "das", "Moos", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lag ich, dir lauschend, still und wonnetrunken.", "tokens": ["Lag", "ich", ",", "dir", "lau\u00b7schend", ",", "still", "und", "won\u00b7ne\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJD", "$,", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wer, o Freundin, nach der dunklen Stunde,", "tokens": ["Und", "wer", ",", "o", "Freun\u00b7din", ",", "nach", "der", "dunk\u00b7len", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "FM", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich, in sternenlose Nacht verirrt,", "tokens": ["Als", "ich", ",", "in", "ster\u00b7nen\u00b7lo\u00b7se", "Nacht", "ver\u00b7irrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den letzten Odem sog von jenem Munde,", "tokens": ["Den", "letz\u00b7ten", "O\u00b7dem", "sog", "von", "je\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich dem mir keiner wieder l\u00e4cheln wird:", "tokens": ["Gleich", "dem", "mir", "kei\u00b7ner", "wie\u00b7der", "l\u00e4\u00b7cheln", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPER", "PIS", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer war's, der aus des Abgrunds Finsternis,", "tokens": ["Wer", "wa\u00b7r's", ",", "der", "aus", "des", "Ab\u00b7grunds", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Von Grab und Tod empor die Seele ri\u00df?", "tokens": ["Von", "Grab", "und", "Tod", "em\u00b7por", "die", "See\u00b7le", "ri\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du, Herrliche! Da alles vom Geschicke,", "tokens": ["Du", ",", "Herr\u00b7li\u00b7che", "!", "Da", "al\u00b7les", "vom", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "KOUS", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was in der Sterblichkeit mir teuer war,", "tokens": ["Was", "in", "der", "Sterb\u00b7lich\u00b7keit", "mir", "teu\u00b7er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geraubt mir worden, zeigtest du dem Blicke", "tokens": ["Ge\u00b7raubt", "mir", "wor\u00b7den", ",", "zeig\u00b7test", "du", "dem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "PPER", "VAPP", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ew'ge Welt, wo immer hell und klar", "tokens": ["Die", "ew'\u00b7ge", "Welt", ",", "wo", "im\u00b7mer", "hell", "und", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die heil'ge Flamme lodert auf dem Herde,", "tokens": ["Die", "heil'\u00b7ge", "Flam\u00b7me", "lo\u00b7dert", "auf", "dem", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die nur gebrochen d\u00e4mmert dieser Erde.", "tokens": ["Die", "nur", "ge\u00b7bro\u00b7chen", "d\u00e4m\u00b7mert", "die\u00b7ser", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie ahnen wir, wenn Dantes Traumgesicht", "tokens": ["Sie", "ah\u00b7nen", "wir", ",", "wenn", "Dan\u00b7tes", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins Paradies uns tr\u00e4gt auf Strahlenwogen,", "tokens": ["Ins", "Pa\u00b7ra\u00b7dies", "uns", "tr\u00e4gt", "auf", "Strah\u00b7len\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Tizian zum Farbenregenbogen", "tokens": ["Wenn", "Ti\u00b7zi\u00b7an", "zum", "Far\u00b7ben\u00b7re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Glanz der gro\u00dfen Geistersonne bricht,", "tokens": ["Den", "Glanz", "der", "gro\u00b7\u00dfen", "Geis\u00b7ter\u00b7son\u00b7ne", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn unter Phidias' Hand, von ihr durchgl\u00fcht,", "tokens": ["Wenn", "un\u00b7ter", "Phi\u00b7dias'", "Hand", ",", "von", "ihr", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Marmorblock zum G\u00f6tterbild erbl\u00fcht.", "tokens": ["Der", "Mar\u00b7mor\u00b7block", "zum", "G\u00f6t\u00b7ter\u00b7bild", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+