{"textgrid.poem.46203": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber meiner Myrten seligen abschid", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In welche sich mein herz und deren herz in mir", "tokens": ["In", "wel\u00b7che", "sich", "mein", "herz", "und", "de\u00b7ren", "herz", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "KON", "PRELAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(durch brunst der wahren lieb entz\u00fcndet) einverleibet,", "tokens": ["(", "durch", "brunst", "der", "wah\u00b7ren", "lieb", "ent\u00b7z\u00fcn\u00b7det", ")", "ein\u00b7ver\u00b7lei\u00b7bet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "NN", "ART", "ADJA", "ADJD", "VVPP", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die lebet nu mit got und in mir, f\u00fcr und f\u00fcr,", "tokens": ["die", "le\u00b7bet", "nu", "mit", "got", "und", "in", "mir", ",", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "APPR", "NE", "KON", "APPR", "PPER", "$,", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hat der tod, blind und taub, schon leider! mich entweibet.", "tokens": ["hat", "der", "tod", ",", "blind", "und", "taub", ",", "schon", "lei\u00b7der", "!", "mich", "ent\u00b7wei\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "ADV", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des himmels engelchor in purer lieb mit ihr,", "tokens": ["Des", "him\u00b7mels", "en\u00b7gel\u00b7chor", "in", "pu\u00b7rer", "lieb", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und fr\u00f6lich sie mit ihm die zeit ohn zeit vertreibet,", "tokens": ["und", "fr\u00f6\u00b7lich", "sie", "mit", "ihm", "die", "zeit", "ohn", "zeit", "ver\u00b7trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "weil selig sie zugleich und ganz unselig wir,", "tokens": ["weil", "se\u00b7lig", "sie", "zu\u00b7gleich", "und", "ganz", "un\u00b7se\u00b7lig", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "KON", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und er durch sie ganz reich, ohn sie die welt arm bleibet.", "tokens": ["und", "er", "durch", "sie", "ganz", "reich", ",", "ohn", "sie", "die", "welt", "arm", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "ADV", "ADJD", "$,", "KOUI", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Schau selig sch\u00f6nste seel, wie doch in meiner brust", "tokens": ["Schau", "se\u00b7lig", "sch\u00f6ns\u00b7te", "seel", ",", "wie", "doch", "in", "mei\u00b7ner", "brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "der heiligen lieb glut kan mit den aschen dauren", "tokens": ["der", "hei\u00b7li\u00b7gen", "lieb", "glut", "kan", "mit", "den", "asc\u00b7hen", "dau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "NN", "VMFIN", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "und was zuvor nur got, jetz auch der welt bewust.", "tokens": ["und", "was", "zu\u00b7vor", "nur", "got", ",", "jetz", "auch", "der", "welt", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dir aber, dem mein leid kan deine freud versauren,", "tokens": ["Dir", "a\u00b7ber", ",", "dem", "mein", "leid", "kan", "dei\u00b7ne", "freud", "ver\u00b7sau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPOSAT", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "wer du auch bist, w\u00fcnsch ich, da\u00df f\u00fcrhin kein verlust", "tokens": ["wer", "du", "auch", "bist", ",", "w\u00fcnsch", "ich", ",", "da\u00df", "f\u00fcr\u00b7hin", "kein", "ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mach dich, wie dieser mich, der ich allzeit mu\u00df trauren.", "tokens": ["mach", "dich", ",", "wie", "die\u00b7ser", "mich", ",", "der", "ich", "all\u00b7zeit", "mu\u00df", "trau\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PDAT", "PPER", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}