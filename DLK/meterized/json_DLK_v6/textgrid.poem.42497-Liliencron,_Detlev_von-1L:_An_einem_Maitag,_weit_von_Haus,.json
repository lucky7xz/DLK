{"textgrid.poem.42497": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An einem Maitag, weit von Haus,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem Maitag, weit von Haus,", "tokens": ["An", "ei\u00b7nem", "Mai\u00b7tag", ",", "weit", "von", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag ich im Fenster schon hinaus", "tokens": ["Lag", "ich", "im", "Fens\u00b7ter", "schon", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Morgens fr\u00fch um viere.", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", "um", "vie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Still tr\u00e4umt die Stadt, kein Hund ist wach,", "tokens": ["Still", "tr\u00e4umt", "die", "Stadt", ",", "kein", "Hund", "ist", "wach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Rauch umkr\u00e4uselt traut das Dach,", "tokens": ["Kein", "Rauch", "um\u00b7kr\u00e4u\u00b7selt", "traut", "das", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch schlafen Mensch und Tiere.", "tokens": ["Noch", "schla\u00b7fen", "Mensch", "und", "Tie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf einmal, unter mir vorbei,", "tokens": ["Auf", "ein\u00b7mal", ",", "un\u00b7ter", "mir", "vor\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging eine kleine K\u00fcchenfei,", "tokens": ["Ging", "ei\u00b7ne", "klei\u00b7ne", "K\u00fc\u00b7chen\u00b7fei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kind von acht, neun Jahren.", "tokens": ["Ein", "Kind", "von", "acht", ",", "neun", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "$,", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sieht mich nicht \u2013 dsching, tut und quiek,", "tokens": ["Sie", "sieht", "mich", "nicht", "\u2013", "dsching", ",", "tut", "und", "qui\u00b7ek", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN", "$,", "VVFIN", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Klingt her die Regimentsmusik", "tokens": ["Klingt", "her", "die", "Re\u00b7gi\u00b7ments\u00b7mu\u00b7sik"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Schritt der Janitscharen.", "tokens": ["Im", "Schritt", "der", "Ja\u00b7nit\u00b7scha\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das M\u00e4del stutzt. Der Korb am Arm", "tokens": ["Das", "M\u00e4\u00b7del", "stutzt", ".", "Der", "Korb", "am", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fa\u00dft Eier, Wurst und andern Kram:", "tokens": ["Fa\u00dft", "Ei\u00b7er", ",", "Wurst", "und", "an\u00b7dern", "Kram", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mais, Reis und Pomeranzen.", "tokens": ["Mais", ",", "Reis", "und", "Po\u00b7me\u00b7ran\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da gehts nicht mehr, sie setzt ihn hin,", "tokens": ["Da", "gehts", "nicht", "mehr", ",", "sie", "setzt", "ihn", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur zu tanzen ist ihr Sinn,", "tokens": ["Und", "nur", "zu", "tan\u00b7zen", "ist", "ihr", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sie f\u00e4ngt an zu tanzen.", "tokens": ["Und", "sie", "f\u00e4ngt", "an", "zu", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Fern die Musik, klingklang rumbum;", "tokens": ["Fern", "die", "Mu\u00b7sik", ",", "kling\u00b7klang", "rum\u00b7bum", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sie tanzt und tanzt, rechtsum, linksum,", "tokens": ["Sie", "tanzt", "und", "tanzt", ",", "recht\u00b7sum", ",", "link\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reizend, wie Engel schweben.", "tokens": ["Rei\u00b7zend", ",", "wie", "En\u00b7gel", "schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Her, hin und her, sie ist allein,", "tokens": ["Her", ",", "hin", "und", "her", ",", "sie", "ist", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KON", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umblitzt vom ersten Sonnenschein,", "tokens": ["Um\u00b7blitzt", "vom", "ers\u00b7ten", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Trieb ganz hingegeben.", "tokens": ["Dem", "Trieb", "ganz", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mal kratzt sie sich den krausen Kopf,", "tokens": ["Mal", "kratzt", "sie", "sich", "den", "krau\u00b7sen", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Spatz machts so mit seinem Schopf,", "tokens": ["Der", "Spatz", "machts", "so", "mit", "sei\u00b7nem", "Schopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das tut sie nicht anfechten.", "tokens": ["Das", "tut", "sie", "nicht", "an\u00b7fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch pl\u00f6tzlich h\u00f6rt der Taumel auf,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "h\u00f6rt", "der", "Tau\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie nimmt den Korb, setzt sich in Lauf,", "tokens": ["Sie", "nimmt", "den", "Korb", ",", "setzt", "sich", "in", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es fliegen ihre Flechten.", "tokens": ["Es", "flie\u00b7gen", "ih\u00b7re", "Flech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hin zur Musik! Sie l\u00e4uft, sie rennt,", "tokens": ["Hin", "zur", "Mu\u00b7sik", "!", "Sie", "l\u00e4uft", ",", "sie", "rennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Nur zu, nur fort, als wenn sie brennt,", "tokens": ["Nur", "zu", ",", "nur", "fort", ",", "als", "wenn", "sie", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sinds f\u00fcr Firlefanzen!", "tokens": ["Was", "sinds", "f\u00fcr", "Fir\u00b7le\u00b7fan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wurst im Korb macht hoppsasa,", "tokens": ["Die", "Wurst", "im", "Korb", "macht", "hop\u00b7psa\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Eier h\u00fcpfen hopplala,", "tokens": ["Die", "Ei\u00b7er", "h\u00fcp\u00b7fen", "hop\u00b7pla\u00b7la", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und auch die Pomeranzen.", "tokens": ["Und", "auch", "die", "Po\u00b7me\u00b7ran\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer wei\u00df, wo jener Tanzplatz war:", "tokens": ["Wer", "wei\u00df", ",", "wo", "je\u00b7ner", "Tanz\u00b7platz", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kiel, in Rom, in Sansibar,", "tokens": ["In", "Kiel", ",", "in", "Rom", ",", "in", "San\u00b7si\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Siebenb\u00fcrgen, China?", "tokens": ["In", "Sie\u00b7ben\u00b7b\u00fcr\u00b7gen", ",", "Chi\u00b7na", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der Reim auf China liegt nicht fern:", "tokens": ["Der", "Reim", "auf", "Chi\u00b7na", "liegt", "nicht", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Leben denk ich immer gern", "tokens": ["Im", "Le\u00b7ben", "denk", "ich", "im\u00b7mer", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der kleinen Ballerina.", "tokens": ["Der", "klei\u00b7nen", "Bal\u00b7le\u00b7ri\u00b7na", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}