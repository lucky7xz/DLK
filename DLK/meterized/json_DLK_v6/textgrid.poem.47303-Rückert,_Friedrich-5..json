{"textgrid.poem.47303": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Augen, geht, den Lenz zu schauen,", "tokens": ["Ihr", "Au\u00b7gen", ",", "geht", ",", "den", "Lenz", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der l\u00e4chelnd liegt auf unsern Auen.", "tokens": ["Der", "l\u00e4\u00b7chelnd", "liegt", "auf", "un\u00b7sern", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Himmelskind in Blumenwiegen,", "tokens": ["Ein", "Him\u00b7mel\u00b7skind", "in", "Blu\u00b7men\u00b7wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ges\u00e4ugt von Milch der Wolkenfrauen.", "tokens": ["Ge\u00b7s\u00e4ugt", "von", "Milch", "der", "Wol\u00b7ken\u00b7frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ostluft ist die Amm' und schaukelt", "tokens": ["Die", "Ost\u00b7luft", "ist", "die", "Amm'", "und", "schau\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wiege mit dem Hauch, dem lauen.", "tokens": ["Die", "Wie\u00b7ge", "mit", "dem", "Hauch", ",", "dem", "lau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Kindlein thut, als schlaf' es, blinzet", "tokens": ["Das", "Kin\u00b7dlein", "thut", ",", "als", "schlaf'", "es", ",", "blin\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit seinen \u00c4ugelein, den schlauen.", "tokens": ["Mit", "sei\u00b7nen", "\u00c4u\u00b7ge\u00b7lein", ",", "den", "schlau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und wie's die Augen aufgeschlagen,", "tokens": ["Und", "wie's", "die", "Au\u00b7gen", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Tr\u00e4uft Tau von seinen Augenbrauen.", "tokens": ["Tr\u00e4uft", "Tau", "von", "sei\u00b7nen", "Au\u00b7gen\u00b7brau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und Bienen kommen, saugen emsig", "tokens": ["Und", "Bie\u00b7nen", "kom\u00b7men", ",", "sau\u00b7gen", "em\u00b7sig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVINF", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Tau, aus dem sie Honig brauen.", "tokens": ["Den", "Tau", ",", "aus", "dem", "sie", "Ho\u00b7nig", "brau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "O kommt und la\u00dft euch doch vom L\u00e4cheln", "tokens": ["O", "kommt", "und", "la\u00dft", "euch", "doch", "vom", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVIMP", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des Himmelkindleins auch durchtauen.", "tokens": ["Des", "Him\u00b7mel\u00b7kin\u00b7dleins", "auch", "durch\u00b7tau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "O kommt aus euern dumpfen Zellen,", "tokens": ["O", "kommt", "aus", "eu\u00b7ern", "dum\u00b7pfen", "Zel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die euch des Himmels Licht verbauen.", "tokens": ["Die", "euch", "des", "Him\u00b7mels", "Licht", "ver\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "La\u00dft uns die Zell' aus Wachs und Honig", "tokens": ["La\u00dft", "uns", "die", "Zell'", "aus", "Wachs", "und", "Ho\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sechseckig, wie die Bienen, bauen.", "tokens": ["Sechs\u00b7ec\u00b7kig", ",", "wie", "die", "Bie\u00b7nen", ",", "bau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Erwarmt am bunten Blumenfeuer,", "tokens": ["Er\u00b7warmt", "am", "bun\u00b7ten", "Blu\u00b7men\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und la\u00dft die Aschen ruhn, die grauen.", "tokens": ["Und", "la\u00dft", "die", "A\u00b7schen", "ruhn", ",", "die", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Bu\u00df' ist tot, die Liebe lebet,", "tokens": ["Die", "Bu\u00df'", "ist", "tot", ",", "die", "Lie\u00b7be", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ihr Atem weht in unsern Gauen.", "tokens": ["Ihr", "A\u00b7tem", "weht", "in", "un\u00b7sern", "Gau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Geht in des Fr\u00fchlings Liebeschenke,", "tokens": ["Geht", "in", "des", "Fr\u00fch\u00b7lings", "Lie\u00b7be\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Trinkt seines Weines ohne Grauen;", "tokens": ["Trinkt", "sei\u00b7nes", "Wei\u00b7nes", "oh\u00b7ne", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Auf da\u00df ihr liebestrunken werdet,", "tokens": ["Auf", "da\u00df", "ihr", "lie\u00b7be\u00b7strun\u00b7ken", "wer\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Eu'r Herz sich \u00f6ffne mit Vertrauen.", "tokens": ["Eu'r", "Herz", "sich", "\u00f6ff\u00b7ne", "mit", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die Lieb' ist wach an Erd' und Himmel,", "tokens": ["Die", "Lieb'", "ist", "wach", "an", "Erd'", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Im Gr\u00fcnen Rose, Sonn' im Blauen.", "tokens": ["Im", "Gr\u00fc\u00b7nen", "Ro\u00b7se", ",", "Sonn'", "im", "Blau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "O Nachtigall, sieh deine Rose;", "tokens": ["O", "Nach\u00b7ti\u00b7gall", ",", "sieh", "dei\u00b7ne", "Ro\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Du Adler sollst zur Sonne schauen.", "tokens": ["Du", "Ad\u00b7ler", "sollst", "zur", "Son\u00b7ne", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}