{"dta.poem.12288": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied des Glaubens .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein feste Burg ist unser Gott,               ", "tokens": ["Ein", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gute Wehr und Waffen,", "tokens": ["Ein", "gu\u00b7te", "Wehr", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hilft uns frei aus aller Noth,", "tokens": ["Er", "hilft", "uns", "frei", "aus", "al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns jetzt hat betroffen;", "tokens": ["Die", "uns", "jetzt", "hat", "be\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der alte b\u00f6se Feind,", "tokens": ["Der", "al\u00b7te", "b\u00f6\u00b7se", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Ernst es jetzt meint,", "tokens": ["Mit", "Ernst", "es", "jetzt", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Gro\u00df Macht und viel List", "tokens": ["Gro\u00df", "Macht", "und", "viel", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sein grausam R\u00fcstung ist;", "tokens": ["Sein", "grau\u00b7sam", "R\u00fcs\u00b7tung", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Auf Erd ist nicht seins Gleichen.", "tokens": ["Auf", "Erd", "ist", "nicht", "seins", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn die Welt voll Teufel w\u00e4r,", "tokens": ["Und", "wenn", "die", "Welt", "voll", "Teu\u00b7fel", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollten uns verschlingen,", "tokens": ["Und", "woll\u00b7ten", "uns", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So f\u00fcrchten wir uns nimmermehr,", "tokens": ["So", "f\u00fcrch\u00b7ten", "wir", "uns", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es soll uns doch gelingen;", "tokens": ["Es", "soll", "uns", "doch", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Feind von dieser Welt,", "tokens": ["Der", "Feind", "von", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie wild er sich stellt,", "tokens": ["Wie", "wild", "er", "sich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Thut er uns doch nichts;", "tokens": ["Thut", "er", "uns", "doch", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Er scheuet ja das Licht,", "tokens": ["Er", "scheu\u00b7et", "ja", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein Wort das kann ihn f\u00e4llen.", "tokens": ["Ein", "Wort", "das", "kann", "ihn", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott Ehr und Preis, der uns zu Gut,", "tokens": ["Gott", "Ehr", "und", "Preis", ",", "der", "uns", "zu", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Feind durch uns will schlagen,", "tokens": ["Den", "Feind", "durch", "uns", "will", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber uns hat treue Hut", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "hat", "treu\u00b7e", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seinem Feuerwagen;", "tokens": ["Auf", "sei\u00b7nem", "Feu\u00b7er\u00b7wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein ganz himmlisch Heer", "tokens": ["Sein", "ganz", "himm\u00b7lisch", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Rondet um uns her,", "tokens": ["Ron\u00b7det", "um", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Lobsingt, lobsinget ihm,", "tokens": ["Lob\u00b7singt", ",", "lob\u00b7sin\u00b7get", "ihm", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Lobsingt mit heller Stimm:", "tokens": ["Lob\u00b7singt", "mit", "hel\u00b7ler", "Stimm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ehr sey Gott in der H\u00f6he!", "tokens": ["Ehr", "sey", "Gott", "in", "der", "H\u00f6\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Sein Wort sie sollen lassen stehn,", "tokens": ["Sein", "Wort", "sie", "sol\u00b7len", "las\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Dank daf\u00fcr nicht haben,", "tokens": ["Kein", "Dank", "da\u00b7f\u00fcr", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben es wohl eingesehn", "tokens": ["Wir", "ha\u00b7ben", "es", "wohl", "ein\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinem Geist und Gaben.", "tokens": ["Mit", "sei\u00b7nem", "Geist", "und", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nehmen sie den Leib,", "tokens": ["Neh\u00b7men", "sie", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gut, Ehr, Kind und Weib,", "tokens": ["Gut", ",", "Ehr", ",", "Kind", "und", "Weib", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "La\u00df fahren dahin,", "tokens": ["La\u00df", "fah\u00b7ren", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sie haben keinen Gewinn;", "tokens": ["Sie", "ha\u00b7ben", "kei\u00b7nen", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Das Reich mu\u00df uns doch bleiben!", "tokens": ["Das", "Reich", "mu\u00df", "uns", "doch", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Lob, Ehr und Preis sey seiner Macht,", "tokens": ["Lob", ",", "Ehr", "und", "Preis", "sey", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein ist die ewge Beste,", "tokens": ["Sein", "ist", "die", "ew\u00b7ge", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wacht und schillert Tag und Nacht,", "tokens": ["Er", "wacht", "und", "schil\u00b7lert", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles geht aufs Beste;", "tokens": ["Da\u00df", "al\u00b7les", "geht", "aufs", "Bes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jesus ist sein Wort,", "tokens": ["Je\u00b7sus", "ist", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ein heimlich offen Wort,", "tokens": ["Ein", "heim\u00b7lich", "of\u00b7fen", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihn ruft Wacht zu Wacht", "tokens": ["Ihn", "ruft", "Wacht", "zu", "Wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Zum Trost durch die Nacht,", "tokens": ["Zum", "Trost", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Bis alle V\u00f6gel ihm singen.", "tokens": ["Bis", "al\u00b7le", "V\u00f6\u00b7gel", "ihm", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}