{"textgrid.poem.59730": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Alterswehmut", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Erde, Land der Tr\u00e4ume,", "tokens": ["O", "Er\u00b7de", ",", "Land", "der", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Erde, Land des Trugs,", "tokens": ["O", "Er\u00b7de", ",", "Land", "des", "Trugs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst in die hellern R\u00e4ume", "tokens": ["Willst", "in", "die", "hel\u00b7lern", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Fl\u00fcgel meines Flugs", "tokens": ["Die", "Fl\u00fc\u00b7gel", "mei\u00b7nes", "Flugs"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir dunkeln stets und k\u00fcrzen?", "tokens": ["Mir", "dun\u00b7keln", "stets", "und", "k\u00fcr\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In deines Jammers Staub", "tokens": ["In", "dei\u00b7nes", "Jam\u00b7mers", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich elend niederst\u00fcrzen", "tokens": ["Mich", "e\u00b7lend", "nie\u00b7ders\u00b7t\u00fcr\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Jagd nach schlechtem Raub?", "tokens": ["In", "Jagd", "nach", "schlech\u00b7tem", "Raub", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es soll dir nicht gelingen,", "tokens": ["Es", "soll", "dir", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe meinen Hort,", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tr\u00e4gt auf Feuerschwingen", "tokens": ["Der", "tr\u00e4gt", "auf", "Feu\u00b7er\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich durch die Himmel fort;", "tokens": ["Mich", "durch", "die", "Him\u00b7mel", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe meinen Meister,", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Held und K\u00f6nig ist \u2013", "tokens": ["Der", "Held", "und", "K\u00f6\u00b7nig", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er ist der F\u00fcrst der Geister", "tokens": ["Er", "ist", "der", "F\u00fcrst", "der", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und hei\u00dfet Jesus Christ.", "tokens": ["Und", "hei\u00b7\u00dfet", "Je\u00b7sus", "Christ", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er stieg vom Himmel nieder", "tokens": ["Er", "stieg", "vom", "Him\u00b7mel", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf unsre Erdenau'n,", "tokens": ["Auf", "uns\u00b7re", "Er\u00b7de\u00b7nau'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit die Menschen wieder", "tokens": ["Da\u00b7mit", "die", "Men\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach oben k\u00f6nnten schaun,", "tokens": ["Nach", "o\u00b7ben", "k\u00f6nn\u00b7ten", "schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Damit die armen Wichte,", "tokens": ["Da\u00b7mit", "die", "ar\u00b7men", "Wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Wahn und Trug umstrickt,", "tokens": ["Von", "Wahn", "und", "Trug", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aufschauten nach dem Lichte,", "tokens": ["Auf\u00b7schau\u00b7ten", "nach", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Woraus die Gottheit blickt.", "tokens": ["Wo\u00b7raus", "die", "Got\u00b7theit", "blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O K\u00f6nig aller Liebe,", "tokens": ["O", "K\u00f6\u00b7nig", "al\u00b7ler", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Glanz des h\u00f6chsten Lichts,", "tokens": ["O", "Glanz", "des", "h\u00f6chs\u00b7ten", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mir auch gar nichts bliebe,", "tokens": ["Wenn", "mir", "auch", "gar", "nichts", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar nichts in diesem Nichts,", "tokens": ["Gar", "nichts", "in", "die\u00b7sem", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Worum die Welt sich rei\u00dfet,", "tokens": ["Wo\u00b7rum", "die", "Welt", "sich", "rei\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du bleibst mein Held und Hort,", "tokens": ["Du", "bleibst", "mein", "Held", "und", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was auch rei\u00dft und splei\u00dfet,", "tokens": ["Und", "was", "auch", "rei\u00dft", "und", "splei\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nichts rei\u00dft von dir mich fort.", "tokens": ["Nichts", "rei\u00dft", "von", "dir", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So mag denn alles schweben", "tokens": ["So", "mag", "denn", "al\u00b7les", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wechsel hin und her,", "tokens": ["Im", "Wech\u00b7sel", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist hinfort gegeben,", "tokens": ["Mir", "ist", "hin\u00b7fort", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was wechselt nimmermehr:", "tokens": ["Was", "wech\u00b7selt", "nim\u00b7mer\u00b7mehr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}