{"textgrid.poem.27758": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einen Reichen nennet, hat alles die\u00df genennt,", "tokens": ["Wer", "ei\u00b7nen", "Rei\u00b7chen", "nen\u00b7net", ",", "hat", "al\u00b7les", "die\u00df", "ge\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PIS", "PDS", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was diese Welt f\u00fcr gut, f\u00fcr hoch, f\u00fcr herrlich kennt.", "tokens": ["Was", "die\u00b7se", "Welt", "f\u00fcr", "gut", ",", "f\u00fcr", "hoch", ",", "f\u00fcr", "herr\u00b7lich", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "APPR", "ADJD", "$,", "APPR", "ADJD", "$,", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer einen Armen nennt, der hat von dem gesagt,", "tokens": ["Wer", "ei\u00b7nen", "Ar\u00b7men", "nennt", ",", "der", "hat", "von", "dem", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "APPR", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was alle Welt veracht und aller Unfall plagt.", "tokens": ["Was", "al\u00b7le", "Welt", "ver\u00b7acht", "und", "al\u00b7ler", "Un\u00b7fall", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVPP", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch dennoch tausch ich nicht; ein Armer in der Zeit", "tokens": ["Noch", "den\u00b7noch", "tausch", "ich", "nicht", ";", "ein", "Ar\u00b7mer", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist fertig, reich zu sein dort in der Ewigkeit.", "tokens": ["Ist", "fer\u00b7tig", ",", "reich", "zu", "sein", "dort", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "PTKZU", "VAINF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}