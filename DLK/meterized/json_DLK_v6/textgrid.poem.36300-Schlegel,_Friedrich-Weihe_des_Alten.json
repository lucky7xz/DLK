{"textgrid.poem.36300": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Weihe des Alten", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm den Becher zur Hand, den freudigen,", "tokens": ["Nimm", "den", "Be\u00b7cher", "zur", "Hand", ",", "den", "freu\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Freund vom Freunde nur dreist!", "tokens": ["Freund", "vom", "Freun\u00b7de", "nur", "dreist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dunkelgolden rollet der Wein", "tokens": ["Dun\u00b7kel\u00b7gol\u00b7den", "rol\u00b7let", "der", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In des hellen Kristalles Blitzen;", "tokens": ["In", "des", "hel\u00b7len", "Kris\u00b7tal\u00b7les", "Blit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es schwebet zum Haupt auf", "tokens": ["Es", "schwe\u00b7bet", "zum", "Haupt", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Duftiger Blume k\u00fchlendes Feuer.", "tokens": ["Duf\u00b7ti\u00b7ger", "Blu\u00b7me", "k\u00fch\u00b7len\u00b7des", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Trinke hinunter die Glut,", "tokens": ["Trin\u00b7ke", "hin\u00b7un\u00b7ter", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "So schwillet der Jugend Herz", "tokens": ["So", "schwil\u00b7let", "der", "Ju\u00b7gend", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Selig von Kraft und liebender Freude.", "tokens": ["Se\u00b7lig", "von", "Kraft", "und", "lie\u00b7ben\u00b7der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Ergreife k\u00fchnlich den Zauberbecher!", "tokens": ["Er\u00b7grei\u00b7fe", "k\u00fchn\u00b7lich", "den", "Zau\u00b7ber\u00b7be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Du bist g\u00f6ttlicher Art,", "tokens": ["Du", "bist", "g\u00f6tt\u00b7li\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Jugendlich heldengesinnt.", "tokens": ["Ju\u00b7gend\u00b7lich", "hel\u00b7den\u00b7ge\u00b7sinnt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Sei du trunken nur stets,", "tokens": ["Sei", "du", "trun\u00b7ken", "nur", "stets", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Und spotte der Furcht,", "tokens": ["Und", "spot\u00b7te", "der", "Furcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Gr\u00fcn umlaubt von Fr\u00fchling das Haar,", "tokens": ["Gr\u00fcn", "um\u00b7laubt", "von", "Fr\u00fch\u00b7ling", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Ewiglich treu der goldenen Dichtkunst,", "tokens": ["E\u00b7wig\u00b7lich", "treu", "der", "gol\u00b7de\u00b7nen", "Dicht\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Wie es uns Deutschen geziemt.", "tokens": ["Wie", "es", "uns", "Deut\u00b7schen", "ge\u00b7ziemt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.18": {"text": "Wer gekostet des heiligen Weins,", "tokens": ["Wer", "ge\u00b7kos\u00b7tet", "des", "hei\u00b7li\u00b7gen", "Weins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Dem entweichen die Schleier.", "tokens": ["Dem", "ent\u00b7wei\u00b7chen", "die", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.20": {"text": "Wo der Freudige naht,", "tokens": ["Wo", "der", "Freu\u00b7di\u00b7ge", "naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Hauchet Sommerwonne die Luft,", "tokens": ["Hau\u00b7chet", "Som\u00b7mer\u00b7won\u00b7ne", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "L\u00fcstern \u00f6ffnet die Rose den Kelch;", "tokens": ["L\u00fcs\u00b7tern", "\u00f6ff\u00b7net", "die", "Ro\u00b7se", "den", "Kelch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Der h\u00f6chsten Gebilde", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Heilige Sch\u00f6nheit schauet das Auge,", "tokens": ["Hei\u00b7li\u00b7ge", "Sch\u00f6n\u00b7heit", "schau\u00b7et", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Rein der H\u00fclle entstiegen.", "tokens": ["Rein", "der", "H\u00fcl\u00b7le", "ent\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.26": {"text": "Nackte Reize umspielt", "tokens": ["Nack\u00b7te", "Rei\u00b7ze", "um\u00b7spielt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Wollustschlagend das Meer", "tokens": ["Wol\u00b7lust\u00b7schla\u00b7gend", "das", "Meer"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Allseliger Liebe.", "tokens": ["All\u00b7se\u00b7li\u00b7ger", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.29": {"text": "Gerne sinkt er hinab,", "tokens": ["Ger\u00b7ne", "sinkt", "er", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "Mit verschlungen im Meer;", "tokens": ["Mit", "ver\u00b7schlun\u00b7gen", "im", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Alles Leben ist sein,", "tokens": ["Al\u00b7les", "Le\u00b7ben", "ist", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VAINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Alle Wesen nur eins,", "tokens": ["Al\u00b7le", "We\u00b7sen", "nur", "eins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PIS", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "In hei\u00dfer Freude verschlungen,", "tokens": ["In", "hei\u00b7\u00dfer", "Freu\u00b7de", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Von tiefer Sehnsucht durchdrungen,", "tokens": ["Von", "tie\u00b7fer", "Sehn\u00b7sucht", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Alles nur Lust und Begierde,", "tokens": ["Al\u00b7les", "nur", "Lust", "und", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.36": {"text": "Schwellend von \u00fcppiger Sch\u00f6nheit", "tokens": ["Schwel\u00b7lend", "von", "\u00fcp\u00b7pi\u00b7ger", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.37": {"text": "Innig umfangender Liebe;", "tokens": ["In\u00b7nig", "um\u00b7fan\u00b7gen\u00b7der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.38": {"text": "In des heiligen Fr\u00fchlinges Garten", "tokens": ["In", "des", "hei\u00b7li\u00b7gen", "Fr\u00fch\u00b7lin\u00b7ges", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--++-+-", "measure": "anapaest.di.plus"}, "line.39": {"text": "Die F\u00fclle der Rosen,", "tokens": ["Die", "F\u00fcl\u00b7le", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Jeder Rose entquollen", "tokens": ["Je\u00b7der", "Ro\u00b7se", "ent\u00b7quol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.41": {"text": "In neu erzeugten Gebilden", "tokens": ["In", "neu", "er\u00b7zeug\u00b7ten", "Ge\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Das sch\u00f6ne Wunder des Leibes,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Wun\u00b7der", "des", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Liebliches Lebensgeheimnis.", "tokens": ["Lieb\u00b7li\u00b7ches", "Le\u00b7bens\u00b7ge\u00b7heim\u00b7nis", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Ahndest du, was dich durchdrang?", "tokens": ["Ahn\u00b7dest", "du", ",", "was", "dich", "durch\u00b7drang", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist m\u00e4nnlich und stark,", "tokens": ["Du", "bist", "m\u00e4nn\u00b7lich", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erd' umfassend dein Herz.", "tokens": ["Erd'", "um\u00b7fas\u00b7send", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "F\u00fchle nun auch den Tod", "tokens": ["F\u00fch\u00b7le", "nun", "auch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Kalten Zornes im Stein,", "tokens": ["Kal\u00b7ten", "Zor\u00b7nes", "im", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Schaue des Abgrunds ewige Greuel,", "tokens": ["Schau\u00b7e", "des", "Ab\u00b7grunds", "e\u00b7wi\u00b7ge", "Greu\u00b7el", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "In der Tiefe untern Kammern", "tokens": ["In", "der", "Tie\u00b7fe", "un\u00b7tern", "Kam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ungeheuersten Schrecken,", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7ers\u00b7ten", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Grimmgefesselter Tiere", "tokens": ["Grimm\u00b7ge\u00b7fes\u00b7sel\u00b7ter", "Tie\u00b7re"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Alte Riesengebilde,", "tokens": ["Al\u00b7te", "Rie\u00b7sen\u00b7ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Ewig da w\u00fctend im Schmerz.", "tokens": ["E\u00b7wig", "da", "w\u00fc\u00b7tend", "im", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.12": {"text": "Steige mein Freund, in den Schacht", "tokens": ["Stei\u00b7ge", "mein", "Freund", ",", "in", "den", "Schacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "K\u00fchn des Todes hinab!", "tokens": ["K\u00fchn", "des", "To\u00b7des", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Dunkel rieselt da unten", "tokens": ["Dun\u00b7kel", "rie\u00b7selt", "da", "un\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Heimlich der Liebesquell.", "tokens": ["Heim\u00b7lich", "der", "Lie\u00b7bes\u00b7quell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Da ist Sehnsucht und kindliche Trauer", "tokens": ["Da", "ist", "Sehn\u00b7sucht", "und", "kind\u00b7li\u00b7che", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Aus dem Herzen der Mutter,", "tokens": ["Aus", "dem", "Her\u00b7zen", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Strebet \u00e4ngstlich zu sterben,", "tokens": ["Stre\u00b7bet", "\u00e4ngst\u00b7lich", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.19": {"text": "M\u00f6chte in Liebe vergehn.", "tokens": ["M\u00f6ch\u00b7te", "in", "Lie\u00b7be", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.20": {"text": "Selten nur dringet ein Strahl", "tokens": ["Sel\u00b7ten", "nur", "drin\u00b7get", "ein", "S