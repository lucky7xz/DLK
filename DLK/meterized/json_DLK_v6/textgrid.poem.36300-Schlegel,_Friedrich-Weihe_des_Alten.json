nheit Scho\u00df", "tokens": ["Aus", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "Scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Dunkle Sonnen erwachen.", "tokens": ["Dunk\u00b7le", "Son\u00b7nen", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.33": {"text": "Mutig vollf\u00fchr' es als Held!", "tokens": ["Mu\u00b7tig", "voll\u00b7f\u00fchr'", "es", "als", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Mich entrei\u00dfet der Sturmwind,", "tokens": ["Mich", "ent\u00b7rei\u00b7\u00dfet", "der", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.35": {"text": "Ruhe nun balde ewiglich heiter", "tokens": ["Ru\u00b7he", "nun", "bal\u00b7de", "e\u00b7wig\u00b7lich", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ADV", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.36": {"text": "Auf dem strahlenden Thron,", "tokens": ["Auf", "dem", "strah\u00b7len\u00b7den", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Allen Heldengeistern vereint. \u2013", "tokens": ["Al\u00b7len", "Hel\u00b7den\u00b7geis\u00b7tern", "ver\u00b7eint", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Sei mir gegr\u00fc\u00dft, mein Sohn!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Wenn ich den Leib dir nicht zeugte,", "tokens": ["Wenn", "ich", "den", "Leib", "dir", "nicht", "zeug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Hab' ich den Mut doch entflammt,", "tokens": ["Hab'", "ich", "den", "Mut", "doch", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.41": {"text": "Dir hohe Sterne gezeigt,", "tokens": ["Dir", "ho\u00b7he", "Ster\u00b7ne", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.42": {"text": "Und allen Segen gespendet,", "tokens": ["Und", "al\u00b7len", "Se\u00b7gen", "ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Dr\u00fccke dich herzlich ans Herz,", "tokens": ["Dr\u00fc\u00b7cke", "dich", "herz\u00b7lich", "ans", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.44": {"text": "Du mein Freund und mein Sohn!", "tokens": ["Du", "mein", "Freund", "und", "mein", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "P