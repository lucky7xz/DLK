{"dta.poem.313": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Zarabellen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Zarabelle deine Wangen", "tokens": ["Za\u00b7ra\u00b7bel\u00b7le", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigen deinen Jammer an/", "tokens": ["Zei\u00b7gen", "dei\u00b7nen", "Jam\u00b7mer", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein mattes Auge kan", "tokens": ["Und", "dein", "mat\u00b7tes", "Au\u00b7ge", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht/ vor Schmertz/ mit Strahlen prangen/", "tokens": ["Nicht", "/", "vor", "Schmertz", "/", "mit", "Strah\u00b7len", "pran\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$(", "APPR", "NN", "$(", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die durch ihren heissen Schein", "tokens": ["Die", "durch", "ih\u00b7ren", "heis\u00b7sen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hertz und Sinnen nehmen ein.", "tokens": ["Hertz", "und", "Sin\u00b7nen", "neh\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Rosen sind erblasset/", "tokens": ["Dei\u00b7ne", "Ro\u00b7sen", "sind", "er\u00b7blas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Lilje wil vergehn/", "tokens": ["Und", "die", "Lil\u00b7je", "wil", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein beliebtes Tausend-Sch\u00f6n", "tokens": ["Dein", "be\u00b7lieb\u00b7tes", "Tau\u00b7sen\u00b7dSch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiche Todes-Angst umfasset.", "tokens": ["Blei\u00b7che", "To\u00b7des\u00b7Angst", "um\u00b7fas\u00b7set", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der betr\u00fcbten Augen-Licht", "tokens": ["Der", "be\u00b7tr\u00fcb\u00b7ten", "Au\u00b7gen\u00b7Licht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur durch Kummer-Wolcken bricht.", "tokens": ["Nur", "durch", "Kum\u00b7mer\u00b7Wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nste fasse dich doch wieder/", "tokens": ["Sch\u00f6ns\u00b7te", "fas\u00b7se", "dich", "doch", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kl\u00e4hre dein Gesichte auf", "tokens": ["Kl\u00e4h\u00b7re", "dein", "Ge\u00b7sich\u00b7te", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df der Freude ihren Lauff/", "tokens": ["La\u00df", "der", "Freu\u00b7de", "ih\u00b7ren", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Anmuth deiner Glieder", "tokens": ["Da\u00df", "die", "An\u00b7muth", "dei\u00b7ner", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich mit neuer Lust belebt/", "tokens": ["Sich", "mit", "neu\u00b7er", "Lust", "be\u00b7lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die vor\u2019ge Pracht erhebt.", "tokens": ["Und", "die", "vor'\u00b7ge", "Pracht", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Lasse doch den Schmertzen fahren/", "tokens": ["Las\u00b7se", "doch", "den", "Schmert\u00b7zen", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vergi\u00df der alten Treu/", "tokens": ["Und", "ver\u00b7gi\u00df", "der", "al\u00b7ten", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denck/ da\u00df es unm\u00fcglich sey", "tokens": ["Denck", "/", "da\u00df", "es", "un\u00b7m\u00fcg\u00b7lich", "sey"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Mit Verstorbnen sich zu paaren.", "tokens": ["Mit", "Ver\u00b7storb\u00b7nen", "sich", "zu", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe den/ wer lieben kan/", "tokens": ["Lie\u00b7be", "den", "/", "wer", "lie\u00b7ben", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$(", "PWS", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was gehn dir die Todten an.", "tokens": ["Was", "gehn", "dir", "die", "Tod\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}