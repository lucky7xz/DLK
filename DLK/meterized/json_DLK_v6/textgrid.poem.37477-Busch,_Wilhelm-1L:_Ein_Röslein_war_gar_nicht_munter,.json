{"textgrid.poem.37477": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein R\u00f6slein war gar nicht munter,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein R\u00f6slein war gar nicht munter,", "tokens": ["Ein", "R\u00f6s\u00b7lein", "war", "gar", "nicht", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weil es im Topfe stand,", "tokens": ["Weil", "es", "im", "Top\u00b7fe", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah immer traurig hinunter", "tokens": ["Sah", "im\u00b7mer", "trau\u00b7rig", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf die Blumen im freien Land.", "tokens": ["Auf", "die", "Blu\u00b7men", "im", "frei\u00b7en", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die Blumen nicken und winken.", "tokens": ["Die", "Blu\u00b7men", "ni\u00b7cken", "und", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie ist es im Freien so sch\u00f6n", "tokens": ["Wie", "ist", "es", "im", "Frei\u00b7en", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Zu tanzen und Tau zu trinken", "tokens": ["Zu", "tan\u00b7zen", "und", "Tau", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bei lustigem Windeswehn.", "tokens": ["Bei", "lus\u00b7ti\u00b7gem", "Win\u00b7des\u00b7wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Von bunten Schmetterlingen", "tokens": ["Von", "bun\u00b7ten", "Schmet\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Umgaukelt, geschmeichelt, gek\u00fc\u00dft;", "tokens": ["Um\u00b7gau\u00b7kelt", ",", "ge\u00b7schmei\u00b7chelt", ",", "ge\u00b7k\u00fc\u00dft", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Dazwischen der V\u00f6glein Singen", "tokens": ["Da\u00b7zwi\u00b7schen", "der", "V\u00f6\u00b7glein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Anmutig zu h\u00f6ren ist.", "tokens": ["An\u00b7mu\u00b7tig", "zu", "h\u00f6\u00b7ren", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.13": {"text": "Wir preisen dich und loben", "tokens": ["Wir", "prei\u00b7sen", "dich", "und", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Dich, fr\u00f6hliche Sommerzeit;", "tokens": ["Dich", ",", "fr\u00f6h\u00b7li\u00b7che", "Som\u00b7mer\u00b7zeit", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ach, R\u00f6slein am Fenster droben,", "tokens": ["Ach", ",", "R\u00f6s\u00b7lein", "am", "Fens\u00b7ter", "dro\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Du tust uns auch gar zu leid.", "tokens": ["Du", "tust", "uns", "auch", "gar", "zu", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Da ist ins Land gekommen", "tokens": ["Da", "ist", "ins", "Land", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der Winter mit seiner Not.", "tokens": ["Der", "Win\u00b7ter", "mit", "sei\u00b7ner", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "In Schnee und Frost verklommen", "tokens": ["In", "Schnee", "und", "Frost", "ver\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die Blumen sind alle tot.", "tokens": ["Die", "Blu\u00b7men", "sind", "al\u00b7le", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Ein M\u00e4gdlein h\u00f6rt es st\u00fcrmen,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "h\u00f6rt", "es", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Macht fest das Fenster zu.", "tokens": ["Macht", "fest", "das", "Fens\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Jetzt will ich dich pflegen und schirmen,", "tokens": ["Jetzt", "will", "ich", "dich", "pfle\u00b7gen", "und", "schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Du liebes R\u00f6slein du.", "tokens": ["Du", "lie\u00b7bes", "R\u00f6s\u00b7lein", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}