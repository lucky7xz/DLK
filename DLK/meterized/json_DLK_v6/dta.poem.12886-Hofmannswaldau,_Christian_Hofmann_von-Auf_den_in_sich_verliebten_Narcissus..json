{"dta.poem.12886": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den in sich verliebten Narcissus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Narcissus flieht die lieb, und wird in sich verliebt,", "tokens": ["Nar\u00b7cis\u00b7sus", "flieht", "die", "lieb", ",", "und", "wird", "in", "sich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "$,", "KON", "VAFIN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ihm ein heller brunn sein bild zu schauen giebt:", "tokens": ["Als", "ihm", "ein", "hel\u00b7ler", "brunn", "sein", "bild", "zu", "schau\u00b7en", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird ihm Diana nun den spiegel nicht entr\u00fccken,", "tokens": ["Wird", "ihm", "Di\u00b7a\u00b7na", "nun", "den", "spie\u00b7gel", "nicht", "ent\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df er zweiffels-ohn in glut und flut ersticken.", "tokens": ["So", "mu\u00df", "er", "zweif\u00b7fels\u00b7ohn", "in", "glut", "und", "flut", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}