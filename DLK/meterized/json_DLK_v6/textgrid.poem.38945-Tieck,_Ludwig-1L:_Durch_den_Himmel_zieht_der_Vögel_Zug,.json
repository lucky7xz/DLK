{"textgrid.poem.38945": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch den Himmel zieht der V\u00f6gel Zug,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den Himmel zieht der V\u00f6gel Zug,", "tokens": ["Durch", "den", "Him\u00b7mel", "zieht", "der", "V\u00f6\u00b7gel", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie sind auf Wanderschaft begriffen,", "tokens": ["Sie", "sind", "auf", "Wan\u00b7der\u00b7schaft", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hort man gezwitschert und gepfiffen", "tokens": ["Da", "hort", "man", "ge\u00b7zwit\u00b7schert", "und", "ge\u00b7pfif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVPP", "KON", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Gro\u00df und Klein der Melodien genug.", "tokens": ["Von", "Gro\u00df", "und", "Klein", "der", "Me\u00b7lo\u00b7dien", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Kleine singt mit feiner Stimm',", "tokens": ["Der", "Klei\u00b7ne", "singt", "mit", "fei\u00b7ner", "Stimm'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gro\u00dfe kr\u00e4chzt gleich wie im Grimm,", "tokens": ["Der", "Gro\u00b7\u00dfe", "kr\u00e4chzt", "gleich", "wie", "im", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "KOKOM", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein'ge stottern, andre schnarren,", "tokens": ["Und", "ein'\u00b7ge", "stot\u00b7tern", ",", "and\u00b7re", "schnar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Drossel, Gimpel, Schwalbe, Starren,", "tokens": ["Und", "Dros\u00b7sel", ",", "Gim\u00b7pel", ",", "Schwal\u00b7be", ",", "Star\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie wissen alle nicht, was sie meinen,", "tokens": ["Sie", "wis\u00b7sen", "al\u00b7le", "nicht", ",", "was", "sie", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie wissen's wohl und sagen's nicht,", "tokens": ["Sie", "wis\u00b7sen's", "wohl", "und", "sa\u00b7gen's", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn sie auch zu reden scheinen,", "tokens": ["Und", "wenn", "sie", "auch", "zu", "re\u00b7den", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ihr Gerede nicht von Gewicht.", "tokens": ["Ist", "ihr", "Ge\u00b7re\u00b7de", "nicht", "von", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "\u2013 \u00bbHolla! warum seid ihr auf der Reise?\u00ab \u2013", "tokens": ["\u2013", "\u00bb", "Hol\u00b7la", "!", "wa\u00b7rum", "seid", "ihr", "auf", "der", "Rei\u00b7se", "?", "\u00ab", "\u2013"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NE", "$.", "PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das ist nun einmal unsre Weise.", "tokens": ["Das", "ist", "nun", "ein\u00b7mal", "uns\u00b7re", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 \u00bbWarum bleibt ihr nicht zu jeglicher Stund?\u00ab \u2013", "tokens": ["\u2013", "\u00bb", "Wa\u00b7rum", "bleibt", "ihr", "nicht", "zu", "jeg\u00b7li\u00b7cher", "Stund", "?", "\u00ab", "\u2013"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "PWAV", "VVFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Erd' ist allenthalben rund.", "tokens": ["Die", "Erd'", "ist", "al\u00b7len\u00b7thal\u00b7ben", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf die armen Lerchen wird Jagd gemacht,", "tokens": ["Auf", "die", "ar\u00b7men", "Ler\u00b7chen", "wird", "Jagd", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Schnepfen gar in Dohnen gefangen,", "tokens": ["Die", "Schnep\u00b7fen", "gar", "in", "Doh\u00b7nen", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort sind die V\u00f6glein aufgehangen,", "tokens": ["Dort", "sind", "die", "V\u00f6\u00b7glein", "auf\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An keine R\u00fcckfahrt mehr gedacht.", "tokens": ["An", "kei\u00b7ne", "R\u00fcck\u00b7fahrt", "mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u2013 Ist das die Art mit uns zu sprechen?", "tokens": ["\u2013", "Ist", "das", "die", "Art", "mit", "uns", "zu", "spre\u00b7chen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ans armen V\u00f6geln den Hals zu brechen?", "tokens": ["Ans", "ar\u00b7men", "V\u00f6\u00b7geln", "den", "Hals", "zu", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u2013 \u00bbVerst\u00e4ndlich ist doch diese Sprache,", "tokens": ["\u2013", "\u00bb", "Ver\u00b7st\u00e4nd\u00b7lich", "ist", "doch", "die\u00b7se", "Spra\u00b7che", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADJD", "VAFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ruft der Mensch, sie dient zur Sache,", "tokens": ["So", "ruft", "der", "Mensch", ",", "sie", "dient", "zur", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In aller Natur die Sprache regiert,", "tokens": ["In", "al\u00b7ler", "Na\u00b7tur", "die", "Spra\u00b7che", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df eins mit dem andern Kriege f\u00fchrt,", "tokens": ["Da\u00df", "eins", "mit", "dem", "an\u00b7dern", "Krie\u00b7ge", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Man dann am besten raisonnirt und beweis't,", "tokens": ["Man", "dann", "am", "bes\u00b7ten", "rai\u00b7son\u00b7nirt", "und", "be\u00b7weis't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKA", "ADJD", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wenn eins vom andern wird aufgespeis't:", "tokens": ["Wenn", "eins", "vom", "an\u00b7dern", "wird", "auf\u00b7ge\u00b7speis't", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Str\u00f6me sind im Meere verschlungen,", "tokens": ["Die", "Str\u00f6\u00b7me", "sind", "im", "Mee\u00b7re", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Vom Schicksal wird der Mensch bezwungen,", "tokens": ["Vom", "Schick\u00b7sal", "wird", "der", "Mensch", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den tapfersten Magen hat die Zeit,", "tokens": ["Den", "tap\u00b7fers\u00b7ten", "Ma\u00b7gen", "hat", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ihr nimmermehr ein Essen gereut,", "tokens": ["Ihr", "nim\u00b7mer\u00b7mehr", "ein", "Es\u00b7sen", "ge\u00b7reut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Doch wie von der Zeit eine alte Fabel besagt", "tokens": ["Doch", "wie", "von", "der", "Zeit", "ei\u00b7ne", "al\u00b7te", "Fa\u00b7bel", "be\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Macht auf sie das j\u00fcngste Gericht einst Jagd.", "tokens": ["Macht", "auf", "sie", "das", "j\u00fcngs\u00b7te", "Ge\u00b7richt", "einst", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "ADV", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Ein' andre Speise giebt's nachher nicht,", "tokens": ["Ein'", "and\u00b7re", "Spei\u00b7se", "giebt's", "nach\u00b7her", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Hei\u00dft wohl mit Recht das letzte Gericht.\u00ab", "tokens": ["Hei\u00dft", "wohl", "mit", "Recht", "das", "letz\u00b7te", "Ge\u00b7richt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}