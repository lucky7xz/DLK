{"textgrid.poem.59837": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, J\u00fcnger, her! Ich will dich Weisheit lehren,", "tokens": ["Komm", ",", "J\u00fcn\u00b7ger", ",", "her", "!", "Ich", "will", "dich", "Weis\u00b7heit", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PTKVZ", "$.", "PPER", "VMFIN", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sollst des Daseins Wert erkennen lernen.", "tokens": ["Du", "sollst", "des", "Da\u00b7seins", "Wert", "er\u00b7ken\u00b7nen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NE", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst zum echten Glauben dich bekehren,", "tokens": ["Du", "sollst", "zum", "ech\u00b7ten", "Glau\u00b7ben", "dich", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Wahre von dem Falschen trennen lernen:", "tokens": ["Das", "Wah\u00b7re", "von", "dem", "Fal\u00b7schen", "tren\u00b7nen", "ler\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Lehre, wie des Wahns, der Torheit Klippen", "tokens": ["Die", "Leh\u00b7re", ",", "wie", "des", "Wahns", ",", "der", "Tor\u00b7heit", "Klip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klug zu umgehn, soll dir im Liede werden \u2013", "tokens": ["Klug", "zu", "um\u00b7gehn", ",", "soll", "dir", "im", "Lie\u00b7de", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "APPRART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohlredenheit und Anmut deinen Lippen,", "tokens": ["Wohl\u00b7re\u00b7den\u00b7heit", "und", "An\u00b7mut", "dei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deinem Herzen Gl\u00fcck und Friede werden!", "tokens": ["Und", "dei\u00b7nem", "Her\u00b7zen", "Gl\u00fcck", "und", "Frie\u00b7de", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Fort aus der alten Satzung dumpfen R\u00e4umen", "tokens": ["Fort", "aus", "der", "al\u00b7ten", "Sat\u00b7zung", "dum\u00b7pfen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich den Fu\u00df zu besserm Streben f\u00fchren \u2013", "tokens": ["Will", "ich", "den", "Fu\u00df", "zu", "bes\u00b7serm", "Stre\u00b7ben", "f\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei Wein und Liebe, unter Rosenb\u00e4umen", "tokens": ["Bei", "Wein", "und", "Lie\u00b7be", ",", "un\u00b7ter", "Ro\u00b7sen\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sollst du ein neues, sch\u00f6nres Leben f\u00fchren!", "tokens": ["Sollst", "du", "ein", "neu\u00b7es", ",", "sch\u00f6n\u00b7res", "Le\u00b7ben", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn du \u00fcbst, was meine Lieder predigen,", "tokens": ["Und", "wenn", "du", "\u00fcbst", ",", "was", "mei\u00b7ne", "Lie\u00b7der", "pre\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sollst du's offen, frohen Mutes \u00fcben: \u2013", "tokens": ["So", "sollst", "du's", "of\u00b7fen", ",", "fro\u00b7hen", "Mu\u00b7tes", "\u00fc\u00b7ben", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKVZ", "$,", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Heuchelei, des Truges dich entledigen", "tokens": ["Der", "Heu\u00b7che\u00b7lei", ",", "des", "Tru\u00b7ges", "dich", "ent\u00b7le\u00b7di\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und im geheimen nichts als Gutes \u00fcben!", "tokens": ["Und", "im", "ge\u00b7hei\u00b7men", "nichts", "als", "Gu\u00b7tes", "\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Kein Schwert hab' ich, die Toren zu bekehren;", "tokens": ["Kein", "Schwert", "hab'", "ich", ",", "die", "To\u00b7ren", "zu", "be\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer Weisheit \u00fcbt, legt andern keinen Zwang auf;", "tokens": ["Wer", "Weis\u00b7heit", "\u00fcbt", ",", "legt", "an\u00b7dern", "kei\u00b7nen", "Zwang", "auf", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "PIS", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mein Joch ist leicht, der Kern von meinen Lehren", "tokens": ["Mein", "Joch", "ist", "leicht", ",", "der", "Kern", "von", "mei\u00b7nen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00f6st sich in Wein, in Liebe und Gesang auf.", "tokens": ["L\u00f6st", "sich", "in", "Wein", ",", "in", "Lie\u00b7be", "und", "Ge\u00b7sang", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Unendlich ist der Sch\u00f6nheit Zauberkreis,", "tokens": ["Un\u00b7end\u00b7lich", "ist", "der", "Sch\u00f6n\u00b7heit", "Zau\u00b7ber\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unendlich sehnsuchtsvollen Dranges bleiben", "tokens": ["Un\u00b7end\u00b7lich", "sehn\u00b7suchts\u00b7vol\u00b7len", "Dran\u00b7ges", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschenherzen \u2013 doch wird stets der Preis", "tokens": ["Die", "Men\u00b7schen\u00b7her\u00b7zen", "\u2013", "doch", "wird", "stets", "der", "Preis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Zaubert\u00f6nen des Gesanges bleiben!", "tokens": ["Den", "Zau\u00b7ber\u00b7t\u00f6\u00b7nen", "des", "Ge\u00b7san\u00b7ges", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}