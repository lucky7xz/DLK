{"textgrid.poem.24147": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem heimlichen Schlosse gehn", "tokens": ["In", "mei\u00b7nem", "heim\u00b7li\u00b7chen", "Schlos\u00b7se", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei M\u00e4dchen nackt Trepp auf, Trepp ab,", "tokens": ["Zwei", "M\u00e4d\u00b7chen", "nackt", "Trepp", "auf", ",", "Trepp", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umarmen sich und k\u00fcssen sich", "tokens": ["Um\u00b7ar\u00b7men", "sich", "und", "k\u00fcs\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sind verliebt in sich und mich", "tokens": ["Und", "sind", "ver\u00b7liebt", "in", "sich", "und", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "PRF", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lachen.", "tokens": ["Und", "la\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "In meinem heimlichen Garten gehn", "tokens": ["In", "mei\u00b7nem", "heim\u00b7li\u00b7chen", "Gar\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei M\u00e4dchen nackt durch Busch und Beet;", "tokens": ["Zwei", "M\u00e4d\u00b7chen", "nackt", "durch", "Busch", "und", "Beet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne ist den Nackten hold,", "tokens": ["Die", "Son\u00b7ne", "ist", "den", "Nack\u00b7ten", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gie\u00dft \u00fcber ihre Sch\u00f6nheit Gold;", "tokens": ["Gie\u00dft", "\u00fc\u00b7ber", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie leuchten.", "tokens": ["Sie", "leuch\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "In meiner heimlichen Kammer ruhn", "tokens": ["In", "mei\u00b7ner", "heim\u00b7li\u00b7chen", "Kam\u00b7mer", "ruhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei M\u00e4dchen nackt auf ", "tokens": ["Zwei", "M\u00e4d\u00b7chen", "nackt", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Sommerwind ist um sie her,", "tokens": ["Der", "Som\u00b7mer\u00b7wind", "ist", "um", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sommernacht ist schw\u00fcl und schwer;", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "ist", "schw\u00fcl", "und", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie tr\u00e4umen.", "tokens": ["Sie", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "In meinem heimlichen Saale stehn", "tokens": ["In", "mei\u00b7nem", "heim\u00b7li\u00b7chen", "Saa\u00b7le", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei M\u00e4dchen nackt und sehn sich an.", "tokens": ["Zwei", "M\u00e4d\u00b7chen", "nackt", "und", "sehn", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor einander in die Knie", "tokens": ["Und", "vor", "ein\u00b7an\u00b7der", "in", "die", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Seufzer sinken sie", "tokens": ["Mit", "ei\u00b7nem", "Seuf\u00b7zer", "sin\u00b7ken", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und weinen.", "tokens": ["Und", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}