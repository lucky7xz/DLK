{"textgrid.poem.35091": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Kannst du noch beten?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst du noch beten? Sag, kannst du es noch?", "tokens": ["Kannst", "du", "noch", "be\u00b7ten", "?", "Sag", ",", "kannst", "du", "es", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nicht, so denk an deine Mutter doch,", "tokens": ["Wenn", "nicht", ",", "so", "denk", "an", "dei\u00b7ne", "Mut\u00b7ter", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie so liebend \u00fcber dir gewaltet", "tokens": ["Wie", "sie", "so", "lie\u00b7bend", "\u00fc\u00b7ber", "dir", "ge\u00b7wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dir die kleinen H\u00e4ndchen fromm gefaltet,", "tokens": ["Und", "dir", "die", "klei\u00b7nen", "H\u00e4nd\u00b7chen", "fromm", "ge\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Damit der liebe Gott ihr Gl\u00fcck bewahre,", "tokens": ["Da\u00b7mit", "der", "lie\u00b7be", "Gott", "ihr", "Gl\u00fcck", "be\u00b7wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dieses Gl\u00fcck warst du \u2013 \u2013 wie viele Jahre?", "tokens": ["Und", "die\u00b7ses", "Gl\u00fcck", "warst", "du", "\u2013", "\u2013", "wie", "vie\u00b7le", "Jah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "$(", "$(", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kannst du noch beten? Sag, kannst du es noch?", "tokens": ["Kannst", "du", "noch", "be\u00b7ten", "?", "Sag", ",", "kannst", "du", "es", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nicht, so denk an deine Kinder doch!", "tokens": ["Wenn", "nicht", ",", "so", "denk", "an", "dei\u00b7ne", "Kin\u00b7der", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4ltst du's f\u00fcr \u00fcberfl\u00fcssig, sie zu lehren,", "tokens": ["H\u00e4ltst", "du's", "f\u00fcr", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ",", "sie", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Herrn und Vater gl\u00e4ubig zu verehren?", "tokens": ["Den", "Herrn", "und", "Va\u00b7ter", "gl\u00e4u\u00b7big", "zu", "ver\u00b7eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was kann der irdische von ihnen wollen,", "tokens": ["Was", "kann", "der", "ir\u00b7di\u00b7sche", "von", "ih\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "APPR", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie den himmlischen nicht achten sollen?", "tokens": ["Wenn", "sie", "den", "himm\u00b7li\u00b7schen", "nicht", "ach\u00b7ten", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kannst du noch beten? Sag, kannst du es noch?", "tokens": ["Kannst", "du", "noch", "be\u00b7ten", "?", "Sag", ",", "kannst", "du", "es", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nicht, so fasse Muth; versuch es doch!", "tokens": ["Wenn", "nicht", ",", "so", "fas\u00b7se", "Muth", ";", "ver\u00b7such", "es", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wartet Gott wohl gar Zeit deines Lebens", "tokens": ["Es", "war\u00b7tet", "Gott", "wohl", "gar", "Zeit", "dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ADV", "NN", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur auf ein kleines W\u00f6rtchen, doch vergebens.", "tokens": ["Nur", "auf", "ein", "klei\u00b7nes", "W\u00f6rt\u00b7chen", ",", "doch", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Todesangst wird dieses Wort dir zeigen,", "tokens": ["Die", "To\u00b7de\u00b7sangst", "wird", "die\u00b7ses", "Wort", "dir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vielleicht zu sp\u00e4t; die Antwort ist dann \u2013 \u2013 Schweigen!", "tokens": ["Viel\u00b7leicht", "zu", "sp\u00e4t", ";", "die", "Ant\u00b7wort", "ist", "dann", "\u2013", "\u2013", "Schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$.", "ART", "NN", "VAFIN", "ADV", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}