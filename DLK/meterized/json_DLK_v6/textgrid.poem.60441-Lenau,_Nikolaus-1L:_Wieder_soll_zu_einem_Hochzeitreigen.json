{"textgrid.poem.60441": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder soll zu einem Hochzeitreigen", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder soll zu einem Hochzeitreigen", "tokens": ["Wie\u00b7der", "soll", "zu", "ei\u00b7nem", "Hoch\u00b7zei\u00b7trei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Zigeuner frische T\u00e4nze geigen;", "tokens": ["Der", "Zi\u00b7geu\u00b7ner", "fri\u00b7sche", "T\u00e4n\u00b7ze", "gei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zimbal, klinge hell vom Hammerschlage!", "tokens": ["Zim\u00b7bal", ",", "klin\u00b7ge", "hell", "vom", "Ham\u00b7mer\u00b7schla\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Klarinette, schmettre ins Gelage!", "tokens": ["Kla\u00b7ri\u00b7net\u00b7te", ",", "schmett\u00b7re", "ins", "Ge\u00b7la\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Im Husarenwams, vielfach geflickt,", "tokens": ["Im", "Hu\u00b7sa\u00b7ren\u00b7wams", ",", "viel\u00b7fach", "ge\u00b7flickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit verblichnem Golde reich gestickt", "tokens": ["Mit", "ver\u00b7blich\u00b7nem", "Gol\u00b7de", "reich", "ge\u00b7stickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und geziert mit mottenhaftem Brame,", "tokens": ["Und", "ge\u00b7ziert", "mit", "mot\u00b7ten\u00b7haf\u00b7tem", "Bra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "N\u00e4hert Mischka sich dem Br\u00e4utigame.", "tokens": ["N\u00e4\u00b7hert", "Mischka", "sich", "dem", "Br\u00e4u\u00b7ti\u00b7ga\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und er spricht mit b\u00fcckendem Verneigen:", "tokens": ["Und", "er", "spricht", "mit", "b\u00fc\u00b7cken\u00b7dem", "Ver\u00b7nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbm\u00f6cht es Eurer Herrlichkeit gefallen,", "tokens": ["\u00bb", "m\u00f6cht", "es", "Eu\u00b7rer", "Herr\u00b7lich\u00b7keit", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Eh die frischen T\u00e4nze hier erschallen,", "tokens": ["Eh", "die", "fri\u00b7schen", "T\u00e4n\u00b7ze", "hier", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mich zu h\u00f6ren erst ein Solo geigen.", "tokens": ["Mich", "zu", "h\u00f6\u00b7ren", "erst", "ein", "So\u00b7lo", "gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Damit m\u00f6cht ich Eure Gunst erwerben;", "tokens": ["Da\u00b7mit", "m\u00f6cht", "ich", "Eu\u00b7re", "Gunst", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Habs zu Eurem Ehrentag erfunden,", "tokens": ["Habs", "zu", "Eu\u00b7rem", "Eh\u00b7ren\u00b7tag", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Sch\u00f6n ists, Herr, so herzlich tief empfunden,", "tokens": ["Sch\u00f6n", "ists", ",", "Herr", ",", "so", "herz\u00b7lich", "tief", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "NN", "$,", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Da\u00df vor Lust der H\u00f6rer m\u00f6chte sterben.\u00ab", "tokens": ["Da\u00df", "vor", "Lust", "der", "H\u00f6\u00b7rer", "m\u00f6ch\u00b7te", "ster\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbsei gew\u00e4hrt der Bitte\u00ab, spricht der Graf,", "tokens": ["\u00bb", "sei", "ge\u00b7w\u00e4hrt", "der", "Bit\u00b7te", "\u00ab", ",", "spricht", "der", "Graf", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVPP", "ART", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den das Auge des Zigeuners traf,", "tokens": ["Den", "das", "Au\u00b7ge", "des", "Zi\u00b7geu\u00b7ners", "traf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hell, wie eines Seelendolches Blinken,", "tokens": ["Hell", ",", "wie", "ei\u00b7nes", "See\u00b7len\u00b7dol\u00b7ches", "Blin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbspiele, sollst daf\u00fcr Tokayer trinken!\u00ab \u2013", "tokens": ["\u00bb", "spie\u00b7le", ",", "sollst", "da\u00b7f\u00fcr", "To\u00b7ka\u00b7yer", "trin\u00b7ken", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VMFIN", "PAV", "NN", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Stille wird der Saal, wie Miras Gruft;", "tokens": ["Stil\u00b7le", "wird", "der", "Saal", ",", "wie", "Mi\u00b7ras", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PWAV", "NE", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Alles hat um Mischka sich geschart,", "tokens": ["Al\u00b7les", "hat", "um", "Mischka", "sich", "ge\u00b7schart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NE", "PRF", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und er l\u00e4\u00dft den Bogen, frisch behaart,", "tokens": ["Und", "er", "l\u00e4\u00dft", "den", "Bo\u00b7gen", ",", "frisch", "be\u00b7haart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie versuchend, sausen durch die Luft.", "tokens": ["Wie", "ver\u00b7su\u00b7chend", ",", "sau\u00b7sen", "durch", "die", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Pl\u00f6tzlich streicht er durch die Saiten alle", "tokens": ["Pl\u00f6tz\u00b7lich", "streicht", "er", "durch", "die", "Sai\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und durch alle Herzen, schnell bemeistert;", "tokens": ["Und", "durch", "al\u00b7le", "Her\u00b7zen", ",", "schnell", "be\u00b7meis\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Seine Geige in der Freudenhalle", "tokens": ["Sei\u00b7ne", "Gei\u00b7ge", "in", "der", "Freu\u00b7den\u00b7hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Hat zur Racheg\u00f6ttin sich begeistert.", "tokens": ["Hat", "zur", "Ra\u00b7che\u00b7g\u00f6t\u00b7tin", "sich", "be\u00b7geis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Frevler! horch! in diesem s\u00fc\u00dfen Liede", "tokens": ["Frev\u00b7ler", "!", "horch", "!", "in", "die\u00b7sem", "s\u00fc\u00b7\u00dfen", "Lie\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "S\u00e4uselt und verweht der Unschuld Friede; \u2013", "tokens": ["S\u00e4u\u00b7selt", "und", "ver\u00b7weht", "der", "Un\u00b7schuld", "Frie\u00b7de", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "H\u00f6rst du, wie der Blitz der Liebe z\u00fcndet?", "tokens": ["H\u00f6rst", "du", ",", "wie", "der", "Blitz", "der", "Lie\u00b7be", "z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Wie ihr ganzes Herz in deines m\u00fcndet? \u2013", "tokens": ["Wie", "ihr", "gan\u00b7zes", "Herz", "in", "dei\u00b7nes", "m\u00fcn\u00b7det", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Jener Brautnacht unerme\u00dfne Wonnen,", "tokens": ["Je\u00b7ner", "Braut\u00b7nacht", "un\u00b7er\u00b7me\u00df\u00b7ne", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Wie sie in ein Meer von Schmerz zerronnen? \u2013", "tokens": ["Wie", "sie", "in", "ein", "Meer", "von", "Schmerz", "zer\u00b7ron\u00b7nen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "St\u00fcrmen h\u00f6rst du der Verla\u00dfnen Klagen;", "tokens": ["St\u00fcr\u00b7men", "h\u00f6rst", "du", "der", "Ver\u00b7la\u00df\u00b7nen", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "H\u00f6rst den Wurm an ihrer Bl\u00fcte nagen; \u2013", "tokens": ["H\u00f6rst", "den", "Wurm", "an", "ih\u00b7rer", "Bl\u00fc\u00b7te", "na\u00b7gen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Horch, wie sie, zum Tod schon auf der Flucht,", "tokens": ["Horch", ",", "wie", "sie", ",", "zum", "Tod", "schon", "auf", "der", "Flucht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Weinend dich durch alle W\u00e4lder sucht;", "tokens": ["Wei\u00b7nend", "dich", "durch", "al\u00b7le", "W\u00e4l\u00b7der", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Wie sie alle G\u00f6tter ruft um Hilfe,", "tokens": ["Wie", "sie", "al\u00b7le", "G\u00f6t\u00b7ter", "ruft", "um", "Hil\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Bis sie tot zusammenbricht im Schilfe. \u2013", "tokens": ["Bis", "sie", "tot", "zu\u00b7sam\u00b7men\u00b7bricht", "im", "Schil\u00b7fe", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Furchtbar l\u00e4\u00dft der Alte deinem Lauschen", "tokens": ["Furcht\u00b7bar", "l\u00e4\u00dft", "der", "Al\u00b7te", "dei\u00b7nem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Durch die Saiten die Vergeltung rauschen! \u2013", "tokens": ["Durch", "die", "Sai\u00b7ten", "die", "Ver\u00b7gel\u00b7tung", "rau\u00b7schen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Aus dem Saal ist jede Lust gewichen,", "tokens": ["Aus", "dem", "Saal", "ist", "je\u00b7de", "Lust", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Dunkles Weh durch alle Herzen schl\u00e4gt;", "tokens": ["Dunk\u00b7les", "Weh", "durch", "al\u00b7le", "Her\u00b7zen", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Und nicht wissend, was sie tief bewegt,", "tokens": ["Und", "nicht", "wis\u00b7send", ",", "was", "sie", "tief", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Hat die Braut sich weinend fortgeschlichen.", "tokens": ["Hat", "die", "Braut", "sich", "wei\u00b7nend", "fort\u00b7ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Von der Macht gejagt des Racheschalls,", "tokens": ["Von", "der", "Macht", "ge\u00b7jagt", "des", "Ra\u00b7ch\u00b7e\u00b7schalls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Eilt der junge Br\u00e4utigam zu Rosse,", "tokens": ["Eilt", "der", "jun\u00b7ge", "Br\u00e4u\u00b7ti\u00b7gam", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sprengt in finstrer Nacht aus seinem Schlosse,", "tokens": ["Sprengt", "in", "finst\u00b7rer", "Nacht", "aus", "sei\u00b7nem", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", 