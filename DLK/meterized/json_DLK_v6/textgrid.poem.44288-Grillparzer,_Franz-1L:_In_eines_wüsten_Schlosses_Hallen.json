{"textgrid.poem.44288": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: In eines w\u00fcsten Schlosses Hallen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eines w\u00fcsten Schlosses Hallen", "tokens": ["In", "ei\u00b7nes", "w\u00fcs\u00b7ten", "Schlos\u00b7ses", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag eingesperrt,", "tokens": ["Lag", "ein\u00b7ge\u00b7sperrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bewacht von eines Drachen Krallen,", "tokens": ["Be\u00b7wacht", "von", "ei\u00b7nes", "Dra\u00b7chen", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Zauberschwert.", "tokens": ["Ein", "Zau\u00b7ber\u00b7schwert", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schon mancher suchte es zu heben,", "tokens": ["Schon", "man\u00b7cher", "such\u00b7te", "es", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergebens doch,", "tokens": ["Ver\u00b7ge\u00b7bens", "doch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Drache machte alles beben,", "tokens": ["Der", "Dra\u00b7che", "mach\u00b7te", "al\u00b7les", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im finstern Loch.", "tokens": ["Im", "fins\u00b7tern", "Loch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Bis doch ein Paladin aus Franken", "tokens": ["Bis", "doch", "ein", "Pa\u00b7la\u00b7din", "aus", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vorm Schlosse h\u00e4lt,", "tokens": ["Vorm", "Schlos\u00b7se", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der oft schon k\u00fchn zerbrach die Schranken", "tokens": ["Der", "oft", "schon", "k\u00fchn", "zer\u00b7brach", "die", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geisterwelt.", "tokens": ["Der", "Geis\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Wagling eilt mit k\u00fchnem Mute", "tokens": ["Der", "Wag\u00b7ling", "eilt", "mit", "k\u00fch\u00b7nem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum finstern Grab", "tokens": ["Zum", "fins\u00b7tern", "Grab"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und steiget rasch mit k\u00fchlem Blute", "tokens": ["Und", "stei\u00b7get", "rasch", "mit", "k\u00fch\u00b7lem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gang hinab.", "tokens": ["Den", "Gang", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Er k\u00f6mmt zur Gruft. Der Drache brauset", "tokens": ["Er", "k\u00f6mmt", "zur", "Gruft", ".", "Der", "Dra\u00b7che", "brau\u00b7set"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Nest hervor,", "tokens": ["Vom", "Nest", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Ritters Damaszener sauset", "tokens": ["Des", "Rit\u00b7ters", "Da\u00b7mas\u00b7ze\u00b7ner", "sau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dem Vieh ums Ohr.", "tokens": ["Dem", "Vieh", "ums", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Sein Rachen gl\u00fcht im Feur und Dampfe,", "tokens": ["Sein", "Ra\u00b7chen", "gl\u00fcht", "im", "Feur", "und", "Damp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Br\u00fcllen g\u00e4llt", "tokens": ["Sein", "Br\u00fcl\u00b7len", "g\u00e4llt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Durchs Schlo\u00df, bis doch, nach langem Kampfe,", "tokens": ["Durchs", "Schlo\u00df", ",", "bis", "doch", ",", "nach", "lan\u00b7gem", "Kamp\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Untier f\u00e4llt.", "tokens": ["Das", "Un\u00b7tier", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Das Schwert durchbohret seine Seite.", "tokens": ["Das", "Schwert", "durch\u00b7boh\u00b7ret", "sei\u00b7ne", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach seinem Fall", "tokens": ["Nach", "sei\u00b7nem", "Fall"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nimmt unser Ritter hin, als Beute,", "tokens": ["Nimmt", "un\u00b7ser", "Rit\u00b7ter", "hin", ",", "als", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Zauberstahl.", "tokens": ["Den", "Zau\u00b7ber\u00b7stahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Nun m\u00e4het, gleich den flei\u00dfgen Schnittern", "tokens": ["Nun", "m\u00e4\u00b7het", ",", "gleich", "den", "flei\u00df\u00b7gen", "Schnit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im \u00c4hrenmeer,", "tokens": ["Im", "\u00c4h\u00b7ren\u00b7meer", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er unter allen schwachen Rittern", "tokens": ["Er", "un\u00b7ter", "al\u00b7len", "schwa\u00b7chen", "Rit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Welt umher.", "tokens": ["Der", "Welt", "um\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Doch endlich kam ein tapfrer Fechter,", "tokens": ["Doch", "end\u00b7lich", "kam", "ein", "tapf\u00b7rer", "Fech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Stimme schallt", "tokens": ["Des", "Stim\u00b7me", "schallt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem ewig nimmer m\u00fcden Schl\u00e4chter", "tokens": ["Dem", "e\u00b7wig", "nim\u00b7mer", "m\u00fc\u00b7den", "Schl\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein donnernd \u00bbHalt!\u00ab", "tokens": ["Ein", "don\u00b7nernd", "\u00bb", "Halt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJD", "$(", "VVIMP", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Sie fechten. Doch im Kampf zerschellet", "tokens": ["Sie", "fech\u00b7ten", ".", "Doch", "im", "Kampf", "zer\u00b7schel\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Talisman;", "tokens": ["Der", "Ta\u00b7lis\u00b7man", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-++-", "measure": "unknown.measure.di"}, "line.3": {"text": "Und, ach! der fremde Ritter f\u00e4llet", "tokens": ["Und", ",", "ach", "!", "der", "frem\u00b7de", "Rit\u00b7ter", "f\u00e4l\u00b7let"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Wundermann.", "tokens": ["Den", "Wun\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Ihr, die, die Rel'gion zu sch\u00e4nden,", "tokens": ["Ihr", ",", "die", ",", "die", "Rel'\u00b7gi\u00b7on", "zu", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sophismen lehrt,", "tokens": ["So\u00b7phis\u00b7men", "lehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Merkts euch! Ihr f\u00fchrt mit frechen H\u00e4nden", "tokens": ["Merkts", "euch", "!", "Ihr", "f\u00fchrt", "mit", "fre\u00b7chen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Zauberschwert.", "tokens": ["Das", "Zau\u00b7ber\u00b7schwert", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Zwar werden manchesmal die Schwachen", "tokens": ["Zwar", "wer\u00b7den", "man\u00b7ches\u00b7mal", "die", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von euch besiegt;", "tokens": ["Von", "euch", "be\u00b7siegt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch mu\u00df der Weise euch verlachen,", "tokens": ["Doch", "mu\u00df", "der", "Wei\u00b7se", "euch", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ihr bekriegt.", "tokens": ["Den", "ihr", "be\u00b7kriegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}