{"textgrid.poem.43052": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich blickte j\u00fcngst in mich \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich blickte j\u00fcngst in mich \u2013", "tokens": ["Ich", "blick\u00b7te", "j\u00fcngst", "in", "mich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So recht in's Herz hinein", "tokens": ["So", "recht", "in's", "Herz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und glaubte noch etwas zu finden", "tokens": ["Und", "glaub\u00b7te", "noch", "et\u00b7was", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Ich sah mein verlornes Eden,", "tokens": ["Ich", "sah", "mein", "ver\u00b7lor\u00b7nes", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein versunkenes Paradies,", "tokens": ["Mein", "ver\u00b7sun\u00b7ke\u00b7nes", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mich selbst den gefallenen Engel,", "tokens": ["Mich", "selbst", "den", "ge\u00b7fal\u00b7le\u00b7nen", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Himmel und Erde verstie\u00df.", "tokens": ["Den", "Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}