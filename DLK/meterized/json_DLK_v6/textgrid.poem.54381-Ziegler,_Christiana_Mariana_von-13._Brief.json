{"textgrid.poem.54381": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "13. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mir dein letzter Brief, entfernter Freund, entdeckt,", "tokens": ["Was", "mir", "dein", "letz\u00b7ter", "Brief", ",", "ent\u00b7fern\u00b7ter", "Freund", ",", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das hat mich wahrlich nicht gewundert und erschreckt.", "tokens": ["Das", "hat", "mich", "wahr\u00b7lich", "nicht", "ge\u00b7wun\u00b7dert", "und", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sorgte vor dein Gl\u00fcck und vor dein Wohlbefinden;", "tokens": ["Ich", "sorg\u00b7te", "vor", "dein", "Gl\u00fcck", "und", "vor", "dein", "Wohl\u00b7be\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Schweigen konnt ich zwar auf keine Art ergr\u00fcnden,", "tokens": ["Dein", "Schwei\u00b7gen", "konnt", "ich", "zwar", "auf", "kei\u00b7ne", "Art", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch endlich schlug ich mir die Sorgen aus dem Sinn.", "tokens": ["Doch", "end\u00b7lich", "schlug", "ich", "mir", "die", "Sor\u00b7gen", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das st\u00f6rt die Freundschaft nicht, du weist schon wie ich bin.", "tokens": ["Das", "st\u00f6rt", "die", "Freund\u00b7schaft", "nicht", ",", "du", "weist", "schon", "wie", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich bin auch wieder gut, nachdem du mir geschrieben.", "tokens": ["Ich", "bin", "auch", "wie\u00b7der", "gut", ",", "nach\u00b7dem", "du", "mir", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bey deinem Aufenthalt kann ich mich nicht betr\u00fcben.", "tokens": ["Bey", "dei\u00b7nem", "Auf\u00b7ent\u00b7halt", "kann", "ich", "mich", "nicht", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dich sucht der Kriegesgott, mir ist der Pallas hold,", "tokens": ["Dich", "sucht", "der", "Krie\u00b7ges\u00b7gott", ",", "mir", "ist", "der", "Pal\u00b7las", "hold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So stehen wir zugleich in beyder G\u00f6tter Sold;", "tokens": ["So", "ste\u00b7hen", "wir", "zu\u00b7gleich", "in", "bey\u00b7der", "G\u00f6t\u00b7ter", "Sold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "La\u00df dich nur nicht den Schein von vielen Sch\u00e4tzen blenden;", "tokens": ["La\u00df", "dich", "nur", "nicht", "den", "Schein", "von", "vie\u00b7len", "Sch\u00e4t\u00b7zen", "blen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das h\u00f6chste Wesen hat blo\u00df unser Gl\u00fcck in H\u00e4nden.", "tokens": ["Das", "h\u00f6chs\u00b7te", "We\u00b7sen", "hat", "blo\u00df", "un\u00b7ser", "Gl\u00fcck", "in", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier gilt kein Wissen nicht, und keine Tapferkeit;", "tokens": ["Hier", "gilt", "kein", "Wis\u00b7sen", "nicht", ",", "und", "kei\u00b7ne", "Tap\u00b7fer\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was dieses uns bestimmt, das kommt zu rechter Zeit;", "tokens": ["Was", "die\u00b7ses", "uns", "be\u00b7stimmt", ",", "das", "kommt", "zu", "rech\u00b7ter", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O! richte dich nach mir, und lebe ohne Sorgen,", "tokens": ["O", "!", "rich\u00b7te", "dich", "nach", "mir", ",", "und", "le\u00b7be", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PRF", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich denke niemals nicht, wie geht es dir wohl morgen?", "tokens": ["Ich", "den\u00b7ke", "nie\u00b7mals", "nicht", ",", "wie", "geht", "es", "dir", "wohl", "mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich folge dem Geschick, bey Sturm und Sonnenschein;", "tokens": ["Ich", "fol\u00b7ge", "dem", "Ge\u00b7schick", ",", "bey", "Sturm", "und", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mein Kopf mu\u00df aufger\u00e4umt und immer munter seyn.", "tokens": ["Mein", "Kopf", "mu\u00df", "auf\u00b7ge\u00b7r\u00e4umt", "und", "im\u00b7mer", "mun\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVPP", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kann mir nichts zu schwer und unertr\u00e4glich fallen.", "tokens": ["So", "kann", "mir", "nichts", "zu", "schwer", "und", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "PTKA", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn ich was w\u00fcnschen soll, so w\u00fcnsch ich mir vor allen", "tokens": ["Wenn", "ich", "was", "w\u00fcn\u00b7schen", "soll", ",", "so", "w\u00fcnsch", "ich", "mir", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADJD", "PPER", "PRF", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nur einen wahren Freund, von altem Schrot und Korn;", "tokens": ["Nur", "ei\u00b7nen", "wah\u00b7ren", "Freund", ",", "von", "al\u00b7tem", "Schrot", "und", "Korn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Alsdann so scheu ich nicht der Sp\u00f6tter Wut und Zorn.", "tokens": ["Als\u00b7dann", "so", "scheu", "ich", "nicht", "der", "Sp\u00f6t\u00b7ter", "Wut", "und", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "PTKNEG", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein Wunsch ist mit Vernunft und Vorbedacht geschehen.", "tokens": ["Mein", "Wunsch", "ist", "mit", "Ver\u00b7nunft", "und", "Vor\u00b7be\u00b7dacht", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das wirst du mir mit Recht, mein Freund, auch zugestehen.", "tokens": ["Das", "wirst", "du", "mir", "mit", "Recht", ",", "mein", "Freund", ",", "auch", "zu\u00b7ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PRF", "APPR", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erinnere dich nur noch der l\u00e4ngst vergangnen Zeit;", "tokens": ["E\u00b7rin\u00b7ne\u00b7re", "dich", "nur", "noch", "der", "l\u00e4ngst", "ver\u00b7gang\u00b7nen", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Man h\u00f6rte damals nur von Rasen, Zank und Streit.", "tokens": ["Man", "h\u00f6r\u00b7te", "da\u00b7mals", "nur", "von", "Ra\u00b7sen", ",", "Zank", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich seh den blassen Neid annoch die Z\u00e4hne fl\u00e4mmen;", "tokens": ["Ich", "seh", "den", "blas\u00b7sen", "Neid", "an\u00b7noch", "die", "Z\u00e4h\u00b7ne", "fl\u00e4m\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Er wolte Thyrsis Gl\u00fcck auf alle Weise hemmen.", "tokens": ["Er", "wol\u00b7te", "Thyr\u00b7sis", "Gl\u00fcck", "auf", "al\u00b7le", "Wei\u00b7se", "hem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was hat er ausgericht? er wird noch ausgelacht;", "tokens": ["Was", "hat", "er", "aus\u00b7ge\u00b7richt", "?", "er", "wird", "noch", "aus\u00b7ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil itzt mehr als ein Freund vor Thyrsis Wohlfarth wacht.", "tokens": ["Weil", "itzt", "mehr", "als", "ein", "Freund", "vor", "Thyr\u00b7sis", "Wohl\u00b7farth", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "KOKOM", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du kannst dir in der That dergleichen Gl\u00fcck versprechen.", "tokens": ["Du", "kannst", "dir", "in", "der", "That", "derg\u00b7lei\u00b7chen", "Gl\u00fcck", "ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wollt ein jeglicher die Freundschaft mit dir brechen", "tokens": ["Und", "wollt", "ein", "jeg\u00b7li\u00b7cher", "die", "Freund\u00b7schaft", "mit", "dir", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "PIAT", "ART", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So kehre dich an nichts, mich findst du einerley,", "tokens": ["So", "keh\u00b7re", "dich", "an", "nichts", ",", "mich", "findst", "du", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PIS", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich scherze niemals nicht mit Freundschaft, Schwur und Treu.", "tokens": ["Ich", "scher\u00b7ze", "nie\u00b7mals", "nicht", "mit", "Freund\u00b7schaft", ",", "Schwur", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "NN", "$,", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mich d\u00fcnkt du hast bereits davon die st\u00e4rksten Proben.", "tokens": ["Mich", "d\u00fcnkt", "du", "hast", "be\u00b7reits", "da\u00b7von", "die", "st\u00e4rks\u00b7ten", "Pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und willst du mich auch gleich nicht ins Gesichte loben,", "tokens": ["Und", "willst", "du", "mich", "auch", "gleich", "nicht", "ins", "Ge\u00b7sich\u00b7te", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So zeiget doch dein Brief, so viel ich lesen kann,", "tokens": ["So", "zei\u00b7get", "doch", "dein", "Brief", ",", "so", "viel", "ich", "le\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Du kennst mein gutes Herz, und zweifelst nicht daran.", "tokens": ["Du", "kennst", "mein", "gu\u00b7tes", "Herz", ",", "und", "zwei\u00b7felst", "nicht", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Indessen will ich mich zu deinem Trost bequemen,", "tokens": ["In\u00b7des\u00b7sen", "will", "ich", "mich", "zu", "dei\u00b7nem", "Trost", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dank und Erkenntlichkeit von dir auch anzunehmen.", "tokens": ["Dank", "und", "Er\u00b7kennt\u00b7lich\u00b7keit", "von", "dir", "auch", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "ADV", "VVIZU", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.41": {"text": "Den Willen sch\u00e4tz ich schon so hoch als wie die That.", "tokens": ["Den", "Wil\u00b7len", "sch\u00e4tz", "ich", "schon", "so", "hoch", "als", "wie", "die", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das ist ja schon genug, wenn man giebt was man hat.", "tokens": ["Das", "ist", "ja", "schon", "ge\u00b7nug", ",", "wenn", "man", "giebt", "was", "man", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "PIS", "VVFIN", "PWS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Hiermit empfehl ich mich zu stetem Angedenken", "tokens": ["Hier\u00b7mit", "emp\u00b7fehl", "ich", "mich", "zu", "ste\u00b7tem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mit diesem will dich hinwiederum beschenken.", "tokens": ["Mit", "die\u00b7sem", "will", "dich", "hin\u00b7wie\u00b7de\u00b7rum", "be\u00b7schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}