{"dta.poem.23407": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ideen zur europ\u00e4ischen V\u00f6lkergeschichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind nur darum Europas Staaten,", "tokens": ["Sind", "nur", "da\u00b7rum", "Eu\u00b7ro\u00b7pas", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df die Soldaten gr\u00fcnen und bl\u00fchn?", "tokens": ["Da\u00df", "die", "Sol\u00b7da\u00b7ten", "gr\u00fc\u00b7nen", "und", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "M\u00fcssen f\u00fcr drei Millionen Soldaten", "tokens": ["M\u00fcs\u00b7sen", "f\u00fcr", "drei", "Mil\u00b7lion\u00b7en", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "NN", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Unsre zweihundert Millionen sich m\u00fchn?", "tokens": ["Uns\u00b7re", "zwei\u00b7hun\u00b7dert", "Mil\u00b7lion\u00b7en", "sich", "m\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "PRF", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Freilich, das ist das Gl\u00fcck das moderne!", "tokens": ["Frei\u00b7lich", ",", "das", "ist", "das", "Gl\u00fcck", "das", "mo\u00b7der\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das uns gelehrt hat Soldaten erziehn:", "tokens": ["Das", "uns", "ge\u00b7lehrt", "hat", "Sol\u00b7da\u00b7ten", "er\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ganz Europa ist eine Kaserne,", "tokens": ["Ganz", "Eu\u00b7ro\u00b7pa", "ist", "ei\u00b7ne", "Ka\u00b7ser\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Alles Dressur und Disciplin.", "tokens": ["Al\u00b7les", "Dres\u00b7sur", "und", "Dis\u00b7cip\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}