{"dta.poem.1289": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dieser Tag ist nun zum Ende/", "tokens": ["Die\u00b7ser", "Tag", "ist", "nun", "zum", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Braunen Schattens tunckler Flor", "tokens": ["Brau\u00b7nen", "Schat\u00b7tens", "tunck\u00b7ler", "Flor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcllt sich um des Himmels W\u00e4nde/", "tokens": ["H\u00fcllt", "sich", "um", "des", "Him\u00b7mels", "W\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Birgt der muntern Sternen-Chor/", "tokens": ["Birgt", "der", "mun\u00b7tern", "Ster\u00b7nen\u00b7Chor", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00fcbe Nacht und d\u00fcstres Schrecken", "tokens": ["Tr\u00fc\u00b7be", "Nacht", "und", "d\u00fcst\u00b7res", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will den Krei\u00df der Erde decken.", "tokens": ["Will", "den", "Krei\u00df", "der", "Er\u00b7de", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dunst und Thau umzieht die Felder/", "tokens": ["Dunst", "und", "Thau", "um\u00b7zieht", "die", "Fel\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die man itzo ledig sp\u00fcrt/", "tokens": ["Die", "man", "it\u00b7zo", "le\u00b7dig", "sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winde spielen durch die W\u00e4lder/", "tokens": ["Win\u00b7de", "spie\u00b7len", "durch", "die", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Haubt sich zitternd r\u00fchrt;", "tokens": ["De\u00b7ren", "Haubt", "sich", "zit\u00b7ternd", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Thiere ruhen/ Menschen schweigen/", "tokens": ["Thie\u00b7re", "ru\u00b7hen", "/", "Men\u00b7schen", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bi\u00df die Sonn ihr Licht wird zeigen.", "tokens": ["Bi\u00df", "die", "Sonn", "ihr", "Licht", "wird", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwartz und finster sind die Thaten/", "tokens": ["Schwartz", "und", "fins\u00b7ter", "sind", "die", "Tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich diesen Tag gehegt/", "tokens": ["Die", "ich", "die\u00b7sen", "Tag", "ge\u00b7hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin aus der Bahn gerathen/", "tokens": ["Ich", "bin", "aus", "der", "Bahn", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Welche zu dem Himmel tr\u00e4gt/", "tokens": ["Wel\u00b7che", "zu", "dem", "Him\u00b7mel", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum f\u00fchl ich auch im Hertzen", "tokens": ["Da\u00b7rum", "f\u00fchl", "ich", "auch", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reu und Furcht und bange Schmertzen.", "tokens": ["Reu", "und", "Furcht", "und", "ban\u00b7ge", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stechen heisser Sonnen Blicke/", "tokens": ["Ste\u00b7chen", "heis\u00b7ser", "Son\u00b7nen", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Zorn sticht noch so sehr/", "tokens": ["Got\u00b7tes", "Zorn", "sticht", "noch", "so", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurt man/ wenn der Himmel dicke/", "tokens": ["Traurt", "man", "/", "wenn", "der", "Him\u00b7mel", "di\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn GOtt wittert/ noch vielmehr/", "tokens": ["Wenn", "Gott", "wit\u00b7tert", "/", "noch", "viel\u00b7mehr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$(", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Besser ists/ als im Gewissen", "tokens": ["Bes\u00b7ser", "ists", "/", "als", "im", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$(", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Gunst/ den Tag vermissen.", "tokens": ["Sei\u00b7ne", "Gunst", "/", "den", "Tag", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Adam mu\u00df das Feuer f\u00fchlen/", "tokens": ["A\u00b7dam", "mu\u00df", "das", "Feu\u00b7er", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches seine Bl\u00f6sse brennt/", "tokens": ["Wel\u00b7ches", "sei\u00b7ne", "Bl\u00f6s\u00b7se", "brennt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch da sich der Tag will k\u00fchlen/", "tokens": ["Doch", "da", "sich", "der", "Tag", "will", "k\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wird das Hertzeleid gewendt/", "tokens": ["Wird", "das", "Hert\u00b7ze\u00b7leid", "ge\u00b7wendt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gottes Ruff/ sein Thau der G\u00fctte/", "tokens": ["Got\u00b7tes", "Ruff", "/", "sein", "Thau", "der", "G\u00fct\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Labt sein schmachtendes Gem\u00fctte.", "tokens": ["Labt", "sein", "schmach\u00b7ten\u00b7des", "Ge\u00b7m\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ruffe mich/ O GOtt/ desgleichen/", "tokens": ["Ruf\u00b7fe", "mich", "/", "O", "Gott", "/", "des\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NE", "NN", "$(", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch in Gnaden/ izt zu dir/", "tokens": ["Doch", "in", "Gna\u00b7den", "/", "izt", "zu", "dir", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$(", "ADV", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich schon f\u00fcr Furcht erbleichen/", "tokens": ["Mu\u00df", "ich", "schon", "f\u00fcr", "Furcht", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil nichts guttes wohnt in mir/", "tokens": ["Weil", "nichts", "gut\u00b7tes", "wohnt", "in", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJA", "VVFIN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tilgt doch dieser mein Verbrechen", "tokens": ["Tilgt", "doch", "die\u00b7ser", "mein", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PDAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sich lie\u00df die Schlange stechen.", "tokens": ["Der", "sich", "lie\u00df", "die", "Schlan\u00b7ge", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ob mich Grab und H\u00f6lle schrecket", "tokens": ["Ob", "mich", "Grab", "und", "H\u00f6l\u00b7le", "schre\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Todes-Nacht mir dr\u00e4ut/", "tokens": ["Und", "die", "To\u00b7des\u00b7Nacht", "mir", "dr\u00e4ut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Meine Fehler sind bedecket", "tokens": ["Mei\u00b7ne", "Feh\u00b7ler", "sind", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des reinen Lammes Kleid", "tokens": ["Durch", "des", "rei\u00b7nen", "Lam\u00b7mes", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Nacht und Finsternissen", "tokens": ["Un\u00b7ter", "Nacht", "und", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan ich Licht im Hertzen wissen.", "tokens": ["Kan", "ich", "Licht", "im", "Hert\u00b7zen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df die Decke meiner S\u00fcnden/", "tokens": ["La\u00df", "die", "De\u00b7cke", "mei\u00b7ner", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Hertz umnebelt hat/", "tokens": ["Die", "mein", "Hertz", "um\u00b7ne\u00b7belt", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der finstern Nacht verschwinden/", "tokens": ["Mit", "der", "fins\u00b7tern", "Nacht", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segne meine Lagerstatt/", "tokens": ["Seg\u00b7ne", "mei\u00b7ne", "La\u00b7ger\u00b7statt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich mit verneuten Sinnen", "tokens": ["Da\u00df", "ich", "mit", "ver\u00b7neu\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgen kan dein Lob beginnen.", "tokens": ["Mor\u00b7gen", "kan", "dein", "Lob", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}