{"textgrid.poem.53529": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ein Deutschland!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feierlich treten wir nunmehr in das Jahr 1919,", "tokens": ["Fei\u00b7er\u00b7lich", "tre\u00b7ten", "wir", "nun\u00b7mehr", "in", "das", "Jahr", "19\u00b719", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "number", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "CARD", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "und es freut uns, da\u00df wir allhier versammelt Feind und Freund sehn;", "tokens": ["und", "es", "freut", "uns", ",", "da\u00df", "wir", "all\u00b7hier", "ver\u00b7sam\u00b7melt", "Feind", "und", "Freund", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "unserm tierischen Gehaben entsprechend wollen wir sie beschnuppern und betrachten,", "tokens": ["un\u00b7serm", "tie\u00b7ri\u00b7schen", "Ge\u00b7ha\u00b7ben", "ent\u00b7spre\u00b7chend", "wol\u00b7len", "wir", "sie", "be\u00b7schnup\u00b7pern", "und", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VMFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+--+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "und, je nachdem, bei\u00dfen oder auf den Popo klapsen oder schweigend achten.", "tokens": ["und", ",", "je", "nach\u00b7dem", ",", "bei\u00b7\u00dfen", "o\u00b7der", "auf", "den", "Po\u00b7po", "klap\u00b7sen", "o\u00b7der", "schwei\u00b7gend", "ach\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOUS", "$,", "VVINF", "KON", "APPR", "ART", "NN", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie ist das zun\u00e4chst mit Oberschlesien?", "tokens": ["Wie", "ist", "das", "zu\u00b7n\u00e4chst", "mit", "O\u00b7berschle\u00b7si\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind da die Herren Schwarzr\u00f6cke im Spiel gewesien?", "tokens": ["Sind", "da", "die", "Her\u00b7ren", "Schwarz\u00b7r\u00f6\u00b7cke", "im", "Spiel", "ge\u00b7we\u00b7si\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-++--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Oder markieren alldort die lieben Polen", "tokens": ["O\u00b7der", "mar\u00b7kie\u00b7ren", "all\u00b7dort", "die", "lie\u00b7ben", "Po\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "den Teufel, der die Deutschen will holen?", "tokens": ["den", "Teu\u00b7fel", ",", "der", "die", "Deut\u00b7schen", "will", "ho\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es knistert aber nicht nur an dieser Stelle im Reiche;", "tokens": ["Es", "knis\u00b7tert", "a\u00b7ber", "nicht", "nur", "an", "die\u00b7ser", "Stel\u00b7le", "im", "Rei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "APPRART", "NE", "$."], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "im Rheinland beobachten wir ganz das gleiche:", "tokens": ["im", "Rhein\u00b7land", "be\u00b7ob\u00b7ach\u00b7ten", "wir", "ganz", "das", "glei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "auch hier m\u00f6chte man sich selbst\u00e4ndig machen, und nicht minder", "tokens": ["auch", "hier", "m\u00f6ch\u00b7te", "man", "sich", "selb\u00b7st\u00e4n\u00b7dig", "ma\u00b7chen", ",", "und", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PRF", "ADJD", "VVINF", "$,", "KON", "PTKNEG", "ADV"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "partikularistisch ergl\u00e4nzt der Vereinszylinder.", "tokens": ["par\u00b7ti\u00b7ku\u00b7la\u00b7ris\u00b7tisch", "er\u00b7gl\u00e4nzt", "der", "Ver\u00b7e\u00b7ins\u00b7zy\u00b7lin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "Und es ert\u00f6nt die alte deutsche Musike:", "tokens": ["Und", "es", "er\u00b7t\u00f6nt", "die", "al\u00b7te", "deut\u00b7sche", "Mu\u00b7si\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wollen unsere eigene kleine Republike!", "tokens": ["Wir", "wol\u00b7len", "un\u00b7se\u00b7re", "ei\u00b7ge\u00b7ne", "klei\u00b7ne", "Re\u00b7pub\u00b7li\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zweitausend Jahre alt ist diese Melodie \u2013", "tokens": ["Zweit\u00b7au\u00b7send", "Jah\u00b7re", "alt", "ist", "die\u00b7se", "Me\u00b7lo\u00b7die", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VAFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und es scheint fast so, als lernten die Deutschen es nie.", "tokens": ["und", "es", "scheint", "fast", "so", ",", "als", "lern\u00b7ten", "die", "Deut\u00b7schen", "es", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Haben sie denn nicht begriffen, was vor sich gegangen?", "tokens": ["Ha\u00b7ben", "sie", "denn", "nicht", "be\u00b7grif\u00b7fen", ",", "was", "vor", "sich", "ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,", "PRELS", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "F\u00fchlen sie nicht im Osten und Westen die klemmenden Zangen?", "tokens": ["F\u00fch\u00b7len", "sie", "nicht", "im", "Os\u00b7ten", "und", "Wes\u00b7ten", "die", "klem\u00b7men\u00b7den", "Zan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "M\u00fcssen sich denn die Deutschen immer untereinander zanken", "tokens": ["M\u00fcs\u00b7sen", "sich", "denn", "die", "Deut\u00b7schen", "im\u00b7mer", "un\u00b7ter\u00b7ein\u00b7an\u00b7der", "zan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PRF", "ADV", "ART", "NN", "ADV", "ADV", "VVINF"], "meter": "+--+-+-+-+--+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "und von K\u00fcrassierstiefel zum Schlafrock hin und wider wanken?", "tokens": ["und", "von", "K\u00fc\u00b7ras\u00b7siers\u00b7tie\u00b7fel", "zum", "Schla\u00b7frock", "hin", "und", "wi\u00b7der", "wan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPRART", "NN", "PTKVZ", "KON", "APPR", "VVINF", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ein Deutschland! Soll das niemals anders werden?", "tokens": ["Ein", "Deutschland", "!", "Soll", "das", "nie\u00b7mals", "an\u00b7ders", "wer\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VMFIN", "PDS", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Deutschland ohne diese l\u00e4cherlichen B\u00fcrgergeb\u00e4rden \u2013", "tokens": ["Ein", "Deutschland", "oh\u00b7ne", "die\u00b7se", "l\u00e4\u00b7cher\u00b7li\u00b7chen", "B\u00fcr\u00b7ger\u00b7ge\u00b7b\u00e4r\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ein Deutschland! Freunde, seid klug und gebt euch die Hand!", "tokens": ["Ein", "Deutschland", "!", "Freun\u00b7de", ",", "seid", "klug", "und", "gebt", "euch", "die", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir pfeifen auf schrilles Hurrageschrei. Wir brauchen", "tokens": ["Wir", "pfei\u00b7fen", "auf", "schril\u00b7les", "Hur\u00b7ra\u00b7ge\u00b7schrei", ".", "Wir", "brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}