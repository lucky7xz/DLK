{"textgrid.poem.52558": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00dfe Wehmuth, liebe, treue,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe Wehmuth, liebe, treue,", "tokens": ["S\u00fc\u00b7\u00dfe", "Weh\u00b7muth", ",", "lie\u00b7be", ",", "treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaumgekannte, langentbehrte,", "tokens": ["Kaum\u00b7ge\u00b7kann\u00b7te", ",", "lan\u00b7gent\u00b7behr\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich zu besingen scheue,", "tokens": ["Die", "ich", "zu", "be\u00b7sin\u00b7gen", "scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich wohl bel\u00e4chelt werde \u2013", "tokens": ["Weil", "ich", "wohl", "be\u00b7l\u00e4\u00b7chelt", "wer\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00dfe Wehmuth, hei\u00dfgen\u00e4hrte,", "tokens": ["S\u00fc\u00b7\u00dfe", "Weh\u00b7muth", ",", "hei\u00df\u00b7ge\u00b7n\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Str\u00f6me, str\u00f6m in Liedern aus!", "tokens": ["Str\u00f6\u00b7me", ",", "str\u00f6m", "in", "Lie\u00b7dern", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprecht ihr Weisen, sprecht ihr Thoren,", "tokens": ["Sprecht", "ihr", "Wei\u00b7sen", ",", "sprecht", "ihr", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach verrauschten sch\u00f6nen Tagen,", "tokens": ["Nach", "ver\u00b7rauschten", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Deren Wonne sich verloren", "tokens": ["De\u00b7ren", "Won\u00b7ne", "sich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In gewohnte Zeit der Plagen,", "tokens": ["In", "ge\u00b7wohn\u00b7te", "Zeit", "der", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, s\u00fc\u00dfe, sch\u00f6ne Klagen", "tokens": ["Lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe", ",", "sch\u00f6\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind nicht diese Trost allein?", "tokens": ["Sind", "nicht", "die\u00b7se", "Trost", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}