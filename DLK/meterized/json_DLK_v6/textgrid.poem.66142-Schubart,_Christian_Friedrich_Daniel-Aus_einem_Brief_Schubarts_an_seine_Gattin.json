{"textgrid.poem.66142": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Aus einem Brief Schubarts an seine Gattin", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich beh\u00fcte der Engel,", "tokens": ["Dich", "be\u00b7h\u00fc\u00b7te", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Den dir Gott zum F\u00fchrer gab,", "tokens": ["Den", "dir", "Gott", "zum", "F\u00fch\u00b7rer", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sp\u00e4t erst s\u00e4useln dich Winde des Himmels", "tokens": ["Und", "sp\u00e4t", "erst", "s\u00e4u\u00b7seln", "dich", "Win\u00b7de", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinunter ins Grab.", "tokens": ["Hin\u00b7un\u00b7ter", "ins", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "An Edens wolkenerbauter Pforte", "tokens": ["An", "E\u00b7dens", "wol\u00b7ke\u00b7ner\u00b7bau\u00b7ter", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erwart' ich dich!", "tokens": ["Er\u00b7wart'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und kommst du, dann ert\u00f6nen die Worte:", "tokens": ["Und", "kommst", "du", ",", "dann", "er\u00b7t\u00f6\u00b7nen", "die", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Umarme mich!", "tokens": ["Um\u00b7ar\u00b7me", "mich", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und wenn an deinem Hals ich hange,", "tokens": ["Und", "wenn", "an", "dei\u00b7nem", "Hals", "ich", "han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So l\u00e4cheln Engel uns zu,", "tokens": ["So", "l\u00e4\u00b7cheln", "En\u00b7gel", "uns", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und f\u00fchren mit wonnestrahlender Wange", "tokens": ["Und", "f\u00fch\u00b7ren", "mit", "won\u00b7nes\u00b7trah\u00b7len\u00b7der", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns ein in die ewige Ruh'.", "tokens": ["Uns", "ein", "in", "die", "e\u00b7wi\u00b7ge", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dir singen die V\u00f6gel in Eden,", "tokens": ["Dir", "sin\u00b7gen", "die", "V\u00f6\u00b7gel", "in", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wenn du der Wolke des Todes entsteigst,", "tokens": ["Wenn", "du", "der", "Wol\u00b7ke", "des", "To\u00b7des", "ent\u00b7steigst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vor Wonne k\u00f6nnen wir beide nicht reden;", "tokens": ["Vor", "Won\u00b7ne", "k\u00f6n\u00b7nen", "wir", "bei\u00b7de", "nicht", "re\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Du faltest die H\u00e4nde und schweigst.", "tokens": ["Du", "fal\u00b7test", "die", "H\u00e4n\u00b7de", "und", "schweigst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}