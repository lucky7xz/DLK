{"textgrid.poem.41259": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Arcadier", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gr\u00fcbelst Tag und Nacht, umringt vom Dichterchor,", "tokens": ["Du", "gr\u00fc\u00b7belst", "Tag", "und", "Nacht", ",", "um\u00b7ringt", "vom", "Dich\u00b7ter\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in Athen und Rom der Kenner Lust gewesen.", "tokens": ["Der", "in", "A\u00b7then", "und", "Rom", "der", "Ken\u00b7ner", "Lust", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NE", "ART", "NN", "NN", "VAPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Was nutzt dein stummer Flei\u00df? Was hilft dein blindes Lesen?", "tokens": ["Was", "nutzt", "dein", "stum\u00b7mer", "Flei\u00df", "?", "Was", "hilft", "dein", "blin\u00b7des", "Le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein bleierner Verstand steigt nicht, durch sie, empor.", "tokens": ["Dein", "blei\u00b7er\u00b7ner", "Ver\u00b7stand", "steigt", "nicht", ",", "durch", "sie", ",", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "$,", "APPR", "PPER", "$,", "PTKVZ", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Es scheint fast jede M\u00fch' vom Ziel dich zu entfernen.", "tokens": ["Es", "scheint", "fast", "je\u00b7de", "M\u00fch'", "vom", "Ziel", "dich", "zu", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An Witze bist du arm, doch an Poeten reich,", "tokens": ["An", "Wit\u00b7ze", "bist", "du", "arm", ",", "doch", "an", "Po\u00b7et\u00b7en", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und nur den schweren Ankern gleich,", "tokens": ["Und", "nur", "den", "schwe\u00b7ren", "An\u00b7kern", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die stets im Wasser sind, und nimmer schwimmen lernen.", "tokens": ["Die", "stets", "im", "Was\u00b7ser", "sind", ",", "und", "nim\u00b7mer", "schwim\u00b7men", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VAFIN", "$,", "KON", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}