{"textgrid.poem.25499": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bin ich n\u00fcchtern, bin ich trunken?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich n\u00fcchtern, bin ich trunken?", "tokens": ["Bin", "ich", "n\u00fcch\u00b7tern", ",", "bin", "ich", "trun\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wach' ich, oder tr\u00e4um' ich nur?", "tokens": ["Wach'", "ich", ",", "o\u00b7der", "tr\u00e4um'", "ich", "nur", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich aus der Welt gesunken?", "tokens": ["Bin", "ich", "aus", "der", "Welt", "ge\u00b7sun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich anderer Natur?", "tokens": ["Bin", "ich", "an\u00b7de\u00b7rer", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlt' ein M\u00e4dchen schon so was?", "tokens": ["F\u00fchlt'", "ein", "M\u00e4d\u00b7chen", "schon", "so", "was", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie begreif' ich alles das?", "tokens": ["Wie", "be\u00b7greif'", "ich", "al\u00b7les", "das", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00df ich, da\u00df die Rosen bl\u00fchen?", "tokens": ["Wei\u00df", "ich", ",", "da\u00df", "die", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6r' ich jene Raben schrein?", "tokens": ["H\u00f6r'", "ich", "je\u00b7ne", "Ra\u00b7ben", "schrein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "F\u00fchl' ich, wie die Wangen gl\u00fchen?", "tokens": ["F\u00fchl'", "ich", ",", "wie", "die", "Wan\u00b7gen", "gl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schmeck' ich einen Tropfen Wein?", "tokens": ["Schmeck'", "ich", "ei\u00b7nen", "Trop\u00b7fen", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Seh' ich dieses Morgenroth? \u2013", "tokens": ["Seh'", "ich", "die\u00b7ses", "Mor\u00b7gen\u00b7roth", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Todt sind alle Sinnen, todt!", "tokens": ["Todt", "sind", "al\u00b7le", "Sin\u00b7nen", ",", "todt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Alle seyd ihr denn gestillet?", "tokens": ["Al\u00b7le", "seyd", "ihr", "denn", "ge\u00b7stil\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Alle? Habet alle Dank!", "tokens": ["Al\u00b7le", "?", "Ha\u00b7bet", "al\u00b7le", "Dank", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "$.", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "K\u00f6nnt' ich so in mich geh\u00fcllet,", "tokens": ["K\u00f6nnt'", "ich", "so", "in", "mich", "ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ohne Speis' und ohne Trank,", "tokens": ["Oh\u00b7ne", "Speis'", "und", "oh\u00b7ne", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Nur so sitzen Tag f\u00fcr Tag", "tokens": ["Nur", "so", "sit\u00b7zen", "Tag", "f\u00fcr", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Bis zum letzten Herzensschlag'.", "tokens": ["Bis", "zum", "letz\u00b7ten", "Her\u00b7zens\u00b7schlag'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "In die Nacht der Freude fliehet", "tokens": ["In", "die", "Nacht", "der", "Freu\u00b7de", "flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Meine Seele wieder hin!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "wie\u00b7der", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "H\u00f6rt und schmeckt, und f\u00fchlt und siehet", "tokens": ["H\u00f6rt", "und", "schmeckt", ",", "und", "f\u00fchlt", "und", "sie\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Mit dem feinen innren Sinn'!", "tokens": ["Mit", "dem", "fei\u00b7nen", "inn\u00b7ren", "Sinn'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "O Ged\u00e4chtni\u00df! schon in dir", "tokens": ["O", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "!", "schon", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Liegt ein ganzer Himmel mir!", "tokens": ["Liegt", "ein", "gan\u00b7zer", "Him\u00b7mel", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Worte, wie sie abgerissen", "tokens": ["Wor\u00b7te", ",", "wie", "sie", "ab\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Kaum ein Seufzer von ihm stie\u00df,", "tokens": ["Kaum", "ein", "Seuf\u00b7zer", "von", "ihm", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "H\u00f6r' ich wieder, f\u00fchl' ihn k\u00fcssen:", "tokens": ["H\u00f6r'", "ich", "wie\u00b7der", ",", "f\u00fchl'", "ihn", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Welche Sprache sagt, wie s\u00fc\u00df?", "tokens": ["Wel\u00b7che", "Spra\u00b7che", "sagt", ",", "wie", "s\u00fc\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Seh' ein Thr\u00e4nchen \u2013 Komm herab!", "tokens": ["Seh'", "ein", "Thr\u00e4n\u00b7chen", "\u2013", "Komm", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Meine Lippe k\u00fc\u00dft dich ab!", "tokens": ["Mei\u00b7ne", "Lip\u00b7pe", "k\u00fc\u00dft", "dich", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wie ich noch so vor ihm stehe,", "tokens": ["Wie", "ich", "noch", "so", "vor", "ihm", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Immer spreche: Gute Nacht!", "tokens": ["Im\u00b7mer", "spre\u00b7che", ":", "Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Bald ihn stockend wieder flehe:", "tokens": ["Bald", "ihn", "sto\u00b7ckend", "wie\u00b7der", "fle\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Bleibe, bis der Hahn erwacht!", "tokens": ["Blei\u00b7be", ",", "bis", "der", "Hahn", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wie mein Fu\u00df bei jedem Schritt'", "tokens": ["Wie", "mein", "Fu\u00df", "bei", "je\u00b7dem", "Schritt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wanket, und mein Liebster mit!", "tokens": ["Wan\u00b7ket", ",", "und", "mein", "Liebs\u00b7ter", "mit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Wie ich nun, an seine Seite", "tokens": ["Wie", "ich", "nun", ",", "an", "sei\u00b7ne", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.38": {"text": "Festgeklammert, k\u00fcssend ihn", "tokens": ["Fest\u00b7ge\u00b7klam\u00b7mert", ",", "k\u00fcs\u00b7send", "ihn"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Durch den Garten hin begleite!", "tokens": ["Durch", "den", "Gar\u00b7ten", "hin", "be\u00b7glei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Bald uns halten, bald uns ziehn!", "tokens": ["Bald", "uns", "hal\u00b7ten", ",", "bald", "uns", "ziehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$,", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Wie da Mond und Sterne stehn,", "tokens": ["Wie", "da", "Mond", "und", "Ster\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Unserm Abschied' zuzusehn.", "tokens": ["Un\u00b7serm", "Ab\u00b7schied'", "zu\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Ach da sind wir an der Th\u00fcre!", "tokens": ["Ach", "da", "sind", "wir", "an", "der", "Th\u00fc\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Bebend h\u00e4lt er in der Hand", "tokens": ["Be\u00b7bend", "h\u00e4lt", "er", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Schon den Schl\u00fcssel. \u2013 Wart', ich sp\u00fcre", "tokens": ["Schon", "den", "Schl\u00fcs\u00b7sel", ".", "\u2013", "Wart'", ",", "ich", "sp\u00fc\u00b7re"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "$(", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Jemand gehen, Amarant!", "tokens": ["Je\u00b7mand", "ge\u00b7hen", ",", "A\u00b7ma\u00b7rant", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Warte nur das Bischen doch!", "tokens": ["War\u00b7te", "nur", "das", "Bi\u00b7schen", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Einen Ku\u00df zum Abschied' noch!", "tokens": ["Ei\u00b7nen", "Ku\u00df", "zum", "Ab\u00b7schied'", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Ich verliere, ich verliere", "tokens": ["Ich", "ver\u00b7lie\u00b7re", ",", "ich", "ver\u00b7lie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Mich in diesem Labyrinth'!", "tokens": ["Mich", "in", "die\u00b7sem", "La\u00b7by\u00b7rinth'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Traumt' ich je, da\u00df ich erf\u00fchre,", "tokens": ["Traumt'", "ich", "je", ",", "da\u00df", "ich", "er\u00b7f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Was f\u00fcr Freuden, Freuden sind?", "tokens": ["Was", "f\u00fcr", "Freu\u00b7den", ",", "Freu\u00b7den", "sind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Wenn die Freude t\u00f6dten kann,", "tokens": ["Wenn", "die", "Freu\u00b7de", "t\u00f6d\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Triffst du nie mich wieder an.", "tokens": ["Triffst", "du", "nie", "mich", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}