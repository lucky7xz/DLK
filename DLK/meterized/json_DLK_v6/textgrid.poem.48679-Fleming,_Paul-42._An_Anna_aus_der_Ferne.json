{"textgrid.poem.48679": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "42. An Anna aus der Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aurora, schlummre noch an deines Liebsten Brust,", "tokens": ["Au\u00b7ro\u00b7ra", ",", "schlumm\u00b7re", "noch", "an", "dei\u00b7nes", "Liebs\u00b7ten", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "es ist der tiefen Nacht kein Morgen noch bewu\u00dft.", "tokens": ["es", "ist", "der", "tie\u00b7fen", "Nacht", "kein", "Mor\u00b7gen", "noch", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Diana f\u00fchrt die Sternen", "tokens": ["Di\u00b7a\u00b7na", "f\u00fchrt", "die", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "noch h\u00f6her in die Luft,", "tokens": ["noch", "h\u00f6\u00b7her", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "will weiter von mir lernen,", "tokens": ["will", "wei\u00b7ter", "von", "mir", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "was ich ihr vorgeruft.", "tokens": ["was", "ich", "ihr", "vor\u00b7ge\u00b7ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Neun Stunden sind nun gleich von N\u00e4chten durchgebracht,", "tokens": ["Neun", "Stun\u00b7den", "sind", "nun", "gleich", "von", "N\u00e4ch\u00b7ten", "durch\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Neun Stunden hab' ich nun an Korilen gedacht,", "tokens": ["Neun", "Stun\u00b7den", "hab'", "ich", "nun", "an", "Ko\u00b7ri\u00b7len", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "an Korilen, die sch\u00f6ne,", "tokens": ["an", "Ko\u00b7ri\u00b7len", ",", "die", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "von der ich bin so weit,", "tokens": ["von", "der", "ich", "bin", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "drum klinget mein Get\u00f6ne", "tokens": ["drum", "klin\u00b7get", "mein", "Ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "nach Nichts denn Traurigkeit.", "tokens": ["nach", "Nichts", "denn", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nehmt Korilen in Acht, ihr W\u00e4chter aller Welt,", "tokens": ["Nehmt", "Ko\u00b7ri\u00b7len", "in", "Acht", ",", "ihr", "W\u00e4ch\u00b7ter", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "CARD", "$,", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "f\u00fcr ihren treuen Sinn, den sie mir vorbeh\u00e4lt.", "tokens": ["f\u00fcr", "ih\u00b7ren", "treu\u00b7en", "Sinn", ",", "den", "sie", "mir", "vor\u00b7be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will nicht m\u00fcde werden", "tokens": ["Ich", "will", "nicht", "m\u00fc\u00b7de", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "in ihrer festen Pflicht,", "tokens": ["in", "ih\u00b7rer", "fes\u00b7ten", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "bis da\u00df der Feind der Erden", "tokens": ["bis", "da\u00df", "der", "Feind", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "auch mir mein Urtheil spricht.", "tokens": ["auch", "mir", "mein", "Ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aurora, lege nun um dich den Purpurflor.", "tokens": ["Au\u00b7ro\u00b7ra", ",", "le\u00b7ge", "nun", "um", "dich", "den", "Pur\u00b7pur\u00b7flor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "PRF", "ART", "NN", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der junge Tag tut auf der Eos g\u00fcldnes Thor.", "tokens": ["Der", "jun\u00b7ge", "Tag", "tut", "auf", "der", "E\u00b7os", "g\u00fcld\u00b7nes", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wirst du mein Lieb ersehen,", "tokens": ["Wirst", "du", "mein", "Lieb", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "so gieb ihr einen Wink,", "tokens": ["so", "gieb", "ihr", "ei\u00b7nen", "Wink", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "als mir von ihr geschehen,", "tokens": ["als", "mir", "von", "ihr", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "indem ich von ihr ging.", "tokens": ["in\u00b7dem", "ich", "von", "ihr", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}