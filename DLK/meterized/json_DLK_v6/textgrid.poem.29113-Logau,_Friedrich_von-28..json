{"textgrid.poem.29113": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch hat zuvor au\u00df f\u00fcr andren Thieren allen,", "tokens": ["Der", "Mensch", "hat", "zu\u00b7vor", "au\u00df", "f\u00fcr", "an\u00b7dren", "Thie\u00b7ren", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er kan sagen her das, was ihm eingefallen.", "tokens": ["Da\u00df", "er", "kan", "sa\u00b7gen", "her", "das", ",", "was", "ihm", "ein\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "APZR", "PDS", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "F\u00fcrwahr wir brauchen ietzt rechtschaffen diese Gabe,", "tokens": ["F\u00fcr\u00b7wahr", "wir", "brau\u00b7chen", "ietzt", "recht\u00b7schaf\u00b7fen", "die\u00b7se", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df unser gantzes Thun als Worte nichts nicht habe.", "tokens": ["Da\u00df", "un\u00b7ser", "gant\u00b7zes", "Thun", "als", "Wor\u00b7te", "nichts", "nicht", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "KOUS", "NN", "PIS", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}