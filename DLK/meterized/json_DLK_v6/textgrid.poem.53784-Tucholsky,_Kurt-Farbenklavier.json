{"textgrid.poem.53784": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Farbenklavier", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rot ist die Leidenschaft,", "tokens": ["Rot", "ist", "die", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "blau ist das Meer,", "tokens": ["blau", "ist", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "gr\u00fcn der Chef vom Hakenkreuz,", "tokens": ["gr\u00fcn", "der", "Chef", "vom", "Ha\u00b7ken\u00b7kreuz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schwarz-wei\u00df-rot das Heer.", "tokens": ["schwa\u00b7rz\u00b7wei\u00df\u00b7rot", "das", "Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Rosa ist die Heckenros,", "tokens": ["Ro\u00b7sa", "ist", "die", "He\u00b7cken\u00b7ros", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "blausa mancher Kreis;", "tokens": ["blau\u00b7sa", "man\u00b7cher", "Kreis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Oberst Nicolai seine", "tokens": ["O\u00b7berst", "Ni\u00b7co\u00b7lai", "sei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NE", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weste \u2013 ist sie wei\u00df?", "tokens": ["Wes\u00b7te", "\u2013", "ist", "sie", "wei\u00df", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gr\u00fcnblau ist der Arrestant", "tokens": ["Gr\u00fcn\u00b7blau", "ist", "der", "Ar\u00b7res\u00b7tant"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "bei der Polizei,", "tokens": ["bei", "der", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "reisgelb Fritz von Unruhs", "tokens": ["reis\u00b7gelb", "Fritz", "von", "Un\u00b7ruhs"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NE", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "B\u00fccherschreiberei.", "tokens": ["B\u00fc\u00b7cher\u00b7schrei\u00b7be\u00b7rei", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Blauwei\u00df ist bayerisch,", "tokens": ["Blau\u00b7wei\u00df", "ist", "bay\u00b7e\u00b7risch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "gr\u00fcn macht die Gans,", "tokens": ["gr\u00fcn", "macht", "die", "Gans", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "gelb f\u00e4rbt der Wasserstoff . . .", "tokens": ["gelb", "f\u00e4rbt", "der", "Was\u00b7ser\u00b7stoff", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Wenn Sie meinen, da\u00df das stundenlang so weitergeht . . .", "tokens": ["Wenn", "Sie", "mei\u00b7nen", ",", "da\u00df", "das", "stun\u00b7den\u00b7lang", "so", "wei\u00b7ter\u00b7geht", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PDS", "VVFIN", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}