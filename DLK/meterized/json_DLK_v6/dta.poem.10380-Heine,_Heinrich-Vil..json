{"dta.poem.10380": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will meine Seele tauchen", "tokens": ["Ich", "will", "mei\u00b7ne", "See\u00b7le", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In den Kelch der Lilie hinein;", "tokens": ["In", "den", "Kelch", "der", "Li\u00b7lie", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Lilie soll klingend hauchen", "tokens": ["Die", "Li\u00b7lie", "soll", "klin\u00b7gend", "hau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVFIN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ein Lied von der Liebsten mein.", "tokens": ["Ein", "Lied", "von", "der", "Liebs\u00b7ten", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Lied soll schauern und beben,", "tokens": ["Das", "Lied", "soll", "schau\u00b7ern", "und", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie der Ku\u00df von ihrem Mund',", "tokens": ["Wie", "der", "Ku\u00df", "von", "ih\u00b7rem", "Mund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den sie mir einst gegeben", "tokens": ["Den", "sie", "mir", "einst", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In wunderbar s\u00fc\u00dfer Stund'.", "tokens": ["In", "wun\u00b7der\u00b7bar", "s\u00fc\u00b7\u00dfer", "Stund'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}