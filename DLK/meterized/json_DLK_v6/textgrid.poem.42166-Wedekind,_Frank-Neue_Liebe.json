{"textgrid.poem.42166": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Neue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du M\u00e4dchen in des Lebens vollster Pracht", "tokens": ["Du", "M\u00e4d\u00b7chen", "in", "des", "Le\u00b7bens", "volls\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hast mich zu lichtem Flammenmeer entfacht;", "tokens": ["Hast", "mich", "zu", "lich\u00b7tem", "Flam\u00b7men\u00b7meer", "ent\u00b7facht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das z\u00fcngelt blutig bis ans Sternenzelt,", "tokens": ["Das", "z\u00fcn\u00b7gelt", "blu\u00b7tig", "bis", "ans", "Ster\u00b7nen\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von keinem Blick beh\u00fctet und bewacht.", "tokens": ["Von", "kei\u00b7nem", "Blick", "be\u00b7h\u00fc\u00b7tet", "und", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und fa\u00dft die Flamme nicht die ganze Welt,", "tokens": ["Und", "fa\u00dft", "die", "Flam\u00b7me", "nicht", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie dich und den, der dich umfangen h\u00e4lt?", "tokens": ["Wie", "dich", "und", "den", ",", "der", "dich", "um\u00b7fan\u00b7gen", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "ART", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein einz'ger Zwieklang durch den weiten Raum,", "tokens": ["Ein", "einz'\u00b7ger", "Zwie\u00b7klang", "durch", "den", "wei\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Jubel der vereinten Sch\u00f6pfung gellt.", "tokens": ["Der", "Ju\u00b7bel", "der", "ver\u00b7ein\u00b7ten", "Sch\u00f6p\u00b7fung", "gellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vergangenheit wird uns ein d\u00fcstrer Traum,", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "wird", "uns", "ein", "d\u00fcst\u00b7rer", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Horizont ein schwarzer Wolkensaum.", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "ein", "schwar\u00b7zer", "Wol\u00b7ken\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch auch das Gl\u00fcck, daraus mein Lied erschallt,", "tokens": ["Doch", "auch", "das", "Gl\u00fcck", ",", "da\u00b7raus", "mein", "Lied", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In seiner G\u00f6ttlichkeit noch fa\u00df ich's kaum.", "tokens": ["In", "sei\u00b7ner", "G\u00f6tt\u00b7lich\u00b7keit", "noch", "fa\u00df", "ich's", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bis da\u00df mich deine irdische Gestalt,", "tokens": ["Bis", "da\u00df", "mich", "dei\u00b7ne", "ir\u00b7di\u00b7sche", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis da\u00df mich deiner Sinne Glutgewalt", "tokens": ["Bis", "da\u00df", "mich", "dei\u00b7ner", "Sin\u00b7ne", "Glut\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von jedem dumpfen Traumgewirr befreit", "tokens": ["Von", "je\u00b7dem", "dum\u00b7pfen", "Traum\u00b7ge\u00b7wirr", "be\u00b7freit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch nie getr\u00e4umter Freuden Wirklichkeit.", "tokens": ["Durch", "nie", "ge\u00b7tr\u00e4um\u00b7ter", "Freu\u00b7den", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}