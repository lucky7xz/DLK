{"textgrid.poem.65263": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Irrlicht", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die tiefsten Felsengr\u00fcnde", "tokens": ["In", "die", "tiefs\u00b7ten", "Fel\u00b7sen\u00b7gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockte mich ein Irrlicht hin:", "tokens": ["Lock\u00b7te", "mich", "ein", "Irr\u00b7licht", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich einen Ausgang finde,", "tokens": ["Wie", "ich", "ei\u00b7nen", "Aus\u00b7gang", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt nicht schwer mir in dem Sinn.", "tokens": ["Liegt", "nicht", "schwer", "mir", "in", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bin gewohnt das irre Gehen,", "tokens": ["Bin", "ge\u00b7wohnt", "das", "ir\u00b7re", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "'s f\u00fchrt ja jeder Weg zum Ziel:", "tokens": ["'s", "f\u00fchrt", "ja", "je\u00b7der", "Weg", "zum", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unsre Freuden, unsre Wehen,", "tokens": ["Uns\u00b7re", "Freu\u00b7den", ",", "uns\u00b7re", "We\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles eines Irrlichts Spiel!", "tokens": ["Al\u00b7les", "ei\u00b7nes", "Irr\u00b7lichts", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch des Bergstroms trockne Rinnen", "tokens": ["Durch", "des", "Berg\u00b7stroms", "trock\u00b7ne", "Rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wind' ich ruhig mich hinab \u2013", "tokens": ["Wind'", "ich", "ru\u00b7hig", "mich", "hin\u00b7ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Strom wird 's Meer gewinnen,", "tokens": ["Je\u00b7der", "Strom", "wird", "'s", "Meer", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Jedes Leiden auch ein Grab.", "tokens": ["Je\u00b7des", "Lei\u00b7den", "auch", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}