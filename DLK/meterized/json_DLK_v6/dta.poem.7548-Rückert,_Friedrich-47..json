{"dta.poem.7548": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rose taucht den Fu\u00df in Wasser doch und Roth;", "tokens": ["Die", "Ro\u00b7se", "taucht", "den", "Fu\u00df", "in", "Was\u00b7ser", "doch", "und", "Roth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was w\u00fcrzt ihr denn den Mund und macht die Wang' ihr roth?", "tokens": ["Was", "w\u00fcrzt", "ihr", "denn", "den", "Mund", "und", "macht", "die", "Wang'", "ihr", "roth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An ihrem Fu\u00dfe steht ein erdgeborner Schwamm,", "tokens": ["An", "ih\u00b7rem", "Fu\u00b7\u00dfe", "steht", "ein", "erd\u00b7ge\u00b7bor\u00b7ner", "Schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den giftig hat geschwellt der gern gesogne Schlamm.", "tokens": ["Den", "gif\u00b7tig", "hat", "ge\u00b7schwellt", "der", "gern", "ge\u00b7sog\u00b7ne", "Schlamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVPP", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er trank den Schaum ihr weg, der Geist ist ihr geblieben,", "tokens": ["Er", "trank", "den", "Schaum", "ihr", "weg", ",", "der", "Geist", "ist", "ihr", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sie zur Bl\u00fct' erschlo\u00df, von ihrer Art getrieben.", "tokens": ["Den", "sie", "zur", "Bl\u00fct'", "er\u00b7schlo\u00df", ",", "von", "ih\u00b7rer", "Art", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sei du die Himmelsros' und nicht der Erdenpilz;", "tokens": ["Sei", "du", "die", "Him\u00b7mels\u00b7ros'", "und", "nicht", "der", "Er\u00b7den\u00b7pilz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Saug Aether in dein Herz, nicht Gift in deine Milz.", "tokens": ["Saug", "A\u00b7e\u00b7ther", "in", "dein", "Herz", ",", "nicht", "Gift", "in", "dei\u00b7ne", "Milz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}