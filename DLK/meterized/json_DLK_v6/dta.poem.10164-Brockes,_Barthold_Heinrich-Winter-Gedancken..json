{"dta.poem.10164": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als unl\u00e4ngst eine Winter Nacht,", "tokens": ["Als", "un\u00b7l\u00e4ngst", "ei\u00b7ne", "Win\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In welcher es, mit einer bittern Macht,", "tokens": ["In", "wel\u00b7cher", "es", ",", "mit", "ei\u00b7ner", "bit\u00b7tern", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hefftig druckender Gewalt,", "tokens": ["Und", "heff\u00b7tig", "dru\u00b7cken\u00b7der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Erden, in der Fluth, und in der Luft gefroren,", "tokens": ["Auf", "Er\u00b7den", ",", "in", "der", "Fluth", ",", "und", "in", "der", "Luft", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit ihrem Schatten sich verlohren;", "tokens": ["Mit", "ih\u00b7rem", "Schat\u00b7ten", "sich", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erstaunt\u2019 ich, als ich die recht schreckende Gestalt", "tokens": ["Er\u00b7staunt'", "ich", ",", "als", "ich", "die", "recht", "schre\u00b7cken\u00b7de", "Ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Der starren Welt, indem es starck gereifft,", "tokens": ["Der", "star\u00b7ren", "Welt", ",", "in\u00b7dem", "es", "starck", "ge\u00b7reifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und der gefrorne Dufft sich \u00fcberall geh\u00e4ufft,", "tokens": ["Und", "der", "ge\u00b7fror\u00b7ne", "Dufft", "sich", "\u00fc\u00b7be\u00b7rall", "ge\u00b7h\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In einem wilden Glantz\u2019 erblickte.", "tokens": ["In", "ei\u00b7nem", "wil\u00b7den", "Glant\u00b7z'", "er\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein blendend Wei\u00df bedeckt\u2019 der Berg\u2019 und H\u00e4user", "tokens": ["Ein", "blen\u00b7dend", "Wei\u00df", "be\u00b7deckt'", "der", "Ber\u00b7g'", "und", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein rauher Schimmer f\u00fcllt\u2019, \u00fcmgabe, dr\u00fcckt\u2019 und schm\u00fcckte", "tokens": ["Ein", "rau\u00b7her", "Schim\u00b7mer", "f\u00fcllt'", ",", "\u00fcm\u00b7ga\u00b7be", ",", "dr\u00fcckt'", "und", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der starren B\u00e4ume Wipfel.", "tokens": ["Der", "star\u00b7ren", "B\u00e4u\u00b7me", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sah, nicht sonder Lust, doch auch nicht sonder Schrecken,", "tokens": ["Man", "sah", ",", "nicht", "son\u00b7der", "Lust", ",", "doch", "auch", "nicht", "son\u00b7der", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PTKNEG", "KON", "NN", "$,", "ADV", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Derselben St\u00e4mm\u2019 und Zweig\u2019 in weissen Rinden stecken,", "tokens": ["Der\u00b7sel\u00b7ben", "St\u00e4mm'", "und", "Zweig'", "in", "weis\u00b7sen", "Rin\u00b7den", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und diese eingeh\u00fcllt durch ein so rauhes Moo\u00df,", "tokens": ["Und", "die\u00b7se", "ein\u00b7ge\u00b7h\u00fcllt", "durch", "ein", "so", "rau\u00b7hes", "Moo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df ieder Wipfel, nicht wie sonsten, Bl\u00e4tter-los,", "tokens": ["Da\u00df", "ie\u00b7der", "Wip\u00b7fel", ",", "nicht", "wie", "sons\u00b7ten", ",", "Bl\u00e4t\u00b7ter\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PTKNEG", "KOKOM", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihres Schmucks entbl\u00f6sst schien, und beraubet;", "tokens": ["Und", "ih\u00b7res", "Schmucks", "ent\u00b7bl\u00f6sst", "schien", ",", "und", "be\u00b7rau\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie liessen dick, und recht als w\u00e4ren sie belaubet.", "tokens": ["Sie", "lies\u00b7sen", "dick", ",", "und", "recht", "als", "w\u00e4\u00b7ren", "sie", "be\u00b7lau\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "ADJD", "KOKOM", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kaum sah man hie und dort, durch sie, die d\u00fcstre Lufft,", "tokens": ["Kaum", "sah", "man", "hie", "und", "dort", ",", "durch", "sie", ",", "die", "d\u00fcst\u00b7re", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "KON", "ADV", "$,", "APPR", "PPER", "$,", "PRELS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die, schwartz durch den verdickten Dufft,", "tokens": ["Die", ",", "schwartz", "durch", "den", "ver\u00b7dick\u00b7ten", "Dufft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den Horizont fast gantz bedeckte,", "tokens": ["Den", "Ho\u00b7ri\u00b7zont", "fast", "gantz", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und worin eigentlich der Grimm des Frostes steckte.", "tokens": ["Und", "wo\u00b7rin", "ei\u00b7gent\u00b7lich", "der", "Grimm", "des", "Fros\u00b7tes", "steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Indem ich dieses wei\u00df mit schwartz vermischte Grau,", "tokens": ["In\u00b7dem", "ich", "die\u00b7ses", "wei\u00df", "mit", "schwartz", "ver\u00b7mischte", "Grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und in demselbigen fast sichtbarlich die Wuth", "tokens": ["Und", "in", "dem\u00b7sel\u00b7bi\u00b7gen", "fast", "sicht\u00b7bar\u00b7lich", "die", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der allversteinernden ergrimmten K\u00e4lte schau,", "tokens": ["Der", "all\u00b7ver\u00b7stei\u00b7nern\u00b7den", "er\u00b7grimm\u00b7ten", "K\u00e4l\u00b7te", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wilde Rauhigkeit der Landschafft \u00fcberlege,", "tokens": ["Die", "wil\u00b7de", "Rau\u00b7hig\u00b7keit", "der", "Land\u00b7schafft", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die fremde D\u00e4mmerung von Wei\u00df und Schwartz erwege;", "tokens": ["Die", "frem\u00b7de", "D\u00e4m\u00b7me\u00b7rung", "von", "Wei\u00df", "und", "Schwartz", "er\u00b7we\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Brach durch dieselbige, voll dunckel-rother Glut,", "tokens": ["Brach", "durch", "die\u00b7sel\u00b7bi\u00b7ge", ",", "voll", "dun\u00b7ckel\u00b7ro\u00b7ther", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Morgen-Sonnen Licht.", "tokens": ["Der", "Mor\u00b7gen\u00b7Son\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Welch blitzend Feuer-Meer ergo\u00df sich dazumahl,", "tokens": ["Welch", "blit\u00b7zend", "Feu\u00b7er\u00b7Meer", "er\u00b7go\u00df", "sich", "da\u00b7zu\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beflo\u00df den luckern Reiff, fiel auf die glatten Spitzen!", "tokens": ["Be\u00b7flo\u00df", "den", "lu\u00b7ckern", "Reiff", ",", "fiel", "auf", "die", "glat\u00b7ten", "Spit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was f\u00fcr ein reiner Glantz, welch angenehmes blitzen", "tokens": ["Was", "f\u00fcr", "ein", "rei\u00b7ner", "Glantz", ",", "welch", "an\u00b7ge\u00b7neh\u00b7mes", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$,", "PWAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drang durch das Aug\u2019 ins Hertz! das allerreinste Wei\u00df,", "tokens": ["Drang", "durch", "das", "Aug'", "ins", "Hertz", "!", "das", "al\u00b7ler\u00b7reins\u00b7te", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das allerreinste Roth schien auf den starren B\u00fcschen,", "tokens": ["Das", "al\u00b7ler\u00b7reins\u00b7te", "Roth", "schien", "auf", "den", "star\u00b7ren", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In einem s\u00fcssen Glantz, der Leib-Farb, sich zu mischen.", "tokens": ["In", "ei\u00b7nem", "s\u00fcs\u00b7sen", "Glantz", ",", "der", "Leib\u00b7Fa\u00b7rb", ",", "sich", "zu", "mi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich sah\u2019, als kleinen Staub, von reinen Berg-Cry-", "tokens": ["Ich", "sah'", ",", "als", "klei\u00b7nen", "Staub", ",", "von", "rei\u00b7nen", "Ber\u00b7gCry"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den hart- und klaren Reiff, bald hie, bald da,", "tokens": ["Den", "har\u00b7t", "und", "kla\u00b7ren", "Reiff", ",", "bald", "hie", ",", "bald", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jm rothen Sonnen-Strahl gemach herunter fallen,", "tokens": ["Jm", "ro\u00b7then", "Son\u00b7nen\u00b7Strahl", "ge\u00b7mach", "her\u00b7un\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wunderw\u00fcrdig lieblich spielen.", "tokens": ["Und", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "lieb\u00b7lich", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich fand, wie ich die Theilchen recht besah,", "tokens": ["Ich", "fand", ",", "wie", "ich", "die", "Theil\u00b7chen", "recht", "be\u00b7sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df wircklich kleine Sternchen fielen;", "tokens": ["Da\u00df", "wir\u00b7ck\u00b7lich", "klei\u00b7ne", "Stern\u00b7chen", "fie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Indem recht ordentlich in sechs-geeckten Spitzen", "tokens": ["In\u00b7dem", "recht", "or\u00b7dent\u00b7lich", "in", "sechs\u00b7ge\u00b7eck\u00b7ten", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Die allermeisten schimmernd blitzen.", "tokens": ["Die", "al\u00b7ler\u00b7meis\u00b7ten", "schim\u00b7mernd", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es fiel, bald an, bald durch des Reiffen zartes Eis", "tokens": ["Es", "fiel", ",", "bald", "an", ",", "bald", "durch", "des", "Reif\u00b7fen", "zar\u00b7tes", "Eis"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der himmlische Rubin. Wer diese Sch\u00f6nheit nicht", "tokens": ["Der", "himm\u00b7li\u00b7sche", "Ru\u00b7bin", ".", "Wer", "die\u00b7se", "Sch\u00f6n\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$.", "PWS", "PDAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Anmuth sieht, und Den nicht preiset,", "tokens": ["Mit", "An\u00b7muth", "sieht", ",", "und", "Den", "nicht", "prei\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "NE", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der, auch zur Winters-Zeit, uns so viel Sch\u00f6nheit wei-", "tokens": ["Der", ",", "auch", "zur", "Win\u00b7ter\u00b7sZeit", ",", "uns", "so", "viel", "Sch\u00f6n\u00b7heit", "wei"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "$,", "PPER", "ADV", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der handelt wider seine Pflicht.", "tokens": ["Der", "han\u00b7delt", "wi\u00b7der", "sei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich f\u00fchlte, vor so hellem Glantz und Licht,", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "vor", "so", "hel\u00b7lem", "Glantz", "und", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", 