{"dta.poem.3332": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum eil ich fliegend aus den Thoren;", "tokens": ["Kaum", "eil", "ich", "flie\u00b7gend", "aus", "den", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So kann ich mich im Gr\u00fcnen sehn;", "tokens": ["So", "kann", "ich", "mich", "im", "Gr\u00fc\u00b7nen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fchl ich freyer L\u00fcfte Wehn:", "tokens": ["So", "f\u00fchl", "ich", "frey\u00b7er", "L\u00fcf\u00b7te", "Wehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lerche singt; ich sehe Floren", "tokens": ["Die", "Ler\u00b7che", "singt", ";", "ich", "se\u00b7he", "Flo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch hundert G\u00e4rten landhaft gehn.", "tokens": ["Durch", "hun\u00b7dert", "G\u00e4r\u00b7ten", "land\u00b7haft", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht mit beseeltem Marmor strahlen,", "tokens": ["Nicht", "mit", "be\u00b7seel\u00b7tem", "Mar\u00b7mor", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht mit Orange-W\u00e4ldern prahlen", "tokens": ["Nicht", "mit", "O\u00b7ran\u00b7ge\u00b7W\u00e4l\u00b7dern", "prah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die G\u00e4rten hier zur sch\u00f6nen Zeit.", "tokens": ["Die", "G\u00e4r\u00b7ten", "hier", "zur", "sch\u00f6\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nebst einem kleinen Sommerhause,", "tokens": ["Nebst", "ei\u00b7nem", "klei\u00b7nen", "Som\u00b7mer\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zu einem abendlichen Schmause,", "tokens": ["Zu", "ei\u00b7nem", "a\u00b7bend\u00b7li\u00b7chen", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Gew\u00e4hren sie der Fr\u00f6hligkeit", "tokens": ["Ge\u00b7w\u00e4h\u00b7ren", "sie", "der", "Fr\u00f6h\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Viel Gras, sich scherzend hinzustrecken,", "tokens": ["Viel", "Gras", ",", "sich", "scher\u00b7zend", "hin\u00b7zu\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und, Amors Freuden zu verstecken,", "tokens": ["Und", ",", "A\u00b7mors", "Freu\u00b7den", "zu", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Viel Schatten, viele Dunkelheit.", "tokens": ["Viel", "Schat\u00b7ten", ",", "vie\u00b7le", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Anmuth lockt auf allen Wegen", "tokens": ["Die", "An\u00b7muth", "lockt", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Jm Schoos des Fr\u00fchlings mir entgegen:", "tokens": ["Jm", "Schoos", "des", "Fr\u00fch\u00b7lings", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dem Reiz begegnet ieder Blick.", "tokens": ["Dem", "Reiz", "be\u00b7geg\u00b7net", "ie\u00b7der", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er schweift herum in weiter Sph\u00e4re:", "tokens": ["Er", "schweift", "he\u00b7rum", "in", "wei\u00b7ter", "Sph\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Damit kein Berg der Aussicht wehre,", "tokens": ["Da\u00b7mit", "kein", "Berg", "der", "Aus\u00b7sicht", "weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Steht ieder ehrfurchtvoll zur\u00fcck.", "tokens": ["Steht", "ie\u00b7der", "ehr\u00b7furcht\u00b7voll", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der Steinsburg kahle Glatze strecket", "tokens": ["Der", "Steins\u00b7burg", "kah\u00b7le", "Glat\u00b7ze", "stre\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Sich in des Donners Aufenthalt;", "tokens": ["Sich", "in", "des", "Don\u00b7ners", "Auf\u00b7ent\u00b7halt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und ihre breite Schultern decket", "tokens": ["Und", "ih\u00b7re", "brei\u00b7te", "Schul\u00b7tern", "de\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Furcht, schwarze Finsterni\u00df und Wald.", "tokens": ["Furcht", ",", "schwar\u00b7ze", "Fins\u00b7ter\u00b7ni\u00df", "und", "Wald", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Gleich furchtbar, noch erhabner th\u00fcrmet", "tokens": ["Gleich", "furcht\u00b7bar", ",", "noch", "er\u00b7hab\u00b7ner", "th\u00fcr\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Das Gleichgeb\u00fcrge sich empor:", "tokens": ["Das", "Gleich\u00b7ge\u00b7b\u00fcr\u00b7ge", "sich", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Von seinen d\u00fcstern Eichen st\u00fcrmet", "tokens": ["Von", "sei\u00b7nen", "d\u00fcs\u00b7tern", "Ei\u00b7chen", "st\u00fcr\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Der Nord in m\u00fcder Wandrer Ohr.", "tokens": ["Der", "Nord", "in", "m\u00fc\u00b7der", "Wand\u00b7rer", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "O du, die Busch und Gras bekleiden,", "tokens": ["O", "du", ",", "die", "Busch", "und", "Gras", "be\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Du, Hartenburg! stehst zwischen Beyden,", "tokens": ["Du", ",", "Har\u00b7ten\u00b7burg", "!", "stehst", "zwi\u00b7schen", "Bey\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Zwar niedrig, aber angenehm!", "tokens": ["Zwar", "nied\u00b7rig", ",", "a\u00b7ber", "an\u00b7ge\u00b7nehm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Das Klettern kan ich niemals leiden;", "tokens": ["Das", "Klet\u00b7tern", "kan", "ich", "nie\u00b7mals", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Doch dich besteig ich ganz bequem.", "tokens": ["Doch", "dich", "be\u00b7steig", "ich", "ganz", "be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ich steig, in k\u00fchlen Abendstunden,", "tokens": ["Ich", "steig", ",", "in", "k\u00fch\u00b7len", "A\u00b7bends\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Zu dir an G\u00e4rten spielend hin:", "tokens": ["Zu", "dir", "an", "G\u00e4r\u00b7ten", "spie\u00b7lend", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "In diesen k\u00fchlen Abendstunden", "tokens": ["In", "die\u00b7sen", "k\u00fch\u00b7len", "A\u00b7bends\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wird hier der B\u00fcrger oft mit seiner Frau gefunden,", "tokens": ["Wird", "hier", "der", "B\u00fcr\u00b7ger", "oft", "mit", "sei\u00b7ner", "Frau", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Oft auch mit einer Nachtbarin.", "tokens": ["Oft", "auch", "mit", "ei\u00b7ner", "Nacht\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Auch Bachus hat, wer sollt es glauben?", "tokens": ["Auch", "Ba\u00b7chus", "hat", ",", "wer", "sollt", "es", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Bekr\u00e4nzt mit essigsauern Trauben,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "es\u00b7si\u00b7gsau\u00b7ern", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Man weis nicht, wie? sich hin verirrt,", "tokens": ["Man", "weis", "nicht", ",", "wie", "?", "sich", "hin", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "PTKNEG", "$,", "PWAV", "$.", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Da\u00df R\u00f6mhild nun durch Wein und Bier verherrlicht", "tokens": ["Da\u00df", "R\u00f6m\u00b7hild", "nun", "durch", "Wein", "und", "Bier", "ver\u00b7herr\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "wird.", "tokens": ["wird", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "-", "measure": "single.down"}, "line.44": {"text": "O Lust! wenn von bebl\u00fchmter Spitze,", "tokens": ["O", "Lust", "!", "wenn", "von", "be\u00b7bl\u00fchm\u00b7ter", "Spit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Wo im Gestr\u00e4uch ich einsam sitze,", "tokens": ["Wo", "im", "Ge\u00b7str\u00e4uch", "ich", "ein\u00b7sam", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Wo mich die Sommerluft vergn\u00fcgt;", "tokens": ["Wo", "mich", "die", "Som\u00b7mer\u00b7luft", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Wenn ich von krausbeb\u00fcschter H\u00f6he", "tokens": ["Wenn", "ich", "von", "kraus\u00b7be\u00b7b\u00fcschter", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Die grossen Weiten \u00fcbersehe,", "tokens": ["Die", "gros\u00b7sen", "Wei\u00b7ten", "\u00fc\u00b7ber\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Die itzt mein Auge frey umfliegt;", "tokens": ["Die", "itzt", "mein", "Au\u00b7ge", "frey", "um\u00b7fliegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Wenn hier ein schattigt W\u00e4ldchen rauschet,", "tokens": ["Wenn", "hier", "ein", "schat\u00b7tigt", "W\u00e4ld\u00b7chen", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Wo Amor, flieht ihr Sch\u00f6nen! lauschet;", "tokens": ["Wo", "A\u00b7mor", ",", "flieht", "ihr", "Sch\u00f6\u00b7nen", "!", "lau\u00b7schet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Dort unbestrahlte W\u00e4lder brausen,", "tokens": ["Dort", "un\u00b7be\u00b7strahl\u00b7te", "W\u00e4l\u00b7der", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Und hier der West mit sanftem Sausen", "tokens": ["Und", "hier", "der", "West", "mit", "sanf\u00b7tem", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Auf wallendem Getraide liegt;", "tokens": ["Auf", "wal\u00b7len\u00b7dem", "Ge\u00b7trai\u00b7de", "liegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Wenn bald mit seinen weissen W\u00e4nden", "tokens": ["Wenn", "bal