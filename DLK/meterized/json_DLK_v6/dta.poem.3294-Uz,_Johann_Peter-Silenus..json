{"dta.poem.3294": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Silenus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den Gott Silen! mit heiligem Erstaunen,", "tokens": ["Ich", "sah", "den", "Gott", "Si\u00b7len", "!", "mit", "hei\u00b7li\u00b7gem", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jhr Enkel! sah ich ihn! er zechte mit den Faunen,", "tokens": ["Ihr", "En\u00b7kel", "!", "sah", "ich", "ihn", "!", "er", "zech\u00b7te", "mit", "den", "Fau\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und lehrte die betrunkne Schaar!", "tokens": ["Und", "lehr\u00b7te", "die", "be\u00b7trunk\u00b7ne", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sang, erf\u00fcllt vom Gott der traubenvollen H\u00f6hen:", "tokens": ["Er", "sang", ",", "er\u00b7f\u00fcllt", "vom", "Gott", "der", "trau\u00b7ben\u00b7vol\u00b7len", "H\u00f6\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Epheukranz verbarg des Alten graues Haar;", "tokens": ["Ein", "E\u00b7pheu\u00b7kranz", "ver\u00b7barg", "des", "Al\u00b7ten", "grau\u00b7es", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Adern schwollen von Ly\u00e4en.", "tokens": ["Die", "A\u00b7dern", "schwol\u00b7len", "von", "Ly\u00b7\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Muse sey verg\u00f6nnt, dir, Vater, nachzulallen!", "tokens": ["Der", "Mu\u00b7se", "sey", "ver\u00b7g\u00f6nnt", ",", "dir", ",", "Va\u00b7ter", ",", "nach\u00b7zu\u00b7lal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPER", "$,", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich h\u00f6r ihr Saitenspiel von deinem Lied erschallen:", "tokens": ["Ich", "h\u00f6r", "ihr", "Sai\u00b7ten\u00b7spiel", "von", "dei\u00b7nem", "Lied", "er\u00b7schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch Nymphen merkten auf dein Lied!", "tokens": ["Auch", "Nym\u00b7phen", "merk\u00b7ten", "auf", "dein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sangst, wie ungest\u00fcm das finstre Chaos br\u00fcllte,", "tokens": ["Du", "sangst", ",", "wie", "un\u00b7ge\u00b7st\u00fcm", "das", "finst\u00b7re", "Chaos", "br\u00fcll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bis Erd und schwarze Fluth und Luft und Feuer schied,", "tokens": ["Bis", "Erd", "und", "schwar\u00b7ze", "Fluth", "und", "Luft", "und", "Feu\u00b7er", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sich die alte Zwietracht stillte.", "tokens": ["Und", "sich", "die", "al\u00b7te", "Zwiet\u00b7racht", "still\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun ward die Harmonie, des Himmels Kind, ge-", "tokens": ["Nun", "ward", "die", "Har\u00b7mo\u00b7nie", ",", "des", "Him\u00b7mels", "Kind", ",", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bohren:", "tokens": ["boh\u00b7ren", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der neuen Sonne ward ihr neu Gebieth erkohren:", "tokens": ["Der", "neu\u00b7en", "Son\u00b7ne", "ward", "ihr", "neu", "Ge\u00b7bieth", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mond nahm seine Herrschaft ein.", "tokens": ["Der", "Mond", "nahm", "sei\u00b7ne", "Herr\u00b7schaft", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald h\u00f6rte der Parna\u00df die jungen Musen singen,", "tokens": ["Bald", "h\u00f6r\u00b7te", "der", "Par\u00b7na\u00df", "die", "jun\u00b7gen", "Mu\u00b7sen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sah die Grazien in seinem Lorbeerhayn", "tokens": ["Und", "sah", "die", "Gra\u00b7zi\u00b7en", "in", "sei\u00b7nem", "Lor\u00b7beer\u00b7hayn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Arme durcheinander schlingen.", "tokens": ["Die", "Ar\u00b7me", "durch\u00b7ein\u00b7an\u00b7der", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du lehrtest, wie Mercur der Leyer Scherz erfun-", "tokens": ["Du", "lehr\u00b7test", ",", "wie", "Mer\u00b7cur", "der", "Le\u00b7yer", "Scherz", "er\u00b7fun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den;", "tokens": ["den", ";"], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und wie das erste Rohr, mit fremder Kunst verbunden,", "tokens": ["Und", "wie", "das", "ers\u00b7te", "Rohr", ",", "mit", "frem\u00b7der", "Kunst", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Pans betr\u00fcbter Hand geklagt", "tokens": ["In", "Pans", "be\u00b7tr\u00fcb\u00b7ter", "Hand", "ge\u00b7klagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Pan von Syrinx, ach! der sch\u00f6nsten Nais, brann-", "tokens": ["Als", "Pan", "von", "Sy\u00b7rinx", ",", "ach", "!", "der", "sch\u00f6ns\u00b7ten", "Nais", ",", "brann"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "NN", "APPR", "NE", "$,", "ITJ", "$.", "ART", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "te,", "tokens": ["te", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Die Ladons Tochter war und in geliebter Jagd", "tokens": ["Die", "La\u00b7dons", "Toch\u00b7ter", "war", "und", "in", "ge\u00b7lieb\u00b7ter", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Arkadiens Geh\u00f6lz durchrannte.", "tokens": ["Ar\u00b7ka\u00b7di\u00b7ens", "Ge\u00b7h\u00f6lz", "durch\u00b7rann\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die sah der Hirten Gott nach scheuem Wilde jagen;", "tokens": ["Die", "sah", "der", "Hir\u00b7ten", "Gott", "nach", "scheu\u00b7em", "Wil\u00b7de", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihr verirrtes Haar die weissen Schultern schlagen,", "tokens": ["Und", "ihr", "ver\u00b7irr\u00b7tes", "Haar", "die", "weis\u00b7sen", "Schul\u00b7tern", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihre holden Wangen gl\u00fchn.", "tokens": ["Und", "ih\u00b7re", "hol\u00b7den", "Wan\u00b7gen", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sah die sch\u00f6nste Brust den freyen Westen offen:", "tokens": ["Er", "sah", "die", "sch\u00f6ns\u00b7te", "Brust", "den", "frey\u00b7en", "Wes\u00b7ten", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhn brannte, was er sah: er war verliebt und k\u00fchn,", "tokens": ["Jhn", "brann\u00b7te", ",", "was", "er", "sah", ":", "er", "war", "ver\u00b7liebt", "und", "k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und fleht\u2019 und wagte, stolz zu hoffen.", "tokens": ["Und", "fleht'", "und", "wag\u00b7te", ",", "stolz", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Umsonst! weil Syrinx floh, wie ein gejagtes Rehe", "tokens": ["Um\u00b7sonst", "!", "weil", "Sy\u00b7rinx", "floh", ",", "wie", "ein", "ge\u00b7jag\u00b7tes", "Re\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "NE", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Tode, der ihm folgt, auf schwarzbeb\u00fcschter H\u00f6he", "tokens": ["Dem", "To\u00b7de", ",", "der", "ihm", "folgt", ",", "auf", "schwarz\u00b7be\u00b7b\u00fcschter", "H\u00f6\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit fl\u00fcgelschneller Flucht entweicht.", "tokens": ["Mit", "fl\u00fc\u00b7gel\u00b7schnel\u00b7ler", "Flucht", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es hemmen seinen Lauf nicht bluhmenvolle Felder,", "tokens": ["Es", "hem\u00b7men", "sei\u00b7nen", "Lauf", "nicht", "bluh\u00b7men\u00b7vol\u00b7le", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch die ein lautrer Bach mit heischerm Murmeln", "tokens": ["Durch", "die", "ein", "laut\u00b7rer", "Bach", "mit", "hei\u00b7scherm", "Mur\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schleicht;", "tokens": ["schleicht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Nicht Schatten sonst gew\u00fcnschter W\u00e4lder.", "tokens": ["Nicht", "Schat\u00b7ten", "sonst", "ge\u00b7w\u00fcnschter", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie floh: ihr folgte Pan, auf ungebahnten We-", "tokens": ["Sie", "floh", ":", "ihr", "folg\u00b7te", "Pan", ",", "auf", "un\u00b7ge\u00b7bahn\u00b7ten", "We"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NE", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen;", "tokens": ["gen", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Aus voller Urne rauscht\u2019 ihr Ladons Fluth entgegen;", "tokens": ["Aus", "vol\u00b7ler", "Ur\u00b7ne", "rauscht'", "ihr", "La\u00b7dons", "Fluth", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Weg war offen, zu entgehn.", "tokens": ["Kein", "Weg", "war", "of\u00b7fen", ",", "zu", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier, wo zum erstenmal die bangen F\u00fcsse ruhten,", "tokens": ["Hier", ",", "wo", "zum", "ers\u00b7ten\u00b7mal", "die", "ban\u00b7gen", "F\u00fcs\u00b7se", "ruh\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPRART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier, Schwestern! rief sie, eilt, mir h\u00fclfreich beyzustehn!", "tokens": ["Hier", ",", "Schwes\u00b7tern", "!", "rief", "sie", ",", "eilt", ",", "mir", "h\u00fclf\u00b7reich", "bey\u00b7zu\u00b7stehn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPER", "$,", "VVFIN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sprang verzweiflend in die Fluthen.", "tokens": ["Und", "sprang", "ver\u00b7zwei\u00b7flend", "in", "die", "Flut\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gleich blieb ihr leichter Fu\u00df an