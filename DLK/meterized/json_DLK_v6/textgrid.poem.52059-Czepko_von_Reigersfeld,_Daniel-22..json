{"textgrid.poem.52059": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seele trug Gentil dem Pfaffen wieder an;", "tokens": ["Die", "See\u00b7le", "trug", "Gen\u00b7til", "dem", "Pfaf\u00b7fen", "wie\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sechs Gulden gab er ihm, die jener bald verthan.", "tokens": ["Sechs", "Gul\u00b7den", "gab", "er", "ihm", ",", "die", "je\u00b7ner", "bald", "ver\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erst galt sie mehr, dann da, als er Romanisch worden,", "tokens": ["Erst", "galt", "sie", "mehr", ",", "dann", "da", ",", "als", "er", "Ro\u00b7ma\u00b7nisch", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,", "KOUS", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hat er ein reiches Ammt, stund in der Landherrn Orden.", "tokens": ["Hat", "er", "ein", "rei\u00b7ches", "Ammt", ",", "stund", "in", "der", "Land\u00b7herrn", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gentil! wie f\u00e4llt der Werth, halt an, wo du die Pflicht", "tokens": ["Gen\u00b7til", "!", "wie", "f\u00e4llt", "der", "Werth", ",", "halt", "an", ",", "wo", "du", "die", "Pflicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch k\u00fcnfftig brichst; glaub es, sechs Heller gilt sie nicht:", "tokens": ["Auch", "k\u00fcnff\u00b7tig", "brichst", ";", "glaub", "es", ",", "sechs", "Hel\u00b7ler", "gilt", "sie", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "VVFIN", "PPER", "$,", "CARD", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Pfaff! unl\u00e4ngst und itzt, und k\u00fcnfftig ist verlohren", "tokens": ["Mein", "Pfaff", "!", "un\u00b7l\u00e4ngst", "und", "itzt", ",", "und", "k\u00fcnff\u00b7tig", "ist", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADJD", "KON", "ADV", "$,", "KON", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Geld. Warum? Er hat die Seele l\u00e4ngst verschworen.", "tokens": ["Dein", "Geld", ".", "Wa\u00b7rum", "?", "Er", "hat", "die", "See\u00b7le", "l\u00e4ngst", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}