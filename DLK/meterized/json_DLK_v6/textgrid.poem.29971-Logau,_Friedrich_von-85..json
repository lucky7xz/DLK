{"textgrid.poem.29971": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was die Kirche glauben heist, soll man glauben ohne wancken;", "tokens": ["Was", "die", "Kir\u00b7che", "glau\u00b7ben", "heist", ",", "soll", "man", "glau\u00b7ben", "oh\u00b7ne", "wan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VAFIN", "$,", "VMFIN", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Also darff man weder Geist, weder Sinnen noch Gedancken.", "tokens": ["Al\u00b7so", "darff", "man", "we\u00b7der", "Geist", ",", "we\u00b7der", "Sin\u00b7nen", "noch", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "KON", "NN", "$,", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}