{"textgrid.poem.62289": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ich legte manch' Grundstein zur Humanit\u00e4t]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich legte manch' Grundstein zur Humanit\u00e4t,", "tokens": ["Ich", "leg\u00b7te", "man\u00b7ch'", "Grund\u00b7stein", "zur", "Hu\u00b7ma\u00b7ni\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch wer sie nicht liebet, sie nicht versteht,", "tokens": ["Doch", "wer", "sie", "nicht", "lie\u00b7bet", ",", "sie", "nicht", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er gleichet dem Maulwurf, er scheuet das Licht,", "tokens": ["Er", "glei\u00b7chet", "dem", "Maul\u00b7wurf", ",", "er", "scheu\u00b7et", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn manchmal hinein in die H\u00f6hle es bricht.", "tokens": ["Wenn", "manch\u00b7mal", "hin\u00b7ein", "in", "die", "H\u00f6h\u00b7le", "es", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Doch V\u00f6gel und Blumen und Menschen vereint,", "tokens": ["Doch", "V\u00f6\u00b7gel", "und", "Blu\u00b7men", "und", "Men\u00b7schen", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie trauern, wenn ihnen die Sonne nicht scheint.", "tokens": ["Sie", "trau\u00b7ern", ",", "wenn", "ih\u00b7nen", "die", "Son\u00b7ne", "nicht", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Sonne des Lebens ist Humanit\u00e4t,", "tokens": ["Die", "Son\u00b7ne", "des", "Le\u00b7bens", "ist", "Hu\u00b7ma\u00b7ni\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wer es verleugnet, es nicht versteht! \u2013", "tokens": ["Und", "wer", "es", "ver\u00b7leug\u00b7net", ",", "es", "nicht", "ver\u00b7steht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}