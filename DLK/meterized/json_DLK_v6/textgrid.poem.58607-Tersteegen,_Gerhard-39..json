{"textgrid.poem.58607": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man will Gerechtigkeit, die nur von au\u00dfen decket,", "tokens": ["Man", "will", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "die", "nur", "von", "au\u00b7\u00dfen", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "$,", "PRELS", "ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da Welt- und S\u00fcndenlust noch tief im Herzen stecket;", "tokens": ["Da", "Welt", "und", "S\u00fcn\u00b7den\u00b7lust", "noch", "tief", "im", "Her\u00b7zen", "ste\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Jesus selber ist, da ist Gerechtigkeit,", "tokens": ["Wo", "Je\u00b7sus", "sel\u00b7ber", "ist", ",", "da", "ist", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hast du ein b\u00f6ses Herz, so ist sie dir noch weit.", "tokens": ["Hast", "du", "ein", "b\u00f6\u00b7ses", "Herz", ",", "so", "ist", "sie", "dir", "noch", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich will mich wickeln ein von au\u00dfen und von innen,", "tokens": ["Ich", "will", "mich", "wi\u00b7ckeln", "ein", "von", "au\u00b7\u00dfen", "und", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "APPR", "ADV", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Jesus, ganz in dich mit Herze, Lust und Sinnen;", "tokens": ["Mein", "Je\u00b7sus", ",", "ganz", "in", "dich", "mit", "Her\u00b7ze", ",", "Lust", "und", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "ADV", "APPR", "PRF", "APPR", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir fehlt Gerechtigkeit \u2013 bring du sie selber nah,", "tokens": ["Mir", "fehlt", "Ge\u00b7rech\u00b7tig\u00b7keit", "\u2013", "bring", "du", "sie", "sel\u00b7ber", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ist dein herrlich Bild und alles Heil auch da!", "tokens": ["So", "ist", "dein", "herr\u00b7lich", "Bild", "und", "al\u00b7les", "Heil", "auch", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "KON", "PIAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}