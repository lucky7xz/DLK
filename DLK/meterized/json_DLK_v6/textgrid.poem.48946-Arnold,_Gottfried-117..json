{"textgrid.poem.48946": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "117.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Salamander soll im Feuer k\u00f6nnen leben:", "tokens": ["Der", "Sa\u00b7la\u00b7man\u00b7der", "soll", "im", "Feu\u00b7er", "k\u00f6n\u00b7nen", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin kein solches Thier/ und leb doch in der Gluht;", "tokens": ["Ich", "bin", "kein", "sol\u00b7ches", "Thier", "/", "und", "leb", "doch", "in", "der", "Gluht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$(", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zorn und Begierde kan den Geist als Feuer heben.", "tokens": ["Zorn", "und", "Be\u00b7gier\u00b7de", "kan", "den", "Geist", "als", "Feu\u00b7er", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ART", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ach Wasser/ Wasser her! lescht ab mein heisses Blut!", "tokens": ["Ach", "Was\u00b7ser", "/", "Was\u00b7ser", "her", "!", "lescht", "ab", "mein", "heis\u00b7ses", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "NN", "PTKVZ", "$.", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}