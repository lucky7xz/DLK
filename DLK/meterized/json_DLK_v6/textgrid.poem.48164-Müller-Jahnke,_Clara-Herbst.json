{"textgrid.poem.48164": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun: der Wind geht hohl und schwer,", "tokens": ["Und", "nun", ":", "der", "Wind", "geht", "hohl", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in wei\u00dfen Wogen sch\u00e4umt das Meer \u2013", "tokens": ["in", "wei\u00b7\u00dfen", "Wo\u00b7gen", "sch\u00e4umt", "das", "Meer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nun ist der Herbst gekommen", "tokens": ["nun", "ist", "der", "Herbst", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und hat vom Feld den Morgentau", "tokens": ["und", "hat", "vom", "Feld", "den", "Mor\u00b7gen\u00b7tau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und hat das letzte St\u00fcckchen Blau", "tokens": ["und", "hat", "das", "letz\u00b7te", "St\u00fcck\u00b7chen", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "vom Himmel weggenommen.", "tokens": ["vom", "Him\u00b7mel", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und nun fahr hin! \u2013 Es rauscht und zieht", "tokens": ["Und", "nun", "fahr", "hin", "!", "\u2013", "Es", "rauscht", "und", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch dunkle Luft ein dunkles Lied;", "tokens": ["durch", "dunk\u00b7le", "Luft", "ein", "dunk\u00b7les", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich mag nicht ruhn und tr\u00e4umen.", "tokens": ["ich", "mag", "nicht", "ruhn", "und", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich liege wach die ganze Nacht", "tokens": ["Ich", "lie\u00b7ge", "wach", "die", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und horche auf die hei\u00dfe Schlacht,", "tokens": ["und", "hor\u00b7che", "auf", "die", "hei\u00b7\u00dfe", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das St\u00f6hnen in den B\u00e4umen.", "tokens": ["das", "St\u00f6h\u00b7nen", "in", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und nun fahr hin. Das war ein Jahr,", "tokens": ["Und", "nun", "fahr", "hin", ".", "Das", "war", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so fr\u00fcchtereif, so freudenklar . . .", "tokens": ["so", "fr\u00fcch\u00b7ter\u00b7eif", ",", "so", "freu\u00b7den\u00b7klar", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nun la\u00df die Bl\u00e4tter treiben.", "tokens": ["nun", "la\u00df", "die", "Bl\u00e4t\u00b7ter", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fahr hin! Die Saat von deiner Hand,", "tokens": ["Fahr", "hin", "!", "Die", "Saat", "von", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Ernte, die in Halmen stand,", "tokens": ["die", "Ern\u00b7te", ",", "die", "in", "Hal\u00b7men", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mu\u00df doch mein eigen bleiben.", "tokens": ["mu\u00df", "doch", "mein", "ei\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}