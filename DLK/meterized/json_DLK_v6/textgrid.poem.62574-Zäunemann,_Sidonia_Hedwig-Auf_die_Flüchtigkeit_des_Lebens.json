{"textgrid.poem.62574": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Fl\u00fcchtigkeit des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist heut ein Mensch sch\u00f6n, munter, weis und roth;", "tokens": ["Ist", "heut", "ein", "Mensch", "sch\u00f6n", ",", "mun\u00b7ter", ",", "weis", "und", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,", "ADJD", "$,", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ist er morgen bla\u00df und todt.", "tokens": ["So", "ist", "er", "mor\u00b7gen", "bla\u00df", "und", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6nheit Schmuck verschwindt", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Schmuck", "ver\u00b7schwindt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Rauch und Wind.", "tokens": ["Wie", "Rauch", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Drum Mensch, bist du mit Sch\u00f6nheit oder Jugend,", "tokens": ["Drum", "Mensch", ",", "bist", "du", "mit", "Sch\u00f6n\u00b7heit", "o\u00b7der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Gl\u00fcck und Kunst und Gaben ausger\u00fcst,", "tokens": ["Mit", "Gl\u00fcck", "und", "Kunst", "und", "Ga\u00b7ben", "aus\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gedenke, da\u00df du Staub und Erden bist.", "tokens": ["Ge\u00b7den\u00b7ke", ",", "da\u00df", "du", "Staub", "und", "Er\u00b7den", "bist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bestrebe dich vielmehr vor allen,", "tokens": ["Be\u00b7stre\u00b7be", "dich", "viel\u00b7mehr", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur Gott; nicht Menschen zu gefallen.", "tokens": ["Nur", "Gott", ";", "nicht", "Men\u00b7schen", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bem\u00fche dich um sch\u00f6ne Geistes-Gaben,", "tokens": ["Be\u00b7m\u00fc\u00b7he", "dich", "um", "sch\u00f6\u00b7ne", "Geis\u00b7tes\u00b7Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So lebst du, hat man dich schon l\u00e4ngst begraben.", "tokens": ["So", "lebst", "du", ",", "hat", "man", "dich", "schon", "l\u00e4ngst", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "PIS", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}