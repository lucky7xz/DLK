{"textgrid.poem.32514": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "[in Friedrich Ludwig Schr\u00f6ders Stammbuch]", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Beifall dich nicht stolz, nicht Tadel furchtsam mache!", "tokens": ["Da\u00df", "Bei\u00b7fall", "dich", "nicht", "stolz", ",", "nicht", "Ta\u00b7del", "furcht\u00b7sam", "ma\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PTKNEG", "ADJD", "$,", "PTKNEG", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des K\u00fcnstlers Sch\u00e4tzung ist nicht jedes F\u00fchlers Sache!", "tokens": ["Des", "K\u00fcnst\u00b7lers", "Sch\u00e4t\u00b7zung", "ist", "nicht", "je\u00b7des", "F\u00fch\u00b7lers", "Sa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKNEG", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn auch den Blinden ", "tokens": ["Denn", "auch", "den", "Blin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und wer dich ", "tokens": ["Und", "wer", "dich"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}}}}