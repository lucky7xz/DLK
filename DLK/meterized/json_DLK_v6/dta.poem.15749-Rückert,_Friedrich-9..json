{"dta.poem.15749": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Schrifterkl\u00e4rers Fluch ist Alles zu erkl\u00e4ren,", "tokens": ["Des", "Schrif\u00b7ter\u00b7kl\u00e4\u00b7rers", "Fluch", "ist", "Al\u00b7les", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob am Himmel nicht auch Nebelsterne w\u00e4ren;", "tokens": ["Al\u00b7sob", "am", "Him\u00b7mel", "nicht", "auch", "Ne\u00b7bels\u00b7ter\u00b7ne", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An einem Blatt im Buch, der Raupe gleich, zu kleben,", "tokens": ["An", "ei\u00b7nem", "Blatt", "im", "Buch", ",", "der", "Rau\u00b7pe", "gleich", ",", "zu", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt wie der Schmetterling die Bl\u00fcte zu beschweben.", "tokens": ["Statt", "wie", "der", "Schmet\u00b7ter\u00b7ling", "die", "Bl\u00fc\u00b7te", "zu", "be\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich aber rathe dir, dich nicht sosehr zu plagen,", "tokens": ["Ich", "a\u00b7ber", "ra\u00b7the", "dir", ",", "dich", "nicht", "so\u00b7sehr", "zu", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was du nicht verstehst, getrost zu \u00fcberschlagen.", "tokens": ["Und", "was", "du", "nicht", "ver\u00b7stehst", ",", "ge\u00b7trost", "zu", "\u00fc\u00b7bersc\u00b7hla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn was dir Einzelnes geblieben unverst\u00e4ndlich,", "tokens": ["Denn", "was", "dir", "Ein\u00b7zel\u00b7nes", "ge\u00b7blie\u00b7ben", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PIS", "VVPP", "ADJD", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aus dem Zusammenhang verstehst du doch es endlich.", "tokens": ["Aus", "dem", "Zu\u00b7sam\u00b7men\u00b7hang", "ver\u00b7stehst", "du", "doch", "es", "end\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Noch besser, wenn du gar nicht suchst Zusammenhang,", "tokens": ["Noch", "bes\u00b7ser", ",", "wenn", "du", "gar", "nicht", "suchst", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dich auf jeden Schritt erfreut der Wandelgang.", "tokens": ["Und", "dich", "auf", "je\u00b7den", "Schritt", "er\u00b7freut", "der", "Wan\u00b7del\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}