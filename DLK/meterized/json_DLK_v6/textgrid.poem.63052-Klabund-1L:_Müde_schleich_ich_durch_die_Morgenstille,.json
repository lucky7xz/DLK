{"textgrid.poem.63052": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcde schleich ich durch die Morgenstille,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcde schleich ich durch die Morgenstille,", "tokens": ["M\u00fc\u00b7de", "schleich", "ich", "durch", "die", "Mor\u00b7gen\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und es bebt in mir ein fremder Wille.", "tokens": ["Und", "es", "bebt", "in", "mir", "ein", "frem\u00b7der", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie die Glocken fernes Ave l\u00e4uten,", "tokens": ["Wie", "die", "Glo\u00b7cken", "fer\u00b7nes", "A\u00b7ve", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Scheint es mir Verachtung zu bedeuten", "tokens": ["Scheint", "es", "mir", "Ver\u00b7ach\u00b7tung", "zu", "be\u00b7deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Meinen Lippen, die noch dunkel bluten", "tokens": ["Mei\u00b7nen", "Lip\u00b7pen", ",", "die", "noch", "dun\u00b7kel", "blu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von des Weibes ungehemmten Gluten;", "tokens": ["Von", "des", "Wei\u00b7bes", "un\u00b7ge\u00b7hemm\u00b7ten", "Glu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ha\u00df, da\u00df ich die Tage frei verprasse,", "tokens": ["Ha\u00df", ",", "da\u00df", "ich", "die", "Ta\u00b7ge", "frei", "ver\u00b7pras\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ein Armer nicht in Zucht sie fasse.", "tokens": ["Und", "ein", "Ar\u00b7mer", "nicht", "in", "Zucht", "sie", "fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "\u2013 Nimmer neid ich euch die Kirchenenge", "tokens": ["\u2013", "Nim\u00b7mer", "neid", "ich", "euch", "die", "Kir\u00b7che\u00b7nen\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und den K\u00fcster. Zerren wir die Str\u00e4nge,", "tokens": ["Und", "den", "K\u00fcs\u00b7ter", ".", "Zer\u00b7ren", "wir", "die", "Str\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Soll ins Land der Kl\u00f6ppel donnernd h\u00e4mmern:", "tokens": ["Soll", "ins", "Land", "der", "Kl\u00f6p\u00b7pel", "don\u00b7nernd", "h\u00e4m\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Morgenrot! Klabund! die Tage d\u00e4mmern!", "tokens": ["Mor\u00b7gen\u00b7rot", "!", "Kla\u00b7bund", "!", "die", "Ta\u00b7ge", "d\u00e4m\u00b7mern", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}