{"textgrid.poem.60461": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Bauern am Tissastrande", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6richte Freunde des toten Alten,", "tokens": ["T\u00f6\u00b7rich\u00b7te", "Freun\u00b7de", "des", "to\u00b7ten", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+----+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Fahrend in ausgeleierten Gleisen,", "tokens": ["Fah\u00b7rend", "in", "aus\u00b7ge\u00b7lei\u00b7er\u00b7ten", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Tanzend nach verklungenen Weisen,", "tokens": ["Tan\u00b7zend", "nach", "ver\u00b7klun\u00b7ge\u00b7nen", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "M\u00f6ge dies M\u00e4rlein euch unterhalten!", "tokens": ["M\u00f6\u00b7ge", "dies", "M\u00e4r\u00b7lein", "euch", "un\u00b7ter\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Warme lebendige L\u00fcfte wallen", "tokens": ["War\u00b7me", "le\u00b7ben\u00b7di\u00b7ge", "L\u00fcf\u00b7te", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u00dcber dem sch\u00f6nen Magyarenlande,", "tokens": ["\u00dc\u00b7ber", "dem", "sch\u00f6\u00b7nen", "Ma\u00b7gy\u00b7ar\u00b7en\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "In den Geb\u00fcschen die Nachtigallen", "tokens": ["In", "den", "Ge\u00b7b\u00fc\u00b7schen", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Singen entz\u00fcckt am Tissastrande.", "tokens": ["Sin\u00b7gen", "ent\u00b7z\u00fcckt", "am", "Tis\u00b7sast\u00b7ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Fischlein, springend mit stillem Ergetzen,", "tokens": ["Fisc\u00b7hlein", ",", "sprin\u00b7gend", "mit", "stil\u00b7lem", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Holen vom Lenz sich fl\u00fcchtigen Ku\u00df,", "tokens": ["Ho\u00b7len", "vom", "Lenz", "sich", "fl\u00fcch\u00b7ti\u00b7gen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "F\u00fcrchten sich nicht vor den silbernen Netzen,", "tokens": ["F\u00fcrch\u00b7ten", "sich", "nicht", "vor", "den", "sil\u00b7ber\u00b7nen", "Net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Welche der Mond warf \u00fcber den Flu\u00df.", "tokens": ["Wel\u00b7che", "der", "Mond", "warf", "\u00fc\u00b7ber", "den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Brausend vor Freude, m\u00fcnden die Quellen,", "tokens": ["Brau\u00b7send", "vor", "Freu\u00b7de", ",", "m\u00fcn\u00b7den", "die", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "VMFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und das lenzbezauberte Land,", "tokens": ["Und", "das", "lenz\u00b7be\u00b7zau\u00b7ber\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weil es nicht bl\u00fchn kann unter den Wellen,", "tokens": ["Weil", "es", "nicht", "bl\u00fchn", "kann", "un\u00b7ter", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bl\u00fcht es hier doppelt als \u00fcppiger Strand,", "tokens": ["Bl\u00fcht", "es", "hier", "dop\u00b7pelt", "als", "\u00fcp\u00b7pi\u00b7ger", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Weil es nicht singen kann unter den Wogen,", "tokens": ["Weil", "es", "nicht", "sin\u00b7gen", "kann", "un\u00b7ter", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Singt es daf\u00fcr hier doppelt so laut,", "tokens": ["Singt", "es", "da\u00b7f\u00fcr", "hier", "dop\u00b7pelt", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Liebest\u00f6nen, schmachtend gezogen,", "tokens": ["Lie\u00b7be\u00b7st\u00f6\u00b7nen", ",", "schmach\u00b7tend", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Lauscht des Sprossers gl\u00fcckselige Braut.", "tokens": ["Lauscht", "des", "Spros\u00b7sers", "gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "R\u00fcstig rudern dort \u00fcber die Wellen", "tokens": ["R\u00fcs\u00b7tig", "ru\u00b7dern", "dort", "\u00fc\u00b7ber", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lustige Bauern mit Scherzen und Lachen,", "tokens": ["Lus\u00b7ti\u00b7ge", "Bau\u00b7ern", "mit", "Scher\u00b7zen", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und die Zigeuner, ihre Gesellen,", "tokens": ["Und", "die", "Zi\u00b7geu\u00b7ner", ",", "ih\u00b7re", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stimmen die Geigen bereits im Nachen,", "tokens": ["Stim\u00b7men", "die", "Gei\u00b7gen", "be\u00b7reits", "im", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sto\u00dfen ans Land und eilen zur Schenke;", "tokens": ["Sto\u00b7\u00dfen", "ans", "Land", "und", "ei\u00b7len", "zur", "Schen\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Weil so laut das heischende Rufen,", "tokens": ["Weil", "so", "laut", "das", "hei\u00b7schen\u00b7de", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Springen die Wirte schon mit dem Getr\u00e4nke", "tokens": ["Sprin\u00b7gen", "die", "Wir\u00b7te", "schon", "mit", "dem", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "\u00dcber die finsteren Kellerstufen.", "tokens": ["\u00dc\u00b7ber", "die", "fins\u00b7te\u00b7ren", "Kel\u00b7ler\u00b7stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Um den Eichtisch sitzen die Alten,", "tokens": ["Um", "den", "Eich\u00b7tisch", "sit\u00b7zen", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor dem Tanze noch Schmaus zu halten.", "tokens": ["Vor", "dem", "Tan\u00b7ze", "noch", "Schmaus", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zum Abschnitt gereicht, in der Runde", "tokens": ["Zum", "Ab\u00b7schnitt", "ge\u00b7reicht", ",", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geht das k\u00f6stliche Weizenbrot,", "tokens": ["Geht", "das", "k\u00f6st\u00b7li\u00b7che", "Wei\u00b7zen\u00b7brot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und sie f\u00fchren behaglich zum Munde", "tokens": ["Und", "sie", "f\u00fch\u00b7ren", "be\u00b7hag\u00b7lich", "zum", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Feurigen Wein, tiefdunkelrot;", "tokens": ["Feu\u00b7ri\u00b7gen", "Wein", ",", "tief\u00b7dun\u00b7kel\u00b7rot", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wischen sich trocken und schieben zur Seite,", "tokens": ["Wi\u00b7schen", "sich", "tro\u00b7cken", "und", "schie\u00b7ben", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Da\u00df er den Speisen den Weg nicht bestreite,", "tokens": ["Da\u00df", "er", "den", "Spei\u00b7sen", "den", "Weg", "nicht", "be\u00b7strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Schnurrbarts buschigten halben Kranz;", "tokens": ["Schnurr\u00b7barts", "bu\u00b7schig\u00b7ten", "hal\u00b7ben", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Braten und Schinken, warme und k\u00fchle,", "tokens": ["Bra\u00b7ten", "und", "Schin\u00b7ken", ",", "war\u00b7me", "und", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "KON", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Wandern geschwind in die kn\u00f6cherne M\u00fchle,", "tokens": ["Wan\u00b7dern", "ge\u00b7schwind", "in", "die", "kn\u00f6\u00b7cher\u00b7ne", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Dort die Jungen fliegen zum Tanz.", "tokens": ["Dort", "die", "Jun\u00b7gen", "flie\u00b7gen", "zum", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Hei! wie die Geigen singen und klingen!", "tokens": ["Hei", "!", "wie", "die", "Gei\u00b7gen", "sin\u00b7gen", "und", "klin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Heil wie die H\u00e4mmer des Zimbals springen", "tokens": ["Heil", "wie", "die", "H\u00e4m\u00b7mer", "des", "Zim\u00b7bals", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00dcber die Saiten frisch auf und nieder,", "tokens": ["\u00dc\u00b7ber", "die", "Sai\u00b7ten", "frisch", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Pochender Herzschlag heimischer Lieder.", "tokens": ["Po\u00b7chen\u00b7der", "Herz\u00b7schlag", "hei\u00b7mi\u00b7scher", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Himmel! wie jauchzen die Geigen so helle,", "tokens": ["Him\u00b7mel", "!", "wie", "jauch\u00b7zen", "die", "Gei\u00b7gen", "so", "hel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Schmetternd schreit Klarinette, die grelle.", "tokens": ["Schmet\u00b7ternd", "schreit", "Kla\u00b7ri\u00b7net\u00b7te", ",", "die", "grel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Weinendes Klagen, Freudengekicher", "tokens": ["Wei\u00b7nen\u00b7des", "Kla\u00b7gen", ",", "Freu\u00b7den\u00b7ge\u00b7ki\u00b7cher"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sch\u00fcttern im schroffen Wechsel die Luft,", "tokens": ["Sch\u00fct\u00b7tern", "im", "schrof\u00b7fen", "Wech\u00b7sel", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Setzen gewaltig, keck und sicher", "tokens": ["Set\u00b7zen", "ge\u00b7wal\u00b7tig", ",", "keck", "und", "si\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00dcber des Mi\u00dfklangs drohende Kluft.", "tokens": ["\u00dc\u00b7ber", "des", "Mi\u00df\u00b7klangs", "dro\u00b7hen\u00b7de", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Alle die T\u00f6ne, sie klettern, sie tanzen,", "tokens": ["Al\u00b7le", "die", "T\u00f6\u00b7ne", ",", "sie", "klet\u00b7tern", ",", "sie", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "PPER", "VVINF", "$,", "PPER", "VVINF", "$,"], "