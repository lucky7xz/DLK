{"textgrid.poem.56255": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Kleidermacher-Mut", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als die Schneider revoltiert, \u2013", "tokens": ["Und", "als", "die", "Schnei\u00b7der", "re\u00b7vol\u00b7tiert", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ART", "NE", "NE", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Courage! Courage!", "tokens": ["Cou\u00b7ra\u00b7ge", "!", "Cou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So haben gar grausam sie massakriert", "tokens": ["So", "ha\u00b7ben", "gar", "grau\u00b7sam", "sie", "mas\u00b7sak\u00b7riert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PPER", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und stolz am Ende parlamentiert:", "tokens": ["Und", "stolz", "am", "En\u00b7de", "par\u00b7la\u00b7men\u00b7tiert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Herr K\u00f6nig, das sollst du uns schw\u00f6ren.", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "das", "sollst", "du", "uns", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PDS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und drei Bedingungen wollen wir stelln: \u2013", "tokens": ["Und", "drei", "Be\u00b7din\u00b7gun\u00b7gen", "wol\u00b7len", "wir", "stelln", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "CARD", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Courage! Courage!", "tokens": ["Cou\u00b7ra\u00b7ge", "!", "Cou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schaff ab, zum ersten, die Schneider-Mamselln,", "tokens": ["Schaff", "ab", ",", "zum", "ers\u00b7ten", ",", "die", "Schnei\u00b7der\u00b7Mam\u00b7selln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "APPRART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die das Brod verk\u00fcrzt uns Schneider-Geselln;", "tokens": ["Die", "das", "Brod", "ver\u00b7k\u00fcrzt", "uns", "Schnei\u00b7der\u00b7Ge\u00b7selln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Herr K\u00f6nig, das sollst du uns schw\u00f6ren.", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "das", "sollst", "du", "uns", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PDS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Die brennende Pfeife, zum andern, sei \u2013", "tokens": ["Die", "bren\u00b7nen\u00b7de", "Pfei\u00b7fe", ",", "zum", "an\u00b7dern", ",", "sei", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "$,", "VAFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Courage! Courage!", "tokens": ["Cou\u00b7ra\u00b7ge", "!", "Cou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zum h\u00f6chsten \u00c4rger der Polizei,", "tokens": ["Zum", "h\u00f6chs\u00b7ten", "\u00c4r\u00b7ger", "der", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf offener Stra\u00dfe uns Schneidern frei;", "tokens": ["Auf", "of\u00b7fe\u00b7ner", "Stra\u00b7\u00dfe", "uns", "Schnei\u00b7dern", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Herr K\u00f6nig, das sollst du uns schw\u00f6ren.", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "das", "sollst", "du", "uns", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PDS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Das dritte, Herr K\u00f6nig, noch wissen wir's nicht, \u2013", "tokens": ["Das", "drit\u00b7te", ",", "Herr", "K\u00f6\u00b7nig", ",", "noch", "wis\u00b7sen", "wir's", "nicht", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "NN", "$,", "ADV", "VVINF", "VAFIN", "PTKNEG", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Courage! Courage!", "tokens": ["Cou\u00b7ra\u00b7ge", "!", "Cou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch bleibt es das Beste an der ganzen Geschicht,", "tokens": ["Doch", "bleibt", "es", "das", "Bes\u00b7te", "an", "der", "gan\u00b7zen", "Ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wir bestehn auch darauf bis ans J\u00fcngste Gericht;", "tokens": ["Wir", "be\u00b7stehn", "auch", "da\u00b7rauf", "bis", "ans", "J\u00fcngs\u00b7te", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Das dritte, das sollst du uns schw\u00f6ren.", "tokens": ["Das", "drit\u00b7te", ",", "das", "sollst", "du", "uns", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PDS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}