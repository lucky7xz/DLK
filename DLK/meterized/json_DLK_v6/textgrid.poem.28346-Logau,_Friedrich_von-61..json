{"textgrid.poem.28346": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sterben war wol immer lieb, dem, der dorte sucht zu leben,", "tokens": ["Ster\u00b7ben", "war", "wol", "im\u00b7mer", "lieb", ",", "dem", ",", "der", "dor\u00b7te", "sucht", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "$,", "PRELS", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--++-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der da wuste, da\u00df die Welt ihm, und er nicht ihr, gegeben,", "tokens": ["Der", "da", "wus\u00b7te", ",", "da\u00df", "die", "Welt", "ihm", ",", "und", "er", "nicht", "ihr", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "$,", "KON", "PPER", "PTKNEG", "PPER", "$,", "VVPP", "$,"], "meter": "--+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df Gast Er, und Sie sey Wirth, da\u00df auch seiner Wohlfahr Lauff", "tokens": ["Da\u00df", "Gast", "Er", ",", "und", "Sie", "sey", "Wirth", ",", "da\u00df", "auch", "sei\u00b7ner", "Wohl\u00b7fahr", "Lauff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "$,", "KON", "PPER", "VAFIN", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Hier im Thale neme Ruh, weiter aber geh Berg-auff.", "tokens": ["Hier", "im", "Tha\u00b7le", "ne\u00b7me", "Ruh", ",", "wei\u00b7ter", "a\u00b7ber", "geh", "Ber\u00b7gauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "NE", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "Sterben wird nun noch so lieb dem, der recht nur wil bedencken,", "tokens": ["Ster\u00b7ben", "wird", "nun", "noch", "so", "lieb", "dem", ",", "der", "recht", "nur", "wil", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "ART", "$,", "PRELS", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Wie der Wirth zum Schelmen wird und die G\u00e4ste pflegt zu kr\u00e4ncken,", "tokens": ["Wie", "der", "Wirth", "zum", "Schel\u00b7men", "wird", "und", "die", "G\u00e4s\u00b7te", "pflegt", "zu", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VAFIN", "KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Da\u00df er au\u00df dem Hause jagt den, der ihn nicht betet an;", "tokens": ["Da\u00df", "er", "au\u00df", "dem", "Hau\u00b7se", "jagt", "den", ",", "der", "ihn", "nicht", "be\u00b7tet", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "ART", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "Der vom from-seyn abzustehn \u00fcbers Hertz nicht bringen kan,", "tokens": ["Der", "vom", "from\u00b7seyn", "ab\u00b7zu\u00b7stehn", "\u00fc\u00b7bers", "Hertz", "nicht", "brin\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVIZU", "APPRART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Der noch glaubet, da\u00df ein Gott, der noch etwa dran gedachte,", "tokens": ["Der", "noch", "glau\u00b7bet", ",", "da\u00df", "ein", "Gott", ",", "der", "noch", "et\u00b7wa", "dran", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Was f\u00fcr Alters Tugend hie\u00df, der noch etwas wo verbrachte,", "tokens": ["Was", "f\u00fcr", "Al\u00b7ters", "Tu\u00b7gend", "hie\u00df", ",", "der", "noch", "et\u00b7was", "wo", "ver\u00b7brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "PWAV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Da\u00df nach Bieder-wesen reucht, der nicht Dienst wil nemen an,", "tokens": ["Da\u00df", "nach", "Bie\u00b7der\u00b7we\u00b7sen", "reucht", ",", "der", "nicht", "Dienst", "wil", "ne\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "NN", "VMFIN", "ADV", "PTKVZ", "$,"], "meter": "--+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wil nicht wider Recht und Zucht treten auff den Frevler-Plan:", "tokens": ["Wil", "nicht", "wi\u00b7der", "Recht", "und", "Zucht", "tre\u00b7ten", "auff", "den", "Frev\u00b7ler\u00b7Plan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Dieser, dieser hat verdient, da\u00df man ihn mit Hunden hetze", "tokens": ["Die\u00b7ser", ",", "die\u00b7ser", "hat", "ver\u00b7dient", ",", "da\u00df", "man", "ihn", "mit", "Hun\u00b7den", "het\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PDS", "VAFIN", "VVPP", "$,", "KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Zu dem grossen Thore zu, bi\u00df er Gut und Blut versetze!", "tokens": ["Zu", "dem", "gros\u00b7sen", "Tho\u00b7re", "zu", ",", "bi\u00df", "er", "Gut", "und", "Blut", "ver\u00b7set\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Drum, wann Gott die blaue Burg \u00f6ffnet und ihm beut die Hand,", "tokens": ["Drum", ",", "wann", "Gott", "die", "blau\u00b7e", "Burg", "\u00f6ff\u00b7net", "und", "ihm", "beut", "die", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "NN", "ART", "ADJA", "NN", "VVFIN", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Freyt ihn von der Trotzer Trotz, setzt ihn in den Friedens-stand,", "tokens": ["Freyt", "ihn", "von", "der", "Trot\u00b7zer", "Trotz", ",", "setzt", "ihn", "in", "den", "Frie\u00b7dens\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.17": {"text": "Rettet ihn au\u00df S\u00fcnd und Noth vom Verterben zum genesen,", "tokens": ["Ret\u00b7tet", "ihn", "au\u00df", "S\u00fcnd", "und", "Noth", "vom", "Ver\u00b7ter\u00b7ben", "zum", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "APPRART", "NN", "APPRART", "VVPP", "$,"], "meter": "+-+-+-+--+--+--", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Nimmt ihm die Verg\u00e4ngligkeit, schencket ihm ein ewig- Wesen:", "tokens": ["Nimmt", "ihm", "die", "Ver\u00b7g\u00e4n\u00b7glig\u00b7keit", ",", "schen\u00b7cket", "ihm", "ein", "e\u00b7wig", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "TRUNC", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Ey, wer w\u00e4r so unbedacht, da\u00df er diesen lasse nicht", "tokens": ["Ey", ",", "wer", "w\u00e4r", "so", "un\u00b7be\u00b7dacht", ",", "da\u00df", "er", "die\u00b7sen", "las\u00b7se", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PDS", "VVFIN", "PTKNEG"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Hin, wo dieser Welt ihr Grimm seine freche H\u00f6rner bricht!", "tokens": ["Hin", ",", "wo", "die\u00b7ser", "Welt", "ihr", "Grimm", "sei\u00b7ne", "fre\u00b7che", "H\u00f6r\u00b7ner", "bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PDAT", "NN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Allzuweit ist nicht von hier, bi\u00df der tolle Schanden-winckel,", "tokens": ["All\u00b7zu\u00b7weit", "ist", "nicht", "von", "hier", ",", "bi\u00df", "der", "tol\u00b7le", "Schan\u00b7den\u00b7win\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "APPR", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Drinnen blinder Willen herrscht und ein tauber Eigen-d\u00fcnckel,", "tokens": ["Drin\u00b7nen", "blin\u00b7der", "Wil\u00b7len", "herrscht", "und", "ein", "tau\u00b7ber", "Ei\u00b7gen\u00b7d\u00fcn\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "F\u00fchlt den letzten Donnerschlag, der ihn schl\u00e4gt in einen Klo\u00df,", "tokens": ["F\u00fchlt", "den", "letz\u00b7ten", "Don\u00b7ner\u00b7schlag", ",", "der", "ihn", "schl\u00e4gt", "in", "ei\u00b7nen", "Klo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Dr\u00fcckt zu Grunde den, der dr\u00fcckt, machet die gedr\u00fcckten los.", "tokens": ["Dr\u00fcckt", "zu", "Grun\u00b7de", "den", ",", "der", "dr\u00fcckt", ",", "ma\u00b7chet", "die", "ge\u00b7dr\u00fcck\u00b7ten", "los", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Wol indessen dem, der dort lacht und schaut die Emsen-Hauffen,", "tokens": ["Wol", "in\u00b7des\u00b7sen", "dem", ",", "der", "dort", "lacht", "und", "schaut", "die", "Em\u00b7sen\u00b7Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Drinnen um das eitle nichts krichen, steigen, dringen, lauffen", "tokens": ["Drin\u00b7nen", "um", "das", "eit\u00b7le", "nichts", "kri\u00b7chen", ",", "stei\u00b7gen", ",", "drin\u00b7gen", ",", "lauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "PIS", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Unbedachte Menschen-Schw\u00e4rme! wol auch dem, der, was ihm lieb,", "tokens": ["Un\u00b7be\u00b7dach\u00b7te", "Men\u00b7schen\u00b7Schw\u00e4r\u00b7me", "!", "wol", "auch", "dem", ",", "der", ",", "was", "ihm", "lieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "ART", "$,", "PRELS", "$,", "PWS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Da hat, wo f\u00fcr Bosheit, Noth, Drang und Zwang es sicher blieb!", "tokens": ["Da", "hat", ",", "wo", "f\u00fcr", "Bos\u00b7heit", ",", "Noth", ",", "Drang", "und", "Zwang", "es", "si\u00b7cher", "blieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}