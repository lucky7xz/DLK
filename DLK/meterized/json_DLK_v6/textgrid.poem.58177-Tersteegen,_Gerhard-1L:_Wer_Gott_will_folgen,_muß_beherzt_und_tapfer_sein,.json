{"textgrid.poem.58177": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer Gott will folgen, mu\u00df beherzt und tapfer sein,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott will folgen, mu\u00df beherzt und tapfer sein,", "tokens": ["Wer", "Gott", "will", "fol\u00b7gen", ",", "mu\u00df", "be\u00b7herzt", "und", "tap\u00b7fer", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "$,", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht immerdar in Furcht, in Sorg' und Klagen leben;", "tokens": ["Nicht", "im\u00b7mer\u00b7dar", "in", "Furcht", ",", "in", "Sor\u00b7g'", "und", "Kla\u00b7gen", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Ergib dich nur getrost und achte keine Pein,", "tokens": ["Er\u00b7gib", "dich", "nur", "ge\u00b7trost", "und", "ach\u00b7te", "kei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVPP", "KON", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Glaube wird dir Licht, die Liebe Kr\u00e4fte geben!", "tokens": ["Der", "Glau\u00b7be", "wird", "dir", "Licht", ",", "die", "Lie\u00b7be", "Kr\u00e4f\u00b7te", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}