{"textgrid.poem.39333": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "85. Christus mu\u00df wachsen; ich aber mu\u00df abnehmen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Jesus Christus wachs' in mir!", "tokens": ["O", "Je\u00b7sus", "Chris\u00b7tus", "wachs'", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles andre schwinde!", "tokens": ["Und", "al\u00b7les", "and\u00b7re", "schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz sey t\u00e4glich n\u00e4her Dir", "tokens": ["Mein", "Herz", "sey", "t\u00e4g\u00b7lich", "n\u00e4\u00b7her", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ferner von der S\u00fcnde!", "tokens": ["Und", "fer\u00b7ner", "von", "der", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df t\u00e4glich deine Huld und Macht", "tokens": ["La\u00df", "t\u00e4g\u00b7lich", "dei\u00b7ne", "Huld", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um meine Schwachheit schweben.", "tokens": ["Um", "mei\u00b7ne", "Schwach\u00b7heit", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Licht verschlinge meine Nacht,", "tokens": ["Dein", "Licht", "ver\u00b7schlin\u00b7ge", "mei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meinen Tod dein Leben!", "tokens": ["Und", "mei\u00b7nen", "Tod", "dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Beym Sonnenstrale deines Lichts", "tokens": ["Beym", "Son\u00b7nen\u00b7stra\u00b7le", "dei\u00b7nes", "Lichts"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df jeden Wahn verschwinden,", "tokens": ["La\u00df", "je\u00b7den", "Wahn", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dein Alles, Christus, und mein Nichts", "tokens": ["Dein", "Al\u00b7les", ",", "Chris\u00b7tus", ",", "und", "mein", "Nichts"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "PIS", "$,", "NE", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df t\u00e4glich mich empfinden!", "tokens": ["La\u00df", "t\u00e4g\u00b7lich", "mich", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sey nahe mir, werf' ich mich hin,", "tokens": ["Sey", "na\u00b7he", "mir", ",", "wer\u00b7f'", "ich", "mich", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "PWS", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weyn' ich vor Dir im Stillen!", "tokens": ["Weyn'", "ich", "vor", "Dir", "im", "Stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein reiner gottgela\u00dfner Sinn", "tokens": ["Dein", "rei\u00b7ner", "gott\u00b7ge\u00b7la\u00df\u00b7ner", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beherrsche meinen Willen!", "tokens": ["Be\u00b7herr\u00b7sche", "mei\u00b7nen", "Wil\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Blick immer herrlicher aus mir,", "tokens": ["Blick", "im\u00b7mer", "herr\u00b7li\u00b7cher", "aus", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Voll Weisheit, Huld und Freude!", "tokens": ["Voll", "Weis\u00b7heit", ",", "Huld", "und", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sey ein lebend Bild von Dir,", "tokens": ["Ich", "sey", "ein", "le\u00b7bend", "Bild", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gl\u00fcck, und wenn ich leide!", "tokens": ["Im", "Gl\u00fcck", ",", "und", "wenn", "ich", "lei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mach alles in mir froh und gut,", "tokens": ["Mach", "al\u00b7les", "in", "mir", "froh", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df stets ich minder fehle,", "tokens": ["Da\u00df", "stets", "ich", "min\u00b7der", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, deiner Menschenliebe Glut", "tokens": ["Herr", ",", "dei\u00b7ner", "Men\u00b7schen\u00b7lie\u00b7be", "Glut"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Durchgl\u00fche meine Seele!", "tokens": ["Durch\u00b7gl\u00fc\u00b7he", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es weiche Stolz, und Tr\u00e4gheit weich',", "tokens": ["Es", "wei\u00b7che", "Stolz", ",", "und", "Tr\u00e4g\u00b7heit", "weich'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder Leichtsinn fliehe,", "tokens": ["Und", "je\u00b7der", "Leicht\u00b7sinn", "flie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn, Herr, nach dir und deinem Reich", "tokens": ["Wenn", ",", "Herr", ",", "nach", "dir", "und", "dei\u00b7nem", "Reich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "NN", "$,", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich redlich mich bem\u00fche.", "tokens": ["Ich", "red\u00b7lich", "mich", "be\u00b7m\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mein eignes eitles leeres Ich", "tokens": ["Mein", "eig\u00b7nes", "eit\u00b7les", "lee\u00b7res", "Ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "ADJA", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey jeden Tag geringer!", "tokens": ["Sey", "je\u00b7den", "Tag", "ge\u00b7rin\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O w\u00fcrd' ich jeden Tag durch dich", "tokens": ["O", "w\u00fcrd'", "ich", "je\u00b7den", "Tag", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein w\u00fcrdigerer J\u00fcnger!", "tokens": ["Dein", "w\u00fcr\u00b7di\u00b7ge\u00b7rer", "J\u00fcn\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Von Dir erf\u00fcllter jeden Tag!", "tokens": ["Von", "Dir", "er\u00b7f\u00fcll\u00b7ter", "je\u00b7den", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeden von mir leerer!", "tokens": ["Und", "je\u00b7den", "von", "mir", "lee\u00b7rer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O du, der \u00fcber Flehn vermag,", "tokens": ["O", "du", ",", "der", "\u00fc\u00b7ber", "Flehn", "ver\u00b7mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey meines Flehns Erh\u00f6rer!", "tokens": ["Sey", "mei\u00b7nes", "Flehns", "Er\u00b7h\u00f6\u00b7rer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Glaub an dich, und deine Kraft", "tokens": ["Der", "Glaub", "an", "dich", ",", "und", "dei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey Trieb von jedem Triebe!", "tokens": ["Sey", "Trieb", "von", "je\u00b7dem", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sey du nur meine Leidenschaft!", "tokens": ["Sey", "du", "nur", "mei\u00b7ne", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du meine Freud und Liebe!", "tokens": ["Du", "mei\u00b7ne", "Freud", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}