{"textgrid.poem.41322": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Hyperbolus", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst uns g\u00fcldne Berge zu,", "tokens": ["Du", "sagst", "uns", "g\u00fcld\u00b7ne", "Ber\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leistest nichts, und darfst dies Geben nennen:", "tokens": ["Und", "leis\u00b7test", "nichts", ",", "und", "darfst", "dies", "Ge\u00b7ben", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KON", "VVFIN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-----", "measure": "unknown.measure.tri"}, "line.3": {"text": "So wirst du heute mir verg\u00f6nnen,", "tokens": ["So", "wirst", "du", "heu\u00b7te", "mir", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freigebiger zu sein, als du.", "tokens": ["Frei\u00b7ge\u00b7bi\u00b7ger", "zu", "sein", ",", "als", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "KOUS", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ich schenke dir, so mancher Wahrheit wegen;", "tokens": ["Ich", "schen\u00b7ke", "dir", ",", "so", "man\u00b7cher", "Wahr\u00b7heit", "we\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich schenke dir, Hyperbolus:", "tokens": ["Ich", "schen\u00b7ke", "dir", ",", "Hy\u00b7per\u00b7bo\u00b7lus", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In deinen B\u00fccherschatz den ganzen Livius;", "tokens": ["In", "dei\u00b7nen", "B\u00fc\u00b7cher\u00b7schatz", "den", "gan\u00b7zen", "Li\u00b7vius", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "In deinen Waffensaal des gro\u00dfen Rolands Degen;", "tokens": ["In", "dei\u00b7nen", "Waf\u00b7fen\u00b7saal", "des", "gro\u00b7\u00dfen", "Ro\u00b7lands", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zehn St\u00fcck, ins Cabinet, von Rubens freier Hand;", "tokens": ["Zehn", "St\u00fcck", ",", "ins", "Ca\u00b7bi\u00b7net", ",", "von", "Ru\u00b7bens", "frei\u00b7er", "Hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "APPRART", "NE", "$,", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein \u00e4chtes Ph\u00f6nixnest, die Beute ferner Reisen:", "tokens": ["Ein", "\u00e4ch\u00b7tes", "Ph\u00f6\u00b7nix\u00b7nest", ",", "die", "Beu\u00b7te", "fer\u00b7ner", "Rei\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00fcr dein Gemahl Pitts gro\u00dfen Diamant;", "tokens": ["F\u00fcr", "dein", "Ge\u00b7mahl", "Pitts", "gro\u00b7\u00dfen", "Di\u00b7a\u00b7mant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr deinen ersten Sohn den Wasserstein der Weisen;", "tokens": ["F\u00fcr", "dei\u00b7nen", "ers\u00b7ten", "Sohn", "den", "Was\u00b7ser\u00b7stein", "der", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und alles, was du sonst, dich zu bereichern, liebst:", "tokens": ["Und", "al\u00b7les", ",", "was", "du", "sonst", ",", "dich", "zu", "be\u00b7rei\u00b7chern", ",", "liebst", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Herr, das empfange, wie du gibst.", "tokens": ["Herr", ",", "das", "emp\u00b7fan\u00b7ge", ",", "wie", "du", "gibst", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}