{"dta.poem.3352": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Kann ein verblendet Volk die Thorheit h\u00f6her treiben?", "tokens": ["Kann", "ein", "ver\u00b7blen\u00b7det", "Volk", "die", "Thor\u00b7heit", "h\u00f6\u00b7her", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "VVPP", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nicht, wie Britten, denkt, will, als ein Brit-", "tokens": ["Der", "nicht", ",", "wie", "Brit\u00b7ten", ",", "denkt", ",", "will", ",", "als", "ein", "Brit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKNEG", "$,", "PWAV", "NN", "$,", "VVFIN", "$,", "VMFIN", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "te, schreiben!", "tokens": ["te", ",", "schrei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Deutsche will ein Britte seyn,", "tokens": ["Der", "Deut\u00b7sche", "will", "ein", "Brit\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kauft ein englisch Kleid auf einem Tr\u00f6del ein.", "tokens": ["Und", "kauft", "ein", "eng\u00b7lisch", "Kleid", "auf", "ei\u00b7nem", "Tr\u00f6\u00b7del", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Aufwand ist gering: ein schw\u00fclstiges Geschw\u00e4tze,", "tokens": ["Der", "Auf\u00b7wand", "ist", "ge\u00b7ring", ":", "ein", "schw\u00fcls\u00b7ti\u00b7ges", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das der Vernunft vergi\u00dft, wie aller Sprachgesetze,", "tokens": ["Das", "der", "Ver\u00b7nunft", "ver\u00b7gi\u00dft", ",", "wie", "al\u00b7ler", "Sprach\u00b7ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Manch Schulwort, manch verwegner Schwung", "tokens": ["Manch", "Schul\u00b7wort", ",", "manch", "ver\u00b7weg\u00b7ner", "Schwung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schw\u00e4rmende Begeisterung", "tokens": ["Und", "schw\u00e4r\u00b7men\u00b7de", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Macht schon ein ziemlich Kleid nach Londons neustem", "tokens": ["Macht", "schon", "ein", "ziem\u00b7lich", "Kleid", "nach", "Lon\u00b7dons", "neus\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADV", "NN", "APPR", "NE", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schnitte:", "tokens": ["Schnit\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Dem Kleide fehlt nur eins! der Britte.", "tokens": ["Dem", "Klei\u00b7de", "fehlt", "nur", "eins", "!", "der", "Brit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIS", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Was hilft ein fremder Schmuck, der, im Gebrauch be-", "tokens": ["Was", "hilft", "ein", "frem\u00b7der", "Schmuck", ",", "der", ",", "im", "Ge\u00b7brauch", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "fleckt,", "tokens": ["fleckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Nur klappernde Gerippe deckt,", "tokens": ["Nur", "klap\u00b7pern\u00b7de", "Ge\u00b7rip\u00b7pe", "deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.16": {"text": "Die nach des Grabes Moder riechen?", "tokens": ["Die", "nach", "des", "Gra\u00b7bes", "Mo\u00b7der", "rie\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wie oft verbirgt in wilder Pracht", "tokens": ["Wie", "oft", "ver\u00b7birgt", "in", "wil\u00b7der", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des Ausdrucks unerhellte Nacht", "tokens": ["Des", "Aus\u00b7drucks", "un\u00b7er\u00b7hell\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gedanken, die im Staube kriechen!", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "die", "im", "Stau\u00b7be", "krie\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die deutsche Dichtkunst weicht von weisrer Alten", "tokens": ["Die", "deut\u00b7sche", "Dicht\u00b7kunst", "weicht", "von", "weis\u00b7rer", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Spur:", "tokens": ["Spur", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Der gr\u00fcndliche Geschmack an Wahrheit und Natur,", "tokens": ["Der", "gr\u00fcnd\u00b7li\u00b7che", "Ge\u00b7schmack", "an", "Wahr\u00b7heit", "und", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Wohlklang in gesunden Ohren,", "tokens": ["Der", "Wohl\u00b7klang", "in", "ge\u00b7sun\u00b7den", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die Sprache selber geht verlohren,", "tokens": ["Die", "Spra\u00b7che", "sel\u00b7ber", "geht", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da alle Scham verlohren geht:", "tokens": ["Da", "al\u00b7le", "Scham", "ver\u00b7loh\u00b7ren", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nous sommes cinq ou six Novateurs hardis qui\navons entrepris de changer la langue du blanc au\nnoir. Et nous en viendrons a bout, s'il plait a\nDieu, en depit de Lope de Vega, de Cervantes &amp;\nde tous les autres beaux esprits qui nous chicannent\nsur nos nouvelles fa&#231;ons de parler.\nAvantures de Gil Blas L. VII. c. 13. Ein Deutscher ist gelehrt, wenn er solch", "tokens": ["Nous", "som\u00b7mes", "cinq", "ou", "six", "No\u00b7va\u00b7teurs", "har\u00b7dis", "qui", "a\u00b7vons", "ent\u00b7re\u00b7pris", "de", "chan\u00b7ger", "la", "lan\u00b7gue", "du", "blanc", "au", "noir", ".", "Et", "nous", "en", "viend\u00b7rons", "a", "bout", ",", "s'il", "plait", "a", "Di\u00b7eu", ",", "en", "de\u00b7pit", "de", "Lo\u00b7pe", "de", "Ve\u00b7ga", ",", "de", "Cer\u00b7van\u00b7tes", "&amp;", "de", "tous", "les", "aut\u00b7res", "be\u00b7aux", "es\u00b7prits", "qui", "nous", "chi\u00b7can\u00b7nent", "sur", "nos", "nou\u00b7vel\u00b7les", "fa", "&#231;", "ons", "de", "par\u00b7ler", ".", "A\u00b7van\u00b7tu\u00b7res", "de", "Gil", "Blas", "L.", "ViI", ".", "c.", "13.", "Ein", "Deut\u00b7scher", "ist", "ge\u00b7lehrt", ",", "wenn", "er", "solch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation", "word", "punct", "abbreviation", "ordinal", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$.", "FM", "FM", "FM", "FM", "FM", "FM", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "NE", "ADJA", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PIAT"], "meter": "-+-+--+--+---+-+--+-+-+-+-+-+-+--+-+-+-+-+-+---+-+-+-+-+--+-+-+-+-+--+-+--+-+-+--+-+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.27": {"text": "Deutsch verstehr.", "tokens": ["Deutsch", "ver\u00b7stehr", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}