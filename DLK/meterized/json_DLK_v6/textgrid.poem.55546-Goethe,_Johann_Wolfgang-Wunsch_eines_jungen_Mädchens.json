{"textgrid.poem.55546": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch eines jungen M\u00e4dchens", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O f\u00e4nde f\u00fcr mich", "tokens": ["O", "f\u00e4n\u00b7de", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein Br\u00e4utigam sich!", "tokens": ["Ein", "Br\u00e4u\u00b7ti\u00b7gam", "sich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "PRF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie sch\u00f6n ist's nicht da,", "tokens": ["Wie", "sch\u00f6n", "ist's", "nicht", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Man nennt uns Mama.", "tokens": ["Man", "nennt", "uns", "Ma\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da braucht man zum N\u00e4hen,", "tokens": ["Da", "braucht", "man", "zum", "N\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zur Schul nicht zu gehen;", "tokens": ["Zur", "Schul", "nicht", "zu", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Da kann man befehlen,", "tokens": ["Da", "kann", "man", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Hat M\u00e4gde, darf schm\u00e4len,", "tokens": ["Hat", "M\u00e4g\u00b7de", ",", "darf", "schm\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Man w\u00e4hlt sich die Kleider,", "tokens": ["Man", "w\u00e4hlt", "sich", "die", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Nach Gusto den Schneider.", "tokens": ["Nach", "Gus\u00b7to", "den", "Schnei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Da l\u00e4\u00dft man spazieren", "tokens": ["Da", "l\u00e4\u00dft", "man", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Auf B\u00e4lle sich f\u00fchren,", "tokens": ["Auf", "B\u00e4l\u00b7le", "sich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und fragt nicht erst lange", "tokens": ["Und", "fragt", "nicht", "erst", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Papa und Mama.", "tokens": ["Pa\u00b7pa", "und", "Ma\u00b7ma", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}