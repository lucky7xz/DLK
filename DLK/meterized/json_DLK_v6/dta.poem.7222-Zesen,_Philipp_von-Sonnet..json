{"dta.poem.7222": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun bin ich vogel-frey/ der s\u00fc\u00dfen angst ent-", "tokens": ["Nun", "bin", "ich", "vo\u00b7gel\u00b7frey", "/", "der", "s\u00fc\u00b7\u00dfen", "angst", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$(", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kommen/", "tokens": ["kom\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich lebe nun nicht mehr in schn\u00f6der liebes-hafft/", "tokens": ["Ich", "le\u00b7be", "nun", "nicht", "mehr", "in", "schn\u00f6\u00b7der", "lie\u00b7bes\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hab\u2019 all\u2019 Eitelkeit vnd Hochmuth abgeschafft/", "tokens": ["Ich", "hab'", "all'", "Ei\u00b7tel\u00b7keit", "vnd", "Hoch\u00b7muth", "ab\u00b7ge\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df nicht was die lieb\u2019 vnd b\u00f6se lust soll", "tokens": ["Ich", "wei\u00df", "nicht", "was", "die", "lieb'", "vnd", "b\u00f6\u00b7se", "lust", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ART", "NN", "KON", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "frommen?", "tokens": ["from\u00b7men", "?"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Sie macht beredt vnd lest doch manchen gar ver-", "tokens": ["Sie", "macht", "be\u00b7redt", "vnd", "lest", "doch", "man\u00b7chen", "gar", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVIMP", "ADV", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "stummen/", "tokens": ["stum\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sie ist ein fre\u00fcdenkrieg/ ein gallens\u00fc\u00dfer-safft/", "tokens": ["Sie", "ist", "ein", "fre\u00fc\u00b7den\u00b7krieg", "/", "ein", "gal\u00b7len\u00b7s\u00fc\u00b7\u00dfe\u00b7r\u00b7safft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ein angenehme gifft vnd halbe todeskrafft;", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7me", "gifft", "vnd", "hal\u00b7be", "to\u00b7des\u00b7krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Fe\u00fcer ohne gluth dabey mann mus ver-", "tokens": ["Ein", "Fe\u00b7\u00fcer", "oh\u00b7ne", "gluth", "da\u00b7bey", "mann", "mus", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PAV", "PIS", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "klummen/", "tokens": ["klum\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Ein joch doch nicht ein joch/ ein selbst begehrter", "tokens": ["Ein", "joch", "doch", "nicht", "ein", "joch", "/", "ein", "selbst", "be\u00b7gehr\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PTKNEG", "ART", "NN", "$(", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "todt/", "tokens": ["todt", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ein vnbehertztes hertz vnd angenehme noth:", "tokens": ["Ein", "vn\u00b7be\u00b7hertz\u00b7tes", "hertz", "vnd", "an\u00b7ge\u00b7neh\u00b7me", "noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dr\u00fcmb bin ich hertzlich froh/ da\u00df ich dasselbe meiden", "tokens": ["Dr\u00fcmb", "bin", "ich", "hertz\u00b7lich", "froh", "/", "da\u00df", "ich", "das\u00b7sel\u00b7be", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "$(", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vnd willig la\u00dfen kann/ wie froh! wie froh bin ich!", "tokens": ["Vnd", "wil\u00b7lig", "la\u00b7\u00dfen", "kann", "/", "wie", "froh", "!", "wie", "froh", "bin", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$(", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich der Geistligkeit kann vnterwerffen mich/", "tokens": ["Da\u00df", "ich", "der", "Geist\u00b7lig\u00b7keit", "kann", "vn\u00b7ter\u00b7werf\u00b7fen", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun liebe/ wer da will/ ich will die Liebe meiden!", "tokens": ["Nun", "lie\u00b7be", "/", "wer", "da", "will", "/", "ich", "will", "die", "Lie\u00b7be", "mei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWS", "ADV", "VMFIN", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}