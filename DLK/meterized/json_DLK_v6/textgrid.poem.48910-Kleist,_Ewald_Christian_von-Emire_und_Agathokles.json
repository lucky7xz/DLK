{"textgrid.poem.48910": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Emire und Agathokles", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Emire fing ihr Leben an zu ha\u00dfen", "tokens": ["E\u00b7mi\u00b7re", "fing", "ihr", "Le\u00b7ben", "an", "zu", "ha\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APZR", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ihr Agathokles leichtsinnig sie verlassen.", "tokens": ["Als", "ihr", "A\u00b7gat\u00b7ho\u00b7kles", "leicht\u00b7sin\u00b7nig", "sie", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie floh die gro\u00dfe Welt, die vormals sie verehrt,", "tokens": ["Sie", "floh", "die", "gro\u00b7\u00dfe", "Welt", ",", "die", "vor\u00b7mals", "sie", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie floh die Freundschafft selbst, allein in sich gekehrt;", "tokens": ["Sie", "floh", "die", "Freund\u00b7schafft", "selbst", ",", "al\u00b7lein", "in", "sich", "ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Welt schien ihr nicht mehr ein Sitz voll Lust und Wonne,", "tokens": ["Die", "Welt", "schien", "ihr", "nicht", "mehr", "ein", "Sitz", "voll", "Lust", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Flur nicht blumenreich und minder hell die Sonne.", "tokens": ["Die", "Flur", "nicht", "blu\u00b7men\u00b7reich", "und", "min\u00b7der", "hell", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Lustschlo\u00df, in der Nacht von einem dicken Wald,", "tokens": ["Ein", "Lust\u00b7schlo\u00df", ",", "in", "der", "Nacht", "von", "ei\u00b7nem", "di\u00b7cken", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "War ihre Zuflucht iezt und liebster Aufenthalt.", "tokens": ["War", "ih\u00b7re", "Zu\u00b7flucht", "iezt", "und", "liebs\u00b7ter", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie ging oft in des Hayns Gew\u00f6lben, lebensm\u00fcde,", "tokens": ["Sie", "ging", "oft", "in", "des", "Hayns", "Ge\u00b7w\u00f6l\u00b7ben", ",", "le\u00b7bens\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht mehr gereitzt, wie sonst, von Philomelens Liede,", "tokens": ["Nicht", "mehr", "ge\u00b7reitzt", ",", "wie", "sonst", ",", "von", "Phi\u00b7lo\u00b7me\u00b7lens", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$,", "PWAV", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch von der Quelle, die durch Blumen flo\u00df. Nicht seyn,", "tokens": ["Noch", "von", "der", "Quel\u00b7le", ",", "die", "durch", "Blu\u00b7men", "flo\u00df", ".", "Nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$.", "PTKNEG", "VAINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "D\u00fcnkt ihr das gr\u00f6\u00dfte Gl\u00fcck, und war ihr Wunsch allein.", "tokens": ["D\u00fcnkt", "ihr", "das", "gr\u00f6\u00df\u00b7te", "Gl\u00fcck", ",", "und", "war", "ihr", "Wunsch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mu\u00dft ich, so dacht sie oft, Agathokles nur lieben", "tokens": ["Mu\u00dft", "ich", ",", "so", "dacht", "sie", "oft", ",", "A\u00b7gat\u00b7ho\u00b7kles", "nur", "lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "NE", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn ewig iezt zu scheun, mich ewig zu betr\u00fcben?", "tokens": ["Ihn", "e\u00b7wig", "iezt", "zu", "scheun", ",", "mich", "e\u00b7wig", "zu", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich glaubt ihn so getreu als liebenswerth. Sein Schmerz", "tokens": ["Ich", "glaubt", "ihn", "so", "ge\u00b7treu", "als", "lie\u00b7bens\u00b7werth", ".", "Sein", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seine Thr\u00e4nen nur erwarben ihm mein Herz;", "tokens": ["Und", "sei\u00b7ne", "Thr\u00e4\u00b7nen", "nur", "er\u00b7war\u00b7ben", "ihm", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht Leichtsinn! Laster nicht! Ich liebte seine Tugend", "tokens": ["Nicht", "Leicht\u00b7sinn", "!", "Las\u00b7ter", "nicht", "!", "Ich", "lieb\u00b7te", "sei\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$.", "NN", "PTKNEG", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seine Seele mehr, als allen Reitz der Jugend.", "tokens": ["Und", "sei\u00b7ne", "See\u00b7le", "mehr", ",", "als", "al\u00b7len", "Reitz", "der", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch alles was er sprach, Versicherung und Schwur,", "tokens": ["Doch", "al\u00b7les", "was", "er", "sprach", ",", "Ver\u00b7si\u00b7che\u00b7rung", "und", "Schwur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kam aus dem Herzen nicht, kam von den Lippen nur.", "tokens": ["Kam", "aus", "dem", "Her\u00b7zen", "nicht", ",", "kam", "von", "den", "Lip\u00b7pen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "$,", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Untreuer! Ich bin zwar der Raub von deinen L\u00fcgen,", "tokens": ["Un\u00b7treu\u00b7er", "!", "Ich", "bin", "zwar", "der", "Raub", "von", "dei\u00b7nen", "L\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allein wirst du, wie mich, den Himmel auch betr\u00fcgen?", "tokens": ["Al\u00b7lein", "wirst", "du", ",", "wie", "mich", ",", "den", "Him\u00b7mel", "auch", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Furcht ihn! er straffet noch! Vielleicht f\u00fchlst du einmal,", "tokens": ["Furcht", "ihn", "!", "er", "straf\u00b7fet", "noch", "!", "Viel\u00b7leicht", "f\u00fchlst", "du", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn dein Gewissen wacht, gedoppelt meine Quaal \u2013", "tokens": ["Wenn", "dein", "Ge\u00b7wis\u00b7sen", "wacht", ",", "ge\u00b7dop\u00b7pelt", "mei\u00b7ne", "Qua\u00b7al", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch dieses w\u00fcnsch ich nicht. Du sollst den Schmerz nicht n\u00e4hren,", "tokens": ["Doch", "die\u00b7ses", "w\u00fcnsch", "ich", "nicht", ".", "Du", "sollst", "den", "Schmerz", "nicht", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nur such einmal mein Grab und schenk ihm einge Z\u00e4hren,", "tokens": ["Nur", "such", "ein\u00b7mal", "mein", "Grab", "und", "schenk", "ihm", "ein\u00b7ge", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und denk: Hier ruhet die, die sich um mich betr\u00fcbt;", "tokens": ["Und", "denk", ":", "Hier", "ru\u00b7het", "die", ",", "die", "sich", "um", "mich", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "ART", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Treue lebte noch, h\u00e4tt' sie mich nicht geliebt. \u2013", "tokens": ["Die", "Treu\u00b7e", "leb\u00b7te", "noch", ",", "h\u00e4tt'", "sie", "mich", "nicht", "ge\u00b7liebt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So bracht Emire hier ihr Leben lange zu;", "tokens": ["So", "bracht", "E\u00b7mi\u00b7re", "hier", "ihr", "Le\u00b7ben", "lan\u00b7ge", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr stiller Gram schien falsch, Gela\u00dfenheit und Ruh.", "tokens": ["Ihr", "stil\u00b7ler", "Gram", "schien", "falsch", ",", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gesucht von Ehr und Gunst der Gro\u00dfen, hatt' inde\u00dfen,", "tokens": ["Ge\u00b7sucht", "von", "Ehr", "und", "Gunst", "der", "Gro\u00b7\u00dfen", ",", "hatt'", "in\u00b7de\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ART", "NN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An fernen H\u00f6fen, sie Agathokles verge\u00dfen.", "tokens": ["An", "fer\u00b7nen", "H\u00f6\u00b7fen", ",", "sie", "A\u00b7gat\u00b7ho\u00b7kles", "ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch endlich \u00fcberfiel ihn unverhofte Reu;", "tokens": ["Doch", "end\u00b7lich", "\u00fc\u00b7berf\u00b7iel", "ihn", "un\u00b7ver\u00b7hof\u00b7te", "Reu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein wankelm\u00fcthig Herz f\u00fchlt alte Lieb und Treu;", "tokens": ["Sein", "wan\u00b7kel\u00b7m\u00fct\u00b7hig", "Herz", "f\u00fchlt", "al\u00b7te", "Lieb", "und", "Treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er kehrte schnell zur\u00fcck \u2013 Er floh nach ihrer Wohnung,", "tokens": ["Er", "kehr\u00b7te", "schnell", "zu\u00b7r\u00fcck", "\u2013", "Er", "floh", "nach", "ih\u00b7rer", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Befl\u00fcgelt von der Lieb und Hoffnung der Belohnung.", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "von", "der", "Lieb", "und", "Hoff\u00b7nung", "der", "Be\u00b7loh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er sahe sie, und nahm die sch\u00f6ne Hand \u2013 Doch wie", "tokens": ["Er", "sa\u00b7he", "sie", ",", "und", "nahm", "die", "sch\u00f6\u00b7ne", "Hand", "\u2013", "Doch", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$(", "KON", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erschrack er! \u2013 wie ger\u00fchrt vom Wetterstrale \u2013 Sie", "tokens": ["Er\u00b7schrack", "er", "!", "\u2013", "wie", "ge\u00b7r\u00fchrt", "vom", "Wet\u00b7ter\u00b7stra\u00b7le", "\u2013", "Sie"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "PWAV", "VVPP", "APPRART", "NN", "$(", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "War starr \u2013 Verzeuch, rief er, nur einge Augenblicke!", "tokens": ["War", "starr", "\u2013", "Ver\u00b7zeuch", ",", "rief", "er", ",", "nur", "ein\u00b7ge", "Au\u00b7gen\u00b7bli\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Emire, h\u00f6re mich, und ruf den Geist zur\u00fccke!", "tokens": ["E\u00b7mi\u00b7re", ",", "h\u00f6\u00b7re", "mich", ",", "und", "ruf", "den", "Geist", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "KON", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verzeuch! Dich und mein Gl\u00fcck hab ich nicht halb gekannt,", "tokens": ["Ver\u00b7zeuch", "!", "Dich", "und", "mein", "Gl\u00fcck", "hab", "ich", "nicht", "halb", "ge\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht Untreu, Irrthum nur, hat mich von dir verbannt.", "tokens": ["Nicht", "Un\u00b7treu", ",", "Irr\u00b7thum", "nur", ",", "hat", "mich", "von", "dir", "ver\u00b7bannt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "NN", "ADV", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Herz h\u00e4tt' alles Gold der Welt, Gl\u00fcck, Ehr und Leben,", "tokens": ["Mein", "Herz", "h\u00e4tt'", "al\u00b7les", "Gold", "der", "Welt", ",", "Gl\u00fcck", ",", "Ehr", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als klein, f\u00fcr dem Besitz von dir, dahin gegeben.", "tokens": ["Als", "klein", ",", "f\u00fcr", "dem", "Be\u00b7sitz", "von", "dir", ",", "da\u00b7hin", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "APPR", "ART", "NN", "APPR", "PPER", "$,", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O sch\u00f6ne Unschuld! sieh mich nur noch einmal an,", "tokens": ["O", "sch\u00f6\u00b7ne", "Un\u00b7schuld", "!", "sieh", "mich", "nur", "noch", "ein\u00b7mal", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVIMP", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und sage mir, da\u00df mich dein Herz nicht hassen kan! \u2013", "tokens": ["Und", "sa\u00b7ge", "mir", ",", "da\u00df", "mich", "dein", "Herz", "nicht", "has\u00b7sen", "kan", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie hatte schon den Geist dem Himmel zugeschickt,", "tokens": ["Sie", "hat\u00b7te", "schon", "den", "Geist", "dem", "Him\u00b7mel", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfing der Treue Lohn, und war bereits begl\u00fcckt.", "tokens": ["Emp\u00b7fing", "der", "Treu\u00b7e", "Lohn", ",", "und", "war", "be\u00b7reits", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er fiel erstarrt dahin, f\u00fcr Schrecken und f\u00fcr Leide. \u2013", "tokens": ["Er", "fiel", "er\u00b7starrt", "da\u00b7hin", ",", "f\u00fcr", "Schre\u00b7cken", "und", "f\u00fcr", "Lei\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$,", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Leben kam zur\u00fcck, doch ohne Ruh und Freude,", "tokens": ["Das", "Le\u00b7ben", "kam", "zu\u00b7r\u00fcck", ",", "doch", "oh\u00b7ne", "Ruh", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und seine Klagen hat die Gegend lang geh\u00f6rt.", "tokens": ["Und", "sei\u00b7ne", "Kla\u00b7gen", "hat", "die", "Ge\u00b7gend", "lang", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch alles was er sah, ward seine Pein gemehrt.", "tokens": ["Durch", "al\u00b7les", "was", "er", "sah", ",", "ward", "sei\u00b7ne", "Pein", "ge\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Stellen wo sie ging und schlief, wo sie gese\u00dfen,", "tokens": ["Die", "Stel\u00b7len", "wo", "sie", "ging", "und", "schlief", ",", "wo", "sie", "ge\u00b7se\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wo sie starb, konnt er nicht sehn, und nicht verge\u00dfen.", "tokens": ["Und", "wo", "sie", "starb", ",", "konnt", "er", "nicht", "sehn", ",", "und", "nicht", "ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr Schlo\u00df, sonst seine Lust, in Bl\u00fcthen ganz versteckt,", "tokens": ["Ihr", "Schlo\u00df", ",", "sonst", "sei\u00b7ne", "Lust", ",", "in", "Bl\u00fc\u00b7then", "ganz", "ver\u00b7steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "D\u00fcnkt ihm anjezo schwarz, er ward dadurch erschreckt.", "tokens": ["D\u00fcnkt", "ihm", "an\u00b7je\u00b7zo", "schwarz", ",", "er", "ward", "da\u00b7durch", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Tod schien ihm ein Gl\u00fcck, das Leben eine Strafe,", "tokens": ["Der", "Tod", "schien", "ihm", "ein", "Gl\u00fcck", ",", "das", "Le\u00b7ben", "ei\u00b7ne", "Stra\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Schwermuth foltert ihn sogar im kurzen Schlafe.", "tokens": ["Und", "Schwer\u00b7muth", "fol\u00b7tert", "ihn", "so\u00b7gar", "im", "kur\u00b7zen", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bis sein bekriegter F\u00fcrst zum Heer ihn gehen hie\u00df,", "tokens": ["Bis", "sein", "be\u00b7krieg\u00b7ter", "F\u00fcrst", "zum", "Heer", "ihn", "ge\u00b7hen", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und Fried und Ruh durch ihn den V\u00f6lkern schenken lie\u00df.", "tokens": ["Und", "Fried", "und", "Ruh", "durch", "ihn", "den", "V\u00f6l\u00b7kern", "schen\u00b7ken", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch weint er j\u00e4hrlich um ihr Grab an diesem Tage,", "tokens": ["Doch", "weint", "er", "j\u00e4hr\u00b7lich", "um", "ihr", "Grab", "an", "die\u00b7sem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und sein ganz Leben war nur eine lange Klage.", "tokens": ["Und", "sein", "ganz", "Le\u00b7ben", "war", "nur", "ei\u00b7ne", "lan\u00b7ge", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}