{"textgrid.poem.55516": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "97", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn was irgend ist geschehen,", "tokens": ["Wenn", "was", "ir\u00b7gend", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt man's noch in sp\u00e4ten Tagen;", "tokens": ["H\u00f6rt", "man's", "noch", "in", "sp\u00e4\u00b7ten", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer klingend wird es wehen,", "tokens": ["Im\u00b7mer", "klin\u00b7gend", "wird", "es", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Glock ist angeschlagen.", "tokens": ["Wenn", "die", "Glock", "ist", "an\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so la\u00dft von diesem Schalle", "tokens": ["Und", "so", "la\u00dft", "von", "die\u00b7sem", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch erheitern, viele, viele!", "tokens": ["Euch", "er\u00b7hei\u00b7tern", ",", "vie\u00b7le", ",", "vie\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PIS", "$,", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn am Ende sind wir alle", "tokens": ["Denn", "am", "En\u00b7de", "sind", "wir", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pilgernd K\u00f6nige zum Ziele.", "tokens": ["Pil\u00b7gernd", "K\u00f6\u00b7ni\u00b7ge", "zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}