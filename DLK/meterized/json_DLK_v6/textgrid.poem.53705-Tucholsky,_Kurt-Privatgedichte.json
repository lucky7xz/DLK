{"textgrid.poem.53705": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Privatgedichte", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In allen meinen Scheingestalten", "tokens": ["In", "al\u00b7len", "mei\u00b7nen", "Schein\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich nicht Schein: bin ich enthalten!", "tokens": ["Bin", "ich", "nicht", "Schein", ":", "bin", "ich", "ent\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$.", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ist starr, was strahlt und weht im Lichte?", "tokens": ["Ist", "starr", ",", "was", "strahlt", "und", "weht", "im", "Lich\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWS", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wahr ist nur Wandlung der Gesichte.", "tokens": ["Wahr", "ist", "nur", "Wand\u00b7lung", "der", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Es blieb mein Mund bei deinem Munde,", "tokens": ["Es", "blieb", "mein", "Mund", "bei", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zutiefst bewahr ich unsre Stunde.", "tokens": ["Zu\u00b7tiefst", "be\u00b7wahr", "ich", "uns\u00b7re", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bin geschmiegt in euer Tasten,", "tokens": ["Und", "bin", "ge\u00b7schmiegt", "in", "eu\u00b7er", "Tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O sch\u00f6ne H\u00e4nde, die mich fa\u00dften.", "tokens": ["O", "sch\u00f6\u00b7ne", "H\u00e4n\u00b7de", ",", "die", "mich", "fa\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Fr\u00fchwache t\u00f6nt in den H\u00f6fen der Kasernen.", "tokens": ["Die", "Fr\u00fch\u00b7wa\u00b7che", "t\u00f6nt", "in", "den", "H\u00f6\u00b7fen", "der", "Ka\u00b7ser\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Morgenwinde blasen auf den Laternen.", "tokens": ["Die", "Mor\u00b7gen\u00b7win\u00b7de", "bla\u00b7sen", "auf", "den", "La\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "und eine \u00dcbertragung aus dem Russischen, deren Autor nicht genannt ist:", "tokens": ["und", "ei\u00b7ne", "\u00dc\u00b7ber\u00b7tra\u00b7gung", "aus", "dem", "Rus\u00b7si\u00b7schen", ",", "de\u00b7ren", "Au\u00b7tor", "nicht", "ge\u00b7nannt", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELAT", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}