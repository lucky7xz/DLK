"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.39": {"line.1": {"text": "Wir schritten durch die Glut, die rings empor sich bauschte,", "tokens": ["Wir", "schrit\u00b7ten", "durch", "die", "Glut", ",", "die", "rings", "em\u00b7por", "sich", "bauschte", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um uns wie Ueberschwang von goldnen Aehren rauschte.", "tokens": ["Um", "uns", "wie", "Ue\u00b7ber\u00b7schwang", "von", "gold\u00b7nen", "A\u00b7eh\u00b7ren", "rauschte", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "KOKOM", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.40": {"line.1": {"text": "Selbst mitten in der Glut war W\u00e4rme nicht zu sp\u00fcren;", "tokens": ["Selbst", "mit\u00b7ten", "in", "der", "Glut", "war", "W\u00e4r\u00b7me", "nicht", "zu", "sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VAFIN", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So linde Feuer kann die Gottes Allmacht sch\u00fcren.", "tokens": ["So", "lin\u00b7de", "Feu\u00b7er", "kann", "die", "Got\u00b7tes", "All\u00b7macht", "sch\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.41": {"line.1": {"text": "Nicht W\u00e4rme f\u00fchlten wir, doch eine milde Glut,