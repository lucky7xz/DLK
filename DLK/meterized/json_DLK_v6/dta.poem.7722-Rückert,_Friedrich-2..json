": "Und selber leise Furcht die doch bewu\u00dfte Seele.", "tokens": ["Und", "sel\u00b7ber", "lei\u00b7se", "Furcht", "die", "doch", "be\u00b7wu\u00df\u00b7te", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.35": {"line.1": {"text": "Wir wu\u00dften, da\u00df ein Schein es w\u00e4re, doch es drang", "tokens": ["Wir", "wu\u00df\u00b7ten", ",", "da\u00df", "ein", "Schein", "es", "w\u00e4\u00b7re", ",", "doch", "es", "drang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VAFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schein als Wirklichkeit sich auf, und macht' uns bang.", "tokens": ["Der", "Schein", "als", "Wirk\u00b7lich\u00b7keit", "sich", "auf", ",", "und", "macht'", "uns", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.36": {"line.1": {"text": "Wir sahen, da\u00df die Glut kein trocknes H\u00e4lmchen sehrte,", "tokens": ["Wir", "sa\u00b7hen", ",", "da\u00df", "die", "Glut", "kein", "trock\u00b7nes", "H\u00e4lm\u00b7chen", "sehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und am bethauten selbst den Tropfen Thau nicht zehrte.", "tokens": ["Und", "am", "be\u00b7t