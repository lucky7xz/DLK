{"dta.poem.7722": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam auf meiner Reis' im Karawanenpfade", "tokens": ["Ich", "kam", "auf", "mei\u00b7ner", "Reis'", "im", "Ka\u00b7ra\u00b7wa\u00b7nen\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unsern dem Kaukasus an's kaspische Gestade;", "tokens": ["Un\u00b7sern", "dem", "Kau\u00b7ka\u00b7sus", "an's", "kas\u00b7pi\u00b7sche", "Ge\u00b7sta\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und lernt' auf Baku's Flur begreifen, wie die Guebern", "tokens": ["Und", "lernt'", "auf", "Ba\u00b7ku's", "Flur", "be\u00b7grei\u00b7fen", ",", "wie", "die", "Gue\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort machte die Natur zu Feuerdienst-Urhebern.", "tokens": ["Dort", "mach\u00b7te", "die", "Na\u00b7tur", "zu", "Feu\u00b7er\u00b7dienst\u00b7Ur\u00b7he\u00b7bern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Halb eine Meile von der Stadt ist eine Stelle,", "tokens": ["Halb", "ei\u00b7ne", "Mei\u00b7le", "von", "der", "Stadt", "ist", "ei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im naftareichen Land die reichste Nafta-Quelle.", "tokens": ["Im", "naf\u00b7ta\u00b7rei\u00b7chen", "Land", "die", "reichs\u00b7te", "Nafta\u00b7Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort ist ein weiter Kreis, in dessen Mitt' ich sah", "tokens": ["Dort", "ist", "ein", "wei\u00b7ter", "Kreis", ",", "in", "des\u00b7sen", "Mitt'", "ich", "sah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ew'gen Flammen bl\u00fch'n das heil'ge Ateschgah.", "tokens": ["In", "ew'\u00b7gen", "Flam\u00b7men", "bl\u00fch'n", "das", "heil'\u00b7ge", "A\u00b7teschgah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Und von den Parsen legt' ein F\u00fchrer mir es aus,", "tokens": ["Und", "von", "den", "Par\u00b7sen", "legt'", "ein", "F\u00fch\u00b7rer", "mir", "es", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Ateschgah bedeut' auf Persisch Feuerhaus.", "tokens": ["Da\u00df", "A\u00b7teschgah", "be\u00b7deut'", "auf", "Per\u00b7sisch", "Feu\u00b7er\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Die heil'ge Flamm' entbl\u00fcht der Erde gelb und blau,", "tokens": ["Die", "heil'\u00b7ge", "Flamm'", "ent\u00b7bl\u00fcht", "der", "Er\u00b7de", "gelb", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Tag ein sch\u00f6ner Glanz, Nachts eine Wunderschau.", "tokens": ["Am", "Tag", "ein", "sch\u00f6\u00b7ner", "Glanz", ",", "Nachts", "ei\u00b7ne", "Wun\u00b7der\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein Volk von Guebern hat im Kreis um diese Flammen", "tokens": ["Ein", "Volk", "von", "Gue\u00b7bern", "hat", "im", "Kreis", "um", "die\u00b7se", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "APPRART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich angebaut und wohnt in stillem Flei\u00df beisammen.", "tokens": ["Sich", "an\u00b7ge\u00b7baut", "und", "wohnt", "in", "stil\u00b7lem", "Flei\u00df", "bei\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Den Feuerehrern hat das Feuer zur Belohnung", "tokens": ["Den", "Feu\u00b7e\u00b7reh\u00b7rern", "hat", "das", "Feu\u00b7er", "zur", "Be\u00b7loh\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gegeben ohne M\u00fch' die sch\u00f6nste Winterwohnung.", "tokens": ["Ge\u00b7ge\u00b7ben", "oh\u00b7ne", "M\u00fch'", "die", "sch\u00f6ns\u00b7te", "Win\u00b7ter\u00b7woh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Aus Steinen leicht gef\u00fcgt, ein Haus mit Dach und Wand", "tokens": ["Aus", "Stei\u00b7nen", "leicht", "ge\u00b7f\u00fcgt", ",", "ein", "Haus", "mit", "Dach", "und", "Wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht jedem nach der Wahl, wo einen Platz er fand.", "tokens": ["Steht", "je\u00b7dem", "nach", "der", "Wahl", ",", "wo", "ei\u00b7nen", "Platz", "er", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sie d\u00fcrfen sich bei'm Bau'n nicht um den Bauplatz streiten,", "tokens": ["Sie", "d\u00fcr\u00b7fen", "sich", "bei'm", "Bau'n", "nicht", "um", "den", "Bau\u00b7platz", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kranz der H\u00e4user w\u00e4chst mit Lust nach allen Seiten.", "tokens": ["Der", "Kranz", "der", "H\u00e4u\u00b7ser", "w\u00e4chst", "mit", "Lust", "nach", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Denn \u00fcberall durchzieht die heil'ge Gluth die Erde,", "tokens": ["Denn", "\u00fc\u00b7be\u00b7rall", "durch\u00b7zieht", "die", "heil'\u00b7ge", "Gluth", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und machet jedes Haus von selbst zum Feuerherde.", "tokens": ["Und", "ma\u00b7chet", "je\u00b7des", "Haus", "von", "selbst", "zum", "Feu\u00b7er\u00b7her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Den untern Boden deckt von Lehm die feste Tenne,", "tokens": ["Den", "un\u00b7tern", "Bo\u00b7den", "deckt", "von", "Lehm", "die", "fes\u00b7te", "Ten\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df den Bewohner sie von seiner Gottheit trenne.", "tokens": ["Da\u00df", "den", "Be\u00b7woh\u00b7ner", "sie", "von", "sei\u00b7ner", "Got\u00b7theit", "tren\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Doch Oeffnungen sind da gelassen, wo erbeten", "tokens": ["Doch", "Oeff\u00b7nun\u00b7gen", "sind", "da", "ge\u00b7las\u00b7sen", ",", "wo", "er\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADV", "VVPP", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Elementes Kraft soll aus dem Boden treten.", "tokens": ["Des", "E\u00b7le\u00b7men\u00b7tes", "Kraft", "soll", "aus", "dem", "Bo\u00b7den", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Du steckest in die Spalt' ein lehmumgeb'nes Rohr,", "tokens": ["Du", "ste\u00b7ckest", "in", "die", "Spalt'", "ein", "leh\u00b7mum\u00b7ge\u00b7b'\u00b7nes", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und leitest wie du willst den Feuergeist empor.", "tokens": ["Und", "lei\u00b7test", "wie", "du", "willst", "den", "Feu\u00b7er\u00b7geist", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "VMFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Und \u00fcberall im Haus, wohin das Rohr du m\u00fcndest,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "im", "Haus", ",", "wo\u00b7hin", "das", "Rohr", "du", "m\u00fcn\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da leuchtet es, sobald du an den Dunststrom z\u00fcndest.", "tokens": ["Da", "leuch\u00b7tet", "es", ",", "so\u00b7bald", "du", "an", "den", "Dunst\u00b7strom", "z\u00fcn\u00b7dest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Es ist ein sch\u00f6nes Licht und brauchst es nicht zu putzen,", "tokens": ["Es", "ist", "ein", "sch\u00f6\u00b7nes", "Licht", "und", "brauchst", "es", "nicht", "zu", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Aufwand kannst du es im Haus beliebig nutzen.", "tokens": ["Ohn'", "Auf\u00b7wand", "kannst", "du", "es", "im", "Haus", "be\u00b7lie\u00b7big", "nut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Leinweber sah ich so die ganze Nacht durch weben,", "tokens": ["Lein\u00b7we\u00b7ber", "sah", "ich", "so", "die", "gan\u00b7ze", "Nacht", "durch", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach Lust mit schwebenden Rohrleuchten rings umgeben.", "tokens": ["Nach", "Lust", "mit", "schwe\u00b7ben\u00b7den", "Rohr\u00b7leuch\u00b7ten", "rings", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Wer aber Kaffe will und wer will Speise kochen,", "tokens": ["Wer", "a\u00b7ber", "Kaf\u00b7fe", "will", "und", "wer", "will", "Spei\u00b7se", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VMFIN", "KON", "PWS", "VMFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus andrer Oeffnung kommt ein andrer Strom gebrochen.", "tokens": ["Aus", "an\u00b7drer", "Oeff\u00b7nung", "kommt", "ein", "an\u00b7drer", "Strom", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Ein Feuerstrom, der, ohn' Holz oder Kohlenfeuer,", "tokens": ["Ein", "Feu\u00b7er\u00b7strom", ",", "der", ",", "ohn'", "Holz", "o\u00b7der", "Koh\u00b7len\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So gut als beides brennt, und lange nicht so theuer.", "tokens": ["So", "gut", "als", "bei\u00b7des", "brennt", ",", "und", "lan\u00b7ge", "nicht", "so", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIS", "VVFIN", "$,", "KON", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Das Feuer sch\u00fcrt sich selbst, und brennt, so lang du's willst,", "tokens": ["Das", "Feu\u00b7er", "sch\u00fcrt", "sich", "selbst", ",", "und", "brennt", ",", "so", "lang", "du's", "willst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,", "KON", "VVFIN", "$,", "ADV", "ADJD", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und still vergeht's, wenn du mit einem Wink es stillst.", "tokens": ["Und", "still", "ver\u00b7geht's", ",", "wenn", "du", "mit", "ei\u00b7nem", "Wink", "es", "stillst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "Aus kleinster Oeffnung bricht's mit gr\u00f6ster Kraft hervor,", "tokens": ["Aus", "kleins\u00b7ter", "Oeff\u00b7nung", "bricht's", "mit", "gr\u00f6s\u00b7ter", "Kraft", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00e4chst, vom Zwang befreit, zur h\u00f6chsten H\u00f6h' empor.", "tokens": ["Und", "w\u00e4chst", ",", "vom", "Zwang", "be\u00b7freit", ",", "zur", "h\u00f6chs\u00b7ten", "H\u00f6h'", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "Aus einer M\u00fcndung von zwei Zollen sah' ich's steigen", "tokens": ["Aus", "ei\u00b7ner", "M\u00fcn\u00b7dung", "von", "zwei", "Zol\u00b7len", "sah'", "ich's", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei Fu\u00df zuerst, und sich zuletzt zu zwei Fu\u00df neigen.", "tokens": ["Drei", "Fu\u00df", "zu\u00b7erst", ",", "und", "sich", "zu\u00b7letzt", "zu", "zwei", "Fu\u00df", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "KON", "PRF", "ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.23": {"line.1": {"text": "Und brauchest du's nicht mehr, so brauchet nur zu f\u00e4cheln", "tokens": ["Und", "brau\u00b7chest", "du's", "nicht", "mehr", ",", "so", "brau\u00b7chet", "nur", "zu", "f\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein F\u00e4cher, und sogleich verschwindet es mit L\u00e4cheln.", "tokens": ["Ein", "F\u00e4\u00b7cher", ",", "und", "sog\u00b7leich", "ver\u00b7schwin\u00b7det", "es", "mit", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.24": {"line.1": {"text": "In's unterird'sche Haus kehrt es zur\u00fcck, sein Thor", "tokens": ["In's", "un\u00b7ter\u00b7ird'\u00b7sche", "Haus", "kehrt", "es", "zu\u00b7r\u00fcck", ",", "sein", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschlie\u00dfest du, und still nun wohnt es wie zuvor.", "tokens": ["Ver\u00b7schlie\u00b7\u00dfest", "du", ",", "und", "still", "nun", "wohnt", "es", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADJD", "ADV", "VVFIN", "PPER", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.25": {"line.1": {"text": "Nur an der W\u00e4rme magst du dann sein Walten sp\u00fcren;", "tokens": ["Nur", "an", "der", "W\u00e4r\u00b7me", "magst", "du", "dann", "sein", "Wal\u00b7ten", "sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wohnen Winterlang daselbst bei offnen Th\u00fcren.", "tokens": ["Sie", "woh\u00b7nen", "Win\u00b7ter\u00b7lang", "da\u00b7selbst", "bei", "off\u00b7nen", "Th\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.26": {"line.1": {"text": "Das ist vom Feuergeist die eine der Gestalten;", "tokens": ["Das", "ist", "vom", "Feu\u00b7er\u00b7geist", "die", "ei\u00b7ne", "der", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einer zweiten ist noch gl\u00e4nzender sein Walten.", "tokens": ["In", "ei\u00b7ner", "zwei\u00b7ten", "ist", "noch", "gl\u00e4n\u00b7zen\u00b7der", "sein", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.27": {"line.1": {"text": "Wie er im Hause ruht als brennbar Element,", "tokens": ["Wie", "er", "im", "Hau\u00b7se", "ruht", "als", "brenn\u00b7bar", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schweift er durch die Flur als Feuer, das nicht brennt.", "tokens": ["So", "schweift", "er", "durch", "die", "Flur", "als", "Feu\u00b7er", ",", "das", "nicht", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KOUS", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.28": {"line.1": {"text": "Oft im September, wann des Herbstes warmer Regen", "tokens": ["Oft", "im", "Sep\u00b7tem\u00b7ber", ",", "wann", "des", "Herbs\u00b7tes", "war\u00b7mer", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die Abendluft erfrischt, dann ist der Geist zugegen.", "tokens": ["Die", "A\u00b7ben\u00b7dluft", "er\u00b7frischt", ",", "dann", "ist", "der", "Geist", "zu\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.29": {"line.1": {"text": "Dann siehst du weit und breit, soweit die Blicke gehn,", "tokens": ["Dann", "siehst", "du", "weit", "und", "breit", ",", "so\u00b7weit", "die", "Bli\u00b7cke", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Felder wie ein Meer in Flammenwogen stehn.", "tokens": ["Die", "Fel\u00b7der", "wie", "ein", "Meer", "in", "Flam\u00b7men\u00b7wo\u00b7gen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.30": {"line.1": {"text": "Oft rollt der Feuerstrom in ungeheuren Massen", "tokens": ["Oft", "rollt", "der", "Feu\u00b7er\u00b7strom", "in", "un\u00b7ge\u00b7heu\u00b7ren", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Berg herab in's Thal, das ihn nicht scheint zu fassen.", "tokens": ["Vom", "Berg", "her\u00b7ab", "in's", "Thal", ",", "das", "ihn", "nicht", "scheint", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.31": {"line.1": {"text": "Dann im Oktober, wann der Mond erhellt die Nacht,", "tokens": ["Dann", "im", "Ok\u00b7to\u00b7ber", ",", "wann", "der", "Mond", "er\u00b7hellt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ganze Westgebirg von blauem Feuer lacht.", "tokens": ["Das", "gan\u00b7ze", "West\u00b7ge\u00b7birg", "von", "blau\u00b7em", "Feu\u00b7er", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.32": {"line.1": {"text": "Doch wann die Nacht ist tr\u00fcb, irrt wimmelndes Gefunkel", "tokens": ["Doch", "wann", "die", "Nacht", "ist",