{"dta.poem.801": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.  \n An  Valerium.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Vmbsonst/ Mein Freund vmbsonst! Ich kan dir nicht ge- ", "tokens": ["Vmbsonst", "/", "Mein", "Freund", "vmbsonst", "!", "Ich", "kan", "dir", "nicht", "ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PPOSAT", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "TRUNC"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "wehren ", "tokens": ["weh\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein denckmal das von fall/ von strenger Tyranney", "tokens": ["Ein", "denck\u00b7mal", "das", "von", "fall", "/", "von", "stren\u00b7ger", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "APPR", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der Jahr/ vnd seiner grufft sich/ dich/ vnd mich befrey.", "tokens": ["Der", "Jahr", "/", "vnd", "sei\u00b7ner", "grufft", "sich", "/", "dich", "/", "vnd", "mich", "be\u00b7frey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PPOSAT", "NN", "PRF", "$(", "PPER", "$(", "KON", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die zeit kan Ertz vnd Stein in kott vnd grau\u00df verkehren.", "tokens": ["Die", "zeit", "kan", "Ertz", "vnd", "Stein", "in", "kott", "vnd", "grau\u00df", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was Menschen hand auffsetzt/ kan Menschen hand ver-", "tokens": ["Was", "Men\u00b7schen", "hand", "auff\u00b7setzt", "/", "kan", "Men\u00b7schen", "hand", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "NN", "VVPP", "$(", "VMFIN", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "heren/ / ", "tokens": ["he\u00b7ren", "/", "/"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Vnd ob sie ruh: die noth/ See/ Erdfall/ schwerd vnd bley.", "tokens": ["Vnd", "ob", "sie", "ruh", ":", "die", "noth", "/", "See", "/", "Erd\u00b7fall", "/", "schwerd", "vnd", "bley", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "$.", "ART", "NN", "$(", "NN", "$(", "NN", "$(", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gesetzt auch da\u00df ich nicht auff einmal sterblich sey?", "tokens": ["Ge\u00b7setzt", "auch", "da\u00df", "ich", "nicht", "auff", "ein\u00b7mal", "sterb\u00b7lich", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PTKNEG", "APPR", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rhumb ist ein blosser wahn/ den Todte nicht begehren.", "tokens": ["Rhumb", "ist", "ein", "blos\u00b7ser", "wahn", "/", "den", "Tod\u00b7te", "nicht", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Meynst du/ da\u00df dis Papir werd\u2019 vnversehrt bestehn;", "tokens": ["Meynst", "du", "/", "da\u00df", "dis", "Pa\u00b7pir", "werd'", "vn\u00b7ver\u00b7sehrt", "be\u00b7stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "KOUS", "PDS", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nun der Erden Baw\u2019 in flammen wird vergehn/", "tokens": ["Wenn", "nun", "der", "Er\u00b7den", "Ba\u00b7w'", "in", "flam\u00b7men", "wird", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "APPR", "VVINF", "VAFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vnd sein beschwertes Grab in eigner Aschen werden?", "tokens": ["Vnd", "sein", "be\u00b7schwer\u00b7tes", "Grab", "in", "eig\u00b7ner", "A\u00b7schen", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O selig wer die Tr\u00e4um/ vnd nichtig Lob verlacht/", "tokens": ["O", "se\u00b7lig", "wer", "die", "Tr\u00e4um", "/", "vnd", "nich\u00b7tig", "Lob", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PWS", "ART", "NN", "$(", "KON", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer jmmer newem Ruhm vnd ew\u2019ger Ehr nach tracht!", "tokens": ["Wer", "jm\u00b7mer", "ne\u00b7wem", "Ruhm", "vnd", "ew'\u00b7ger", "Ehr", "nach", "tracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die vns der Himmel schenckt/ nicht die verg\u00e4nglich-Erden.", "tokens": ["Die", "vns", "der", "Him\u00b7mel", "schenckt", "/", "nicht", "die", "ver\u00b7g\u00e4ng\u00b7lich\u00b7Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$(", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}