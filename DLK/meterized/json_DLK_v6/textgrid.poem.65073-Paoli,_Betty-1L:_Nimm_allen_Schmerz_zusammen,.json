{"textgrid.poem.65073": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm allen Schmerz zusammen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm allen Schmerz zusammen,", "tokens": ["Nimm", "al\u00b7len", "Schmerz", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Seelen je erdr\u00fcckt,", "tokens": ["Der", "See\u00b7len", "je", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Gottesflammen,", "tokens": ["Und", "al\u00b7le", "Got\u00b7tes\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Seelen je entz\u00fcckt;", "tokens": ["Die", "See\u00b7len", "je", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was je an Freud' und Qualen", "tokens": ["Was", "je", "an", "Freud'", "und", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Welt ein R\u00e4thsel blieb,", "tokens": ["Der", "Welt", "ein", "R\u00e4th\u00b7sel", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird's zusammenhallen", "tokens": ["Dann", "wird's", "zu\u00b7sam\u00b7men\u00b7hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie meine tiefe Lieb'.", "tokens": ["Wie", "mei\u00b7ne", "tie\u00b7fe", "Lieb'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}