{"textgrid.poem.44973": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die leeren Logen in der Zauberfl\u00f6te", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df euch die Oper nicht gef\u00e4llt,", "tokens": ["Da\u00df", "euch", "die", "O\u00b7per", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wundert uns f\u00fcrwahr im ganzen,", "tokens": ["Es", "wun\u00b7dert", "uns", "f\u00fcr\u00b7wahr", "im", "gan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sahn doch euresgleichen drei", "tokens": ["Wir", "sahn", "doch", "eu\u00b7res\u00b7glei\u00b7chen", "drei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Froh zu Taminos Fl\u00f6te tanzen.", "tokens": ["Froh", "zu", "Ta\u00b7mi\u00b7nos", "Fl\u00f6\u00b7te", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}