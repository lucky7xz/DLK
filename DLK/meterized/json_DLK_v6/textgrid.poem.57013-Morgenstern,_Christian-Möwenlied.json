{"textgrid.poem.57013": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "M\u00f6wenlied", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00f6wen sehen alle aus,", "tokens": ["Die", "M\u00f6\u00b7wen", "se\u00b7hen", "al\u00b7le", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "als ob sie Emma hie\u00dfen.", "tokens": ["als", "ob", "sie", "Em\u00b7ma", "hie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie tragen einen wei\u00dfen Flaus", "tokens": ["Sie", "tra\u00b7gen", "ei\u00b7nen", "wei\u00b7\u00dfen", "Flaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und sind mit Schrot zu schie\u00dfen.", "tokens": ["und", "sind", "mit", "Schrot", "zu", "schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schie\u00dfe keine M\u00f6we tot,", "tokens": ["Ich", "schie\u00b7\u00dfe", "kei\u00b7ne", "M\u00f6\u00b7we", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich la\u00df sie lieber leben \u2013", "tokens": ["ich", "la\u00df", "sie", "lie\u00b7ber", "le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und f\u00fcttre sie mit Roggenbrot", "tokens": ["und", "f\u00fctt\u00b7re", "sie", "mit", "Rog\u00b7gen\u00b7brot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und r\u00f6tlichen Zibeben.", "tokens": ["und", "r\u00f6t\u00b7li\u00b7chen", "Zi\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "O Mensch, du wirst nie nebenbei", "tokens": ["O", "Mensch", ",", "du", "wirst", "nie", "ne\u00b7ben\u00b7bei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADV", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "der M\u00f6we Flug erreichen.", "tokens": ["der", "M\u00f6\u00b7we", "Flug", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wofern du Emma hei\u00dfest, sei", "tokens": ["Wo\u00b7fern", "du", "Em\u00b7ma", "hei\u00b7\u00dfest", ",", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zufrieden, ihr zu gleichen.", "tokens": ["zu\u00b7frie\u00b7den", ",", "ihr", "zu", "glei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}