{"dta.poem.15692": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fehler, die an dir du selbst nicht sehen kannst,", "tokens": ["Die", "Feh\u00b7ler", ",", "die", "an", "dir", "du", "selbst", "nicht", "se\u00b7hen", "kannst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du an andern. Wei\u00dft, was du daran gewannst?", "tokens": ["Siehst", "du", "an", "an\u00b7dern", ".", "Wei\u00dft", ",", "was", "du", "da\u00b7ran", "ge\u00b7wannst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "$.", "VVFIN", "$,", "PWS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht bessern kannst du sie an andern, doch villeicht", "tokens": ["Nicht", "bes\u00b7sern", "kannst", "du", "sie", "an", "an\u00b7dern", ",", "doch", "vil\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dir; das ist der Dienst, den dir ein Spiegel reicht.", "tokens": ["An", "dir", ";", "das", "ist", "der", "Dienst", ",", "den", "dir", "ein", "Spie\u00b7gel", "reicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Spiegel dient, dir selbst die Flecken zu entdecken;", "tokens": ["Der", "Spie\u00b7gel", "dient", ",", "dir", "selbst", "die", "Fle\u00b7cken", "zu", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Spiegel wische nicht, an dir wisch' ab die Flecken!", "tokens": ["Am", "Spie\u00b7gel", "wi\u00b7sche", "nicht", ",", "an", "dir", "wisch'", "ab", "die", "Fle\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "$,", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}