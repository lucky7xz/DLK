{"textgrid.poem.65997": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Knarrt die Stiege? Schritt vor Schritt,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Knarrt die Stiege? Schritt vor Schritt,", "tokens": ["Knarrt", "die", "Stie\u00b7ge", "?", "Schritt", "vor", "Schritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlurfend, schleifend kommt es nah.", "tokens": ["Schlur\u00b7fend", ",", "schlei\u00b7fend", "kommt", "es", "nah", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kenne dich am Tapp und Tritt,", "tokens": ["Ken\u00b7ne", "dich", "am", "Tapp", "und", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sorge, bist du wieder da?", "tokens": ["Sor\u00b7ge", ",", "bist", "du", "wie\u00b7der", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00c4rgert dich mein Wohlergehn,", "tokens": ["\u00c4r\u00b7gert", "dich", "mein", "Woh\u00b7ler\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser ganz bescheidene Glanz?", "tokens": ["Die\u00b7ser", "ganz", "be\u00b7schei\u00b7de\u00b7ne", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Kannst du niemand fr\u00f6hlich sehn?", "tokens": ["Kannst", "du", "nie\u00b7mand", "fr\u00f6h\u00b7lich", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zerrst und zaust an jedem Kranz?", "tokens": ["Zerrst", "und", "zaust", "an", "je\u00b7dem", "Kranz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "G\u00f6nn' mir doch das wenige Gut,", "tokens": ["G\u00f6nn'", "mir", "doch", "das", "we\u00b7ni\u00b7ge", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das ein harter Flei\u00df beschert,", "tokens": ["Das", "ein", "har\u00b7ter", "Flei\u00df", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6sch des Friedens sanfte Glut", "tokens": ["L\u00f6sch", "des", "Frie\u00b7dens", "sanf\u00b7te", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neidisch nicht auf meinem Herd.", "tokens": ["Nei\u00b7disch", "nicht", "auf", "mei\u00b7nem", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Wiege dort, davor", "tokens": ["Und", "die", "Wie\u00b7ge", "dort", ",", "da\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "ADV", "$,", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutterangst Gebete spricht,", "tokens": ["Mut\u00b7ter\u00b7angst", "Ge\u00b7be\u00b7te", "spricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe lauscht mit wachem Ohr,", "tokens": ["Lie\u00b7be", "lauscht", "mit", "wa\u00b7chem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All mein Gl\u00fcck, o st\u00f6r' es nicht.", "tokens": ["All", "mein", "Gl\u00fcck", ",", "o", "st\u00f6r'", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}