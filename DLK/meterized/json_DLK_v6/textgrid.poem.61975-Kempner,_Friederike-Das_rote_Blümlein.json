{"textgrid.poem.61975": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Das rote Bl\u00fcmlein", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rotes Bl\u00fcmlein auf gr\u00fcner Au,", "tokens": ["Ein", "ro\u00b7tes", "Bl\u00fcm\u00b7lein", "auf", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein kleines W\u00f6lkchen an Himmels Blau,", "tokens": ["Ein", "klei\u00b7nes", "W\u00f6lk\u00b7chen", "an", "Him\u00b7mels", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein feines M\u00e4gdlein im leichten Kahn,", "tokens": ["Ein", "fei\u00b7nes", "M\u00e4gd\u00b7lein", "im", "leich\u00b7ten", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es eilet, es eilet die Flut hinan.", "tokens": ["Es", "ei\u00b7let", ",", "es", "ei\u00b7let", "die", "Flut", "hi\u00b7nan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Das Bl\u00fcmlein zittert auf gr\u00fcner Au,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "zit\u00b7tert", "auf", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das W\u00f6lkchen am Himmel wird schwarz und grau,", "tokens": ["Das", "W\u00f6lk\u00b7chen", "am", "Him\u00b7mel", "wird", "schwarz", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das M\u00e4gdlein bebet im leichten Kahn,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "be\u00b7bet", "im", "leich\u00b7ten", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und m\u00e4chtiger eilet die Flut hinan.", "tokens": ["Und", "m\u00e4ch\u00b7ti\u00b7ger", "ei\u00b7let", "die", "Flut", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Das Bl\u00fcmlein zerstoben auf gr\u00fcner Au,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "zer\u00b7sto\u00b7ben", "auf", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das W\u00f6lkchen verschwunden am himmlischen Blau,", "tokens": ["Das", "W\u00f6lk\u00b7chen", "ver\u00b7schwun\u00b7den", "am", "himm\u00b7li\u00b7schen", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das M\u00e4gdlein versunken im leichten Kahn,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "ver\u00b7sun\u00b7ken", "im", "leich\u00b7ten", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es steiget die Flut die H\u00f6hen hinan.", "tokens": ["Es", "stei\u00b7get", "die", "Flut", "die", "H\u00f6\u00b7hen", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was st\u00fcrmst Du, Flut, den Himmel hinan,", "tokens": ["Was", "st\u00fcrmst", "Du", ",", "Flut", ",", "den", "Him\u00b7mel", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was willst Du, gr\u00e4ulicher Wassersmann? \u2013", "tokens": ["Was", "willst", "Du", ",", "gr\u00e4u\u00b7li\u00b7cher", "Was\u00b7sers\u00b7mann", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O stolzer Knabe, sei ruhig, sei still,", "tokens": ["O", "stol\u00b7zer", "Kna\u00b7be", ",", "sei", "ru\u00b7hig", ",", "sei", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Spiel und Dein Traum und Dein Lieb ich will! \u2013", "tokens": ["Dein", "Spiel", "und", "Dein", "Traum", "und", "Dein", "Lieb", "ich", "will", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PPER", "VMFIN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Mein Spiel ist hin, und mein Traum ist hin,", "tokens": ["Mein", "Spiel", "ist", "hin", ",", "und", "mein", "Traum", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was k\u00f6mmt Dir, Du t\u00f6richter Mann, in den Sinn?", "tokens": ["Was", "k\u00f6mmt", "Dir", ",", "Du", "t\u00f6\u00b7rich\u00b7ter", "Mann", ",", "in", "den", "Sinn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mein Lieb lebt unter der brausenden Flut,", "tokens": ["Mein", "Lieb", "lebt", "un\u00b7ter", "der", "brau\u00b7sen\u00b7den", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nimmer verlischt uns're Liebesglut.", "tokens": ["Und", "nim\u00b7mer", "ver\u00b7lischt", "un\u00b7s'\u00b7re", "Lie\u00b7bes\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Das rote Bl\u00fcmlein, das war mein Spiel,", "tokens": ["Das", "ro\u00b7te", "Bl\u00fcm\u00b7lein", ",", "das", "war", "mein", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem W\u00f6lkchen am Himmel traut' ich so viel,", "tokens": ["Dem", "W\u00f6lk\u00b7chen", "am", "Him\u00b7mel", "traut'", "ich", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem M\u00e4gdlein folgte mein ganzes Herz,", "tokens": ["Dem", "M\u00e4gd\u00b7lein", "folg\u00b7te", "mein", "gan\u00b7zes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch H\u00fctte und Kerker und Not allerw\u00e4rts.", "tokens": ["Durch", "H\u00fct\u00b7te", "und", "Ker\u00b7ker", "und", "Not", "al\u00b7ler\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Und schl\u00e4gt die Woge auch \u00fcber ihm her,", "tokens": ["Und", "schl\u00e4gt", "die", "Wo\u00b7ge", "auch", "\u00fc\u00b7ber", "ihm", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das M\u00e4gdlein erstehet aus Nacht und Meer,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "er\u00b7ste\u00b7het", "aus", "Nacht", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich seh' es behalten so wohl auf dem Grund", "tokens": ["Ich", "seh'", "es", "be\u00b7hal\u00b7ten", "so", "wohl", "auf", "dem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als wie es mir nah' vor den Augen stund.", "tokens": ["Als", "wie", "es", "mir", "nah'", "vor", "den", "Au\u00b7gen", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}}}}