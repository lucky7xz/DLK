{"textgrid.poem.36638": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wunderbar umf\u00e4ngst mich", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wunderbar umf\u00e4ngst mich", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "um\u00b7f\u00e4ngst", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allliebend,", "tokens": ["A\u00b7lllie\u00b7bend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Heiliges Licht?", "tokens": ["Hei\u00b7li\u00b7ges", "Licht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Aus jungem Gr\u00fcn hebt", "tokens": ["Aus", "jun\u00b7gem", "Gr\u00fcn", "hebt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dunkel-einsam, wie ein Geist,", "tokens": ["Dun\u00b7kel\u00b7ein\u00b7sam", ",", "wie", "ein", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Grau verwittert Gestein,", "tokens": ["Grau", "ver\u00b7wit\u00b7tert", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "S\u00e4ul' an S\u00e4ule", "tokens": ["S\u00e4ul'", "an", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sich empor:", "tokens": ["Sich", "em\u00b7por", ":"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Webt um alte W\u00f6lbung", "tokens": ["Webt", "um", "al\u00b7te", "W\u00f6l\u00b7bung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Weich-schwellend, umstrickend,", "tokens": ["Weich\u00b7schwel\u00b7lend", ",", "um\u00b7stri\u00b7ckend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wie ein l\u00e4chelnd Kind", "tokens": ["Wie", "ein", "l\u00e4\u00b7chelnd", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Um den ernsten Vater,", "tokens": ["Um", "den", "erns\u00b7ten", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Liebend-innig Epheugebl\u00e4tter,", "tokens": ["Lie\u00b7ben\u00b7din\u00b7nig", "E\u00b7pheu\u00b7ge\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Dr\u00e4ngt hinan", "tokens": ["Dr\u00e4ngt", "hi\u00b7nan"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "---", "measure": "unknown.measure.zero"}, "line.15": {"text": "Fl\u00fcsternd zu alter Tr\u00fcmmer", "tokens": ["Fl\u00fcs\u00b7ternd", "zu", "al\u00b7ter", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.16": {"text": "Ehrw\u00fcrdigen Gipfeln:", "tokens": ["Ehr\u00b7w\u00fcr\u00b7di\u00b7gen", "Gip\u00b7feln", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Und die Sonne fa\u00dft", "tokens": ["Und", "die", "Son\u00b7ne", "fa\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.18": {"text": "Alllebend, umquillend,", "tokens": ["A\u00b7llle\u00b7bend", ",", "um\u00b7quil\u00b7lend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Laubgr\u00fcn, s\u00e4ulengrau,", "tokens": ["Laub\u00b7gr\u00fcn", ",", "s\u00e4u\u00b7len\u00b7grau", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "F\u00fcllet alles,", "tokens": ["F\u00fcl\u00b7let", "al\u00b7les", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Mit Liebe, mit Liebe!", "tokens": ["Mit", "Lie\u00b7be", ",", "mit", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Fort dr\u00e4ngt mich's", "tokens": ["Fort", "dr\u00e4ngt", "mich's"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Im schwellenden Busen!", "tokens": ["Im", "schwel\u00b7len\u00b7den", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Ach wohin?", "tokens": ["Ach", "wo\u00b7hin", "?"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PWAV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Wie du weh'st", "tokens": ["Wie", "du", "weh'st"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Auf luftiger H\u00f6h',", "tokens": ["Auf", "luf\u00b7ti\u00b7ger", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Um Wang' und Locken,", "tokens": ["Um", "Wang'", "und", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lieblicher Wind!", "tokens": ["Lieb\u00b7li\u00b7cher", "Wind", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ahnest du, weinest du,", "tokens": ["Ah\u00b7nest", "du", ",", "wei\u00b7nest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Liebend Herz?", "tokens": ["Lie\u00b7bend", "Herz", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Bist so lauter und mild", "tokens": ["Bist", "so", "lau\u00b7ter", "und", "mild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In deines Blau's", "tokens": ["In", "dei\u00b7nes", "Blau's"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Unendlicher F\u00fclle,", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Heiterer Himmel!", "tokens": ["Hei\u00b7te\u00b7rer", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Alleinig liebt und webt", "tokens": ["Al\u00b7lei\u00b7nig", "liebt", "und", "webt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und treibt und keimt", "tokens": ["Und", "treibt", "und", "keimt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Alles, deine Kinder alle,", "tokens": ["Al\u00b7les", ",", "dei\u00b7ne", "Kin\u00b7der", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PPOSAT", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die dich schauen, lieben,", "tokens": ["Die", "dich", "schau\u00b7en", ",", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Heilige Sonne,", "tokens": ["Hei\u00b7li\u00b7ge", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Auge des Himmels,", "tokens": ["Au\u00b7ge", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "An der alten Erde", "tokens": ["An", "der", "al\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Keuschem, w\u00e4rmendem Busen.", "tokens": ["Keu\u00b7schem", ",", "w\u00e4r\u00b7men\u00b7dem", "Bu\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Du bist's! Du bist's!", "tokens": ["Du", "bist's", "!", "Du", "bist's", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "PPER", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bildende! Liebende!", "tokens": ["Bil\u00b7den\u00b7de", "!", "Lie\u00b7ben\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Fassest mich, ziehest mich", "tokens": ["Fas\u00b7sest", "mich", ",", "zie\u00b7hest", "mich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ganz zu dir!", "tokens": ["Ganz", "zu", "dir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Hin\u00fcber!", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ueber das Hellgr\u00fcn", "tokens": ["Ue\u00b7ber", "das", "Hell\u00b7gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und graue Tr\u00fcmmer,", "tokens": ["Und", "grau\u00b7e", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ueber Berg und Meer,", "tokens": ["Ue\u00b7ber", "Berg", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ueber die blauen Inseln!", "tokens": ["Ue\u00b7ber", "die", "blau\u00b7en", "In\u00b7seln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Hin\u00fcber! hin\u00fcber!", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "!", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ach! verschwimmen", "tokens": ["Ach", "!", "ver\u00b7schwim\u00b7men"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$.", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Ganz in dich,", "tokens": ["Ganz", "in", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Du heiterer Himmel!", "tokens": ["Du", "hei\u00b7te\u00b7rer", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}