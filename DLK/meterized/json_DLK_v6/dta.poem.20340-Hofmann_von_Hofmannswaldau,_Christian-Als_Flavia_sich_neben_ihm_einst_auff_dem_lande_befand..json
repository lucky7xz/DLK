{"dta.poem.20340": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Als Flavia sich neben ihm einst auff  \n dem lande befand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mein tr\u00fcbes licht nach diesem orte richte/", "tokens": ["Wenn", "ich", "mein", "tr\u00fc\u00b7bes", "licht", "nach", "die\u00b7sem", "or\u00b7te", "rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da Flavia bey mir vor wenig tagen sa\u00df/", "tokens": ["Da", "Fla\u00b7via", "bey", "mir", "vor", "we\u00b7nig", "ta\u00b7gen", "sa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als ich die lilien von ihrem angesichte", "tokens": ["Als", "ich", "die", "li\u00b7li\u00b7en", "von", "ih\u00b7rem", "an\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und rosen um das feld der zarten lippen la\u00df;", "tokens": ["Und", "ro\u00b7sen", "um", "das", "feld", "der", "zar\u00b7ten", "lip\u00b7pen", "la\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So seuffz\u2019 ich: sch\u00f6ner ort! du hast zu viel verlohren!", "tokens": ["So", "seuff\u00b7z'", "ich", ":", "sch\u00f6\u00b7ner", "ort", "!", "du", "hast", "zu", "viel", "ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$.", "ADJA", "NN", "$.", "PPER", "VAFIN", "PTKA", "PIS", "VVPP", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Doch deine Flavia die findest du bey mir.", "tokens": ["Doch", "dei\u00b7ne", "Fla\u00b7via", "die", "fin\u00b7dest", "du", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ich fluchte/ da\u00df ein weib mich zu der welt gebohren/", "tokens": ["Ich", "fluch\u00b7te", "/", "da\u00df", "ein", "weib", "mich", "zu", "der", "welt", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn mir mein hertze sie nicht zeigte f\u00fcr und f\u00fcr.", "tokens": ["Wenn", "mir", "mein", "hert\u00b7ze", "sie", "nicht", "zeig\u00b7te", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "PPER", "PTKNEG", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O wald! mu\u00df gleich dein blat noch vor dem winter weichen/", "tokens": ["O", "wald", "!", "mu\u00df", "gleich", "dein", "blat", "noch", "vor", "dem", "win\u00b7ter", "wei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das bild der Flavia steht hier doch unber\u00fchrt.", "tokens": ["Das", "bild", "der", "Fla\u00b7via", "steht", "hier", "doch", "un\u00b7be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wird gleich der bleiche tod mir um die schl\u00e4ffe streichen/", "tokens": ["Wird", "gleich", "der", "blei\u00b7che", "tod", "mir", "um", "die", "schl\u00e4f\u00b7fe", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wei\u00df ich/ da\u00df mein hertz sie mit zu grabe f\u00fchrt.", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "mein", "hertz", "sie", "mit", "zu", "gra\u00b7be", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "PTKZU", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}