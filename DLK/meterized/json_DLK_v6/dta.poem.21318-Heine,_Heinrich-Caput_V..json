{"dta.poem.21318": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und trommelt er dir einen schlechten Witz,", "tokens": ["Und", "trom\u00b7melt", "er", "dir", "ei\u00b7nen", "schlech\u00b7ten", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So pfeifen wir ihm einen schlimmern,", "tokens": ["So", "pfei\u00b7fen", "wir", "ihm", "ei\u00b7nen", "schlim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir pfeifen ihm vor was ihm passirt", "tokens": ["Wir", "pfei\u00b7fen", "ihm", "vor", "was", "ihm", "pas\u00b7sirt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRELS", "PPER", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei sch\u00f6nen Frauenzimmern.", "tokens": ["Bei", "sch\u00f6\u00b7nen", "Frau\u00b7en\u00b7zim\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gieb dich zufrieden, Vater Rhein,", "tokens": ["Gieb", "dich", "zu\u00b7frie\u00b7den", ",", "Va\u00b7ter", "Rhein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denk\u2019 nicht an schlechte Lieder,", "tokens": ["Denk'", "nicht", "an", "schlech\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein besseres Lied vernimmst du bald \u2013", "tokens": ["Ein", "bes\u00b7se\u00b7res", "Lied", "ver\u00b7nimmst", "du", "bald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Leb wohl, wir sehen uns wieder.", "tokens": ["Leb", "wohl", ",", "wir", "se\u00b7hen", "uns", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}