{"dta.poem.811": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.  \n Die H\u00f6lle.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! vnd weh! ", "tokens": ["Ach", "!", "vnd", "weh", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Mord! Zetter! Jammer! Angst! Creutz! Marter! W\u00fcrmel ", "tokens": ["Mord", "!", "Zet\u00b7ter", "!", "Jam\u00b7mer", "!", "Angst", "!", "Creutz", "!", "Mar\u00b7ter", "!", "W\u00fcr\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Plagen. ", "tokens": ["Pla\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Pech! Folter! Hencker! Flam\u0303! stanck! Geister! k\u00e4lte. Zagen!", "tokens": ["Pech", "!", "Fol\u00b7ter", "!", "Hen\u00b7cker", "!", "Flam\u0303", "!", "stanck", "!", "Geis\u00b7ter", "!", "k\u00e4l\u00b7te", ".", "Za\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "VVFIN", "$.", "NN", "$.", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach vergeh! ", "tokens": ["Ach", "ver\u00b7geh", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Tieff' vnd H\u00f6h'! ", "tokens": ["Tief\u00b7f'", "vnd", "H\u00f6h'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Meer! H\u00fcgel! Berge! Fel\u00df! wer kan die Pein ertragen?", "tokens": ["Meer", "!", "H\u00fc\u00b7gel", "!", "Ber\u00b7ge", "!", "Fel\u00df", "!", "wer", "kan", "die", "Pein", "er\u00b7tra\u00b7gen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NE", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schluck abgrund! ach schluck\u2019 eyn! die nichts denn ewig kla-", "tokens": ["Schluck", "ab\u00b7grund", "!", "ach", "schluck'", "eyn", "!", "die", "nichts", "denn", "e\u00b7wig", "kla"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "XY", "XY", "XY", "$.", "ART", "PIS", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gen. ", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Je vnd Eh! ", "tokens": ["Je", "vnd", "Eh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Schreckliche Geister der tunckelen h\u00f6len/ Jhr die jhr mar-", "tokens": ["Schreck\u00b7li\u00b7che", "Geis\u00b7ter", "der", "tun\u00b7cke\u00b7len", "h\u00f6\u00b7len", "/", "Ihr", "die", "jhr", "ma\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$(", "PPER", "ART", "PPOSAT", "TRUNC"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "tert vnd Marter erduldet ", "tokens": ["tert", "vnd", "Mar\u00b7ter", "er\u00b7dul\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kan denn der ewigen Ewigkeit Fewer/ nimmermehr b\u00fcssen", "tokens": ["Kan", "denn", "der", "e\u00b7wi\u00b7gen", "E\u00b7wig\u00b7keit", "Fe\u00b7wer", "/", "nim\u00b7mer\u00b7mehr", "b\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "NN", "$(", "ADV", "VVINF"], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "dis was jhr verschuldet? ", "tokens": ["dis", "was", "jhr", "ver\u00b7schul\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O grausamm' Angst/ stets sterben sonder sterben/ ", "tokens": ["O", "grau\u00b7samm'", "Angst", "/", "stets", "ster\u00b7ben", "son\u00b7der", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ADV", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Di\u00df ist die Flamme der grimmigen Rache/ die der erhitze-", "tokens": ["Di\u00df", "ist", "die", "Flam\u00b7me", "der", "grim\u00b7mi\u00b7gen", "Ra\u00b7che", "/", "die", "der", "er\u00b7hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$(", "PRELS", "ART", "TRUNC"], "meter": "-+-+--+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "te Zorn angeblasen: ", "tokens": ["te", "Zorn", "an\u00b7ge\u00b7bla\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hier ist der Fiuch der vnendlichen Straffe; hier ist das jm-", "tokens": ["Hier", "ist", "der", "Fiuch", "der", "vnend\u00b7li\u00b7chen", "Straf\u00b7fe", ";", "hier", "ist", "das", "jm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "merdar wachsende rasen: ", "tokens": ["mer\u00b7dar", "wach\u00b7sen\u00b7de", "ra\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "O Mensch! Verdirb/ vmb hier nicht zuverderben. ", "tokens": ["O", "Mensch", "!", "Ver\u00b7dirb", "/", "vmb", "hier", "nicht", "zu\u00b7ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$(", "APPR", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}