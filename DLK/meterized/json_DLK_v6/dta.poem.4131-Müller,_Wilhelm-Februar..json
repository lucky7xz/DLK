{"dta.poem.4131": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Februar.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit meiner Pritsche, meinem Schellenhut,", "tokens": ["Mit", "mei\u00b7ner", "Prit\u00b7sche", ",", "mei\u00b7nem", "Schel\u00b7len\u00b7hut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinem unerm\u00fcdlich krausen Muth,", "tokens": ["Mit", "mei\u00b7nem", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "krau\u00b7sen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll Scherz und Rank und Witz und Schadenfreude?", "tokens": ["Voll", "Scherz", "und", "Rank", "und", "Witz", "und", "Scha\u00b7den\u00b7freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch zapft man hier, zu meinem gro\u00dfen Leide,", "tokens": ["Doch", "zapft", "man", "hier", ",", "zu", "mei\u00b7nem", "gro\u00b7\u00dfen", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir j\u00e4hrlich ab ein Becken wildes Blut:", "tokens": ["Mir", "j\u00e4hr\u00b7lich", "ab", "ein", "Be\u00b7cken", "wil\u00b7des", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Humanitas meint es mit mir nicht gut,", "tokens": ["Hu\u00b7ma\u00b7ni\u00b7tas", "meint", "es", "mit", "mir", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und schw\u00e4rzt mich an mit unhumanem Neide.", "tokens": ["Und", "schw\u00e4rzt", "mich", "an", "mit", "un\u00b7hu\u00b7ma\u00b7nem", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich darf nicht mehr frei durch die Stra\u00dfe wandern,", "tokens": ["Ich", "darf", "nicht", "mehr", "frei", "durch", "die", "Stra\u00b7\u00dfe", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In enge S\u00e4le schlie\u00dfen sie mich ein,", "tokens": ["In", "en\u00b7ge", "S\u00e4\u00b7le", "schlie\u00b7\u00dfen", "sie", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wollen gar, ich soll vern\u00fcnftig sein.", "tokens": ["Und", "wol\u00b7len", "gar", ",", "ich", "soll", "ver\u00b7n\u00fcnf\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$,", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie thut mir's weh um dich vor allen Andern!", "tokens": ["Wie", "thut", "mir's", "weh", "um", "dich", "vor", "al\u00b7len", "An\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADV", "APPR", "PRF", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich m\u00f6chte gern dich r\u00f6misch lustig sehn,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "gern", "dich", "r\u00f6\u00b7misch", "lus\u00b7tig", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00fc\u00dft' ich selbst dabei zu Grunde gehn.", "tokens": ["Und", "m\u00fc\u00dft'", "ich", "selbst", "da\u00b7bei", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}