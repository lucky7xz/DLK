{"textgrid.poem.58672": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gnadenmittel sind wie Adern an dem Leib,", "tokens": ["Die", "Gna\u00b7den\u00b7mit\u00b7tel", "sind", "wie", "A\u00b7dern", "an", "dem", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das \u00e4u\u00dfre Fleisch und Haut sind Lehr' und Werk' und Pflichten,", "tokens": ["Das", "\u00e4u\u00df\u00b7re", "Fleisch", "und", "Haut", "sind", "Lehr'", "und", "Werk'", "und", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die auch ein toter Heid' kann von Natur verrichten;", "tokens": ["Die", "auch", "ein", "to\u00b7ter", "Heid'", "kann", "von", "Na\u00b7tur", "ver\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich leb' nicht wie ein Christ, solang ich so nur bleib'.", "tokens": ["Ich", "leb'", "nicht", "wie", "ein", "Christ", ",", "so\u00b7lang", "ich", "so", "nur", "bleib'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geist ist es, der mir fehlt, sonst hilft mir nichts, f\u00fcrwahr,", "tokens": ["Geist", "ist", "es", ",", "der", "mir", "fehlt", ",", "sonst", "hilft", "mir", "nichts", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIS", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schein ist es ohne Sein, ein Leichnam ohne Leben,", "tokens": ["Schein", "ist", "es", "oh\u00b7ne", "Sein", ",", "ein", "Leich\u00b7nam", "oh\u00b7ne", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPOSAT", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gott mu\u00df mir Jesu Geist in meinem Innern geben,", "tokens": ["Gott", "mu\u00df", "mir", "Je\u00b7su", "Geist", "in", "mei\u00b7nem", "In\u00b7nern", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NE", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ich wie Jesus leb' und seine Kraft erfahr.", "tokens": ["Da\u00df", "ich", "wie", "Je\u00b7sus", "leb'", "und", "sei\u00b7ne", "Kraft", "er\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}