{"textgrid.poem.42972": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Leben wie im Karneval", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder summt sein S\u00fcmmchen", "tokens": ["Je\u00b7der", "summt", "sein", "S\u00fcmm\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Oder brummt sein Br\u00fcmmchen", "tokens": ["O\u00b7der", "brummt", "sein", "Br\u00fcmm\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein B\u00e4r oder wie ein Bienchen,", "tokens": ["Wie", "ein", "B\u00e4r", "o\u00b7der", "wie", "ein", "Bien\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "PWAV", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn er ganz in sich", "tokens": ["Wenn", "er", "ganz", "in", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hind\u00f6st. \u2013 Aber \u00f6ffentlich", "tokens": ["Hin\u00b7d\u00f6st", ".", "\u2013", "A\u00b7ber", "\u00f6f\u00b7fent\u00b7lich"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["ADV", "$.", "$(", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht dann jeder, jede,", "tokens": ["Zieht", "dann", "je\u00b7der", ",", "je\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$,", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jedes sein Mienchen. \u2013 \u2013 \u2013", "tokens": ["Je\u00b7des", "sein", "Mien\u00b7chen", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$.", "$(", "$(", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "(f\u00e4llt mir pl\u00f6tzlich ein Gerede", "tokens": ["(", "f\u00e4llt", "mir", "pl\u00f6tz\u00b7lich", "ein", "Ge\u00b7re\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein, eines Arztes mit schizophrenen Fraun.", "tokens": ["Ein", ",", "ei\u00b7nes", "Arz\u00b7tes", "mit", "schi\u00b7zop\u00b7hre\u00b7nen", "Fraun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hielt der Arzt sie heimlich lieb am Z\u00fcgel.", "tokens": ["Hielt", "der", "Arzt", "sie", "heim\u00b7lich", "lieb", "am", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sagte eine: \u00bbH\u00e4ngen Sie meinen", "tokens": ["Sag\u00b7te", "ei\u00b7ne", ":", "\u00bb", "H\u00e4n\u00b7gen", "Sie", "mei\u00b7nen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "$.", "$(", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Linken Lungenfl\u00fcgel", "tokens": ["Lin\u00b7ken", "Lun\u00b7gen\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "An den Gartenzaun!\u00ab)", "tokens": ["An", "den", "Gar\u00b7ten\u00b7zaun", "!", "\u00ab", ")"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "$("], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Jedes flucht sein Fl\u00fcchlein,", "tokens": ["Je\u00b7des", "flucht", "sein", "Fl\u00fcch\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wenn's nicht ging, wie's ihmnach gehen soll.", "tokens": ["Wenn's", "nicht", "ging", ",", "wie's", "ihm\u00b7nach", "ge\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Manches weint ein T\u00fcchlein", "tokens": ["Man\u00b7ches", "weint", "ein", "T\u00fcch\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Oder schei\u00dft ein H\u00f6slein voll.", "tokens": ["O\u00b7der", "schei\u00dft", "ein", "H\u00f6s\u00b7lein", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das st\u00f6rend niedrige Geschmei\u00df", "tokens": ["Das", "st\u00f6\u00b7rend", "nied\u00b7ri\u00b7ge", "Ge\u00b7schmei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist schwierig zu erreichen.", "tokens": ["Ist", "schwie\u00b7rig", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es bleibt Gesetz: Die Schnake wei\u00df,", "tokens": ["Es", "bleibt", "Ge\u00b7setz", ":", "Die", "Schna\u00b7ke", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Kuhschwanz auszuweichen.", "tokens": ["Dem", "Kuh\u00b7schwanz", "aus\u00b7zu\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}