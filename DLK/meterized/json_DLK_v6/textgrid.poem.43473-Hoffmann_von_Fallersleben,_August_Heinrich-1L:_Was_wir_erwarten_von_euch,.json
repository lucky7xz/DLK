{"textgrid.poem.43473": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was wir erwarten von euch,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wir erwarten von euch,", "tokens": ["Was", "wir", "er\u00b7war\u00b7ten", "von", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ihr Hochgelahrten und Weisen?", "tokens": ["Ihr", "Hoch\u00b7ge\u00b7lahr\u00b7ten", "und", "Wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "H\u00f6rt es von uns nun einmal!", "tokens": ["H\u00f6rt", "es", "von", "uns", "nun", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Andre Dinge gewi\u00df,", "tokens": ["And\u00b7re", "Din\u00b7ge", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Als da\u00df ihr von eurer Regierung", "tokens": ["Als", "da\u00df", "ihr", "von", "eu\u00b7rer", "Re\u00b7gie\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Immer und ewiglich hofft.", "tokens": ["Im\u00b7mer", "und", "e\u00b7wig\u00b7lich", "hofft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+-++", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Was von jener ihr hofft,", "tokens": ["Was", "von", "je\u00b7ner", "ihr", "hofft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das wissen leider wir l\u00e4ngst schon:", "tokens": ["Das", "wis\u00b7sen", "lei\u00b7der", "wir", "l\u00e4ngst", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Titel und Orden und Geld.", "tokens": ["Ti\u00b7tel", "und", "Or\u00b7den", "und", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Was wir erwarten von euch,", "tokens": ["Was", "wir", "er\u00b7war\u00b7ten", "von", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ihr Hochgelahrten und Weisen?", "tokens": ["Ihr", "Hoch\u00b7ge\u00b7lahr\u00b7ten", "und", "Wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hofft nicht und leistet vielmehr.", "tokens": ["Hofft", "nicht", "und", "leis\u00b7tet", "viel\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KON", "VVFIN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}