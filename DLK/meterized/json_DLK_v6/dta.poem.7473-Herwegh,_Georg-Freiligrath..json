{"dta.poem.7473": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Freiligrath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel fing von Neuem an zu blauen,", "tokens": ["Der", "Him\u00b7mel", "fing", "von", "Neu\u00b7em", "an", "zu", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Winter sich zum Abmarsch anzuschicken,", "tokens": ["Der", "Win\u00b7ter", "sich", "zum", "Ab\u00b7marsch", "an\u00b7zu\u00b7schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Erde sich mit jungem Gr\u00fcn zu sticken, \u2014", "tokens": ["Die", "Er\u00b7de", "sich", "mit", "jun\u00b7gem", "Gr\u00fcn", "zu", "sti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich nahm Dein Buch, recht tief darein zu schauen.", "tokens": ["Ich", "nahm", "Dein", "Buch", ",", "recht", "tief", "da\u00b7rein", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mich erfa\u00dft ein heimlich l\u00fcstern Grauen;", "tokens": ["Und", "mich", "er\u00b7fa\u00dft", "ein", "heim\u00b7lich", "l\u00fcs\u00b7tern", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh' die alten Strau\u00dfenfedern nicken,", "tokens": ["Ich", "seh'", "die", "al\u00b7ten", "Strau\u00b7\u00dfen\u00b7fe\u00b7dern", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und glaub' in Tausend Eine Nacht zu blicken \u2014", "tokens": ["Und", "glaub'", "in", "Tau\u00b7send", "Ei\u00b7ne", "Nacht", "zu", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier, denk' ich, w\u00e4ren so f\u00fcr mich die Frauen!", "tokens": ["Hier", ",", "denk'", "ich", ",", "w\u00e4\u00b7ren", "so", "f\u00fcr", "mich", "die", "Frau\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da bringt mein M\u00e4dchen mir die ersten Veilchen,", "tokens": ["Da", "bringt", "mein", "M\u00e4d\u00b7chen", "mir", "die", "ers\u00b7ten", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im blauen Shawl, im leichten Rosakleide,", "tokens": ["Im", "blau\u00b7en", "Shawl", ",", "im", "leich\u00b7ten", "Ro\u00b7sak\u00b7lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die weiche Hand das Einzige von Seide.", "tokens": ["Die", "wei\u00b7che", "Hand", "das", "Ein\u00b7zi\u00b7ge", "von", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein Orient ruht wieder auf ein Weilchen;", "tokens": ["Dein", "O\u00b7rient", "ruht", "wie\u00b7der", "auf", "ein", "Weil\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Herz, kaum nach der Fremde so begehrlich,", "tokens": ["Mein", "Herz", ",", "kaum", "nach", "der", "Frem\u00b7de", "so", "be\u00b7gehr\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleibt gern im Lande nun und n\u00e4hrt sich ehrlich.", "tokens": ["Bleibt", "gern", "im", "Lan\u00b7de", "nun", "und", "n\u00e4hrt", "sich", "ehr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADV", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}