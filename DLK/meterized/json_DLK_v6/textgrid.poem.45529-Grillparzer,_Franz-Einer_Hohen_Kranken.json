{"textgrid.poem.45529": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Einer Hohen Kranken", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Krank bist du: einen w\u00e4rmern Aufenthalt", "tokens": ["Krank", "bist", "du", ":", "ei\u00b7nen", "w\u00e4r\u00b7mern", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "R\u00e4t mit dem Arzt der Sohn, und er erbats.", "tokens": ["R\u00e4t", "mit", "dem", "Arzt", "der", "Sohn", ",", "und", "er", "er\u00b7bats", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wem allzu rauh die Luft ist der Gewalt,", "tokens": ["Wem", "all\u00b7zu", "rauh", "die", "Luft", "ist", "der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "ADJD", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der flieht zum mildern Klima des Verrats.", "tokens": ["Der", "flieht", "zum", "mil\u00b7dern", "Kli\u00b7ma", "des", "Ver\u00b7rats", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}