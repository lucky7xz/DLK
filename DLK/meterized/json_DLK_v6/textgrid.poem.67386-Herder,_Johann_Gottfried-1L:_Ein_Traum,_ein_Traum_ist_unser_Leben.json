{"textgrid.poem.67386": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Traum, ein Traum ist unser Leben", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Traum, ein Traum ist unser Leben", "tokens": ["Ein", "Traum", ",", "ein", "Traum", "ist", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Erden hier.", "tokens": ["Auf", "Er\u00b7den", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie Schatten auf den Wogen schweben", "tokens": ["Wie", "Schat\u00b7ten", "auf", "den", "Wo\u00b7gen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwinden wir", "tokens": ["Und", "schwin\u00b7den", "wir"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und messen unsre tr\u00e4gen Tritte", "tokens": ["Und", "mes\u00b7sen", "uns\u00b7re", "tr\u00e4\u00b7gen", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Raum und Zeit;", "tokens": ["Nach", "Raum", "und", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und sind (und wissen's nicht) in Mitte", "tokens": ["Und", "sind", "(", "und", "wis\u00b7sen's", "nicht", ")", "in", "Mit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "$(", "KON", "VVFIN", "PTKNEG", "$(", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Ewigkeit.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nach manchem voller M\u00fch und Sehnen", "tokens": ["Nach", "man\u00b7chem", "vol\u00b7ler", "M\u00fch", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verseufzten Jahr", "tokens": ["Ver\u00b7seufz\u00b7ten", "Jahr"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Umarmte sich in frohen Thr\u00e4nen", "tokens": ["Um\u00b7arm\u00b7te", "sich", "in", "fro\u00b7hen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein liebend Paar.", "tokens": ["Ein", "lie\u00b7bend", "Paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der Mond sah freundlich auf sie nieder;", "tokens": ["Der", "Mond", "sah", "freund\u00b7lich", "auf", "sie", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein zarter Ton", "tokens": ["Ein", "zar\u00b7ter", "Ton"], "token_info": ["word", "word", "word"], "pos": ["ART", "AD