{"textgrid.poem.39486": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht blo\u00df spielt aus des Sonnenstrahles Reine", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht blo\u00df spielt aus des Sonnenstrahles Reine", "tokens": ["Nicht", "blo\u00df", "spielt", "aus", "des", "Son\u00b7nen\u00b7strah\u00b7les", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze Farbenwelt; die gl\u00fch'nde Helle", "tokens": ["Die", "gan\u00b7ze", "Far\u00b7ben\u00b7welt", ";", "die", "gl\u00fch'n\u00b7de", "Hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird auch auf Erden hoher Farben Quelle,", "tokens": ["Wird", "auch", "auf", "Er\u00b7den", "ho\u00b7her", "Far\u00b7ben", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sie hervorruft, gleich als Widerscheine.", "tokens": ["Die", "sie", "her\u00b7vor\u00b7ruft", ",", "gleich", "als", "Wi\u00b7der\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da brennen Blumen, regt durch goldne Haine", "tokens": ["Da", "bren\u00b7nen", "Blu\u00b7men", ",", "regt", "durch", "gold\u00b7ne", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich des Gefieders tausendf\u00e4rb'ge Welle,", "tokens": ["Sich", "des", "Ge\u00b7fie\u00b7ders", "tau\u00b7send\u00b7f\u00e4r\u00b7b'\u00b7ge", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Raubthier schleicht in buntgestreiftem Felle;", "tokens": ["Das", "Raubt\u00b7hier", "schleicht", "in", "bunt\u00b7ge\u00b7streif\u00b7tem", "Fel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in der Tiefe funkeln edle Steine.", "tokens": ["Und", "in", "der", "Tie\u00b7fe", "fun\u00b7keln", "ed\u00b7le", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So reift der Liebe Glut und hei\u00df Err\u00f6then,", "tokens": ["So", "reift", "der", "Lie\u00b7be", "Glut", "und", "hei\u00df", "Er\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Sonnenkraft die irdischen Naturen,", "tokens": ["Wie", "Son\u00b7nen\u00b7kraft", "die", "ir\u00b7di\u00b7schen", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Farbenglanz der Phantasie Gebilde.", "tokens": ["Zum", "Far\u00b7ben\u00b7glanz", "der", "Phan\u00b7ta\u00b7sie", "Ge\u00b7bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr ebnen sich smaragdner die Gefilde,", "tokens": ["Ihr", "eb\u00b7nen", "sich", "sma\u00b7ragd\u00b7ner", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr w\u00f6lbt der Himmel voller die Azuren,", "tokens": ["Ihr", "w\u00f6lbt", "der", "Him\u00b7mel", "vol\u00b7ler", "die", "A\u00b7zu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo sch\u00f6ner zuckend auch die Blitze t\u00f6dten.", "tokens": ["Wo", "sch\u00f6\u00b7ner", "zu\u00b7ckend", "auch", "die", "Blit\u00b7ze", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}