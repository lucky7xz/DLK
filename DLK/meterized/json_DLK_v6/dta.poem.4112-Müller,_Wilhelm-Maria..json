{"dta.poem.4112": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Maria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz hat stets dich so genannt. \u2014", "tokens": ["Mein", "Herz", "hat", "stets", "dich", "so", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh' ich ein klares B\u00e4chlein flie\u00dfen,", "tokens": ["Seh'", "ich", "ein", "kla\u00b7res", "B\u00e4ch\u00b7lein", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Setz' ich mich still an seinen Rand:", "tokens": ["Setz'", "ich", "mich", "still", "an", "sei\u00b7nen", "Rand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Maria, rieseln seine Wogen,", "tokens": ["Ma\u00b7ria", ",", "rie\u00b7seln", "sei\u00b7ne", "Wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Maria soll ihr Name sein;", "tokens": ["Ma\u00b7ria", "soll", "ihr", "Na\u00b7me", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein wei\u00dfes T\u00e4ubchen kommt geflogen,", "tokens": ["Ein", "wei\u00b7\u00dfes", "T\u00e4ub\u00b7chen", "kommt", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schwebt \u00fcber mir im Sonnenschein.", "tokens": ["Schwebt", "\u00fc\u00b7ber", "mir", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geliebte, hast du nichts vernommen,", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "hast", "du", "nichts", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Orgelton und Wasserfall?", "tokens": ["Wie", "Or\u00b7gel\u00b7ton", "und", "Was\u00b7ser\u00b7fall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der heil'ge Jordan kommt geschwommen", "tokens": ["Der", "heil'\u00b7ge", "Jor\u00b7dan", "kommt", "ge\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Berg und Meer mit Jubelschall.", "tokens": ["Durch", "Berg", "und", "Meer", "mit", "Ju\u00b7bel\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Geist des Herrn schwingt sein Gefieder", "tokens": ["Der", "Geist", "des", "Herrn", "schwingt", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ruft: Wo ist die Tochter mein?", "tokens": ["Und", "ruft", ":", "Wo", "ist", "die", "Toch\u00b7ter", "mein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tauch' in die Liebesfluthen nieder:", "tokens": ["Tauch'", "in", "die", "Lie\u00b7bes\u00b7flut\u00b7hen", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Maria soll dein Name sein!", "tokens": ["Ma\u00b7ria", "soll", "dein", "Na\u00b7me", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}