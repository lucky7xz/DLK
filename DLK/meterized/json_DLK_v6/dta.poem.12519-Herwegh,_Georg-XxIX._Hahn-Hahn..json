{"dta.poem.12519": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.  \n  Hahn-Hahn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201elauter \u00e4chte Vollblut-K\u00fcchlein", "tokens": ["\u201e", "lau\u00b7ter", "\u00e4ch\u00b7te", "Voll\u00b7blut\u00b7K\u00fcch\u00b7lein"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PIAT", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "zog ich in den letzten Wintern:", "tokens": ["zog", "ich", "in", "den", "letz\u00b7ten", "Win\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zum Beweise tragen alle", "tokens": ["Zum", "Be\u00b7wei\u00b7se", "tra\u00b7gen", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "noch die Eierschaal' am Hintern.\u201c", "tokens": ["noch", "die", "Ei\u00b7er\u00b7schaal'", "am", "Hin\u00b7tern", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}