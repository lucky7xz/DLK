{"textgrid.poem.54121": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Parteimarsch der Parteilosen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da streiten sich die Leute rum:", "tokens": ["Da", "strei\u00b7ten", "sich", "die", "Leu\u00b7te", "rum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Jejner w\u00e4ren imma dumm \u2013", "tokens": ["die", "Jej\u00b7ner", "w\u00e4\u00b7ren", "im\u00b7ma", "dumm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Is ja jahnich wahr!", "tokens": ["Is", "ja", "jah\u00b7nich", "wahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Un wie se alle br\u00fclln un schrein,", "tokens": ["Un", "wie", "se", "al\u00b7le", "br\u00fclln", "un", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und jeda sacht, det mu\u00df so sein \u2013", "tokens": ["und", "je\u00b7da", "sacht", ",", "det", "mu\u00df", "so", "sein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PDS", "VMFIN", "ADV", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Is ja jahnich wahr!", "tokens": ["Is", "ja", "jah\u00b7nich", "wahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nu sieh ma unsereinen an,", "tokens": ["Nu", "sieh", "ma", "un\u00b7ser\u00b7ei\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "vaehrtet Publikum!", "tokens": ["va\u00b7ehr\u00b7tet", "Pub\u00b7li\u00b7kum", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wir treten vor dich Mann fier Mann", "tokens": ["Wir", "tre\u00b7ten", "vor", "dich", "Mann", "fier", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "als Individium.", "tokens": ["als", "In\u00b7di\u00b7vi\u00b7di\u00b7um", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es t\u00f6nt die Stra\u00dfen lang", "tokens": ["Es", "t\u00f6nt", "die", "Stra\u00b7\u00dfen", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "der herrliche Jesang:", "tokens": ["der", "herr\u00b7li\u00b7che", "Je\u00b7sang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wir brauchen keine Innung,", "tokens": ["Wir", "brau\u00b7chen", "kei\u00b7ne", "In\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "wir brauchn kein Vaein!", "tokens": ["wir", "brau\u00b7chn", "kein", "Va\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Wir machn uns \u2013 wir machn uns", "tokens": ["Wir", "machn", "uns", "\u2013", "wir", "machn", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "unsan Dreck allein!", "tokens": ["un\u00b7san", "Dreck", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Wir ham doch die Jesinnung", "tokens": ["Wir", "ham", "doch", "die", "Je\u00b7sin\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "un ooch die St\u00e4nkerein \u2013", "tokens": ["un", "o\u00b7och", "die", "St\u00e4n\u00b7ke\u00b7rein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "drum brauchn wa keene Innung", "tokens": ["drum", "brau\u00b7chn", "wa", "kee\u00b7ne", "In\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "und brauchn auch keen Vaein \u2013!", "tokens": ["und", "brau\u00b7chn", "auch", "ke\u00b7en", "Va\u00b7ein", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$(", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem eenen weht die Fahne rot \u2013", "tokens": ["Dem", "e\u00b7e\u00b7nen", "weht", "die", "Fah\u00b7ne", "rot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "un wer nich mitmacht, isn Idiot . . .", "tokens": ["un", "wer", "nich", "mit\u00b7macht", ",", "isn", "I\u00b7diot", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVPP", "$,", "NE", "NE", "$.", "$.", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Is ja kaum ze jlohm!", "tokens": ["Is", "ja", "kaum", "ze", "jlohm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der annre hat 'n braunet Hemd;", "tokens": ["Der", "ann\u00b7re", "hat", "'n", "brau\u00b7net", "Hemd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "det heest: det hat a sich jeklemmt", "tokens": ["det", "heest", ":", "det", "hat", "a", "sich", "je\u00b7klemmt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "PDS", "VAFIN", "NE", "PRF", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "aus Rom.", "tokens": ["aus", "Rom", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Der dritte, der sitzt mittenmang,", "tokens": ["Der", "drit\u00b7te", ",", "der", "sitzt", "mit\u00b7ten\u00b7mang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die Hosn m\u00e4chtig voll.", "tokens": ["die", "Hosn", "m\u00e4ch\u00b7tig", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.9": {"text": "Nur wir, wir wissen janz jenau,", "tokens": ["Nur", "wir", ",", "wir", "wis\u00b7sen", "janz", "je\u00b7nau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "wat jeda machn soll.", "tokens": ["wat", "je\u00b7da", "machn", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Wir ssiehn vajn\u00fccht vorbei.", "tokens": ["Wir", "ssiehn", "va\u00b7jn\u00fccht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wir sinn die Nullpachtei . . . !", "tokens": ["Wir", "sinn", "die", "Null\u00b7pach\u00b7tei", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wir brauchen keine Innung,", "tokens": ["Wir", "brau\u00b7chen", "kei\u00b7ne", "In\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "wir brauchen kein Vaein!", "tokens": ["wir", "brau\u00b7chen", "kein", "Va\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Wir machn uns, wir machn uns", "tokens": ["Wir", "machn", "uns", ",", "wir", "machn", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "unsan Dreck allein!", "tokens": ["un\u00b7san", "Dreck", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Wir ham doch die Jesinnung", "tokens": ["Wir", "ham", "doch", "die", "Je\u00b7sin\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "un ooch die St\u00e4nkerein \u2013", "tokens": ["un", "o\u00b7och", "die", "St\u00e4n\u00b7ke\u00b7rein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "drum brauchn wa keene Innung", "tokens": ["drum", "brau\u00b7chn", "wa", "kee\u00b7ne", "In\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "und brauchn auch keen Vaein \u2013!", "tokens": ["und", "brau\u00b7chn", "auch", "ke\u00b7en", "Va\u00b7ein", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$(", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}