{"dta.poem.15825": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Mitleid hab' ich heut' ein schlechtes Buch gelesen,", "tokens": ["Aus", "Mit\u00b7leid", "hab'", "ich", "heut'", "ein", "schlech\u00b7tes", "Buch", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ein vortreffliches zu seiner Zeit gewesen.", "tokens": ["Das", "ein", "vor\u00b7treff\u00b7li\u00b7ches", "zu", "sei\u00b7ner", "Zeit", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So f\u00fchrt ein Junggesell zum Tanz aus Christenliebe", "tokens": ["So", "f\u00fchrt", "ein", "Jung\u00b7ge\u00b7sell", "zum", "Tanz", "aus", "Chris\u00b7ten\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein altes J\u00fcngferchen, das ohn' ihn sitzen bliebe.", "tokens": ["Ein", "al\u00b7tes", "J\u00fcng\u00b7fer\u00b7chen", ",", "das", "ohn'", "ihn", "sit\u00b7zen", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure"