{"textgrid.poem.52668": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Jugend", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging vorbei am Gotteshaus,", "tokens": ["Ich", "ging", "vor\u00b7bei", "am", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darin ich lang nicht war,", "tokens": ["Da\u00b7rin", "ich", "lang", "nicht", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Orgel l\u00e4rmte waldhinaus", "tokens": ["Die", "Or\u00b7gel", "l\u00e4rm\u00b7te", "wald\u00b7hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So feierlich und klar.", "tokens": ["So", "fei\u00b7er\u00b7lich", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gar d\u00fcster sah die Kirche drein", "tokens": ["Gar", "d\u00fcs\u00b7ter", "sah", "die", "Kir\u00b7che", "drein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrem morschen Thurm,", "tokens": ["Mit", "ih\u00b7rem", "mor\u00b7schen", "Thurm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch rastlos dr\u00e4nget aus und ein", "tokens": ["Doch", "rast\u00b7los", "dr\u00e4n\u00b7get", "aus", "und", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der frommen Menge Sturm.", "tokens": ["Der", "from\u00b7men", "Men\u00b7ge", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Horch! jetzt verstummt der Feierklang", "tokens": ["Horch", "!", "jetzt", "ver\u00b7stummt", "der", "Fei\u00b7er\u00b7klang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruhig wirds umher,", "tokens": ["Und", "ru\u00b7hig", "wirds", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Menge betet still und lang,", "tokens": ["Die", "Men\u00b7ge", "be\u00b7tet", "still", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir wird es auch nicht schwer.", "tokens": ["Mir", "wird", "es", "auch", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist ein Abend freundlich lind,", "tokens": ["Es", "ist", "ein", "A\u00b7bend", "freund\u00b7lich", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winde schlummern all,", "tokens": ["Die", "Win\u00b7de", "schlum\u00b7mern", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn erst die Winde stille sind,", "tokens": ["Wenn", "erst", "die", "Win\u00b7de", "stil\u00b7le", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vernehm ich innern Schall.", "tokens": ["Ver\u00b7nehm", "ich", "in\u00b7nern", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In meiner Seele Tiefen singt", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein guter Genius,", "tokens": ["Mein", "gu\u00b7ter", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Aus meines Herzens Kammern springt,", "tokens": ["Aus", "mei\u00b7nes", "Her\u00b7zens", "Kam\u00b7mern", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befreit sich ein Entschlu\u00df.", "tokens": ["Be\u00b7freit", "sich", "ein", "Ent\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}