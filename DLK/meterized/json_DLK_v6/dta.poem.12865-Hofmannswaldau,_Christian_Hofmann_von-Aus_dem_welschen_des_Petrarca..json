{"dta.poem.12865": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem welschen des   Petrarca.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djs ist die w\u00fcsteney, die sich mein fu\u00df erkiest,", "tokens": ["Djs", "ist", "die", "w\u00fcs\u00b7te\u00b7ney", ",", "die", "sich", "mein", "fu\u00df", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "PPOSAT", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich des hertzens qvaal mit stillen lippen klage,", "tokens": ["Wenn", "ich", "des", "hert\u00b7zens", "qvaal", "mit", "stil\u00b7len", "lip\u00b7pen", "kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich, der ich voller furcht noch wol den augen sage,", "tokens": ["Ich", ",", "der", "ich", "vol\u00b7ler", "furcht", "noch", "wol", "den", "au\u00b7gen", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich flei\u00dfig umzusehn, ob alles einsam ist.", "tokens": ["Sich", "flei\u00b7\u00dfig", "um\u00b7zu\u00b7sehn", ",", "ob", "al\u00b7les", "ein\u00b7sam", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVIZU", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wo du, einsamkeit! mir deinen schutz entziehst,", "tokens": ["Denn", "wo", "du", ",", "ein\u00b7sam\u00b7keit", "!", "mir", "dei\u00b7nen", "schutz", "ent\u00b7ziehst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "NN", "$.", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sieht die gantze welt, was ich im hertzen trage;", "tokens": ["So", "sieht", "die", "gant\u00b7ze", "welt", ",", "was", "ich", "im", "hert\u00b7zen", "tra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil man den klaren grund der tief-verborgnen plage", "tokens": ["Weil", "man", "den", "kla\u00b7ren", "grund", "der", "tie\u00b7fver\u00b7bor\u00b7gnen", "pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Leicht aus der nassen schrifft der tr\u00fcben augen liest.", "tokens": ["Leicht", "aus", "der", "nas\u00b7sen", "schrifft", "der", "tr\u00fc\u00b7ben", "au\u00b7gen", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wiewol, ob meine pein gleich keine menschen schauen,", "tokens": ["Wie\u00b7wol", ",", "ob", "mei\u00b7ne", "pein", "gleich", "kei\u00b7ne", "men\u00b7schen", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So keunet ihr sie doch, ihr felsen, p\u00fcsch\u2019 und auen!", "tokens": ["So", "keu\u00b7net", "ihr", "sie", "doch", ",", "ihr", "fel\u00b7sen", ",", "p\u00fcsch'", "und", "au\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zum minsten hab ich noch kein so gar einsam feld,", "tokens": ["Zum", "mins\u00b7ten", "hab", "ich", "noch", "kein", "so", "gar", "ein\u00b7sam", "feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "PIAT", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wie sehr ich mich bem\u00fcht, in dieser w\u00fcst\u2019 ergr\u00fcndet,", "tokens": ["Wie", "sehr", "ich", "mich", "be\u00b7m\u00fcht", ",", "in", "die\u00b7ser", "w\u00fcst'", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "VVPP", "$,", "APPR", "PDAT", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da sich die liebe nicht an meiner seite findet,", "tokens": ["Da", "sich", "die", "lie\u00b7be", "nicht", "an", "mei\u00b7ner", "sei\u00b7te", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unver\u00e4nderlich mit mir gespr\u00e4che h\u00e4lt.", "tokens": ["Und", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "mit", "mir", "ge\u00b7spr\u00e4\u00b7che", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}