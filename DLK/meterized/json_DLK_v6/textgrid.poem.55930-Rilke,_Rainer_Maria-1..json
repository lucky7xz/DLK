{"textgrid.poem.55930": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Hand hat nur noch eine", "tokens": ["Mei\u00b7ne", "Hand", "hat", "nur", "noch", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geb\u00e4rde, mit der sie verscheucht;", "tokens": ["Ge\u00b7b\u00e4r\u00b7de", ",", "mit", "der", "sie", "ver\u00b7scheucht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auf die alten Steine", "tokens": ["auf", "die", "al\u00b7ten", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "f\u00e4llt es aus Felsen feucht.", "tokens": ["f\u00e4llt", "es", "aus", "Fel\u00b7sen", "feucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re nur dieses Klopfen", "tokens": ["Ich", "h\u00f6\u00b7re", "nur", "die\u00b7ses", "Klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und mein Herz h\u00e4lt Schritt", "tokens": ["und", "mein", "Herz", "h\u00e4lt", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "mit dem Gehen der Tropfen", "tokens": ["mit", "dem", "Ge\u00b7hen", "der", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "und vergeht damit.", "tokens": ["und", "ver\u00b7geht", "da\u00b7mit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Tropften sie doch schneller,", "tokens": ["Tropf\u00b7ten", "sie", "doch", "schnel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "k\u00e4me doch wieder ein Tier.", "tokens": ["k\u00e4\u00b7me", "doch", "wie\u00b7der", "ein", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Irgendwo war es heller \u2013.", "tokens": ["Ir\u00b7gend\u00b7wo", "war", "es", "hel\u00b7ler", "\u2013", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aber was wissen wir.", "tokens": ["A\u00b7ber", "was", "wis\u00b7sen", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}