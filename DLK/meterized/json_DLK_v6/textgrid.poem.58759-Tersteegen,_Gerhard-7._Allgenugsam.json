{"textgrid.poem.58759": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "7. Allgenugsam", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du nicht bist, kannst du entbehrn,", "tokens": ["Was", "du", "nicht", "bist", ",", "kannst", "du", "ent\u00b7behrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VAFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dein Denken, Lieben und Begehrn", "tokens": ["Dein", "Den\u00b7ken", ",", "Lie\u00b7ben", "und", "Be\u00b7gehrn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat alles schon vollkommen drinnen.", "tokens": ["Hat", "al\u00b7les", "schon", "voll\u00b7kom\u00b7men", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist dir g'nug, doch suchst du mich;", "tokens": ["Du", "bist", "dir", "g'\u00b7nug", ",", "doch", "suchst", "du", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ich such' und kann nicht missen dich,", "tokens": ["Ich", "such'", "und", "kann", "nicht", "mis\u00b7sen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was h\u00fclf's, k\u00f6nnt' ich die Welt gewinnen!", "tokens": ["Was", "h\u00fcl\u00b7f's", ",", "k\u00f6nnt'", "ich", "die", "Welt", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In mir und all'm ist D\u00fcrftigkeit,", "tokens": ["In", "mir", "und", "all'm", "ist", "D\u00fcrf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADJD", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du bist mein' Allgenugsamkeit.", "tokens": ["Du", "bist", "mein'", "All\u00b7ge\u00b7nu\u00b7gsam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}