{"textgrid.poem.33559": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebrochen ist dein Herz, ich wei\u00df es wohl,", "tokens": ["Ge\u00b7bro\u00b7chen", "ist", "dein", "Herz", ",", "ich", "wei\u00df", "es", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6r' ja die zerrissne Saite schwirren,", "tokens": ["Ich", "h\u00f6r'", "ja", "die", "zer\u00b7riss\u00b7ne", "Sai\u00b7te", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh' den Wahnsinn, o ich seh' ihn wohl", "tokens": ["Ich", "seh'", "den", "Wahn\u00b7sinn", ",", "o", "ich", "seh'", "ihn", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "FM", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch deiner Wimpern Nacht im Dunkel irren.", "tokens": ["Durch", "dei\u00b7ner", "Wim\u00b7pern", "Nacht", "im", "Dun\u00b7kel", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O deine Hand ist kalt, und Fieberglut", "tokens": ["O", "dei\u00b7ne", "Hand", "ist", "kalt", ",", "und", "Fie\u00b7berg\u00b7lut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchras't den Puls; von deinem sch\u00f6nen Leben", "tokens": ["Durch\u00b7ras't", "den", "Puls", ";", "von", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blieb nichts mehr, als ein stolzer Todesmut", "tokens": ["Blieb", "nichts", "mehr", ",", "als", "ein", "stol\u00b7zer", "To\u00b7des\u00b7mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deiner Lippen schmerzliches Vergeben.", "tokens": ["Und", "dei\u00b7ner", "Lip\u00b7pen", "schmerz\u00b7li\u00b7ches", "Ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Zug um deine Lippen ist so hart", "tokens": ["Ein", "Zug", "um", "dei\u00b7ne", "Lip\u00b7pen", "ist", "so", "hart"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie dein Los so voll der herbsten Herbe.", "tokens": ["Und", "wie", "dein", "Los", "so", "voll", "der", "herbs\u00b7ten", "Her\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du l\u00e4chelst, doch dein L\u00e4cheln ist erstarrt,", "tokens": ["Du", "l\u00e4\u00b7chelst", ",", "doch", "dein", "L\u00e4\u00b7cheln", "ist", "er\u00b7starrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es zeigt nur, da\u00df es noch mit Anmut sterbe.", "tokens": ["Es", "zeigt", "nur", ",", "da\u00df", "es", "noch", "mit", "An\u00b7mut", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}