{"dta.poem.13291": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein F\u00fchrer redte nichts, er wies nur hin und her,", "tokens": ["Mein", "F\u00fch\u00b7rer", "red\u00b7te", "nichts", ",", "er", "wies", "nur", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schaute mich oft an, wie wann er Willens w\u00e4r,", "tokens": ["Und", "schau\u00b7te", "mich", "oft", "an", ",", "wie", "wann", "er", "Wil\u00b7lens", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PWAV", "PPER", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich ihm, ob das St\u00fcck mir wohlgefiele, sagte;", "tokens": ["Da\u00df", "ich", "ihm", ",", "ob", "das", "St\u00fcck", "mir", "wohl\u00b7ge\u00b7fie\u00b7le", ",", "sag\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "100Allein ich wu\u00dfte nichts zu sprechen; nein: ich fragte;", "tokens": ["ich", "wu\u00df\u00b7te", "nichts", "zu", "spre\u00b7chen", ";", "nein", ":", "ich", "frag\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKZU", "VVINF", "$.", "PTKANT", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So fieng er endlich an: \u201eVernehmt des Himmels Schlu\u00df!", "tokens": ["So", "fi\u00b7eng", "er", "end\u00b7lich", "an", ":", "\u201e", "Ver\u00b7nehmt", "des", "Him\u00b7mels", "Schlu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$(", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "\u201eder die Nachkommenschaft und euch begl\u00fccken mu\u00df:", "tokens": ["\u201e", "der", "die", "Nach\u00b7kom\u00b7men\u00b7schaft", "und", "euch", "be\u00b7gl\u00fc\u00b7cken", "mu\u00df", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso wie das andre Bild besiegte Schaaren weiset,", "tokens": ["\u201e", "so", "wie", "das", "and\u00b7re", "Bild", "be\u00b7sieg\u00b7te", "Schaa\u00b7ren", "wei\u00b7set", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eso wird der Mit-Regent, wann er zum Kriegs-Heer reiset,", "tokens": ["\u201e", "so", "wird", "der", "Mit\u00b7Re\u00b7gent", ",", "wann", "er", "zum", "Kriegs\u00b7Heer", "rei\u00b7set", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-++-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "105\u201dDer Feinde Macht und Muth zerst\u00f6rt, bek\u00e4mpfet sehn,", "tokens": ["\"", "Der", "Fein\u00b7de", "Macht", "und", "Muth", "zer\u00b7st\u00f6rt", ",", "be\u00b7k\u00e4mp\u00b7fet", "sehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "VVPP", "$,", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ehernach, wie dort das Bild des Welt-Monarchens, stehn.", "tokens": ["\u201e", "her\u00b7nach", ",", "wie", "dort", "das", "Bild", "des", "Welt\u00b7Mon\u00b7a\u00b7rchens", ",", "stehn", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "$,", "PWAV", "ADV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ezu was das Prinzen-Paar f\u00fcr Oesterreich gebohren,", "tokens": ["\u201e", "zu", "was", "das", "Prin\u00b7zen\u00b7Paar", "f\u00fcr", "O\u00b7es\u00b7ter\u00b7reich", "ge\u00b7boh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "\u201edas sagt schon eine Welt der andern in die Ohren.", "tokens": ["\u201e", "das", "sagt", "schon", "ei\u00b7ne", "Welt", "der", "an\u00b7dern", "in", "die", "Oh\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eder Himmel ist ihr Schuz: genug.... Hier ward er still;", "tokens": ["\u201e", "der", "Him\u00b7mel", "ist", "ihr", "Schuz", ":", "ge\u00b7nug", "....", "Hier", "ward", "er", "still", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "$.", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "110So wu\u00dft ich pl\u00f6zlich nicht was dieses Schweigen will.", "tokens": ["wu\u00dft", "ich", "pl\u00f6z\u00b7lich", "nicht", "was", "die\u00b7ses", "Schwei\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKNEG", "PWS", "PDAT", "NN", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Doch ward ich gleich gewahr, da\u00df wieder Alle kamen,", "tokens": ["Doch", "ward", "ich", "gleich", "ge\u00b7wahr", ",", "da\u00df", "wie\u00b7der", "Al\u00b7le", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie vorher den Plaz in einem Kreise nahmen;", "tokens": ["Und", "wie", "vor\u00b7her", "den", "Plaz", "in", "ei\u00b7nem", "Krei\u00b7se", "nah\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Daher verweilten wir bey diesem Priester nicht,", "tokens": ["Da\u00b7her", "ver\u00b7weil\u00b7ten", "wir", "bey", "die\u00b7sem", "Pries\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wir suchten Plaz, und er entwich uns vom Gesicht.", "tokens": ["Wir", "such\u00b7ten", "Plaz", ",", "und", "er", "ent\u00b7wich", "uns", "vom", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "PPER", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "115Der Saal ward nach und nach ein Schau-Spiel voll Erg\u00f6zen;", "tokens": ["Saal", "ward", "nach", "und", "nach", "ein", "Schau\u00b7Spiel", "voll", "Er\u00b7g\u00f6\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "KON", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Fast jede Tugend kam, der Lust was beyzusezen.", "tokens": ["Fast", "je\u00b7de", "Tu\u00b7gend", "kam", ",", "der", "Lust", "was", "bey\u00b7zu\u00b7se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "ART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}