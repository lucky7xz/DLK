{"textgrid.poem.31234": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lustig-seyn und nicht studiren/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lustig-seyn und nicht studiren/", "tokens": ["Lus\u00b7tig\u00b7seyn", "und", "nicht", "stu\u00b7di\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "durch die Gassen kreutz und krumm", "tokens": ["durch", "die", "Gas\u00b7sen", "kreutz", "und", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "nach den M\u00e4gdgens scharmutziren/", "tokens": ["nach", "den", "M\u00e4gd\u00b7gens", "schar\u00b7mut\u00b7zi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lustig-sein und nicht studiren/", "tokens": ["lus\u00b7tig\u00b7sein", "und", "nicht", "stu\u00b7di\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "dihses ist mein Bropprium!", "tokens": ["dih\u00b7ses", "ist", "mein", "Brop\u00b7pri\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bluhder-Hosen/ Bontac-Flaschen/", "tokens": ["Bluh\u00b7der\u00b7Ho\u00b7sen", "/", "Bon\u00b7tac\u00b7Fla\u00b7schen", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$