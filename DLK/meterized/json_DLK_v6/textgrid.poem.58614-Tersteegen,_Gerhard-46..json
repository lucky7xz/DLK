{"textgrid.poem.58614": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, wie lieblich ist es nicht, wenn die Seel' nach langem Klagen", "tokens": ["Ja", ",", "wie", "lieb\u00b7lich", "ist", "es", "nicht", ",", "wenn", "die", "Seel'", "nach", "lan\u00b7gem", "Kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Diese Botschaft h\u00f6rt in sich von dem Heiland selber sagen:", "tokens": ["Die\u00b7se", "Bot\u00b7schaft", "h\u00f6rt", "in", "sich", "von", "dem", "Hei\u00b7land", "sel\u00b7ber", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "PRF", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dein Gott K\u00f6nig ist in dir, dein bedr\u00e4ngtes Herz ist frei", "tokens": ["Dein", "Gott", "K\u00f6\u00b7nig", "ist", "in", "dir", ",", "dein", "be\u00b7dr\u00e4ng\u00b7tes", "Herz", "ist", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-++--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Von dem Dienst der Eigenheit, von der S\u00fcnden Sklaverei;", "tokens": ["Von", "dem", "Dienst", "der", "Ei\u00b7gen\u00b7heit", ",", "von", "der", "S\u00fcn\u00b7den", "Skla\u00b7ve\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Friede, Friede sei mit dir, alles Gute, Heil und Leben", "tokens": ["Frie\u00b7de", ",", "Frie\u00b7de", "sei", "mit", "dir", ",", "al\u00b7les", "Gu\u00b7te", ",", "Heil", "und", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VAFIN", "APPR", "PPER", "$,", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Wird dir in dem einen Gut frei verk\u00fcndigt und gegeben,", "tokens": ["Wird", "dir", "in", "dem", "ei\u00b7nen", "Gut", "frei", "ver\u00b7k\u00fcn\u00b7digt", "und", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ART", "NN", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Deine Augen sehen's nun, da\u00df dein Seufzen ist erh\u00f6rt! \u2013", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "se\u00b7hen's", "nun", ",", "da\u00df", "dein", "Seuf\u00b7zen", "ist", "er\u00b7h\u00f6rt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Das hei\u00dft recht bekehret sein, wenn der Herr uns selbst bekehrt.", "tokens": ["Das", "hei\u00dft", "recht", "be\u00b7keh\u00b7ret", "sein", ",", "wenn", "der", "Herr", "uns", "selbst", "be\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVPP", "VAINF", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}