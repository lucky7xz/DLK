{"textgrid.poem.34279": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo, du Reuter", "tokens": ["Wo", ",", "du", "Reu\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["PWAV", "$,", "PPER", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meinst du hin?", "tokens": ["Meinst", "du", "hin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kannst du w\u00e4hnen", "tokens": ["Kannst", "du", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wer ich bin?", "tokens": ["Wer", "ich", "bin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Leis' umfa\u00df ich", "tokens": ["Leis'", "um\u00b7fa\u00df", "ich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dich als Geist,", "tokens": ["Dich", "als", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Den dein Trauren", "tokens": ["Den", "dein", "Trau\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Von sich weist.", "tokens": ["Von", "sich", "weist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Sey zufrieden", "tokens": ["Sey", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word"], "pos": ["VAIMP", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "G\u00f6the mein!", "tokens": ["G\u00f6\u00b7the", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Wisse, ietzt erst", "tokens": ["Wis\u00b7se", ",", "ietzt", "erst"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Bin ich dein;", "tokens": ["Bin", "ich", "dein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Dein auf ewig", "tokens": ["Dein", "auf", "e\u00b7wig"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "APPR", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Hier und dort \u2013", "tokens": ["Hier", "und", "dort", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Also wein mich", "tokens": ["Al\u00b7so", "wein", "mich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Nicht mehr fort.", "tokens": ["Nicht", "mehr", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}