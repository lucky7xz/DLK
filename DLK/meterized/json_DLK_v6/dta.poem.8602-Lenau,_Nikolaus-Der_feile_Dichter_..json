{"dta.poem.8602": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der feile Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Die Muse mu\u00df zur Metze sich erniedern\u201a", "tokens": ["Die", "Mu\u00b7se", "mu\u00df", "zur", "Met\u00b7ze", "sich", "er\u00b7nie\u00b7dern", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dichter sendet sie zum M\u00e4cenaten,", "tokens": ["Der", "Dich\u00b7ter", "sen\u00b7det", "sie", "zum", "M\u00e4\u00b7ce\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, frechgesch\u00fcrzt, mit schaugestellten Gliedern,", "tokens": ["Und", ",", "frech\u00b7ge\u00b7sch\u00fcrzt", ",", "mit", "schau\u00b7ge\u00b7stell\u00b7ten", "Glie\u00b7dern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der G\u00f6ttlichkeit vergessend, tief entrathen\u201a", "tokens": ["Der", "G\u00f6tt\u00b7lich\u00b7keit", "ver\u00b7ges\u00b7send", ",", "tief", "ent\u00b7ra\u00b7then", "\u201a"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Umtanzt sie ihn mit schn\u00f6den Schmeichelliedern,", "tokens": ["Um\u00b7tanzt", "sie", "ihn", "mit", "schn\u00f6\u00b7den", "Schmei\u00b7chel\u00b7lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lieb\u00e4ugelnd mit den blinkenden Ducaten.", "tokens": ["Lie\u00b7b\u00e4u\u00b7gelnd", "mit", "den", "blin\u00b7ken\u00b7den", "Du\u00b7ca\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sie mu\u00df den Gott in ihm zum Schlaf beth\u00f6ren,", "tokens": ["Sie", "mu\u00df", "den", "Gott", "in", "ihm", "zum", "Schlaf", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Thier zur wilden Gluth und Flamm' emp\u00f6ren.", "tokens": ["Das", "Thier", "zur", "wil\u00b7den", "Gluth", "und", "Flamm'", "em\u00b7p\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}