{"textgrid.poem.35603": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Passion", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Orpheus silbern die Laute r\u00fchrt,", "tokens": ["Wenn", "Or\u00b7pheus", "sil\u00b7bern", "die", "Lau\u00b7te", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beklagend ein Totes im Abendgarten,", "tokens": ["Be\u00b7kla\u00b7gend", "ein", "To\u00b7tes", "im", "A\u00b7bend\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wer bist du Ruhendes unter hohen B\u00e4umen?", "tokens": ["Wer", "bist", "du", "Ru\u00b7hen\u00b7des", "un\u00b7ter", "ho\u00b7hen", "B\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es rauscht die Klage das herbstliche Rohr,", "tokens": ["Es", "rauscht", "die", "Kla\u00b7ge", "das", "herbst\u00b7li\u00b7che", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der blaue Teich,", "tokens": ["Der", "blau\u00b7e", "Teich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Hinsterbend unter gr\u00fcnenden B\u00e4umen", "tokens": ["Hins\u00b7ter\u00b7bend", "un\u00b7ter", "gr\u00fc\u00b7nen\u00b7den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und folgend dem Schatten der Schwester;", "tokens": ["Und", "fol\u00b7gend", "dem", "Schat\u00b7ten", "der", "Schwes\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Dunkle Liebe", "tokens": ["Dunk\u00b7le", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Eines wilden Geschlechts,", "tokens": ["Ei\u00b7nes", "wil\u00b7den", "Ge\u00b7schlechts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Dem auf goldenen R\u00e4dern der Tag davonrauscht.", "tokens": ["Dem", "auf", "gol\u00b7de\u00b7nen", "R\u00e4\u00b7dern", "der", "Tag", "da\u00b7von\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Stille Nacht.", "tokens": ["Stil\u00b7le", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Unter finsteren Tannen", "tokens": ["Un\u00b7ter", "fins\u00b7te\u00b7ren", "Tan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mischten zwei W\u00f6lfe ihr Blut", "tokens": ["Mischten", "zwei", "W\u00f6l\u00b7fe", "ihr", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "CARD", "NN", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In steinerner Umarmung; ein Goldnes", "tokens": ["In", "stei\u00b7ner\u00b7ner", "Um\u00b7ar\u00b7mung", ";", "ein", "Gold\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verlor sich die Wolke \u00fcber dem Steg,", "tokens": ["Ver\u00b7lor", "sich", "die", "Wol\u00b7ke", "\u00fc\u00b7ber", "dem", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Geduld und Schweigen der Kindheit.", "tokens": ["Ge\u00b7duld", "und", "Schwei\u00b7gen", "der", "Kind\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wieder begegnet der zarte Leichnam", "tokens": ["Wie\u00b7der", "be\u00b7geg\u00b7net", "der", "zar\u00b7te", "Leich\u00b7nam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Am Tritonsteich", "tokens": ["Am", "Tri\u00b7ton\u00b7steich"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Schlummernd in seinem hyazinthenen Haar.", "tokens": ["Schlum\u00b7mernd", "in", "sei\u00b7nem", "hy\u00b7a\u00b7zin\u00b7the\u00b7nen", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Da\u00df endlich zerbr\u00e4che das k\u00fchle Haupt!", "tokens": ["Da\u00df", "end\u00b7lich", "zer\u00b7br\u00e4\u00b7che", "das", "k\u00fch\u00b7le", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Denn immer folgt, ein blaues Wild,", "tokens": ["Denn", "im\u00b7mer", "folgt", ",", "ein", "blau\u00b7es", "Wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00c4ugendes unter d\u00e4mmernden B\u00e4umen,", "tokens": ["Ein", "\u00c4u\u00b7gen\u00b7des", "un\u00b7ter", "d\u00e4m\u00b7mern\u00b7den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dieser dunkleren Pfaden", "tokens": ["Die\u00b7ser", "dunk\u00b7le\u00b7ren", "Pfa\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wachend und bewegt von n\u00e4chtigem Wohllaut,", "tokens": ["Wa\u00b7chend", "und", "be\u00b7wegt", "von", "n\u00e4ch\u00b7ti\u00b7gem", "Wohl\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--++", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sanftem Wahnsinn;", "tokens": ["Sanf\u00b7tem", "Wahn\u00b7sinn", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Oder es t\u00f6nte dunkler Verz\u00fcckung", "tokens": ["O\u00b7der", "es", "t\u00f6n\u00b7te", "dunk\u00b7ler", "Ver\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Voll das Saitenspiel", "tokens": ["Voll", "das", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Zu den k\u00fchlen F\u00fc\u00dfen der B\u00fc\u00dferin", "tokens": ["Zu", "den", "k\u00fch\u00b7len", "F\u00fc\u00b7\u00dfen", "der", "B\u00fc\u00b7\u00dfe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In der steinernen Stadt.", "tokens": ["In", "der", "stei\u00b7ner\u00b7nen", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}