{"textgrid.poem.60337": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Am Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir reisten zusammen mit andern", "tokens": ["Wir", "reis\u00b7ten", "zu\u00b7sam\u00b7men", "mit", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu Schiff hinunter den Rhein,", "tokens": ["Zu", "Schiff", "hin\u00b7un\u00b7ter", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es war ein seliges Wandern;", "tokens": ["Es", "war", "ein", "se\u00b7li\u00b7ges", "Wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch waren wir selten allein.", "tokens": ["Doch", "wa\u00b7ren", "wir", "sel\u00b7ten", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie traten heran, zu lauschen,", "tokens": ["Sie", "tra\u00b7ten", "he\u00b7ran", ",", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du lie\u00dfest nur hier und dort", "tokens": ["Du", "lie\u00b7\u00dfest", "nur", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "ADV"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Mir fallen unter das Rauschen", "tokens": ["Mir", "fal\u00b7len", "un\u00b7ter", "das", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Stroms ein heimliches Wort.", "tokens": ["Des", "Stroms", "ein", "heim\u00b7li\u00b7ches", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich sprach: Bald trennt uns die Reise!", "tokens": ["Ich", "sprach", ":", "Bald", "trennt", "uns", "die", "Rei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ob hier wir uns wiedersehn?", "tokens": ["Ob", "hier", "wir", "uns", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbdort vielleicht einst!\u00ab sagtest du leise,", "tokens": ["\u00bb", "dort", "viel\u00b7leicht", "einst", "!", "\u00ab", "sag\u00b7test", "du", "lei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADV", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Ich konnte dich kaum verstehn.", "tokens": ["Ich", "konn\u00b7te", "dich", "kaum", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir flogen vor\u00fcber am Strande,", "tokens": ["Wir", "flo\u00b7gen", "vor\u00b7\u00fc\u00b7ber", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Dampf durchbrauste den Schlot,", "tokens": ["Der", "Dampf", "durch\u00b7braus\u00b7te", "den", "Schlot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie ein zorniger Neger die Bande", "tokens": ["Wie", "ein", "zor\u00b7ni\u00b7ger", "Ne\u00b7ger", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wildschnaubend zu sprengen droht.", "tokens": ["Wild\u00b7schnau\u00b7bend", "zu", "spren\u00b7gen", "droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und sie begannen zu preisen,", "tokens": ["Und", "sie", "be\u00b7gan\u00b7nen", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie schnell man sich heute bewegt,", "tokens": ["Wie", "schnell", "man", "sich", "heu\u00b7te", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wie das r\u00fchrige Eisen", "tokens": ["Und", "wie", "das", "r\u00fch\u00b7ri\u00b7ge", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Man \u00fcber die Stra\u00dfen legt;", "tokens": ["Man", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfen", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Als wollten zu Grabe sie tragen", "tokens": ["Als", "woll\u00b7ten", "zu", "Gra\u00b7be", "sie", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "APPR", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Elends t\u00fcrmenden Wust", "tokens": ["Des", "E\u00b7lends", "t\u00fcr\u00b7men\u00b7den", "Wust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wieder das Eden erjagen,", "tokens": ["Und", "wie\u00b7der", "das", "E\u00b7den", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den uralt bittern Verlust.", "tokens": ["Den", "ur\u00b7alt", "bit\u00b7tern", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Es hat doch den rechten Fergen", "tokens": ["Es", "hat", "doch", "den", "rech\u00b7ten", "Fer\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Schifflein lange noch nicht,", "tokens": ["Das", "Schif\u00b7flein", "lan\u00b7ge", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Solange noch Liebe verbergen", "tokens": ["So\u00b7lan\u00b7ge", "noch", "Lie\u00b7be", "ver\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sich mu\u00df wie ein S\u00fcndergesicht,", "tokens": ["Sich", "mu\u00df", "wie", "ein", "S\u00fcn\u00b7der\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Noch lange nicht hat, ihr Gesellen,", "tokens": ["Noch", "lan\u00b7ge", "nicht", "hat", ",", "ihr", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das Eisen den rechten Gu\u00df,", "tokens": ["Das", "Ei\u00b7sen", "den", "rech\u00b7ten", "Gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn sich die Liebe bestellen", "tokens": ["Wenn", "sich", "die", "Lie\u00b7be", "be\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Noch hinter die Gr\u00e4ber mu\u00df!", "tokens": ["Noch", "hin\u00b7ter", "die", "Gr\u00e4\u00b7ber", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "So dacht ich und blickte verdrossen", "tokens": ["So", "dacht", "ich", "und", "blick\u00b7te", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hinab in die rollende Flut;", "tokens": ["Hin\u00b7ab", "in", "die", "rol\u00b7len\u00b7de", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dich umringten deine Genossen", "tokens": ["Dich", "um\u00b7ring\u00b7ten", "dei\u00b7ne", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und scherzten; die hatten es gut.", "tokens": ["Und", "scherz\u00b7ten", ";", "die", "hat\u00b7ten", "es", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Die Nacht war dunkelnd gekommen,", "tokens": ["Die", "Nacht", "war", "dun\u00b7kelnd", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da stiegen am Strande wir aus,", "tokens": ["Da", "stie\u00b7gen", "am", "Stran\u00b7de", "wir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich folgte dir stumm und beklommen", "tokens": ["Ich", "folg\u00b7te", "dir", "stumm", "und", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von ferne bis an dem Haus.", "tokens": ["Von", "fer\u00b7ne", "bis", "an", "dem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Und als du, noch einmal nickend,", "tokens": ["Und", "als", "du", ",", "noch", "ein\u00b7mal", "ni\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Verschwunden im schlie\u00dfenden Tor,", "tokens": ["Ver\u00b7schwun\u00b7den", "im", "schlie\u00b7\u00dfen\u00b7den", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Stand ich eine Weile noch, blickend", "tokens": ["Stand", "ich", "ei\u00b7ne", "Wei\u00b7le", "noch", ",", "bli\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ADJD"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nach deinem Fenster empor.", "tokens": ["Nach", "dei\u00b7nem", "Fens\u00b7ter", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Ich schied von deinem Quartiere", "tokens": ["Ich", "schied", "von", "dei\u00b7nem", "Quar\u00b7tie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ging hin\u00fcber in meins,", "tokens": ["Und", "ging", "hin\u00b7\u00fc\u00b7ber", "in", "meins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das lag im fernen Reviere", "tokens": ["Das", "lag", "im", "fer\u00b7nen", "Re\u00b7vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Am andern Ufer des Rheins.", "tokens": ["Am", "an\u00b7dern", "U\u00b7fer", "des", "Rheins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Ich betrat mein trauriges Zimmer", "tokens": ["Ich", "be\u00b7trat", "mein", "trau\u00b7ri\u00b7ges", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und starrte unverwandt", "tokens": ["Und", "starr\u00b7te", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hin\u00fcber zum Kerzenschimmer,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "zum", "Ker\u00b7zen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den mir dein Fenster gesandt.", "tokens": ["Den", "mir", "dein", "Fens\u00b7ter", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Die Lichter dr\u00fcben am Strande", "tokens": ["Die", "Lich\u00b7ter", "dr\u00fc\u00b7ben", "am", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erloschen nach und nach,", "tokens": ["Er\u00b7lo\u00b7schen", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wie zu traulichem Pf\u00e4nde", "tokens": ["Doch", "wie", "zu", "trau\u00b7li\u00b7chem", "Pf\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blieb deines immer noch wach.", "tokens": ["Blieb", "dei\u00b7nes", "im\u00b7mer", "noch", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Wie ich im einsamen Leide", "tokens": ["Wie", "ich", "im", "ein\u00b7sa\u00b7men", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinstarrte \u00fcber die Flut:", "tokens": ["Hins\u00b7tarr\u00b7te", "\u00fc\u00b7ber", "die", "Flut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.c