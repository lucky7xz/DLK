{"textgrid.poem.34481": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00fchl athmet der Berge rauschende Nacht.", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchl athmet der Berge rauschende Nacht.", "tokens": ["K\u00fchl", "ath\u00b7met", "der", "Ber\u00b7ge", "rau\u00b7schen\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwarzspitzige Tannen nicken im Schlaf.", "tokens": ["Schwarz\u00b7spit\u00b7zi\u00b7ge", "Tan\u00b7nen", "ni\u00b7cken", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stumm ruht der See, vom Dunkel umlauscht;", "tokens": ["Stumm", "ruht", "der", "See", ",", "vom", "Dun\u00b7kel", "um\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und droben fl\u00fcchten die Wolken.", "tokens": ["und", "dro\u00b7ben", "fl\u00fcch\u00b7ten", "die", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie Silber durchwirkt ein schwarzes Gewand,", "tokens": ["Wie", "Sil\u00b7ber", "durch\u00b7wirkt", "ein", "schwar\u00b7zes", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "weiss glitzernd und fahl \u2013 so zittert der Mond", "tokens": ["weiss", "glit\u00b7zernd", "und", "fahl", "\u2013", "so", "zit\u00b7tert", "der", "Mond"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "auf der bebenden Fluth; und die Fledermaus", "tokens": ["auf", "der", "be\u00b7ben\u00b7den", "Fluth", ";", "und", "die", "Fle\u00b7der\u00b7maus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "huscht eckig hervor aus dem Dickicht. \u2013", "tokens": ["huscht", "ec\u00b7kig", "her\u00b7vor", "aus", "dem", "Di\u00b7ckicht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und zwischen den St\u00e4mmen am Uferrand", "tokens": ["Und", "zwi\u00b7schen", "den", "St\u00e4m\u00b7men", "am", "Uf\u00b7er\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "tritt hurtig hervor auf das feuchte Gestein", "tokens": ["tritt", "hur\u00b7tig", "her\u00b7vor", "auf", "das", "feuch\u00b7te", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "ein nacktes Kind mit rundlichem Leib:", "tokens": ["ein", "nack\u00b7tes", "Kind", "mit", "rund\u00b7li\u00b7chem", "Leib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "das patscht voller Lust mit den H\u00e4ndchen.", "tokens": ["das", "patscht", "vol\u00b7ler", "Lust", "mit", "den", "H\u00e4nd\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und weiter getrost beschreitets die Fluth.", "tokens": ["Und", "wei\u00b7ter", "ge\u00b7trost", "be\u00b7schrei\u00b7tets", "die", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schon steht es im Mond. Und jauchzend und schnell", "tokens": ["Schon", "steht", "es", "im", "Mond", ".", "Und", "jauch\u00b7zend", "und", "schnell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "KON", "VVPP", "KON", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "l\u00e4ufts \u00fcber die Tiefe, die willig es tr\u00e4gt,", "tokens": ["l\u00e4ufts", "\u00fc\u00b7ber", "die", "Tie\u00b7fe", ",", "die", "wil\u00b7lig", "es", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "und strampelt und stampft mit den Beinen. \u2013", "tokens": ["und", "stram\u00b7pelt", "und", "stampft", "mit", "den", "Bei\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Fort zogen die Wolken. Es leuchtet die Nacht.", "tokens": ["Fort", "zo\u00b7gen", "die", "Wol\u00b7ken", ".", "Es", "leuch\u00b7tet", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein silberner Spiegel lagert der See.", "tokens": ["Ein", "sil\u00b7ber\u00b7ner", "Spie\u00b7gel", "la\u00b7gert", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drauf springt und spielt das lustige Kind", "tokens": ["Drauf", "springt", "und", "spielt", "das", "lus\u00b7ti\u00b7ge", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und hascht nach den Flederm\u00e4usen.", "tokens": ["und", "hascht", "nach", "den", "Fle\u00b7der\u00b7m\u00e4u\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Bald hat es die M\u00fch als vergeblich erkannt.", "tokens": ["Bald", "hat", "es", "die", "M\u00fch", "als", "ver\u00b7geb\u00b7lich", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da gewahrts einen dicken, schlafenden Frosch", "tokens": ["Da", "ge\u00b7wahrts", "ei\u00b7nen", "di\u00b7cken", ",", "schla\u00b7fen\u00b7den", "Frosch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "am Ufer. Den weckt es, der springt in die Fluth", "tokens": ["am", "U\u00b7fer", ".", "Den", "weckt", "es", ",", "der", "springt", "in", "die", "Fluth"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "ART", "VVFIN", "PPER", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und m\u00fcht sich voll Angst unterm Wasser.", "tokens": ["und", "m\u00fcht", "sich", "voll", "Angst", "un\u00b7term", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "Ihm folgt das Kind, wohin er auch flieht,", "tokens": ["Ihm", "folgt", "das", "Kind", ",", "wo\u00b7hin", "er", "auch", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "lacht \u00fcber den zappelnden, dicken Geselln ...", "tokens": ["lacht", "\u00fc\u00b7ber", "den", "zap\u00b7peln\u00b7den", ",", "di\u00b7cken", "Ge\u00b7selln", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da \u2013 blickt es empor. Ein Schatten fiel", "tokens": ["Da", "\u2013", "blickt", "es", "em\u00b7por", ".", "Ein", "Schat\u00b7ten", "fiel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$(", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "auf den n\u00e4chtig leuchtenden Spiegel.", "tokens": ["auf", "den", "n\u00e4ch\u00b7tig", "leuch\u00b7ten\u00b7den", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Durchsichtige Schwingen tragen ein Kind", "tokens": ["Durch\u00b7sich\u00b7ti\u00b7ge", "Schwin\u00b7gen", "tra\u00b7gen", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wie ein W\u00f6lkchen herab durch die schweigende Luft.", "tokens": ["wie", "ein", "W\u00f6lk\u00b7chen", "her\u00b7ab", "durch", "die", "schwei\u00b7gen\u00b7de", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ein zartes Gesch\u00f6pf, ein schw\u00e4chlicher Leib", "tokens": ["Ein", "zar\u00b7tes", "Ge\u00b7sch\u00f6pf", ",", "ein", "schw\u00e4ch\u00b7li\u00b7cher", "Leib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und schmerzensdunkle Augen.", "tokens": ["und", "schmer\u00b7zens\u00b7dunk\u00b7le", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das neigt sich zum Knaben. Der steht und starrt", "tokens": ["Das", "neigt", "sich", "zum", "Kna\u00b7ben", ".", "Der", "steht", "und", "starrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "$.", "ART", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "mit offenen Augen und offenem Mund.", "tokens": ["mit", "of\u00b7fe\u00b7nen", "Au\u00b7gen", "und", "of\u00b7fe\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da spricht es mit leise flehendem Ton:", "tokens": ["Da", "spricht", "es", "mit", "lei\u00b7se", "fle\u00b7hen\u00b7dem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u2013 O spiele mit mir, mein Bruder!", "tokens": ["\u2013", "O", "spie\u00b7le", "mit", "mir", ",", "mein", "Bru\u00b7der", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Knabe sch\u00fcttelt den Lockenkopf.", "tokens": ["Der", "Kna\u00b7be", "sch\u00fct\u00b7telt", "den", "Lo\u00b7cken\u00b7kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Du kannst ja fliegen. Ich kann das nicht.", "tokens": ["\u2013", "Du", "kannst", "ja", "flie\u00b7gen", ".", "Ich", "kann", "das", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "$.", "PPER", "VMFIN", "PDS", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn ich dich hasche, fang ich dich nie:", "tokens": ["Und", "wenn", "ich", "dich", "ha\u00b7sche", ",", "fang", "ich", "dich", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ich bin ja gar nicht dein Bruder.", "tokens": ["ich", "bin", "ja", "gar", "nicht", "dein", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u2013 Ja, wir sind Br\u00fcder. O spiele mit mir!", "tokens": ["\u2013", "Ja", ",", "wir", "sind", "Br\u00fc\u00b7der", ".", "O", "spie\u00b7le", "mit", "mir", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VAFIN", "NN", "$.", "NE", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein einziger hat uns beide gezeugt.", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "hat", "uns", "bei\u00b7de", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zu Gespielen sind wir einander bestimmt,", "tokens": ["Zu", "Ge\u00b7spie\u00b7len", "sind", "wir", "ein\u00b7an\u00b7der", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und ich habe dich lieb, mein Bruder.", "tokens": ["und", "ich", "ha\u00b7be", "dich", "lieb", ",", "mein", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Vom Vater die Mutter empfing dich so gern", "tokens": ["Vom", "Va\u00b7ter", "die", "Mut\u00b7ter", "emp\u00b7fing", "dich", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und l\u00e4chelte, da sie mit Schmerzen gebar.", "tokens": ["und", "l\u00e4\u00b7chel\u00b7te", ",", "da", "sie", "mit", "Schmer\u00b7zen", "ge\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die meine, verzweifelt, sp\u00fcrte mich kaum \u2013", "tokens": ["Die", "mei\u00b7ne", ",", "ver\u00b7zwei\u00b7felt", ",", "sp\u00fcr\u00b7te", "mich", "kaum", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "bin ungeboren gestorben.", "tokens": ["bin", "un\u00b7ge\u00b7bo\u00b7ren", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Du athmest den Tag \u2013 ich athme die Nacht.", "tokens": ["Du", "ath\u00b7mest", "den", "Tag", "\u2013", "ich", "ath\u00b7me", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie f\u00fchl ich der Sonne brausendes Licht.", "tokens": ["Nie", "f\u00fchl", "ich", "der", "Son\u00b7ne", "brau\u00b7sen\u00b7des", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dich tr\u00e4gt die Fluth \u2013 bist wegebegabt \u2013", "tokens": ["Dich", "tr\u00e4gt", "die", "Fluth", "\u2013", "bist", "we\u00b7ge\u00b7be\u00b7gabt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "VAFIN", "VAPP", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "o spiele mit mir, mein Bruder!", "tokens": ["o", "spie\u00b7le", "mit", "mir", ",", "mein", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Da schaut voller Jammer der andre empor:", "tokens": ["Da", "schaut", "vol\u00b7ler", "Jam\u00b7mer", "der", "and\u00b7re", "em\u00b7por", ":"], "token_info": ["word", "w