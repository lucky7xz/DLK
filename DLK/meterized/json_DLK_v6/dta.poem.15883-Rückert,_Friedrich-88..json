{"dta.poem.15883": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Herzog ward befragt, ob er auch Jagdhund' halte,", "tokens": ["Ein", "Her\u00b7zog", "ward", "be\u00b7fragt", ",", "ob", "er", "auch", "Jagd\u00b7hund'", "hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit des Waidwerks er nach Herzogsw\u00fcrden walte?", "tokens": ["Da\u00b7mit", "des", "Waid\u00b7werks", "er", "nach", "Her\u00b7zogs\u00b7w\u00fcr\u00b7den", "wal\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf eine Tafel wies er hin voll armer Leute,", "tokens": ["Auf", "ei\u00b7ne", "Ta\u00b7fel", "wies", "er", "hin", "voll", "ar\u00b7mer", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die er da speisen lie\u00df: dis hier ist meine Meute.", "tokens": ["Die", "er", "da", "spei\u00b7sen", "lie\u00df", ":", "dis", "hier", "ist", "mei\u00b7ne", "Meu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$.", "PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr m\u00f6gt mit euerer nur Hirsch' und Rehe plagen;", "tokens": ["Ihr", "m\u00f6gt", "mit", "eu\u00b7e\u00b7rer", "nur", "Hirsch'", "und", "Re\u00b7he", "pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit meiner hier will ich das Himmelreich erjagen.", "tokens": ["Mit", "mei\u00b7ner", "hier", "will", "ich", "das", "Him\u00b7mel\u00b7reich", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}