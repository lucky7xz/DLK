{"textgrid.poem.25548": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei deines Morgens erstem Sonnenstrahle,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei deines Morgens erstem Sonnenstrahle,", "tokens": ["Bei", "dei\u00b7nes", "Mor\u00b7gens", "ers\u00b7tem", "Son\u00b7nen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steh' ich, dich mit Gesang erwartend, dankbar da,", "tokens": ["Steh'", "ich", ",", "dich", "mit", "Ge\u00b7sang", "er\u00b7war\u00b7tend", ",", "dank\u00b7bar", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRF", "APPR", "NN", "VVPP", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dich, o du Tag, an dem zum ersten male", "tokens": ["Dich", ",", "o", "du", "Tag", ",", "an", "dem", "zum", "ers\u00b7ten", "ma\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "PPER", "NN", "$,", "APPR", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich Amaranten sah.", "tokens": ["Ich", "A\u00b7ma\u00b7ran\u00b7ten", "sah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die schwarzen Locken troffen noch von Regen,", "tokens": ["Die", "schwar\u00b7zen", "Lo\u00b7cken", "trof\u00b7fen", "noch", "von", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hingen, wie sein Frack, im Wirwar um ihn her;", "tokens": ["Und", "hin\u00b7gen", ",", "wie", "sein", "Frack", ",", "im", "Wir\u00b7war", "um", "ihn", "her", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "$,", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wir M\u00e4dchen alle waren zwar verlegen", "tokens": ["Wir", "M\u00e4d\u00b7chen", "al\u00b7le", "wa\u00b7ren", "zwar", "ver\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PIS", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dabei, allein nicht er.", "tokens": ["Da\u00b7bei", ",", "al\u00b7lein", "nicht", "er", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "PTKNEG", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mit seinen Augen, blau wie junge Veilchen,", "tokens": ["Mit", "sei\u00b7nen", "Au\u00b7gen", ",", "blau", "wie", "jun\u00b7ge", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Blickt' er uns l\u00e4chelnd an. Als er so \u00fcbersah", "tokens": ["Blickt'", "er", "uns", "l\u00e4\u00b7chelnd", "an", ".", "Als", "er", "so", "\u00fc\u00b7ber\u00b7sah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$.", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den bunten Kreis, ruht' er auf mir ein Weilchen;", "tokens": ["Den", "bun\u00b7ten", "Kreis", ",", "ruht'", "er", "auf", "mir", "ein", "Weil\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "O Herz! wie schlugst du da!", "tokens": ["O", "Herz", "!", "wie", "schlugst", "du", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Gleich aber wandt' er seine Adlerblicke", "tokens": ["Gleich", "a\u00b7ber", "wandt'", "er", "sei\u00b7ne", "Ad\u00b7ler\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von meiner R\u00f6the weg, sucht' einen Weisen sich,", "tokens": ["Von", "mei\u00b7ner", "R\u00f6\u00b7the", "weg", ",", "sucht'", "ei\u00b7nen", "Wei\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "War um ihn her, wie um ein Licht die M\u00fccke,", "tokens": ["War", "um", "ihn", "her", ",", "wie", "um", "ein", "Licht", "die", "M\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und ach! vergessen ich.", "tokens": ["Und", "ach", "!", "ver\u00b7ges\u00b7sen", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "So gleichen wir den Puppen denn im Schache,", "tokens": ["So", "glei\u00b7chen", "wir", "den", "Pup\u00b7pen", "denn", "im", "Scha\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Womit der Mann von Geist kaum zur Erholung spielt?", "tokens": ["Wo\u00b7mit", "der", "Mann", "von", "Geist", "kaum", "zur", "Er\u00b7ho\u00b7lung", "spielt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und mehr bei dem Geschw\u00e4tz' von einem Bache", "tokens": ["Und", "mehr", "bei", "dem", "Ge\u00b7schw\u00e4tz'", "von", "ei\u00b7nem", "Ba\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Als bei dem unsern f\u00fchlt?", "tokens": ["Als", "bei", "dem", "un\u00b7sern", "f\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ha! sollte da mein Blut nicht st\u00e4rker wallen?", "tokens": ["Ha", "!", "soll\u00b7te", "da", "mein", "Blut", "nicht", "st\u00e4r\u00b7ker", "wal\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Sagt, welches M\u00e4dchen nicht auch Eitelkeit besitzt?", "tokens": ["Sagt", ",", "wel\u00b7ches", "M\u00e4d\u00b7chen", "nicht", "auch", "Ei\u00b7tel\u00b7keit", "be\u00b7sitzt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "PTKNEG", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "O h\u00e4tt' ihm nicht mein frohes Herz gefallen:", "tokens": ["O", "h\u00e4tt'", "ihm", "nicht", "mein", "fro\u00b7hes", "Herz", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Was, Nante, w\u00e4rst du itzt?", "tokens": ["Was", ",", "Nan\u00b7te", ",", "w\u00e4rst", "du", "itzt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NE", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "So aber sitz' ich hier in einer Laube,", "tokens": ["So", "a\u00b7ber", "sitz'", "ich", "hier", "in", "ei\u00b7ner", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die diesen Mann und mich in k\u00fcnft'ger Nacht versteckt,", "tokens": ["Die", "die\u00b7sen", "Mann", "und", "mich", "in", "k\u00fcnft'\u00b7ger", "Nacht", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "KON", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und dann in Deutschland, wie ich glaube,", "tokens": ["Und", "dann", "in", "Deutschland", ",", "wie", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Die Gl\u00fccklichsten bedeckt.", "tokens": ["Die", "Gl\u00fcck\u00b7lichs\u00b7ten", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}}}}}