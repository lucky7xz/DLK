{"textgrid.poem.43189": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies Buch voll dunkler, trauriger Gedanken,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Buch voll dunkler, trauriger Gedanken,", "tokens": ["Dies", "Buch", "voll", "dunk\u00b7ler", ",", "trau\u00b7ri\u00b7ger", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es gleichet nimmer Deinen frohen, lichten,", "tokens": ["Es", "glei\u00b7chet", "nim\u00b7mer", "Dei\u00b7nen", "fro\u00b7hen", ",", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gleicht nicht Deinen bl\u00fchenden Gedichten,", "tokens": ["Es", "gleicht", "nicht", "Dei\u00b7nen", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht Deinen ", "tokens": ["Nicht", "Dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["PTKNEG", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Und doch \u2013 denn Beide mu\u00dften wir erkranken", "tokens": ["Und", "doch", "\u2013", "denn", "Bei\u00b7de", "mu\u00df\u00b7ten", "wir", "er\u00b7kran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "KON", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am gleichen Leid, im wundgeweinten Herzen,", "tokens": ["Am", "glei\u00b7chen", "Leid", ",", "im", "wund\u00b7ge\u00b7wein\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis wir im Ueberma\u00dfe dumpfer Schmerzen", "tokens": ["Bis", "wir", "im", "Ue\u00b7ber\u00b7ma\u00b7\u00dfe", "dum\u00b7pfer", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Kunst vertrauend in die Arme sanken.", "tokens": ["Der", "Kunst", "ver\u00b7trau\u00b7end", "in", "die", "Ar\u00b7me", "san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn wir auch Vergessenheit nicht tranken,", "tokens": ["Und", "wenn", "wir", "auch", "Ver\u00b7ges\u00b7sen\u00b7heit", "nicht", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die alten Schmerzen nimmer ganz beschworen,", "tokens": ["Die", "al\u00b7ten", "Schmer\u00b7zen", "nim\u00b7mer", "ganz", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wurde doch aus Thr\u00e4nen uns geboren", "tokens": ["So", "wur\u00b7de", "doch", "aus", "Thr\u00e4\u00b7nen", "uns", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Blumendichten \u2013 meine Liederranken.", "tokens": ["Dein", "Blu\u00b7men\u00b7dich\u00b7ten", "\u2013", "mei\u00b7ne", "Lie\u00b7der\u00b7ran\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}