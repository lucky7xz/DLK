{"textgrid.poem.49391": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Epitaph", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gute Mann, den wir zu Grabe tragen,", "tokens": ["Der", "gu\u00b7te", "Mann", ",", "den", "wir", "zu", "Gra\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht w\u00e4chsern aus und scheint erstarrt zu sein.", "tokens": ["Sieht", "w\u00e4ch\u00b7sern", "aus", "und", "scheint", "er\u00b7starrt", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PTKVZ", "KON", "VVFIN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch war er so verliebt in allen Schein,", "tokens": ["Doch", "war", "er", "so", "ver\u00b7liebt", "in", "al\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df man sich h\u00fcten mu\u00df, ihn tot zu sagen.", "tokens": ["Da\u00df", "man", "sich", "h\u00fc\u00b7ten", "mu\u00df", ",", "ihn", "tot", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVINF", "VMFIN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er liebte es in allen Lebenslagen", "tokens": ["Er", "lieb\u00b7te", "es", "in", "al\u00b7len", "Le\u00b7bens\u00b7la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Unerh\u00f6rten nur Geh\u00f6r zu leihn.", "tokens": ["Dem", "Un\u00b7er\u00b7h\u00f6r\u00b7ten", "nur", "Ge\u00b7h\u00f6r", "zu", "leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umgeben so von hundert Fabulein", "tokens": ["Um\u00b7ge\u00b7ben", "so", "von", "hun\u00b7dert", "Fa\u00b7bu\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kann man nur z\u00f6gernd ihm zu glauben wagen.", "tokens": ["Kann", "man", "nur", "z\u00f6\u00b7gernd", "ihm", "zu", "glau\u00b7ben", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum, wenn auch jetzt sein schmaler Maskenmund", "tokens": ["Drum", ",", "wenn", "auch", "jetzt", "sein", "schma\u00b7ler", "Mas\u00b7ken\u00b7mund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschlossen liegt und nicht mehr sprechen mag:", "tokens": ["Ge\u00b7schlos\u00b7sen", "liegt", "und", "nicht", "mehr", "spre\u00b7chen", "mag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er lauscht vielleicht nur in den Sch\u00f6pfergrund ...", "tokens": ["Er", "lauscht", "viel\u00b7leicht", "nur", "in", "den", "Sch\u00f6p\u00b7fer\u00b7grund", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und steht dann wieder auf wie jeden Tag.", "tokens": ["Und", "steht", "dann", "wie\u00b7der", "auf", "wie", "je\u00b7den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00dft ihn getrost bei seinem Leichenspiele.", "tokens": ["La\u00dft", "ihn", "ge\u00b7trost", "bei", "sei\u00b7nem", "Lei\u00b7chen\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er l\u00e4chelt schon und wir sind kaum am Ziele.", "tokens": ["Er", "l\u00e4\u00b7chelt", "schon", "und", "wir", "sind", "kaum", "am", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}