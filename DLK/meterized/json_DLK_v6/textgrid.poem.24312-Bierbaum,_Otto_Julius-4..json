{"textgrid.poem.24312": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf Uhr: heilige Nacht. Wie ein Gesumme", "tokens": ["Zw\u00f6lf", "Uhr", ":", "hei\u00b7li\u00b7ge", "Nacht", ".", "Wie", "ein", "Ge\u00b7sum\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$.", "ADJA", "NN", "$.", "PWAV", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Von Bienen klingt das L\u00e4uten der hundert Glocken", "tokens": ["Von", "Bie\u00b7nen", "klingt", "das", "L\u00e4u\u00b7ten", "der", "hun\u00b7dert", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "CARD", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In meine Gartenstille aus Florenz herauf.", "tokens": ["In", "mei\u00b7ne", "Gar\u00b7ten\u00b7stil\u00b7le", "aus", "Flo\u00b7renz", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun knien im ungeheuren Dome dort", "tokens": ["Nun", "kni\u00b7en", "im", "un\u00b7ge\u00b7heu\u00b7ren", "Do\u00b7me", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die Betenden, und unter der Kuppel hebt", "tokens": ["Die", "Be\u00b7ten\u00b7den", ",", "und", "un\u00b7ter", "der", "Kup\u00b7pel", "hebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die ringgeschm\u00fcckte Hand der Erzbischof", "tokens": ["Die", "ring\u00b7ge\u00b7schm\u00fcck\u00b7te", "Hand", "der", "Erz\u00b7bi\u00b7schof"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum Weihnachtssegen. Gloria in excelsis!", "tokens": ["Zum", "Weih\u00b7nachts\u00b7se\u00b7gen", ".", "Glo\u00b7ria", "in", "ex\u00b7cel\u00b7sis", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und Pax vobiscum!", "tokens": ["Und", "Pax", "vo\u00b7bis\u00b7cum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Te laudamus,", "tokens": ["Te", "lau\u00b7da\u00b7mus", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Domine!", "tokens": ["Do\u00b7mi\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Mir hat der Himmel einen Weihnachtsbaum", "tokens": ["Mir", "hat", "der", "Him\u00b7mel", "ei\u00b7nen", "Weih\u00b7nachts\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus ungez\u00e4hlten Sternen angez\u00fcndet.", "tokens": ["Aus", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Ster\u00b7nen", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo m\u00fc\u00dften heute die drei K\u00f6nige hin,", "tokens": ["Wo", "m\u00fc\u00df\u00b7ten", "heu\u00b7te", "die", "drei", "K\u00f6\u00b7ni\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wenn sie den Sternen folgen wollten! Wohl!", "tokens": ["Wenn", "sie", "den", "Ster\u00b7nen", "fol\u00b7gen", "woll\u00b7ten", "!", "Wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$.", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dies sei mir Omen: \u00fcberall gebiert", "tokens": ["Dies", "sei", "mir", "O\u00b7men", ":", "\u00fc\u00b7be\u00b7rall", "ge\u00b7biert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "NN", "$.", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Liebe Geist und Kraft und Herrlichkeit.", "tokens": ["Die", "Lie\u00b7be", "Geist", "und", "Kraft", "und", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Frieden aber hab ich in mir selbst,", "tokens": ["Den", "Frie\u00b7den", "a\u00b7ber", "hab", "ich", "in", "mir", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seitdem ich wei\u00df, da\u00df keine Liebe ihn:", "tokens": ["Seit\u00b7dem", "ich", "wei\u00df", ",", "da\u00df", "kei\u00b7ne", "Lie\u00b7be", "ihn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ihn die Kraft verb\u00fcrgt, die sich erkennt", "tokens": ["Da\u00df", "ihn", "die", "Kraft", "ver\u00b7b\u00fcrgt", ",", "die", "sich", "er\u00b7kennt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ohne Furcht den Weg zur Treue geht:", "tokens": ["Und", "oh\u00b7ne", "Furcht", "den", "Weg", "zur", "Treu\u00b7e", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum t\u00e4tigen Selbst, das, wenn es sein mu\u00df, froh", "tokens": ["Zum", "t\u00e4\u00b7ti\u00b7gen", "Selbst", ",", "das", ",", "wenn", "es", "sein", "mu\u00df", ",", "froh"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PDS", "$,", "KOUS", "PPER", "VAINF", "VMFIN", "$,", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Schwert gebraucht. Nichts ist so friedestark,", "tokens": ["Das", "Schwert", "ge\u00b7braucht", ".", "Nichts", "ist", "so", "frie\u00b7de\u00b7stark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als Selbstgef\u00fchl im Kampf. Friedlos ist nur,", "tokens": ["Als", "Selbst\u00b7ge\u00b7f\u00fchl", "im", "Kampf", ".", "Fried\u00b7los", "ist", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$.", "NE", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer Fratzen f\u00fcrchtet und um Freundschaft buhlt,", "tokens": ["Wer", "Frat\u00b7zen", "f\u00fcrch\u00b7tet", "und", "um", "Freund\u00b7schaft", "buhlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wo Feindschaft vorbestimmt und Wonne ist.", "tokens": ["Wo", "Feind\u00b7schaft", "vor\u00b7be\u00b7stimmt", "und", "Won\u00b7ne", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}