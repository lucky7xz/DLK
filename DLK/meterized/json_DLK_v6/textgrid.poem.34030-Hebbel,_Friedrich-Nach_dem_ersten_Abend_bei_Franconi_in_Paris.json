{"textgrid.poem.34030": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem ersten Abend bei Franconi in Paris", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jammer, du r\u00fchrst mich nicht mehr! Denn da\u00df es dem feurigen Proteus", "tokens": ["Jam\u00b7mer", ",", "du", "r\u00fchrst", "mich", "nicht", "mehr", "!", "Denn", "da\u00df", "es", "dem", "feu\u00b7ri\u00b7gen", "Pro\u00b7teus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "KON", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "In des Odysseus Arm, der ihn nicht einmal befragt,", "tokens": ["In", "des", "O\u00b7dys\u00b7seus", "Arm", ",", "der", "ihn", "nicht", "ein\u00b7mal", "be\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der ihn nur stumm erdr\u00fcckt und an der Verwandlung verhindert,", "tokens": ["Der", "ihn", "nur", "stumm", "er\u00b7dr\u00fcckt", "und", "an", "der", "Ver\u00b7wand\u00b7lung", "ver\u00b7hin\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df es ihm \u00fcbel behagt, dieses versteht sich von selbst.", "tokens": ["Da\u00df", "es", "ihm", "\u00fc\u00b7bel", "be\u00b7hagt", ",", "die\u00b7ses", "ver\u00b7steht", "sich", "von", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,", "PDS", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber, wenn er sich l\u00f6s't und sich die g\u00f6ttliche Freiheit", "tokens": ["A\u00b7ber", ",", "wenn", "er", "sich", "l\u00f6s't", "und", "sich", "die", "g\u00f6tt\u00b7li\u00b7che", "Frei\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "VVFIN", "KON", "PRF", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wieder erobert, und w\u00e4r's auch nur f\u00fcr Einen Moment:", "tokens": ["Wie\u00b7der", "er\u00b7o\u00b7bert", ",", "und", "w\u00e4r's", "auch", "nur", "f\u00fcr", "Ei\u00b7nen", "Mo\u00b7ment", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KON", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ja, da r\u00fchrt er mich tief, da f\u00fchl' ich mich doppelt und dreifach", "tokens": ["Ja", ",", "da", "r\u00fchrt", "er", "mich", "tief", ",", "da", "f\u00fchl'", "ich", "mich", "dop\u00b7pelt", "und", "drei\u00b7fach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "PAV"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Selber gebunden, da wird eilig das Auge mir feucht.", "tokens": ["Sel\u00b7ber", "ge\u00b7bun\u00b7den", ",", "da", "wird", "ei\u00b7lig", "das", "Au\u00b7ge", "mir", "feucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "VAFIN", "ADJD", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Zeigt mir ein Bettler die Wunden, so reich' ich ihm freilich den Pfenning,", "tokens": ["Zeigt", "mir", "ein", "Bett\u00b7ler", "die", "Wun\u00b7den", ",", "so", "reich'", "ich", "ihm", "frei\u00b7lich", "den", "Pfen\u00b7ning", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Doch ich wusch sie noch nie mild mit der Thr\u00e4ne ihm aus,", "tokens": ["Doch", "ich", "wusch", "sie", "noch", "nie", "mild", "mit", "der", "Thr\u00e4\u00b7ne", "ihm", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Aber ich weine dem Lear, und auch nicht, weil es dem K\u00f6nig", "tokens": ["A\u00b7ber", "ich", "wei\u00b7ne", "dem", "Le\u00b7ar", ",", "und", "auch", "nicht", ",", "weil", "es", "dem", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NE", "$,", "KON", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+--+--+---+-+-+-", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Mi\u00dflich ergeht in dem St\u00fcck, nein, weil ein Mensch es gemacht.", "tokens": ["Mi\u00df\u00b7lich", "er\u00b7geht", "in", "dem", "St\u00fcck", ",", "nein", ",", "weil", "ein", "Mensch", "es", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,", "PTKANT", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Ja, ich will es bekennen, da\u00df selbst die Reiter-Gesellschaft", "tokens": ["Ja", ",", "ich", "will", "es", "be\u00b7ken\u00b7nen", ",", "da\u00df", "selbst", "die", "Rei\u00b7ter\u00b7Ge\u00b7sell\u00b7schaft"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Mir heut' Abend den Thau s\u00fc\u00dfer Bewundrung entlockt.", "tokens": ["Mir", "heut'", "A\u00b7bend", "den", "Thau", "s\u00fc\u00b7\u00dfer", "Be\u00b7wund\u00b7rung", "ent\u00b7lockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Ist es dem Vogel nicht nah', dies zierliche M\u00e4dchen? Der J\u00fcngling,", "tokens": ["Ist", "es", "dem", "Vo\u00b7gel", "nicht", "nah'", ",", "dies", "zier\u00b7li\u00b7che", "M\u00e4d\u00b7chen", "?", "Der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NE", "PTKNEG", "PTKVZ", "$,", "PDS", "ADJA", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Beugt er dem dumpfen Gesetz irdischer Schwere sich noch?", "tokens": ["Beugt", "er", "dem", "dum\u00b7pfen", "Ge\u00b7setz", "ir\u00b7di\u00b7scher", "Schwe\u00b7re", "sich", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Und auf den Schultern des Bruders, das Kn\u00e4bchen, die Stellungen wechselnd,", "tokens": ["Und", "auf", "den", "Schul\u00b7tern", "des", "Bru\u00b7ders", ",", "das", "Kn\u00e4b\u00b7chen", ",", "die", "Stel\u00b7lun\u00b7gen", "wech\u00b7selnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Scheint's nicht lebendiger Thon, welcher nach Laune sich formt?", "tokens": ["Scheint's", "nicht", "le\u00b7ben\u00b7di\u00b7ger", "Thon", ",", "wel\u00b7cher", "nach", "Lau\u00b7ne", "sich", "formt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Gaukeln nicht Alle vor\u00fcber, wie gl\u00e4nzende Schatten, und zeigen,", "tokens": ["Gau\u00b7keln", "nicht", "Al\u00b7le", "vor\u00b7\u00fc\u00b7ber", ",", "wie", "gl\u00e4n\u00b7zen\u00b7de", "Schat\u00b7ten", ",", "und", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PIS", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "$,", "KON", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Da\u00df der Leib, wie der Geist, frei ist, sobald er nur will?", "tokens": ["Da\u00df", "der", "Leib", ",", "wie", "der", "Geist", ",", "frei", "ist", ",", "so\u00b7bald", "er", "nur", "will", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Ja, und w\u00fcrde auch Jedes ein Opfer des k\u00fchnsten Versuches,", "tokens": ["Ja", ",", "und", "w\u00fcr\u00b7de", "auch", "Je\u00b7des", "ein", "Op\u00b7fer", "des", "k\u00fchns\u00b7ten", "Ver\u00b7su\u00b7ches", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VAFIN", "ADV", "PIAT", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Den die Begeisterung wagt: st\u00fcrzt denn nicht Psyche noch sch\u00f6n,", "tokens": ["Den", "die", "Be\u00b7geis\u00b7te\u00b7rung", "wagt", ":", "st\u00fcrzt", "denn", "nicht", "Psy\u00b7che", "noch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$.", "VVFIN", "ADV", "PTKNEG", "NN", "ADV", "ADJD", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Wenn sie's im Taumel vergi\u00dft, da\u00df sie den tr\u00fcg'rischen Fittig", "tokens": ["Wenn", "sie's", "im", "Tau\u00b7mel", "ver\u00b7gi\u00dft", ",", "da\u00df", "sie", "den", "tr\u00fcg'\u00b7ri\u00b7schen", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Wieder zersch\u00fcttelt zu Sand, den sie zu muthig bewegt?", "tokens": ["Wie\u00b7der", "zer\u00b7sch\u00fct\u00b7telt", "zu", "Sand", ",", "den", "sie", "zu", "mut\u00b7hig", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}