{"textgrid.poem.30310": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Thiere gleich nicht reden, das weisen die Geberden;", "tokens": ["Was", "Thie\u00b7re", "gleich", "nicht", "re\u00b7den", ",", "das", "wei\u00b7sen", "die", "Ge\u00b7ber\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "PTKNEG", "VVINF", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Menschen werden reden, was sie nicht meinen werden,", "tokens": ["Die", "Men\u00b7schen", "wer\u00b7den", "re\u00b7den", ",", "was", "sie", "nicht", "mei\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "VAINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sind also bey den Menschen, als Thieren, mehr gef\u00e4rden.", "tokens": ["Sind", "al\u00b7so", "bey", "den", "Men\u00b7schen", ",", "als", "Thie\u00b7ren", ",", "mehr", "ge\u00b7f\u00e4r\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}