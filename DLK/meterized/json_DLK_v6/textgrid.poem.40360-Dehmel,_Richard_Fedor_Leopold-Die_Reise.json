{"textgrid.poem.40360": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Die Reise", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tipp, tapp, Stuhlbein,", "tokens": ["Tipp", ",", "tapp", ",", "Stuhl\u00b7bein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "h\u00fch, du sollst mein Pferdchen sein!", "tokens": ["h\u00fch", ",", "du", "sollst", "mein", "Pferd\u00b7chen", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klipp, klapp, Hutsche,", "tokens": ["Klipp", ",", "klapp", ",", "Hut\u00b7sche", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "du bist meine Kutsche,", "tokens": ["du", "bist", "mei\u00b7ne", "Kut\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "wutsch!", "tokens": ["wutsch", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Wipp, wapp, zu langsam;", "tokens": ["Wipp", ",", "wapp", ",", "zu", "lang\u00b7sam", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "hott, wir fahren Eisenbahn!", "tokens": ["hott", ",", "wir", "fah\u00b7ren", "Ei\u00b7sen\u00b7bahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle meine Pferde,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "um die ganze Erde,", "tokens": ["um", "die", "gan\u00b7ze", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "rrrutsch!", "tokens": ["rr\u00b7rutsch", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Tipp, tapp; zipp, zapp;", "tokens": ["Tipp", ",", "tapp", ";", "zipp", ",", "zapp", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "VVFIN", "$,", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "halt, wann geht das Luftschiff ab?", "tokens": ["halt", ",", "wann", "geht", "das", "Luft\u00b7schiff", "ab", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fertig, Kinder, eingestiegen,", "tokens": ["Fer\u00b7tig", ",", "Kin\u00b7der", ",", "ein\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wollen in den Himmel fliegen,", "tokens": ["wol\u00b7len", "in", "den", "Him\u00b7mel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "futsch!", "tokens": ["fut\u00b7sch", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}