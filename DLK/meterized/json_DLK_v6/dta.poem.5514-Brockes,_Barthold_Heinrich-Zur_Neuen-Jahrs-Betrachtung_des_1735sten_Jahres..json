{"dta.poem.5514": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zur Neuen-Jahrs-Betrachtung  \n des 1735sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey der wunderbahren Ordnung der Bewegung unsrer\nWelt,", "tokens": ["Bey", "der", "wun\u00b7der\u00b7bah\u00b7ren", "Ord\u00b7nung", "der", "Be\u00b7we\u00b7gung", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Draus dem menschlichen Geschlecht ein so grosser Nutz ent-", "tokens": ["Draus", "dem", "menschli\u00b7chen", "Ge\u00b7schlecht", "ein", "so", "gros\u00b7ser", "Nutz", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und wodurch der weise Sch\u00f6pfer alles n\u00e4hret und erh\u00e4lt,", "tokens": ["Und", "wo\u00b7durch", "der", "wei\u00b7se", "Sch\u00f6p\u00b7fer", "al\u00b7les", "n\u00e4h\u00b7ret", "und", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Ist es billig unsre Pflicht, da\u00df man ihm ein Opfer bringet;", "tokens": ["Ist", "es", "bil\u00b7lig", "uns\u00b7re", "Pflicht", ",", "da\u00df", "man", "ihm", "ein", "Op\u00b7fer", "brin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Sonderlich zu dieser Zeit, da die Fl\u00e4chen unsrer Erden,", "tokens": ["Son\u00b7der\u00b7lich", "zu", "die\u00b7ser", "Zeit", ",", "da", "die", "Fl\u00e4\u00b7chen", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Die von uns bewohnet sind, sich nicht mehr vom Sonnen-", "tokens": ["Die", "von", "uns", "be\u00b7woh\u00b7net", "sind", ",", "sich", "nicht", "mehr", "vom", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "VAFIN", "$,", "PRF", "PTKNEG", "ADV", "APPRART", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Wie bi\u00dfher geschehn, entfernen; sondern, welches jetzt ge-", "tokens": ["Wie", "bi\u00df\u00b7her", "ge\u00b7schehn", ",", "ent\u00b7fer\u00b7nen", ";", "son\u00b7dern", ",", "wel\u00b7ches", "jetzt", "ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVPP", "$,", "VVINF", "$.", "KON", "$,", "PRELS", "ADV", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Nach der W\u00e4rm\u2019 und Lebens-Quelle wieder hingelencket", "tokens": ["Nach", "der", "W\u00e4rm'", "und", "Le\u00b7bens\u00b7Quel\u00b7le", "wie\u00b7der", "hin\u00b7ge\u00b7len\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Die\u00df soll nun hierin bestehn, da\u00df wir, da im vor\u2019gen Jahr,", "tokens": ["Die\u00df", "soll", "nun", "hie\u00b7rin", "be\u00b7stehn", ",", "da\u00df", "wir", ",", "da", "im", "vor'\u00b7gen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VVINF", "$,", "KOUS", "PPER", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+----+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Unsrer Rede Wunder-Werck, GOtt zum Ruhm, betrach-", "tokens": ["Uns\u00b7rer", "Re\u00b7de", "Wun\u00b7der\u00b7\u00b7Werck", ",", "Gott", "zum", "Ruhm", ",", "be\u00b7trach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "NN", "APPRART", "NN", "$,", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Auch nunmehr von unsrer Seelen, die sich mit dem Ton", "tokens": ["Auch", "nun\u00b7mehr", "von", "uns\u00b7rer", "See\u00b7len", ",", "die", "sich", "mit", "dem", "Ton"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Und die, da sie in uns redet, ihn formiret, ihn beseelet,", "tokens": ["Und", "die", ",", "da", "sie", "in", "uns", "re\u00b7det", ",", "ihn", "for\u00b7mi\u00b7ret", ",", "ihn", "be\u00b7see\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Und so viele Wunder wirckt, etwas dem zum Ruhm ge-", "tokens": ["Und", "so", "vie\u00b7le", "Wun\u00b7der", "wirckt", ",", "et\u00b7was", "dem", "zum", "Ruhm", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "$,", "ADV", "ART", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Welcher mit so reger Kraft sie gew\u00fcrdigt zu beschencken.", "tokens": ["Wel\u00b7cher", "mit", "so", "re\u00b7ger", "Kraft", "sie", "ge\u00b7w\u00fcr\u00b7digt", "zu", "be\u00b7schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ADV", "ADJA", "NN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Aller Wesen HErr und Sch\u00f6pfer! ew'ger Ursprung\naller Kr\u00e4fte!", "tokens": ["Al\u00b7ler", "We\u00b7sen", "Herr", "und", "Sch\u00f6p\u00b7fer", "!", "ew'\u00b7ger", "Ur\u00b7sprung", "al\u00b7ler", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$.", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ew'ge Liebe, Macht und Weisheit! segne dieses\nmein Gesch\u00e4fte!", "tokens": ["Ew'\u00b7ge", "Lie\u00b7be", ",", "Macht", "und", "Weis\u00b7heit", "!", "seg\u00b7ne", "die\u00b7ses", "mein", "Ge\u00b7sch\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$.", "VVFIN", "PDAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Es gereiche mein Beginnen uns zur Andacht und zur\nLehre!", "tokens": ["Es", "ge\u00b7rei\u00b7che", "mein", "Be\u00b7gin\u00b7nen", "uns", "zur", "An\u00b7dacht", "und", "zur", "Leh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber auch absonderlich dir, HErr Zebaoth, zur Ehre!", "tokens": ["A\u00b7ber", "auch", "ab\u00b7son\u00b7der\u00b7lich", "dir", ",", "Herr", "Ze\u00b7bao\u00b7th", ",", "zur", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "$,", "NN", "NE", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}}}}