{"textgrid.poem.61131": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Leichter Sinn", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie w\u00e4r' es nicht zu tragen,", "tokens": ["Und", "wie", "w\u00e4r'", "es", "nicht", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Leben in der Welt?", "tokens": ["Die\u00b7ses", "Le\u00b7ben", "in", "der", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4glich wechseln Lust und Plagen,", "tokens": ["T\u00e4g\u00b7lich", "wech\u00b7seln", "Lust", "und", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was betr\u00fcbt, und was gef\u00e4llt.", "tokens": ["Was", "be\u00b7tr\u00fcbt", ",", "und", "was", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KON", "PWS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4gt die Zeit dir manche Wunde,", "tokens": ["Schl\u00e4gt", "die", "Zeit", "dir", "man\u00b7che", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche Freude bringt ihr Lauf;", "tokens": ["Man\u00b7che", "Freu\u00b7de", "bringt", "ihr", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber ", "tokens": ["A\u00b7ber"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Wiegt ein Jahr von Schmerzen auf.", "tokens": ["Wiegt", "ein", "Jahr", "von", "Schmer\u00b7zen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wisse nur das Gl\u00fcck zu fassen,", "tokens": ["Wis\u00b7se", "nur", "das", "Gl\u00fcck", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es l\u00e4chelnd dir sich beut!", "tokens": ["Wenn", "es", "l\u00e4\u00b7chelnd", "dir", "sich", "beut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Brust und auf den Gassen", "tokens": ["In", "der", "Brust", "und", "auf", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Such' es morgen, such' es heut.", "tokens": ["Such'", "es", "mor\u00b7gen", ",", "such'", "es", "heut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch bedr\u00e4ngt in deinem Kreise", "tokens": ["Doch", "be\u00b7dr\u00e4ngt", "in", "dei\u00b7nem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich ein fl\u00fcchtig Mi\u00dfgeschick,", "tokens": ["Dich", "ein", "fl\u00fcch\u00b7tig", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00e4chle leise, hoffe weise", "tokens": ["L\u00e4ch\u00b7le", "lei\u00b7se", ",", "hof\u00b7fe", "wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den n\u00e4chsten Augenblick.", "tokens": ["Auf", "den", "n\u00e4chs\u00b7ten", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur kein m\u00fc\u00dfig Schmerzbehagen!", "tokens": ["Nur", "kein", "m\u00fc\u00b7\u00dfig", "Schmerz\u00b7be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur kein weichlich Selbstverzeihn!", "tokens": ["Nur", "kein", "weich\u00b7lich", "Selbst\u00b7ver\u00b7zeihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommen Grillen, dich zu plagen,", "tokens": ["Kom\u00b7men", "Gril\u00b7len", ",", "dich", "zu", "pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiege sie mit Liedern ein.", "tokens": ["Wie\u00b7ge", "sie", "mit", "Lie\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Froh und ernst, doch immer heiter", "tokens": ["Froh", "und", "ernst", ",", "doch", "im\u00b7mer", "hei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leite dich die Poesie,", "tokens": ["Lei\u00b7te", "dich", "die", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und die Welle tr\u00e4gt dich weiter,", "tokens": ["Und", "die", "Wel\u00b7le", "tr\u00e4gt", "dich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und du wei\u00dft es selbst nicht wie.", "tokens": ["Und", "du", "wei\u00dft", "es", "selbst", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}