{"textgrid.poem.34907": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Asra", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00e4glich ging die wundersch\u00f6ne", "tokens": ["T\u00e4g\u00b7lich", "ging", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sultanstochter auf und nieder", "tokens": ["Sul\u00b7tan\u00b7stoch\u00b7ter", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Abendzeit am Springbrunn,", "tokens": ["Um", "die", "A\u00b7bend\u00b7zeit", "am", "Spring\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die wei\u00dfen Wasser pl\u00e4tschern.", "tokens": ["Wo", "die", "wei\u00b7\u00dfen", "Was\u00b7ser", "pl\u00e4t\u00b7schern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00e4glich stand der junge Sklave", "tokens": ["T\u00e4g\u00b7lich", "stand", "der", "jun\u00b7ge", "Skla\u00b7ve"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Abendzeit am Springbrunn,", "tokens": ["Um", "die", "A\u00b7bend\u00b7zeit", "am", "Spring\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die wei\u00dfen Wasser pl\u00e4tschern;", "tokens": ["Wo", "die", "wei\u00b7\u00dfen", "Was\u00b7ser", "pl\u00e4t\u00b7schern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich ward er bleich und bleicher.", "tokens": ["T\u00e4g\u00b7lich", "ward", "er", "bleich", "und", "blei\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eines Abends trat die F\u00fcrstin", "tokens": ["Ei\u00b7nes", "A\u00b7bends", "trat", "die", "F\u00fcrs\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf ihn zu mit raschen Worten:", "tokens": ["Auf", "ihn", "zu", "mit", "ra\u00b7schen", "Wor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbdeinen Namen will ich wissen,", "tokens": ["\u00bb", "dei\u00b7nen", "Na\u00b7men", "will", "ich", "wis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Heimat, deine Sippschaft!\u00ab", "tokens": ["Dei\u00b7ne", "Hei\u00b7mat", ",", "dei\u00b7ne", "Sipp\u00b7schaft", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Sklave sprach: \u00bbIch hei\u00dfe", "tokens": ["Und", "der", "Skla\u00b7ve", "sprach", ":", "\u00bb", "Ich", "hei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mohamet, ich bin aus Yemmen,", "tokens": ["Mo\u00b7ha\u00b7met", ",", "ich", "bin", "aus", "Yem\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mein Stamm sind jene Asra,", "tokens": ["Und", "mein", "Stamm", "sind", "je\u00b7ne", "As\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Welche sterben, wenn sie lieben.\u00ab", "tokens": ["Wel\u00b7che", "ster\u00b7ben", ",", "wenn", "sie", "lie\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAT", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}