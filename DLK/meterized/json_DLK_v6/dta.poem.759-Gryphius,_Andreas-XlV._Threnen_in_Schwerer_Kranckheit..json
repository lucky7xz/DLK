{"dta.poem.759": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlV.  Threnen in Schwerer Kranckheit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mjr ist ich wei\u00df nicht wie/ ich seufftze f\u00fcr vnd f\u00fcr. ", "tokens": ["Mjr", "ist", "ich", "wei\u00df", "nicht", "wie", "/", "ich", "seufft\u00b7ze", "f\u00fcr", "vnd", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$(", "PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich weyne tag vnd nacht/ ich sitz in tausend schmertzen; ", "tokens": ["Ich", "wey\u00b7ne", "tag", "vnd", "nacht", "/", "ich", "sitz", "in", "tau\u00b7send", "schmert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$(", "PPER", "PRF", "APPR", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd tausend f\u00fcrcht ich noch/ die krafft in meinem hertzen", "tokens": ["Vnd", "tau\u00b7send", "f\u00fcrcht", "ich", "noch", "/", "die", "krafft", "in", "mei\u00b7nem", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verschwind/ der geist verschmacht/ die h\u00e4nde sincken mir.", "tokens": ["Ver\u00b7schwind", "/", "der", "geist", "ver\u00b7schmacht", "/", "die", "h\u00e4n\u00b7de", "sin\u00b7cken", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "VVPP", "$(", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die wangen werden bleich/ der sch\u00f6nen Augen zier ", "tokens": ["Die", "wan\u00b7gen", "wer\u00b7den", "bleich", "/", "der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergeht/ gleich als der Schnee der schon verbrandten kertzen", "tokens": ["Ver\u00b7geht", "/", "gleich", "als", "der", "Schnee", "der", "schon", "ver\u00b7brand\u00b7ten", "kert\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "ADV", "KOKOM", "ART", "NN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seele wird besturmbt gleich wie die ", "tokens": ["Die", "See\u00b7le", "wird", "be\u00b7sturmbt", "gleich", "wie", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "ADV", "KOKOM", "ART"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Was ist di\u00df leben doch", "tokens": ["Was", "ist", "di\u00df", "le\u00b7ben", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was bilden wir vns eyn! was w\u00fcndschen wir zu haben?", "tokens": ["Was", "bil\u00b7den", "wir", "vns", "eyn", "!", "was", "w\u00fcnd\u00b7schen", "wir", "zu", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "VVINF", "$.", "PWS", "VVFIN", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jtzt sindt wir hoch vnd gro\u00df/ vnd morgen schon vergraben:", "tokens": ["Jtzt", "sindt", "wir", "hoch", "vnd", "gro\u00df", "/", "vnd", "mor\u00b7gen", "schon", "ver\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$(", "KON", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt Blumen/ morgen kot/ wir sindt ein windt/ ein schaum.", "tokens": ["Jtzt", "Blu\u00b7men", "/", "mor\u00b7gen", "kot", "/", "wir", "sindt", "ein", "windt", "/", "ein", "schaum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ADV", "VVFIN", "$(", "PPER", "VAFIN", "ART", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Nebel/ eine Bach/ ein reiff/ ein Taw\u2019 ein schaten.", "tokens": ["Ein", "Ne\u00b7bel", "/", "ei\u00b7ne", "Bach", "/", "ein", "reiff", "/", "ein", "Ta\u00b7w'", "ein", "scha\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "ADJD", "$(", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jtzt was vnd morgen nichts/ vnd was sind vnser thaten?", "tokens": ["Jtzt", "was", "vnd", "mor\u00b7gen", "nichts", "/", "vnd", "was", "sind", "vn\u00b7ser", "tha\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "KON", "ADV", "PIS", "$(", "KON", "PWS", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ein/ mit herber angst durchau\u00df vermischter Traum.", "tokens": ["Als", "ein", "/", "mit", "her\u00b7ber", "angst", "durch\u00b7au\u00df", "ver\u00b7mischter", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}