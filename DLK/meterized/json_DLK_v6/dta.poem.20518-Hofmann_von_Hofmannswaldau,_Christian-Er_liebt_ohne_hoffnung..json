{"dta.poem.20518": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Er liebt ohne hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was denckt ihr doch/ ihr k\u00fchnen sinnen?", "tokens": ["Was", "denckt", "ihr", "doch", "/", "ihr", "k\u00fch\u00b7nen", "sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr geht auff allzuhoher bahn;", "tokens": ["Ihr", "geht", "auff", "all\u00b7zu\u00b7ho\u00b7her", "bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn euer frevelndes beginnen", "tokens": ["Denn", "eu\u00b7er", "fre\u00b7veln\u00b7des", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will weiter/ als es steigen kan;", "tokens": ["Will", "wei\u00b7ter", "/", "als", "es", "stei\u00b7gen", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ihr dasselbe lieben wollet/", "tokens": ["Weil", "ihr", "das\u00b7sel\u00b7be", "lie\u00b7ben", "wol\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ihr doch nur anbeten sollet.", "tokens": ["Was", "ihr", "doch", "nur", "an\u00b7be\u00b7ten", "sol\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "--+----+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Die gantze welt steht euch ja offen/", "tokens": ["Die", "gant\u00b7ze", "welt", "steht", "euch", "ja", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und steckt nicht voll unm\u00f6glichkeit.", "tokens": ["Und", "steckt", "nicht", "voll", "un\u00b7m\u00f6g\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum liebt ihr denn sonder hoffen/", "tokens": ["Wa\u00b7rum", "liebt", "ihr", "denn", "son\u00b7der", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was euch die liebe doch verbeut.", "tokens": ["Was", "euch", "die", "lie\u00b7be", "doch", "ver\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo furcht und noth gesetze schreiben/", "tokens": ["Wo", "furcht", "und", "noth", "ge\u00b7set\u00b7ze", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df lieb und lust zur\u00fccke bleiben.", "tokens": ["Mu\u00df", "lieb", "und", "lust", "zu\u00b7r\u00fc\u00b7cke", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KON", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedoch ich wei\u00df nicht/ was ich sage/", "tokens": ["Je\u00b7doch", "ich", "wei\u00df", "nicht", "/", "was", "ich", "sa\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich will nicht/ was ich wollen soll.", "tokens": ["Ich", "will", "nicht", "/", "was", "ich", "wol\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$(", "PWS", "PPER", "VMFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darum erwehl ich nur die plage/", "tokens": ["Da\u00b7rum", "er\u00b7wehl", "ich", "nur", "die", "pla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und such im finstern meinen pol;", "tokens": ["Und", "such", "im", "fins\u00b7tern", "mei\u00b7nen", "pol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht kan mir der schmertz/ wie bienen", "tokens": ["Viel\u00b7leicht", "kan", "mir", "der", "schmertz", "/", "wie", "bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "$(", "KOKOM", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die nesseln/ noch zu honig dienen.", "tokens": ["Die", "nes\u00b7seln", "/", "noch", "zu", "ho\u00b7nig", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hab ich nicht hoffnung das zu kriegen/", "tokens": ["Hab", "ich", "nicht", "hoff\u00b7nung", "das", "zu", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "NN", "ART", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich mit flammen \u00fcberstreut;", "tokens": ["Was", "mich", "mit", "flam\u00b7men", "\u00fc\u00b7bers\u00b7treut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So qvillet dennoch mein vergn\u00fcgen", "tokens": ["So", "qvil\u00b7let", "den\u00b7noch", "mein", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch noch aus der unm\u00f6glichkeit:", "tokens": ["Auch", "noch", "aus", "der", "un\u00b7m\u00f6g\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Denn darff ich gleich die glut nicht nennen/", "tokens": ["Denn", "darff", "ich", "gleich", "die", "glut", "nicht", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich doch in gedancken brennen.", "tokens": ["Will", "ich", "doch", "in", "ge\u00b7dan\u00b7cken", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und also lieb ich mein verderben/", "tokens": ["Und", "al\u00b7so", "lieb", "ich", "mein", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und heg ein feur in meiner brust/", "tokens": ["Und", "heg", "ein", "feur", "in", "mei\u00b7ner", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Andem ich noch zuletzt mu\u00df sterben.", "tokens": ["An\u00b7dem", "ich", "noch", "zu\u00b7letzt", "mu\u00df", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein untergang ist mir bewust:", "tokens": ["Mein", "un\u00b7ter\u00b7gang", "ist", "mir", "be\u00b7wust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das macht: ich habe lieben wollen/", "tokens": ["Das", "macht", ":", "ich", "ha\u00b7be", "lie\u00b7ben", "wol\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich doch nur anbeten sollen.", "tokens": ["Was", "ich", "doch", "nur", "an\u00b7be\u00b7ten", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}