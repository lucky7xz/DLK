{"dta.poem.2676": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtn\u00fc\u00df  \n  Hn. G. A. K\u00e4is. H. u. W. der St. B. den 11.  \n Aug. 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun hastu wahre Ruh' und Freyheit dir erstritten/", "tokens": ["Nun", "has\u00b7tu", "wah\u00b7re", "Ruh'", "und", "Frey\u00b7heit", "dir", "er\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "KON", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gehst den Siegern gleich ins Schlo\u00df der Freuden", "tokens": ["Du", "gehst", "den", "Sie\u00b7gern", "gleich", "ins", "Schlo\u00df", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun kan kein eintzig Feind dir mehr die Stirne bieten/", "tokens": ["Nun", "kan", "kein", "eint\u00b7zig", "Feind", "dir", "mehr", "die", "Stir\u00b7ne", "bie\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ADJD", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil S\u00fcnde/ Welt und Tod gantz ausgetilget seyn.", "tokens": ["Weil", "S\u00fcn\u00b7de", "/", "Welt", "und", "Tod", "gantz", "aus\u00b7ge\u00b7til\u00b7get", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er mag ins Capitol ein R\u00f6mer Lorbern tragen/", "tokens": ["Er", "mag", "ins", "Ca\u00b7pi\u00b7tol", "ein", "R\u00f6\u00b7mer", "Lor\u00b7bern", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn dich die g\u00f6ldne Kron der Ewigkeiten ziert.", "tokens": ["Wenn", "dich", "die", "g\u00f6ld\u00b7ne", "Kron", "der", "E\u00b7wig\u00b7kei\u00b7ten", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir h\u00f6ren umb dein Grab den Nachruhm dieses sagen/", "tokens": ["Wir", "h\u00f6\u00b7ren", "umb", "dein", "Grab", "den", "Nach\u00b7ruhm", "die\u00b7ses", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PDAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df/ ", "tokens": ["Da\u00df", "/"], "token_info": ["word", "punct"], "pos": ["KOUS", "$("], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Wie stimm\u2019 ich dieses an? die sch\u00fcchteren Camenen", "tokens": ["Wie", "stimm'", "ich", "die\u00b7ses", "an", "?", "die", "sch\u00fcch\u00b7te\u00b7ren", "Ca\u00b7me\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "(b\u00fchrt.", "tokens": ["(", "b\u00fchrt", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Und Ph\u00f6bus Lauten-Spiel stehn bey Soldaten nicht.", "tokens": ["Und", "Ph\u00f6\u00b7bus", "Lau\u00b7ten\u00b7Spiel", "stehn", "bey", "Sol\u00b7da\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hier must dir Mavors selbst ein Donner-Lied erth\u00f6nen/", "tokens": ["Hier", "must", "dir", "Ma\u00b7vors", "selbst", "ein", "Don\u00b7ner\u00b7Lied", "er\u00b7th\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das durch die L\u00fcffte saust und durch die Ohren bricht.", "tokens": ["Das", "durch", "die", "L\u00fcff\u00b7te", "saust", "und", "durch", "die", "Oh\u00b7ren", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mich d\u00fcnckt ich seh auch schon/ wie sich Bellona m\u00fchet/", "tokens": ["Mich", "d\u00fcnckt", "ich", "seh", "auch", "schon", "/", "wie", "sich", "Bel\u00b7lo\u00b7na", "m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "$(", "PWAV", "PRF", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den letzten Ehren-Dienst mit ihrer Pracht zu thun.", "tokens": ["Den", "letz\u00b7ten", "Eh\u00b7ren\u00b7Dienst", "mit", "ih\u00b7rer", "Pracht", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie sich ein blanckes Schwerd/ das noch gef\u00e4rbet sihet/", "tokens": ["Wie", "sich", "ein", "blan\u00b7ckes", "Schwerd", "/", "das", "noch", "ge\u00b7f\u00e4r\u00b7bet", "si\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "$(", "PDS", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Als Bild der Tapfferkeit heist auff der Bahre ruhn.", "tokens": ["Als", "Bild", "der", "Tapf\u00b7fer\u00b7keit", "heist", "auff", "der", "Bah\u00b7re", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kein Blumwerck gilt hier nicht. Der Hagel von Musqueten/", "tokens": ["Kein", "Blum\u00b7werck", "gilt", "hier", "nicht", ".", "Der", "Ha\u00b7gel", "von", "Mus\u00b7que\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PTKNEG", "$.", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Fahnen rother Flug/ der Trummeln rauher Klang/", "tokens": ["Der", "Fah\u00b7nen", "ro\u00b7ther", "Flug", "/", "der", "Trum\u00b7meln", "rau\u00b7her", "Klang", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Spie\u00df- und Degen-Schall/ das Blitzen von Falckneten", "tokens": ["Der", "Spie\u00df", "und", "De\u00b7gen\u00b7Schall", "/", "das", "Blit\u00b7zen", "von", "Falck\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "$(", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Singt dir/ ", "tokens": ["Singt", "dir", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Und die Kleinodien/ so deine Leiche schm\u00fccken/", "tokens": ["Und", "die", "Klein\u00b7o\u00b7di\u00b7en", "/", "so", "dei\u00b7ne", "Lei\u00b7che", "schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Sind Wunden/ die du jetzt als Purpur-Binden tr\u00e4gst.", "tokens": ["Sind", "Wun\u00b7den", "/", "die", "du", "jetzt", "als", "Pur\u00b7pur\u00b7Bin\u00b7den", "tr\u00e4gst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "PRELS", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Himmel konte dich nicht herrlicher begl\u00fccken/", "tokens": ["Der", "Him\u00b7mel", "kon\u00b7te", "dich", "nicht", "herr\u00b7li\u00b7cher", "be\u00b7gl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Als da\u00df du Ehren-satt dich so zur Ruhe legst.", "tokens": ["Als", "da\u00df", "du", "Eh\u00b7ren\u00b7satt", "dich", "so", "zur", "Ru\u00b7he", "legst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du tapfferer Soldat/ der in den ersten Jahren", "tokens": ["Du", "tapf\u00b7fe\u00b7rer", "Sol\u00b7dat", "/", "der", "in", "den", "ers\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sich bald ins Feld gemacht/ und Rauch und Dampff gesehn/", "tokens": ["Sich", "bald", "ins", "Feld", "ge\u00b7macht", "/", "und", "Rauch", "und", "Dampff", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "VVPP", "$(", "KON", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Viel Ungemach erduldt/ viel Ungel\u00fcck erfahren/", "tokens": ["Viel", "Un\u00b7ge\u00b7mach", "er\u00b7duldt", "/", "viel", "Un\u00b7ge\u00b7l\u00fcck", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$(", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Verlacht Hitz oder K\u00e4lt/ und scharffer Winde wehn/", "tokens": ["Ver\u00b7lacht", "Hitz", "o\u00b7der", "K\u00e4lt", "/", "und", "scharf\u00b7fer", "Win\u00b7de", "wehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$(", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bist von der Picken an zum Hauptmanns-Stab gestiegen:", "tokens": ["Bist", "von", "der", "Pi\u00b7cken", "an", "zum", "Haupt\u00b7manns\u00b7Stab", "ge\u00b7stie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach wie viel beissen nicht/ eh\u2019 di\u00df geschicht/ in Sand!", "tokens": ["Ach", "wie", "viel", "beis\u00b7sen", "nicht", "/", "eh'", "di\u00df", "ge\u00b7schicht", "/", "in", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADV", "PTKNEG", "$(", "KOUS", "PDS", "VVPP", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und deine Tapfferkeit in den so vielen Z\u00fcgen", "tokens": ["Und", "dei\u00b7ne", "Tapf\u00b7fer\u00b7keit", "in", "den", "so", "vie\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hat zwar der Freund ger\u00fchmt/ noch mehr der Feind erkant.", "tokens": ["Hat", "zwar", "der", "Freund", "ge\u00b7r\u00fchmt", "/", "noch", "mehr", "der", "Feind", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$(", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es preiset Freyberg noch den L\u00f6wen-Muth bey St\u00fcrmen/", "tokens": ["Es", "prei\u00b7set", "Frey\u00b7berg", "noch", "den", "L\u00f6\u00b7wen\u00b7Muth", "bey", "St\u00fcr\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie dein bew\u00e4hrter Arm so manchen hat gest\u00fcrtzt/", "tokens": ["Wie", "dein", "be\u00b7w\u00e4hr\u00b7ter", "Arm", "so", "man\u00b7chen", "hat", "ge\u00b7st\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADV", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wie deine Wachsamkeit bey schon erstiegnen Th\u00fcrmen", "tokens": ["Wie", "dei\u00b7ne", "Wach\u00b7sam\u00b7keit", "bey", "schon", "er\u00b7sti\u00b7eg\u00b7nen", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Dem eingefallnem Volck den fernern Weg verk\u00fcrtzt.", "tokens": ["Dem", "ein\u00b7ge\u00b7fall\u00b7nem", "Volck", "den", "fer\u00b7nern", "Weg", "ver\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Brieg siht dich noch behertzt aus seinen Mauren setzen/", "tokens": ["Brieg", "siht", "dich", "noch", "be\u00b7hertzt", "aus", "sei\u00b7nen", "Mau\u00b7ren", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wie dein blasser Arm so unerm\u00fcdet schlacht/", "tokens": ["Und", "wie", "dein", "blas\u00b7ser", "Arm", "so", "un\u00b7er\u00b7m\u00fc\u00b7det", "schlacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ja springt die Mine gleich/ sie kan dich nicht verletzen/", "tokens": ["Ja", "springt", "die", "Mi\u00b7ne", "gleich", "/", "sie", "kan", "dich", "nicht", "ver\u00b7let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ART", "NN", "ADV", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Weil GOttes G\u00fcttigkeit auch \u00fcber Helden wacht.", "tokens": ["Weil", "Got\u00b7tes", "G\u00fct\u00b7tig\u00b7keit", "auch", "\u00fc\u00b7ber", "Hel\u00b7den", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Du Edler Krieges-Mann/ der keinen Feind gescheuet/", "tokens": ["Du", "Ed\u00b7ler", "Krie\u00b7ge\u00b7sMann", "/", "der", "kei\u00b7nen", "Feind", "ge\u00b7scheu\u00b7et", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexa