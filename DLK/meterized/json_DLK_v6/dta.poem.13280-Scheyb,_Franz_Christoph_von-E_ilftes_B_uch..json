{"dta.poem.13280": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egemahl\u201e, so sprache sie, verlasse du dich nur", "tokens": ["\u201e", "ge\u00b7mahl", "\u201e", ",", "so", "spra\u00b7che", "sie", ",", "ver\u00b7las\u00b7se", "du", "dich", "nur"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eauf dieses Kreuzes Schuz, den unser Haus erfuhr!", "tokens": ["\u201e", "auf", "die\u00b7ses", "Kreu\u00b7zes", "Schuz", ",", "den", "un\u00b7ser", "Haus", "er\u00b7fuhr", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ejhr aber, Tugenden! und niemand ausgenommen,", "tokens": ["\u201e", "jhr", "a\u00b7ber", ",", "Tu\u00b7gen\u00b7den", "!", "und", "nie\u00b7mand", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "NN", "$.", "KON", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201emacht, da\u00df der Adler nicht umsonst zur\u00fcck gekommen!", "tokens": ["\u201e", "macht", ",", "da\u00df", "der", "Ad\u00b7ler", "nicht", "um\u00b7sonst", "zu\u00b7r\u00fcck", "ge\u00b7kom\u00b7men", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADV", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "\u201ebegleitet den Gemahl mit der gewohnten Pflicht", "tokens": ["\u201e", "be\u00b7glei\u00b7tet", "den", "Ge\u00b7mahl", "mit", "der", "ge\u00b7wohn\u00b7ten", "Pflicht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "610\u201dUnd scheuet unsers Feinds zahlreiche Waffen nicht.", "tokens": ["\"", "Und", "scheu\u00b7et", "un\u00b7sers", "Feinds", "zahl\u00b7rei\u00b7che", "Waf\u00b7fen", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eindessen la\u00df\u2019 ich GOtt und jenen Priester walten:", "tokens": ["\u201e", "in\u00b7des\u00b7sen", "la\u00df'", "ich", "Gott", "und", "je\u00b7nen", "Pries\u00b7ter", "wal\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "KON", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie seynds, die meinen Thron besch\u00fczt, und aufrecht halten.", "tokens": ["\u201e", "die", "seynds", ",", "die", "mei\u00b7nen", "Thron", "be\u00b7sch\u00fczt", ",", "und", "auf\u00b7recht", "hal\u00b7ten", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}