{"textgrid.poem.58926": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "145. Merk auf und folg!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir uns ganz der Weisheit schenken,", "tokens": ["Wenn", "wir", "uns", "ganz", "der", "Weis\u00b7heit", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00fchrt sie weiter, als wir denken;", "tokens": ["So", "f\u00fchrt", "sie", "wei\u00b7ter", ",", "als", "wir", "den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Merk nur auf ihren Zug im Grunde", "tokens": ["Merk", "nur", "auf", "ih\u00b7ren", "Zug", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und folge dem zu aller Stunde!", "tokens": ["Und", "fol\u00b7ge", "dem", "zu", "al\u00b7ler", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}