{"textgrid.poem.49714": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Altbrandenburgisch", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Gott \u2013 f\u00fcr K\u00f6nig \u2013 und f\u00fcrs Vaterland!", "tokens": ["Mit", "Gott", "\u2013", "f\u00fcr", "K\u00f6\u00b7nig", "\u2013", "und", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "NN", "$(", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir kennen die Devise als geschichtlich", "tokens": ["Wir", "ken\u00b7nen", "die", "De\u00b7vi\u00b7se", "als", "ge\u00b7schicht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wissen auch, da\u00df Preu\u00dfens Adelsstand", "tokens": ["Und", "wis\u00b7sen", "auch", ",", "da\u00df", "Preu\u00b7\u00dfens", "A\u00b7delss\u00b7tand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich von ihr leiten l\u00e4\u00dft, ganz offensichtlich.", "tokens": ["Sich", "von", "ihr", "lei\u00b7ten", "l\u00e4\u00dft", ",", "ganz", "of\u00b7fen\u00b7sicht\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVINF", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Gott \u2013 der protestantischen Kul\u00f6r \u2013", "tokens": ["Mit", "Gott", "\u2013", "der", "pro\u00b7tes\u00b7tan\u00b7ti\u00b7schen", "Ku\u00b7l\u00f6r", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch kann man sich f\u00fcr Geld dazu verstehen", "tokens": ["Doch", "kann", "man", "sich", "f\u00fcr", "Geld", "da\u00b7zu", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "PRF", "APPR", "NN", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ist katholisch noch so contre coeur,", "tokens": ["Und", "ist", "ka\u00b7tho\u00b7lisch", "noch", "so", "con\u00b7tre", "co\u00b7eur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "ADV", "FM", "FM", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit Rom und Zentrum Arm in Arm zu gehen.", "tokens": ["Mit", "Rom", "und", "Zent\u00b7rum", "Arm", "in", "Arm", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und f\u00fcr den K\u00f6nig! Majest\u00e4t geh\u00f6rt", "tokens": ["Und", "f\u00fcr", "den", "K\u00f6\u00b7nig", "!", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein jedes der Gef\u00fchle, die nicht rosten,", "tokens": ["Ein", "je\u00b7des", "der", "Ge\u00b7f\u00fch\u00b7le", ",", "die", "nicht", "ros\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deren Heilighaltung jeder schw\u00f6rt,", "tokens": ["Und", "de\u00b7ren", "Hei\u00b7lig\u00b7hal\u00b7tung", "je\u00b7der", "schw\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und die \u2013 was Vorbedingung ist \u2013 nichts kosten.", "tokens": ["Und", "die", "\u2013", "was", "Vor\u00b7be\u00b7din\u00b7gung", "ist", "\u2013", "nichts", "kos\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "PWS", "NN", "VAFIN", "$(", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dem Vaterlande weiht die Adelsschar", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de", "weiht", "die", "A\u00b7dels\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit breiten M\u00e4ulern jederlei Gesinnung,", "tokens": ["Mit", "brei\u00b7ten", "M\u00e4u\u00b7lern", "je\u00b7der\u00b7lei", "Ge\u00b7sin\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch fordert weiter nichts! Besonders bar", "tokens": ["Doch", "for\u00b7dert", "wei\u00b7ter", "nichts", "!", "Be\u00b7son\u00b7ders", "bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bezahlt sie nicht, die Patrioteninnung.", "tokens": ["Be\u00b7zahlt", "sie", "nicht", ",", "die", "Pat\u00b7ri\u00b7o\u00b7te\u00b7nin\u00b7nung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und so mit Gott \u2013 die Phrase bleibt in Kraft \u2013", "tokens": ["Und", "so", "mit", "Gott", "\u2013", "die", "Phra\u00b7se", "bleibt", "in", "Kraft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$(", "ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht f\u00fcr den K\u00f6nig \u2013 f\u00fcr die eignen Taschen \u2013", "tokens": ["Nicht", "f\u00fcr", "den", "K\u00f6\u00b7nig", "\u2013", "f\u00fcr", "die", "eig\u00b7nen", "Ta\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht f\u00fcr das Land \u2013 f\u00fcr die Genossenschaft", "tokens": ["Nicht", "f\u00fcr", "das", "Land", "\u2013", "f\u00fcr", "die", "Ge\u00b7nos\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sucht jeden Vorteil schmutzig ihr zu haschen.", "tokens": ["Sucht", "je\u00b7den", "Vor\u00b7teil", "schmut\u00b7zig", "ihr", "zu", "ha\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}