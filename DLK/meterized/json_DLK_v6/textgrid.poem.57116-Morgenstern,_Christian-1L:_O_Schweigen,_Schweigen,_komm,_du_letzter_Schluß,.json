{"textgrid.poem.57116": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Schweigen, Schweigen, komm, du letzter Schlu\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schweigen, Schweigen, komm, du letzter Schlu\u00df,", "tokens": ["O", "Schwei\u00b7gen", ",", "Schwei\u00b7gen", ",", "komm", ",", "du", "letz\u00b7ter", "Schlu\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da mitzuteilen Ha\u00df nur weckt und Fehde.", "tokens": ["da", "mit\u00b7zu\u00b7tei\u00b7len", "Ha\u00df", "nur", "weckt", "und", "Feh\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ergreif an ihrer Wurzel meine Rede,", "tokens": ["Er\u00b7greif", "an", "ih\u00b7rer", "Wur\u00b7zel", "mei\u00b7ne", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "la\u00df einw\u00e4rts sprossen, was denn sprossen mu\u00df.", "tokens": ["la\u00df", "ein\u00b7w\u00e4rts", "spros\u00b7sen", ",", "was", "denn", "spros\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will dich tragen, wohin niemand kommt,", "tokens": ["Ich", "will", "dich", "tra\u00b7gen", ",", "wo\u00b7hin", "nie\u00b7mand", "kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in W\u00e4lder, wo nur Tiere uns erfahren, \u2013", "tokens": ["in", "W\u00e4l\u00b7der", ",", "wo", "nur", "Tie\u00b7re", "uns", "er\u00b7fah\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "ADJA", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bis du vielleicht nach vielen, vielen Jahren", "tokens": ["bis", "du", "viel\u00b7leicht", "nach", "vie\u00b7len", ",", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Wort mir schenkst, das mir und andern frommt.", "tokens": ["das", "Wort", "mir", "schenkst", ",", "das", "mir", "und", "an\u00b7dern", "frommt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "PPER", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann la\u00df mich noch einmal vor Menschen stehn", "tokens": ["Dann", "la\u00df", "mich", "noch", "ein\u00b7mal", "vor", "Men\u00b7schen", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und ihnen dieses eine Tiefste sagen \u2013", "tokens": ["und", "ih\u00b7nen", "die\u00b7ses", "ei\u00b7ne", "Tiefs\u00b7te", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDAT", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und dich dann wieder in die W\u00e4lder tragen", "tokens": ["und", "dich", "dann", "wie\u00b7der", "in", "die", "W\u00e4l\u00b7der", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und wie ein Wild dort fallen und vergehn.", "tokens": ["und", "wie", "ein", "Wild", "dort", "fal\u00b7len", "und", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}