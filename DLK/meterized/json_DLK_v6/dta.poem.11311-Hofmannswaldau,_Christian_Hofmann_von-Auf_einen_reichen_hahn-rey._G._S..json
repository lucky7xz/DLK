{"dta.poem.11311": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen reichen hahn-rey.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein pr\u00e4chtiger pallast,", "tokens": ["Dein", "pr\u00e4ch\u00b7ti\u00b7ger", "pal\u00b7last", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der sch\u00f6nen kinder meng\u2019, und alles, was du hast,", "tokens": ["Der", "sch\u00f6\u00b7nen", "kin\u00b7der", "meng'", ",", "und", "al\u00b7les", ",", "was", "du", "hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "KON", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bleibt ohne widerspruch stets deine.", "tokens": ["Bleibt", "oh\u00b7ne", "wi\u00b7der\u00b7spruch", "stets", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00dfwegen l\u00e4\u00dft dich auch ein jeder gern in ruh:", "tokens": ["De\u00df\u00b7we\u00b7gen", "l\u00e4\u00dft", "dich", "auch", "ein", "je\u00b7der", "gern", "in", "ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein nettes weib alleine", "tokens": ["Dein", "net\u00b7tes", "weib", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steht anderu leuten zu.", "tokens": ["Steht", "an\u00b7de\u00b7ru", "leu\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}