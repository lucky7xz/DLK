{"dta.poem.21189": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In siebenf\u00e4ltge Nacht", "tokens": ["In", "sie\u00b7ben\u00b7f\u00e4lt\u00b7ge", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Neigt sich das Haupt des Sohns der Allmacht\nhin.", "tokens": ["Neigt", "sich", "das", "Haupt", "des", "Sohns", "der", "All\u00b7macht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er gab den Thron des Himmels, Glanz und\nMacht", "tokens": ["Er", "gab", "den", "Thron", "des", "Him\u00b7mels", ",", "Glanz", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr S\u00fcnder hin.", "tokens": ["F\u00fcr", "S\u00fcn\u00b7der", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und dennoch liegen die Geschlechter", "tokens": ["Und", "den\u00b7noch", "lie\u00b7gen", "die", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Unempfindlichkeit?", "tokens": ["In", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer sah vom Himmel mehr Barmherzigkeit,", "tokens": ["Wer", "sah", "vom", "Him\u00b7mel", "mehr", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und von der Erde mehr Undankbarkeit?", "tokens": ["Und", "von", "der", "Er\u00b7de", "mehr", "Un\u00b7dank\u00b7bar\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}}}}}