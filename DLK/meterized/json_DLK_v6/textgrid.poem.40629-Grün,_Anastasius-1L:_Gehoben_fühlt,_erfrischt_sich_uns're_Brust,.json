{"textgrid.poem.40629": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gehoben f\u00fchlt, erfrischt sich uns're Brust,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehoben f\u00fchlt, erfrischt sich uns're Brust,", "tokens": ["Ge\u00b7ho\u00b7ben", "f\u00fchlt", ",", "er\u00b7frischt", "sich", "un\u00b7s'\u00b7re", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wenn wir den vollen Kreis der H\u00f6rer sehn", "tokens": ["Wenn", "wir", "den", "vol\u00b7len", "Kreis", "der", "H\u00f6\u00b7rer", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versammelt, mitzubauen an dem Mal,", "tokens": ["Ver\u00b7sam\u00b7melt", ",", "mit\u00b7zu\u00b7bau\u00b7en", "an", "dem", "Mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVIZU", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das nicht nur Denkmal, auch ein Dankmal sei", "tokens": ["Das", "nicht", "nur", "Denk\u00b7mal", ",", "auch", "ein", "Dank\u00b7mal", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "ADV", "NN", "$,", "ADV", "ART", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem gro\u00dfen Geist, dem Deutschland, dem die Welt", "tokens": ["Dem", "gro\u00b7\u00dfen", "Geist", ",", "dem", "Deutschland", ",", "dem", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "So viel des Ruhms, doch mehr der Liebe zollt.", "tokens": ["So", "viel", "des", "Ruhms", ",", "doch", "mehr", "der", "Lie\u00b7be", "zollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und doch, und doch \u2013 ein leiser Zweifel fr\u00e4gt", "tokens": ["Und", "doch", ",", "und", "doch", "\u2013", "ein", "lei\u00b7ser", "Zwei\u00b7fel", "fr\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wohl da und dort, ob nicht in solchem Ziel", "tokens": ["Wohl", "da", "und", "dort", ",", "ob", "nicht", "in", "sol\u00b7chem", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "$,", "KOUS", "PTKNEG", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auch eine Krankheit schleiche dieser Zeit,", "tokens": ["Auch", "ei\u00b7ne", "Krank\u00b7heit", "schlei\u00b7che", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Im Gr\u00f6\u00dfencultus kleiner Bilderdienst?", "tokens": ["Im", "Gr\u00f6\u00b7\u00dfen\u00b7cul\u00b7tus", "klei\u00b7ner", "Bil\u00b7der\u00b7dienst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch Antwort gibt der Dichterheros selbst:", "tokens": ["Doch", "Ant\u00b7wort", "gibt", "der", "Dich\u00b7ter\u00b7he\u00b7ros", "selbst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdie sch\u00f6ne Seele kennt kein s\u00fc\u00dfer Gl\u00fcck,", "tokens": ["\u00bb", "die", "sch\u00f6\u00b7ne", "See\u00b7le", "kennt", "kein", "s\u00fc\u00b7\u00dfer", "Gl\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als au\u00dferhalb verwirklicht auch zu sehn", "tokens": ["Als", "au\u00b7\u00dfer\u00b7halb", "ver\u00b7wirk\u00b7licht", "auch", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Edle, Sch\u00f6ne, das sie in sich tr\u00e4gt.\u00ab", "tokens": ["Das", "Ed\u00b7le", ",", "Sch\u00f6\u00b7ne", ",", "das", "sie", "in", "sich", "tr\u00e4gt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die\u00df wahre Wort, der Dichter sprach es aus.", "tokens": ["Die\u00df", "wah\u00b7re", "Wort", ",", "der", "Dich\u00b7ter", "sprach", "es", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Blickt um euch, in euch, und ihr f\u00fchlt: so ist's!", "tokens": ["Blickt", "um", "euch", ",", "in", "euch", ",", "und", "ihr", "f\u00fchlt", ":", "so", "ist's", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus Blumen gr\u00fc\u00dft, was in uns selber bl\u00fcht,", "tokens": ["Aus", "Blu\u00b7men", "gr\u00fc\u00dft", ",", "was", "in", "uns", "sel\u00b7ber", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im Lichtstrahl spricht, was in uns leuchtend flammt,", "tokens": ["Im", "Licht\u00b7strahl", "spricht", ",", "was", "in", "uns", "leuch\u00b7tend", "flammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von Alpenh\u00f6hn, was in uns aufw\u00e4rts strebt,", "tokens": ["Von", "Al\u00b7pen\u00b7h\u00f6hn", ",", "was", "in", "uns", "auf\u00b7w\u00e4rts", "strebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In Wetterwolken, was auch in uns grollt;", "tokens": ["In", "Wet\u00b7ter\u00b7wol\u00b7ken", ",", "was", "auch", "in", "uns", "grollt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aus fl\u00fccht'gem Strom flie\u00dft durch die Seelen auch", "tokens": ["Aus", "fl\u00fccht'\u00b7gem", "Strom", "flie\u00dft", "durch", "die", "See\u00b7len", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das Rauschen der Verg\u00e4nglichkeit im Sein. \u2013", "tokens": ["Das", "Rau\u00b7schen", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "im", "Sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Kunst, die Unverg\u00e4ngliches erstrebt,", "tokens": ["Die", "Kunst", ",", "die", "Un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "er\u00b7strebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch sie erfa\u00dft und hegt es liebevoll", "tokens": ["Auch", "sie", "er\u00b7fa\u00dft", "und", "hegt", "es", "lie\u00b7be\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVPP", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die\u00df Band, das In- und Au\u00dfenwelt umflicht,", "tokens": ["Die\u00df", "Band", ",", "das", "In", "und", "Au\u00b7\u00dfen\u00b7welt", "um\u00b7flicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ART", "TRUNC", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fcgt zu festem Stoff von Erz und Stein,", "tokens": ["Und", "f\u00fcgt", "zu", "fes\u00b7tem", "Stoff", "von", "Erz", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das fest're, den Gedanken, der nicht stirbt.", "tokens": ["Das", "fest'\u00b7re", ",", "den", "Ge\u00b7dan\u00b7ken", ",", "der", "nicht", "stirbt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wenn sie dieses Mannes ragend Bild", "tokens": ["Und", "wenn", "sie", "die\u00b7ses", "Man\u00b7nes", "ra\u00b7gend", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Einst mitten in das Volksgewoge stellt,", "tokens": ["Einst", "mit\u00b7ten", "in", "das", "Volks\u00b7ge\u00b7wo\u00b7ge", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie wei\u00df: dann geht ein still geheimer Zug", "tokens": ["Sie", "wei\u00df", ":", "dann", "geht", "ein", "still", "ge\u00b7hei\u00b7mer", "Zug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von ihm zum Volkesherzen und zur\u00fcck,", "tokens": ["Von", "ihm", "zum", "Vol\u00b7kes\u00b7her\u00b7zen", "und", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und was im Volk an edlen Keimen lebt,", "tokens": ["Und", "was", "im", "Volk", "an", "ed\u00b7len", "Kei\u00b7men", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Was rein und gut, gesund und sch\u00f6n, das rankt", "tokens": ["Was", "rein", "und", "gut", ",", "ge\u00b7sund", "und", "sch\u00f6n", ",", "das", "rankt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und w\u00e4chst an ihm empor in F\u00fcll' und Kraft,", "tokens": ["Und", "w\u00e4chst", "an", "ihm", "em\u00b7por", "in", "F\u00fcll'", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zur Zierde ihm, zur h\u00f6hern Zier sich selbst:", "tokens": ["Zur", "Zier\u00b7de", "ihm", ",", "zur", "h\u00f6\u00b7hern", "Zier", "sich", "selbst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPRART", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Denn hohen Sinnes gibt, was er empfing,", "tokens": ["Denn", "ho\u00b7hen", "Sin\u00b7nes", "gibt", ",", "was", "er", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Veredelt und versch\u00f6nt er nur zur\u00fcck.", "tokens": ["Ver\u00b7e\u00b7delt", "und", "ver\u00b7sch\u00f6nt", "er", "nur", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Was er gedichtet und was er gelebt,", "tokens": ["Was", "er", "ge\u00b7dich\u00b7tet", "und", "was", "er", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Was ihn so gro\u00df, unsterblich ihn gemacht,", "tokens": ["Was", "ihn", "so", "gro\u00df", ",", "uns\u00b7terb\u00b7lich", "ihn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein fruchtbar Eigen sei es dieses Volks:", "tokens": ["Ein", "frucht\u00b7bar", "Ei\u00b7gen", "sei", "es", "die\u00b7ses", "Volks", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der strenge Sinn f\u00fcr Sitte, Wahrheit, Recht,", "tokens": ["Der", "stren\u00b7ge", "Sinn", "f\u00fcr", "Sit\u00b7te", ",", "Wahr\u00b7heit", ",", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der klare Blick f\u00fcr das, was sch\u00f6n und gut,", "tokens": ["Der", "kla\u00b7re", "Blick", "f\u00fcr", "das", ",", "was", "sch\u00f6n", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der Hochgedanke: Freiheit, Vaterland,", "tokens": ["Der", "Hoch\u00b7ge\u00b7dan\u00b7ke", ":", "Frei\u00b7heit", ",", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der Glaube an ein edles Menschenthum,", "tokens": ["Der", "Glau\u00b7be", "an", "ein", "ed\u00b7les", "Men\u00b7schen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Des Geistes ewig frische Jugendkraft,", "tokens": ["Des", "Geis\u00b7tes", "e\u00b7wig", "fri\u00b7sche", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und Eins zumeist: das ganze deutsche Herz.", "tokens": ["Und", "Eins", "zu\u00b7meist", ":", "das", "gan\u00b7ze", "deut\u00b7sche", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "ART", "ADJA", "ADJA", "NN", "$."], "met