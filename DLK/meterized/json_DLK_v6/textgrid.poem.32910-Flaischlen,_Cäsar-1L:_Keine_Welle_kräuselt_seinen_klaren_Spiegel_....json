{"textgrid.poem.32910": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Keine Welle kr\u00e4uselt seinen klaren Spiegel ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Welle kr\u00e4uselt seinen klaren Spiegel ...", "tokens": ["Kei\u00b7ne", "Wel\u00b7le", "kr\u00e4u\u00b7selt", "sei\u00b7nen", "kla\u00b7ren", "Spie\u00b7gel", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "nur wenn eine wei\u00dfe Wasserrose in froher Sonnensehnsucht sich aus seiner Tiefe hebt", "tokens": ["nur", "wenn", "ei\u00b7ne", "wei\u00b7\u00dfe", "Was\u00b7ser\u00b7ro\u00b7se", "in", "fro\u00b7her", "Son\u00b7nen\u00b7sehn\u00b7sucht", "sich", "aus", "sei\u00b7ner", "Tie\u00b7fe", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+--+-+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "oder wenn ein kleiner Vogel, ein Liedchen zwitschernd, \u00fcber ihn streift, mit leichtem Fl\u00fcgel", "tokens": ["o\u00b7der", "wenn", "ein", "klei\u00b7ner", "Vo\u00b7gel", ",", "ein", "Lied\u00b7chen", "zwit\u00b7schernd", ",", "\u00fc\u00b7ber", "ihn", "streift", ",", "mit", "leich\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,", "APPR", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "oder wenn", "tokens": ["o\u00b7der", "wenn"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "ein braunes Reh wo aus den Tannen tritt, an ihm zu trinken.", "tokens": ["ein", "brau\u00b7nes", "Reh", "wo", "aus", "den", "Tan\u00b7nen", "tritt", ",", "an", "ihm", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}}}}