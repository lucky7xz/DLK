{"dta.poem.20430": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entferne dich du eitles wesen/", "tokens": ["Ent\u00b7fer\u00b7ne", "dich", "du", "eit\u00b7les", "we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein schn\u00f6des blendwerck treugt mich nicht/", "tokens": ["Dein", "schn\u00f6\u00b7des", "blend\u00b7werck", "treugt", "mich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein fester sinn hat ihm erlesen/", "tokens": ["Mein", "fes\u00b7ter", "sinn", "hat", "ihm", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das weder zeit noch wechsel bricht.", "tokens": ["Das", "we\u00b7der", "zeit", "noch", "wech\u00b7sel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein falsches scheinen", "tokens": ["Dein", "fal\u00b7sches", "schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In gl\u00fcck und freud/", "tokens": ["In", "gl\u00fcck", "und", "freud", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Kehrt bald in weinen", "tokens": ["Kehrt", "bald", "in", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der schnellen zeit", "tokens": ["Der", "schnel\u00b7len", "zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Verg\u00e4nglichkeit.", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie blumen/ die des sommers bl\u00fchen/", "tokens": ["Wie", "blu\u00b7men", "/", "die", "des", "som\u00b7mers", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$(", "ART", "ART", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn der abend sich einstellt/", "tokens": ["Und", "wenn", "der", "a\u00b7bend", "sich", "ein\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich zu der erden niederziehen/", "tokens": ["Sich", "zu", "der", "er\u00b7den", "nie\u00b7der\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist das wesen dieser welt.", "tokens": ["So", "ist", "das", "we\u00b7sen", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl! wer im leben/", "tokens": ["Wohl", "!", "wer", "im", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bey freud und leid/", "tokens": ["Bey", "freud", "und", "leid", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sich nicht ergeben", "tokens": ["Sich", "nicht", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PRF", "PTKNEG", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der schnellen Zeit", "tokens": ["Der", "schnel\u00b7len", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Verg\u00e4ngligkeit.", "tokens": ["Ver\u00b7g\u00e4n\u00b7glig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wo sind die theure Mausoloen?", "tokens": ["Wo", "sind", "die", "theu\u00b7re", "Mau\u00b7so\u00b7loen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo der pall\u00e4ste g\u00f6ldne pracht?", "tokens": ["Wo", "der", "pal\u00b7l\u00e4s\u00b7te", "g\u00f6ld\u00b7ne", "pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sind Egypten deine hohen?", "tokens": ["Wo", "sind", "E\u00b7gyp\u00b7ten", "dei\u00b7ne", "ho\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die zeit hat staub daraus gemacht.", "tokens": ["Die", "zeit", "hat", "staub", "da\u00b7raus", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl! wer im leben rc.", "tokens": ["Wohl", "!", "wer", "im", "le\u00b7ben", "rc", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wen itzt das gl\u00fccke hochgestellet/", "tokens": ["Wen", "itzt", "das", "gl\u00fc\u00b7cke", "hoch\u00b7ge\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den man gek\u00fcsset f\u00fc\u00df und hand/", "tokens": ["Den", "man", "ge\u00b7k\u00fcs\u00b7set", "f\u00fc\u00df", "und", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des stuhl wird alsbald umgef\u00e4llet/", "tokens": ["Des", "stuhl", "wird", "als\u00b7bald", "um\u00b7ge\u00b7f\u00e4l\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dieser zeit in unbestand.", "tokens": ["Von", "die\u00b7ser", "zeit", "in", "un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl! wer im leben rc.", "tokens": ["Wohl", "!", "wer", "im", "le\u00b7ben", "rc", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der sch\u00f6nheit theure himmels-waare/", "tokens": ["Der", "sch\u00f6n\u00b7heit", "theu\u00b7re", "him\u00b7mels\u00b7waa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gestern man als einen GOtt", "tokens": ["Der", "ge\u00b7stern", "man", "als", "ei\u00b7nen", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIS", "KOKOM", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Gebauet tempel und altare/", "tokens": ["Ge\u00b7bau\u00b7et", "tem\u00b7pel", "und", "al\u00b7ta\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wird heute motten/ asch und koth.", "tokens": ["Wird", "heu\u00b7te", "mot\u00b7ten", "/", "asch", "und", "koth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$(", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl! wer im leben rc.", "tokens": ["Wohl", "!", "wer", "im", "le\u00b7ben", "rc", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was hilfft der schatz/ der kaum zu zehlen?", "tokens": ["Was", "hilfft", "der", "schatz", "/", "der", "kaum", "zu", "zeh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er mehret nur der sorgen harm/", "tokens": ["Er", "meh\u00b7ret", "nur", "der", "sor\u00b7gen", "harm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein b\u00f6ser tag kan uns ihn stehlen/", "tokens": ["Ein", "b\u00f6\u00b7ser", "tag", "kan", "uns", "ihn", "steh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sind wir gleich den bettlern arm.", "tokens": ["So", "sind", "wir", "gleich", "den", "bett\u00b7lern", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl! wer im leben rc.", "tokens": ["Wohl", "!", "wer", "im", "le\u00b7ben", "rc", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zwar g\u00fcter/ muth und frische glieder", "tokens": ["Zwar", "g\u00fc\u00b7ter", "/", "muth", "und", "fri\u00b7sche", "glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind werthe gaben/ wer sie hat;", "tokens": ["Sind", "wert\u00b7he", "ga\u00b7ben", "/", "wer", "sie", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "VVFIN", "$(", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schl\u00e4gt ein unfall jene nieder/", "tokens": ["Doch", "schl\u00e4gt", "ein", "un\u00b7fall", "je\u00b7ne", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PDS", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und diese macht ein windlein matt.", "tokens": ["Und", "die\u00b7se", "macht", "ein", "wind\u00b7lein", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl! wer im leben rc.", "tokens": ["Wohl", "!", "wer", "im", "le\u00b7ben", "rc", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "APPRART", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die zeit reist kron und purpur abe/", "tokens": ["Die", "zeit", "reist", "kron", "und", "pur\u00b7pur", "a\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeit ist der dinge rauberin;", "tokens": ["Zeit", "ist", "der", "din\u00b7ge", "rau\u00b7be\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die zeit tr\u00e4gt alle welt zu grabe/", "tokens": ["Die", "zeit", "tr\u00e4gt", "al\u00b7le", "welt", "zu", "gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word