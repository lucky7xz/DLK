{"textgrid.poem.57719": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lied vom Lande", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schatz, mein Schatz, reise nicht so weit von hier,", "tokens": ["Schatz", ",", "mein", "Schatz", ",", "rei\u00b7se", "nicht", "so", "weit", "von", "hier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schatz, mein Schatz, reise nicht so weit von hier;", "tokens": ["Schatz", ",", "mein", "Schatz", ",", "rei\u00b7se", "nicht", "so", "weit", "von", "hier", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD", "APPR", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im Rosengarten will ich deiner warten,", "tokens": ["Im", "Ro\u00b7sen\u00b7gar\u00b7ten", "will", "ich", "dei\u00b7ner", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im gr\u00fcnen Klee, im wei\u00dfen Schnee.", "tokens": ["Im", "gr\u00fc\u00b7nen", "Klee", ",", "im", "wei\u00b7\u00dfen", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der jungen blassen Frau im vierten Stock", "tokens": ["Der", "jun\u00b7gen", "blas\u00b7sen", "Frau", "im", "vier\u00b7ten", "Stock"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fallen die H\u00e4nde m\u00fcd' herab am Rock.", "tokens": ["Fal\u00b7len", "die", "H\u00e4n\u00b7de", "m\u00fcd'", "her\u00b7ab", "am", "Rock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie lange h\u00f6rte sie nicht mehr das Lied!", "tokens": ["Wie", "lan\u00b7ge", "h\u00f6r\u00b7te", "sie", "nicht", "mehr", "das", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein bunter Traum ihr vor die Seele zieht:", "tokens": ["Ein", "bun\u00b7ter", "Traum", "ihr", "vor", "die", "See\u00b7le", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der gr\u00fcne Wald, das weite breite Feld,", "tokens": ["Der", "gr\u00fc\u00b7ne", "Wald", ",", "das", "wei\u00b7te", "brei\u00b7te", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Dorfes H\u00e4user, weit und breit gestellt,", "tokens": ["Des", "Dor\u00b7fes", "H\u00e4u\u00b7ser", ",", "weit", "und", "breit", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Schwalben spielen lustig in der Luft,", "tokens": ["Die", "Schwal\u00b7ben", "spie\u00b7len", "lus\u00b7tig", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vom Friedhof kommt der Linden s\u00fc\u00dfer Duft,", "tokens": ["Vom", "Fried\u00b7hof", "kommt", "der", "Lin\u00b7den", "s\u00fc\u00b7\u00dfer", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vom Giebel eine Amsel fr\u00f6hlich singt,", "tokens": ["Vom", "Gie\u00b7bel", "ei\u00b7ne", "Am\u00b7sel", "fr\u00f6h\u00b7lich", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Kinder Jauchzen aus der Wiese klingt.", "tokens": ["Der", "Kin\u00b7der", "Jauch\u00b7zen", "aus", "der", "Wie\u00b7se", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie enge ist es in der gro\u00dfen Stadt!", "tokens": ["Wie", "en\u00b7ge", "ist", "es", "in", "der", "gro\u00b7\u00dfen", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Mauern, nirgendwo ein gr\u00fcnes Blatt.", "tokens": ["Nur", "Mau\u00b7ern", ",", "nir\u00b7gend\u00b7wo", "ein", "gr\u00fc\u00b7nes", "Blatt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Bauwich dr\u00fcben ist nun zugebaut,", "tokens": ["Der", "Bau\u00b7wich", "dr\u00fc\u00b7ben", "ist", "nun", "zu\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein St\u00fcckchen Himmel hatte dort geblaut,", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Him\u00b7mel", "hat\u00b7te", "dort", "ge\u00b7blaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Ast schob keck sich vor der Mauer her,", "tokens": ["Ein", "Ast", "schob", "keck", "sich", "vor", "der", "Mau\u00b7er", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Winter kahl, im Sommer bl\u00e4tterschwer.", "tokens": ["Im", "Win\u00b7ter", "kahl", ",", "im", "Som\u00b7mer", "bl\u00e4t\u00b7ter\u00b7schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So oft die blasse Frau dorthin geblickt,", "tokens": ["So", "oft", "die", "blas\u00b7se", "Frau", "dor\u00b7thin", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hat sie ihr m\u00fcdes Herz daran erquickt;", "tokens": ["Hat", "sie", "ihr", "m\u00fc\u00b7des", "Herz", "da\u00b7ran", "er\u00b7quickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ihr Lied des Abends da die Amsel sang", "tokens": ["Ihr", "Lied", "des", "A\u00b7bends", "da", "die", "Am\u00b7sel", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Frohlockend in den Gro\u00dfstadtl\u00e4rm es klang.", "tokens": ["Froh\u00b7lo\u00b7ckend", "in", "den", "Gro\u00df\u00b7stadt\u00b7l\u00e4rm", "es", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt ragt dort eine kahle Wand empor;", "tokens": ["Jetzt", "ragt", "dort", "ei\u00b7ne", "kah\u00b7le", "Wand", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Frau am Fenster wei\u00df, was sie verlor:", "tokens": ["Die", "Frau", "am", "Fens\u00b7ter", "wei\u00df", ",", "was", "sie", "ver\u00b7lor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das bi\u00dfchen Bl\u00e4ttergr\u00fcn, das St\u00fcckchen Blau,", "tokens": ["Das", "bi\u00df\u00b7chen", "Bl\u00e4t\u00b7ter\u00b7gr\u00fcn", ",", "das", "St\u00fcck\u00b7chen", "Blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr war es Herzenstrost und Seelentau.", "tokens": ["Ihr", "war", "es", "Her\u00b7zen\u00b7strost", "und", "See\u00b7len\u00b7tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gleichg\u00fcltig sieht sie sich im Zimmer um,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "sieht", "sie", "sich", "im", "Zim\u00b7mer", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der billige Tand bleibt seelenlos und stumm;", "tokens": ["Der", "bil\u00b7li\u00b7ge", "Tand", "bleibt", "see\u00b7len\u00b7los", "und", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Daheim im Dorfe sprach ein jedes St\u00fcck,", "tokens": ["Da\u00b7heim", "im", "Dor\u00b7fe", "sprach", "ein", "je\u00b7des", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dieses von Last und Leid und das von Gl\u00fcck.", "tokens": ["Die\u00b7ses", "von", "Last", "und", "Leid", "und", "das", "von", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "KON", "NN", "KON", "ART", "APPR", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.9": {"text": "Die junge Frau ein Schluchzen niederzwingt,", "tokens": ["Die", "jun\u00b7ge", "Frau", "ein", "Schluch\u00b7zen", "nie\u00b7der\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Von irgendwo die Stimme weitersingt:", "tokens": ["Von", "ir\u00b7gend\u00b7wo", "die", "Stim\u00b7me", "wei\u00b7ter\u00b7singt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich heirate nicht nach Geld und nicht nach Gut,", "tokens": ["Ich", "hei\u00b7ra\u00b7te", "nicht", "nach", "Geld", "und", "nicht", "nach", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich heirate nicht nach Geld und nicht nach Gut;", "tokens": ["Ich", "hei\u00b7ra\u00b7te", "nicht", "nach", "Geld", "und", "nicht", "nach", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Eine junge h\u00fcbsche Seele tu' ich mir w\u00e4hlen,", "tokens": ["Ei\u00b7ne", "jun\u00b7ge", "h\u00fcb\u00b7sche", "See\u00b7le", "tu'", "ich", "mir", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wer's glauben tut, wer's glauben tut.", "tokens": ["Wer's", "glau\u00b7ben", "tut", ",", "wer's", "glau\u00b7ben", "tut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,", "VAFIN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}