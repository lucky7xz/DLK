{"dta.poem.2997": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo warest du? Ich schlief. So wird an dir sich strafen,", "tokens": ["Wo", "wa\u00b7rest", "du", "?", "Ich", "schlief", ".", "So", "wird", "an", "dir", "sich", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du verschlafen hast. Was hab' ich denn verschlafen?", "tokens": ["Was", "du", "ver\u00b7schla\u00b7fen", "hast", ".", "Was", "hab'", "ich", "denn", "ver\u00b7schla\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel gro\u00dfe Dichter, die indes verklungen sind,", "tokens": ["Viel", "gro\u00b7\u00dfe", "Dich\u00b7ter", ",", "die", "in\u00b7des", "ver\u00b7klun\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Weise, die vom Urungrund verschlungen sind,", "tokens": ["Und", "Wei\u00b7se", ",", "die", "vom", "U\u00b7run\u00b7grund", "ver\u00b7schlun\u00b7gen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weltneurungsblasen die lautlos zersprungen sind,", "tokens": ["Welt\u00b7neu\u00b7rungs\u00b7bla\u00b7sen", "die", "laut\u00b7los", "zer\u00b7sprun\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alte Gr\u00f6\u00dfen, die verh\u00f6hnt von Jungen sind.", "tokens": ["Und", "al\u00b7te", "Gr\u00f6\u00b7\u00dfen", ",", "die", "ver\u00b7h\u00f6hnt", "von", "Jun\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "VVPP", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bedauerst du es nicht? Ja wohl, ich armer Mann", "tokens": ["Be\u00b7dau\u00b7erst", "du", "es", "nicht", "?", "Ja", "wohl", ",", "ich", "ar\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PTKANT", "ADV", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedaure, da\u00df ich nicht noch l\u00e4nger schlafen kann.", "tokens": ["Be\u00b7dau\u00b7re", ",", "da\u00df", "ich", "nicht", "noch", "l\u00e4n\u00b7ger", "schla\u00b7fen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}