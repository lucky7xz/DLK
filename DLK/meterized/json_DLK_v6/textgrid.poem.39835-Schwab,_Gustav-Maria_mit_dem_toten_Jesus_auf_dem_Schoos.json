{"textgrid.poem.39835": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Maria mit dem toten Jesus auf dem Schoos", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbso hielt ich dich, ein zartes Kind, umfangen,", "tokens": ["\u00bb", "so", "hielt", "ich", "dich", ",", "ein", "zar\u00b7tes", "Kind", ",", "um\u00b7fan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das erste L\u00e4cheln bl\u00fcht' auf deinem Munde,", "tokens": ["Das", "ers\u00b7te", "L\u00e4\u00b7cheln", "bl\u00fcht'", "auf", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sanft gehoben aus des Herzens Grunde", "tokens": ["Und", "sanft", "ge\u00b7ho\u00b7ben", "aus", "des", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trat dir das Blut err\u00f6tend in die Wangen.", "tokens": ["Trat", "dir", "das", "Blut", "er\u00b7r\u00f6\u00b7tend", "in", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie sind erbleicht: ihr junges Blut vergangen,", "tokens": ["Sie", "sind", "er\u00b7bleicht", ":", "ihr", "jun\u00b7ges", "Blut", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es str\u00f6mt vers\u00f6hnend aus der Seitenwunde,", "tokens": ["Es", "str\u00f6mt", "ver\u00b7s\u00f6h\u00b7nend", "aus", "der", "Sei\u00b7ten\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das letzte L\u00e4cheln stirbt auf deinem Munde,", "tokens": ["Das", "letz\u00b7te", "L\u00e4\u00b7cheln", "stirbt", "auf", "dei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinem Blick das himmlische Verlangen.", "tokens": ["In", "dei\u00b7nem", "Blick", "das", "himm\u00b7li\u00b7sche", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und mitten doch in allem Weh und Leide,", "tokens": ["Und", "mit\u00b7ten", "doch", "in", "al\u00b7lem", "Weh", "und", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinen Schmerzensanblick tief verloren,", "tokens": ["In", "dei\u00b7nen", "Schmer\u00b7zen\u00b7san\u00b7blick", "tief", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Quillt mir ein sanftes Licht in meinem Herzen.", "tokens": ["Quillt", "mir", "ein", "sanf\u00b7tes", "Licht", "in", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es fa\u00dft mich eine m\u00fctterliche Freude,", "tokens": ["Es", "fa\u00dft", "mich", "ei\u00b7ne", "m\u00fct\u00b7ter\u00b7li\u00b7che", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir wird, als h\u00e4tt' ich dich in s\u00fc\u00dfen Schmerzen", "tokens": ["Mir", "wird", ",", "als", "h\u00e4tt'", "ich", "dich", "in", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt eben erst f\u00fcr's Heil der Welt geboren.\u00ab", "tokens": ["Jetzt", "e\u00b7ben", "erst", "f\u00fcr's", "Heil", "der", "Welt", "ge\u00b7bo\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}