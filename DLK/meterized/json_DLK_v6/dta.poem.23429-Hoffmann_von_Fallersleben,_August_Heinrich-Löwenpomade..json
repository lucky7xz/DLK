{"dta.poem.23429": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "L\u00f6wenpomade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schnauz- und Backenb\u00e4rte sprie\u00dfen", "tokens": ["Schnauz", "und", "Ba\u00b7cken\u00b7b\u00e4r\u00b7te", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh vier Wochen kaum verflie\u00dfen!", "tokens": ["Eh", "vier", "Wo\u00b7chen", "kaum", "ver\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie gro\u00df ist Gottes Gnade!", "tokens": ["O", "wie", "gro\u00df", "ist", "Got\u00b7tes", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf! wir wollen allenthalben", "tokens": ["Auf", "!", "wir", "wol\u00b7len", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "PPER", "VMFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns am Leib und Herzen salben", "tokens": ["Uns", "am", "Leib", "und", "Her\u00b7zen", "sal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der deutschen L\u00f6wenpomade!", "tokens": ["Mit", "der", "deut\u00b7schen", "L\u00f6\u00b7wen\u00b7po\u00b7ma\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wagt's, Franzosen, wagt es nimmer,", "tokens": ["Wagt's", ",", "Fran\u00b7zo\u00b7sen", ",", "wagt", "es", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es ging' euch heute schlimmer", "tokens": ["Denn", "es", "ging'", "euch", "heu\u00b7te", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als dereinst im Katzbachbade:", "tokens": ["Als", "de\u00b7reinst", "im", "Katz\u00b7bach\u00b7ba\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Kriegsheer ist gar m\u00e4chtig,", "tokens": ["Un\u00b7ser", "Kriegs\u00b7heer", "ist", "gar", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Muth und Barthaar wuchs ihm pr\u00e4chtig", "tokens": ["Muth", "und", "Bart\u00b7haar", "wuchs", "ihm", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der deutschen L\u00f6wenpomade.", "tokens": ["Von", "der", "deut\u00b7schen", "L\u00f6\u00b7wen\u00b7po\u00b7ma\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}