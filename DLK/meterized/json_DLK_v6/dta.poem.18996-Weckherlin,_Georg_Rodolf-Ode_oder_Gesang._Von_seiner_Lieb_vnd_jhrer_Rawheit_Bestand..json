{"dta.poem.18996": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode oder Gesang.  \n Von seiner Lieb vnd jhrer Rawheit  \n Bestand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "An Sch\u00f6n vnd Rawheit kan man euch/", "tokens": ["An", "Sch\u00f6n", "vnd", "Raw\u00b7heit", "kan", "man", "euch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VMFIN", "PIS", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Myrta/ keine Nymff vergleichen:", "tokens": ["O", "Myr\u00b7ta", "/", "kei\u00b7ne", "Nymff", "ver\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An Lieb ist mir auch keiner gleich/", "tokens": ["An", "Lieb", "ist", "mir", "auch", "kei\u00b7ner", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "PIS", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd an Trew will ich keinem weichen:", "tokens": ["Vnd", "an", "Trew", "will", "ich", "kei\u00b7nem", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dan hertzen haben wir von stein/", "tokens": ["Dan", "hert\u00b7zen", "ha\u00b7ben", "wir", "von", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PPER", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie/ Myrta/ m\u00f6niglich mag sehen/", "tokens": ["Wie", "/", "Myr\u00b7ta", "/", "m\u00f6\u00b7nig\u00b7lich", "mag", "se\u00b7hen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "NE", "$(", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich/ au\u00dfzustehen solche pein/", "tokens": ["Ich", "/", "au\u00df\u00b7zu\u00b7ste\u00b7hen", "sol\u00b7che", "pein", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr/ meine pein nicht zu verstehen.", "tokens": ["Ihr", "/", "mei\u00b7ne", "pein", "nicht", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dan wir ja l\u00e4ngst/ ohn widerstand/", "tokens": ["Dan", "wir", "ja", "l\u00e4ngst", "/", "ohn", "wi\u00b7der\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADV", "$(", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beed weichend weren vberwunden;", "tokens": ["Beed", "wei\u00b7chend", "we\u00b7ren", "vber\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jhr/ Myrt/ von meiner Lieb bestand/", "tokens": ["Ihr", "/", "Myrt", "/", "von", "mei\u00b7ner", "Lieb", "be\u00b7stand", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich/ von vnz\u00e4hlich schweren wunden:", "tokens": ["Ich", "/", "von", "vn\u00b7z\u00e4h\u00b7lich", "schwe\u00b7ren", "wun\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "ADJD", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Also da\u00df die selbs st\u00f6ck vnd stein/", "tokens": ["Al\u00b7so", "da\u00df", "die", "selbs", "st\u00f6ck", "vnd", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die zu hal\u00dfstarrig vns nicht sehen/", "tokens": ["Die", "zu", "hal\u00df\u00b7star\u00b7rig", "vns", "nicht", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich/ au\u00df zustehen solche pein/", "tokens": ["Mich", "/", "au\u00df", "zu\u00b7ste\u00b7hen", "sol\u00b7che", "pein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Euch/ solche pein nicht zu verstehen.", "tokens": ["Euch", "/", "sol\u00b7che", "pein", "nicht", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gantz steinin ist gewi\u00df mein hertz", "tokens": ["Gantz", "stei\u00b7nin", "ist", "ge\u00b7wi\u00df", "mein", "hertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gequ\u00e4let stehts/ euch stehts zu lieben:", "tokens": ["Ge\u00b7qu\u00e4\u00b7let", "stehts", "/", "euch", "stehts", "zu", "lie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ewrig auch/ dieweil mein schmertz", "tokens": ["Das", "ew\u00b7rig", "auch", "/", "die\u00b7weil", "mein", "schmertz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADV", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch (gnadlo\u00df) gar nicht kan betr\u00fcben:", "tokens": ["Euch", "(", "gnad\u00b7lo\u00df", ")", "gar", "nicht", "kan", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "$(", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Also mu\u00df man vns beede stein/", "tokens": ["Al\u00b7so", "mu\u00df", "man", "vns", "bee\u00b7de", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ja harte felsen sein/ wol sehen/", "tokens": ["Ja", "har\u00b7te", "fel\u00b7sen", "sein", "/", "wol", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "VAINF", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich wegen grosser lieb vnd pein/", "tokens": ["Mich", "we\u00b7gen", "gros\u00b7ser", "lieb", "vnd", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "ADJD", "KON", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Euch meine pein nicht zu verstehen.", "tokens": ["Euch", "mei\u00b7ne", "pein", "nicht", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hab best\u00e4ndig meine trew/", "tokens": ["Ich", "hab", "be\u00b7st\u00e4n\u00b7dig", "mei\u00b7ne", "trew", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Al\u00dfbald Ich Euch sah/ Euch geschworen:", "tokens": ["Al\u00df\u00b7bald", "Ich", "Euch", "sah", "/", "Euch", "ge\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey Euch best\u00e4ndig stoltz vnd frey/", "tokens": ["Bey", "Euch", "be\u00b7st\u00e4n\u00b7dig", "stoltz", "vnd", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist all mein laid vnd lieb verloren:", "tokens": ["Ist", "all", "mein", "laid", "vnd", "lieb", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df also ich an trew ein stein/", "tokens": ["Da\u00df", "al\u00b7so", "ich", "an", "trew", "ein", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd jhr an stoltz ein fel\u00df zusehen;", "tokens": ["Vnd", "jhr", "an", "stoltz", "ein", "fel\u00df", "zu\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich/ ohn abla\u00df so schwere pein/", "tokens": ["Ich", "/", "ohn", "ab\u00b7la\u00df", "so", "schwe\u00b7re", "pein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr/ mein laid ohn laid au\u00dfzustehen.", "tokens": ["Ihr", "/", "mein", "laid", "ohn", "laid", "au\u00df\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPOSAT", "NN", "APPR", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wan dan/ O fel\u00df an h\u00e4rtigkeit/", "tokens": ["Wan", "dan", "/", "O", "fel\u00df", "an", "h\u00e4r\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$(", "NE", "NE", "APPR", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mein weinen Euch nicht kan erwaichen:", "tokens": ["Mein", "wei\u00b7nen", "Euch", "nicht", "kan", "er\u00b7wai\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wan ich fel\u00df an best\u00e4ndigkeit", "tokens": ["Wan", "ich", "fel\u00df", "an", "be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch vndanck nicht kan tods verblaichen;", "tokens": ["Durch", "vn\u00b7danck", "nicht", "kan", "tods", "ver\u00b7blai\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So seind wir ja zween harte stein/", "tokens": ["So", "seind", "wir", "ja", "zween", "har\u00b7te", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich/ solche marter au\u00dfzustehen;", "tokens": ["Ich", "/", "sol\u00b7che", "mar\u00b7ter", "au\u00df\u00b7zu\u00b7ste\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr/ ewer macht vnd meine pein", "tokens": ["Ihr", "/", "e\u00b7wer", "macht", "vnd", "mei\u00b7ne", "pein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PPOSAT", "VVFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd vnsern verlust nicht zu sehen.", "tokens": ["Vnd", "vn\u00b7sern", "ver\u00b7lust", "nicht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "PTKZU"