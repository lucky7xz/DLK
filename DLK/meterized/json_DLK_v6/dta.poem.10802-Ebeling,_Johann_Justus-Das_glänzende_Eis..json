{"dta.poem.10802": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das gl\u00e4nzende Eis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00e4nzend Eis scheint wie ein Spie-\ngel,", "tokens": ["Gl\u00e4n\u00b7zend", "Eis", "scheint", "wie", "ein", "Spie", "gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "KOKOM", "ART", "TRUNC", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der da brennt und flammend macht,", "tokens": ["Der", "da", "brennt", "und", "flam\u00b7mend", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wenn man diesen H\u00fcgel", "tokens": ["A\u00b7ber", "wenn", "man", "die\u00b7sen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4her siehet mit Bedacht,", "tokens": ["N\u00e4\u00b7her", "sie\u00b7het", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist es nur ein strahlend Bild,", "tokens": ["Ist", "es", "nur", "ein", "strah\u00b7lend", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das mit K\u00e4lte angef\u00fcllt.", "tokens": ["Das", "mit", "K\u00e4l\u00b7te", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also sind auch viele Seelen,", "tokens": ["Al\u00b7so", "sind", "auch", "vie\u00b7le", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die des Himmels Gnadenlicht,", "tokens": ["Die", "des", "Him\u00b7mels", "Gna\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich zum Gegenstand erw\u00e4hlen,", "tokens": ["Sich", "zum", "Ge\u00b7gen\u00b7stand", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihr Mund bezeugend spricht,", "tokens": ["Wie", "ihr", "Mund", "be\u00b7zeu\u00b7gend", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die von Andacht feurig hei\u00df:", "tokens": ["Die", "von", "An\u00b7dacht", "feu\u00b7rig", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sind doch so kalt als Eis.", "tokens": ["Und", "sind", "doch", "so", "kalt", "als", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Dran sind Heuchler leicht zu mer-\nken,", "tokens": ["Dran", "sind", "Heuch\u00b7ler", "leicht", "zu", "mer", "ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ADJD", "APPR", "TRUNC", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche mit den Schein vergn\u00fcgt,", "tokens": ["Wel\u00b7che", "mit", "den", "Schein", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00df in Worten, kalt im Werken;", "tokens": ["Hei\u00df", "in", "Wor\u00b7ten", ",", "kalt", "im", "Wer\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhr gemahlter Strahl betriegt:", "tokens": ["Ihr", "ge\u00b7mahl\u00b7ter", "Strahl", "be\u00b7triegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andacht ist kein \u00e4usres Spiel,", "tokens": ["An\u00b7dacht", "ist", "kein", "\u00e4us\u00b7res", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie zeigt sich auch im Gef\u00fchl.", "tokens": ["Sie", "zeigt", "sich", "auch", "im", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}