{"dta.poem.7432": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Gang um Mitternacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schreite mit dem Geist der Mitternacht", "tokens": ["Ich", "schrei\u00b7te", "mit", "dem", "Geist", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die weiten stillen Stra\u00dfen auf und nieder \u2014", "tokens": ["Die", "wei\u00b7ten", "stil\u00b7len", "Stra\u00b7\u00dfen", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie hastig ward geweint hier und gelacht", "tokens": ["Wie", "has\u00b7tig", "ward", "ge\u00b7weint", "hier", "und", "ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "VVPP", "ADV", "KON", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor einer Stunde noch! ... Nun tr\u00e4umt man wieder.", "tokens": ["Vor", "ei\u00b7ner", "Stun\u00b7de", "noch", "!", "...", "Nun", "tr\u00e4umt", "man", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "$(", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Lust ist, einer Blume gleich, verdorrt,", "tokens": ["Die", "Lust", "ist", ",", "ei\u00b7ner", "Blu\u00b7me", "gleich", ",", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die tollsten Becher h\u00f6rten auf zu sch\u00e4umen,", "tokens": ["Die", "tolls\u00b7ten", "Be\u00b7cher", "h\u00f6r\u00b7ten", "auf", "zu", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es zog der Kummer mit der Sonne fort,", "tokens": ["Es", "zog", "der", "Kum\u00b7mer", "mit", "der", "Son\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Welt ist m\u00fcde \u2014 la\u00dft sie, la\u00dft sie tr\u00e4umen!", "tokens": ["Die", "Welt", "ist", "m\u00fc\u00b7de", "la\u00dft", "sie", ",", "la\u00dft", "sie", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "VVIMP", "PPER", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie all mein Ha\u00df und Groll in Scherben bricht,", "tokens": ["Wie", "all", "mein", "Ha\u00df", "und", "Groll", "in", "Scher\u00b7ben", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ausgerungen eines Tages Wetter,", "tokens": ["Wenn", "aus\u00b7ge\u00b7run\u00b7gen", "ei\u00b7nes", "Ta\u00b7ges", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Mond ergie\u00dfet sein vers\u00f6hnend Licht,", "tokens": ["Der", "Mond", "er\u00b7gie\u00b7\u00dfet", "sein", "ver\u00b7s\u00f6h\u00b7nend", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00e4r's auch \u00fcber ", "tokens": ["Und", "w\u00e4r's", "auch", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Leicht wie ein Ton, unh\u00f6rbar wie ein Stern,", "tokens": ["Leicht", "wie", "ein", "Ton", ",", "un\u00b7h\u00f6r\u00b7bar", "wie", "ein", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Fliegt meine Seele um in diesen R\u00e4umen;", "tokens": ["Fliegt", "mei\u00b7ne", "See\u00b7le", "um", "in", "die\u00b7sen", "R\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Wie in sich selbst, versenkte sie sich gern", "tokens": ["Wie", "in", "sich", "selbst", ",", "ver\u00b7senk\u00b7te", "sie", "sich", "gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PRF", "ADV", "$,", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In aller Menschen tiefgeheimstes Tr\u00e4umen!", "tokens": ["In", "al\u00b7ler", "Men\u00b7schen", "tief\u00b7ge\u00b7heims\u00b7tes", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Schatten schleicht mir nach wie ein Spion,", "tokens": ["Mein", "Schat\u00b7ten", "schleicht", "mir", "nach", "wie", "ein", "Spi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ich stehe still vor eines Kerkers Gitter.", "tokens": ["Ich", "ste\u00b7he", "still", "vor", "ei\u00b7nes", "Ker\u00b7kers", "Git\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Vaterland, dein zu getreuer Sohn,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "dein", "zu", "ge\u00b7treu\u00b7er", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er b\u00fc\u00dfte seine Liebe bitter, bitter!", "tokens": ["Er", "b\u00fc\u00df\u00b7te", "sei\u00b7ne", "Lie\u00b7be", "bit\u00b7ter", ",", "bit\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er schl\u00e4ft, \u2014 und f\u00fchlt er, was man ihm geraubt?", "tokens": ["Er", "schl\u00e4ft", ",", "und", "f\u00fchlt", "er", ",", "was", "man", "ihm", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "KON", "VVFIN", "PPER", "$,", "PRELS", "PIS", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Tr\u00e4umt er vielleicht von seinen Eichenb\u00e4umen?", "tokens": ["Tr\u00e4umt", "er", "viel\u00b7leicht", "von", "sei\u00b7nen", "Ei\u00b7chen\u00b7b\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Tr\u00e4umt er sich einen Siegerkranz um's Haubt? \u2014", "tokens": ["Tr\u00e4umt", "er", "sich", "ei\u00b7nen", "Sie\u00b7ger\u00b7kranz", "um's", "Haubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Gott der Freiheit, la\u00df ihn weiter tr\u00e4umen!", "tokens": ["O", "Gott", "der", "Frei\u00b7heit", ",", "la\u00df", "ihn", "wei\u00b7ter", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gigantisch th\u00fcrmt sich vor mir ein Palast,", "tokens": ["Gi\u00b7gan\u00b7tisch", "th\u00fcrmt", "sich", "vor", "mir", "ein", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich schaue durch die purpurnen Gardinen,", "tokens": ["Ich", "schau\u00b7e", "durch", "die", "pur\u00b7pur\u00b7nen", "Gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie man im Schlaf nach einem Schwerte fa\u00dft,", "tokens": ["Wie", "man", "im", "Schlaf", "nach", "ei\u00b7nem", "Schwer\u00b7te", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit s\u00fcndigen, mit angstverwirrten Mienen.", "tokens": ["Mit", "s\u00fcn\u00b7di\u00b7gen", ",", "mit", "angst\u00b7ver\u00b7wirr\u00b7ten", "Mie\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gelb, wie die Krone, ist sein Angesicht,", "tokens": ["Gelb", ",", "wie", "die", "Kro\u00b7ne", ",", "ist", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Er l\u00e4\u00dft zur Flucht sich tausend Rosse z\u00e4umen,", "tokens": ["Er", "l\u00e4\u00dft", "zur", "Flucht", "sich", "tau\u00b7send", "Ros\u00b7se", "z\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er st\u00fcrzt zur Erde, und die Erde bricht \u2014", "tokens": ["Er", "st\u00fcrzt", "zur", "Er\u00b7de", ",", "und", "die", "Er\u00b7de", "bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Gott der Rache, la\u00df ihn weiter tr\u00e4umen!", "tokens": ["O", "Gott", "der", "Ra\u00b7che", ",", "la\u00df", "ihn", "wei\u00b7ter", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das H\u00e4uschen dort am Bach \u2014 ein schmaler Raum!", "tokens": ["Das", "H\u00e4usc\u00b7hen", "dort", "am", "Bach", "ein", "schma\u00b7ler", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unschuld und Hunger theilen drin Ein Bette.", "tokens": ["Un\u00b7schuld", "und", "Hun\u00b7ger", "thei\u00b7len", "drin", "Ein", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch gab der Herr dem Landmann seinen Traum,", "tokens": ["Doch", "gab", "der", "Herr", "dem", "Land\u00b7mann", "sei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihn der Traum aus wachen Aengsten rette;", "tokens": ["Da\u00df", "ihn", "der", "Traum", "aus", "wa\u00b7chen", "A\u00b7engs\u00b7ten", "ret\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mit jedem Korn, das Morpheus Hand entf\u00e4llt,", "tokens": ["Mit", "je\u00b7dem", "Korn", ",", "das", "Mor\u00b7pheus", "Hand", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sieht er ein Saatenland sich golden s\u00e4umen,", "tokens": ["Sieht", "er", "ein", "Saa\u00b7ten\u00b7land", "sich", "gol\u00b7den", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die enge H\u00fctte weitet sich zur Welt \u2014", "tokens": ["Die", "en\u00b7ge", "H\u00fct\u00b7te", "wei\u00b7tet", "sich", "zur", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Gott der Armut, la\u00df die Armen tr\u00e4umen!", "tokens": ["O", "Gott", "der", "Ar\u00b7mut", ",", "la\u00df", "die", "Ar\u00b7men", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Beim letzten Hause, auf der Bank von Stein,", "tokens": ["Beim", "letz\u00b7ten", "Hau\u00b7se", ",", "auf", "der", "Bank", "von", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will segenflehend ich noch kurz verweilen;", "tokens": ["Will", "se\u00b7gen\u00b7fle\u00b7hend", "ich", "noch", "kurz", "ver\u00b7wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Treu lieb' ich dich, mein Kind, doch nicht allein,", "tokens": ["Treu", "lieb'", "ich", "dich", ",", "mein", "Kind", ",", "doch", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du wirst mich ewig mit der Freiheit theilen.", "tokens": ["Du", "wirst", "mich", "e\u00b7wig", "mit", "der", "Frei\u00b7heit", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dich wiegt in goldner Luft ein Taubenpaar,", "tokens": ["Dich", "wiegt", "in", "gold\u00b7ner", "Luft", "ein", "Tau\u00b7ben\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich sehe wilde Rosse nur sich b\u00e4umen;", "tokens": ["Ich", "se\u00b7he", "wil\u00b7de", "Ros\u00b7se", "nur", "sich", "b\u