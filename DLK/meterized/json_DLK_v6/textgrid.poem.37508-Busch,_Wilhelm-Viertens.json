{"textgrid.poem.37508": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Viertens", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Sachen des Klosters ausgesandt,", "tokens": ["In", "Sa\u00b7chen", "des", "Klos\u00b7ters", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kam Bruder Antonio einst \u00fcber Land.", "tokens": ["Kam", "Bru\u00b7der", "An\u00b7to\u00b7nio", "einst", "\u00fc\u00b7ber", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ihm zur Seite, mit leichtem Fu\u00df,", "tokens": ["Und", "ihm", "zur", "Sei\u00b7te", ",", "mit", "leich\u00b7tem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schritt Doktor Alopecius.", "tokens": ["Schritt", "Dok\u00b7tor", "A\u00b7lo\u00b7pe\u00b7ci\u00b7us", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "(ach! das war auch so einer von denen!)", "tokens": ["(", "ach", "!", "das", "war", "auch", "so", "ei\u00b7ner", "von", "de\u00b7nen", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "PDS", "VAFIN", "ADV", "ADV", "ART", "APPR", "PRELS", "$.", "$("], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Rechts und links begr\u00fc\u00dft er die l\u00e4ndlichen Sch\u00f6nen,", "tokens": ["Rechts", "und", "links", "be\u00b7gr\u00fc\u00dft", "er", "die", "l\u00e4nd\u00b7li\u00b7chen", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Fa\u00dft sie beim Kinn, anmutig-milde,", "tokens": ["Fa\u00dft", "sie", "beim", "Kinn", ",", "an\u00b7mu\u00b7tig\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schenkt ihnen gar sch\u00f6ne Heiligenbilde,", "tokens": ["Schenkt", "ih\u00b7nen", "gar", "sch\u00f6\u00b7ne", "Hei\u00b7li\u00b7gen\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und macht auch wohl so hin und wieder", "tokens": ["Und", "macht", "auch", "wohl", "so", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dominus vobiscum! \u00fcber das Mieder.", "tokens": ["Do\u00b7mi\u00b7nus", "vo\u00b7bis\u00b7cum", "!", "\u00fc\u00b7ber", "das", "Mie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$.", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie man denn meistens auf der Reis'", "tokens": ["Wie", "man", "denn", "meis\u00b7tens", "auf", "der", "Reis'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00f6nheit der Natur erst recht zu w\u00fcrdigen wei\u00df.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", "erst", "recht", "zu", "w\u00fcr\u00b7di\u00b7gen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Bruder Antonio aber dagegen,", "tokens": ["Bru\u00b7der", "An\u00b7to\u00b7nio", "a\u00b7ber", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "PAV", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dem nichts an irdischer Liebe gelegen,", "tokens": ["Dem", "nichts", "an", "ir\u00b7di\u00b7scher", "Lie\u00b7be", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Trug einzig allein in Herz und Sinn", "tokens": ["Trug", "ein\u00b7zig", "al\u00b7lein", "in", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die s\u00fc\u00dfe Himmelsk\u00f6nigin.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er wandelt abseit und schaut sich nicht um,", "tokens": ["Er", "wan\u00b7delt", "ab\u00b7seit", "und", "schaut", "sich", "nicht", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er spricht das salve und sub tuum praesidium.", "tokens": ["Er", "spricht", "das", "sal\u00b7ve", "und", "sub", "tu\u00b7um", "prae\u00b7si\u00b7di\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "FM", "FM", "FM", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So zogen sie weiter. Der Tag verstrich.", "tokens": ["So", "zo\u00b7gen", "sie", "wei\u00b7ter", ".", "Der", "Tag", "ver\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Abend wird schw\u00fcl. Es t\u00fcrmet sich", "tokens": ["Der", "A\u00b7bend", "wird", "schw\u00fcl", ".", "Es", "t\u00fcr\u00b7met", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein grau Gew\u00f6lk am Horizonte,", "tokens": ["Ein", "grau", "Ge\u00b7w\u00f6lk", "am", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Worin's schon ferne zu donnern begonnte.", "tokens": ["Worin's", "schon", "fer\u00b7ne", "zu", "don\u00b7nern", "be\u00b7gonn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dokter Alopecius, in diesen Sachen", "tokens": ["Dok\u00b7ter", "A\u00b7lo\u00b7pe\u00b7ci\u00b7us", ",", "in", "die\u00b7sen", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein arger Sp\u00f6tter, spricht mit Lachen:", "tokens": ["Ein", "ar\u00b7ger", "Sp\u00f6t\u00b7ter", ",", "spricht", "mit", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbna, was hat denn wieder der alte Brummer?", "tokens": ["\u00bb", "na", ",", "was", "hat", "denn", "wie\u00b7der", "der", "al\u00b7te", "Brum\u00b7mer", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Rumort ja erschr\u00f6cklich in den Wolken 'rummer?\u00ab", "tokens": ["Ru\u00b7mort", "ja", "er\u00b7schr\u00f6ck\u00b7lich", "in", "den", "Wol\u00b7ken", "'rum\u00b7mer", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADJD", "APPR", "ART", "NN", "NE", "$.", "$("], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und n\u00e4her w\u00e4lzt sich der Wolkenballen.", "tokens": ["Und", "n\u00e4\u00b7her", "w\u00e4lzt", "sich", "der", "Wol\u00b7ken\u00b7bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gewaltig braust der Sturm. Die Donner schallen.", "tokens": ["Ge\u00b7wal\u00b7tig", "braust", "der", "Sturm", ".", "Die", "Don\u00b7ner", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bruder Antonio schaut sich nicht um,", "tokens": ["Bru\u00b7der", "An\u00b7to\u00b7nio", "schaut", "sich", "nicht", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Er spricht das salve und sub tuum praesidium.", "tokens": ["Er", "spricht", "das", "sal\u00b7ve", "und", "sub", "tu\u00b7um", "prae\u00b7si\u00b7di\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "FM", "FM", "FM", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Doktor aber nimmt sein Parapl\u00fc,", "tokens": ["Der", "Dok\u00b7tor", "a\u00b7ber", "nimmt", "sein", "Pa\u00b7ra\u00b7pl\u00fc", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Spannt's auf und spricht: \u00bbJetzt kommt die Br\u00fch!!\u00ab", "tokens": ["Spannt's", "auf", "und", "spricht", ":", "\u00bb", "Jetzt", "kommt", "die", "Br\u00fch", "!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Horch! \u2013 Pl\u00f6tzlich, wie des Gerichts Trompete,", "tokens": ["Horch", "!", "\u2013", "Pl\u00f6tz\u00b7lich", ",", "wie", "des", "Ge\u00b7richts", "Trom\u00b7pe\u00b7te", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Donnert von oben eine Stimme: \u00bbT\u00f6te!! T\u00f6te!!!\u00ab", "tokens": ["Don\u00b7nert", "von", "o\u00b7ben", "ei\u00b7ne", "Stim\u00b7me", ":", "\u00bb", "T\u00f6\u00b7te", "!!", "T\u00f6\u00b7te", "!!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "APPR", "ADV", "ART", "NN", "$.", "$(", "NN", "$.", "NN", "$.", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "\u00bbschon recht!!!\u00ab \u2013 ert\u00f6nt voll Grimme", "tokens": ["\u00bb", "schon", "recht", "!!!", "\u00ab", "\u2013", "er\u00b7t\u00f6nt", "voll", "Grim\u00b7me"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$.", "$(", "$(", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eine zweite Stimme.", "tokens": ["Ei\u00b7ne", "zwei\u00b7te", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Huitt!! \u2013 Knatteradoms!! \u2013 ein Donnerkeil \u2013", "tokens": ["Huitt", "!!", "\u2013", "Knat\u00b7te\u00b7ra\u00b7doms", "!!", "\u2013", "ein", "Don\u00b7ner\u00b7keil", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NE", "$.", "$(", "ART", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Alopecius hat sein Teil.", "tokens": ["Und", "A\u00b7lo\u00b7pe\u00b7ci\u00b7us", "hat", "sein", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bruder Antonio schaut sich nicht um,", "tokens": ["Bru\u00b7der", "An\u00b7to\u00b7nio", "schaut", "sich", "nicht", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Er betet das salve und sub tuum praesidium.", "tokens": ["Er", "be\u00b7tet", "das", "sal\u00b7ve", "und", "sub", "tu\u00b7um", "prae\u00b7si\u00b7di\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "FM", "FM", "FM", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.8": {"line.1": {"text": "So wandelt er weiter in stillem Gebete. \u2013", "tokens": ["So", "wan\u00b7delt", "er", "wei\u00b7ter", "in", "stil\u00b7lem", "Ge\u00b7be\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wieder donnert die erste Stimme: \u00bbT\u00f6te! T\u00f6te!!!\u00ab", "tokens": ["Und", "wie\u00b7der", "don\u00b7nert", "die", "ers\u00b7te", "Stim\u00b7me", ":", "\u00bb", "T\u00f6\u00b7te", "!", "T\u00f6\u00b7te", "!!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "NN", "$.", "NN", "$.", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbja, t\u00f6te, t\u00f6te!! Sie leid't's halt nit!!!\u00ab", "tokens": ["\u00bb", "ja", ",", "t\u00f6\u00b7te", ",", "t\u00f6\u00b7te", "!!", "Sie", "lei\u00b7d't's", "halt", "nit", "!!!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "So ruft voll Grimme", "tokens": ["So", "ruft", "voll", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die zweite Stimme.", "tokens": ["Die", "zwei\u00b7te", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und grollend zog das Wetter hinunter. \u2013", "tokens": ["Und", "grol\u00b7lend", "zog", "das", "Wet\u00b7ter", "hin\u00b7un\u00b7ter", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "\u2013 Antonio aber, getrost und munter,", "tokens": ["\u2013", "An\u00b7to\u00b7nio", "a\u00b7ber", ",", "ge\u00b7trost", "und", "mun\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADV", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Zieht seines Weges f\u00fcrderhin", "tokens": 