{"textgrid.poem.61070": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wie es geht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie redeten ihr zu: Er liebt dich nicht,", "tokens": ["Sie", "re\u00b7de\u00b7ten", "ihr", "zu", ":", "Er", "liebt", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er spielt mit dir - Da neigte sie das Haupt,", "tokens": ["Er", "spielt", "mit", "dir", "Da", "neig\u00b7te", "sie", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Tr\u00e4nen perlten ihr vom Angesicht", "tokens": ["Und", "Tr\u00e4\u00b7nen", "perl\u00b7ten", "ihr", "vom", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Tau von Rosen; o, da\u00df sie's geglaubt!", "tokens": ["Wie", "Tau", "von", "Ro\u00b7sen", ";", "o", ",", "da\u00df", "sie's", "ge\u00b7glaubt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$.", "FM", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn als er kam und zweifelnd fand die Braut,", "tokens": ["Denn", "als", "er", "kam", "und", "zwei\u00b7felnd", "fand", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ward er voll Trotz, nicht tr\u00fcbe wollt' er scheinen;", "tokens": ["Ward", "er", "voll", "Trotz", ",", "nicht", "tr\u00fc\u00b7be", "wollt'", "er", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "$,", "PTKNEG", "ADJA", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er sang und spielte, trank und lachte laut,", "tokens": ["Er", "sang", "und", "spiel\u00b7te", ",", "trank", "und", "lach\u00b7te", "laut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Um dann die Nacht hindurch zu weinen.", "tokens": ["Um", "dann", "die", "Nacht", "hin\u00b7durch", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl pocht' ein guter Engel an ihr Herz:", "tokens": ["Wohl", "pocht'", "ein", "gu\u00b7ter", "En\u00b7gel", "an", "ihr", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bber ist doch treu, gib ihm die Hand, o gib!\u00ab", "tokens": ["\u00bb", "er", "ist", "doch", "treu", ",", "gib", "ihm", "die", "Hand", ",", "o", "gib", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "$,", "VVIMP", "PPER", "ART", "NN", "$,", "FM", "VVIMP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl f\u00fchlt' auch er durch Bitterkeit und Schmerz:", "tokens": ["Wohl", "f\u00fchlt'", "auch", "er", "durch", "Bit\u00b7ter\u00b7keit", "und", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbsie liebt dich doch, sie ist ja doch dein Lieb,", "tokens": ["\u00bb", "sie", "liebt", "dich", "doch", ",", "sie", "ist", "ja", "doch", "dein", "Lieb", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein freundlich Wort nur sprich, ein Wort vernimm,", "tokens": ["Ein", "freund\u00b7lich", "Wort", "nur", "sprich", ",", "ein", "Wort", "ver\u00b7nimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So ist der Zauber, der euch trennt, gebrochen.\u00ab", "tokens": ["So", "ist", "der", "Zau\u00b7ber", ",", "der", "euch", "trennt", ",", "ge\u00b7bro\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie gingen, sahn sich - o, der Stolz ist schlimm! -", "tokens": ["Sie", "gin\u00b7gen", ",", "sahn", "sich", "o", ",", "der", "Stolz", "ist", "schlimm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "$(", "FM", "$,", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Da schieden sie. Und wie im M\u00fcnsterchor", "tokens": ["Da", "schie\u00b7den", "sie", ".", "Und", "wie", "im", "M\u00fcns\u00b7ter\u00b7chor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verglimmt der Altarlampe roter Glanz -", "tokens": ["Ver\u00b7glimmt", "der", "Al\u00b7tar\u00b7lam\u00b7pe", "ro\u00b7ter", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erst wird er matt, dann flackert er empor", "tokens": ["Erst", "wird", "er", "matt", ",", "dann", "fla\u00b7ckert", "er", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch einmal hell, und dann verlischt er ganz -", "tokens": ["Noch", "ein\u00b7mal", "hell", ",", "und", "dann", "ver\u00b7lischt", "er", "ganz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "KON", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So starb die Lieb' in ihnen, erst beweint,", "tokens": ["So", "starb", "die", "Lieb'", "in", "ih\u00b7nen", ",", "erst", "be\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann hei\u00df zur\u00fcckersehnt und dann - vergessen,", "tokens": ["Dann", "hei\u00df", "zu\u00b7r\u00fc\u00b7cker\u00b7sehnt", "und", "dann", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "ADV", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bis sie zuletzt, es sei ein Wahn, gemeint,", "tokens": ["Bis", "sie", "zu\u00b7letzt", ",", "es", "sei", "ein", "Wahn", ",", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie sich je dereinst besessen.", "tokens": ["Da\u00df", "sie", "sich", "je", "de\u00b7reinst", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur manchmal fuhren sie im Mondenlicht", "tokens": ["Nur", "manch\u00b7mal", "fuh\u00b7ren", "sie", "im", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Kissen auf. Von Tr\u00e4nen war es na\u00df,", "tokens": ["Vom", "Kis\u00b7sen", "auf", ".", "Von", "Tr\u00e4\u00b7nen", "war", "es", "na\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und na\u00df von Tr\u00e4nen war noch ihr Gesicht;", "tokens": ["Und", "na\u00df", "von", "Tr\u00e4\u00b7nen", "war", "noch", "ihr", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Getr\u00e4umet hatten sie - ich wei\u00df nicht was.", "tokens": ["Ge\u00b7tr\u00e4u\u00b7met", "hat\u00b7ten", "sie", "ich", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$(", "PPER", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dann dachten sie der alten sch\u00f6nen Zeit", "tokens": ["Dann", "dach\u00b7ten", "sie", "der", "al\u00b7ten", "sch\u00f6\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und an ihr nichtig Zweifeln, an ihr Scheiden,", "tokens": ["Und", "an", "ihr", "nich\u00b7tig", "Zwei\u00b7feln", ",", "an", "ihr", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wie sie nun so weit, so ewig weit. -", "tokens": ["Und", "wie", "sie", "nun", "so", "weit", ",", "so", "e\u00b7wig", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O Gott, vergib, vergib den beiden!", "tokens": ["O", "Gott", ",", "ver\u00b7gib", ",", "ver\u00b7gib", "den", "bei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "$,", "VVIMP", "ART", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}