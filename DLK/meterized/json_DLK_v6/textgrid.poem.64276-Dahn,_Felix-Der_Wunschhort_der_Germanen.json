{"textgrid.poem.64276": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Wunschhort der Germanen", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ruht versenkt an stillem Ort,", "tokens": ["Es", "ruht", "ver\u00b7senkt", "an", "stil\u00b7lem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief unter Urwalds Eichen,", "tokens": ["Tief", "un\u00b7ter", "Ur\u00b7walds", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein teurer, bergentr\u00fcckter Hort,", "tokens": ["Ein", "teu\u00b7rer", ",", "ber\u00b7gen\u00b7tr\u00fcck\u00b7ter", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wunschhort ohnegleichen.", "tokens": ["Ein", "Wun\u00b7schhort", "oh\u00b7ne\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da liegt Herrn Wotans Runenspeer,", "tokens": ["Da", "liegt", "Herrn", "Wo\u00b7tans", "Ru\u00b7nen\u00b7speer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dabei Frau Friggas Spule,", "tokens": ["Da\u00b7bei", "Frau", "Frig\u00b7gas", "Spu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NE", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Dort blinkt der Becher, goldesschwer,", "tokens": ["Dort", "blinkt", "der", "Be\u00b7cher", ",", "gol\u00b7des\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des K\u00f6nigs Ring von Thule.", "tokens": ["Des", "K\u00f6\u00b7nigs", "Ring", "von", "Thu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Amalungen wei\u00dfer Schild, \u2013", "tokens": ["Der", "A\u00b7ma\u00b7lun\u00b7gen", "wei\u00b7\u00dfer", "Schild", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schwert Herrn Karls, das scharfe:", "tokens": ["Das", "Schwert", "Herrn", "Karls", ",", "das", "schar\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NE", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Leis' t\u00f6net, wie vertr\u00e4umt, so mild", "tokens": ["Leis'", "t\u00f6\u00b7net", ",", "wie", "ver\u00b7tr\u00e4umt", ",", "so", "mild"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWAV", "VVPP", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Vogelweiders Harfe;", "tokens": ["Des", "Vo\u00b7gel\u00b7wei\u00b7ders", "Har\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Sch\u00f6ppenspruch auf Pergament,", "tokens": ["Der", "Sch\u00f6p\u00b7pen\u00b7spruch", "auf", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schapel holder Maide, \u2013", "tokens": ["Der", "Scha\u00b7pel", "hol\u00b7der", "Mai\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Manch Lied, des S\u00e4nger niemand kennt,", "tokens": ["Manch", "Lied", ",", "des", "S\u00e4n\u00b7ger", "nie\u00b7mand", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und steinbesp\u00e4ngt Geschmeide;", "tokens": ["Und", "stein\u00b7be\u00b7sp\u00e4ngt", "Ge\u00b7schmei\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Rotbart flatternd Kreuzpanier,", "tokens": ["Des", "Rot\u00b7bart", "flat\u00b7ternd", "Kreuz\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Rathausdaches Giebel,", "tokens": ["Des", "Rat\u00b7haus\u00b7da\u00b7ches", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Hansa stolze Flaggenzier", "tokens": ["Der", "Han\u00b7sa", "stol\u00b7ze", "Flag\u00b7gen\u00b7zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Doktor Luthers Bibel!", "tokens": ["Und", "Dok\u00b7tor", "Lu\u00b7thers", "Bi\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dar\u00fcberhin ein Hauch, ein Duft", "tokens": ["Da\u00b7r\u00fc\u00b7berh\u00b7in", "ein", "Hauch", ",", "ein", "Duft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "ART", "NN", "$,", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kernfirnen Rheinweins br\u00fctet:", "tokens": ["Kern\u00b7fir\u00b7nen", "Rhein\u00b7weins", "br\u00fc\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O dringet k\u00fchn in diese Gruft,", "tokens": ["O", "drin\u00b7get", "k\u00fchn", "in", "die\u00b7se", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die quellend Leben h\u00fctet!", "tokens": ["Die", "quel\u00b7lend", "Le\u00b7ben", "h\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Allauf, Genossen, unverwandt", "tokens": ["Al\u00b7lauf", ",", "Ge\u00b7nos\u00b7sen", ",", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PAV", "$,", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft nach dem Schatz uns sch\u00fcrfen:", "tokens": ["La\u00dft", "nach", "dem", "Schatz", "uns", "sch\u00fcr\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur reines Herz und reine Hand", "tokens": ["Nur", "rei\u00b7nes", "Herz", "und", "rei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ihn erheben d\u00fcrfen!", "tokens": ["Wird", "ihn", "er\u00b7he\u00b7ben", "d\u00fcr\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er ist nicht tot: er w\u00e4chst, er bl\u00fcht,", "tokens": ["Er", "ist", "nicht", "tot", ":", "er", "w\u00e4chst", ",", "er", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er steigt uns selbst entgegen,", "tokens": ["Er", "steigt", "uns", "selbst", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er will in Geist und in Gem\u00fct", "tokens": ["Er", "will", "in", "Geist", "und", "in", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns seinen Segen legen:", "tokens": ["Uns", "sei\u00b7nen", "Se\u00b7gen", "le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Den Segen deutscher Herrlichkeit,", "tokens": ["Den", "Se\u00b7gen", "deut\u00b7scher", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heldenschaft der Ahnen;", "tokens": ["Die", "Hel\u00b7den\u00b7schaft", "der", "Ah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft uns ihn heben allezeit:", "tokens": ["La\u00dft", "uns", "ihn", "he\u00b7ben", "al\u00b7le\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Volkshort der Germanen!", "tokens": ["Den", "Volks\u00b7hort", "der", "Ger\u00b7ma\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}