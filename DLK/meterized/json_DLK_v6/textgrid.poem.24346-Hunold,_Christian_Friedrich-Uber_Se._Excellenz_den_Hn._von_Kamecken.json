{"textgrid.poem.24346": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber Se. Excellenz den Hn. von Kamecken", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel r\u00fchmliches hat zwar die Welt von dir geh\u00f6rt;", "tokens": ["Viel", "r\u00fchm\u00b7li\u00b7ches", "hat", "zwar", "die", "Welt", "von", "dir", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch prei\u00dft der schlechte Kiel nur zwey Vollkomenheiten:", "tokens": ["Doch", "prei\u00dft", "der", "schlech\u00b7te", "Kiel", "nur", "zwey", "Voll\u00b7ko\u00b7men\u00b7hei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du liebst des K\u00f6nigs Wohl/ des Landes Nutzbarkeiten/", "tokens": ["Du", "liebst", "des", "K\u00f6\u00b7nigs", "Wohl", "/", "des", "Lan\u00b7des", "Nutz\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dich der K\u00f6nig liebt/ und auch das Land verehrt.", "tokens": ["Da\u00df", "dich", "der", "K\u00f6\u00b7nig", "liebt", "/", "und", "auch", "das", "Land", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}