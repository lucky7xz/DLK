{"dta.poem.289": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das unbest\u00e4ndige Frauen-Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Des Frauen-Zimmers artigs Stellen/", "tokens": ["Des", "Frau\u00b7en\u00b7Zim\u00b7mers", "ar\u00b7tigs", "Stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleicht denen wilden Meeres-Wellen;", "tokens": ["Gleicht", "de\u00b7nen", "wil\u00b7den", "Mee\u00b7res\u00b7Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wer ihren glatten Worten traut", "tokens": ["Wer", "ih\u00b7ren", "glat\u00b7ten", "Wor\u00b7ten", "traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Glas die h\u00f6chsten Th\u00fcrme baut;", "tokens": ["Auf", "Glas", "die", "h\u00f6chs\u00b7ten", "Th\u00fcr\u00b7me", "baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er fischet in der Lufft/ er pfl\u00fcget in der See/", "tokens": ["Er", "fi\u00b7schet", "in", "der", "Lufft", "/", "er", "pfl\u00fc\u00b7get", "in", "der", "See", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sucht ein Schwalben-Nest in dem erstarten Schnee.", "tokens": ["Und", "sucht", "ein", "Schwal\u00b7ben\u00b7Nest", "in", "dem", "er\u00b7star\u00b7ten", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie lieben hefftig mit dem Munde", "tokens": ["Sie", "lie\u00b7ben", "heff\u00b7tig", "mit", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch steht die Lieb\u2019 auf schlechten Grunde/", "tokens": ["Doch", "steht", "die", "Lieb'", "auf", "schlech\u00b7ten", "Grun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nur ein kleiner Wind entsieht/", "tokens": ["Wenn", "nur", "ein", "klei\u00b7ner", "Wind", "ent\u00b7sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich Lieb und Gunst zu Boden geht/", "tokens": ["Gleich", "Lieb", "und", "Gunst", "zu", "Bo\u00b7den", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jm Wandeln siegen sie des Mondes Wandel an/", "tokens": ["Jm", "Wan\u00b7deln", "sie\u00b7gen", "sie", "des", "Mon\u00b7des", "Wan\u00b7del", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da selbst der Unbestand sie nicht besiegen kan.", "tokens": ["Da", "selbst", "der", "Un\u00b7be\u00b7stand", "sie", "nicht", "be\u00b7sie\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}