{"textgrid.poem.53928": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Vor und nach den Wahlen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also diesmal mu\u00df alles ganz anders werden!", "tokens": ["Al\u00b7so", "dies\u00b7mal", "mu\u00df", "al\u00b7les", "ganz", "an\u00b7ders", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Diesmal: endg\u00fcltiger Original-Friede auf Erden!", "tokens": ["Dies\u00b7mal", ":", "end\u00b7g\u00fcl\u00b7ti\u00b7ger", "O\u00b7ri\u00b7gi\u00b7na\u00b7lFrie\u00b7de", "auf", "Er\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+--++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Diesmal: Aufbau! Abbau! und Demokratie!", "tokens": ["Dies\u00b7mal", ":", "Auf\u00b7bau", "!", "Ab\u00b7bau", "!", "und", "De\u00b7mo\u00b7kra\u00b7tie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "$.", "NN", "$.", "KON", "NN", "$."], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Diesmal; die Herrschaft des arbeitenden Volkes wie noch nie!", "tokens": ["Dies\u00b7mal", ";", "die", "Herr\u00b7schaft", "des", "ar\u00b7bei\u00b7ten\u00b7den", "Vol\u00b7kes", "wie", "noch", "nie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "ART", "ADJA", "NN", "KOKOM", "ADV", "ADV", "$."], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und mit ernsten Gesichtern sagen Propheten prophetische Sachen:", "tokens": ["Und", "mit", "erns\u00b7ten", "Ge\u00b7sich\u00b7tern", "sa\u00b7gen", "Pro\u00b7phe\u00b7ten", "pro\u00b7phe\u00b7ti\u00b7sche", "Sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u00bbwas meinen Sie, werden die deutschen Wahlen im Ausland f\u00fcr Eindruck machen!\u00ab", "tokens": ["\u00bb", "was", "mei\u00b7nen", "Sie", ",", "wer\u00b7den", "die", "deut\u00b7schen", "Wah\u00b7len", "im", "Aus\u00b7land", "f\u00fcr", "Ein\u00b7druck", "ma\u00b7chen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und sie verk\u00fcnden aus B\u00e4rten und unter deutschen Brillen", "tokens": ["Und", "sie", "ver\u00b7k\u00fcn\u00b7den", "aus", "B\u00e4r\u00b7ten", "und", "un\u00b7ter", "deut\u00b7schen", "Bril\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u2013 wegen Nichtkiekenk\u00f6nnens \u2013 den h\u00f6chstwahrscheinlichen Volkeswillen.", "tokens": ["\u2013", "we\u00b7gen", "Nicht\u00b7kie\u00b7ken\u00b7k\u00f6n\u00b7nens", "\u2013", "den", "h\u00f6chst\u00b7wahr\u00b7schein\u00b7li\u00b7chen", "Vol\u00b7kes\u00b7wil\u00b7len", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "$(", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sprechen wird aus der Urne die gro\u00dfe Sphinx:", "tokens": ["Spre\u00b7chen", "wird", "aus", "der", "Ur\u00b7ne", "die", "gro\u00b7\u00dfe", "Sphinx", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Die Wahlen ergeben diesmal einen Ruck nach links.", "tokens": ["Die", "Wah\u00b7len", "er\u00b7ge\u00b7ben", "dies\u00b7mal", "ei\u00b7nen", "Ruck", "nach", "links", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Diesmal werden sie nach den Wahlen den Reichstag betreten,", "tokens": ["Dies\u00b7mal", "wer\u00b7den", "sie", "nach", "den", "Wah\u00b7len", "den", "Reichs\u00b7tag", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "diesmal werden sie zum Heiligen Kompromisius beten;", "tokens": ["dies\u00b7mal", "wer\u00b7den", "sie", "zum", "Hei\u00b7li\u00b7gen", "Kom\u00b7pro\u00b7mis\u00b7ius", "be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "diesmal erscheinen die \u00e4ltesten Greise mit Podagra,", "tokens": ["dies\u00b7mal", "er\u00b7schei\u00b7nen", "die", "\u00e4l\u00b7tes\u00b7ten", "Grei\u00b7se", "mit", "Po\u00b7da\u00b7gra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.4": {"text": "denn wenn die Wahlen vorbei sein werden, sind sie alle wieder da.", "tokens": ["denn", "wenn", "die", "Wah\u00b7len", "vor\u00b7bei", "sein", "wer\u00b7den", ",", "sind", "sie", "al\u00b7le", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VAINF", "VAFIN", "$,", "VAFIN", "PPER", "PIS", "ADV", "ADV", "$."], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und mit ernsten Gesichtern werden sie unter langem Parlamentieren", "tokens": ["Und", "mit", "erns\u00b7ten", "Ge\u00b7sich\u00b7tern", "wer\u00b7den", "sie", "un\u00b7ter", "lan\u00b7gem", "Par\u00b7la\u00b7men\u00b7tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+-+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "wirklich einen Ruck nach links konstatieren.", "tokens": ["wirk\u00b7lich", "ei\u00b7nen", "Ruck", "nach", "links", "kons\u00b7ta\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.5": {"line.1": {"text": "Damit es aber kein Ungl\u00fcck gibt in der himmlischsten aller Welten,", "tokens": ["Da\u00b7mit", "es", "a\u00b7ber", "kein", "Un\u00b7gl\u00fcck", "gibt", "in", "der", "himm\u00b7lischs\u00b7ten", "al\u00b7ler", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "und damit sich die Richter nicht am Zug der Freiheit erk\u00e4lten,", "tokens": ["und", "da\u00b7mit", "sich", "die", "Rich\u00b7ter", "nicht", "am", "Zug", "der", "Frei\u00b7heit", "er\u00b7k\u00e4l\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PRF", "ART", "NN", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "und \u00fcberhaupt zur Rettung des deutsch-katholischen-industriellen Junkergeschlechts", "tokens": ["und", "\u00fc\u00b7ber\u00b7haupt", "zur", "Ret\u00b7tung", "des", "deut\u00b7schka\u00b7tho\u00b7li\u00b7schen\u00b7in\u00b7dus\u00b7tri\u00b7el\u00b7len", "Jun\u00b7ker\u00b7ge\u00b7schlechts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+--+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "machen nach den Wahlen alle Parteien einen Ruck nach rechts.", "tokens": ["ma\u00b7chen", "nach", "den", "Wah\u00b7len", "al\u00b7le", "Par\u00b7tei\u00b7en", "ei\u00b7nen", "Ruck", "nach", "rechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIAT", "NN", "ART", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf diese Weise geht in dem deutschen Reichstagshaus", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", "geht", "in", "dem", "deut\u00b7schen", "Reichs\u00b7tags\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "alle Gewalt nebbich vom Volke aus.", "tokens": ["al\u00b7le", "Ge\u00b7walt", "neb\u00b7bich", "vom", "Vol\u00b7ke", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}