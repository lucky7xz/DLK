{"dta.poem.4359": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lehrreiche Sprache der Gesch\u00f6pfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst vielleicht: &#8220;Ich gebe zu,", "tokens": ["Du", "sprichst", "viel\u00b7leicht", ":", "&#8220;", "Ich", "ge\u00b7be", "zu", ","], "token_info": ["word", "word", "word", "punct", "XML_entity", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "$(", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eda\u00df GOtt in Seinem Werk sich zeige, und da\u00df Er", "tokens": ["\u201e", "da\u00df", "Gott", "in", "Sei\u00b7nem", "Werk", "sich", "zei\u00b7ge", ",", "und", "da\u00df", "Er"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "KON", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wohl darinn zu sehn.", "tokens": ["wohl", "da\u00b7rinn", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "\u201eallein, du wirst mir auch gestehn:", "tokens": ["\u201e", "al\u00b7lein", ",", "du", "wirst", "mir", "auch", "ge\u00b7stehn", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eda\u00df Er es halb verdeckt, und nicht recht deutlich thu.", "tokens": ["\u201e", "da\u00df", "Er", "es", "halb", "ver\u00b7deckt", ",", "und", "nicht", "recht", "deut\u00b7lich", "thu", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,", "KON", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So h\u00f6r: Es liegt die Schuld an Jhm nicht, blo\u00df an dir.", "tokens": ["So", "h\u00f6r", ":", "Es", "liegt", "die", "Schuld", "an", "Jhm", "nicht", ",", "blo\u00df", "an", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKNEG", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du siehst in Seiner Werke Zier,", "tokens": ["Du", "siehst", "in", "Sei\u00b7ner", "Wer\u00b7ke", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u00fcberall, von Jhm ein\u2019 unleugbare Spur.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "von", "Jhm", "ein'", "un\u00b7leug\u00b7ba\u00b7re", "Spur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gew\u00f6hne du dich nur,", "tokens": ["Ge\u00b7w\u00f6h\u00b7ne", "du", "dich", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Von jedem Blatt, von jeder Bluhm\u2019, und einer jeden", "tokens": ["Von", "je\u00b7dem", "Blatt", ",", "von", "je\u00b7der", "Bluhm'", ",", "und", "ei\u00b7ner", "je\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,", "KON", "ART", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Creatur,", "tokens": ["Crea\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Die Bildung und der Farben Pracht", "tokens": ["Die", "Bil\u00b7dung", "und", "der", "Far\u00b7ben", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht anders anzusehn,", "tokens": ["Nicht", "an\u00b7ders", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da sie ja wesentlich, und in der That so sch\u00f6n,", "tokens": ["Da", "sie", "ja", "we\u00b7sent\u00b7lich", ",", "und", "in", "der", "That", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "KON", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als einen wirklichen Beweis von Dessen Lieb\u2019 und Macht,", "tokens": ["Als", "ei\u00b7nen", "wirk\u00b7li\u00b7chen", "Be\u00b7weis", "von", "Des\u00b7sen", "Lieb'", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Der sie hervorgebracht.", "tokens": ["Der", "sie", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Betrachtest du sie so; so wirst du gleichsam h\u00f6ren,", "tokens": ["Be\u00b7trach\u00b7test", "du", "sie", "so", ";", "so", "wirst", "du", "gleich\u00b7sam", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie jegliche zu dir, in fanfter Sprache, spricht:", "tokens": ["Wie", "jeg\u00b7li\u00b7che", "zu", "dir", ",", "in", "fanf\u00b7ter", "Spra\u00b7che", ",", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich zeige dir den Sch\u00f6pfer. Diese Lehren", "tokens": ["Ich", "zei\u00b7ge", "dir", "den", "Sch\u00f6p\u00b7fer", ".", "Die\u00b7se", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Von ihres grossen Ursprungs Wesen", "tokens": ["Von", "ih\u00b7res", "gros\u00b7sen", "Ur\u00b7sprungs", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Kannst du, so oft du willt, durch dein Gesicht,", "tokens": ["Kannst", "du", ",", "so", "oft", "du", "willt", ",", "durch", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Auf allen ihren Bl\u00e4ttern lesen.", "tokens": ["Auf", "al\u00b7len", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ja, jede kann und wird die Ursach\u2019 und den Grund,", "tokens": ["Ja", ",", "je\u00b7de", "kann", "und", "wird", "die", "Ur\u00b7sach'", "und", "den", "Grund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VMFIN", "KON", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "(dir zum Beweis, und Dem, aus welchem sie entstund,", "tokens": ["(", "dir", "zum", "Be\u00b7weis", ",", "und", "Dem", ",", "aus", "wel\u00b7chem", "sie", "ent\u00b7stund", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPRART", "NN", "$,", "KON", "PDS", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zum Lob, und Ruhm, und Preis, und Ehren)", "tokens": ["Zum", "Lob", ",", "und", "Ruhm", ",", "und", "Preis", ",", "und", "Eh\u00b7ren", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Durch alle Sinnen, dir erkl\u00e4ren.", "tokens": ["Durch", "al\u00b7le", "Sin\u00b7nen", ",", "dir", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}