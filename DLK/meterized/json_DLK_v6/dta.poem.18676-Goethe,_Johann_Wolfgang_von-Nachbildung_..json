{"dta.poem.18676": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Nachbildung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "In deine Reimart hoff\u2019 ich mich zu finden,", "tokens": ["In", "dei\u00b7ne", "Rei\u00b7mart", "hoff'", "ich", "mich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wiederholen soll mir auch gefallen,", "tokens": ["Das", "Wie\u00b7der\u00b7ho\u00b7len", "soll", "mir", "auch", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erst werd\u2019 ich Sinn, sodann auch Worte finden;", "tokens": ["Erst", "werd'", "ich", "Sinn", ",", "so\u00b7dann", "auch", "Wor\u00b7te", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum zweytenmal soll mir kein Klang erschallen,", "tokens": ["Zum", "zwey\u00b7ten\u00b7mal", "soll", "mir", "kein", "Klang", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Er m\u00fcsste denn besondern Sinn begr\u00fcnden,", "tokens": ["Er", "m\u00fcss\u00b7te", "denn", "be\u00b7son\u00b7dern", "Sinn", "be\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie du\u2019s vermagst beg\u00fcnstigter vor allen.", "tokens": ["Wie", "du's", "ver\u00b7magst", "be\u00b7g\u00fcns\u00b7tig\u00b7ter", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADJD", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn wie ein Funke f\u00e4hig zu entz\u00fcnden", "tokens": ["Denn", "wie", "ein", "Fun\u00b7ke", "f\u00e4\u00b7hig", "zu", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kaiserstadt, wenn Flammen grimmig wallen,", "tokens": ["Die", "Kai\u00b7ser\u00b7stadt", ",", "wenn", "Flam\u00b7men", "grim\u00b7mig", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich winderzeugend, gl\u00fchn von eignen Winden,", "tokens": ["Sich", "win\u00b7der\u00b7zeu\u00b7gend", ",", "gl\u00fchn", "von", "eig\u00b7nen", "Win\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er, schon erloschen, schwand zu Sternenhallen;", "tokens": ["Er", ",", "schon", "er\u00b7lo\u00b7schen", ",", "schwand", "zu", "Ster\u00b7nen\u00b7hal\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "VVINF", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So schlangs von dir sich fort mit ew\u2019gen Gluten", "tokens": ["So", "schlangs", "von", "dir", "sich", "fort", "mit", "ew'\u00b7gen", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "PRF", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein deutsches Herz von frischem zu ermuthen.", "tokens": ["Ein", "deut\u00b7sches", "Herz", "von", "fri\u00b7schem", "zu", "er\u00b7mu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zugemessne Rhythmen reizen freylich,", "tokens": ["Zu\u00b7ge\u00b7mess\u00b7ne", "Rhyth\u00b7men", "rei\u00b7zen", "frey\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das Talent erfreut sich wohl darin;", "tokens": ["Das", "Ta\u00b7lent", "er\u00b7freut", "sich", "wohl", "da\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch wie schnelle widern sie abscheulich,", "tokens": ["Doch", "wie", "schnel\u00b7le", "wi\u00b7dern", "sie", "ab\u00b7scheu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hohle Masken ohne Blut und Sinn.", "tokens": ["Hoh\u00b7le", "Mas\u00b7ken", "oh\u00b7ne", "Blut", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Selbst der Geist erscheint sich nicht erfreulich,", "tokens": ["Selbst", "der", "Geist", "er\u00b7scheint", "sich", "nicht", "er\u00b7freu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn er nicht, auf neue Form bedacht,", "tokens": ["Wenn", "er", "nicht", ",", "auf", "neu\u00b7e", "Form", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Jener todten Form ein Ende macht.", "tokens": ["Je\u00b7ner", "tod\u00b7ten", "Form", "ein", "En\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}