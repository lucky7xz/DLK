{"dta.poem.15760": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Ungl\u00fcck ist es wohl, da\u00df sich auf lange nicht", "tokens": ["Ein", "Un\u00b7gl\u00fcck", "ist", "es", "wohl", ",", "da\u00df", "sich", "auf", "lan\u00b7ge", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PRF", "APPR", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erh\u00e4lt in dir das hergestellte Gleichgewicht.", "tokens": ["Er\u00b7h\u00e4lt", "in", "dir", "das", "her\u00b7ge\u00b7stell\u00b7te", "Gleich\u00b7ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ist es schon ein Gl\u00fcck, da\u00df es nur her sich stellt", "tokens": ["Doch", "ist", "es", "schon", "ein", "Gl\u00fcck", ",", "da\u00df", "es", "nur", "her", "sich", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "APZR", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Augenblick, wenns auch nicht lange h\u00e4lt.", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7blick", ",", "wenns", "auch", "nicht", "lan\u00b7ge", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wem dankst du dieses Gl\u00fcck? dem Hauch der Poesie;", "tokens": ["Wem", "dankst", "du", "die\u00b7ses", "Gl\u00fcck", "?", "dem", "Hauch", "der", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Ungl\u00fcck aber ist, da\u00df nur ein Hauch ist sie.", "tokens": ["Das", "Un\u00b7gl\u00fcck", "a\u00b7ber", "ist", ",", "da\u00df", "nur", "ein", "Hauch", "ist", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}