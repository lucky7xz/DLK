{"textgrid.poem.50205": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "37. Sophonisbe an den Syphax", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrst, deine Noth ist gross, doch kleiner als mein Schmertz',", "tokens": ["F\u00fcrst", ",", "dei\u00b7ne", "Noth", "ist", "gross", ",", "doch", "klei\u00b7ner", "als", "mein", "Schmertz'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ach hatt' ich ", "tokens": ["Ach", "hatt'", "ich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Doch weil das Schicksall tauscht, so z\u00e4hm' ich mein Verlangen,", "tokens": ["Doch", "weil", "das", "Schick\u00b7sall", "tauscht", ",", "so", "z\u00e4hm'", "ich", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und thue was er heischt von schn\u00f6der Hoffnung satt:", "tokens": ["Und", "thue", "was", "er", "heischt", "von", "schn\u00f6\u00b7der", "Hoff\u00b7nung", "satt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er schliesst ", "tokens": ["Er", "schliesst"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}}}}