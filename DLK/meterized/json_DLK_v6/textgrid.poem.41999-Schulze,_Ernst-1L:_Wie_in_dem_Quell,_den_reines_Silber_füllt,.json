{"textgrid.poem.41999": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie in dem Quell, den reines Silber f\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in dem Quell, den reines Silber f\u00fcllt,", "tokens": ["Wie", "in", "dem", "Quell", ",", "den", "rei\u00b7nes", "Sil\u00b7ber", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das geist'ge Bild des Mondes sich entfaltet,", "tokens": ["Das", "geist'\u00b7ge", "Bild", "des", "Mon\u00b7des", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, von der Welle zartem Hauch umwaltet,", "tokens": ["Und", ",", "von", "der", "Wel\u00b7le", "zar\u00b7tem", "Hauch", "um\u00b7wal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mit hellerm Glanz aus seinem Bade quillt;", "tokens": ["Mit", "hel\u00b7lerm", "Glanz", "aus", "sei\u00b7nem", "Ba\u00b7de", "quillt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wohnt in meinem Inneren dein Bild,", "tokens": ["So", "wohnt", "in", "mei\u00b7nem", "In\u00b7ne\u00b7ren", "dein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Sehnsucht nicht zum K\u00f6rper umgestaltet,", "tokens": ["Durch", "Sehn\u00b7sucht", "nicht", "zum", "K\u00f6r\u00b7per", "um\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht durch Genu\u00df, nicht durch die Zeit veraltet,", "tokens": ["Nicht", "durch", "Ge\u00b7nu\u00df", ",", "nicht", "durch", "die", "Zeit", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in der Reinheit Silberflor geh\u00fcllt.", "tokens": ["Und", "in", "der", "Rein\u00b7heit", "Sil\u00b7ber\u00b7flor", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Liebe taucht's in ew'ge Morgenr\u00f6the,", "tokens": ["Die", "Lie\u00b7be", "taucht's", "in", "ew'\u00b7ge", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schm\u00fcckt seine Stirn mit einem Strahlenkranze,", "tokens": ["Schm\u00fcckt", "sei\u00b7ne", "Stirn", "mit", "ei\u00b7nem", "Strah\u00b7len\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und g\u00f6ttlich wird, was sonst nur irdisch war.", "tokens": ["Und", "g\u00f6tt\u00b7lich", "wird", ",", "was", "sonst", "nur", "ir\u00b7disch", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Sehnsucht leises Flehn wird zum Gebete,", "tokens": ["Der", "Sehn\u00b7sucht", "lei\u00b7ses", "Flehn", "wird", "zum", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Auge strahlt von keuscher Andacht Glanze,", "tokens": ["Das", "Au\u00b7ge", "strahlt", "von", "keu\u00b7scher", "An\u00b7dacht", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reiner gl\u00fcht der Busen, dein Altar.", "tokens": ["Und", "rei\u00b7ner", "gl\u00fcht", "der", "Bu\u00b7sen", ",", "dein", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}