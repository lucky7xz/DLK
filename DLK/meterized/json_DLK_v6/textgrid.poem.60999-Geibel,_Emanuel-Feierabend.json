{"textgrid.poem.60999": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Feierabend", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich am westlichen Himmel", "tokens": ["Wie", "sich", "am", "west\u00b7li\u00b7chen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinter den Bergen im Purpurgeflock", "tokens": ["Hin\u00b7ter", "den", "Ber\u00b7gen", "im", "Pur\u00b7pur\u00b7ge\u00b7flock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Die Sonne verliert,", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Atmet die Brust freudiger auf", "tokens": ["At\u00b7met", "die", "Brust", "freu\u00b7di\u00b7ger", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und saugt begierig", "tokens": ["Und", "saugt", "be\u00b7gie\u00b7rig"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den k\u00fchl erfrischenden Hauch des Abends.", "tokens": ["Den", "k\u00fchl", "er\u00b7fri\u00b7schen\u00b7den", "Hauch", "des", "A\u00b7bends", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Stiller wird's in der Seele;", "tokens": ["Stil\u00b7ler", "wird's", "in", "der", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ein ruhig heitrer See,", "tokens": ["Ein", "ru\u00b7hig", "hei\u00b7trer", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dehnt sie sich weit;", "tokens": ["Dehnt", "sie", "sich", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schw\u00e4nen gleich", "tokens": ["Schw\u00e4\u00b7nen", "gleich"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ziehen Erinnerungen", "tokens": ["Zie\u00b7hen", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "\u00dcber den friedlichen Spiegel hin.", "tokens": ["\u00dc\u00b7ber", "den", "fried\u00b7li\u00b7chen", "Spie\u00b7gel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ruhe, Ruhe", "tokens": ["Ru\u00b7he", ",", "Ru\u00b7he"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "S\u00e4uselt mich an aus der H\u00f6he.", "tokens": ["S\u00e4u\u00b7selt", "mich", "an", "aus", "der", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00dcber das Auge sinkt", "tokens": ["\u00dc\u00b7ber", "das", "Au\u00b7ge", "sinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Leise die Wimper,", "tokens": ["Lei\u00b7se", "die", "Wim\u00b7per", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und vom Wunderbaume der Nacht", "tokens": ["Und", "vom", "Wun\u00b7der\u00b7bau\u00b7me", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Brech' ich des Schlummers liebliche Bl\u00fcte,", "tokens": ["Brech'", "ich", "des", "Schlum\u00b7mers", "lieb\u00b7li\u00b7che", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Des Traumes Goldfrucht.", "tokens": ["Des", "Trau\u00b7mes", "Gold\u00b7frucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}