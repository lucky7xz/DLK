{"textgrid.poem.46994": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Festlied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns z\u00e4hlen,", "tokens": ["Las\u00b7set", "uns", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Welch's sind unsre Bundsgenossen,", "tokens": ["Welch's", "sind", "uns\u00b7re", "Bunds\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Damit wir sehn unverdrossen,", "tokens": ["Da\u00b7mit", "wir", "sehn", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob's uns kann fehlen!", "tokens": ["Ob's", "uns", "kann", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer ist der erste der Bundsgenossen?", "tokens": ["Wer", "ist", "der", "ers\u00b7te", "der", "Bunds\u00b7ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist der Herr mit dem himmlischen Heere,", "tokens": ["Das", "ist", "der", "Herr", "mit", "dem", "himm\u00b7li\u00b7schen", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit dem blitzenden Speere,", "tokens": ["Mit", "dem", "blit\u00b7zen\u00b7den", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit den donnernden Rossen.", "tokens": ["Mit", "den", "don\u00b7nern\u00b7den", "Ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Er ist ausgefahren", "tokens": ["Er", "ist", "aus\u00b7ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Siegeswagen,", "tokens": ["Auf", "Sie\u00b7ges\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hat Feinde erschlagen,", "tokens": ["Hat", "Fein\u00b7de", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wer z\u00e4hlt die Scharen?", "tokens": ["Wer", "z\u00e4hlt", "die", "Scha\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sei mit deinen Wettern", "tokens": ["Sei", "mit", "dei\u00b7nen", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In unserm heiligen Streite", "tokens": ["In", "un\u00b7serm", "hei\u00b7li\u00b7gen", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch k\u00fcnftig uns zur Seite", "tokens": ["Auch", "k\u00fcnf\u00b7tig", "uns", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hilf uns, sie zerschmettern!", "tokens": ["Und", "hilf", "uns", ",", "sie", "zer\u00b7schmet\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer ist der zweite der Bundsgenossen?", "tokens": ["Wer", "ist", "der", "zwei\u00b7te", "der", "Bunds\u00b7ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist ein Nordlands-Riese,", "tokens": ["Das", "ist", "ein", "Nord\u00b7lands\u00b7Rie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit eisblankem Spie\u00dfe,", "tokens": ["Mit", "eis\u00b7blan\u00b7kem", "Spie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit starren Sennen, aus Eis gegossen.", "tokens": ["Mit", "star\u00b7ren", "Sen\u00b7nen", ",", "aus", "Eis", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Er hat sich erhoben,", "tokens": ["Er", "hat", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit dem Panzer gerasselt,", "tokens": ["Mit", "dem", "Pan\u00b7zer", "ge\u00b7ras\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Feinde zusammengeprasselt,", "tokens": ["Da\u00df", "die", "Fein\u00b7de", "zu\u00b7sam\u00b7men\u00b7ge\u00b7pras\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie vom Nordwind auseinander gestoben.", "tokens": ["Wie", "vom", "Nord\u00b7wind", "aus\u00b7ein\u00b7an\u00b7der", "ge\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "La\u00df noch weiter sich w\u00e4lzen", "tokens": ["La\u00df", "noch", "wei\u00b7ter", "sich", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "PRF", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Deine nordischen Schauer!", "tokens": ["Dei\u00b7ne", "nor\u00b7di\u00b7schen", "Schau\u00b7er", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die Kraft soll kein lauer", "tokens": ["Die", "Kraft", "soll", "kein", "lau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "S\u00fcdwind dir schmelzen.", "tokens": ["S\u00fcd\u00b7wind", "dir", "schmel\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Wer ist der dritte der Bundsgenossen?", "tokens": ["Wer", "ist", "der", "drit\u00b7te", "der", "Bunds\u00b7ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist eine Heldenjungfrau in S\u00fcden;", "tokens": ["Das", "ist", "ei\u00b7ne", "Hel\u00b7den\u00b7jung\u00b7frau", "in", "S\u00fc\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie wei\u00df die hesperischen \u00c4pfel zu h\u00fcten,", "tokens": ["Sie", "wei\u00df", "die", "hes\u00b7pe\u00b7ri\u00b7schen", "\u00c4p\u00b7fel", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die in ihren Hainen sprossen.", "tokens": ["Die", "in", "ih\u00b7ren", "Hai\u00b7nen", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie hat die Diebe,", "tokens": ["Sie", "hat", "die", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die sie raubten, zu Boden gelegt;", "tokens": ["Die", "sie", "raub\u00b7ten", ",", "zu", "Bo\u00b7den", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sie hat sie aus ihrem Lande gefegt,", "tokens": ["Sie", "hat", "sie", "aus", "ih\u00b7rem", "Lan\u00b7de", "ge\u00b7fegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Spreu im Siebe.", "tokens": ["Wie", "Spreu", "im", "Sie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Blicke himmelw\u00e4rts", "tokens": ["Bli\u00b7cke", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Von deinen Pyren\u00e4en!", "tokens": ["Von", "dei\u00b7nen", "Py\u00b7re\u00b7n\u00e4\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df deine gl\u00fchenden Blicke sp\u00e4hen", "tokens": ["La\u00df", "dei\u00b7ne", "gl\u00fc\u00b7hen\u00b7den", "Bli\u00b7cke", "sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Frankreichs, deiner Feindin, Herz!", "tokens": ["In", "Fran\u00b7kreichs", ",", "dei\u00b7ner", "Fein\u00b7din", ",", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wer ist der vierte der Bundsgenossen?", "tokens": ["Wer", "ist", "der", "vier\u00b7te", "der", "Bunds\u00b7ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist in Westen ein Drache,", "tokens": ["Das", "ist", "in", "Wes\u00b7ten", "ein", "Dra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der \u00fcber die Freiheit der Welt h\u00e4lt Wache,", "tokens": ["Der", "\u00fc\u00b7ber", "die", "Frei\u00b7heit", "der", "Welt", "h\u00e4lt", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von seiner ewigen See umflossen.", "tokens": ["Von", "sei\u00b7ner", "e\u00b7wi\u00b7gen", "See", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Wenn du schl\u00e4gst in die Welle,", "tokens": ["Wenn", "du", "schl\u00e4gst", "in", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Tobt sie und streckt", "tokens": ["Tobt", "sie", "und", "streckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sch\u00e4umende Zungen aus und leckt", "tokens": ["Sch\u00e4u\u00b7men\u00b7de", "Zun\u00b7gen", "aus", "und", "leckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "An deiner feindlichen Nachbarin Schwelle.", "tokens": ["An", "dei\u00b7ner", "feind\u00b7li\u00b7chen", "Nach\u00b7ba\u00b7rin", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Speie, mit treuer", "tokens": ["Spei\u00b7e", ",", "mit", "treu\u00b7er"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kraft, zu verderben feindliche Rotten,", "tokens": ["Kraft", ",", "zu", "ver\u00b7der\u00b7ben", "feind\u00b7li\u00b7che", "Rot\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Spei' aus deine goldenen Flotten,", "tokens": ["Spei'", "aus", "dei\u00b7ne", "gol\u00b7de\u00b7nen", "Flot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und dein Congrev'sches Feuer!", "tokens": ["Und", "dein", "Con\u00b7gre\u00b7v'\u00b7sches", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wer ist der f\u00fcnfte der Bundsgenossen?", "tokens": ["Wer", "ist", "der", "f\u00fcnf\u00b7te", "der", "Bunds\u00b7ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist die Eintracht, die da wieder", "tokens": ["Das", "ist", "die", "Ein\u00b7tracht", ",", "die", "da", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Deines Leibes zerfallene Glieder,", "tokens": ["Dei\u00b7nes", "Lei\u00b7bes", "zer\u00b7fal\u00b7le\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "O Deutschland, hat zusammengeschlossen.", "tokens": ["O", "Deutschland", ",", "hat", "zu\u00b7sam\u00b7men\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.15": {"line.1": {"text": "Du warst in dir zerfallen,", "tokens": ["Du", "warst", "in", "dir", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Haushalt zerr\u00fcttet,", "tokens": ["Dein", "Haus\u00b7halt", "zer\u00b7r\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dein Schatz versch\u00fcttet", "tokens": ["Dein", "Schatz", "ver\u00b7sch\