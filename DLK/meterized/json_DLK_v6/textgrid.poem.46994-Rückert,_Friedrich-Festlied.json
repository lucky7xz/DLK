, "line.3": {"text": "Hat sie selbst in den Kot", "tokens": ["Hat", "sie", "selbst", "in", "den", "Kot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gest\u00fcrzt ihre Majest\u00e4t.", "tokens": ["Ge\u00b7st\u00fcrzt", "ih\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.23": {"line.1": {"text": "Und ist aufgestanden,", "tokens": ["Und", "ist", "auf\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und hat die Welt durchlaufen,", "tokens": ["Und", "hat", "die", "Welt", "durch\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und alles \u00fcber'n Haufen", "tokens": ["Und", "al\u00b7les", "\u00fc\u00b7ber'n", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geworfen in allen Landen.", "tokens": ["Ge\u00b7wor\u00b7fen", "in", "al\u00b7len", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.24": {"line.1": {"text": "Sie ist \u00fcber mich hergefahren,", "tokens": ["Sie", "ist", "\u00fc\u00b7ber", "mich", "her\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da ich zu geduldig war,", "tokens": ["Da", "ich", "zu", "ge\u00b7dul\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hat mich zertreten ganz und gar,", "tokens": ["Hat", "mich", "zer\u00b7tre\u00b7ten", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich geschleift bei den Haaren.", "tokens": ["Und", "mich", "ge\u00b7schleift", "bei", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.25": {"line.1": {"text": "Mein altes Haus", "tokens": ["Mein", "al\u00b7tes", "Haus"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hat sie mir zerbrochen,", "tokens": ["Hat", "sie", "mir", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und hat mir versprochen,", "tokens": ["Und", "hat", "mir", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mir ein bessers zu bauen daraus.", "tokens": ["Mir", "ein", "bes\u00b7sers", "zu", "bau\u00b7en", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "PTKZU", "VVINF", "PAV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.26": {"line.1": {"text": "Ja! was hat sie bestellt?", "tokens": ["Ja", "!", "was", "hat", "sie", "be\u00b7stellt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "St\u00e4rker und st\u00e4rker", "tokens": ["St\u00e4r\u00b7ker", "und", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Baute sie, blutverkittet, zum Kerker", "tokens": ["Bau\u00b7te", "sie", ",", "blut\u00b7ver\u00b7kit\u00b7tet", ",", "zum", "Ker\u00b7ker"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$,", "APPRART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die ganze Welt.", "tokens": ["Die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.27": {"line.1": {"text": "Nur da\u00df das Meer", "tokens": ["Nur", "da\u00df", "das", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchlte noch nicht", "tokens": ["F\u00fchl\u00b7te", "noch", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Des Kerkers Gewicht,", "tokens": ["Des", "Ker\u00b7kers", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das kr\u00e4nkte den Kerkermeister so sehr.", "tokens": ["Das", "kr\u00e4nk\u00b7te", "den", "Ker\u00b7ker\u00b7meis\u00b7ter", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.28": {"line.1": {"text": "Der Wehruf stieg", "tokens": ["Der", "We\u00b7hruf", "stieg"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aus aller Welt", "tokens": ["Aus", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum Sternenzelt,", "tokens": ["Zum", "Ster\u00b7nen\u00b7zelt", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Des Herr noch schwieg.", "tokens": ["Des", "Herr", "noch", "schwieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.29": {"line.1": {"text": "Bis Moskows Brand", "tokens": ["Bis", "Mos\u00b7kows", "Brand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Vor die Augen ihm trat;", "tokens": ["Vor", "die", "Au\u00b7gen", "ihm", "trat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da war es sein Rat,", "tokens": ["Da", "war", "es", "sein", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zu heben die Hand.", "tokens": ["Zu", "he\u00b7ben", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.30": {"line.1": {"text": "Der Herr, der lange drein gesehn,", "tokens": ["Der", "Herr", ",", "der", "lan\u00b7ge", "drein", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat endlich drein geschlagen;", "tokens": ["Hat", "end\u00b7lich", "drein", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt darf ich es wagen", "tokens": ["Jetzt", "darf", "ich", "es", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auch aufzustehn.", "tokens": ["Auch", "auf\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.31": {"line.1": {"text": "An Spaniens Glut", "tokens": ["An", "Spa\u00b7ni\u00b7ens", "Glut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hast du zuerst dir den Finger verbrannt;", "tokens": ["Hast", "du", "zu\u00b7erst", "dir", "den", "Fin\u00b7ger", "ver\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "In Ru\u00dflands frostiger Hand", "tokens": ["In", "Ru\u00df\u00b7lands", "fros\u00b7ti\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Erstarrte dein Blut.", "tokens": ["Er\u00b7starr\u00b7te", "dein", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.32": {"line.1": {"text": "Aber der Geist,", "tokens": ["A\u00b7ber", "der", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der die Preu\u00dfen hat anger\u00fchrt,", "tokens": ["Der", "die", "Preu\u00b7\u00dfen", "hat", "an\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der hat es vollf\u00fchrt,", "tokens": ["Der", "hat", "es", "voll\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der ist's, der hat dich geschlagen zumeist.", "tokens": ["Der", "ist's", ",", "der", "hat", "dich", "ge\u00b7schla\u00b7gen", "zu\u00b7meist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.33": {"line.1": {"text": "Alle die V\u00f6lker der Erde zusammen", "tokens": ["Al\u00b7le", "die", "V\u00f6l\u00b7ker", "der", "Er\u00b7de", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Haben wacker gerungen;", "tokens": ["Ha\u00b7ben", "wa\u00b7cker", "ge\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Aber wer dich bezwungen,", "tokens": ["A\u00b7ber", "wer", "dich", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das sind Gottes geistige Flammen.", "tokens": ["Das", "sind", "Got\u00b7tes", "geis\u00b7ti\u00b7ge", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.34": {"line.1": {"text": "Und Gott der Herr sprach:", "tokens": ["Und", "Gott", "der", "Herr", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Da\u00df Friede dem Erdkreis werde,", "tokens": ["Da\u00df", "Frie\u00b7de", "dem", "Erd\u00b7kreis", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr V\u00f6lker der Erde,", "tokens": ["Ihr", "V\u00f6l\u00b7ker", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "H\u00f6rt und thuet danach.", "tokens": ["H\u00f6rt", "und", "thu\u00b7et", "da\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.35": {"line.1": {"text": "In eh'rnes Band", "tokens": ["In", "eh'r\u00b7nes", "Band"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Schlagt mir die Unruhstifterin,", "tokens": ["Schlagt", "mir", "die", "Un\u00b7ruh\u00b7stif\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df f\u00fcrderhin", "tokens": ["Da\u00df", "f\u00fcr\u00b7der\u00b7hin"], "token_info": ["word", "word"], "pos": ["KOUS", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sie heben nicht k\u00f6nne die frevelnde Hand.", "tokens": ["Sie", "he\u00b7ben", "nicht", "k\u00f6n\u00b7ne", "die", "fre\u00b7veln\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.36": {"line.1": {"text": "Dann gehet heim, und jeder auf seinem", "tokens": ["Dann", "ge\u00b7het", "heim", ",", "und", "je\u00b7der", "auf", "sei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KON", "PIS", "APPR", "PPOSAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sitze, wie es euch ist beschieden,", "tokens": ["Sit\u00b7ze", ",", "wie", "es", "euch", "ist", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sitzt in Frieden,", "tokens": ["Sitzt", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und \u00fcber euch will ich sitzen auf meinem.", "tokens": ["Und", "\u00fc\u00b7ber", "euch", "will", "ich", "sit\u00b7zen", "auf", "mei\u00b7nem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}