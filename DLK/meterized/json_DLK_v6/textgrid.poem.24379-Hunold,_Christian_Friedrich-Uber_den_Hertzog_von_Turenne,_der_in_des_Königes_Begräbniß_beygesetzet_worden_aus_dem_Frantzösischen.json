{"textgrid.poem.24379": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber den Hertzog von Turenne, der in des K\u00f6niges Begr\u00e4bni\u00df beygesetzet worden/ aus dem Frantz\u00f6sischen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey Franckreichs K\u00f6nigen darf ein ", "tokens": ["Bey", "Fran\u00b7ck\u00b7reichs", "K\u00f6\u00b7ni\u00b7gen", "darf", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VMFIN", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Di\u00df ist die gr\u00f6ste Frucht von den ber\u00fchmten Siegen.", "tokens": ["Di\u00df", "ist", "die", "gr\u00f6s\u00b7te", "Frucht", "von", "den", "be\u00b7r\u00fchm\u00b7ten", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man kr\u00f6nt durch diese That des Helden Tapfferkeit/", "tokens": ["Man", "kr\u00f6nt", "durch", "die\u00b7se", "That", "des", "Hel\u00b7den", "Tapf\u00b7fer\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit die k\u00fcnfftge Zeit", "tokens": ["Da\u00b7mit", "die", "k\u00fcnfft\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von gleicher W\u00fcrde sch\u00e4tz/ den/ der in Purpur sitzet/", "tokens": ["Von", "glei\u00b7cher", "W\u00fcr\u00b7de", "sch\u00e4tz", "/", "den", "/", "der", "in", "Pur\u00b7pur", "sit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$(", "ART", "$(", "ART", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den/ welcher Cronen tr\u00e4gt/ und den/ der sie besch\u00fctzet.", "tokens": ["Den", "/", "wel\u00b7cher", "Cro\u00b7nen", "tr\u00e4gt", "/", "und", "den", "/", "der", "sie", "be\u00b7sch\u00fct\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAT", "NN", "VVFIN", "$(", "KON", "ART", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}