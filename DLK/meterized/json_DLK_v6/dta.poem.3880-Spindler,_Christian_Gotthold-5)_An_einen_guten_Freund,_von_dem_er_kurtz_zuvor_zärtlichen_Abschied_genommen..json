{"dta.poem.3880": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "5) An einen guten Freund, von dem er kurtz  \n zuvor z\u00e4rtlichen Abschied genommen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "O! Lust, o! eitle Lust! so fl\u00fcchtig sind die Sa-", "tokens": ["O", "!", "Lust", ",", "o", "!", "eit\u00b7le", "Lust", "!", "so", "fl\u00fcch\u00b7tig", "sind", "die", "Sa"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "$,", "FM", "$.", "ADJA", "NN", "$.", "ADV", "ADJD", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "chen,", "tokens": ["chen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Daraus wir Sterblichen ein himmlisch Gl\u00fccke", "tokens": ["Da\u00b7raus", "wir", "Sterb\u00b7li\u00b7chen", "ein", "himm\u00b7lisch", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "machen.", "tokens": ["ma\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Kaum hab ich dich gesehn, so heist es: Lebe wohl!", "tokens": ["Kaum", "hab", "ich", "dich", "ge\u00b7sehn", ",", "so", "heist", "es", ":", "Le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "ADV", "VAFIN", "PPER", "$.", "VVIMP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da uns des Schicksals Wuth von neuen trennen", "tokens": ["Da", "uns", "des", "Schick\u00b7sals", "Wuth", "von", "neu\u00b7en", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "soll.", "tokens": ["soll", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Nun mu\u00df ich weit entfernt dich nur im Bilde schaue\u0303,", "tokens": ["Nun", "mu\u00df", "ich", "weit", "ent\u00b7fernt", "dich", "nur", "im", "Bil\u00b7de", "schau\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mich trennte allzufr\u00fch ein h\u00f6chst verha\u00dftes Mu\u00df,", "tokens": ["Mich", "trenn\u00b7te", "all\u00b7zu\u00b7fr\u00fch", "ein", "h\u00f6chst", "ver\u00b7ha\u00df\u00b7tes", "Mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der zeigte mir den Ort des kalten Sehma-Flu\u00df:", "tokens": ["Der", "zeig\u00b7te", "mir", "den", "Ort", "des", "kal\u00b7ten", "Seh\u00b7ma\u00b7Flu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da leb ich armes Ding als wie in einer Celle,", "tokens": ["Da", "leb", "ich", "ar\u00b7mes", "Ding", "als", "wie", "in", "ei\u00b7ner", "Cel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "KOUS", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sehe weiter nichts als tr\u00fcbe Wasser-F\u00e4lle,", "tokens": ["Und", "se\u00b7he", "wei\u00b7ter", "nichts", "als", "tr\u00fc\u00b7be", "Was\u00b7ser\u00b7F\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als Berge, Wald und Thal; Die bange Einsamkeit", "tokens": ["Als", "Ber\u00b7ge", ",", "Wald", "und", "Thal", ";", "Die", "ban\u00b7ge", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bejammert itzt, nebst mir, die vormahls gute Zeit.", "tokens": ["Be\u00b7jam\u00b7mert", "itzt", ",", "nebst", "mir", ",", "die", "vor\u00b7mahls", "gu\u00b7te", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PPER", "$,", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Der Fr\u00f6sche heisres Lied scheint in den faulen", "tokens": ["Der", "Fr\u00f6\u00b7sche", "heis\u00b7res", "Lied", "scheint", "in", "den", "fau\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "S\u00fcmpffen", "tokens": ["S\u00fcmpf\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Mein Schicksal und mich selbst durch ihr Coac zu", "tokens": ["Mein", "Schick\u00b7sal", "und", "mich", "selbst", "durch", "ihr", "Coac", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKZU"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "schimpffen.", "tokens": ["schimpf\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Mich n\u00e4hrt ein schlechtes Dorff, dem Witz und", "tokens": ["Mich", "n\u00e4hrt", "ein", "schlech\u00b7tes", "Dorff", ",", "dem", "Witz", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Lust gebricht,", "tokens": ["Lust", "ge\u00b7bricht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Allwo man gar von nichts als nur vom Haber spricht", "tokens": ["All\u00b7wo", "man", "gar", "von", "nichts", "als", "nur", "vom", "Ha\u00b7ber", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PIS", "KOKOM", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie viel derselbe werth. Da man den Feldbau", "tokens": ["Wie", "viel", "der\u00b7sel\u00b7be", "werth", ".", "Da", "man", "den", "Feld\u00b7bau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PDAT", "ADJD", "$.", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "suchet,", "tokens": ["su\u00b7chet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Und auf die schlimme Zeit mit Sturm und Nach-", "tokens": ["Und", "auf", "die", "schlim\u00b7me", "Zeit", "mit", "Sturm", "und", "Nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "druck fluchet;", "tokens": ["druck", "flu\u00b7chet", ";"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Da wei\u00df man keinen Grund von dem erleuchten", "tokens": ["Da", "wei\u00df", "man", "kei\u00b7nen", "Grund", "von", "dem", "er\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Witz", "tokens": ["Witz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Hier hat der Eigennutz den l\u00e4ngst erbauten Sitz.", "tokens": ["Hier", "hat", "der", "Ei\u00b7gen\u00b7nutz", "den", "l\u00e4ngst", "er\u00b7bau\u00b7ten", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O! k\u00f6nte dir mein Kiel den Trotz von Weibes-", "tokens": ["O", "!", "k\u00f6n\u00b7te", "dir", "mein", "Kiel", "den", "Trotz", "von", "Wei\u00b7bes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Bildern,", "tokens": ["Bil\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Den dieser Flecken hegt, nach ihrer Art abschildern!", "tokens": ["Den", "die\u00b7ser", "Fle\u00b7cken", "hegt", ",", "nach", "ih\u00b7rer", "Art", "ab\u00b7schil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie sind verwegen, frey, arglistig, frech und wild,", "tokens": ["Sie", "sind", "ver\u00b7we\u00b7gen", ",", "frey", ",", "arg\u00b7lis\u00b7tig", ",", "frech", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das ist ihr Contrefait, das ist ihr wahres Bild.", "tokens": ["Das", "ist", "ihr", "Con\u00b7tre\u00b7fait", ",", "das", "ist", "ihr", "wah\u00b7res", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Anmuth ist verbannt, die Sittsamkeit verflogen,", "tokens": ["Die", "An\u00b7muth", "ist", "ver\u00b7bannt", ",", "die", "Sitt\u00b7sam\u00b7keit", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das Mitleid hat ein B\u00e4r und Tyger auferzogen.", "tokens": ["Das", "Mit\u00b7leid", "hat", "ein", "B\u00e4r", "und", "Ty\u00b7ger", "auf\u00b7er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So kr\u00e4nckt mich dein Verlust bey \u00fcberh\u00e4uffter", "tokens": ["So", "kr\u00e4nckt", "mich", "dein", "Ver\u00b7lust", "bey", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Quaal.", "tokens": ["Qua\u00b7al", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Mich h\u00e4lt weit h\u00f6hre Macht, sonst lief ich hundert", "tokens": ["Mich", "h\u00e4lt", "weit", "h\u00f6h\u00b7re", "Macht", ",", "sonst", "lief", "ich", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "mahl.", "tokens": ["mahl", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.40": {"text": "O! wolte doch die Treu des Him\u0303els mich versorgen,", "tokens": ["O", "!", "wol\u00b7te", "doch", "die", "Treu", "des", "Him\u0303els", "mich", "ver\u00b7sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Denn meiner Jahre Lentz vergeht im fr\u00fchen Morge\u0303,", "tokens": ["Denn", "mei\u00b7ner", "Jah\u00b7re", "Lentz", "ver\u00b7geht", "im", "fr\u00fc\u00b7hen", "Mor\u00b7g\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wie ein verscheuchtes Wild such ich die s\u00fcsse Ruh,", "tokens": ["Wie", "ein", "ver\u00b7scheuch\u00b7tes", "Wild", "such", "ich", "die", "s\u00fcs\u00b7se", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich bringe meine Zeit in Mi\u00dfvergn\u00fcgen zu.", "tokens": ["Ich", "brin\u00b7ge", "mei\u00b7ne", "Zeit", "in", "Mi\u00df\u00b7ver\u00b7gn\u00fc\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die Hoffnung n\u00e4hrt die Lust auf frohes Wieder-", "tokens": ["Die", "Hoff\u00b7nung", "n\u00e4hrt", "die", "Lust", "auf", "fro\u00b7hes", "Wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "sehen,", "tokens": ["se\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.46": {"text": "La\u00df mich nur fernerhin in deiner Gnade stehen.", "tokens": ["La\u00df", "mich", "nur", "fer\u00b7ner\u00b7hin", "in", "dei\u00b7ner", "Gna\u00b7de", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}