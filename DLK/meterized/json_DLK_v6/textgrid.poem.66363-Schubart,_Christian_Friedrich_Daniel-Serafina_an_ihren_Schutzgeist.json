{"textgrid.poem.66363": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Serafina an ihren Schutzgeist", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel, den ich", "tokens": ["Mein", "En\u00b7gel", ",", "den", "ich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vom Himmel erbat", "tokens": ["Vom", "Him\u00b7mel", "er\u00b7bat"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu leiten mich hier", "tokens": ["Zu", "lei\u00b7ten", "mich", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Auf dornigem Pfad,", "tokens": ["Auf", "dor\u00b7ni\u00b7gem", "Pfad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Zu f\u00fchren mich einst", "tokens": ["Zu", "f\u00fch\u00b7ren", "mich", "einst"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ins wonnige Land,", "tokens": ["Ins", "won\u00b7ni\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "O l\u00e4chle mir, Bote,", "tokens": ["O", "l\u00e4ch\u00b7le", "mir", ",", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Vom Himmel gesandt.", "tokens": ["Vom", "Him\u00b7mel", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Oft hab' ich geweint,", "tokens": ["Oft", "hab'", "ich", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Oft hab' ich geklagt,", "tokens": ["Oft", "hab'", "ich", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da\u00df hier so der Wurm", "tokens": ["Da\u00df", "hier", "so", "der", "Wurm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das R\u00f6slein zernagt,", "tokens": ["Das", "R\u00f6s\u00b7lein", "zer\u00b7nagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da\u00df Unschuld, so rein,", "tokens": ["Da\u00df", "Un\u00b7schuld", ",", "so", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie Himmelskrystall,", "tokens": ["Wie", "Him\u00b7mels\u00b7krys\u00b7tall", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Oft pl\u00f6tzlich sich neigt", "tokens": ["Oft", "pl\u00f6tz\u00b7lich", "sich", "neigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Zum t\u00f6dlichen Fall.", "tokens": ["Zum", "t\u00f6d\u00b7li\u00b7chen", "Fall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "O Engel, sei mir", "tokens": ["O", "En\u00b7gel", ",", "sei", "mir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vor Tausenden hold", "tokens": ["Vor", "Tau\u00b7sen\u00b7den", "hold"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und h\u00fclle mich ein", "tokens": ["Und", "h\u00fcl\u00b7le", "mich", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In Fl\u00fcgel von Gold.", "tokens": ["In", "Fl\u00fc\u00b7gel", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Noch bin ich so jung", "tokens": ["Noch", "bin", "ich", "so", "jung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und kenne noch nicht", "tokens": ["Und", "ken\u00b7ne", "noch", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der L\u00fcste Betrug", "tokens": ["Der", "L\u00fcs\u00b7te", "Be\u00b7trug"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Im schlauen Gesicht.", "tokens": ["Im", "schlau\u00b7en", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wenn Eitelkeit oft", "tokens": ["Wenn", "Ei\u00b7tel\u00b7keit", "oft"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit Bl\u00fcmlein mich neckt,", "tokens": ["Mit", "Bl\u00fcm\u00b7lein", "mich", "neckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und unter dem Strau\u00df", "tokens": ["Und", "un\u00b7ter", "dem", "Strau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Schlange versteckt;", "tokens": ["Die", "Schlan\u00b7ge", "ver\u00b7steckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wenn Thorheit mich sucht", "tokens": ["Wenn", "Thor\u00b7heit", "mich", "sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "So eile geschwind", "tokens": ["So", "ei\u00b7le", "ge\u00b7schwind"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und warne mich selbst,", "tokens": ["Und", "war\u00b7ne", "mich", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Du himmlisches Kind.", "tokens": ["Du", "himm\u00b7li\u00b7sches", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wenn Amor mir winkt", "tokens": ["Wenn", "A\u00b7mor", "mir", "winkt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit frechem Gesicht,", "tokens": ["Mit", "fre\u00b7chem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und h\u00f6hnisch verlacht", "tokens": ["Und", "h\u00f6h\u00b7nisch", "ver\u00b7lacht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die heilige Pflicht;", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Pflicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mein Engel, so schlag", "tokens": ["Mein", "En\u00b7gel", ",", "so", "schlag"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Fl\u00fcgel so laut,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bis Amor entflieht", "tokens": ["Bis", "A\u00b7mor", "ent\u00b7flieht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und bis er mir graut.", "tokens": ["Und", "bis", "er", "mir", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Doch sitz' ich allein", "tokens": ["Doch", "sitz'", "ich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Am goldnen Klavier", "tokens": ["Am", "gold\u00b7nen", "Kla\u00b7vier"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sing' ich ein Lied,", "tokens": ["Und", "sing'", "ich", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mein Engel, von dir;", "tokens": ["Mein", "En\u00b7gel", ",", "von", "dir", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So s\u00e4us'le in mich", "tokens": ["So", "s\u00e4us'\u00b7le", "in", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dein Himmelsgef\u00fchl", "tokens": ["Dein", "Him\u00b7mels\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und r\u00fcste mit Kraft", "tokens": ["Und", "r\u00fcs\u00b7te", "mit", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Mein goldenes Spiel.", "tokens": ["Mein", "gol\u00b7de\u00b7nes", "Spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Wenn Andacht mein Herz", "tokens": ["Wenn", "An\u00b7dacht", "mein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zum Himmel erhebt,", "tokens": ["Zum", "Him\u00b7mel", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da\u00df unter der Faust", "tokens": ["Da\u00df", "un\u00b7ter", "der", "Faust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der Fl\u00fcgel erbebt,", "tokens": ["Der", "Fl\u00fc\u00b7gel", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So \u00f6ffne du mir", "tokens": ["So", "\u00f6ff\u00b7ne", "du", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die k\u00fcnftige Welt,", "tokens": ["Die", "k\u00fcnf\u00b7ti\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bis gl\u00fchend vom Aug'", "tokens": ["Bis", "gl\u00fc\u00b7hend", "vom", "Aug'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Z\u00e4hre mir f\u00e4llt.", "tokens": ["Die", "Z\u00e4h\u00b7re", "mir", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Einst dr\u00fcckest du mir", "tokens": ["Einst", "dr\u00fc\u00b7ckest", "du", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zur ewigen Ruh'", "tokens": ["Zur", "e\u00b7wi\u00b7gen", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit Fingern von Duft", "tokens": ["Mit", "Fin\u00b7gern", "von", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Aeugelein zu.", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann st\u00fcrz' ich dir, ach!", "tokens": ["Dann", "st\u00fcrz'", "ich", "dir", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ITJ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von Seligkeit warm", "tokens": ["Von", "Se\u00b7lig\u00b7keit", "warm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Als deine Vertraute,", "tokens": ["Als", "dei\u00b7ne", "Ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mein Engel, in Arm.", "tokens": ["Mein", "En\u00b7gel", ",", "in", "Arm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Dann l\u00e4chelst du mir,", "tokens": ["Dann", "l\u00e4\u00b7chelst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dann nennst du mich Braut,", "tokens": ["Dann", "nennst", "du", "mich", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und k\u00fcssest mich sanft", "tokens": ["Und", "k\u00fcs\u00b7sest", "mich", "sanft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und himmlisch vertraut,", "tokens": ["Und", "himm\u00b7lisch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und f\u00fchrest mich selbst", "tokens": ["Und", "f\u00fch\u00b7rest", "mich", "selbst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "An rosiger Hand,", "tokens": ["An", "ro\u00b7si\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Du Bote des Herrn,", "tokens": ["Du", "Bo\u00b7te", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ins wonnige Land.", "tokens": ["Ins", "won\u00b7ni\u00b7ge", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}