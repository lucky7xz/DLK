{"textgrid.poem.58215": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "253. Durch die H\u00f6lle gen Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6nste Wunderbau der wahren Heiligkeit", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Wun\u00b7der\u00b7bau", "der", "wah\u00b7ren", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Elend, Kreuz und Tod vom Herren wird gegr\u00fcndet;", "tokens": ["In", "E\u00b7lend", ",", "Kreuz", "und", "Tod", "vom", "Her\u00b7ren", "wird", "ge\u00b7gr\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdammnis f\u00fchlt man erst, hernach die Seligkeit,", "tokens": ["Ver\u00b7damm\u00b7nis", "f\u00fchlt", "man", "erst", ",", "her\u00b7nach", "die", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer seine Seel' verliert, in Gott sie wieder findet.", "tokens": ["Wer", "sei\u00b7ne", "Seel'", "ver\u00b7liert", ",", "in", "Gott", "sie", "wie\u00b7der", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "mea