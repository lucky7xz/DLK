{"textgrid.poem.35469": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00fcb schleicht die Zeit und n\u00fcchtern ...", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb schleicht die Zeit und n\u00fcchtern ...", "tokens": ["Tr\u00fcb", "schleicht", "die", "Zeit", "und", "n\u00fcch\u00b7tern", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und glanzlos liegt die Welt,", "tokens": ["Und", "glanz\u00b7los", "liegt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von keinem goldnen Sonnenblick", "tokens": ["Von", "kei\u00b7nem", "gold\u00b7nen", "Son\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchleuchtet und erhellt.", "tokens": ["Durch\u00b7leuch\u00b7tet", "und", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Felsen lastet's mir die Brust,", "tokens": ["Wie", "Fel\u00b7sen", "las\u00b7tet's", "mir", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und halb bewu\u00dft, halb unbewu\u00dft", "tokens": ["Und", "halb", "be\u00b7wu\u00dft", ",", "halb", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kommt da ein Tr\u00e4umen sch\u00fcchtern \u2013", "tokens": ["Kommt", "da", "ein", "Tr\u00e4u\u00b7men", "sch\u00fcch\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Tr\u00fcb schleicht die Zeit und n\u00fcchtern,", "tokens": ["Tr\u00fcb", "schleicht", "die", "Zeit", "und", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und glanzlos liegt die Welt.", "tokens": ["Und", "glanz\u00b7los", "liegt", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da aber regt sich's leise,", "tokens": ["Da", "a\u00b7ber", "regt", "sich's", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gemach der Bann zerrinnt,", "tokens": ["Ge\u00b7mach", "der", "Bann", "zer\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und leuchtend dr\u00e4ngt sich Bild an Bild,", "tokens": ["Und", "leuch\u00b7tend", "dr\u00e4ngt", "sich", "Bild", "an", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lockt und webt und spinnt", "tokens": ["Und", "lockt", "und", "webt", "und", "spinnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und fesselt mich mit Zauberkraft,", "tokens": ["Und", "fes\u00b7selt", "mich", "mit", "Zau\u00b7ber\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4ngstverkohlte Leidenschaft", "tokens": ["Und", "l\u00e4ngst\u00b7ver\u00b7kohl\u00b7te", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zieht mich in ihre Kreise \u2013", "tokens": ["Zieht", "mich", "in", "ih\u00b7re", "Krei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da regt sich's leise, leise,", "tokens": ["Da", "regt", "sich's", "lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gemach der Bann zerrinnt.", "tokens": ["Ge\u00b7mach", "der", "Bann", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In hoher Sch\u00f6nheit prangend", "tokens": ["In", "ho\u00b7her", "Sch\u00f6n\u00b7heit", "pran\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schau' ich da pl\u00f6tzlich dich,", "tokens": ["Schau'", "ich", "da", "pl\u00f6tz\u00b7lich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mondenlang, viel Monden lang,", "tokens": ["Die", "mon\u00b7den\u00b7lang", ",", "viel", "Mon\u00b7den", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ganz vergessen ich \u2013", "tokens": ["So", "ganz", "ver\u00b7ges\u00b7sen", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ich gedacht kein einzig Mal,", "tokens": ["Der", "ich", "ge\u00b7dacht", "kein", "ein\u00b7zig", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ich in bitter harter Qual", "tokens": ["Als", "ich", "in", "bit\u00b7ter", "har\u00b7ter", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gerungen, lichtverlangend \u2013", "tokens": ["Ge\u00b7run\u00b7gen", ",", "licht\u00b7ver\u00b7lan\u00b7gend", "\u2013"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In hoher Sch\u00f6nheit prangend", "tokens": ["In", "ho\u00b7her", "Sch\u00f6n\u00b7heit", "pran\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Schau' ich da pl\u00f6tzlich dich ...", "tokens": ["Schau'", "ich", "da", "pl\u00f6tz\u00b7lich", "dich", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch ob's auch wie verschleiert", "tokens": ["Doch", "ob's", "auch", "wie", "ver\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "KOKOM", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Glanzl\u00e4cheln dich umschwebt,", "tokens": ["Glanz\u00b7l\u00e4\u00b7cheln", "dich", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob auch dein Haar, dein golden Haar,", "tokens": ["Ob", "auch", "dein", "Haar", ",", "dein", "gol\u00b7den", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Diadem dir webt \u2013", "tokens": ["Ein", "Di\u00b7a\u00b7dem", "dir", "webt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein Diadem, so flammenlicht,", "tokens": ["Ein", "Di\u00b7a\u00b7dem", ",", "so", "flam\u00b7men\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So zauberhold wie ein Gedicht,", "tokens": ["So", "zau\u00b7ber\u00b7hold", "wie", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das deine Sch\u00f6nheit feiert \u2013", "tokens": ["Das", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "fei\u00b7ert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ob's auch wie halb verschleiert", "tokens": ["Ob's", "auch", "wie", "halb", "ver\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KOKOM", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Glanzl\u00e4cheln dich umschwebt:", "tokens": ["Glanz\u00b7l\u00e4\u00b7cheln", "dich", "um\u00b7schwebt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In deinen schwermutsvollen", "tokens": ["In", "dei\u00b7nen", "schwer\u00b7muts\u00b7vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Glutaugen ruht's wie Leid \u2013", "tokens": ["Glu\u00b7tau\u00b7gen", "ruht's", "wie", "Leid", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie hei\u00dfes, namenloses Weh, \u2013", "tokens": ["Wie", "hei\u00b7\u00dfes", ",", "na\u00b7men\u00b7lo\u00b7ses", "Weh", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJA", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine Seele schreit,", "tokens": ["Wie", "ei\u00b7ne", "See\u00b7le", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn gnadenarm und sonnenlos", "tokens": ["Wenn", "gna\u00b7den\u00b7arm", "und", "son\u00b7nen\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie der Verzweiflung liegt im Scho\u00df \u2013", "tokens": ["Sie", "der", "Ver\u00b7zwei\u00b7flung", "liegt", "im", "Scho\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gl\u00fccklos die Stunden rollen ...", "tokens": ["Gl\u00fcc\u00b7klos", "die", "Stun\u00b7den", "rol\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In deinen schwermutsvollen", "tokens": ["In", "dei\u00b7nen", "schwer\u00b7muts\u00b7vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Glutaugen ruht's wie Leid ...", "tokens": ["Glu\u00b7tau\u00b7gen", "ruht's", "wie", "Leid", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mich aber packt ein Trauern", "tokens": ["Mich", "a\u00b7ber", "packt", "ein", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um dich, mein armes Lieb,", "tokens": ["Um", "dich", ",", "mein", "ar\u00b7mes", "Lieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich erl\u00f6sungsd\u00fcrstig wild", "tokens": ["Da\u00df", "ich", "er\u00b7l\u00f6\u00b7sungs\u00b7d\u00fcrs\u00b7tig", "wild"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufschreien m\u00f6cht'! Doch tr\u00fcb", "tokens": ["Auf\u00b7schrei\u00b7en", "m\u00f6cht'", "!", "Doch", "tr\u00fcb"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "$.", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und tr\u00fcber wird mir Herz und Hirn,", "tokens": ["Und", "tr\u00fc\u00b7ber", "wird", "mir", "Herz", "und", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Fieber irrt um Wang' und Stirn", "tokens": ["Das", "Fie\u00b7ber", "irrt", "um", "Wang'", "und", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mich durchschie\u00dft ein Schauern ...", "tokens": ["Und", "mich", "durch\u00b7schie\u00dft", "ein", "Schau\u00b7ern", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich packt ein j\u00e4hes Trauern", "tokens": ["Mich", "packt", "ein", "j\u00e4\u00b7hes", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Um dich, um dich, mein Lieb! \u2013 \u2013 \u2013", "tokens": ["Um", "dich", ",", "um", "dich", ",", "mein", "Lieb", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KOUI", "PPER", "$,", "KOUI", "PPER", "$,", "PPOSAT", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Tr\u00fcb schleicht die Zeit und n\u00fcchtern,", "tokens": ["Tr\u00fcb", "schleicht", "die", "Zeit", "und", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und glanzlos liegt die Welt,", "tokens": ["Und", "glanz\u00b7los", "liegt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von keinem goldnen Sonnenblick", "tokens": ["Von", "kei\u00b7nem", "gold\u00b7nen", "Son\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchleuchtet und erhellt.", "tokens": ["Durch\u00b7leuch\u00b7tet", "und", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir auf der Brust starrt's schwer und hart:", "tokens": ["Mir", "auf", "der", "Brust", "starrt's", "schwer", "und", "hart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verspielt, verloren und genarrt", "tokens": ["Ver\u00b7spielt", ",", "ver\u00b7lo\u00b7ren", "und", "ge\u00b7narrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von bl\u00f6den Traumgesichtern \u2013", "tokens": ["Von", "bl\u00f6\u00b7den", "Traum\u00b7ge\u00b7sich\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Tr\u00fcb schleicht die Zeit und n\u00fcchtern,", "tokens": ["Tr\u00fcb", "schleicht", "die", "Zeit", "und", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und glanzlos liegt die Welt ...", "tokens": ["Und", "glanz\u00b7los", "liegt", "die", "Welt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}