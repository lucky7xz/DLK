{"textgrid.poem.38054": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Zwey Schelme", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es tr\u00e4gt ein J\u00e4ger ein gr\u00fcnen Huth,", "tokens": ["Es", "tr\u00e4gt", "ein", "J\u00e4\u00b7ger", "ein", "gr\u00fc\u00b7nen", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er tr\u00e4gt drey Federn auf seinem Huth,", "tokens": ["Er", "tr\u00e4gt", "drey", "Fe\u00b7dern", "auf", "sei\u00b7nem", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Juchhey, Rassey! Hesasa, Faldrida!", "tokens": ["Juch\u00b7hey", ",", "Ras\u00b7sey", "!", "He\u00b7sa\u00b7sa", ",", "Fald\u00b7ri\u00b7da", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "NE", "$,", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Er tr\u00e4gt drey Federn auf seinem Huth.", "tokens": ["Er", "tr\u00e4gt", "drey", "Fe\u00b7dern", "auf", "sei\u00b7nem", "Huth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die eine war mit Gold beschlagen,", "tokens": ["Die", "ei\u00b7ne", "war", "mit", "Gold", "be\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das kann ein jeder J\u00e4ger tragen:", "tokens": ["Das", "kann", "ein", "je\u00b7der", "J\u00e4\u00b7ger", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Der J\u00e4ger der jagt ein wildes Schwein", "tokens": ["Der", "J\u00e4\u00b7ger", "der", "jagt", "ein", "wil\u00b7des", "Schwein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bey Nacht, bey Tag, bey Mondenschein:", "tokens": ["Bey", "Nacht", ",", "bey", "Tag", ",", "bey", "Mon\u00b7den\u00b7schein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Er jagt \u00fcber Berg und tiefe Straus,", "tokens": ["Er", "jagt", "\u00fc\u00b7ber", "Berg", "und", "tie\u00b7fe", "Straus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er jagt ein schwarzbraunes M\u00e4del heraus:", "tokens": ["Er", "jagt", "ein", "schwarz\u00b7brau\u00b7nes", "M\u00e4\u00b7del", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Wonaus, wohin, du wildes Thier,", "tokens": ["Wo\u00b7naus", ",", "wo\u00b7hin", ",", "du", "wil\u00b7des", "Thier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin ein J\u00e4ger und fang dich schier?", "tokens": ["Ich", "bin", "ein", "J\u00e4\u00b7ger", "und", "fang", "dich", "schier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Du bist ein J\u00e4ger und f\u00e4ngst mich nicht,", "tokens": ["Du", "bist", "ein", "J\u00e4\u00b7ger", "und", "f\u00e4ngst", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du kennst meine krumme Spr\u00fcnglein noch nicht:", "tokens": ["Du", "kennst", "mei\u00b7ne", "krum\u00b7me", "Spr\u00fcn\u00b7glein", "noch", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Deine krumme Spr\u00fcnge kenn ich gar wohl,", "tokens": ["Dei\u00b7ne", "krum\u00b7me", "Spr\u00fcn\u00b7ge", "kenn", "ich", "gar", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Leid ists mir, da\u00df ich dich fangen soll:", "tokens": ["Leid", "ists", "mir", ",", "da\u00df", "ich", "dich", "fan\u00b7gen", "soll", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Er warf ihr das B\u00e4ndlein an den Arm.", "tokens": ["Er", "warf", "ihr", "das", "B\u00e4n\u00b7dlein", "an", "den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jezt bin ich gefangen, da\u00df Gott erbarm:", "tokens": ["Jezt", "bin", "ich", "ge\u00b7fan\u00b7gen", ",", "da\u00df", "Gott", "er\u00b7barm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "ADJD", "$."], "meter": "++--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Er nahm sie bey ihrem rothen Rock,", "tokens": ["Er", "nahm", "sie", "bey", "ih\u00b7rem", "ro\u00b7then", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er schwang sie hinter sich auf sein Ro\u00df:", "tokens": ["Er", "schwang", "sie", "hin\u00b7ter", "sich", "auf", "sein", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Er ritt vor seiner Frau Mutter Haus,", "tokens": ["Er", "ritt", "vor", "sei\u00b7ner", "Frau", "Mut\u00b7ter", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Frau Mutter schaute zum Fenster hinaus:", "tokens": ["Frau", "Mut\u00b7ter", "schau\u00b7te", "zum", "Fens\u00b7ter", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Sey mir willkommen, o Sohne mein,", "tokens": ["Sey", "mir", "will\u00b7kom\u00b7men", ",", "o", "Soh\u00b7ne", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "FM", "NN", "PPOSAT", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Was bringst du f\u00fcr ein wildes Schwein:", "tokens": ["Was", "bringst", "du", "f\u00fcr", "ein", "wil\u00b7des", "Schwein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.12": {"line.1": {"text": "Frau Mutter, es ist kein wildes Schwein,", "tokens": ["Frau", "Mut\u00b7ter", ",", "es", "ist", "kein", "wil\u00b7des", "Schwein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist ein zartes Jungfr\u00e4uelein:", "tokens": ["Es", "ist", "ein", "zar\u00b7tes", "Jung\u00b7fr\u00e4u\u00b7e\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.13": {"line.1": {"text": "Ist es ein zartes Jungfr\u00e4uelein,", "tokens": ["Ist", "es", "ein", "zar\u00b7tes", "Jung\u00b7fr\u00e4u\u00b7e\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So soll sie mir willkommen seyn:", "tokens": ["So", "soll", "sie", "mir", "will\u00b7kom\u00b7men", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Juchhey u.s.w.", "tokens": ["Juch\u00b7hey", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.14": {"line.1": {"text": "Sie setzt das Jungfr\u00e4ulein an den Tisch,", "tokens": ["Sie", "setzt", "das", "Jung\u00b7fr\u00e4u\u00b7lein", "an", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie trug ihr auf gut Wildpret und Fisch:", "tokens": ["Sie", "trug", "ihr", "auf", "gut", "Wild\u00b7pret", "und", "Fisch", ":"], "to