{"dta.poem.9999": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Prei\u00df, und Ehre dem gro\u00dfen Allm\u00e4chtgen, er-", "tokens": ["Prei\u00df", ",", "und", "Eh\u00b7re", "dem", "gro\u00b7\u00dfen", "All\u00b7m\u00e4cht\u00b7gen", ",", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KON", "NN", "ART", "ADJA", "NN", "$,", "TRUNC"], "meter": "+-+--+-++-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Prei\u00df ihm, da\u00df er sich unser erbarmt, und seinen gefallnen,", "tokens": ["Prei\u00df", "ihm", ",", "da\u00df", "er", "sich", "un\u00b7ser", "er\u00b7barmt", ",", "und", "sei\u00b7nen", "ge\u00b7fall\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PRF", "PPOSAT", "VVPP", "$,", "KON", "PPOSAT", "ADJA", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Seinen nunmehr begnadigten Knechten Vers\u00f6hnung ge-", "tokens": ["Sei\u00b7nen", "nun\u00b7mehr", "be\u00b7gna\u00b7dig\u00b7ten", "Knech\u00b7ten", "Ver\u00b7s\u00f6h\u00b7nung", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "NN", "TRUNC"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Heil uns! da\u00df er uns w\u00fcrdig erkannt, ihm wieder zu", "tokens": ["Heil", "uns", "!", "da\u00df", "er", "uns", "w\u00fcr\u00b7dig", "er\u00b7kannt", ",", "ihm", "wie\u00b7der", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,", "PPER", "ADV", "PTKZU"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und die Gebete der Reu, die wir in tiefer Betr\u00fcbni\u00df", "tokens": ["Und", "die", "Ge\u00b7be\u00b7te", "der", "Reu", ",", "die", "wir", "in", "tie\u00b7fer", "Be\u00b7tr\u00fcb\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NE", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Jhm geopfert, nicht ganz verschm\u00e4ht \u2014 Gott, Rich-", "tokens": ["Jhm", "ge\u00b7o\u00b7pfert", ",", "nicht", "ganz", "ver\u00b7schm\u00e4ht", "Gott", ",", "Rich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVPP", "$,", "PTKNEG", "ADV", "VVPP", "$(", "NN", "$,", "TRUNC"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Sey gelobt, von Gefallnen gelobt! sie wollen nicht wieder", "tokens": ["Sey", "ge\u00b7lobt", ",", "von", "Ge\u00b7fall\u00b7nen", "ge\u00b7lobt", "!", "sie", "wol\u00b7len", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "APPR", "NN", "VVPP", "$.", "PPER", "VMFIN", "PTKNEG", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Fallen; nicht wieder von dir und von dem Wege des", "tokens": ["Fal\u00b7len", ";", "nicht", "wie\u00b7der", "von", "dir", "und", "von", "dem", "We\u00b7ge", "des"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PTKNEG", "ADV", "APPR", "PPER", "KON", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Weder zur Rechten, noch Linken entweichen! Mit wei-", "tokens": ["We\u00b7der", "zur", "Rech\u00b7ten", ",", "noch", "Lin\u00b7ken", "ent\u00b7wei\u00b7chen", "!", "Mit", "wei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "ADV", "NN", "VVINF", "$.", "APPR", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.10": {"text": "Wollen wir k\u00fcnftig zur Tugend die neuen Unsterblichen", "tokens": ["Wol\u00b7len", "wir", "k\u00fcnf\u00b7tig", "zur", "Tu\u00b7gend", "die", "neu\u00b7en", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+---", "measure": "dactylic.tri.plus"}, "line.11": {"text": "F\u00fchr uns, wir folgen dir nach, o gro\u00dfer Gesandter des", "tokens": ["F\u00fchr", "uns", ",", "wir", "fol\u00b7gen", "dir", "nach", ",", "o", "gro\u00b7\u00dfer", "Ge\u00b7sand\u00b7ter", "des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "ADJA", "NN", "ART"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.12": {"text": "F\u00fchr uns zu unsrer Bestimmung; doch eh wir den", "tokens": ["F\u00fchr", "uns", "zu", "uns\u00b7rer", "Be\u00b7stim\u00b7mung", ";", "doch", "eh", "wir", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$.", "ADV", "KOUS", "PPER", "ART"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Unsern Geburtssitz, welchen wir einst nach Jahren der", "tokens": ["Un\u00b7sern", "Ge\u00b7burts\u00b7sitz", ",", "wel\u00b7chen", "wir", "einst", "nach", "Jah\u00b7ren", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "PPER", "ADV", "APPR", "NN", "ART"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.14": {"text": "Herrlicher wieder besuchen mit unserm Brudergeschlechte,", "tokens": ["Herr\u00b7li\u00b7cher", "wie\u00b7der", "be\u00b7su\u00b7chen", "mit", "un\u00b7serm", "Bru\u00b7der\u00b7ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Mit den Menschen; so falle vorher anbetend, und dankend,", "tokens": ["Mit", "den", "Men\u00b7schen", ";", "so", "fal\u00b7le", "vor\u00b7her", "an\u00b7be\u00b7tend", ",", "und", "dan\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "ADV", "VVPP", "$,", "KON", "ADJD", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Jeder von uns in den Staub, und preise den Richter,", "tokens": ["Je\u00b7der", "von", "uns", "in", "den", "Staub", ",", "und", "prei\u00b7se", "den", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}}}}