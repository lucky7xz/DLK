{"textgrid.poem.52131": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Leser, meine Lust, mein Reichthum Gl\u00fcck und Heil,", "tokens": ["O", "Le\u00b7ser", ",", "mei\u00b7ne", "Lust", ",", "mein", "Reicht\u00b7hum", "Gl\u00fcck", "und", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Vers kan dir die Ehr', in der ich lebe, sagen", "tokens": ["Kein", "Vers", "kan", "dir", "die", "Ehr'", ",", "in", "der", "ich", "le\u00b7be", ",", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn ie ein Frembder spricht: Ist nicht mein Wolbehagen,", "tokens": ["Wenn", "ie", "ein", "Fremb\u00b7der", "spricht", ":", "Ist", "nicht", "mein", "Wol\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein deutscher Martial in Perferts Laden feil.", "tokens": ["Ein", "deut\u00b7scher", "Mar\u00b7ti\u00b7al", "in", "Per\u00b7ferts", "La\u00b7den", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Fabeln la\u00df ich seyn. Der deine H\u00e4nde fleucht,", "tokens": ["Die", "Fa\u00b7beln", "la\u00df", "ich", "seyn", ".", "Der", "dei\u00b7ne", "H\u00e4n\u00b7de", "fleucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "VAINF", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nichts als G\u00f6tter redt, die er sucht zu erg\u00f6tzen,", "tokens": ["Und", "nichts", "als", "G\u00f6t\u00b7ter", "redt", ",", "die", "er", "sucht", "zu", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mag an der Sternen Wand die Deutsche Feder setzen:", "tokens": ["Mag", "an", "der", "Ster\u00b7nen", "Wand", "die", "Deut\u00b7sche", "Fe\u00b7der", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Genung, wenn nur mein Blat nach Menschenkindern reucht.", "tokens": ["Ge\u00b7nung", ",", "wenn", "nur", "mein", "Blat", "nach", "Men\u00b7schen\u00b7kin\u00b7dern", "reucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O Leser, meine Cron und auch mein K\u00f6nig Reich,", "tokens": ["O", "Le\u00b7ser", ",", "mei\u00b7ne", "Cron", "und", "auch", "mein", "K\u00f6\u00b7nig", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sind ja etwas werth, wann du mich pflegst zu tragen,", "tokens": ["Wir", "sind", "ja", "et\u00b7was", "werth", ",", "wann", "du", "mich", "pflegst", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Lob ist mein und dein. Ein andrer der mag klagen,", "tokens": ["Das", "Lob", "ist", "mein", "und", "dein", ".", "Ein", "an\u00b7drer", "der", "mag", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "KON", "PPOSAT", "$.", "ART", "ADJA", "ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den meine Musa trifft, ist mir und dir nicht gleich.", "tokens": ["Den", "mei\u00b7ne", "Mu\u00b7sa", "trifft", ",", "ist", "mir", "und", "dir", "nicht", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "KON", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}