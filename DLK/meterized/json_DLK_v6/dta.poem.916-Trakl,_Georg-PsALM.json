{"dta.poem.916": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "PsALM", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Licht, das der Wind ausgel\u00f6scht hat.", "tokens": ["Es", "ist", "ein", "Licht", ",", "das", "der", "Wind", "aus\u00b7ge\u00b7l\u00f6scht", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist ein Heidekrug, den am Nachmittag ein Betrunkener verl\u00e4\u00dft.", "tokens": ["Es", "ist", "ein", "Hei\u00b7de\u00b7krug", ",", "den", "am", "Nach\u00b7mit\u00b7tag", "ein", "Be\u00b7trun\u00b7ke\u00b7ner", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+---+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Es ist ein Weinberg, verbrannt und schwarz mit L\u00f6chern voll Spinnen.", "tokens": ["Es", "ist", "ein", "Wein\u00b7berg", ",", "ver\u00b7brannt", "und", "schwarz", "mit", "L\u00f6\u00b7chern", "voll", "Spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVPP", "KON", "ADJD", "APPR", "NN", "ADJD", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es ist ein Raum, den sie mit Milch get\u00fcncht haben.", "tokens": ["Es", "ist", "ein", "Raum", ",", "den", "sie", "mit", "Milch", "ge\u00b7t\u00fcncht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Der Wahnsinnige ist gestorben. Es ist eine Insel der S\u00fcdsee,", "tokens": ["Der", "Wahn\u00b7sin\u00b7ni\u00b7ge", "ist", "ge\u00b7stor\u00b7ben", ".", "Es", "ist", "ei\u00b7ne", "In\u00b7sel", "der", "S\u00fcd\u00b7see", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+---+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Den Sonnengott zu empfangen. Man r\u00fchrt die Trommeln.", "tokens": ["Den", "Son\u00b7nen\u00b7gott", "zu", "emp\u00b7fan\u00b7gen", ".", "Man", "r\u00fchrt", "die", "Trom\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die M\u00e4nner f\u00fchren kriegerische T\u00e4nze auf.", "tokens": ["Die", "M\u00e4n\u00b7ner", "f\u00fch\u00b7ren", "krie\u00b7ge\u00b7ri\u00b7sche", "T\u00e4n\u00b7ze", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Frauen wiegen die H\u00fcften in Schlinggew\u00e4chsen und Feuerblumen,", "tokens": ["Die", "Frau\u00b7en", "wie\u00b7gen", "die", "H\u00fcf\u00b7ten", "in", "Schling\u00b7ge\u00b7w\u00e4ch\u00b7sen", "und", "Feu\u00b7er\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Wenn das Meer singt. O unser verlorenes Paradies.", "tokens": ["Wenn", "das", "Meer", "singt", ".", "O", "un\u00b7ser", "ver\u00b7lo\u00b7re\u00b7nes", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Die Nymphen haben die goldenen W\u00e4lder verlassen.", "tokens": ["Die", "Nym\u00b7phen", "ha\u00b7ben", "die", "gol\u00b7de\u00b7nen", "W\u00e4l\u00b7der", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Man begr\u00e4bt den Fremden. Dann hebt ein Flimmerregen an.", "tokens": ["Man", "be\u00b7gr\u00e4bt", "den", "Frem\u00b7den", ".", "Dann", "hebt", "ein", "Flim\u00b7mer\u00b7re\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Der Sohn des Pan erscheint in Gestalt eines Erdarbeiters,", "tokens": ["Der", "Sohn", "des", "Pan", "er\u00b7scheint", "in", "Ge\u00b7stalt", "ei\u00b7nes", "Erd\u00b7ar\u00b7bei\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der den Mittag am gl\u00fchenden Asphalt verschl\u00e4ft.", "tokens": ["Der", "den", "Mit\u00b7tag", "am", "gl\u00fc\u00b7hen\u00b7den", "As\u00b7phalt", "ver\u00b7schl\u00e4ft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Es sind kleine M\u00e4dchen in einem Hof in Kleidchen voll herzzerrei\u00dfender Armut!", "tokens": ["Es", "sind", "klei\u00b7ne", "M\u00e4d\u00b7chen", "in", "ei\u00b7nem", "Hof", "in", "Kleid\u00b7chen", "voll", "herz\u00b7zer\u00b7rei\u00b7\u00dfen\u00b7der", "Ar\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Es sind Zimmer, erf\u00fcllt von Akkorden und Sonaten.", "tokens": ["Es", "sind", "Zim\u00b7mer", ",", "er\u00b7f\u00fcllt", "von", "Ak\u00b7kor\u00b7den", "und", "So\u00b7na\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Es sind Schatten, die sich vor einem erblindeten Spiegel umarmen.", "tokens": ["Es", "sind", "Schat\u00b7ten", ",", "die", "sich", "vor", "ei\u00b7nem", "er\u00b7blin\u00b7de\u00b7ten", "Spie\u00b7gel", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+--+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "An den Fenstern des Spitals w\u00e4rmen sich Genesende.", "tokens": ["An", "den", "Fens\u00b7tern", "des", "Spi\u00b7tals", "w\u00e4r\u00b7men", "sich", "Ge\u00b7ne\u00b7sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PRF", "NN", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ein wei\u00dfer Dampfer am Kanal tr\u00e4gt blutige Seuchen herauf.", "tokens": ["Ein", "wei\u00b7\u00dfer", "Damp\u00b7fer", "am", "Ka\u00b7nal", "tr\u00e4gt", "blu\u00b7ti\u00b7ge", "Seu\u00b7chen", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die fremde Schwester erscheint wieder in jemands b\u00f6sen Tr\u00e4umen.", "tokens": ["Die", "frem\u00b7de", "Schwes\u00b7ter", "er\u00b7scheint", "wie\u00b7der", "in", "je\u00b7mands", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ruhend im Haselgeb\u00fcsch spielt sie mit seinen Sternen.", "tokens": ["Ru\u00b7hend", "im", "Ha\u00b7sel\u00b7ge\u00b7b\u00fcsch", "spielt", "sie", "mit", "sei\u00b7nen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.3": {"text": "Der Student, vielleicht ein Doppelg\u00e4nger, schaut ihr lange vom Fenster nach.", "tokens": ["Der", "Stu\u00b7dent", ",", "viel\u00b7leicht", "ein", "Dop\u00b7pel\u00b7g\u00e4n\u00b7ger", ",", "schaut", "ihr", "lan\u00b7ge", "vom", "Fens\u00b7ter", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Hinter ihm steht sein toter Bruder, oder er geht die alte Wendeltreppe herab.", "tokens": ["Hin\u00b7ter", "ihm", "steht", "sein", "to\u00b7ter", "Bru\u00b7der", ",", "o\u00b7der", "er", "geht", "die", "al\u00b7te", "Wen\u00b7del\u00b7trep\u00b7pe", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+-+-+-+--+-+-+-+--+", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "Im Dunkel brauner Kastanien verbla\u00dft die Gestalt des jungen Novizen.", "tokens": ["Im", "Dun\u00b7kel", "brau\u00b7ner", "Kas\u00b7ta\u00b7ni\u00b7en", "ver\u00b7bla\u00dft", "die", "Ge\u00b7stalt", "des", "jun\u00b7gen", "No\u00b7vi\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Der Garten ist im Abend. Im Kreuzgang flattern die Flederm\u00e4use umher.", "tokens": ["Der", "Gar\u00b7ten", "ist", "im", "A\u00b7bend", ".", "Im", "Kreuz\u00b7gang", "flat\u00b7tern", "die", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Die Kinder des Hausmeisters h\u00f6ren zu spielen auf und suchen das Gold des Himmels.", "tokens": ["Die", "Kin\u00b7der", "des", "Haus\u00b7meis\u00b7ters", "h\u00f6\u00b7ren", "zu", "spie\u00b7len", "auf", "und", "su\u00b7chen", "das", "Gold", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "PTKZU", "VVINF", "PTKVZ", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-+-+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "Endakkorde eines Quartetts. Die kleine Blinde l\u00e4uft zitternd durch die Allee,", "tokens": ["En\u00b7dak\u00b7kor\u00b7de", "ei\u00b7nes", "Quar\u00b7tetts", ".", "Die", "klei\u00b7ne", "Blin\u00b7de", "l\u00e4uft", "zit\u00b7ternd", "durch", "die", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+--+-++-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Und sp\u00e4ter tastet ihr Schatten an kalten Mauern hin, umgeben vom M\u00e4rchen und heiligen               Legenden.", "tokens": ["Und", "sp\u00e4\u00b7ter", "tas\u00b7tet", "ihr", "Schat\u00b7ten", "an", "kal\u00b7ten", "Mau\u00b7ern", "hin", ",", "um\u00b7ge\u00b7ben", "vom", "M\u00e4r\u00b7chen", "und", "hei\u00b7li\u00b7gen", "Le\u00b7gen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,", "VVPP", "APPRART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+-+--+--+--+--", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Es ist ein leeres Boot, das am Abend den schwarzen Kanal heruntertreibt.", "tokens": ["Es", "ist", "ein", "lee\u00b7res", "Boot", ",", "das", "am", "A\u00b7bend", "den", "schwar\u00b7zen", "Ka\u00b7nal", "her\u00b7un\u00b7ter\u00b7treibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "In der D\u00fcsternis des alten Asyls verfallen menschliche Ruinen.", "tokens": ["In", "der", "D\u00fcs\u00b7ter\u00b7nis", "des", "al\u00b7ten", "A\u00b7syls", "ver\u00b7fal\u00b7len", "menschli\u00b7che", "Ru\u00b7i\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "ADJA", "NN", "$."], "meter": "--+-+-+----+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die toten Waisen liegen an der Gartenmauer.", "tokens": ["Die", "to\u00b7ten", "Wai\u00b7sen", "lie\u00b7gen", "an", "der", "Gar\u00b7ten\u00b7mau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus grauen Zimmern treten Engel mit kotgefleckten Fl\u00fcgeln.", "tokens": ["Aus", "grau\u00b7en", "Zim\u00b7mern", "tre\u00b7ten", "En\u00b7gel", "mit", "kot\u00b7ge\u00b7fleck\u00b7ten", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "W\u00fcrmer tropfen von ihren vergilbten Lidern.", "tokens": ["W\u00fcr\u00b7mer", "trop\u00b7fen", "von", "ih\u00b7ren", "ver\u00b7gilb\u00b7ten", "Li\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der Platz vor der Kirche ist finster und schweigsam, wie in den Tagen der               Kindheit.", "tokens": ["Der", "Platz", "vor", "der", "Kir\u00b7che", "ist", "fins\u00b7ter", "und", "schweig\u00b7sam", ",", "wie", "in", "den", "Ta\u00b7gen", "der", "Kind\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-+--+--+-", "measure": "amphibrach.tetra.plus"}, "line.7": {"text": "Auf silbernen Sohlen gleiten fr\u00fchere Leben vorbei", "tokens": ["Auf", "sil\u00b7ber\u00b7nen", "Soh\u00b7len", "glei\u00b7ten", "fr\u00fc\u00b7he\u00b7re", "Le\u00b7ben", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und die Schatten der Verdammten steigen zu den seufzenden Wassern nieder.", "tokens": ["Und", "die", "Schat\u00b7ten", "der", "Ver\u00b7damm\u00b7ten", "stei\u00b7gen", "zu", "den", "seuf\u00b7zen\u00b7den", "Was\u00b7sern", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "A