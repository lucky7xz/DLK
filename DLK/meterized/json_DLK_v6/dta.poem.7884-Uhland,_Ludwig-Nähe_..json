{"dta.poem.7884": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4he .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich tret\u2019 in deinen Garten;", "tokens": ["Ich", "tret'", "in", "dei\u00b7nen", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo, S\u00fc\u00dfe, weilst du heut?", "tokens": ["Wo", ",", "S\u00fc\u00b7\u00dfe", ",", "weilst", "du", "heut", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur Schmetterlinge flattern", "tokens": ["Nur", "Schmet\u00b7ter\u00b7lin\u00b7ge", "flat\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch diese Einsamkeit.", "tokens": ["Durch", "die\u00b7se", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wie in bunter F\u00fclle", "tokens": ["Doch", "wie", "in", "bun\u00b7ter", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier deine Beete stehn!", "tokens": ["Hier", "dei\u00b7ne", "Bee\u00b7te", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit den Blumend\u00fcfteu", "tokens": ["Und", "mit", "den", "Blu\u00b7men\u00b7d\u00fcf\u00b7teu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Weste mich umwehn!", "tokens": ["Die", "Wes\u00b7te", "mich", "um\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle dich mir nahe,", "tokens": ["Ich", "f\u00fch\u00b7le", "dich", "mir", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Einsamkeit belebt;", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "be\u00b7lebt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie \u00fcber seinen Welten", "tokens": ["Wie", "\u00fc\u00b7ber", "sei\u00b7nen", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Unsichtbare schwebt.", "tokens": ["Der", "Un\u00b7sicht\u00b7ba\u00b7re", "schwebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}