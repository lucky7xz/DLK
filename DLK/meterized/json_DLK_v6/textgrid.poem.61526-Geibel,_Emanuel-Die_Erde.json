{"textgrid.poem.61526": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Erde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl hast du einst mit hoher Wonne", "tokens": ["Wohl", "hast", "du", "einst", "mit", "ho\u00b7her", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein junges Herz getr\u00e4nkt, Natur,", "tokens": ["Mein", "jun\u00b7ges", "Herz", "ge\u00b7tr\u00e4nkt", ",", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mich der Glanz der Fr\u00fchlingssonne", "tokens": ["Wenn", "mich", "der", "Glanz", "der", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Ferne zog durch Wald und Flur;", "tokens": ["Zur", "Fer\u00b7ne", "zog", "durch", "Wald", "und", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vertieft in mich, mit halbem Lauschen", "tokens": ["Ver\u00b7tieft", "in", "mich", ",", "mit", "hal\u00b7bem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An deinen Wundern streift' ich hin", "tokens": ["An", "dei\u00b7nen", "Wun\u00b7dern", "streift'", "ich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wob in all dein Bl\u00fchn und Rauschen", "tokens": ["Und", "wob", "in", "all", "dein", "Bl\u00fchn", "und", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der eignen Brust geheimsten Sinn.", "tokens": ["Der", "eig\u00b7nen", "Brust", "ge\u00b7heims\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch heilig-ernster ist die Feier,", "tokens": ["Doch", "hei\u00b7lig\u00b7erns\u00b7ter", "ist", "die", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit du jetzt mein Herz umwebst,", "tokens": ["Da\u00b7mit", "du", "jetzt", "mein", "Herz", "um\u00b7webst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn du den falt'gen Isisschleier", "tokens": ["Wenn", "du", "den", "falt'\u00b7gen", "I\u00b7sis\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom hohen Antlitz l\u00fcftend hebst;", "tokens": ["Vom", "ho\u00b7hen", "Ant\u00b7litz", "l\u00fcf\u00b7tend", "hebst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn du vom Reiz der bunten Schale", "tokens": ["Wenn", "du", "vom", "Reiz", "der", "bun\u00b7ten", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Auge still zur Tiefe lenkst", "tokens": ["Mein", "Au\u00b7ge", "still", "zur", "Tie\u00b7fe", "lenkst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und aus des heut'gen Tages Strahle", "tokens": ["Und", "aus", "des", "heut'\u00b7gen", "Ta\u00b7ges", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ins D\u00e4mmerlicht der Urzeit senkst.", "tokens": ["Ins", "D\u00e4m\u00b7mer\u00b7licht", "der", "Ur\u00b7zeit", "senkst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da offenbart im Schwung der Auen,", "tokens": ["Da", "of\u00b7fen\u00b7bart", "im", "Schwung", "der", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In schwarzer Grotten S\u00e4ulenscho\u00df", "tokens": ["In", "schwar\u00b7zer", "Grot\u00b7ten", "S\u00e4u\u00b7len\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich mir der Welle leises Bauen,", "tokens": ["Sich", "mir", "der", "Wel\u00b7le", "lei\u00b7ses", "Bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Feuers jacher Zornessto\u00df;", "tokens": ["Des", "Feu\u00b7ers", "ja\u00b7cher", "Zor\u00b7nes\u00b7sto\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da singt der Gurt geborstner Schichten", "tokens": ["Da", "singt", "der", "Gurt", "ge\u00b7borst\u00b7ner", "Schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein heilig Lied mir vom Entstehn", "tokens": ["Ein", "hei\u00b7lig", "Lied", "mir", "vom", "Ent\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4\u00dft in wandelnden Gesichten", "tokens": ["Und", "l\u00e4\u00dft", "in", "wan\u00b7deln\u00b7den", "Ge\u00b7sich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sch\u00f6pfung mir vor\u00fcbergehn.", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "mir", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wieder schau ich's, wie mit Toben,", "tokens": ["Und", "wie\u00b7der", "schau", "ich's", ",", "wie", "mit", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PIS", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom unterird'schen Dunst gedr\u00e4ngt,", "tokens": ["Vom", "un\u00b7ter\u00b7ird'\u00b7schen", "Dunst", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der fl\u00fcss'ge Kern des Erdballs droben", "tokens": ["Der", "fl\u00fcss'\u00b7ge", "Kern", "des", "Erd\u00b7balls", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die meergebornen Krusten sprengt;", "tokens": ["Die", "meer\u00b7ge\u00b7bor\u00b7nen", "Krus\u00b7ten", "sprengt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie er, ein Strom von z\u00e4hen Gluten,", "tokens": ["Wie", "er", ",", "ein", "Strom", "von", "z\u00e4\u00b7hen", "Glu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis in die Wolken rauchend st\u00fcrmt", "tokens": ["Bis", "in", "die", "Wol\u00b7ken", "rau\u00b7chend", "st\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber T\u00e4ler dann und Fluten", "tokens": ["Und", "\u00fc\u00b7ber", "T\u00e4\u00b7ler", "dann", "und", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zergipfelt zum Gebirg' sich t\u00fcrmt.", "tokens": ["Zer\u00b7gip\u00b7felt", "zum", "Ge\u00b7bir\u00b7g'", "sich", "t\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "O Riesenkampf der Urgewalten,", "tokens": ["O", "Rie\u00b7sen\u00b7kampf", "der", "Ur\u00b7ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drin eine Welt sich g\u00e4rend r\u00fchrt,", "tokens": ["Drin", "ei\u00b7ne", "Welt", "sich", "g\u00e4\u00b7rend", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der von Gestalten zu Gestalten", "tokens": ["Der", "von", "Ge\u00b7stal\u00b7ten", "zu", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich auf ein letzt Geheimnis f\u00fchrt!", "tokens": ["Mich", "auf", "ein", "letzt", "Ge\u00b7heim\u00b7nis", "f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wie ich rastlos r\u00fcckw\u00e4rts dringe", "tokens": ["Denn", "wie", "ich", "rast\u00b7los", "r\u00fcck\u00b7w\u00e4rts", "drin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Form zu Form, erlischt die Spur;", "tokens": ["Von", "Form", "zu", "Form", ",", "er\u00b7lischt", "die", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich steh' am Abgrund, draus die Dinge", "tokens": ["Ich", "steh'", "am", "Ab\u00b7grund", ",", "draus", "die", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der erste Lebenspuls durchfuhr.", "tokens": ["Der", "ers\u00b7te", "Le\u00b7bens\u00b7puls", "durch\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da f\u00e4llt ins zagende Gem\u00fcte", "tokens": ["Da", "f\u00e4llt", "ins", "za\u00b7gen\u00b7de", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein Glanz aus tiefsten Tiefen mir:", "tokens": ["Ein", "Glanz", "aus", "tiefs\u00b7ten", "Tie\u00b7fen", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbim Anfang war die ew'ge G\u00fcte,", "tokens": ["\u00bb", "im", "An\u00b7fang", "war", "die", "ew'\u00b7ge", "G\u00fc\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend Engel dienen ihr!\u00ab", "tokens": ["Und", "tau\u00b7send", "En\u00b7gel", "die\u00b7nen", "ihr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie sie licht in Flammen wallen,", "tokens": ["Und", "wie", "sie", "licht", "in", "Flam\u00b7men", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Fluten brausen allerorts,", "tokens": ["In", "Flu\u00b7ten", "brau\u00b7sen", "al\u00b7le\u00b7rorts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Empfind' ich schauernd \u00fcber allen", "tokens": ["Emp\u00b7find'", "ich", "schau\u00b7ernd", "\u00fc\u00b7ber", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Hauch des unerschaffnen Worts.", "tokens": ["Den", "Hauch", "des", "un\u00b7er\u00b7schaff\u00b7nen", "Worts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}