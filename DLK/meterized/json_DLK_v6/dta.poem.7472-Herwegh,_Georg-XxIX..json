{"dta.poem.7472": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tief, tief im Meere sprach einst eine Welle:", "tokens": ["Tief", ",", "tief", "im", "Mee\u00b7re", "sprach", "einst", "ei\u00b7ne", "Wel\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie gl\u00fccklich m\u00fcssen meine Schwestern leben,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "m\u00fcs\u00b7sen", "mei\u00b7ne", "Schwes\u00b7tern", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die droben strahlend auf und nieder schweben;", "tokens": ["Die", "dro\u00b7ben", "strah\u00b7lend", "auf", "und", "nie\u00b7der", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O d\u00fcrft' ich einmal an des Tages Helle!", "tokens": ["O", "d\u00fcrft'", "ich", "ein\u00b7mal", "an", "des", "Ta\u00b7ges", "Hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sie gebeten, so geschah ihr schnelle,", "tokens": ["Wie", "sie", "ge\u00b7be\u00b7ten", ",", "so", "ge\u00b7schah", "ihr", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie durfte aus dem dunkeln Schoos sich heben;", "tokens": ["Sie", "durf\u00b7te", "aus", "dem", "dun\u00b7keln", "Schoos", "sich", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch kaum war ihr Ein Sonnenstrahl gegeben,", "tokens": ["Doch", "kaum", "war", "ihr", "Ein", "Son\u00b7nen\u00b7strahl", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lag sie schon sterbend an des Ufers Schwelle.", "tokens": ["Lag", "sie", "schon", "ster\u00b7bend", "an", "des", "U\u00b7fers", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O m\u00f6gen Alle doch ihr Schicksal loben,", "tokens": ["O", "m\u00f6\u00b7gen", "Al\u00b7le", "doch", "ihr", "Schick\u00b7sal", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die still geheim des Lebens Kreis beschreiben", "tokens": ["Die", "still", "ge\u00b7heim", "des", "Le\u00b7bens", "Kreis", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nie die Wut der offnen See erproben.", "tokens": ["Und", "nie", "die", "Wut", "der", "off\u00b7nen", "See", "er\u00b7pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O m\u00f6gen sie in tiefer Nacht verbleiben,", "tokens": ["O", "m\u00f6\u00b7gen", "sie", "in", "tie\u00b7fer", "Nacht", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihrer Keiner streben je nach oben,", "tokens": ["Und", "ih\u00b7rer", "Kei\u00b7ner", "stre\u00b7ben", "je", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um mit den Winden auf den Sand zu treiben.", "tokens": ["Um", "mit", "den", "Win\u00b7den", "auf", "den", "Sand", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}