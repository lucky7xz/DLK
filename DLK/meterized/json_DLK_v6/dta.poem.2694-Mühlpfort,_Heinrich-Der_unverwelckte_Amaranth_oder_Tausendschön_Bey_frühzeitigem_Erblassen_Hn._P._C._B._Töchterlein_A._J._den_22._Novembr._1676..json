{"dta.poem.2694": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der unverwelckte Amaranth oder Tausendsch\u00f6n  \n  Bey fr\u00fchzeitigem Erblassen Hn. P. C. B.  \n T\u00f6chterlein A. J. den 22. Novembr. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gje\u00dft Nimfen eure heisse Z\u00e4hren", "tokens": ["Gje\u00dft", "Nim\u00b7fen", "eu\u00b7re", "heis\u00b7se", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auff diesen kalten Leichen-Stein.", "tokens": ["Auff", "die\u00b7sen", "kal\u00b7ten", "Lei\u00b7chen\u00b7Stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Eltern hoffen und begehren/", "tokens": ["Der", "El\u00b7tern", "hof\u00b7fen", "und", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr Kleinod/ schleust der Sarch jetzt ein.", "tokens": ["Ihr", "Klei\u00b7nod", "/", "schleust", "der", "Sarch", "jetzt", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Streut Blumen aus/ denn eure Blume", "tokens": ["Streut", "Blu\u00b7men", "aus", "/", "denn", "eu\u00b7re", "Blu\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die lieblich\u2019 Amarant erblast.", "tokens": ["Die", "lieb\u00b7lich'", "A\u00b7ma\u00b7rant", "er\u00b7blast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Singt ihr ein Lied zum Ehren-Ruhme", "tokens": ["Singt", "ihr", "ein", "Lied", "zum", "Eh\u00b7ren\u00b7Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Lieb\u2019 und Treu hat abgefast.", "tokens": ["Das", "Lieb'", "und", "Treu", "hat", "ab\u00b7ge\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob schon des Winters kaltes Rasen", "tokens": ["Ob", "schon", "des", "Win\u00b7ters", "kal\u00b7tes", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den G\u00e4rten ihren Purpur nimmt/", "tokens": ["Den", "G\u00e4r\u00b7ten", "ih\u00b7ren", "Pur\u00b7pur", "nimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fcr der L\u00fcffte strengem blasen", "tokens": ["Und", "f\u00fcr", "der", "L\u00fcff\u00b7te", "stren\u00b7gem", "bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts weiter mehr zum Wachsthum k\u00f6mmt:", "tokens": ["Nichts", "wei\u00b7ter", "mehr", "zum", "Wach\u00b7sthum", "k\u00f6mmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bl\u00fcht doch eure Amaranthe/", "tokens": ["So", "bl\u00fcht", "doch", "eu\u00b7re", "A\u00b7ma\u00b7ran\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das wunder-holde Tausendsch\u00f6n/", "tokens": ["Das", "wun\u00b7der\u00b7hol\u00b7de", "Tau\u00b7send\u00b7sch\u00f6n", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wird/ des Himmels Anverwandte/", "tokens": ["Und", "wird", "/", "des", "Him\u00b7mels", "An\u00b7ver\u00b7wand\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort unter Cherubinen stehn/", "tokens": ["Dort", "un\u00b7ter", "Che\u00b7ru\u00b7bi\u00b7nen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Vor-Welt hat ihr zugeschrieben/", "tokens": ["Die", "Vor\u00b7Welt", "hat", "ihr", "zu\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihre Pracht unsterblich sey.", "tokens": ["Da\u00df", "ih\u00b7re", "Pracht", "uns\u00b7terb\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nichts von Blumen \u00fcbrig blieben/", "tokens": ["Wenn", "nichts", "von", "Blu\u00b7men", "\u00fcb\u00b7rig", "blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kr\u00f6nt ihren Strauch ein ewig May.", "tokens": ["Kr\u00f6nt", "ih\u00b7ren", "Strauch", "ein", "e\u00b7wig", "May", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es weichen ihr die bunten Nelcken/", "tokens": ["Es", "wei\u00b7chen", "ihr", "die", "bun\u00b7ten", "Nel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tulpen Sammt/ der Lilgen Schnee.", "tokens": ["Der", "Tul\u00b7pen", "Sammt", "/", "der", "Lil\u00b7gen", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn diese fangen an zu welcken/", "tokens": ["Wenn", "die\u00b7se", "fan\u00b7gen", "an", "zu", "wel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Steigt ihre Blume in die H\u00f6h.", "tokens": ["Steigt", "ih\u00b7re", "Blu\u00b7me", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es mag ein tieffer Schnee sie decken/", "tokens": ["Es", "mag", "ein", "tief\u00b7fer", "Schnee", "sie", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bl\u00fchet unter Frost und Ei\u00df.", "tokens": ["Sie", "bl\u00fc\u00b7het", "un\u00b7ter", "Frost", "und", "Ei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hundsstern kan sie nicht erschrecken/", "tokens": ["Der", "Hunds\u00b7stern", "kan", "sie", "nicht", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr macht des Sommers Gluth nicht hei\u00df.", "tokens": ["Ihr", "macht", "des", "Som\u00b7mers", "Gluth", "nicht", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Egypten hat sie hoch gesch\u00e4tzet/", "tokens": ["E\u00b7gyp\u00b7ten", "hat", "sie", "hoch", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und von der Blumen Liebligkeit", "tokens": ["Und", "von", "der", "Blu\u00b7men", "Lieb\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Alten Kronen auffgesetzet/", "tokens": ["Den", "Al\u00b7ten", "Kro\u00b7nen", "auff\u00b7ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Zeichen der gesunden Zeit.", "tokens": ["Zum", "Zei\u00b7chen", "der", "ge\u00b7sun\u00b7den", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sagt Nymfen ob des Lebens Gr\u00fcne", "tokens": ["Sagt", "Nym\u00b7fen", "ob", "des", "Le\u00b7bens", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Anmuth von der Seel\u2019gen weicht?", "tokens": ["Und", "An\u00b7muth", "von", "der", "Seel'\u00b7gen", "weicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob die verschwesterte ", "tokens": ["Ob", "die", "ver\u00b7schwes\u00b7ter\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich nicht den Amaranthen gleicht?", "tokens": ["Sich", "nicht", "den", "A\u00b7ma\u00b7ran\u00b7then", "gleicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sehen sie zwar hier erblassen/", "tokens": ["Wir", "se\u00b7hen", "sie", "zwar", "hier", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nur zu gr\u00f6\u00dfrer Herrligkeit:", "tokens": ["Doch", "nur", "zu", "gr\u00f6\u00df\u00b7rer", "Herr\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn sie wird dort ein Glantz umbfassen/", "tokens": ["Denn", "sie", "wird", "dort", "ein", "Glantz", "umb\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der edlen Steinen Kampf anbeut.", "tokens": ["Der", "ed\u00b7len", "Stei\u00b7nen", "Kampf", "an\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Augen/ so jetzund versincken/", "tokens": ["Die", "Au\u00b7gen", "/", "so", "je\u00b7tzund", "ver\u00b7sin\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verkl\u00e4rt ein Diamanten Licht.", "tokens": ["Ver\u00b7kl\u00e4rt", "ein", "Di\u00b7a\u00b7man\u00b7ten", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sollen wie die Sterne blincken", "tokens": ["Sie", "sol\u00b7len", "wie", "die", "Ster\u00b7ne", "blin\u00b7cken"], "token_info": ["word", "word", "word", "wo