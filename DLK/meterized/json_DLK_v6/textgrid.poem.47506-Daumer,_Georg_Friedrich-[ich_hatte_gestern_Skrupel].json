{"textgrid.poem.47506": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich hatte gestern Skrupel]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte gestern Skrupel", "tokens": ["Ich", "hat\u00b7te", "ge\u00b7stern", "Skru\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ging zu meinem alten Wirth;", "tokens": ["Und", "ging", "zu", "mei\u00b7nem", "al\u00b7ten", "Wirth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der l\u00f6s'te jede Frage,", "tokens": ["Der", "l\u00f6s'\u00b7te", "je\u00b7de", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist ein \u00e4chter Seelenhirt.", "tokens": ["Er", "ist", "ein", "\u00e4ch\u00b7ter", "See\u00b7len\u00b7hirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sogleich zu seinem Stalle", "tokens": ["Sog\u00b7leich", "zu", "sei\u00b7nem", "Stal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zeucht er zur\u00fcck ein jegliches", "tokens": ["Zeucht", "er", "zu\u00b7r\u00fcck", "ein", "jeg\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Schafe seiner Herde,", "tokens": ["Der", "Scha\u00b7fe", "sei\u00b7ner", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sich vom rechten Weg verirrt.", "tokens": ["Das", "sich", "vom", "rech\u00b7ten", "Weg", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er zeigte mir im Glase", "tokens": ["Er", "zeig\u00b7te", "mir", "im", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Becher Dschem's, den magischen;", "tokens": ["Den", "Be\u00b7cher", "Dschem's", ",", "den", "ma\u00b7gi\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seiner Tiefe sah ich", "tokens": ["In", "sei\u00b7ner", "Tie\u00b7fe", "sah", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Wahre klar und unverwirrt.", "tokens": ["Das", "Wah\u00b7re", "klar", "und", "un\u00b7ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich sahe, da\u00df voll Weisheit", "tokens": ["Ich", "sa\u00b7he", ",", "da\u00df", "voll", "Weis\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur was die s\u00fc\u00dfe Nachtigall", "tokens": ["Nur", "was", "die", "s\u00fc\u00b7\u00dfe", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihrem Aste fl\u00f6tet,", "tokens": ["Auf", "ih\u00b7rem", "As\u00b7te", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Taube, die verliebte, girrt.", "tokens": ["Die", "Tau\u00b7be", ",", "die", "ver\u00b7lieb\u00b7te", ",", "girrt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich sahe, da\u00df voll Tiefsinn", "tokens": ["Ich", "sa\u00b7he", ",", "da\u00df", "voll", "Tief\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wunderliche Stanze nur,", "tokens": ["Die", "wun\u00b7der\u00b7li\u00b7che", "Stan\u00b7ze", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die uns der K\u00e4fer murmelt,", "tokens": ["Die", "uns", "der", "K\u00e4\u00b7fer", "mur\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der um den Kelch der Lilie schwirrt.", "tokens": ["Der", "um", "den", "Kelch", "der", "Li\u00b7lie", "schwirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}