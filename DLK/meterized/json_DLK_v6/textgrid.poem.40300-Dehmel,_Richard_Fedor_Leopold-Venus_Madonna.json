{"textgrid.poem.40300": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Madonna", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Mannesadel w\u00e4chst des Weibes Tugend;", "tokens": ["Aus", "Man\u00b7ne\u00b7sa\u00b7del", "w\u00e4chst", "des", "Wei\u00b7bes", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "er tr\u00e4umt ein Ziel, sie soll es ihm geb\u00e4ren.", "tokens": ["er", "tr\u00e4umt", "ein", "Ziel", ",", "sie", "soll", "es", "ihm", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Griechen Sch\u00f6nheitsinbrunst sah die Sph\u00e4ren", "tokens": ["Des", "Grie\u00b7chen", "Sch\u00f6n\u00b7heits\u00b7in\u00b7brunst", "sah", "die", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "beherrscht von Aphroditens Reiz und Jugend;", "tokens": ["be\u00b7herrscht", "von", "A\u00b7phro\u00b7di\u00b7tens", "Reiz", "und", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "dem Christen aber ward die Reinheit Wesen,", "tokens": ["dem", "Chris\u00b7ten", "a\u00b7ber", "ward", "die", "Rein\u00b7heit", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "selbst noch die Mutter will er sich verkl\u00e4ren", "tokens": ["selbst", "noch", "die", "Mut\u00b7ter", "will", "er", "sich", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und beugt sich vor Marias Hochalt\u00e4ren,", "tokens": ["und", "beugt", "sich", "vor", "Ma\u00b7rias", "Hoch\u00b7al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die keusch des Sohns, des keuscheren, genesen.", "tokens": ["die", "keusch", "des", "Sohns", ",", "des", "keu\u00b7sche\u00b7ren", ",", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wann kommt die Zeit, da\u00df M\u00e4nner freier denken", "tokens": ["Wann", "kommt", "die", "Zeit", ",", "da\u00df", "M\u00e4n\u00b7ner", "frei\u00b7er", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und ihre eigne Welt von Gottess\u00f6hnen", "tokens": ["und", "ih\u00b7re", "eig\u00b7ne", "Welt", "von", "Got\u00b7tes\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hell mit dem Huldbild ihrer Freiheit kr\u00f6nen,", "tokens": ["hell", "mit", "dem", "Huld\u00b7bild", "ih\u00b7rer", "Frei\u00b7heit", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "bis Alle Allen die Erl\u00f6sung schenken,", "tokens": ["bis", "Al\u00b7le", "Al\u00b7len", "die", "Er\u00b7l\u00f6\u00b7sung", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Wir uns schenkten, meine Magd und Sonne,", "tokens": ["die", "Wir", "uns", "schenk\u00b7ten", ",", "mei\u00b7ne", "Magd", "und", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du keusche Venus, reizende Madonne!", "tokens": ["Du", "keu\u00b7sche", "Ve\u00b7nus", ",", "rei\u00b7zen\u00b7de", "Ma\u00b7don\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}