{"textgrid.poem.33391": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00fcster und sein Esel", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein reicher K\u00fcster hatt' einmal,", "tokens": ["Ein", "rei\u00b7cher", "K\u00fcs\u00b7ter", "hatt'", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nebst vielen Ochsen, Schaafen, Schweinen,", "tokens": ["Nebst", "vie\u00b7len", "Och\u00b7sen", ",", "Schaa\u00b7fen", ",", "Schwei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch einen Esel in dem Stall:", "tokens": ["Auch", "ei\u00b7nen", "E\u00b7sel", "in", "dem", "Stall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Seit Bileams Zeiten gab's so keinen;", "tokens": ["Seit", "Bi\u00b7leams", "Zei\u00b7ten", "gab's", "so", "kei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ADV", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn so wie jener, ward auch der", "tokens": ["Denn", "so", "wie", "je\u00b7ner", ",", "ward", "auch", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "PDS", "$,", "VAFIN", "ADV", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Im ganzen Dorf ber\u00fchmter, als sein Herr.", "tokens": ["Im", "gan\u00b7zen", "Dorf", "be\u00b7r\u00fchm\u00b7ter", ",", "als", "sein", "Herr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Esels Kraft bestand im Schreien, das zu stillen", "tokens": ["Des", "E\u00b7sels", "Kraft", "be\u00b7stand", "im", "Schrei\u00b7en", ",", "das", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Unm\u00f6glich war, wenn er begann,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "war", ",", "wenn", "er", "be\u00b7gann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wenn er in der Heerde ging, so h\u00f6rte man", "tokens": ["Und", "wenn", "er", "in", "der", "Heer\u00b7de", "ging", ",", "so", "h\u00f6r\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vom Bl\u00f6cken, Grunzen, Wiehern, Br\u00fcllen", "tokens": ["Vom", "Bl\u00f6\u00b7cken", ",", "Grun\u00b7zen", ",", "Wie\u00b7hern", ",", "Br\u00fcl\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der ganzen Heerde nichts, als sein Iha!", "tokens": ["Der", "gan\u00b7zen", "Heer\u00b7de", "nichts", ",", "als", "sein", "I\u00b7ha", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So oft man ihn nun auf der Gasse sah,", "tokens": ["So", "oft", "man", "ihn", "nun", "auf", "der", "Gas\u00b7se", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Entstand im Dorf ein allgemein Gefl\u00fcster,", "tokens": ["Ent\u00b7stand", "im", "Dorf", "ein", "all\u00b7ge\u00b7mein", "Ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Man lief, und sah dem Wunderthiere nach,", "tokens": ["Man", "lief", ",", "und", "sah", "dem", "Wun\u00b7der\u00b7thie\u00b7re", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und niemand war, der nicht vom K\u00fcster,", "tokens": ["Und", "nie\u00b7mand", "war", ",", "der", "nicht", "vom", "K\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und seinem Wunderesel sprach.", "tokens": ["Und", "sei\u00b7nem", "Wun\u00b7de\u00b7re\u00b7sel", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das Aufseh'n, das der Esel machte,", "tokens": ["Das", "Auf\u00b7seh'n", ",", "das", "der", "E\u00b7sel", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Gefiel dem K\u00fcster sehr; er dachte:", "tokens": ["Ge\u00b7fiel", "dem", "K\u00fcs\u00b7ter", "sehr", ";", "er", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So lang' die Welt von meinem Esel spricht,", "tokens": ["So", "lang'", "die", "Welt", "von", "mei\u00b7nem", "E\u00b7sel", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Vergi\u00dft sie sicherlich auch meiner nicht.", "tokens": ["Ver\u00b7gi\u00dft", "sie", "si\u00b7cher\u00b7lich", "auch", "mei\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Damit nun fernerhin von ihm gesprochen wurde,", "tokens": ["Da\u00b7mit", "nun", "fer\u00b7ner\u00b7hin", "von", "ihm", "ge\u00b7spro\u00b7chen", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Macht' er den Esel gar zum F\u00fchrer seiner Heerde", "tokens": ["Macht'", "er", "den", "E\u00b7sel", "gar", "zum", "F\u00fch\u00b7rer", "sei\u00b7ner", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.23": {"text": "Und wies den ersten Platz in seinem Stall ihm an.", "tokens": ["Und", "wies", "den", "ers\u00b7ten", "Platz", "in", "sei\u00b7nem", "Stall", "ihm", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der neue F\u00fchrer nun begann", "tokens": ["Der", "neu\u00b7e", "F\u00fch\u00b7rer", "nun", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sein Amt mit ungemeiner Freude,", "tokens": ["Sein", "Amt", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Schrie alle Morgen Rind und Schaaf", "tokens": ["Schrie", "al\u00b7le", "Mor\u00b7gen", "Rind", "und", "Schaaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und Schwein und Widder aus dem Schlaf,", "tokens": ["Und", "Schwein", "und", "Wid\u00b7der", "aus", "dem", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und f\u00fchrte sie stolzirend auf die Weide.", "tokens": ["Und", "f\u00fchr\u00b7te", "sie", "stol\u00b7zi\u00b7rend", "auf", "die", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Das Dorf fand diesen Einfall sch\u00f6n,", "tokens": ["Das", "Dorf", "fand", "die\u00b7sen", "Ein\u00b7fall", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So lang' er neu noch war, und lachte;", "tokens": ["So", "lang'", "er", "neu", "noch", "war", ",", "und", "lach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "ADV", "VAFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Der faule K\u00fcster aber dachte:", "tokens": ["Der", "fau\u00b7le", "K\u00fcs\u00b7ter", "a\u00b7ber", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn K\u00fch' und Ochsen auf den Ruf des Esels geh'n,", "tokens": ["Wenn", "K\u00fch'", "und", "Och\u00b7sen", "auf", "den", "Ruf", "des", "E\u00b7sels", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So werden auch die Menschen ihn versteh'n,", "tokens": ["So", "wer\u00b7den", "auch", "die", "Men\u00b7schen", "ihn", "ver\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und lie\u00df auch, um nicht mehr zur Messe selbst zu l\u00e4uten,", "tokens": ["Und", "lie\u00df", "auch", ",", "um", "nicht", "mehr", "zur", "Mes\u00b7se", "selbst", "zu", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUI", "PTKNEG", "ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Den Esel dies durch einen Schrei bedeuten.", "tokens": ["Den", "E\u00b7sel", "dies", "durch", "ei\u00b7nen", "Schrei", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Das Kirchspiel f\u00fcgte sich und lief", "tokens": ["Das", "Kirch\u00b7spiel", "f\u00fcg\u00b7te", "sich", "und", "lief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Lautlachend zum Gebet, so oft der Esel rief.", "tokens": ["Laut\u00b7la\u00b7chend", "zum", "Ge\u00b7bet", ",", "so", "oft", "der", "E\u00b7sel", "rief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Am Ende ward dem Volk das L\u00e4rmen doch zuwider,", "tokens": ["Am", "En\u00b7de", "ward", "dem", "Volk", "das", "L\u00e4r\u00b7men", "doch", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die guten Leute w\u00fcnschten sich", "tokens": ["Die", "gu\u00b7ten", "Leu\u00b7te", "w\u00fcnschten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Die Thurm- und Rinderglocken wieder. \u2013", "tokens": ["Die", "Thur\u00b7m", "und", "Rin\u00b7der\u00b7glo\u00b7cken", "wie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "ADV", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Der Esel legte sich auch endlich wirklich nieder \u2013", "tokens": ["Der", "E\u00b7sel", "leg\u00b7te", "sich", "auch", "end\u00b7lich", "wirk\u00b7lich", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Schrie immer schw\u00e4cher und verblich.", "tokens": ["Schrie", "im\u00b7mer", "schw\u00e4\u00b7cher", "und", "ver\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der K\u00fcster weinte bitterlich", "tokens": ["Der", "K\u00fcs\u00b7ter", "wein\u00b7te", "bit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Um seinen Freund; denn, ach! dahin gefahren", "tokens": ["Um", "sei\u00b7nen", "Freund", ";", "denn", ",", "ach", "!", "da\u00b7hin", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "ADV", "$,", "ITJ", "$.", "PAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "War mit dem Esel auch sein ganzer Ruhm,", "tokens": ["War", "mit", "dem", "E\u00b7sel", "auch", "sein", "gan\u00b7zer", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und seine Stelle zu ersetzen, waren", "tokens": ["Und", "sei\u00b7ne", "Stel\u00b7le", "zu", "er\u00b7set\u00b7zen", ",", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Die andern Esel alle viel zu dumm.", "tokens": ["Die", "an\u00b7dern", "E\u00b7sel", "al\u00b7le", "viel", "zu", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Doch endlich gl\u00fcckt' es ihm, ein Mittel auszufinden", "tokens": ["Doch", "end\u00b7lich", "gl\u00fcckt'", "es", "ihm", ",", "ein", "Mit\u00b7tel", "aus\u00b7zu\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Um der Vergessenheit sich zu entzieh'n:", "tokens": ["Um", "der", "Ver\u0