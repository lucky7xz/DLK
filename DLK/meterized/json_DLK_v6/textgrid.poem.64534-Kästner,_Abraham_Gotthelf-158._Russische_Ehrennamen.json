{"textgrid.poem.64534": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "158. Russische Ehrennamen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mundart Galliens nennt oft der Fremde sch\u00f6n,", "tokens": ["Die", "Mund\u00b7art", "Gal\u00b7li\u00b7ens", "nennt", "oft", "der", "Frem\u00b7de", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kann darin nur fluchen oder schm\u00e4hn;", "tokens": ["Und", "kann", "da\u00b7rin", "nur", "flu\u00b7chen", "o\u00b7der", "schm\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch aus der russischen wei\u00df ich nur Ehrentitel,", "tokens": ["Doch", "aus", "der", "rus\u00b7si\u00b7schen", "wei\u00df", "ich", "nur", "Eh\u00b7ren\u00b7ti\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als: Zaar, Bojar, Knees und Utschitel.", "tokens": ["Als", ":", "Zaar", ",", "Bo\u00b7jar", ",", "Knees", "und", "Ut\u00b7schi\u00b7tel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "NN", "$,", "NE", "$,", "NE", "KON", "NN", "$."], "meter": "-++-+-+--", "measure": "unknown.measure.tetra"}}}}}