{"textgrid.poem.59596": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frischauf, ihr deutschen Scharen!", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischauf, ihr deutschen Scharen!", "tokens": ["Frisc\u00b7hauf", ",", "ihr", "deut\u00b7schen", "Scha\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frischauf zum heil'gen Krieg!", "tokens": ["Frisc\u00b7hauf", "zum", "heil'\u00b7gen", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wird sich offenbaren", "tokens": ["Gott", "wird", "sich", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Tode und im Sieg:", "tokens": ["Im", "To\u00b7de", "und", "im", "Sieg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Gott, dem Frommen, Starken,", "tokens": ["Mit", "Gott", ",", "dem", "From\u00b7men", ",", "Star\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seid fr\u00f6hlich und geschwind,", "tokens": ["Seid", "fr\u00f6h\u00b7lich", "und", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "K\u00e4mpft f\u00fcr des Landes Marken,", "tokens": ["K\u00e4mpft", "f\u00fcr", "des", "Lan\u00b7des", "Mar\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Eltern, Weib und Kind.", "tokens": ["F\u00fcr", "El\u00b7tern", ",", "Weib", "und", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frischauf! Ihr tragt das Zeichen", "tokens": ["Frisc\u00b7hauf", "!", "Ihr", "tragt", "das", "Zei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Heils an eurem Hut.", "tokens": ["Des", "Heils", "an", "eu\u00b7rem", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem mu\u00df die H\u00f6lle weichen", "tokens": ["Dem", "mu\u00df", "die", "H\u00f6l\u00b7le", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Und Satans Frevelmut,", "tokens": ["Und", "Sa\u00b7tans", "Fre\u00b7vel\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ihr mit treuem Herzen", "tokens": ["Wenn", "ihr", "mit", "treu\u00b7em", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und rechtem Glauben denkt,", "tokens": ["Und", "rech\u00b7tem", "Glau\u00b7ben", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr wie viel bittre Schmerzen", "tokens": ["F\u00fcr", "wie", "viel", "bitt\u00b7re", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich Gottes Sohn geschenkt.", "tokens": ["Sich", "Got\u00b7tes", "Sohn", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum auf f\u00fcr deutsche Ehre,", "tokens": ["Drum", "auf", "f\u00fcr", "deut\u00b7sche", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du tapfres Teutsgeschlecht!", "tokens": ["Du", "tapf\u00b7res", "Teuts\u00b7ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der beste Schild der Heere", "tokens": ["Der", "bes\u00b7te", "Schild", "der", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hei\u00dft Vaterland und Recht,", "tokens": ["Hei\u00dft", "Va\u00b7ter\u00b7land", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als sch\u00f6nste Losung klinget", "tokens": ["Als", "sch\u00f6ns\u00b7te", "Lo\u00b7sung", "klin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Freiheit in das Feld,", "tokens": ["Die", "Frei\u00b7heit", "in", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo sie die Fahne schwinget,", "tokens": ["Wo", "sie", "die", "Fah\u00b7ne", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird jedes Kind ein Held.", "tokens": ["Wird", "je\u00b7des", "Kind", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum auf, ihr deutschen Scharen!", "tokens": ["Drum", "auf", ",", "ihr", "deut\u00b7schen", "Scha\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frischauf zum heil'gen Krieg!", "tokens": ["Frisc\u00b7hauf", "zum", "heil'\u00b7gen", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wird sich offenbaren", "tokens": ["Gott", "wird", "sich", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Tode und im Sieg;", "tokens": ["Im", "To\u00b7de", "und", "im", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die ganze H\u00f6lle", "tokens": ["Und", "wenn", "die", "gan\u00b7ze", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich g\u00f6sse \u00fcber euch,", "tokens": ["Sich", "g\u00f6s\u00b7se", "\u00fc\u00b7ber", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr sp\u00fclt sie, wie die Welle", "tokens": ["Ihr", "sp\u00fclt", "sie", ",", "wie", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Sandkorn, weg von euch.", "tokens": ["Das", "Sand\u00b7korn", ",", "weg", "von", "euch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}