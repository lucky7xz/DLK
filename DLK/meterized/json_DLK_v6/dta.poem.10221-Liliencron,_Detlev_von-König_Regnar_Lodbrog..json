{"dta.poem.10221": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Regnar Lodbrog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Das war der K\u00f6nig Regnar,             ", "tokens": ["Das", "war", "der", "K\u00f6\u00b7nig", "Re\u00b7gnar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der lebte fromm und frei.", "tokens": ["Der", "leb\u00b7te", "fromm", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das war ihm einerlei.", "tokens": ["Das", "war", "ihm", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er liebte schneidig Sch\u00f6n Thora,", "tokens": ["Er", "lieb\u00b7te", "schnei\u00b7dig", "Sch\u00f6n", "Tho\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NE", "NE", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die wohnte fern im Turm.", "tokens": ["Die", "wohn\u00b7te", "fern", "im", "Turm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da lag um den Turm ein Wurm.", "tokens": ["Da", "lag", "um", "den", "Turm", "ein", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der sah den K\u00f6nig nahen", "tokens": ["Der", "sah", "den", "K\u00f6\u00b7nig", "na\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Flut und Schaumgefurch.", "tokens": ["Durch", "Flut", "und", "Schaum\u00b7ge\u00b7furch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es starb vor Schreck der Lurch.", "tokens": ["Es", "starb", "vor", "Schreck", "der", "Lurch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er freite schnell und befreite", "tokens": ["Er", "frei\u00b7te", "schnell", "und", "be\u00b7frei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sch\u00f6n Thora von Angst und Weh.", "tokens": ["Sch\u00f6n", "Tho\u00b7ra", "von", "Angst", "und", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gar bis Ninive.", "tokens": ["Und", "gar", "bis", "Ni\u00b7ni\u00b7ve", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Regnar, der edle R\u00e4uber,", "tokens": ["Re\u00b7gnar", ",", "der", "ed\u00b7le", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er raubte, was sich fand.", "tokens": ["Er", "raub\u00b7te", ",", "was", "sich", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trat wo sein Fu\u00df aus Land.", "tokens": ["Trat", "wo", "sein", "Fu\u00df", "aus", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bald spielten um ihn drei S\u00f6hne,", "tokens": ["Bald", "spiel\u00b7ten", "um", "ihn", "drei", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Genannt Ebb\u2019, Ubbe, Obb\u2019.", "tokens": ["Ge\u00b7nannt", "Ebb'", ",", "Ub\u00b7be", ",", "Obb'", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann ward er sacksiedegrob.", "tokens": ["Dann", "ward", "er", "sack\u00b7sie\u00b7de\u00b7grob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Einst segelt er nach England,", "tokens": ["Einst", "se\u00b7gelt", "er", "nach", "En\u00b7gland", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die S\u00f6hne blieben zur\u00fcck.", "tokens": ["Die", "S\u00f6h\u00b7ne", "blie\u00b7ben", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Regnar, wo blieb dein Gl\u00fcck?", "tokens": ["Re\u00b7gnar", ",", "wo", "blieb", "dein", "Gl\u00fcck", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O K\u00f6nig Regnar, Vieledler,", "tokens": ["O", "K\u00f6\u00b7nig", "Re\u00b7gnar", ",", "Vie\u00b7led\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es ging dir diesmal schief.", "tokens": ["Es", "ging", "dir", "dies\u00b7mal", "schief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die packten spitz und tief.", "tokens": ["Die", "pack\u00b7ten", "spitz", "und", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der K\u00f6nig am Marterpfahle", "tokens": ["Der", "K\u00f6\u00b7nig", "am", "Mar\u00b7ter\u00b7pfah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schrie laut in Schmerz und Ha\u00df:", "tokens": ["Schrie", "laut", "in", "Schmerz", "und", "Ha\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr Fortignang ward bla\u00df.", "tokens": ["Herr", "For\u00b7tig\u00b7nang", "ward", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Ferkel kamen geschwommen,", "tokens": ["Die", "Fer\u00b7kel", "ka\u00b7men", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie h\u00f6rten des Keilers Geschrei.", "tokens": ["Sie", "h\u00f6r\u00b7ten", "des", "Kei\u00b7lers", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Herrn Fortignang entzwei.", "tokens": ["Herrn", "For\u00b7tig\u00b7nang", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}