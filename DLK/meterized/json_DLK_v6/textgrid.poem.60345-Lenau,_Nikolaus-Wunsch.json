{"textgrid.poem.60345": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort m\u00f6cht ich reisen", "tokens": ["Fort", "m\u00f6cht", "ich", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Weit, weit in die See,", "tokens": ["Weit", ",", "weit", "in", "die", "See", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O meine Geliebte,", "tokens": ["O", "mei\u00b7ne", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit dir allein!", "tokens": ["Mit", "dir", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Dr\u00e4nger und Lauscher", "tokens": ["Die", "Dr\u00e4n\u00b7ger", "und", "Lau\u00b7scher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und kalten St\u00f6rer,", "tokens": ["Und", "kal\u00b7ten", "St\u00f6\u00b7rer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sie hielt' uns ferne", "tokens": ["Sie", "hielt'", "uns", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der wallende Abgrund,", "tokens": ["Der", "wal\u00b7len\u00b7de", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das drohende Meer,", "tokens": ["Das", "dro\u00b7hen\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wir w\u00e4ren so sicher", "tokens": ["Wir", "w\u00e4\u00b7ren", "so", "si\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und selig allein.", "tokens": ["Und", "se\u00b7lig", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und k\u00e4me der Sturm,", "tokens": ["Und", "k\u00e4\u00b7me", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Ich w\u00fcrde dich halten", "tokens": ["Ich", "w\u00fcr\u00b7de", "dich", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "An meiner Brust.", "tokens": ["An", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wenn donnernde Wogen", "tokens": ["Wenn", "don\u00b7nern\u00b7de", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Zum Himmel schl\u00fcgen,", "tokens": ["Zum", "Him\u00b7mel", "schl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Doch h\u00f6her schl\u00fcge", "tokens": ["Doch", "h\u00f6\u00b7her", "schl\u00fc\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Mein trunkenes Herz;", "tokens": ["Mein", "trun\u00b7ke\u00b7nes", "Herz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Und meine Liebe,", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Die ewige, starke,", "tokens": ["Die", "e\u00b7wi\u00b7ge", ",", "star\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Sie w\u00fcrde frohlockend", "tokens": ["Sie", "w\u00fcr\u00b7de", "froh\u00b7lo\u00b7ckend"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Dich halten im Sturm.", "tokens": ["Dich", "hal\u00b7ten", "im", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Du w\u00fcrdest zitternd", "tokens": ["Du", "w\u00fcr\u00b7dest", "zit\u00b7ternd"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Mir blicken ins Auge", "tokens": ["Mir", "bli\u00b7cken", "ins", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Und w\u00fcrdest erblicken,", "tokens": ["Und", "w\u00fcr\u00b7dest", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Was nimmer scheitert", "tokens": ["Was", "nim\u00b7mer", "schei\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "In allen St\u00fcrmen,", "tokens": ["In", "al\u00b7len", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Und w\u00fcrdest l\u00e4cheln", "tokens": ["Und", "w\u00fcr\u00b7dest", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Und nicht mehr zittern.", "tokens": ["Und", "nicht", "mehr", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Sieh, nun erm\u00fcdet", "tokens": ["Sieh", ",", "nun", "er\u00b7m\u00fc\u00b7det"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Der tobende Aufruhr,", "tokens": ["Der", "to\u00b7ben\u00b7de", "Auf\u00b7ruhr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "In Schlummer sinken", "tokens": ["In", "Schlum\u00b7mer", "sin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Die Wellen und Winde,", "tokens": ["Die", "Wel\u00b7len", "und", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Und \u00fcber den Wassern", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Was\u00b7sern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Ist tiefe Stille.", "tokens": ["Ist", "tie\u00b7fe", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Da ruhst du sinnend", "tokens": ["Da", "ruhst", "du", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "An meiner Brust.", "tokens": ["An", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "So tiefe Stille:", "tokens": ["So", "tie\u00b7fe", "Stil\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Mein lauschendes Herz", "tokens": ["Mein", "lau\u00b7schen\u00b7des", "Herz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "H\u00f6rt Antwort pochen", "tokens": ["H\u00f6rt", "Ant\u00b7wort", "po\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Dein lauschendes Herz.", "tokens": ["Dein", "lau\u00b7schen\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.38": {"text": "Wir sind allein,", "tokens": ["Wir", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Doch fl\u00fcsterst du leise,", "tokens": ["Doch", "fl\u00fcs\u00b7terst", "du", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Um nicht zu st\u00f6ren", "tokens": ["Um", "nicht", "zu", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "Das sinnende Meer.", "tokens": ["Das", "sin\u00b7nen\u00b7de", "Meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.42": {"text": "Nur sanft erzittern", "tokens": ["Nur", "sanft", "er\u00b7zit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Die Lippen dir,", "tokens": ["Die", "Lip\u00b7pen", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.44": {"text": "Die schwellenden Bl\u00e4tter", "tokens": ["Die", "schwel\u00b7len\u00b7den", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Der s\u00fc\u00dfen Rose,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Ich sauge dein Wort,", "tokens": ["Ich", "sau\u00b7ge", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.47": {"text": "Den klingenden Duft", "tokens": ["Den", "klin\u00b7gen\u00b7den", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.48": {"text": "Der s\u00fc\u00dfen Rose.", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.49": {"text": "Im Osten hebt sich", "tokens": ["Im", "Os\u00b7ten", "hebt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.50": {"text": "Der klare Mond,", "tokens": ["Der", "kla\u00b7re", "Mond", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.51": {"text": "Und Gott bedecket", "tokens": ["Und", "Gott", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.52": {"text": "Den Himmel mit Sternen,", "tokens": ["Den", "Him\u00b7mel", "mit", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.53": {"text": "Und ich bedecke,", "tokens": ["Und", "ich", "be\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.54": {"text": "Selig wie er,", "tokens": ["Se\u00b7lig", "wie", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.55": {"text": "Dein liebes Antlitz,", "tokens": ["Dein", "lie\u00b7bes", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.56": {"text": "Den sch\u00f6nern Himmel,", "tokens": ["Den", "sch\u00f6\u00b7nern", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.57": {"text": "Mit feurigen K\u00fcssen.", "tokens": ["Mit", "feu\u00b7ri\u00b7gen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}