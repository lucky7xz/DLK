{"textgrid.poem.60390": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Mein T\u00fcrkenkopf", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Pfeifchen traut, mir ist dein Rauch;", "tokens": ["Mein", "Pfei\u00b7fchen", "traut", ",", "mir", "ist", "dein", "Rauch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll duftender Narkose,", "tokens": ["Voll", "duf\u00b7ten\u00b7der", "Nar\u00b7ko\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch lieber als der s\u00fc\u00dfe Hauch", "tokens": ["Noch", "lie\u00b7ber", "als", "der", "s\u00fc\u00b7\u00dfe", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der aufgebl\u00fchten Rose.", "tokens": ["Der", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4lt die Rose Streit mit dir,", "tokens": ["Und", "h\u00e4lt", "die", "Ro\u00b7se", "Streit", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von beiden sch\u00f6ner welche?", "tokens": ["Von", "bei\u00b7den", "sch\u00f6\u00b7ner", "wel\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "PWAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bist du die sch\u00f6nre Rose mir", "tokens": ["Bist", "du", "die", "sch\u00f6n\u00b7re", "Ro\u00b7se", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem Glutenkelche.", "tokens": ["Mit", "dei\u00b7nem", "Glu\u00b7ten\u00b7kel\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn wie die Rose duftend bl\u00fcht", "tokens": ["Denn", "wie", "die", "Ro\u00b7se", "duf\u00b7tend", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Gr\u00fcn der Fr\u00fchlingsb\u00e4ume,", "tokens": ["Im", "Gr\u00fcn", "der", "Fr\u00fch\u00b7lings\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Also mein Pfeifchen duftend gl\u00fcht", "tokens": ["Al\u00b7so", "mein", "Pfei\u00b7fchen", "duf\u00b7tend", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zum Fr\u00fchling meiner Tr\u00e4ume.", "tokens": ["Zum", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Weckt mir der Rose Freudenstrahl", "tokens": ["Weckt", "mir", "der", "Ro\u00b7se", "Freu\u00b7den\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schmerzlich Angedenken,", "tokens": ["Ein", "schmerz\u00b7lich", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hilfst du zu kurzer Rast einmal,", "tokens": ["Hilfst", "du", "zu", "kur\u00b7zer", "Rast", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich verlor, \u2013 versenken.", "tokens": ["Was", "ich", "ver\u00b7lor", ",", "\u2013", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn dein blauer Wolkenzug", "tokens": ["Und", "wenn", "dein", "blau\u00b7er", "Wol\u00b7ken\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stirne mir umsponnen,", "tokens": ["Die", "Stir\u00b7ne", "mir", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umkreist mich gern der rasche Flug", "tokens": ["Um\u00b7kreist", "mich", "gern", "der", "ra\u00b7sche", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von dichterischen Wonnen.", "tokens": ["Von", "dich\u00b7te\u00b7ri\u00b7schen", "Won\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn dann die Qual versank in Ruh,", "tokens": ["Wenn", "dann", "die", "Qual", "ver\u00b7sank", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So d\u00fcnket mich, mir wehte", "tokens": ["So", "d\u00fcn\u00b7ket", "mich", ",", "mir", "weh\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein heilend L\u00fcftchen Nebel zu", "tokens": ["Ein", "hei\u00b7lend", "L\u00fcft\u00b7chen", "Ne\u00b7bel", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom stillen Tal des Lethe.", "tokens": ["Vom", "stil\u00b7len", "Tal", "des", "Le\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum, Pfeifchen traut, ist mir dein Rauch,", "tokens": ["Drum", ",", "Pfei\u00b7fchen", "traut", ",", "ist", "mir", "dein", "Rauch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll durtender Narkose,", "tokens": ["Voll", "dur\u00b7ten\u00b7der", "Nar\u00b7ko\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch lieber als der s\u00fc\u00dfe Hauch", "tokens": ["Noch", "lie\u00b7ber", "als", "der", "s\u00fc\u00b7\u00dfe", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der aufgebl\u00fchten Rose!", "tokens": ["Der", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}