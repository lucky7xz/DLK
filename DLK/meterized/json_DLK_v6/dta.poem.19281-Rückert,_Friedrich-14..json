{"dta.poem.19281": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Zeit und Raum ist viel zu h\u00f6ren und zu lesen,", "tokens": ["Von", "Zeit", "und", "Raum", "ist", "viel", "zu", "h\u00f6\u00b7ren", "und", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als seien beide gleich, und stets zugleich gewesen.", "tokens": ["Als", "sei\u00b7en", "bei\u00b7de", "gleich", ",", "und", "stets", "zu\u00b7gleich", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "ADV", "$,", "KON", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch eher ist die Zeit gewesen als der Raum,", "tokens": ["Doch", "e\u00b7her", "ist", "die", "Zeit", "ge\u00b7we\u00b7sen", "als", "der", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VAPP", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Wachsthum eher war als der gewachsne Baum.", "tokens": ["Wie", "Wach\u00b7sthum", "e\u00b7her", "war", "als", "der", "ge\u00b7wachs\u00b7ne", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entstanden war die Zeit sobald als Geister dachten,", "tokens": ["Ent\u00b7stan\u00b7den", "war", "die", "Zeit", "so\u00b7bald", "als", "Geis\u00b7ter", "dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Raum erst als sich breit darinnen K\u00f6rper machten.", "tokens": ["Der", "Raum", "erst", "als", "sich", "breit", "da\u00b7rin\u00b7nen", "K\u00f6r\u00b7per", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "PRF", "ADJD", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und mit den K\u00f6rpern wird der Raum zusammenfallen,", "tokens": ["Und", "mit", "den", "K\u00f6r\u00b7pern", "wird", "der", "Raum", "zu\u00b7sam\u00b7men\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mit den Geistern erst die Zeit in Gott entwallen.", "tokens": ["Doch", "mit", "den", "Geis\u00b7tern", "erst", "die", "Zeit", "in", "Gott", "ent\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}