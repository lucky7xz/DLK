{"textgrid.poem.39603": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An R\u00fcckert", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aller morgenl\u00e4nd'schen Z\u00e4une K\u00f6nig,", "tokens": ["Al\u00b7ler", "mor\u00b7gen\u00b7l\u00e4n\u00b7d'\u00b7schen", "Z\u00e4u\u00b7ne", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wechselsweise zeisigkranicht\u00f6nig!", "tokens": ["Wech\u00b7sels\u00b7wei\u00b7se", "zei\u00b7sig\u00b7kra\u00b7nich\u00b7t\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df' im Jahrbuch hegelscher Kritiken", "tokens": ["La\u00df'", "im", "Jahr\u00b7buch", "he\u00b7gel\u00b7scher", "Kri\u00b7ti\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "H\u00f6ren deine Zaungeb\u00fcschmusiken!", "tokens": ["H\u00f6\u00b7ren", "dei\u00b7ne", "Zaun\u00b7ge\u00b7b\u00fcschmu\u00b7si\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Will Pisatschen Tadel dich pisacken,", "tokens": ["Will", "Pi\u00b7sat\u00b7schen", "Ta\u00b7del", "dich", "pi\u00b7sa\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hegel wird mit Rackschas-Rach' ihn packen.", "tokens": ["He\u00b7gel", "wird", "mit", "Rack\u00b7schas\u00b7Rach'", "ihn", "pa\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Lauschen soll mein Ohr dir unverr\u00fccklich,", "tokens": ["Lau\u00b7schen", "soll", "mein", "Ohr", "dir", "un\u00b7ver\u00b7r\u00fcck\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "R\u00fcckert, denn du singst ber\u00fcck-entz\u00fccklich.", "tokens": ["R\u00fc\u00b7ckert", ",", "denn", "du", "singst", "be\u00b7r\u00fc\u00b7ck\u00b7ent\u00b7z\u00fc\u00b7ck\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Dir geb\u00fchrt ein edler Palmen-Sch\u00f6\u00dfling,", "tokens": ["Dir", "ge\u00b7b\u00fchrt", "ein", "ed\u00b7ler", "Pal\u00b7men\u00b7Sch\u00f6\u00df\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Apsarasen- und Gandharven-Spr\u00f6\u00dfling!", "tokens": ["A\u00b7psa\u00b7ra\u00b7sen", "und", "Gand\u00b7ha\u00b7rven\u00b7Spr\u00f6\u00df\u00b7ling", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}