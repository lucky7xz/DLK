{"dta.poem.4266": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "T\u00eate \u00e0 t\u00eate.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ist eine kleine Laube, die wir ", "tokens": ["Noch", "ist", "ei\u00b7ne", "klei\u00b7ne", "Lau\u00b7be", ",", "die", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Weil sich ihrer zween darinn f\u00fcglich unterhalten k\u00f6nnen.", "tokens": ["Weil", "sich", "ih\u00b7rer", "zween", "da\u00b7rinn", "f\u00fcg\u00b7lich", "un\u00b7ter\u00b7hal\u00b7ten", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "VVFIN", "PAV", "ADJD", "VVINF", "VMINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Diese, worinn keine Sonne, wenn es noch so schw\u00fchl ist,", "tokens": ["Die\u00b7se", ",", "wo\u00b7rinn", "kei\u00b7ne", "Son\u00b7ne", ",", "wenn", "es", "noch", "so", "schw\u00fchl", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "dringt,", "tokens": ["dringt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ist von sieben jungen Eichen von Natur fast ganz umringt,", "tokens": ["Ist", "von", "sie\u00b7ben", "jun\u00b7gen", "Ei\u00b7chen", "von", "Na\u00b7tur", "fast", "ganz", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "CARD", "ADJA", "NN", "APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Die die kleine Rasen-Bank, mit nicht mindrer Anmuht,", "tokens": ["Die", "die", "klei\u00b7ne", "Ra\u00b7sen\u00b7Bank", ",", "mit", "nicht", "mind\u00b7rer", "An\u00b7muht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "decken.", "tokens": ["de\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Sch\u00f6nes Caprifolium scheint auch hier sich zu bem\u00fch\u2019n,", "tokens": ["Sch\u00f6\u00b7nes", "Cap\u00b7ri\u00b7fo\u00b7li\u00b7um", "scheint", "auch", "hier", "sich", "zu", "be\u00b7m\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ADV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Wenn, auf seinen schlanken Zweigen, die gef\u00e4rbte Bluhmen", "tokens": ["Wenn", ",", "auf", "sei\u00b7nen", "schlan\u00b7ken", "Zwei\u00b7gen", ",", "die", "ge\u00b7f\u00e4rb\u00b7te", "Bluh\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "bl\u00fch'n,", "tokens": ["bl\u00fch'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Uns, nicht nur im gr\u00fcnen Laub, auch in Bluhmen, zu", "tokens": ["Uns", ",", "nicht", "nur", "im", "gr\u00fc\u00b7nen", "Laub", ",", "auch", "in", "Bluh\u00b7men", ",", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "$,", "APPR"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "verstecken,", "tokens": ["ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Da denn, wenn der s\u00fcsse Duft aus der s\u00fcssen Bl\u00fchte", "tokens": ["Da", "denn", ",", "wenn", "der", "s\u00fcs\u00b7se", "Duft", "aus", "der", "s\u00fcs\u00b7sen", "Bl\u00fch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "quillt,", "tokens": ["quillt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Sich im lieblichen Geruch unsre Nas\u2019 und Herze f\u00fcllt,", "tokens": ["Sich", "im", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", "uns\u00b7re", "Nas'", "und", "Her\u00b7ze", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "So, da\u00df hier, indem es k\u00fchl,", "tokens": ["So", ",", "da\u00df", "hier", ",", "in\u00b7dem", "es", "k\u00fchl", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Nebst den ", "tokens": ["Nebst", "den"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Gef\u00fchl,", "tokens": ["Ge\u00b7f\u00fchl", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Sich zu gleicher Zeit vergn\u00fcget.", "tokens": ["Sich", "zu", "glei\u00b7cher", "Zeit", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Der verliebten V\u00f6gel Ch\u00f6re", "tokens": ["Der", "ver\u00b7lieb\u00b7ten", "V\u00f6\u00b7gel", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Reizen, mit den hellen Stimmen, oft zugleich auch das", "tokens": ["Rei\u00b7zen", ",", "mit", "den", "hel\u00b7len", "Stim\u00b7men", ",", "oft", "zu\u00b7gleich", "auch", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "Geh\u00f6re;", "tokens": ["Ge\u00b7h\u00f6\u00b7re", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Ja, hier kann uns auch das ", "tokens": ["Ja", ",", "hier", "kann", "uns", "auch", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VMFIN", "PPER", "ADV", "ART"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Eine s\u00fcsse Lust erwecken,", "tokens": ["Ei\u00b7ne", "s\u00fcs\u00b7se", "Lust", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wenn die nah\u2019 gelegne Quell\u2019 uns ihr s\u00fcsses Wasser", "tokens": ["Wenn", "die", "nah'", "ge\u00b7leg\u00b7ne", "Quell'", "uns", "ihr", "s\u00fcs\u00b7ses", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "schenkt,", "tokens": ["schenkt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Und man mit den reinen Tropfen, die so k\u00fchl, wenns", "tokens": ["Und", "man", "mit", "den", "rei\u00b7nen", "Trop\u00b7fen", ",", "die", "so", "k\u00fchl", ",", "wenns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIS", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "KOUS"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "warm, sich tr\u00e4nkt.", "tokens": ["warm", ",", "sich", "tr\u00e4nkt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRF", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Wenn wir hier, in stiller Ruh\u2019, so verschiedner Lust genies-", "tokens": ["Wenn", "wir", "hier", ",", "in", "stil\u00b7ler", "Ruh'", ",", "so", "ver\u00b7schied\u00b7ner", "Lust", "ge\u00b7ni\u00b7es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "TRUNC"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "sen,", "tokens": ["sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Sollt\u2019 ein billiges GOtt Lob! sich aus unsrer Brust", "tokens": ["Sollt'", "ein", "bil\u00b7li\u00b7ges", "Gott", "Lob", "!", "sich", "aus", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "NN", "$.", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+--++-++-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "ergiessen,", "tokens": ["er\u00b7gies\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Und man sollte GOtt verehren, Der uns hier in diesem", "tokens": ["Und", "man", "soll\u00b7te", "Gott", "ver\u00b7eh\u00b7ren", ",", "Der", "uns", "hier", "in", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "NN", "VVFIN", "$,", "ART", "PPER", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.34": {"text": "Leben,", "tokens": ["Le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Auf so mannigfache Weise, so verschiedne Lust gegeben.", "tokens": ["Auf", "so", "man\u00b7nig\u00b7fa\u00b7che", "Wei\u00b7se", ",", "so", "ver\u00b7schied\u00b7ne", "Lust", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}