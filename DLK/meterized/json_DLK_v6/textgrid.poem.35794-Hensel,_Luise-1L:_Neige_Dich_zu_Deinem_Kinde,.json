{"textgrid.poem.35794": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Neige Dich zu Deinem Kinde,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neige Dich zu Deinem Kinde,", "tokens": ["Nei\u00b7ge", "Dich", "zu", "Dei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh das Elend, sieh die S\u00fcnde;", "tokens": ["Sieh", "das", "E\u00b7lend", ",", "sieh", "die", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannst Du es so irren sehn?", "tokens": ["Kannst", "Du", "es", "so", "ir\u00b7ren", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr, Du hast ja einst geschworen:", "tokens": ["Herr", ",", "Du", "hast", "ja", "einst", "ge\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbjedes Sch\u00e4flein, das verloren,", "tokens": ["\u00bb", "je\u00b7des", "Sch\u00e4f\u00b7lein", ",", "das", "ver\u00b7lo\u00b7ren", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will Ich treulich suchen gehn.\u00ab", "tokens": ["Will", "Ich", "treu\u00b7lich", "su\u00b7chen", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, zu Dir nur will ich schreien,", "tokens": ["Ach", ",", "zu", "Dir", "nur", "will", "ich", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPER", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du nur kannst mir Kraft verleihen", "tokens": ["Du", "nur", "kannst", "mir", "Kraft", "ver\u00b7lei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den tr\u00fcben Augen Licht.", "tokens": ["Und", "den", "tr\u00fc\u00b7ben", "Au\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieb mir Thr\u00e4nen, gieb mir Bu\u00dfe,", "tokens": ["Gieb", "mir", "Thr\u00e4\u00b7nen", ",", "gieb", "mir", "Bu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, so fall' ich Dir zu Fu\u00dfe,", "tokens": ["Herr", ",", "so", "fall'", "ich", "Dir", "zu", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die S\u00fcndenfessel bricht.", "tokens": ["Und", "die", "S\u00fcn\u00b7den\u00b7fes\u00b7sel", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ohne Dich will ich nicht leben,", "tokens": ["Oh\u00b7ne", "Dich", "will", "ich", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zu Dir kann ich nicht schweben,", "tokens": ["Und", "zu", "Dir", "kann", "ich", "nicht", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein Haupt dr\u00fcckt schwere Schuld. \u2013", "tokens": ["Denn", "mein", "Haupt", "dr\u00fcckt", "schwe\u00b7re", "Schuld", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu Ihm hin mit offnen Armen!", "tokens": ["Zu", "Ihm", "hin", "mit", "off\u00b7nen", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unbegr\u00e4nzt ist Sein Erbarmen,", "tokens": ["Un\u00b7be\u00b7gr\u00e4nzt", "ist", "Sein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unerme\u00dflich Seine Huld.", "tokens": ["Un\u00b7er\u00b7me\u00df\u00b7lich", "Sei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine Seele sei verloren:", "tokens": ["Kei\u00b7ne", "See\u00b7le", "sei", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen ist der Herr geboren,", "tokens": ["Al\u00b7len", "ist", "der", "Herr", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat getragen Spott und Schmach;", "tokens": ["Hat", "ge\u00b7tra\u00b7gen", "Spott", "und", "Schmach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist f\u00fcr Alle hingegangen,", "tokens": ["Ist", "f\u00fcr", "Al\u00b7le", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat am blut'gen Kreuz gehangen", "tokens": ["Hat", "am", "blut'\u00b7gen", "Kreuz", "ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausgestreckt den ganzen Tag.", "tokens": ["Aus\u00b7ge\u00b7streckt", "den", "gan\u00b7zen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist zur H\u00f6ll' hinabgestiegen,", "tokens": ["Ist", "zur", "H\u00f6ll'", "hin\u00b7ab\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "That f\u00fcr uns im Grabe liegen; \u2013", "tokens": ["That", "f\u00fcr", "uns", "im", "Gra\u00b7be", "lie\u00b7gen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O, wo ist dein Stachel, Tod?", "tokens": ["O", ",", "wo", "ist", "dein", "Sta\u00b7chel", ",", "Tod", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auferstanden, aufgeschwebet", "tokens": ["Auf\u00b7er\u00b7stan\u00b7den", ",", "auf\u00b7ge\u00b7schwe\u00b7bet"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist Er auch f\u00fcr uns, und lebet", "tokens": ["Ist", "Er", "auch", "f\u00fcr", "uns", ",", "und", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vertritt uns nun bei Gott.", "tokens": ["Und", "ver\u00b7tritt", "uns", "nun", "bei", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Neige Dich zu Deinen Kindern,", "tokens": ["Nei\u00b7ge", "Dich", "zu", "Dei\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hilf, o Herr, uns armen S\u00fcndern,", "tokens": ["Hilf", ",", "o", "Herr", ",", "uns", "ar\u00b7men", "S\u00fcn\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rette, was zu retten ist!", "tokens": ["Ret\u00b7te", ",", "was", "zu", "ret\u00b7ten", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, erh\u00f6re unser Sehnen,", "tokens": ["Ach", ",", "er\u00b7h\u00f6\u00b7re", "un\u00b7ser", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh das Ringen, sieh die Thr\u00e4nen,", "tokens": ["Sieh", "das", "Rin\u00b7gen", ",", "sieh", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott, der Du die Liebe bist! Amen.", "tokens": ["Gott", ",", "der", "Du", "die", "Lie\u00b7be", "bist", "!", "A\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "$.", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}