{"dta.poem.7573": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fc\u00dfte nicht, wem ich noch Blumen sollte bringen,", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "nicht", ",", "wem", "ich", "noch", "Blu\u00b7men", "soll\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "D\u00fcrft' ich sie nicht ums Grab geliebter Kinder schlingen.", "tokens": ["D\u00fcrft'", "ich", "sie", "nicht", "ums", "Grab", "ge\u00b7lieb\u00b7ter", "Kin\u00b7der", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Mutter wird schon ernst, die Br\u00fcder werden gro\u00df,", "tokens": ["Die", "Mut\u00b7ter", "wird", "schon", "ernst", ",", "die", "Br\u00fc\u00b7der", "wer\u00b7den", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unver\u00e4nderlich bleibt ihr nur Kinder blo\u00df.", "tokens": ["Und", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "bleibt", "ihr", "nur", "Kin\u00b7der", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr nehmt an jedem Tag mit immer gleicher Liebe", "tokens": ["Ihr", "nehmt", "an", "je\u00b7dem", "Tag", "mit", "im\u00b7mer", "glei\u00b7cher", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die euch von Vaterhand gebrachten Fr\u00fchlingstriebe.", "tokens": ["Die", "euch", "von", "Va\u00b7ter\u00b7hand", "ge\u00b7brach\u00b7ten", "Fr\u00fch\u00b7ling\u00b7strie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}