{"dta.poem.701": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar! ", "tokens": ["Zwar", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da\u00df man/ vn\u00df/ die wir sein\u2019 vn\u00df die er au\u00dferwehlet/", "tokens": ["Da\u00df", "man", "/", "vn\u00df", "/", "die", "wir", "sein'", "vn\u00df", "die", "er", "au\u00b7\u00dfer\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "NE", "$(", "PRELS", "PPER", "PPOSAT", "APPR", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die er f\u00fcr eigen sch\u00e4tzt vnd derer Haar' er zehlet ", "tokens": ["Die", "er", "f\u00fcr", "ei\u00b7gen", "sch\u00e4tzt", "vnd", "de\u00b7rer", "Haa\u00b7r'", "er", "zeh\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJD", "VVFIN", "KON", "PDS", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+---+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In dem ", "tokens": ["In", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Man hat was noch vielmehr die Fessel auff sein wortt", "tokens": ["Man", "hat", "was", "noch", "viel\u00b7mehr", "die", "Fes\u00b7sel", "auff", "sein", "wortt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PWS", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Vmb vnsern ", "tokens": ["Vmb", "vn\u00b7sern"], "token_info": ["word", "word"], "pos": ["KOUI", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Zu vnserm Kampffplatz au\u00dferkiest.", "tokens": ["Zu", "vn\u00b7serm", "Kampff\u00b7platz", "au\u00b7\u00dfer\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er hat was f\u00fcr vnd vmb vn\u00df ist/ ", "tokens": ["Er", "hat", "was", "f\u00fcr", "vnd", "vmb", "vn\u00df", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "KON", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit Waffen wider vns gest\u00e4rckt/", "tokens": ["Mit", "Waf\u00b7fen", "wi\u00b7der", "vns", "ge\u00b7st\u00e4rckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd wie ein J\u00e4ger/ scharff bemerckt;", "tokens": ["Vnd", "wie", "ein", "J\u00e4\u00b7ger", "/", "scharff", "be\u00b7merckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Damit wir ja im Garne blieben. ", "tokens": ["Da\u00b7mit", "wir", "ja", "im", "Gar\u00b7ne", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er spreche nur ein wortt so wird der ", "tokens": ["Er", 