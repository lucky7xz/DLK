{"textgrid.poem.49430": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fernher sucht des Windes Fl\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fernher sucht des Windes Fl\u00fcgel", "tokens": ["Fern\u00b7her", "sucht", "des", "Win\u00b7des", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Wetterfahnen Streit,", "tokens": ["Mit", "den", "Wet\u00b7ter\u00b7fah\u00b7nen", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Sch\u00f6ffen Schloss, wo weit", "tokens": ["Auf", "des", "Sch\u00f6f\u00b7fen", "Schloss", ",", "wo", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schiefer gl\u00e4nzt und rote Ziegel", "tokens": ["Schie\u00b7fer", "gl\u00e4nzt", "und", "ro\u00b7te", "Zie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf der Wiesen hell Gebreit.", "tokens": ["Auf", "der", "Wie\u00b7sen", "hell", "Ge\u00b7breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eschen, wie im M\u00e4rchen, ziehen", "tokens": ["E\u00b7schen", ",", "wie", "im", "M\u00e4r\u00b7chen", ",", "zie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend Wellen rings durch das", "tokens": ["Tau\u00b7send", "Wel\u00b7len", "rings", "durch", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "APPR", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weite Land, so zart und blass.", "tokens": ["Wei\u00b7te", "Land", ",", "so", "zart", "und", "blass", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Sahara der Pr\u00e4rien", "tokens": ["Die", "Sa\u00b7ha\u00b7ra", "der", "Pr\u00e4\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Prangt mit Klee und weissem Gras.", "tokens": ["Prangt", "mit", "Klee", "und", "weis\u00b7sem", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Waggons ziehn leise ihre", "tokens": ["Die", "Wag\u00b7gons", "ziehn", "lei\u00b7se", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Bahn durchs Land, das friedlich ruht.", "tokens": ["Bahn", "durchs", "Land", ",", "das", "fried\u00b7lich", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlaft ihr K\u00fche, schlummert gut", "tokens": ["Schlaft", "ihr", "K\u00fc\u00b7he", ",", "schlum\u00b7mert", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Ebne, sanfte Stiere,", "tokens": ["In", "der", "Eb\u00b7ne", ",", "sanf\u00b7te", "Stie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit des Blicks ged\u00e4mpfter Glut.", "tokens": ["Mit", "des", "Blicks", "ge\u00b7d\u00e4mpf\u00b7ter", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lautlos sanft dahingetrieben", "tokens": ["Laut\u00b7los", "sanft", "da\u00b7hin\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird ein jeglicher Waggon", "tokens": ["Wird", "ein", "jeg\u00b7li\u00b7cher", "Wag\u00b7gon"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sacht ein plaudernder Salon,", "tokens": ["Sacht", "ein", "plau\u00b7dern\u00b7der", "Sa\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die sch\u00f6ne Flur wir lieben,", "tokens": ["Wo", "die", "sch\u00f6\u00b7ne", "Flur", "wir", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie geschm\u00fcckt f\u00fcr F\u00e9nelon.", "tokens": ["Wie", "ge\u00b7schm\u00fcckt", "f\u00fcr", "F\u00e9\u00b7ne\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}