{"textgrid.poem.42058": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Heilige", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Schritte schreiten mir entwandt,", "tokens": ["Mei\u00b7ne", "Schrit\u00b7te", "schrei\u00b7ten", "mir", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Finger gehen nach der Frau,", "tokens": ["Mei\u00b7ne", "Fin\u00b7ger", "ge\u00b7hen", "nach", "der", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch im Muttergottesmantel stirbt die Hand.", "tokens": ["Doch", "im", "Mut\u00b7ter\u00b7got\u00b7tes\u00b7man\u00b7tel", "stirbt", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Bl\u00e4ue meines Blicks lockt Meer ins Blau,", "tokens": ["Bl\u00e4u\u00b7e", "mei\u00b7nes", "Blicks", "lockt", "Meer", "ins", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Das in Sturm dem Feind Zerst\u00f6rung sann,", "tokens": ["Das", "in", "Sturm", "dem", "Feind", "Zer\u00b7st\u00f6\u00b7rung", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und das gelbe Haar schwebt segnend \u00fcberm Weizenfelde des Verfluchten.", "tokens": ["Und", "das", "gel\u00b7be", "Haar", "schwebt", "seg\u00b7nend", "\u00fc\u00b7berm", "Wei\u00b7zen\u00b7fel\u00b7de", "des", "Ver\u00b7fluch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Ach, da ich mit List die Netze spann", "tokens": ["Ach", ",", "da", "ich", "mit", "List", "die", "Net\u00b7ze", "spann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ihn zu stellen, kamen Engel, die mich suchten,", "tokens": ["Ihn", "zu", "stel\u00b7len", ",", "ka\u00b7men", "En\u00b7gel", ",", "die", "mich", "such\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Und sie gr\u00fc\u00dften mich: den Toderretter des Verha\u00dften, demuthaft.", "tokens": ["Und", "sie", "gr\u00fc\u00df\u00b7ten", "mich", ":", "den", "To\u00b7der\u00b7ret\u00b7ter", "des", "Ver\u00b7ha\u00df\u00b7ten", ",", "de\u00b7mut\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "ART", "NN", "ART", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.10": {"text": "Mord stand rot im Abend um mich her,", "tokens": ["Mord", "stand", "rot", "im", "A\u00b7bend", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Doch ich sank ins Aug des Rehs, mir ganz entrafft,", "tokens": ["Doch", "ich", "sank", "ins", "Aug", "des", "Rehs", ",", "mir", "ganz", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Und am Stamm des schmerzenlosen Leibes brach der Speer.", "tokens": ["Und", "am", "Stamm", "des", "schmer\u00b7zen\u00b7lo\u00b7sen", "Lei\u00b7bes", "brach", "der", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "So entflohn, \u2013 oh Tr\u00e4nen meiner Klage,", "tokens": ["So", "ent\u00b7flohn", ",", "\u2013", "oh", "Tr\u00e4\u00b7nen", "mei\u00b7ner", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "$(", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Milder S\u00fcnde sehnend zugesandt! \u2013", "tokens": ["Mil\u00b7der", "S\u00fcn\u00b7de", "seh\u00b7nend", "zu\u00b7ge\u00b7sandt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Schreiten meine Schritte mir entwandt:", "tokens": ["Schrei\u00b7ten", "mei\u00b7ne", "Schrit\u00b7te", "mir", "ent\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Steil die wei\u00dfe Lilie meiner Tage", "tokens": ["Steil", "die", "wei\u00b7\u00dfe", "Li\u00b7lie", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "H\u00e4lt der Gott in seiner harten Hand.", "tokens": ["H\u00e4lt", "der", "Gott", "in", "sei\u00b7ner", "har\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}