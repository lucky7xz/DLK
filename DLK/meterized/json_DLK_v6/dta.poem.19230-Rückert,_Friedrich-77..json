{"dta.poem.19230": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint alsob Natur bei jedes Thieres Bilden", "tokens": ["Es", "scheint", "al\u00b7sob", "Na\u00b7tur", "bei", "je\u00b7des", "Thie\u00b7res", "Bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Hauptsach' es gemacht allein vor allen Gilden,", "tokens": ["Zur", "Haupt\u00b7sach'", "es", "ge\u00b7macht", "al\u00b7lein", "vor", "al\u00b7len", "Gil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Absicht nur gehabt, es v\u00f6llig auszur\u00fcsten,", "tokens": ["Die", "Ab\u00b7sicht", "nur", "ge\u00b7habt", ",", "es", "v\u00f6l\u00b7lig", "aus\u00b7zu\u00b7r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAPP", "$,", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob nicht neben ihm bestehn mehr andre m\u00fcsten.", "tokens": ["Al\u00b7sob", "nicht", "ne\u00b7ben", "ihm", "be\u00b7stehn", "mehr", "and\u00b7re", "m\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "ADV", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Alswie ein Bildner wol in jedes seiner Werke,", "tokens": ["Als\u00b7wie", "ein", "Bild\u00b7ner", "wol", "in", "je\u00b7des", "sei\u00b7ner", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht denkend anderer, legt seine ganze St\u00e4rke.", "tokens": ["Nicht", "den\u00b7kend", "an\u00b7de\u00b7rer", ",", "legt", "sei\u00b7ne", "gan\u00b7ze", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJA", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie stellt dem Krokodil die Z\u00e4hne so im Rachen,", "tokens": ["Sie", "stellt", "dem", "Kro\u00b7ko\u00b7dil", "die", "Z\u00e4h\u00b7ne", "so", "im", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wollte sie zur Beut' ihm alles Leben machen.", "tokens": ["Als", "woll\u00b7te", "sie", "zur", "Beut'", "ihm", "al\u00b7les", "Le\u00b7ben", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPRART", "NN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die ehrnen Zacken stehn wie festgefugte Mauern,", "tokens": ["Die", "ehr\u00b7nen", "Za\u00b7cken", "stehn", "wie", "fest\u00b7ge\u00b7fug\u00b7te", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hinter jedem mu\u00df ein neuer Zahnkeim lauern.", "tokens": ["Doch", "hin\u00b7ter", "je\u00b7dem", "mu\u00df", "ein", "neu\u00b7er", "Zahn\u00b7keim", "lau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wie der erste bricht, so dringt der andre vor,", "tokens": ["Und", "wie", "der", "ers\u00b7te", "bricht", ",", "so", "dringt", "der", "and\u00b7re", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00e4rfer ist als er, und schlie\u00dft die L\u00fcck' im Thor.", "tokens": ["Der", "sch\u00e4r\u00b7fer", "ist", "als", "er", ",", "und", "schlie\u00dft", "die", "L\u00fcck", "im", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KOUS", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und unter diesem lauscht ein andrer noch verborgen,", "tokens": ["Und", "un\u00b7ter", "die\u00b7sem", "lauscht", "ein", "an\u00b7drer", "noch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein andrer unter dem, kein Mangel zu besorgen.", "tokens": ["Ein", "an\u00b7drer", "un\u00b7ter", "dem", ",", "kein", "Man\u00b7gel", "zu", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und so nach seiner Art der Tiger und der Hai,", "tokens": ["Und", "so", "nach", "sei\u00b7ner", "Art", "der", "Ti\u00b7ger", "und", "der", "Hai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ebenso bedacht viel andre mancherlei.", "tokens": ["Und", "e\u00b7ben\u00b7so", "be\u00b7dacht", "viel", "and\u00b7re", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Bei jedem \u00fcbet sie gleich unumschr\u00e4nkten Brauch,", "tokens": ["Bei", "je\u00b7dem", "\u00fc\u00b7bet", "sie", "gleich", "un\u00b7um\u00b7schr\u00e4nk\u00b7ten", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alle Sch\u00f6pfung sei nur Futter seinem Bauch.", "tokens": ["Da\u00df", "al\u00b7le", "Sch\u00f6p\u00b7fung", "sei", "nur", "Fut\u00b7ter", "sei\u00b7nem", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und h\u00e4lt sie dadurch nicht allein das Gleichgewicht,", "tokens": ["Und", "h\u00e4lt", "sie", "da\u00b7durch", "nicht", "al\u00b7lein", "das", "Gleich\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jede Spitze sich an einer andern bricht?", "tokens": ["Da\u00df", "je\u00b7de", "Spit\u00b7ze", "sich", "an", "ei\u00b7ner", "an\u00b7dern", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Sie schuf die einzelnen, als diene alles ihnen,", "tokens": ["Sie", "schuf", "die", "ein\u00b7zel\u00b7nen", ",", "als", "die\u00b7ne", "al\u00b7les", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "KOUS", "PDS", "PIS", "PPER", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da sie einander all', und all dem Ganzen dienen.", "tokens": ["Da", "sie", "ein\u00b7an\u00b7der", "all'", ",", "und", "all", "dem", "Gan\u00b7zen", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIS", "$,", "KON", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}