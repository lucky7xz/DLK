{"dta.poem.20816": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den geneigten  \n Leser.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Swer \u00f6ch Turniren minnet also sere/", "tokens": ["Swer", "\u00f6ch", "Tur\u00b7ni\u00b7ren", "min\u00b7net", "al\u00b7so", "se\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Daz er da bi vergisset der Husere:", "tokens": ["Daz", "er", "da", "bi", "ver\u00b7gis\u00b7set", "der", "Hu\u00b7se\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dern hat die Mazze niht behalten/", "tokens": ["Dern", "hat", "die", "Maz\u00b7ze", "niht", "be\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gen\u00fcg Turniren das ist gut;", "tokens": ["Ein", "ge\u00b7n\u00fcg", "Tur\u00b7ni\u00b7ren", "das", "ist", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ze vil an allen Dingen/ tut", "tokens": ["Ze", "vil", "an", "al\u00b7len", "Din\u00b7gen", "/", "tut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "APPR", "PIAT", "NN", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bruch an den Lobe: so sagen die wisen Alten.", "tokens": ["Bruch", "an", "den", "Lo\u00b7be", ":", "so", "sa\u00b7gen", "die", "wi\u00b7sen", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wer seinen gantzen Sinn auff das Turniren lenckt/", "tokens": ["Wer", "sei\u00b7nen", "gant\u00b7zen", "Sinn", "auff", "das", "Tur\u00b7ni\u00b7ren", "lenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an sein Haus und Weib dabey nicht mehr gedenckt/", "tokens": ["Und", "an", "sein", "Haus", "und", "Weib", "da\u00b7bey", "nicht", "mehr", "ge\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN", "PAV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der hat kein rechtes Ma\u00df gehalten/", "tokens": ["Der", "hat", "kein", "rech\u00b7tes", "Ma\u00df", "ge\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein m\u00e4ssiger Turnir ist l\u00f6blich und auch gut/", "tokens": ["Ein", "m\u00e4s\u00b7si\u00b7ger", "Tur\u00b7nir", "ist", "l\u00f6b\u00b7lich", "und", "auch", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch wer zu viel in allen Sachen thut/", "tokens": ["Doch", "wer", "zu", "viel", "in", "al\u00b7len", "Sa\u00b7chen", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKA", "PIS", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verlihrt den Ruhm/ nach laut der weisen Alteu.", "tokens": ["Ver\u00b7lihrt", "den", "Ruhm", "/", "nach", "laut", "der", "wei\u00b7sen", "Al\u00b7teu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}