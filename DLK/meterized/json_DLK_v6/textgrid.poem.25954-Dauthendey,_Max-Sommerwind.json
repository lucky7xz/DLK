{"textgrid.poem.25954": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Sommerwind", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sommerwind durch die Felder rennt,", "tokens": ["Som\u00b7mer\u00b7wind", "durch", "die", "Fel\u00b7der", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Heupferdchen springt, die Sonne brennt.", "tokens": ["Heu\u00b7pferd\u00b7chen", "springt", ",", "die", "Son\u00b7ne", "brennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mittag schlug's auf der Dorfkirch schon,", "tokens": ["Mit\u00b7tag", "schlug's", "auf", "der", "Dorf\u00b7kirch", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stunden Wege niemand kennt,", "tokens": ["Der", "Stun\u00b7den", "We\u00b7ge", "nie\u00b7mand", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz l\u00e4uft mit dem Wind davon.", "tokens": ["Das", "Herz", "l\u00e4uft", "mit", "dem", "Wind", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}