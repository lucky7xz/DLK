{"textgrid.poem.40913": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Vogel schwankt so tief und still,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel schwankt so tief und still,", "tokens": ["Der", "Vo\u00b7gel", "schwankt", "so", "tief", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er wei\u00df nit, woner ane will.", "tokens": ["er", "wei\u00df", "nit", ",", "wo\u00b7ner", "a\u00b7ne", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "NE", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es chunnt so schwarz, und chunnt so schwer,", "tokens": ["Es", "chunnt", "so", "schwarz", ",", "und", "chunnt", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und in de L\u00fcfte hangt e Meer", "tokens": ["und", "in", "de", "L\u00fcf\u00b7te", "hangt", "e", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "voll Dunst und Wetter. Los, wie's schallt", "tokens": ["voll", "Dunst", "und", "Wet\u00b7ter", ".", "Los", ",", "wie's", "schallt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN", "$.", "NE", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "am Blauen, und wie's widerhallt.", "tokens": ["am", "Blau\u00b7en", ",", "und", "wie's", "wi\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In gro\u00dfe Wirble fliegt der Staub", "tokens": ["In", "gro\u00b7\u00dfe", "Wirb\u00b7le", "fliegt", "der", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "zum Himmel uf, mit Halm und Laub,", "tokens": ["zum", "Him\u00b7mel", "uf", ",", "mit", "Halm", "und", "Laub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "und lueg mer d\u00f6rt sel W\u00fclkli a!", "tokens": ["und", "lu\u00b7eg", "mer", "d\u00f6rt", "sel", "W\u00fclk\u00b7li", "a", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "I ha ke gro\u00dfe Gfalle dra;", "tokens": ["I", "ha", "ke", "gro\u00b7\u00dfe", "Gfal\u00b7le", "dra", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "lueg, wie mers usenander rupft,", "tokens": ["lu\u00b7eg", ",", "wie", "mers", "u\u00b7sen\u00b7an\u00b7der", "rupft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "wie \u00fcser eis, wenn's Wulle zupft.", "tokens": ["wie", "\u00fc\u00b7ser", "eis", ",", "wenn's", "Wul\u00b7le", "zupft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKVZ", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Se helfis Gott, und bh\u00fcetis Gott!", "tokens": ["Se", "hel\u00b7fis", "Gott", ",", "und", "bh\u00fce\u00b7tis", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie zuckt's dur's Gw\u00fclch so f\u00fc\u00fcrig rot,", "tokens": ["Wie", "zuckt's", "dur's", "Gw\u00fclch", "so", "f\u00fc\u00fc\u00b7rig", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPRART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "und 's chracht und sto\u00dft, es isch e Gruus,", "tokens": ["und", "'s", "chracht", "und", "sto\u00dft", ",", "es", "isch", "e", "Gru\u00b7us", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "KON", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "a\u00df d'Fenster zitteren und 's Hus.", "tokens": ["a\u00df", "d'\u00b7Fens\u00b7ter", "zit\u00b7te\u00b7ren", "und", "'s", "Hus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "VVINF", "KON", "PPER", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Lueg 's B\u00fcbli in der Waglen a!", "tokens": ["Lu\u00b7eg", "'s", "B\u00fcb\u00b7li", "in", "der", "Wag\u00b7len", "a", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Es schloft, und nimmt si n\u00fct drum a.", "tokens": ["Es", "schloft", ",", "und", "nimmt", "si", "n\u00fct", "drum", "a."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sie l\u00fcte z'Schlienge druf und druf,", "tokens": ["Sie", "l\u00fc\u00b7te", "z'\u00b7Sch\u00b7li\u00b7en\u00b7ge", "druf", "und", "druf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PAV", "KON", "PAV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "je, und 's h\u00f6rt ebe doch nit uf.", "tokens": ["je", ",", "und", "'s", "h\u00f6rt", "e\u00b7be", "doch", "nit", "uf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Sel bruucht me gar, wenn's dundere soll,", "tokens": ["Sel", "bruucht", "me", "gar", ",", "wenn's", "dun\u00b7de\u00b7re", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "$,", "KOUS", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "und 's l\u00fctet eim no d'Ohre voll. \u2013", "tokens": ["und", "'s", "l\u00fc\u00b7tet", "eim", "no", "d'\u00b7Oh\u00b7re", "voll", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NE", "NE", "ADJD", "$.", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "O, helfis Gott! \u2013 Es isch e Schlag!", "tokens": ["O", ",", "hel\u00b7fis", "Gott", "!", "\u2013", "Es", "isch", "e", "Schlag", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "$(", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "D\u00f6rt siehsch im Baum am Gartehag?", "tokens": ["D\u00f6rt", "sieh\u00b7sch", "im", "Baum", "am", "Gar\u00b7te\u00b7hag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Lueg, 's B\u00fcbli schloft no alliwil", "tokens": ["Lu\u00b7eg", ",", "'s", "B\u00fcb\u00b7li", "schloft", "no", "al\u00b7li\u00b7wil"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "NN", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "und us dem Dundere macht's nit vil.", "tokens": ["und", "us", "dem", "Dun\u00b7de\u00b7re", "macht's", "nit", "vil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Es denkt: \u203aDas ficht mi wenig a,", "tokens": ["Es", "denkt", ":", "\u203a", "Das", "ficht", "mi", "we\u00b7nig", "a", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "NE", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "er wird jo d'Auge binem ha.\u2039", "tokens": ["er", "wird", "jo", "d'\u00b7Au\u00b7ge", "bi\u00b7nem", "ha", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Es schn\u00fcfelet, es dreiht si hott", "tokens": ["Es", "schn\u00fc\u00b7fe\u00b7let", ",", "es", "dreiht", "si", "hott"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "ufs ander \u00d6hrli. Gunn der's Gott!", "tokens": ["ufs", "an\u00b7der", "\u00d6hr\u00b7li", ".", "Gunn", "der's", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "O, siehsch die helle Streife d\u00f6rt?", "tokens": ["O", ",", "sieh\u00b7sch", "die", "hel\u00b7le", "Strei\u00b7fe", "d\u00f6rt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "O los! Hesch nit das Ra\u00dfle gh\u00f6rt?", "tokens": ["O", "los", "!", "Hesch", "nit", "das", "Ra\u00df\u00b7le", "gh\u00f6rt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NE", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Es chunnt. Gott wellis gn\u00e4dig si!", "tokens": ["Es", "chunnt", ".", "Gott", "wel\u00b7lis", "gn\u00e4\u00b7dig", "si", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "G\u00f6hnt weidli, h\u00e4nket d'L\u00e4den i!", "tokens": ["G\u00f6hnt", "weid\u00b7li", ",", "h\u00e4n\u00b7ket", "d'\u00b7L\u00e4\u00b7den", "i", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "VVFIN", "ART", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "'s isch wieder akurat wie fern.", "tokens": ["'s", "isch", "wie\u00b7der", "a\u00b7ku\u00b7rat", "wie", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVFIN", "KOKOM", "ADJD", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.36": {"text": "Gut Nacht, du sch\u00f6ni Weizenern.", "tokens": ["Gut", "Nacht", ",", "du", "sch\u00f6\u00b7ni", "Wei\u00b7ze\u00b7nern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.37": {"text": "Es schettert uffem Chilchedach;", "tokens": ["Es", "schet\u00b7tert", "uf\u00b7fem", "Chil\u00b7che\u00b7dach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "und vorem Hus, wie g\u00e4utscht's im Bach,", "tokens": ["und", "vo\u00b7rem", "Hus", ",", "wie", "g\u00e4utscht's", "im", "Bach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PWAV", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "und 's lo\u00dft nit no \u2013 da\u00df Gott erbarm!", "tokens": ["und", "'s", "lo\u00dft", "nit", "no", "\u2013", "da\u00df", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", 