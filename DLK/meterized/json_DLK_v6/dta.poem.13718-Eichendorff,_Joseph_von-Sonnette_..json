{"dta.poem.13718": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mag dann der Steuermann nach langem Irren,", "tokens": ["Mag", "dann", "der", "Steu\u00b7er\u00b7mann", "nach", "lan\u00b7gem", "Ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rasch ziehend alle Pfeile aus der Wunde,", "tokens": ["Rasch", "zie\u00b7hend", "al\u00b7le", "Pfei\u00b7le", "aus", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Todt an der Heimathk\u00fcste niedersinken!", "tokens": ["Todt", "an", "der", "Hei\u00b7math\u00b7k\u00fcs\u00b7te", "nie\u00b7der\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}