{"dta.poem.3350": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Liebling, unerquickt vom g\u00fcldnen Sonnenlichte", "tokens": ["Ihr", "Lieb\u00b7ling", ",", "un\u00b7er\u00b7quickt", "vom", "g\u00fcld\u00b7nen", "Son\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stund mit erstauntem Angesichte,", "tokens": ["Stund", "mit", "er\u00b7staun\u00b7tem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Hoheit eines Gotts aus vielen Z\u00fcgen sah,", "tokens": ["Dem", "Ho\u00b7heit", "ei\u00b7nes", "Gotts", "aus", "vie\u00b7len", "Z\u00fc\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Voll feuriger Entz\u00fcckung, da:", "tokens": ["Voll", "feu\u00b7ri\u00b7ger", "Ent\u00b7z\u00fc\u00b7ckung", ",", "da", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "KOUS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Engel, Teufel, Himmel, H\u00f6lle", "tokens": ["Und", "En\u00b7gel", ",", "Teu\u00b7fel", ",", "Him\u00b7mel", ",", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NE", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vermischten, unverwirrt, sich an dem Fu\u00dfgestelle.", "tokens": ["Ver\u00b7mischten", ",", "un\u00b7ver\u00b7wirrt", ",", "sich", "an", "dem", "Fu\u00df\u00b7ge\u00b7stel\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "F\u00fcr ihn, den Deutschland halb verg\u00f6ttert, halb ver-", "tokens": ["F\u00fcr", "ihn", ",", "den", "Deutschland", "halb", "ver\u00b7g\u00f6t\u00b7tert", ",", "halb", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "ADJD", "VVPP", "$,", "ADJD", "TRUNC"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "dammt,", "tokens": ["dammt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "F\u00fcr ihn und andre junge Britten,", "tokens": ["F\u00fcr", "ihn", "und", "and\u00b7re", "jun\u00b7ge", "Brit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Aus derer Augen selbst, wie oft aus ihren Sitten,", "tokens": ["Aus", "de\u00b7rer", "Au\u00b7gen", "selbst", ",", "wie", "oft", "aus", "ih\u00b7ren", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "ADV", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was k\u00fchnes und fast wildes flammt;", "tokens": ["Was", "k\u00fch\u00b7nes", "und", "fast", "wil\u00b7des", "flammt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "KON", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Steigt soviel Weihrauch auf aus hundert Opferschaalen,", "tokens": ["Steigt", "so\u00b7viel", "Weih\u00b7rauch", "auf", "aus", "hun\u00b7dert", "Op\u00b7fer\u00b7schaa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "APPR", "CARD", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Da\u00df dicker Wolken Dampf die alten Dichter deckt,", "tokens": ["Da\u00df", "di\u00b7cker", "Wol\u00b7ken", "Dampf", "die", "al\u00b7ten", "Dich\u00b7ter", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verdunkelt, aber nicht befleckt:", "tokens": ["Ver\u00b7dun\u00b7kelt", ",", "a\u00b7ber", "nicht", "be\u00b7fleckt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie werden ewig sch\u00f6n mit reinem Glanze strahlen.", "tokens": ["Sie", "wer\u00b7den", "e\u00b7wig", "sch\u00f6n", "mit", "rei\u00b7nem", "Glan\u00b7ze", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}