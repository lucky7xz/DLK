{"textgrid.poem.52791": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es donnert. Gott! Wie schrecklich ist", "tokens": ["Es", "don\u00b7nert", ".", "Gott", "!", "Wie", "schreck\u00b7lich", "ist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "PWAV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels Wetternacht.", "tokens": ["Des", "Him\u00b7mels", "Wet\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es blitzt und donnert, guter Christ,", "tokens": ["Es", "blitzt", "und", "don\u00b7nert", ",", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00f6r' doch, wie es kracht!", "tokens": ["So", "h\u00f6r'", "doch", ",", "wie", "es", "kracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gott reinigt seine Luft f\u00fcr uns,", "tokens": ["Gott", "rei\u00b7nigt", "sei\u00b7ne", "Luft", "f\u00fcr", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum f\u00fcrchtet euch und dankt", "tokens": ["Drum", "f\u00fcrch\u00b7tet", "euch", "und", "dankt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem weisen Sch\u00f6pfer, ob er uns", "tokens": ["Dem", "wei\u00b7sen", "Sch\u00f6p\u00b7fer", ",", "ob", "er", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch t\u00fcchtig heute zankt.", "tokens": ["Auch", "t\u00fcch\u00b7tig", "heu\u00b7te", "zankt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}