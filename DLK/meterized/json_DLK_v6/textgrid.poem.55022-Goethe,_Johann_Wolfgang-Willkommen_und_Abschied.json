{"textgrid.poem.55022": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Willkommen und Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schlug mein Herz, geschwind zu Pferde!", "tokens": ["Es", "schlug", "mein", "Herz", ",", "ge\u00b7schwind", "zu", "Pfer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es war getan fast eh gedacht;", "tokens": ["Es", "war", "ge\u00b7tan", "fast", "eh", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Abend wiegte schon die Erde,", "tokens": ["Der", "A\u00b7bend", "wieg\u00b7te", "schon", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und an den Bergen hing die Nacht:", "tokens": ["Und", "an", "den", "Ber\u00b7gen", "hing", "die", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon stand im Nebelkleid die Eiche,", "tokens": ["Schon", "stand", "im", "Ne\u00b7bel\u00b7kleid", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein aufget\u00fcrmter Riese, da,", "tokens": ["Ein", "auf\u00b7ge\u00b7t\u00fcrm\u00b7ter", "Rie\u00b7se", ",", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Finsternis aus dem Gestr\u00e4uche", "tokens": ["Wo", "Fins\u00b7ter\u00b7nis", "aus", "dem", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hundert schwarzen Augen sah.", "tokens": ["Mit", "hun\u00b7dert", "schwar\u00b7zen", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mond von einem Wolkenh\u00fcgel", "tokens": ["Der", "Mond", "von", "ei\u00b7nem", "Wol\u00b7ken\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah kl\u00e4glich aus dem Duft hervor,", "tokens": ["Sah", "kl\u00e4g\u00b7lich", "aus", "dem", "Duft", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Winde schwangen leise Fl\u00fcgel,", "tokens": ["Die", "Win\u00b7de", "schwan\u00b7gen", "lei\u00b7se", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umsausten schauerlich mein Ohr;", "tokens": ["Um\u00b7saus\u00b7ten", "schau\u00b7er\u00b7lich", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nacht schuf tausend Ungeheuer;", "tokens": ["Die", "Nacht", "schuf", "tau\u00b7send", "Un\u00b7ge\u00b7heu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch frisch und fr\u00f6hlich war mein Mut:", "tokens": ["Doch", "frisch", "und", "fr\u00f6h\u00b7lich", "war", "mein", "Mut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In meinen Adern welches Feuer!", "tokens": ["In", "mei\u00b7nen", "A\u00b7dern", "wel\u00b7ches", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In meinem Herzen welche Glut!", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "wel\u00b7che", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich sah ich, und die milde Freude", "tokens": ["Dich", "sah", "ich", ",", "und", "die", "mil\u00b7de", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flo\u00df von dem s\u00fc\u00dfen Blick auf mich;", "tokens": ["Flo\u00df", "von", "dem", "s\u00fc\u00b7\u00dfen", "Blick", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz war mein Herz an deiner Seite", "tokens": ["Ganz", "war", "mein", "Herz", "an", "dei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Atemzug f\u00fcr dich.", "tokens": ["Und", "je\u00b7der", "A\u00b7tem\u00b7zug", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein rosenfarbnes Fr\u00fchlingswetter", "tokens": ["Ein", "ro\u00b7sen\u00b7farb\u00b7nes", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umgab das liebliche Gesicht,", "tokens": ["Um\u00b7gab", "das", "lieb\u00b7li\u00b7che", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Z\u00e4rtlichkeit f\u00fcr mich \u2013 ihr G\u00f6tter!", "tokens": ["Und", "Z\u00e4rt\u00b7lich\u00b7keit", "f\u00fcr", "mich", "\u2013", "ihr", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hofft es, ich verdient es nicht!", "tokens": ["Ich", "hofft", "es", ",", "ich", "ver\u00b7dient", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ach, schon mit der Morgensonne", "tokens": ["Doch", "ach", ",", "schon", "mit", "der", "Mor\u00b7gen\u00b7son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verengt der Abschied mir das Herz:", "tokens": ["Ver\u00b7engt", "der", "Ab\u00b7schied", "mir", "das", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinen K\u00fcssen welche Wonne!", "tokens": ["In", "dei\u00b7nen", "K\u00fcs\u00b7sen", "wel\u00b7che", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Auge welcher Schmerz!", "tokens": ["In", "dei\u00b7nem", "Au\u00b7ge", "wel\u00b7cher", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich ging, du standst und sahst zur Erden,", "tokens": ["Ich", "ging", ",", "du", "standst", "und", "sahst", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sahst mir nach mit nassem Blick:", "tokens": ["Und", "sahst", "mir", "nach", "mit", "nas\u00b7sem", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch, welch Gl\u00fcck, geliebt zu werden!", "tokens": ["Und", "doch", ",", "welch", "Gl\u00fcck", ",", "ge\u00b7liebt", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAT", "NN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und lieben, G\u00f6tter, welch ein Gl\u00fcck!", "tokens": ["Und", "lie\u00b7ben", ",", "G\u00f6t\u00b7ter", ",", "welch", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}