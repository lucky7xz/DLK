{"textgrid.poem.67204": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6here Menschheit", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Menschen ist der Sinn ins Innere gegeben,", "tokens": ["Den", "Men\u00b7schen", "ist", "der", "Sinn", "ins", "In\u00b7ne\u00b7re", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie als anerkannt das Be\u00dfre w\u00e4hlen,", "tokens": ["Da\u00df", "sie", "als", "an\u00b7er\u00b7kannt", "das", "Be\u00df\u00b7re", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gilt als Ziel, es ist das wahre Leben,", "tokens": ["Es", "gilt", "als", "Ziel", ",", "es", "ist", "das", "wah\u00b7re", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dem sich geistiger des Lebens Jahre z\u00e4hlen.", "tokens": ["Von", "dem", "sich", "geis\u00b7ti\u00b7ger", "des", "Le\u00b7bens", "Jah\u00b7re", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}