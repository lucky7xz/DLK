{"dta.poem.7391": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "So hat er sich umsonst gequ\u00e4lt, umsonst verkauft die werthe", "tokens": ["So", "hat", "er", "sich", "um\u00b7sonst", "ge\u00b7qu\u00e4lt", ",", "um\u00b7sonst", "ver\u00b7kauft", "die", "wert\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "St\u00e4tte,", "tokens": ["St\u00e4t\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wo seiner Kindheit Linde steht und seiner Eltern Sterbe-", "tokens": ["Wo", "sei\u00b7ner", "Kind\u00b7heit", "Lin\u00b7de", "steht", "und", "sei\u00b7ner", "El\u00b7tern", "Ster\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "VVFIN", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "bette,", "tokens": ["bet\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Umsonst hat er so manchen Tag den frostbeklemmten Hauch", "tokens": ["Um\u00b7sonst", "hat", "er", "so", "man\u00b7chen", "Tag", "den", "frost\u00b7be\u00b7klemm\u00b7ten", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gesogen,", "tokens": ["ge\u00b7so\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "In seiner starren Hand den Zaum, umknistert von des", "tokens": ["In", "sei\u00b7ner", "star\u00b7ren", "Hand", "den", "Zaum", ",", "um\u00b7knis\u00b7tert", "von", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schnees Wogen,", "tokens": ["Schnees", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Beim Morgenroth, beim Abendroth,", "tokens": ["Beim", "Mor\u00b7gen\u00b7roth", ",", "beim", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur um ein St\u00fcckchen ehrlich Brod!", "tokens": ["Nur", "um", "ein", "St\u00fcck\u00b7chen", "ehr\u00b7lich", "Brod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der T\u00e4uscher kniet am Pflastergrund, er streicht des Rosses", "tokens": ["Der", "T\u00e4u\u00b7scher", "kniet", "am", "Pflas\u00b7ter\u00b7grund", ",", "er", "streicht", "des", "Ros\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hei\u00dfe Flanken,", "tokens": ["hei\u00b7\u00dfe", "Flan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Von des Geb\u00e4lkes Sparren l\u00e4\u00dft die Leuchte irre Schatten", "tokens": ["Von", "des", "Ge\u00b7b\u00e4l\u00b7kes", "Spar\u00b7ren", "l\u00e4\u00dft", "die", "Leuch\u00b7te", "ir\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "wanken;", "tokens": ["wan\u00b7ken", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Bei Gott, es lebt! \u2014 im Aug' ein Blitz! \u2014 es schaudert,", "tokens": ["Bei", "Gott", ",", "es", "lebt", "!", "im", "Aug'", "ein", "Blitz", "!", "es", "schau\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VVFIN", "$.", "$(", "APPRART", "NN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "zittert, h\u00fcben, dr\u00fcben,", "tokens": ["zit\u00b7tert", ",", "h\u00fc\u00b7ben", ",", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$,", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dann streckt es sich, die N\u00fcstern stehn, vom wilden Schreie", "tokens": ["Dann", "streckt", "es", "sich", ",", "die", "N\u00fcs\u00b7tern", "stehn", ",", "vom", "wil\u00b7den", "Schrei\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VVINF", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "aufgetrieben,", "tokens": ["auf\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und aus den Gliedern wirbelt Dampf,", "tokens": ["Und", "aus", "den", "Glie\u00b7dern", "wir\u00b7belt", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Lebensw\u00e4rme letzter Kampf.", "tokens": ["Der", "Le\u00b7bens\u00b7w\u00e4r\u00b7me", "letz\u00b7ter", "Kampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der T\u00e4uscher kniet und streichelt fort, nicht trauen will er", "tokens": ["Der", "T\u00e4u\u00b7scher", "kniet", "und", "strei\u00b7chelt", "fort", ",", "nicht", "trau\u00b7en", "will", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "PTKNEG", "VVINF", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "seinem Auge,", "tokens": ["sei\u00b7nem", "Au\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und schwellend in die Wimper steigt der Mannesthr\u00e4ne", "tokens": ["Und", "schwel\u00b7lend", "in", "die", "Wim\u00b7per", "steigt", "der", "Man\u00b7nest\u00b7hr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "bittre Lauge,", "tokens": ["bitt\u00b7re", "Lau\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sacht langt die Decke er herbei und schl\u00e4gt sie um des", "tokens": ["Sacht", "langt", "die", "De\u00b7cke", "er", "her\u00b7bei", "und", "schl\u00e4gt", "sie", "um", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Thieres Weichen,", "tokens": ["Thie\u00b7res", "Wei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dann l\u00e4\u00dft er der Laterne Schein ob den gespannten Sehnen", "tokens": ["Dann", "l\u00e4\u00dft", "er", "der", "La\u00b7ter\u00b7ne", "Schein", "ob", "den", "ge\u00b7spann\u00b7ten", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "streichen;", "tokens": ["strei\u00b7chen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Es ist vorbei, kein Odemhauch,", "tokens": ["Es", "ist", "vor\u00b7bei", ",", "kein", "O\u00b7dem\u00b7hauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schon verschwimmt der Flanken Rauch.", "tokens": ["Und", "schon", "ver\u00b7schwimmt", "der", "Flan\u00b7ken", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vom Boden hebt er sich, er steht, der schwergebeugte Mann", "tokens": ["Vom", "Bo\u00b7den", "hebt", "er", "sich", ",", "er", "steht", ",", "der", "schwer\u00b7ge\u00b7beug\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "der Sorgen,", "tokens": ["der", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und langsam hat er seine Stirn, hat sie in hohler Hand", "tokens": ["Und", "lang\u00b7sam", "hat", "er", "sei\u00b7ne", "Stirn", ",", "hat", "sie", "in", "hoh\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "geborgen;", "tokens": ["ge\u00b7bor\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Was heute war? was morgen wird? wie k\u00f6nnt' er dessen", "tokens": ["Was", "heu\u00b7te", "war", "?", "was", "mor\u00b7gen", "wird", "?", "wie", "k\u00f6nnt'", "er", "des\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "$.", "PWS", "ADV", "VAFIN", "$.", "PWAV", "VMFIN", "PPER", "PDS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "sich entsinnen!", "tokens": ["sich", "ent\u00b7sin\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Und der Verzweiflung Schlange f\u00fchlt er kalt zum Herzen", "tokens": ["Und", "der", "Ver\u00b7zwei\u00b7flung", "Schlan\u00b7ge", "f\u00fchlt", "er", "kalt", "zum", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "niederrinnen;", "tokens": ["nie\u00b7der\u00b7rin\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Was war? was ist? \u2014 er f\u00e4hrt empor,", "tokens": ["Was", "war", "?", "was", "ist", "?", "er", "f\u00e4hrt", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PWS", "VAFIN", "$.", "$(", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ein Klirren, dicht an seinem Ohr!", "tokens": ["Ein", "Klir\u00b7ren", ",", "dicht", "an", "sei\u00b7nem", "Ohr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und an dem n\u00e4chsten St\u00e4nder lehnt, des todten Rappen", "tokens": ["Und", "an", "dem", "n\u00e4chs\u00b7ten", "St\u00e4n\u00b7der", "lehnt", ",", "des", "tod\u00b7ten", "Rap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zaum und Z\u00fcgel", "tokens": ["Zaum", "und", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gelassen w\u00e4gend in der Hand, ein Mann mit Hafermaa\u00df", "tokens": ["Ge\u00b7las\u00b7sen", "w\u00e4\u00b7gend", "in", "der", "Hand", ",", "ein", "Mann", "mit", "Ha\u00b7fer\u00b7maa\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "und Striegel,", "tokens": ["und", "Strie\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So st\u00e4mmig wie durch Frost und Staub der K\u00e4rrner treibt", "tokens": ["So", "st\u00e4m\u00b7mig", "wie", "durch", "Frost", "und", "Staub", "der", "K\u00e4rr\u00b7ner", "treibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die derben Glieder,", "tokens": ["die", "der\u00b7ben", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In seinen breiten Nacken h\u00e4ngt der breite Schlapphut tr\u00f6pfelnd", "tokens": ["In", "sei\u00b7nen", "brei\u00b7ten", "Na\u00b7cken", "h\u00e4ngt", "der", "brei\u00b7te", "Schlap\u00b7phut", "tr\u00f6p\u00b7felnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "nieder,", "tokens": ["nie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und ruhig auf den T\u00e4uscher itzt", "tokens": ["Und", "ru\u00b7hig", "auf", "den", "T\u00e4u\u00b7scher", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sein graubewimpert Auge blitzt.", "tokens": ["Sein", "grau\u00b7be\u00b7wim\u00b7pert", "Au\u00b7ge", "blitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eherr!\u201c hebt er an: \u201eihr dauert mich, ein feines Thier ist", "tokens": ["\u201e", "herr", "!", "\u201c", "hebt", "er", "an", ":", "\u201e", "ihr", "dau\u00b7ert", "mich", ",", "ein", "fei\u00b7nes", "Thier", "ist"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "euch gefallen,", "tokens": ["euch", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Doch wei\u00df ich eins, ihm gleich wie sich am Paternoster zwei", "tokens": ["Doch", "wei\u00df", "ich", "eins", ",", "ihm", "gleich", "wie", "sich", "am", "Pa\u00b7ter\u00b7nos\u00b7ter", "zwei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KO