{"textgrid.poem.26926": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6ne Individualit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Vernunft bist du eins, einig mit ihm durch das Herz.", "tokens": ["Durch", "die", "Ver\u00b7nunft", "bist", "du", "eins", ",", "ei\u00b7nig", "mit", "ihm", "durch", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PIS", "$,", "ADJD", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Stimme des Ganzen ist deine Vernunft, dein Herz bist du selber,", "tokens": ["Stim\u00b7me", "des", "Gan\u00b7zen", "ist", "dei\u00b7ne", "Ver\u00b7nunft", ",", "dein", "Herz", "bist", "du", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Wohl dir, wenn die Vernunft immer im Herzen dir wohnt.", "tokens": ["Wohl", "dir", ",", "wenn", "die", "Ver\u00b7nunft", "im\u00b7mer", "im", "Her\u00b7zen", "dir", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}