{"textgrid.poem.36563": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Herausfordrungslied vor der Schlacht bei Ro\u00dfbach", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heraus, aus deiner Wolfesgruft,", "tokens": ["He\u00b7raus", ",", "aus", "dei\u00b7ner", "Wol\u00b7fes\u00b7gruft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Furchtbares Heldenheer!", "tokens": ["Furcht\u00b7ba\u00b7res", "Hel\u00b7den\u00b7heer", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Heraus zum Streit in frische Luft,", "tokens": ["He\u00b7raus", "zum", "Streit", "in", "fri\u00b7sche", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Mut und Schlachtgewehr!", "tokens": ["Mit", "Mut", "und", "Schlacht\u00b7ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir kleiner Haufe, wachen schon,", "tokens": ["Wir", "klei\u00b7ner", "Hau\u00b7fe", ",", "wa\u00b7chen", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen Schlachtgesang,", "tokens": ["Und", "sin\u00b7gen", "Schlacht\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wecken dich mit Kriegeston,", "tokens": ["Und", "we\u00b7cken", "dich", "mit", "Krie\u00b7ge\u00b7ston", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit L\u00e4rm und Waffenklang.", "tokens": ["Mit", "L\u00e4rm", "und", "Waf\u00b7fen\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was schlummerst du? Die tr\u00e4ge Rast,", "tokens": ["Was", "schlum\u00b7merst", "du", "?", "Die", "tr\u00e4\u00b7ge", "Rast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schickt die f\u00fcr Helden sich?", "tokens": ["Schickt", "die", "f\u00fcr", "Hel\u00b7den", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "PRF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wenn du gerechte Sache hast,", "tokens": ["Wenn", "du", "ge\u00b7rech\u00b7te", "Sa\u00b7che", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum verkriechst du dich?", "tokens": ["Wa\u00b7rum", "ver\u00b7kriechst", "du", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}