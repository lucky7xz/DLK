{"textgrid.poem.44165": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Eil mu\u00df auch noch, werther Freund,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Eil mu\u00df auch noch, werther Freund,", "tokens": ["In", "Eil", "mu\u00df", "auch", "noch", ",", "wert\u00b7her", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Pegasus vor dir erscheinen.", "tokens": ["Mein", "Pe\u00b7ga\u00b7sus", "vor", "dir", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast es allzeit wohlgemeint,", "tokens": ["Du", "hast", "es", "all\u00b7zeit", "wohl\u00b7ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will es allzeit ehrlich meinen.", "tokens": ["Ich", "will", "es", "all\u00b7zeit", "ehr\u00b7lich", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Abendstunde, da wir oft", "tokens": ["Die", "A\u00b7bends\u00b7tun\u00b7de", ",", "da", "wir", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey B\u00fcchern und Taback gese\u00dfen,", "tokens": ["Bey", "B\u00fc\u00b7chern", "und", "Ta\u00b7back", "ge\u00b7se\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bleibt, wie mein schlechter Phoebus hoft,", "tokens": ["Bleibt", ",", "wie", "mein", "schlech\u00b7ter", "Phoe\u00b7bus", "hoft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOKOM", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bey dir auch k\u00fcnftig unverge\u00dfen.", "tokens": ["Bey", "dir", "auch", "k\u00fcnf\u00b7tig", "un\u00b7ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer weis, wo wir noch in der Welt", "tokens": ["Wer", "weis", ",", "wo", "wir", "noch", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einander wiedersehn und k\u00fc\u00dfen!", "tokens": ["Ein\u00b7an\u00b7der", "wie\u00b7der\u00b7sehn", "und", "k\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da will ich, wenn es Gott gef\u00e4llt,", "tokens": ["Da", "will", "ich", ",", "wenn", "es", "Gott", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir die Freundschaft n\u00e4her schlie\u00dfen.", "tokens": ["Mit", "dir", "die", "Freund\u00b7schaft", "n\u00e4\u00b7her", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jezt habe Danck vor deine Gunst,", "tokens": ["Jezt", "ha\u00b7be", "Danck", "vor", "dei\u00b7ne", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die mich in Armuth oft erhoben,", "tokens": ["Die", "mich", "in", "Ar\u00b7muth", "oft", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erhebt mich einmahl meine Kunst,", "tokens": ["Er\u00b7hebt", "mich", "ein\u00b7mahl", "mei\u00b7ne", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So geb ich dir auch gleiche Proben.", "tokens": ["So", "geb", "ich", "dir", "auch", "glei\u00b7che", "Pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}