{"textgrid.poem.43440": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das freie Wort von Ort zu Ort,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das freie Wort von Ort zu Ort,", "tokens": ["Das", "frei\u00b7e", "Wort", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jedem Munde das freie Wort!", "tokens": ["In", "je\u00b7dem", "Mun\u00b7de", "das", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir wollen es singen, wir wollen es sagen,", "tokens": ["Wir", "wol\u00b7len", "es", "sin\u00b7gen", ",", "wir", "wol\u00b7len", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im Herzen wie auf den Lippen tragen.", "tokens": ["Im", "Her\u00b7zen", "wie", "auf", "den", "Lip\u00b7pen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frei ist das Wort! wer ", "tokens": ["Frei", "ist", "das", "Wort", "!", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PWS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der sei kein Deutscher je genannt,", "tokens": ["Der", "sei", "kein", "Deut\u00b7scher", "je", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem reiche kein Deutscher je die Hand,", "tokens": ["Dem", "rei\u00b7che", "kein", "Deut\u00b7scher", "je", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der sei verbannt aus dem Vaterland!", "tokens": ["Der", "sei", "ver\u00b7bannt", "aus", "dem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Freies Wort immerfort!", "tokens": ["Frei\u00b7es", "Wort", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Sto\u00dfet an auf's freie Wort!", "tokens": ["Sto\u00b7\u00dfet", "an", "auf's", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn des Freisinns reine edele That", "tokens": ["Wenn", "des", "Frei\u00b7sinns", "rei\u00b7ne", "e\u00b7de\u00b7le", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Verdammt und verfolgt wird als Hochverrath,", "tokens": ["Ver\u00b7dammt", "und", "ver\u00b7folgt", "wird", "als", "Hoch\u00b7ver\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn die Wahrheit in dunkeln Kerkermauern", "tokens": ["Wenn", "die", "Wahr\u00b7heit", "in", "dun\u00b7keln", "Ker\u00b7ker\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Verstummen mu\u00df und ihr Leben vertrauern \u2013", "tokens": ["Ver\u00b7stum\u00b7men", "mu\u00df", "und", "ihr", "Le\u00b7ben", "ver\u00b7trau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frei ist das Wort! wer ", "tokens": ["Frei", "ist", "das", "Wort", "!", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PWS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der sei kein Deutscher je genannt,", "tokens": ["Der", "sei", "kein", "Deut\u00b7scher", "je", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem reiche kein Deutscher je die Hand,", "tokens": ["Dem", "rei\u00b7che", "kein", "Deut\u00b7scher", "je", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der sei verbannt aus dem Vaterland!", "tokens": ["Der", "sei", "ver\u00b7bannt", "aus", "dem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Freies Wort immerfort!", "tokens": ["Frei\u00b7es", "Wort", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Sto\u00dfet an auf's freie Wort!", "tokens": ["Sto\u00b7\u00dfet", "an", "auf's", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn Buch und Schrift in Knechtsgestalt", "tokens": ["Wenn", "Buch", "und", "Schrift", "in", "Knechts\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur dienen darf der frechen Gewalt,", "tokens": ["Nur", "die\u00b7nen", "darf", "der", "fre\u00b7chen", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wenn die Kunst und Wissenschaft schmeichelt und kriechet,", "tokens": ["Wenn", "die", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", "schmei\u00b7chelt", "und", "krie\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn Kraft und Gesinnung kr\u00e4nkelt und siechet \u2013", "tokens": ["Wenn", "Kraft", "und", "Ge\u00b7sin\u00b7nung", "kr\u00e4n\u00b7kelt", "und", "sie\u00b7chet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frei ist das Wort! wer ", "tokens": ["Frei", "ist", "das", "Wort", "!", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PWS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der sei kein Deutscher je genannt,", "tokens": ["Der", "sei", "kein", "Deut\u00b7scher", "je", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem reiche kein Deutscher je die Hand,", "tokens": ["Dem", "rei\u00b7che", "kein", "Deut\u00b7scher", "je", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der sei verbannt aus dem Vaterland!", "tokens": ["Der", "sei", "ver\u00b7bannt", "aus", "dem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Freies Wort immerfort!", "tokens": ["Frei\u00b7es", "Wort", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Sto\u00dfet an auf's freie Wort!", "tokens": ["Sto\u00b7\u00dfet", "an", "auf's", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn Lug und Trug die Welt umschw\u00e4rmt,", "tokens": ["Wenn", "Lug", "und", "Trug", "die", "Welt", "um\u00b7schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn das Laster jauchzt, wenn die Tugend sich h\u00e4rmt,", "tokens": ["Wenn", "das", "Las\u00b7ter", "jauchzt", ",", "wenn", "die", "Tu\u00b7gend", "sich", "h\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn der Teufel im Bunde mit B\u00f6sen und Schlechten", "tokens": ["Wenn", "der", "Teu\u00b7fel", "im", "Bun\u00b7de", "mit", "B\u00f6\u00b7sen", "und", "Schlech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Uns machen m\u00f6chte zu Feigen und Knechten \u2013", "tokens": ["Uns", "ma\u00b7chen", "m\u00f6ch\u00b7te", "zu", "Fei\u00b7gen", "und", "Knech\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frei ist das Wort! wer ", "tokens": ["Frei", "ist", "das", "Wort", "!", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PWS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der sei kein Deutscher je genannt,", "tokens": ["Der", "sei", "kein", "Deut\u00b7scher", "je", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem reiche kein Deutscher je die Hand,", "tokens": ["Dem", "rei\u00b7che", "kein", "Deut\u00b7scher", "je", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der sei verbannt aus dem Vaterland!", "tokens": ["Der", "sei", "ver\u00b7bannt", "aus", "dem", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Freies Wort immerfort!", "tokens": ["Frei\u00b7es", "Wort", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Sto\u00dfet an auf's freie Wort!", "tokens": ["Sto\u00b7\u00dfet", "an", "auf's", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Frisch auf, mein freies Wort, ins Gefecht", "tokens": ["Frisch", "auf", ",", "mein", "frei\u00b7es", "Wort", ",", "ins", "Ge\u00b7fecht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "F\u00fcr's Vaterland, f\u00fcr die Ehr' und das Recht!", "tokens": ["F\u00fcr's", "Va\u00b7ter\u00b7land", ",", "f\u00fcr", "die", "Ehr'", "und", "das", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und saus' und brause wie Donnerwetter,", "tokens": ["Und", "saus'", "und", "brau\u00b7se", "wie", "Don\u00b7ner\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verk\u00fcnd' uns den Sieg mit Drommetengeschmetter!", "tokens": ["Ver\u00b7k\u00fcnd'", "uns", "den", "Sieg", "mit", "Drom\u00b7me\u00b7ten\u00b7ge\u00b7schmet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Frei bleibt das Wort! wer ", "tokens": ["Frei", "bleibt", "das", "Wort", "!", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "PWS"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der wird ein Deutscher stets genannt,", "tokens": ["Der", "wird", "ein", "Deut\u00b7scher", "stets", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem reicht ein Deutscher stets die Hand,", "tokens": ["Dem", "reicht", "ein", "Deut\u00b7scher", "stets", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den r\u00fchmt und preist das Vaterland.", "tokens": ["Den", "r\u00fchmt", "und", "preist", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Freies Wort immerfort!", "tokens": ["Frei\u00b7es", "Wort", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Sto\u00dfet an auf's freie Wort!", "tokens": ["Sto\u00b7\u00dfet", "an", "auf's", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}