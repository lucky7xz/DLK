{"textgrid.poem.62895": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Samogitien genas", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Samogitien genas", "tokens": ["In", "Sa\u00b7mo\u00b7gi\u00b7ti\u00b7en", "ge\u00b7nas"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ehschatz eines edeln B\u00e4ren,", "tokens": ["Der", "Eh\u00b7schatz", "ei\u00b7nes", "e\u00b7deln", "B\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der mit im Parlamente sa\u00df,", "tokens": ["Der", "mit", "im", "Par\u00b7la\u00b7men\u00b7te", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Sohn. Die J\u00e4ger lehren:", "tokens": ["Von", "ei\u00b7nem", "Sohn", ".", "Die", "J\u00e4\u00b7ger", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein B\u00e4r, der aus der Mutter Schoos", "tokens": ["Ein", "B\u00e4r", ",", "der", "aus", "der", "Mut\u00b7ter", "Schoos"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hervorkriecht, sey ein roher Klos,", "tokens": ["Her\u00b7vor\u00b7kriecht", ",", "sey", "ein", "ro\u00b7her", "Klos", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der erst durch Lecken Form und Sch\u00f6ne", "tokens": ["Der", "erst", "durch", "Le\u00b7cken", "Form", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bek\u00f6mmt. Das wu\u00dfte die Mama", "tokens": ["Be\u00b7k\u00f6mmt", ".", "Das", "wu\u00df\u00b7te", "die", "Ma\u00b7ma"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch besser als wir Menschens\u00f6hne,", "tokens": ["Noch", "bes\u00b7ser", "als", "wir", "Men\u00b7schen\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und schwur, als sie das B\u00fcbchen sah,", "tokens": ["Und", "schwur", ",", "als", "sie", "das", "B\u00fcb\u00b7chen", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von Lust berauscht, beym gro\u00dfen Petze", "tokens": ["Von", "Lust", "be\u00b7rauscht", ",", "beym", "gro\u00b7\u00dfen", "Pet\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Des Firmaments, ihr kleiner G\u00f6tze", "tokens": ["Des", "Fir\u00b7ma\u00b7ments", ",", "ihr", "klei\u00b7ner", "G\u00f6t\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sollt ein Adonis seyn. Sie feilt,", "tokens": ["Sollt", "ein", "A\u00b7do\u00b7nis", "seyn", ".", "Sie", "feilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAINF", "$.", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Sie hobelt mit der rauhen Zunge", "tokens": ["Sie", "ho\u00b7belt", "mit", "der", "rau\u00b7hen", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ihn bis aufs Blut. Der arme Junge!", "tokens": ["Ihn", "bis", "aufs", "Blut", ".", "Der", "ar\u00b7me", "Jun\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Er brummt, er wendet sich, er heult;", "tokens": ["Er", "brummt", ",", "er", "wen\u00b7det", "sich", ",", "er", "heult", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Umsonst, sie bleibt bey ihrer Mode,", "tokens": ["Um\u00b7sonst", ",", "sie", "bleibt", "bey", "ih\u00b7rer", "Mo\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und leckt ihn endlich gar zu Tode.", "tokens": ["Und", "leckt", "ihn", "end\u00b7lich", "gar", "zu", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}