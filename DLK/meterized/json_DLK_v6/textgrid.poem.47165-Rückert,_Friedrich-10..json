{"textgrid.poem.47165": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, da\u00df mich der Himmel liebt,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mich", "der", "Him\u00b7mel", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil du mich liebst, mein Leben!", "tokens": ["Weil", "du", "mich", "liebst", ",", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er mir meine Schuld vergibt,", "tokens": ["Da\u00df", "er", "mir", "mei\u00b7ne", "Schuld", "ver\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil er dich mir gegeben.", "tokens": ["Weil", "er", "dich", "mir", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, weil du schw\u00f6rst, da\u00df ohne mich", "tokens": ["Ja", ",", "weil", "du", "schw\u00f6rst", ",", "da\u00df", "oh\u00b7ne", "mich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Gl\u00fcck dir k\u00f6nne lachen,", "tokens": ["Kein", "Gl\u00fcck", "dir", "k\u00f6n\u00b7ne", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df, um zu machen gl\u00fccklich dich,", "tokens": ["Mu\u00df", ",", "um", "zu", "ma\u00b7chen", "gl\u00fcck\u00b7lich", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUI", "PTKZU", "VVINF", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herr mich gl\u00fccklich machen.", "tokens": ["Der", "Herr", "mich", "gl\u00fcck\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}