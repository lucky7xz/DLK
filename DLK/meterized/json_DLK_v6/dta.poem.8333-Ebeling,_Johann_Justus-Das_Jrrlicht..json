{"dta.poem.8333": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das Jrrlicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein flatternd Feur sah ich von Ferne,", "tokens": ["Ein", "flat\u00b7ternd", "Feur", "sah", "ich", "von", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es spr\u00fczte \u00f6ffters lichte Sterne,", "tokens": ["Es", "spr\u00fcz\u00b7te", "\u00f6ff\u00b7ters", "lich\u00b7te", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einer nassen Wiese aus:", "tokens": ["Auf", "ei\u00b7ner", "nas\u00b7sen", "Wie\u00b7se", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich dachte teutscht mich ein Gesichte:", "tokens": ["Ich", "dach\u00b7te", "teutscht", "mich", "ein", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es \u00fcberfiel mich bei dem Lichte,", "tokens": ["Es", "\u00fc\u00b7berf\u00b7iel", "mich", "bei", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein von der Furcht entstandner Graus.", "tokens": ["Ein", "von", "der", "Furcht", "ent\u00b7stand\u00b7ner", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Allein so bald ich mich besonnen,", "tokens": ["Al\u00b7lein", "so", "bald", "ich", "mich", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df es ein Jrrlicht, wards zerronnen,", "tokens": ["Da\u00df", "es", "ein", "Jrr\u00b7licht", ",", "wards", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dunst verging im Augenblik,", "tokens": ["Der", "Dunst", "ver\u00b7ging", "im", "Au\u00b7gen\u00b7blik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch gleich wars wieder in dem Glanze,", "tokens": ["Doch", "gleich", "wars", "wie\u00b7der", "in", "dem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es sprung als, wie bei einem Tanze,", "tokens": ["Es", "sprung", "als", ",", "wie", "bei", "ei\u00b7nem", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch ging es immer mehr zur\u00fck.", "tokens": ["Doch", "ging", "es", "im\u00b7mer", "mehr", "zu\u00b7r\u00fck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sah es nur beim schwarzen Dunkeln,", "tokens": ["Ich", "sah", "es", "nur", "beim", "schwar\u00b7zen", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht am lichten Tage funkeln,", "tokens": ["Und", "nicht", "am", "lich\u00b7ten", "Ta\u00b7ge", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das gab mir diese Lehre ein:", "tokens": ["Das", "gab", "mir", "die\u00b7se", "Leh\u00b7re", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lichter die uns nur verwirren,", "tokens": ["Die", "Lich\u00b7ter", "die", "uns", "nur", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In sumpfigten Morasten irren,", "tokens": ["In", "sump\u00b7fig\u00b7ten", "Mo\u00b7ras\u00b7ten", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die haben einen falschen Schein.", "tokens": ["Die", "ha\u00b7ben", "ei\u00b7nen", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So geht es auch den Jrrelehrern,", "tokens": ["So", "geht", "es", "auch", "den", "Jr\u00b7re\u00b7leh\u00b7rern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denjenigen Gem\u00fcths Verkehrern,", "tokens": ["Den\u00b7je\u00b7ni\u00b7gen", "Ge\u00b7m\u00fcths", "Ver\u00b7keh\u00b7rern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die da verr\u00fckken das Gehirn,", "tokens": ["Die", "da", "ver\u00b7r\u00fck\u00b7ken", "das", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie scheinen als wie grosse Lichter,", "tokens": ["Sie", "schei\u00b7nen", "als", "wie", "gros\u00b7se", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sind doch falsche B\u00f6sewichter,", "tokens": ["Und", "sind", "doch", "fal\u00b7sche", "B\u00f6\u00b7se\u00b7wich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie gleichen einem Jrrgestirn.", "tokens": ["Sie", "glei\u00b7chen", "ei\u00b7nem", "Jrr\u00b7ges\u00b7tirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sind wie Lichter anzusehen,", "tokens": ["Sie", "sind", "wie", "Lich\u00b7ter", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die flatterhafft aus Dunst entstehen,", "tokens": ["Die", "flat\u00b7ter\u00b7hafft", "aus", "Dunst", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie leuchten nur bei dunkler Nacht", "tokens": ["Sie", "leuch\u00b7ten", "nur", "bei", "dunk\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bald die schwarzen Schatten weichen,", "tokens": ["So", "bald", "die", "schwar\u00b7zen", "Schat\u00b7ten", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df ihr Glanz geschw\u00e4cht erbleichen,", "tokens": ["So", "mu\u00df", "ihr", "Glanz", "ge\u00b7schw\u00e4cht", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Warheit alles helle macht.", "tokens": ["Wenn", "War\u00b7heit", "al\u00b7les", "hel\u00b7le", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So wie im Sumpf ein Jrrlicht brennet,", "tokens": ["So", "wie", "im", "Sumpf", "ein", "Jrr\u00b7licht", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und an den feuchten Oertern rennet;", "tokens": ["Und", "an", "den", "feuch\u00b7ten", "O\u00b7er\u00b7tern", "ren\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So ist wo falsche Lehr sich zeigt,", "tokens": ["So", "ist", "wo", "fal\u00b7sche", "Lehr", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PWAV", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schwindelgeist in seinem Leben,", "tokens": ["Ein", "Schwin\u00b7del\u00b7geist", "in", "sei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Lastern auch gar sehr ergeben,", "tokens": ["Den", "Las\u00b7tern", "auch", "gar", "sehr", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die machen ihm zum Wahn geneigt.", "tokens": ["Die", "ma\u00b7chen", "ihm", "zum", "Wahn", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie viele scheinen in die Augen,", "tokens": ["Wie", "vie\u00b7le", "schei\u00b7nen", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil sie nicht in Herzen taugen,", "tokens": ["Die\u00b7weil", "sie", "nicht", "in", "Her\u00b7zen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem falschen Licht erf\u00fcllt,", "tokens": ["Mit", "ei\u00b7nem", "fal\u00b7schen", "Licht", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die, weil sie gern in Lastern w\u00fchlen,", "tokens": ["Die", ",", "weil", "sie", "gern", "in", "Las\u00b7tern", "w\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Rolle eines Sp\u00f6tters spielen,", "tokens": ["Die", "Rol\u00b7le", "ei\u00b7nes", "Sp\u00f6t\u00b7ters", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sind des Jrrlichts Gegenbild.", "tokens": ["Sie", "sind", "des", "Jrr\u00b7lichts", "Ge\u00b7gen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}