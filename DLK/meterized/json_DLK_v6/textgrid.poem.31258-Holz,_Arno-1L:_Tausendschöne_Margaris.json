{"textgrid.poem.31258": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tausendsch\u00f6ne Margaris/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausendsch\u00f6ne Margaris/", "tokens": ["Tau\u00b7send\u00b7sch\u00f6\u00b7ne", "Mar\u00b7ga\u00b7ris", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "deine Br\u00fcste/ gantz gewi\u00df/", "tokens": ["dei\u00b7ne", "Br\u00fcs\u00b7te", "/", "gantz", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sind zween Zokker-Ballen.", "tokens": ["sind", "zween", "Zok\u00b7ker\u00b7Bal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "L\u00e4chelstu/ so kan man sehn", "tokens": ["L\u00e4\u00b7chel\u00b7stu", "/", "so", "kan", "man", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "zweyunddreyssig B\u00e4rlen stehn", "tokens": ["zwey\u00b7und\u00b7dreys\u00b7sig", "B\u00e4r\u00b7len", "stehn"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "zwischen Mund-Korallen.", "tokens": ["zwi\u00b7schen", "Mun\u00b7dKo\u00b7ral\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Von seidnen B\u00e4ndern gleisst dein Zopff/", "tokens": ["Von", "seid\u00b7nen", "B\u00e4n\u00b7dern", "gleisst", "dein", "Zopff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "doch hastu auch Verstand im Kopff?", "tokens": ["doch", "has\u00b7tu", "auch", "Ver\u00b7stand", "im", "Kopff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lihderlicher al\u00df ", "tokens": ["Lih\u00b7der\u00b7li\u00b7cher", "al\u00df"], "token_info": ["word", "word"], "pos": ["ADJD", "KOKOM"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "zwikkt ", "tokens": ["zwikkt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "mich bald forn/ bald hindten.", "tokens": ["mich", "bald", "forn", "/", "bald", "hind\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "$(", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dr\u00fcmb so brech ich gern diskret/", "tokens": ["Dr\u00fcmb", "so", "brech", "ich", "gern", "dis\u00b7kret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn der Mohnd am Himmel steht/", "tokens": ["wenn", "der", "Mohnd", "am", "Him\u00b7mel", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "deine Hyazinthen.", "tokens": ["dei\u00b7ne", "Hy\u00b7a\u00b7zin\u00b7then", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Doch kaum da\u00df dan ", "tokens": ["Doch", "kaum", "da\u00df", "dan"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "b\u00fcn ich dir auch schon au\u00dfgeflizzt!", "tokens": ["b\u00fcn", "ich", "dir", "auch", "schon", "au\u00df\u00b7ge\u00b7flizzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo/ von Bluhmen bundt \u00fcmbblinckt/", "tokens": ["Wo", "/", "von", "Bluh\u00b7men", "bundt", "\u00fcmb\u00b7blinckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "zwischen Rohsen-Str\u00e4uchen/", "tokens": ["zwi\u00b7schen", "Roh\u00b7sen\u00b7Str\u00e4u\u00b7chen", "/"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "str\u00e4kke ich mich froh und frey/", "tokens": ["str\u00e4k\u00b7ke", "ich", "mich", "froh", "und", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "itzt steh andern Sch\u00e4ffern bey/", "tokens": ["itzt", "steh", "an\u00b7dern", "Sch\u00e4f\u00b7fern", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die da\u00df b\u00e4sser br\u00e4uchen!", "tokens": ["die", "da\u00df", "b\u00e4s\u00b7ser", "br\u00e4u\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "den g\u00f6ldnen ", "tokens": ["den", "g\u00f6ld\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}}}}