{"textgrid.poem.37645": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "In Frankfurt am Main", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen that mir das Herz so weh,", "tokens": ["Zu\u00b7wei\u00b7len", "that", "mir", "das", "Herz", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als ob es w\u00e4r gesprungen,", "tokens": ["Als", "ob", "es", "w\u00e4r", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich dann recht in mich geh',", "tokens": ["Und", "wenn", "ich", "dann", "recht", "in", "mich", "geh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hat mir das Ohr geklungen.", "tokens": ["So", "hat", "mir", "das", "Ohr", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was klingt das Ohr, was schl\u00e4gt das Herz,", "tokens": ["Was", "klingt", "das", "Ohr", ",", "was", "schl\u00e4gt", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So laut an die gro\u00dfen Glocken?", "tokens": ["So", "laut", "an", "die", "gro\u00b7\u00dfen", "Glo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Es treibt der Himmel im Herzen Scherz,", "tokens": ["Es", "treibt", "der", "Him\u00b7mel", "im", "Her\u00b7zen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da ist der Verstand erschrocken,", "tokens": ["Da", "ist", "der", "Ver\u00b7stand", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und schiebt es wohl auf die Witterung", "tokens": ["Und", "schiebt", "es", "wohl", "auf", "die", "Wit\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und auf die vergangnen Zeiten,", "tokens": ["Und", "auf", "die", "ver\u00b7gang\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Wer liebt, der ist noch im Himmel jung", "tokens": ["Wer", "liebt", ",", "der", "ist", "noch", "im", "Him\u00b7mel", "jung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VAFIN", "ADV", "APPRART", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und schauet die Erden von Weitem.", "tokens": ["Und", "schau\u00b7et", "die", "Er\u00b7den", "von", "Wei\u00b7tem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}