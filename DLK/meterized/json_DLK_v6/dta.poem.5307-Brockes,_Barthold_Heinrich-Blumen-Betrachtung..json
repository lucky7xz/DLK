{"dta.poem.5307": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Blumen-Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich zwischen Blumen gehe,", "tokens": ["Da", "ich", "zwi\u00b7schen", "Blu\u00b7men", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und, mit tausendfacher Lust,", "tokens": ["Und", ",", "mit", "tau\u00b7send\u00b7fa\u00b7cher", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausendfache Farben sehe;", "tokens": ["Tau\u00b7send\u00b7fa\u00b7che", "Far\u00b7ben", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird das Hertz in meiner Brust,", "tokens": ["Wird", "das", "Hertz", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht nur durch die bunte Pracht,", "tokens": ["Nicht", "nur", "durch", "die", "bun\u00b7te", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und durch den Geruch ger\u00fchret;", "tokens": ["Und", "durch", "den", "Ge\u00b7ruch", "ge\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sondern mein vergn\u00fcgter Geist,", "tokens": ["Son\u00b7dern", "mein", "ver\u00b7gn\u00fcg\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird zu dem, der sie gemacht,", "tokens": ["Wird", "zu", "dem", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Voller Brunst empor gef\u00fchret.", "tokens": ["Vol\u00b7ler", "Brunst", "em\u00b7por", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Von des Sch\u00f6pfers Wunder-Wesen,", "tokens": ["Von", "des", "Sch\u00f6p\u00b7fers", "Wun\u00b7der\u00b7We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "L\u00e4sset ihrer Farben-Zier,", "tokens": ["L\u00e4s\u00b7set", "ih\u00b7rer", "Fa\u00b7rben\u00b7Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "In gef\u00e4rbten Lettern mir,", "tokens": ["In", "ge\u00b7f\u00e4rb\u00b7ten", "Let\u00b7tern", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Viel, auf vielen Bl\u00e4ttern, lesen.", "tokens": ["Viel", ",", "auf", "vie\u00b7len", "Bl\u00e4t\u00b7tern", ",", "le\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PIAT", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, wie wir durch Stimmen uns weiter, als wir", "tokens": ["Ja", ",", "wie", "wir", "durch", "Stim\u00b7men", "uns", "wei\u00b7ter", ",", "als", "wir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "APPR", "NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wir das, so wir gedencken, dadurch in die Fern\u2019 entdecken:", "tokens": ["Und", "wir", "das", ",", "so", "wir", "ge\u00b7den\u00b7cken", ",", "da\u00b7durch", "in", "die", "Fern'", "ent\u00b7de\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "$,", "ADV", "PPER", "NN", "$,", "PAV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also scheint vom Blumen-Heer ebenfals auf allen Seiten", "tokens": ["Al\u00b7so", "scheint", "vom", "Blu\u00b7men\u00b7Heer", "e\u00b7ben\u00b7fals", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Sich, im lieblichen Geruch, eine Rede zu verbreiten.", "tokens": ["Sich", ",", "im", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", ",", "ei\u00b7ne", "Re\u00b7de", "zu", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Denn sie lassen, GOtt zu Ehren, nicht nur s\u00fcsse D\u00fcnste", "tokens": ["Denn", "sie", "las\u00b7sen", ",", "Gott", "zu", "Eh\u00b7ren", ",", "nicht", "nur", "s\u00fcs\u00b7se", "D\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "NN", "APPR", "NN", "$,", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Sondern in dem s\u00fcssen Hauchen", "tokens": ["Son\u00b7dern", "in", "dem", "s\u00fcs\u00b7sen", "Hau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter Lobes-Lieder h\u00f6ren,", "tokens": ["Lau\u00b7ter", "Lo\u00b7bes\u00b7Lie\u00b7der", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche der gantz deutlich sp\u00fchrt,", "tokens": ["Wel\u00b7che", "der", "gantz", "deut\u00b7lich", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher, wenn der Duft ihn r\u00fchrt,", "tokens": ["Wel\u00b7cher", ",", "wenn", "der", "Duft", "ihn", "r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er sich daran erquicket,", "tokens": ["Und", "er", "sich", "da\u00b7ran", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Lust des Sch\u00f6pffers denckt,", "tokens": ["In", "der", "Lust", "des", "Sch\u00f6pf\u00b7fers", "denckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der die Welt so sch\u00f6n geschm\u00fccket,", "tokens": ["Der", "die", "Welt", "so", "sch\u00f6n", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ihm so viel Anmuth schenckt.", "tokens": ["Und", "ihm", "so", "viel", "An\u00b7muth", "schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}