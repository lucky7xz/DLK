{"textgrid.poem.46525": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du warest klein, und kleine Blumen schling' ich]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warest klein, und kleine Blumen schling' ich", "tokens": ["Du", "wa\u00b7rest", "klein", ",", "und", "klei\u00b7ne", "Blu\u00b7men", "schling'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KON", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Kranze dir, und kleine Lieder sing' ich,", "tokens": ["Zum", "Kran\u00b7ze", "dir", ",", "und", "klei\u00b7ne", "Lie\u00b7der", "sing'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kleine Gaben gro\u00dfer Liebe bring' ich.", "tokens": ["So", "klei\u00b7ne", "Ga\u00b7ben", "gro\u00b7\u00dfer", "Lie\u00b7be", "bring'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An Blumen hast du wohl wie sonst Gefallen,", "tokens": ["An", "Blu\u00b7men", "hast", "du", "wohl", "wie", "sonst", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "KOKOM", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Lieder, die hier nicht verstand dein Lallen,", "tokens": ["Und", "Lie\u00b7der", ",", "die", "hier", "nicht", "ver\u00b7stand", "dein", "Lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die lernst du singen nun in jenen Hallen.", "tokens": ["Die", "lernst", "du", "sin\u00b7gen", "nun", "in", "je\u00b7nen", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf meine Blumen blickst du l\u00e4chelnd nieder,", "tokens": ["Auf", "mei\u00b7ne", "Blu\u00b7men", "blickst", "du", "l\u00e4\u00b7chelnd", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und singt auf Erden niemand meine Lieder,", "tokens": ["Und", "singt", "auf", "Er\u00b7den", "nie\u00b7mand", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So t\u00f6nen sie vom Himmel sch\u00f6ner wieder.", "tokens": ["So", "t\u00f6\u00b7nen", "sie", "vom", "Him\u00b7mel", "sch\u00f6\u00b7ner", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}