{"textgrid.poem.40519": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tod, du warst, Ungleiches auszugleichen,", "tokens": ["O", "Tod", ",", "du", "warst", ",", "Un\u00b7glei\u00b7ches", "aus\u00b7zu\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "$,", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch allzuhart und gar zu eifrig hier!", "tokens": ["Doch", "all\u00b7zu\u00b7hart", "und", "gar", "zu", "eif\u00b7rig", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADV", "PTKA", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, keine Inschrift und kein Liebeszeichen,", "tokens": ["Ach", ",", "kei\u00b7ne", "In\u00b7schrift", "und", "kein", "Lie\u00b7bes\u00b7zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur leises Ahnen nennt die Schl\u00e4fer mir!", "tokens": ["Nur", "lei\u00b7ses", "Ah\u00b7nen", "nennt", "die", "Schl\u00e4\u00b7fer", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Hirte wohl ruht hier im duft'gen Rasen:", "tokens": ["Ein", "Hir\u00b7te", "wohl", "ruht", "hier", "im", "duft'\u00b7gen", "Ra\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich seh' ja frei um seinen gr\u00fcnen Rain", "tokens": ["Ich", "seh'", "ja", "frei", "um", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Rain"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Alpenheerde in den Kr\u00e4utern grasen;", "tokens": ["Die", "Al\u00b7pen\u00b7heer\u00b7de", "in", "den", "Kr\u00e4u\u00b7tern", "gra\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wo die Heerde, mu\u00df der Hirte sein!", "tokens": ["Und", "wo", "die", "Heer\u00b7de", ",", "mu\u00df", "der", "Hir\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein J\u00e4ger tr\u00e4umt da unter k\u00fchler Decke:", "tokens": ["Ein", "J\u00e4\u00b7ger", "tr\u00e4umt", "da", "un\u00b7ter", "k\u00fch\u00b7ler", "De\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir sagt's das Rehlein, weidend hier bei Nacht,", "tokens": ["Mir", "sagt's", "das", "Reh\u00b7lein", ",", "wei\u00b7dend", "hier", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ob es sanft die todte Hand ihm lecke;", "tokens": ["Als", "ob", "es", "sanft", "die", "tod\u00b7te", "Hand", "ihm", "le\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wem w\u00e4re sonst so milde Rach' erdacht?", "tokens": ["Wem", "w\u00e4\u00b7re", "sonst", "so", "mil\u00b7de", "Rach'", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Schnitter schlummert dort am fernen Saume:", "tokens": ["Ein", "Schnit\u00b7ter", "schlum\u00b7mert", "dort", "am", "fer\u00b7nen", "Sau\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh' es an der Blumen selt'nem Tanz,", "tokens": ["Ich", "seh'", "es", "an", "der", "Blu\u00b7men", "selt'\u00b7nem", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als w\u00fchle seine Hand darin im Traume,", "tokens": ["Als", "w\u00fch\u00b7le", "sei\u00b7ne", "Hand", "da\u00b7rin", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu flechten sie zum heit'ren Erntekranz!", "tokens": ["Zu", "flech\u00b7ten", "sie", "zum", "heit'\u00b7ren", "Ern\u00b7te\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch will zum Grab des Lieben Liebe wandern,", "tokens": ["Doch", "will", "zum", "Grab", "des", "Lie\u00b7ben", "Lie\u00b7be", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf welches str\u00f6me sie den Thr\u00e4nenzoll?", "tokens": ["Auf", "wel\u00b7ches", "str\u00f6\u00b7me", "sie", "den", "Thr\u00e4\u00b7nen\u00b7zoll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun, was verschl\u00e4gt's, erquickt er einen Andern,", "tokens": ["Nun", ",", "was", "ver\u00b7schl\u00e4gt's", ",", "er\u00b7quickt", "er", "ei\u00b7nen", "An\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dem vielleicht noch keine Z\u00e4hre quoll?!", "tokens": ["Zu", "dem", "viel\u00b7leicht", "noch", "kei\u00b7ne", "Z\u00e4h\u00b7re", "quoll", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Trauer, suchst du nur nach Einer Welle?", "tokens": ["O", "Trau\u00b7er", ",", "suchst", "du", "nur", "nach", "Ei\u00b7ner", "Wel\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ist das ganze dunkle Meer doch dein!", "tokens": ["Und", "ist", "das", "gan\u00b7ze", "dunk\u00b7le", "Meer", "doch", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "ADJA", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00fcnkt dir ein einzig Sternlein tr\u00f6stend helle?", "tokens": ["D\u00fcnkt", "dir", "ein", "ein\u00b7zig", "Stern\u00b7lein", "tr\u00f6s\u00b7tend", "hel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein soll der ganze Strahlenhimmel sein!", "tokens": ["Dein", "soll", "der", "gan\u00b7ze", "Strah\u00b7len\u00b7him\u00b7mel", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O Liebe, sp\u00e4hst du nur nach Einem Halme?", "tokens": ["O", "Lie\u00b7be", ",", "sp\u00e4hst", "du", "nur", "nach", "Ei\u00b7nem", "Hal\u00b7me", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze Erde fiel dir ja zum Loos!", "tokens": ["Die", "gan\u00b7ze", "Er\u00b7de", "fiel", "dir", "ja", "zum", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verletze nicht die Tanne ob der Palme,", "tokens": ["Ver\u00b7let\u00b7ze", "nicht", "die", "Tan\u00b7ne", "ob", "der", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht ob des Blumenstrauchs das arme Moos!", "tokens": ["Nicht", "ob", "des", "Blu\u00b7men\u00b7strauchs", "das", "ar\u00b7me", "Moos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}