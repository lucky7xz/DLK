{"textgrid.poem.35578": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Am M\u00f6nchsberg", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo im Schatten herbstlicher Ulmen der verfallene Pfad hinabsinkt,", "tokens": ["Wo", "im", "Schat\u00b7ten", "herbst\u00b7li\u00b7cher", "Ul\u00b7men", "der", "ver\u00b7fal\u00b7le\u00b7ne", "Pfad", "hin\u00b7ab\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+---+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ferne den H\u00fctten von Laub, schlafenden Hirten,", "tokens": ["Fer\u00b7ne", "den", "H\u00fct\u00b7ten", "von", "Laub", ",", "schla\u00b7fen\u00b7den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Immer folgt dem Wandrer die dunkle Gestalt der K\u00fchle", "tokens": ["Im\u00b7mer", "folgt", "dem", "Wand\u00b7rer", "die", "dunk\u00b7le", "Ge\u00b7stalt", "der", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00dcber kn\u00f6chernen Steg, die hyazinthene Stimme des Knaben,", "tokens": ["\u00dc\u00b7ber", "kn\u00f6\u00b7cher\u00b7nen", "Steg", ",", "die", "hy\u00b7a\u00b7zin\u00b7the\u00b7ne", "Stim\u00b7me", "des", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Leise sagend die vergessene Legende des Walds,", "tokens": ["Lei\u00b7se", "sa\u00b7gend", "die", "ver\u00b7ges\u00b7se\u00b7ne", "Le\u00b7gen\u00b7de", "des", "Walds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Sanfter ein Krankes nun die wilde Klage des Bruders.", "tokens": ["Sanf\u00b7ter", "ein", "Kran\u00b7kes", "nun", "die", "wil\u00b7de", "Kla\u00b7ge", "des", "Bru\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Also r\u00fchrt ein sp\u00e4rliches Gr\u00fcn das Knie des Fremdlings,", "tokens": ["Al\u00b7so", "r\u00fchrt", "ein", "sp\u00e4r\u00b7li\u00b7ches", "Gr\u00fcn", "das", "Knie", "des", "Fremd\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das versteinerte Haupt;", "tokens": ["Das", "ver\u00b7stei\u00b7ner\u00b7te", "Haupt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "N\u00e4her rauscht der blaue Quell die Klage der Frauen.", "tokens": ["N\u00e4\u00b7her", "rauscht", "der", "blau\u00b7e", "Quell", "die", "Kla\u00b7ge", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}}}}