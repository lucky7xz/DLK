{"dta.poem.11105": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen verarmten gold-macher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein hau\u00dfrath ist ein topff, zwey gl\u00e4ser und drey tiegel,", "tokens": ["Dein", "hau\u00df\u00b7rath", "ist", "ein", "topff", ",", "zwey", "gl\u00e4\u00b7ser", "und", "drey", "tie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "CARD", "ADJA", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier pfund gehacktes bley, f\u00fcnff kolben und sechs ziegel,", "tokens": ["Vier", "pfund", "ge\u00b7hack\u00b7tes", "bley", ",", "f\u00fcnff", "kol\u00b7ben", "und", "sechs", "zie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$,", "CARD", "VVINF", "KON", "CARD", "NN", "$,"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein beutel ohne geld, ein schurtz mit staub gemahlt;", "tokens": ["Ein", "beu\u00b7tel", "oh\u00b7ne", "geld", ",", "ein", "schurtz", "mit", "staub", "ge\u00b7mahlt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "ADJD", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch aber reich genung vor den, der rauch bezahlt.", "tokens": ["Doch", "a\u00b7ber", "reich", "ge\u00b7nung", "vor", "den", ",", "der", "rauch", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "APPR", "ART", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}