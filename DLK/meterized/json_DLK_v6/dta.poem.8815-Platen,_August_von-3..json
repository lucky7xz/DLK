{"dta.poem.8815": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich f\u00fcr des Lebens M\u00fchen hab' erfleht zum Lohne dich,", "tokens": ["Da", "ich", "f\u00fcr", "des", "Le\u00b7bens", "M\u00fc\u00b7hen", "hab'", "er\u00b7fleht", "zum", "Loh\u00b7ne", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "VAFIN", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Welch ein Recht erwarb die Stunde, zu verstreichen ohne", "tokens": ["Welch", "ein", "Recht", "er\u00b7warb", "die", "Stun\u00b7de", ",", "zu", "ver\u00b7strei\u00b7chen", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "APPR"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Komm, o komm! Doch willst du ferne bleiben, sey auch", "tokens": ["Komm", ",", "o", "komm", "!", "Doch", "willst", "du", "fer\u00b7ne", "blei\u00b7ben", ",", "sey", "auch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "$.", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,", "VAFIN", "ADV"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Liebe, Liebe nur umgaukle, Friede nur umwohne dich!", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", "nur", "um\u00b7gauk\u00b7le", ",", "Frie\u00b7de", "nur", "um\u00b7woh\u00b7ne", "dich", "!"], "token_info": ["word"