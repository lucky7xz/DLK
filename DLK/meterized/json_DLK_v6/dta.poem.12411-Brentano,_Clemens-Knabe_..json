{"dta.poem.12411": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fche liebes Veilchen,               ", "tokens": ["Bl\u00fc\u00b7he", "lie\u00b7bes", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das so lieblich roch,", "tokens": ["Das", "so", "lieb\u00b7lich", "roch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fche noch ein Weilchen,", "tokens": ["Bl\u00fc\u00b7he", "noch", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werde sch\u00f6ner noch.", "tokens": ["Wer\u00b7de", "sch\u00f6\u00b7ner", "noch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Weist du was ich denke,", "tokens": ["Weist", "du", "was", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Liebchen zum Geschenke,", "tokens": ["Lieb\u00b7chen", "zum", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Pfl\u00fcck ich Veilchen dich,", "tokens": ["Pfl\u00fcck", "ich", "Veil\u00b7chen", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Veilchen freue dich!", "tokens": ["Veil\u00b7chen", "freu\u00b7e", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Brich mich stilles Veilchen,", "tokens": ["Brich", "mich", "stil\u00b7les", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bin die Liebste dein,", "tokens": ["Bin", "die", "Liebs\u00b7te", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und in einem Weilchen", "tokens": ["Und", "in", "ei\u00b7nem", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werd ich sch\u00f6ner seyn!", "tokens": ["Werd", "ich", "sch\u00f6\u00b7ner", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Weist du, was ich denke,", "tokens": ["Weist", "du", ",", "was", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn ich duftend schwenke", "tokens": ["Wenn", "ich", "duf\u00b7tend", "schwen\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Meinen Duft um dich:", "tokens": ["Mei\u00b7nen", "Duft", "um", "dich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Knabe liebe mich!", "tokens": ["Kna\u00b7be", "lie\u00b7be", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}