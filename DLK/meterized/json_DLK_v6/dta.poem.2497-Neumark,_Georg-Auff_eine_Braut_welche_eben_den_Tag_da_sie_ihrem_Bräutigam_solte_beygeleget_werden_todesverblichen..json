{"dta.poem.2497": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auff eine Braut/ welche eben den Tag/  \n da sie ihrem Br\u00e4utigam solte beygeleget  \n werden/ todesverblichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Auff eben diesen Tag da ich als eine Braut/", "tokens": ["Auff", "e\u00b7ben", "die\u00b7sen", "Tag", "da", "ich", "als", "ei\u00b7ne", "Braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "KOUS", "PPER", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu meinem Br\u00e4utigam/ solt\u2019 an die Seite kommen/", "tokens": ["Zu", "mei\u00b7nem", "Br\u00e4u\u00b7ti\u00b7gam", "/", "solt'", "an", "die", "Sei\u00b7te", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat mich der h\u00f6chste Gott gen\u00e4digst angeschaut/", "tokens": ["Hat", "mich", "der", "h\u00f6chs\u00b7te", "Gott", "ge\u00b7n\u00e4\u00b7digst", "an\u00b7ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mich von dieser Welt zur Liebsten auffgenommen.", "tokens": ["Und", "mich", "von", "die\u00b7ser", "Welt", "zur", "Liebs\u00b7ten", "auff\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}