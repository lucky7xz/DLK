unct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nun ist entglommen", "tokens": ["Nun", "ist", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVINF"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Des Fr\u00fchlings Segen.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Freund zieht weiter,", "tokens": ["Der", "Freund", "zieht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich seh' es heiter,", "tokens": ["Ich", "seh'", "es", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Denn er bleibt mein auf allen Wegen.", "tokens": ["Denn", "er", "bleibt", "mein", "auf", "al\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}