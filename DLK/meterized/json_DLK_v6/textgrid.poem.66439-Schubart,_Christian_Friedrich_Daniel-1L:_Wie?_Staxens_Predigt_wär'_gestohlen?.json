{"textgrid.poem.66439": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? Staxens Predigt w\u00e4r' gestohlen?", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? Staxens Predigt w\u00e4r' gestohlen?", "tokens": ["Wie", "?", "Sta\u00b7xens", "Pre\u00b7digt", "w\u00e4r'", "ge\u00b7stoh\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verleumdung ist's! Ich sag es frei!", "tokens": ["Ver\u00b7leum\u00b7dung", "ist's", "!", "Ich", "sag", "es", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lie\u00df, ich selber stund dabei,", "tokens": ["Er", "lie\u00df", ",", "ich", "sel\u00b7ber", "stund", "da\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr baares Geld sie aus dem Laden holen.", "tokens": ["F\u00fcr", "baa\u00b7res", "Geld", "sie", "aus", "dem", "La\u00b7den", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}