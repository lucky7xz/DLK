{"textgrid.poem.40161": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Gebet an die Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffe, hoffe! da\u00df auch Ich kann hoffen!", "tokens": ["Hof\u00b7fe", ",", "hof\u00b7fe", "!", "da\u00df", "auch", "Ich", "kann", "hof\u00b7fen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "KOUS", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schleicht der Winter schon in unser Leben,", "tokens": ["Schleicht", "der", "Win\u00b7ter", "schon", "in", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "das noch kaum ein Fr\u00fchlingsstrahl getroffen?", "tokens": ["das", "noch", "kaum", "ein", "Fr\u00fch\u00b7lings\u00b7strahl", "ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sahen ", "tokens": ["sa\u00b7hen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "da\u00df wir nun zu Grabe sollen streben?!", "tokens": ["da\u00df", "wir", "nun", "zu", "Gra\u00b7be", "sol\u00b7len", "stre\u00b7ben", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Glaube, glaube! nimm mir nicht den Segen,", "tokens": ["Glau\u00b7be", ",", "glau\u00b7be", "!", "nimm", "mir", "nicht", "den", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "VVIMP", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "da\u00df ich ", "tokens": ["da\u00df", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Oh, es geht sich schwer auf meinen Wegen:", "tokens": ["Oh", ",", "es", "geht", "sich", "schwer", "auf", "mei\u00b7nen", "We\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schnee und Eis starrt von den H\u00f6h'n entgegen,", "tokens": ["Schnee", "und", "Eis", "starrt", "von", "den", "H\u00f6h'n", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und im Abgrund g\u00e4hnen Finsternisse!", "tokens": ["und", "im", "Ab\u00b7grund", "g\u00e4h\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nein! von ", "tokens": ["Nein", "!", "von"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$.", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "mu\u00dft es selber f\u00fchlen, ob die Gluten", "tokens": ["mu\u00dft", "es", "sel\u00b7ber", "f\u00fch\u00b7len", ",", "ob", "die", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "dir empor zu heil'gen Flammen schlagen,", "tokens": ["dir", "em\u00b7por", "zu", "heil'\u00b7gen", "Flam\u00b7men", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "in der Lohe uns gen Himmel tragen,", "tokens": ["in", "der", "Lo\u00b7he", "uns", "gen", "Him\u00b7mel", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schnee und Eis zerschmilzt in ", "tokens": ["Schnee", "und", "Eis", "zer\u00b7schmilzt", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}