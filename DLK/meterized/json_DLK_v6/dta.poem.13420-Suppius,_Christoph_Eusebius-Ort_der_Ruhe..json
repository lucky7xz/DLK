{"dta.poem.13420": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Ort der Ruhe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Du g\u00fcldne Mittelm\u00e4ssigkeit,", "tokens": ["Du", "g\u00fcld\u00b7ne", "Mit\u00b7tel\u00b7m\u00e4s\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fort! hilf mir jenes Land beschreiben,", "tokens": ["Fort", "!", "hilf", "mir", "je\u00b7nes", "Land", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo meiner Tage stille Zeit", "tokens": ["Wo", "mei\u00b7ner", "Ta\u00b7ge", "stil\u00b7le", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichtkunst Werk soll ungehindert treiben,", "tokens": ["Der", "Dicht\u00b7kunst", "Werk", "soll", "un\u00b7ge\u00b7hin\u00b7dert", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf! meiner W\u00fcnsche K\u00f6nigin!", "tokens": ["Auf", "!", "mei\u00b7ner", "W\u00fcn\u00b7sche", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, f\u00fchre mich im Geist dahin,", "tokens": ["Komm", ",", "f\u00fch\u00b7re", "mich", "im", "Geist", "da\u00b7hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo rauschende Gew\u00e4sser wallen,", "tokens": ["Wo", "rau\u00b7schen\u00b7de", "Ge\u00b7w\u00e4s\u00b7ser", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Allwo ein ganz gelinder West", "tokens": ["All\u00b7wo", "ein", "ganz", "ge\u00b7lin\u00b7der", "West"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jm voraus mich versichern l\u00e4\u00dft;", "tokens": ["Jm", "vo\u00b7raus", "mich", "ver\u00b7si\u00b7chern", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df Musenlieder da wohl zehnfach wiederschallen.", "tokens": ["Da\u00df", "Mu\u00b7sen\u00b7lie\u00b7der", "da", "wohl", "zehn\u00b7fach", "wie\u00b7der\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}