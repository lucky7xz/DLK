{"textgrid.poem.67678": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "43. Philosophie", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich zu vergn\u00fcgen, wie es sei,", "tokens": ["Mich", "zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "wie", "es", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh, das ist mir Philosophei.", "tokens": ["Sieh", ",", "das", "ist", "mir", "Phi\u00b7lo\u00b7so\u00b7phei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPER", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mein Tagewerk ist abgethan,", "tokens": ["Mein", "Ta\u00b7ge\u00b7werk", "ist", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich mich meiner Lebensbahn", "tokens": ["Wenn", "ich", "mich", "mei\u00b7ner", "Le\u00b7bens\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So nach gerade", "tokens": ["So", "nach", "ge\u00b7ra\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Entlade.", "tokens": ["Ent\u00b7la\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}