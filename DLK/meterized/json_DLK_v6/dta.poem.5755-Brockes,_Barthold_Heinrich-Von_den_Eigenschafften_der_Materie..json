{"dta.poem.5755": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Darum, weil man nichts merckt, den Raum f\u00fcr leer zu", "tokens": ["Da\u00b7rum", ",", "weil", "man", "nichts", "merckt", ",", "den", "Raum", "f\u00fcr", "leer", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PIS", "PIS", "VVFIN", "$,", "ART", "NN", "APPR", "ADJD", "PTKZU"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "achten;", "tokens": ["ach\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ist gar ein kindischer und grober Unverstand.", "tokens": ["Ist", "gar", "ein", "kin\u00b7di\u00b7scher", "und", "gro\u00b7ber", "Un\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man mu\u00df die C\u00f6rper nicht betrachten", "tokens": ["Man", "mu\u00df", "die", "C\u00f6r\u00b7per", "nicht", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als f\u00fchlbar, als gef\u00e4rbt, nur blos, als ", "tokens": ["Als", "f\u00fchl\u00b7bar", ",", "als", "ge\u00b7f\u00e4rbt", ",", "nur", "blos", ",", "als"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "VVPP", "$,", "ADV", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht ob sie von Natur, gro\u00df oder klein,", "tokens": ["Nicht", "ob", "sie", "von", "Na\u00b7tur", ",", "gro\u00df", "o\u00b7der", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Grob, oder zart, und d\u00fcnne, seyn.", "tokens": ["Grob", ",", "o\u00b7der", "zart", ",", "und", "d\u00fcn\u00b7ne", ",", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ADJD", "$,", "KON", "VVFIN", "$,", "VAINF", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Es h\u00e4lt die Lufft, die sich so schnell beweget,", "tokens": ["Es", "h\u00e4lt", "die", "Lufft", ",", "die", "sich", "so", "schnell", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die unsichtbar, und die von solcher Leichtigkeit,", "tokens": ["Die", "un\u00b7sicht\u00b7bar", ",", "und", "die", "von", "sol\u00b7cher", "Leich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KON", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In einem Raum, der eines Fu\u00dfes breit,", "tokens": ["In", "ei\u00b7nem", "Raum", ",", "der", "ei\u00b7nes", "Fu\u00b7\u00dfes", "breit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht weniger Materie,", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "Ma\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Als wie ein Klumpen Bley von gleicher Gr\u00f6sse heget.", "tokens": ["Als", "wie", "ein", "Klum\u00b7pen", "Bley", "von", "glei\u00b7cher", "Gr\u00f6s\u00b7se", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}