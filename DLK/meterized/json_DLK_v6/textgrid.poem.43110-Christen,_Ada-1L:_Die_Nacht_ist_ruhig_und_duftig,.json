{"textgrid.poem.43110": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist ruhig und duftig,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist ruhig und duftig,", "tokens": ["Die", "Nacht", "ist", "ru\u00b7hig", "und", "duf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Luft weht lau und lind;", "tokens": ["Die", "Luft", "weht", "lau", "und", "lind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unter den Sternenaugen", "tokens": ["Un\u00b7ter", "den", "Ster\u00b7nen\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Such' ich die deinen, mein Kind!", "tokens": ["Such'", "ich", "die", "dei\u00b7nen", ",", "mein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte dich sehen und k\u00fcssen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "se\u00b7hen", "und", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein Einz'ges, das Alles mir gab,", "tokens": ["Mein", "Einz'\u00b7ges", ",", "das", "Al\u00b7les", "mir", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich m\u00f6chte still bei dir liegen", "tokens": ["Ich", "m\u00f6ch\u00b7te", "still", "bei", "dir", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im kleinen stillen Grab.", "tokens": ["Im", "klei\u00b7nen", "stil\u00b7len", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}