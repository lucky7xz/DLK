{"dta.poem.3608": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Liebscherz mit dem neugebornen  \n Kinde Maria .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann w\u00fcnschen w\u00e4r k\u00f6nnen, Maria rein,               ", "tokens": ["Wann", "w\u00fcn\u00b7schen", "w\u00e4r", "k\u00f6n\u00b7nen", ",", "Ma\u00b7ria", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "VAFIN", "VMINF", "$,", "NE", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So m\u00f6cht ich jezt wohl ein Baumeister seyn;", "tokens": ["So", "m\u00f6cht", "ich", "jezt", "wohl", "ein", "Bau\u00b7meis\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich w\u00fcnschte mir Salomons Sch\u00e4tze,", "tokens": ["Ich", "w\u00fcnschte", "mir", "Sa\u00b7lo\u00b7mons", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dukaten und Thaler viel Metzen,", "tokens": ["Du\u00b7ka\u00b7ten", "und", "Tha\u00b7ler", "viel", "Met\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Blos deinen Geburtsort zu ehren,", "tokens": ["Blos", "dei\u00b7nen", "Ge\u00b7burts\u00b7ort", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mein Andacht und Trost zu vermehren.", "tokens": ["Mein", "An\u00b7dacht", "und", "Trost", "zu", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich wollte dir bauen ein Kirchelein,", "tokens": ["Ich", "woll\u00b7te", "dir", "bau\u00b7en", "ein", "Kir\u00b7che\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das sollte mit Golde gepflastert seyn;", "tokens": ["Das", "soll\u00b7te", "mit", "Gol\u00b7de", "ge\u00b7pflas\u00b7tert", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Von Edelstein alle Gew\u00f6lbe,", "tokens": ["Von", "E\u00b7del\u00b7stein", "al\u00b7le", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der Altar, das w\u00e4re ich selber;", "tokens": ["Der", "Al\u00b7tar", ",", "das", "w\u00e4\u00b7re", "ich", "sel\u00b7ber", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mein Herze, das m\u00fcsse der Altardom seyn,", "tokens": ["Mein", "Her\u00b7ze", ",", "das", "m\u00fcs\u00b7se", "der", "Al\u00b7tar\u00b7dom", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "PDS", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Drauf m\u00fcssest du wohnen mein Kindelein.", "tokens": ["Drauf", "m\u00fcs\u00b7sest", "du", "woh\u00b7nen", "mein", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Mein Seel sollt ein g\u00fcldenes Rauchfa\u00df seyn,", "tokens": ["Mein", "Seel", "sollt", "ein", "g\u00fcl\u00b7de\u00b7nes", "Rauch\u00b7fa\u00df", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit dem ich dir t\u00e4glich wollt opfern sein,", "tokens": ["Mit", "dem", "ich", "dir", "t\u00e4g\u00b7lich", "wollt", "op\u00b7fern", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ADJD", "VMFIN", "VVINF", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gew\u00fcrzwerk, so viel dir behaget,", "tokens": ["Ge\u00b7w\u00fcrz\u00b7werk", ",", "so", "viel", "dir", "be\u00b7ha\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So viel ganz Arabia traget;", "tokens": ["So", "viel", "ganz", "A\u00b7ra\u00b7bia", "tra\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Menschen, die h\u00e4tt ich an einer Kett,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "h\u00e4tt", "ich", "an", "ei\u00b7ner", "Kett", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und jeder ein englische Stimmlein h\u00e4tt.", "tokens": ["Und", "je\u00b7der", "ein", "eng\u00b7li\u00b7sche", "Stimm\u00b7lein", "h\u00e4tt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Maria, du jezt ein Kindlein bist,", "tokens": ["Ma\u00b7ria", ",", "du", "jezt", "ein", "Kin\u00b7dlein", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sauget der heiligen Mutter Br\u00fcst;", "tokens": ["Das", "sau\u00b7get", "der", "hei\u00b7li\u00b7gen", "Mut\u00b7ter", "Br\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Kinder gern alles verschenken,", "tokens": ["Die", "Kin\u00b7der", "gern", "al\u00b7les", "ver\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Drum wollest auch meiner gedenken;", "tokens": ["Drum", "wol\u00b7lest", "auch", "mei\u00b7ner", "ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mein Grobheit, die wollest verzeihen,", "tokens": ["Mein", "Grob\u00b7heit", ",", "die", "wol\u00b7lest", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Viel Gnade daf\u00fcr mir verleihen.", "tokens": ["Viel", "Gna\u00b7de", "da\u00b7f\u00fcr", "mir", "ver\u00b7lei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn schlafest, so will ich aufwecken dich,", "tokens": ["Wenn", "schla\u00b7fest", ",", "so", "will", "ich", "auf\u00b7we\u00b7cken", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Thust weinen, so will ich erfreuen mich;", "tokens": ["Thust", "wei\u00b7nen", ",", "so", "will", "ich", "er\u00b7freu\u00b7en", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Engel, die werden dich stillen,", "tokens": ["Die", "En\u00b7gel", ",", "die", "wer\u00b7den", "dich", "stil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gott selber wird thun deinen Willen;", "tokens": ["Gott", "sel\u00b7ber", "wird", "thun", "dei\u00b7nen", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm opfre ein kleines Paar Z\u00e4hrlein,", "tokens": ["Ihm", "opf\u00b7re", "ein", "klei\u00b7nes", "Paar", "Z\u00e4hr\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es wird ihm viel lieber als Perlen seyn.", "tokens": ["Es", "wird", "ihm", "viel", "lie\u00b7ber", "als", "Per\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "KOUS", "NN", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}