{"textgrid.poem.39738": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zum 17. Februar 1822", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine Hoffnung und sein Sehnen", "tokens": ["Sei\u00b7ne", "Hoff\u00b7nung", "und", "sein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's, was an der Liebsten Fest", "tokens": ["Ist's", ",", "was", "an", "der", "Liebs\u00b7ten", "Fest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Seufzern, unter Thr\u00e4nen,", "tokens": ["Un\u00b7ter", "Seuf\u00b7zern", ",", "un\u00b7ter", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcnglings Leier t\u00f6nen l\u00e4\u00dft.", "tokens": ["J\u00fcng\u00b7lings", "Lei\u00b7er", "t\u00f6\u00b7nen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer in seines Weibes Arme,", "tokens": ["Wer", "in", "sei\u00b7nes", "Wei\u00b7bes", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwischen Kinderwiegen, ruht,", "tokens": ["Zwi\u00b7schen", "Kin\u00b7der\u00b7wie\u00b7gen", ",", "ruht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie kann der von Liebesharme", "tokens": ["Wie", "kann", "der", "von", "Lie\u00b7bes\u00b7har\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Singen und von Sehnsuchtsglut?", "tokens": ["Sin\u00b7gen", "und", "von", "Sehn\u00b7suchts\u00b7glut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber \u2013 Miteinander lieben", "tokens": ["A\u00b7ber", "\u2013", "Mi\u00b7tein\u00b7an\u00b7der", "lie\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$(", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lohnt es keinen Leierklang?", "tokens": ["Lohnt", "es", "kei\u00b7nen", "Lei\u00b7er\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil die Bl\u00fcte Frucht getrieben,", "tokens": ["Weil", "die", "Bl\u00fc\u00b7te", "Frucht", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt vom Baum kein Vogelsang?", "tokens": ["T\u00f6nt", "vom", "Baum", "kein", "Vo\u00b7gel\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Anders mag das Lied erschallen,", "tokens": ["An\u00b7ders", "mag", "das", "Lied", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber jubeln wird es doch.", "tokens": ["A\u00b7ber", "ju\u00b7beln", "wird", "es", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fchling w\u00fcrde nicht gefallen,", "tokens": ["Fr\u00fch\u00b7ling", "w\u00fcr\u00b7de", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Folgte nicht ein Sommer noch.", "tokens": ["Folg\u00b7te", "nicht", "ein", "Som\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Miteinander zu erstreben,", "tokens": ["Mi\u00b7tein\u00b7an\u00b7der", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Miteinander zu verstreun,", "tokens": ["Mi\u00b7tein\u00b7an\u00b7der", "zu", "ver\u00b7streun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu nehmen, und zu geben,", "tokens": ["Und", "zu", "neh\u00b7men", ",", "und", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nach Leide sich zu freun;", "tokens": ["Und", "nach", "Lei\u00b7de", "sich", "zu", "freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Miteinander zu verlachen", "tokens": ["Mi\u00b7tein\u00b7an\u00b7der", "zu", "ver\u00b7la\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stolz und Geiz der armen Zeit,", "tokens": ["Stolz", "und", "Geiz", "der", "ar\u00b7men", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eins das Andre zu bewachen", "tokens": ["Eins", "das", "And\u00b7re", "zu", "be\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PIS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem Strom der Eitelkeit; \u2013", "tokens": ["In", "dem", "Strom", "der", "Ei\u00b7tel\u00b7keit", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Miteinander zu entbrennen,", "tokens": ["Mi\u00b7tein\u00b7an\u00b7der", "zu", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo's die h\u00f6chsten G\u00fcter gilt,", "tokens": ["Wo's", "die", "h\u00f6chs\u00b7ten", "G\u00fc\u00b7ter", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins des Andern Herz zu kennen,", "tokens": ["Eins", "des", "An\u00b7dern", "Herz", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das von Freiheitsliebe schwillt,", "tokens": ["Das", "von", "Frei\u00b7heits\u00b7lie\u00b7be", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der t\u00fccht'gen Kinder Augen", "tokens": ["Aus", "der", "t\u00fccht'\u00b7gen", "Kin\u00b7der", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jugendlust und Hoffnungsmut,", "tokens": ["Ju\u00b7gend\u00b7lust", "und", "Hoff\u00b7nungs\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und f\u00fcrs Alter Trost zu saugen,", "tokens": ["Und", "f\u00fcrs", "Al\u00b7ter", "Trost", "zu", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zu flehn zum h\u00f6chsten Gut; \u2013", "tokens": ["Und", "zu", "flehn", "zum", "h\u00f6chs\u00b7ten", "Gut", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Miteinander so zu pflegen", "tokens": ["Mi\u00b7tein\u00b7an\u00b7der", "so", "zu", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden irdischen Gewinn,", "tokens": ["Je\u00b7den", "ir\u00b7di\u00b7schen", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sich kehrt beim Erdensegen", "tokens": ["Da\u00df", "sich", "kehrt", "beim", "Er\u00b7den\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem ew'gen Heil der Sinn;", "tokens": ["Zu", "dem", "ew'\u00b7gen", "Heil", "der", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn am freudenreichen Morgen", "tokens": ["Wenn", "am", "freu\u00b7den\u00b7rei\u00b7chen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Solch Gef\u00fchl ist Liedes werth:", "tokens": ["Solch", "Ge\u00b7f\u00fchl", "ist", "Lie\u00b7des", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun, so bleib' es nicht verborgen,", "tokens": ["Nun", ",", "so", "bleib'", "es", "nicht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Str\u00f6m' es aus am eignen Herd!", "tokens": ["Str\u00f6m'", "es", "aus", "am", "eig\u00b7nen", "Herd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}