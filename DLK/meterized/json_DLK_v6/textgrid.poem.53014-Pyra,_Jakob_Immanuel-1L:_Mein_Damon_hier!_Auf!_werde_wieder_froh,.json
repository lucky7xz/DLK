{"textgrid.poem.53014": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Damon hier! Auf! werde wieder froh,", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Damon hier! Auf! werde wieder froh,", "tokens": ["Mein", "Da\u00b7mon", "hier", "!", "Auf", "!", "wer\u00b7de", "wie\u00b7der", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$.", "APPR", "$.", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Komm, reich ihm selbst, o Muse, meine Faust,", "tokens": ["Komm", ",", "reich", "ihm", "selbst", ",", "o", "Mu\u00b7se", ",", "mei\u00b7ne", "Faust", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "PPER", "ADV", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit z\u00e4rtlich redlichem Vergn\u00fcgen,", "tokens": ["Mit", "z\u00e4rt\u00b7lich", "red\u00b7li\u00b7chem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hand, das Pfand der Treu, und hei\u00df ihn hier willkommen.", "tokens": ["Die", "Hand", ",", "das", "Pfand", "der", "Treu", ",", "und", "hei\u00df", "ihn", "hier", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "KON", "ADJD", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Liebkose dein und meinen Freund", "tokens": ["Lieb\u00b7ko\u00b7se", "dein", "und", "mei\u00b7nen", "Freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit unverstelltem frohen Schmeicheln,", "tokens": ["Mit", "un\u00b7ver\u00b7stell\u00b7tem", "fro\u00b7hen", "Schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und zeig ihm, was vor Lust in Brust und Adern wallt,", "tokens": ["Und", "zeig", "ihm", ",", "was", "vor", "Lust", "in", "Brust", "und", "A\u00b7dern", "wallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da du sein Angesicht nun wiederum erblickest.", "tokens": ["Da", "du", "sein", "An\u00b7ge\u00b7sicht", "nun", "wie\u00b7de\u00b7rum", "er\u00b7bli\u00b7ckest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O sieh, wie froh die arme Clio ist.", "tokens": ["O", "sieh", ",", "wie", "froh", "die", "ar\u00b7me", "Clio", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "ADJD", "ART", "ADJA", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh, liebster Freund, sieh, wie sie h\u00fcpft und springt,", "tokens": ["Sieh", ",", "liebs\u00b7ter", "Freund", ",", "sieh", ",", "wie", "sie", "h\u00fcpft", "und", "springt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sich vor Lust kaum wei\u00df zu lassen,", "tokens": ["Und", "sich", "vor", "Lust", "kaum", "wei\u00df", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich nicht satt kan sehn, da sie dich wieder siehet.", "tokens": ["Und", "sich", "nicht", "satt", "kan", "sehn", ",", "da", "sie", "dich", "wie\u00b7der", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie macht ihr Haar mit Blumen bunt,", "tokens": ["Sie", "macht", "ihr", "Haar", "mit", "Blu\u00b7men", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie holt ihr eingeschlafnes Spiel", "tokens": ["Sie", "holt", "ihr", "ein\u00b7ge\u00b7schlaf\u00b7nes", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von dem entlaubten Arm der magern Weide wieder.", "tokens": ["Von", "dem", "ent\u00b7laub\u00b7ten", "Arm", "der", "ma\u00b7gern", "Wei\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie stimmet es und spielt von nichts, als Lust und Jauchzen.", "tokens": ["Sie", "stim\u00b7met", "es", "und", "spielt", "von", "nichts", ",", "als", "Lust", "und", "Jauch\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So freudig ist die treue Galatee,", "tokens": ["So", "freu\u00b7dig", "ist", "die", "treu\u00b7e", "Ga\u00b7la\u00b7tee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie von ferne sieht, aus fremder Luft", "tokens": ["Wenn", "sie", "von", "fer\u00b7ne", "sieht", ",", "aus", "frem\u00b7der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den liebsten Sch\u00e4fer wieder kommen.", "tokens": ["Den", "liebs\u00b7ten", "Sch\u00e4\u00b7fer", "wie\u00b7der", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ruft und eilet ihm mit offnen Arm entgegen,", "tokens": ["Sie", "ruft", "und", "ei\u00b7let", "ihm", "mit", "off\u00b7nen", "Arm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und schl\u00e4gt ihr fliegend Haar zur\u00fcck,", "tokens": ["Und", "schl\u00e4gt", "ihr", "flie\u00b7gend", "Haar", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn sie ihn erreichet hat,", "tokens": ["Und", "wenn", "sie", "ihn", "er\u00b7rei\u00b7chet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Umf\u00e4ngt sie ihn und k\u00fc\u00dft und streichelt Mund und Wangen,", "tokens": ["Um\u00b7f\u00e4ngt", "sie", "ihn", "und", "k\u00fc\u00dft", "und", "strei\u00b7chelt", "Mund", "und", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und straft ihn oft, da\u00df er so lange ausgeblieben.", "tokens": ["Und", "straft", "ihn", "oft", ",", "da\u00df", "er", "so", "lan\u00b7ge", "aus\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O wie betr\u00fcbt war meine Poesie,", "tokens": ["O", "wie", "be\u00b7tr\u00fcbt", "war", "mei\u00b7ne", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da du den Fu\u00df aus unsern Gr\u00e4ntzen trugst,", "tokens": ["Da", "du", "den", "Fu\u00df", "aus", "un\u00b7sern", "Gr\u00e4nt\u00b7zen", "trugst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie warf Lust, Krantz, und Laute nieder,", "tokens": ["Sie", "warf", "Lust", ",", "Krantz", ",", "und", "Lau\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlo\u00df die Hand nicht mehr mit denen Sch\u00e4fferinnen", "tokens": ["Und", "schlo\u00df", "die", "Hand", "nicht", "mehr", "mit", "de\u00b7nen", "Sch\u00e4f\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "PRELS", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zum Tantz in einen Circkel ein,", "tokens": ["Zum", "Tantz", "in", "ei\u00b7nen", "Cir\u00b7ckel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sa\u00df an dem gelehrten Bach,", "tokens": ["Sie", "sa\u00df", "an", "dem", "ge\u00b7lehr\u00b7ten", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Worinn die Thr\u00e4nen stets von ihren Wangen fielen,", "tokens": ["Wo\u00b7rinn", "die", "Thr\u00e4\u00b7nen", "stets", "von", "ih\u00b7ren", "Wan\u00b7gen", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seufzte, Damon, nur, und Thal und Wald ruft Damon.", "tokens": ["Und", "seufz\u00b7te", ",", "Da\u00b7mon", ",", "nur", ",", "und", "Thal", "und", "Wald", "ruft", "Da\u00b7mon", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NE", "$,", "ADV", "$,", "KON", "NN", "KON", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch nun singt sie mit froh und hellerm Ton,", "tokens": ["Doch", "nun", "singt", "sie", "mit", "froh", "und", "hel\u00b7lerm", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da du sie wieder h\u00f6rst, du der sie sonst geliebt,", "tokens": ["Da", "du", "sie", "wie\u00b7der", "h\u00f6rst", ",", "du", "der", "sie", "sonst", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "PPER", "ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, ja, ich seh es schon im Geiste,", "tokens": ["Und", ",", "ja", ",", "ich", "seh", "es", "schon", "im", "Geis\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wir in Einigkeit mit Liedern streiten werden,", "tokens": ["Wie", "wir", "in", "Ei\u00b7nig\u00b7keit", "mit", "Lie\u00b7dern", "strei\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "NN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn du den Sitz bewohnen wirst,", "tokens": ["Wenn", "du", "den", "Sitz", "be\u00b7woh\u00b7nen", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "(wozu mein Mund dir Gl\u00fccke w\u00fcnscht,)", "tokens": ["(", "wo\u00b7zu", "mein", "Mund", "dir", "Gl\u00fc\u00b7cke", "w\u00fcnscht", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo nach dem Garten sich bebl\u00fcmte Wiesen breiten,", "tokens": ["Wo", "nach", "dem", "Gar\u00b7ten", "sich", "be\u00b7bl\u00fcm\u00b7te", "Wie\u00b7sen", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An die das W\u00e4ldchen schlie\u00dft, wodurch ein Fl\u00fc\u00dfchen murmelt.", "tokens": ["An", "die", "das", "W\u00e4ld\u00b7chen", "schlie\u00dft", ",", "wo\u00b7durch", "ein", "Fl\u00fc\u00df\u00b7chen", "mur\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}