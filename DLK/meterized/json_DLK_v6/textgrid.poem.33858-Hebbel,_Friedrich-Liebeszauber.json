{"textgrid.poem.33858": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Liebeszauber", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00fcl wird diese Nacht. Am Himmelsbogen", "tokens": ["Schw\u00fcl", "wird", "die\u00b7se", "Nacht", ".", "Am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PDAT", "NN", "$.", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zieh'n die Wolken dichter sich zusammen,", "tokens": ["Zieh'n", "die", "Wol\u00b7ken", "dich\u00b7ter", "sich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Breit begl\u00e4nzt von Wetterleuchtens Flammen", "tokens": ["Breit", "be\u00b7gl\u00e4nzt", "von", "Wet\u00b7ter\u00b7leuch\u00b7tens", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und von rothen Blitzen scharf durchzogen.", "tokens": ["Und", "von", "ro\u00b7then", "Blit\u00b7zen", "scharf", "durch\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Alles Leben ist in sich verschlossen,", "tokens": ["Al\u00b7les", "Le\u00b7ben", "ist", "in", "sich", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kaum nur, da\u00df ich m\u00fchsam Athem hole;", "tokens": ["Kaum", "nur", ",", "da\u00df", "ich", "m\u00fch\u00b7sam", "A\u00b7them", "ho\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Selbst im Beete dort die Nachtviole", "tokens": ["Selbst", "im", "Bee\u00b7te", "dort", "die", "Nacht\u00b7vi\u00b7o\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat den s\u00fc\u00dfen Duft noch nicht ergossen.", "tokens": ["Hat", "den", "s\u00fc\u00b7\u00dfen", "Duft", "noch", "nicht", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jedes Auge w\u00e4r' schon zugefallen,", "tokens": ["Je\u00b7des", "Au\u00b7ge", "w\u00e4r'", "schon", "zu\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch die Herzen sind voll Angst und zittern", "tokens": ["Doch", "die", "Her\u00b7zen", "sind", "voll", "Angst", "und", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor den zwei sich kreuzenden Gewittern,", "tokens": ["Vor", "den", "zwei", "sich", "kreu\u00b7zen\u00b7den", "Ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "PRF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deren Donnergr\u00fc\u00dfe bald erschallen.", "tokens": ["De\u00b7ren", "Don\u00b7ner\u00b7gr\u00fc\u00b7\u00dfe", "bald", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jene Alte schleppt sich zur Kapelle,", "tokens": ["Je\u00b7ne", "Al\u00b7te", "schleppt", "sich", "zur", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch sie wird den Heil'gen nicht erblicken,", "tokens": ["Doch", "sie", "wird", "den", "Heil'\u00b7gen", "nicht", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eh' die Wolken ihre Blitze schicken,", "tokens": ["Eh'", "die", "Wol\u00b7ken", "ih\u00b7re", "Blit\u00b7ze", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Betend kauert sie sich auf der Schwelle.", "tokens": ["Be\u00b7tend", "kau\u00b7ert", "sie", "sich", "auf", "der", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ist das nicht des Liebchens taube Muhme?", "tokens": ["Ist", "das", "nicht", "des", "Lieb\u00b7chens", "tau\u00b7be", "Muh\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ja! So will ich hier nicht l\u00e4nger weilen,", "tokens": ["Ja", "!", "So", "will", "ich", "hier", "nicht", "l\u00e4n\u00b7ger", "wei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Will zu ihr, zu ihrem Fenster eilen,", "tokens": ["Will", "zu", "ihr", ",", "zu", "ih\u00b7rem", "Fens\u00b7ter", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dort lauschen, statt am Heiligthume.", "tokens": ["Und", "dort", "lau\u00b7schen", ",", "statt", "am", "Hei\u00b7lig\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "KOUI", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wei\u00df ich's denn? Kann nicht ein Blitz da z\u00fcnden?", "tokens": ["Wei\u00df", "ich's", "denn", "?", "Kann", "nicht", "ein", "Blitz", "da", "z\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$.", "VMFIN", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kann ich, wenn ich aus der Glut sie rette,", "tokens": ["Kann", "ich", ",", "wenn", "ich", "aus", "der", "Glut", "sie", "ret\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht \u2013 o da\u00df er schon gez\u00fcndet h\u00e4tte! \u2013", "tokens": ["Nicht", "\u2013", "o", "da\u00df", "er", "schon", "ge\u00b7z\u00fcn\u00b7det", "h\u00e4t\u00b7te", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "$(", "FM", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$.", "$("], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Ihr mein s\u00fc\u00df Geheimni\u00df endlich k\u00fcnden?", "tokens": ["Ihr", "mein", "s\u00fc\u00df", "Ge\u00b7heim\u00b7ni\u00df", "end\u00b7lich", "k\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Sieh, da bin ich schon! Bei'm Lampenlichte", "tokens": ["Sieh", ",", "da", "bin", "ich", "schon", "!", "Bei'm", "Lam\u00b7pen\u00b7lich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADV", "$.", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sitzt sie, in die wei\u00dfe Hand das K\u00f6pfchen", "tokens": ["Sitzt", "sie", ",", "in", "die", "wei\u00b7\u00dfe", "Hand", "das", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "St\u00fctzend, mit noch aufgeflochtnen Z\u00f6pfchen,", "tokens": ["St\u00fct\u00b7zend", ",", "mit", "noch", "auf\u00b7ge\u00b7flocht\u00b7nen", "Z\u00f6pf\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stillen Schmerz im blassen Angesichte.", "tokens": ["Stil\u00b7len", "Schmerz", "im", "blas\u00b7sen", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Horch', der erste Donnerschlag! Es krachen", "tokens": ["Horch'", ",", "der", "ers\u00b7te", "Don\u00b7ner\u00b7schlag", "!", "Es", "kra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Th\u00fcr und Thor! Sie scheint es nicht zu h\u00f6ren!", "tokens": ["Th\u00fcr", "und", "Thor", "!", "Sie", "scheint", "es", "nicht", "zu", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wessen denkt sie? W\u00fc\u00dft' ich's, w\u00fcrd' ich schw\u00f6ren:", "tokens": ["Wes\u00b7sen", "denkt", "sie", "?", "W\u00fc\u00dft'", "ich's", ",", "w\u00fcrd'", "ich", "schw\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "VVFIN", "PIS", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heut' noch will ich den Garaus ihm machen.", "tokens": ["Heut'", "noch", "will", "ich", "den", "Ga\u00b7raus", "ihm", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Sie erlebt sich. Willst du dich entkleiden?", "tokens": ["Sie", "er\u00b7lebt", "sich", ".", "Willst", "du", "dich", "ent\u00b7klei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gute Nacht! Warum? Zur rechten Stunde", "tokens": ["Gu\u00b7te", "Nacht", "!", "Wa\u00b7rum", "?", "Zur", "rech\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWAV", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00f6scht sie selbst das Licht, und giebt dir Kunde:", "tokens": ["L\u00f6scht", "sie", "selbst", "das", "Licht", ",", "und", "giebt", "dir", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mehr ist nicht erlaubt! Dann magst du scheiden!", "tokens": ["Mehr", "ist", "nicht", "er\u00b7laubt", "!", "Dann", "magst", "du", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "VVPP", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Was? Sie kn\u00fcpft ein Tuch um ihre Locken?", "tokens": ["Was", "?", "Sie", "kn\u00fcpft", "ein", "Tuch", "um", "ih\u00b7re", "Lo\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00fcllt sich in der Muhme alten Mantel?", "tokens": ["H\u00fcllt", "sich", "in", "der", "Muh\u00b7me", "al\u00b7ten", "Man\u00b7tel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist sie \u2013 Oder stach mich die Tarantel?", "tokens": ["Ist", "sie", "\u2013", "O\u00b7der", "stach", "mich", "die", "Ta\u00b7ran\u00b7tel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird sie \u2013 Die Besinnung will mir stocken?", "tokens": ["Wird", "sie", "\u2013", "Die", "Be\u00b7sin\u00b7nung", "will", "mir", "sto\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Ja, schon knarrt die Th\u00fcr. Da kommt sie. Nimmer", "tokens": ["Ja", ",", "schon", "knarrt", "die", "Th\u00fcr.", "Da", "kommt", "sie", ".", "Nim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "abbreviation", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "VVFIN", "PPER", "$.", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "W\u00fcrd' ich selbst sie, so vermummt, erkennen,", "tokens": ["W\u00fcrd'", "ich", "selbst", "sie", ",", "so", "ver\u00b7mummt", ",", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "$,", "ADV", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00e4tt' ich nicht \u2013 \u2013 Die Lampe l\u00e4\u00dft man brennen,", "tokens": ["H\u00e4tt'", "ich", "nicht", "\u2013", "\u2013", "Die", "Lam\u00b7pe", "l\u00e4\u00dft", "man", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "$(", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df es scheint, man sei im frommen Zimmer.", "tokens": ["Da\u00df", "es", "scheint", ",", "man", "sei", "im", "from\u00b7men", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PIS", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Rasch an mir vorbei! Sie ist, wie Alle!", "tokens": ["Rasch", "an", "mir", "vor\u00b7bei", "!", "Sie", "ist", ",", "wie", "Al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "$,", "PWAV", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Folg' ich ihr? Ja freilich! Um zu schauen,", "tokens": ["Fol\u00b7g'", "ich", "ihr", "?", "Ja", "frei\u00b7lich", "!", "Um", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$.", "PTKANT", "ADV", "$.", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob man ihr mit braunen oder blauen", "tokens": ["Ob", "man", "ihr", "mit", "brau\u00b7nen", "o\u00b7der", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ADJA", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Augen \u2013 schwarze hab' ich selbst \u2013 gefalle.", "tokens": ["Au\u00b7gen", "\u2013", "schwar\u00b7ze", "hab'", "ich", "selbst", "\u2013", "ge\u00b7fal\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "VAFIN", "PPER", "ADV", "$(", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Waldhorn-Kl\u00e4nge aus dem J\u00e4gerh\u00e4uschen!", "tokens": ["Wald\u00b7horn\u00b7Kl\u00e4n\u00b7ge", "aus", "dem", "J\u00e4\u00b7ger\u00b7h\u00e4usc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bei'm Gewitter? O, das ist ein Zeichen!", "tokens": ["Bei'm", "Ge\u00b7wit\u00b7ter", "?", "O", ",", "das", "ist", "ein", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NE", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So ist das der J\u00fcngling sonder Gleichen?", "tokens": ["So", "ist", "das", "der", "J\u00fcng\u00b7ling", "son\u00b7der", "Glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohl! Doch n\u00e4chstens pfl\u00fccken wir ein Str\u00e4u\u00dfchen.", "tokens": ["Wohl", "!", "Doch", "n\u00e4chs\u00b7tens", "pfl\u00fc\u00b7cken", "wir", "ein", "Str\u00e4u\u00df\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Und weshalb? Hat sie dir was versprochen?", "tokens": ["Und", "we\u00b7shalb", "?", "Hat", "sie", "dir", "was", "ver\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "VAFIN", "PPER", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nein! Und dennoch mu\u00df ich sie verklagen,", "tokens": ["Nein", "!", "Und", "den\u00b7noch", "mu\u00df", "ich", "sie", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df sie, ja, so darf, so darf ich sagen,", "tokens": ["Da\u00df", "sie", ",", "ja", ",", "so", "darf", ",", "so", "darf", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PTKANT", "$,", "ADV", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einen stillen Bund mit mir gebrochen.", "tokens": ["Ei\u00b7nen", "stil\u00b7len", "Bund", "mit", "mir", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Weiter! Weiter? So vergieb, Geliebte!", "tokens": ["Wei\u00b7ter", "!", "Wei\u00b7ter", "?", "So", "ver\u00b7gieb", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "NN", "$.", "ADV", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch wohin? Hier zieht der Wald sich d\u00fcster,", "tokens": ["Doch", "wo\u00b7hin", "?", "Hier", "zieht", "der", "Wald", "sich", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und dort wohnt die Alte an der R\u00fcster,", "tokens": ["Und", "dort", "wohnt", "die", "Al\u00b7te", "an", "der", "R\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die in mancher dunklen Kunst ge\u00fcbte.", "tokens": ["Die", "in", "man\u00b7cher", "dunk\u00b7len", "Kunst", "ge\u00b7\u00fcb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Gilt es der? Halt ein! Dein Herz mu\u00df klopfen!", "tokens": ["Gilt", "es", "der", "?", "Halt", "ein", "!", "Dein", "Herz", "mu\u00df", "klop\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$.", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Rastlos donnert's ja, zur Feuergarbe", "tokens": ["Rast\u00b7los", "don\u00b7nert's", "ja", ",", "zur", "Feu\u00b7er\u00b7gar\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "ADV", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schwillt der Blitz, blutroth wird seine Farbe,", "tokens": ["Schwillt", "der", "Blitz", ",", "blut\u00b7roth", "wird", "sei\u00b7ne", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und noch immer f\u00e4llt kein milder Tropfen.", "tokens": ["Und", "noch", "im\u00b7mer", "f\u00e4llt", "kein", "mil\u00b7der", "Trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "Fort! Und fort! Und unter falschen B\u00e4umen,", "tokens": ["Fort", "!", "Und", "fort", "!", "Und", "un\u00b7ter", "fal\u00b7schen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PTKVZ", "$.", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die der Blitz \u2013 \u2013 Ihr n\u00e4her! da\u00df sie keiner", "tokens": ["Die", "der", "Blitz", "\u2013", "\u2013", "Ihr", "n\u00e4\u00b7her", "!", "da\u00df", "sie", "kei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "$(", "$(", "PPER", "ADJD", "$.", "KOUS", "PPER", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Treffen kann, der mich verschont, nicht einer!", "tokens": ["Tref\u00b7fen", "kann", ",", "der", "mich", "ver\u00b7schont", ",", "nicht", "ei\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKNEG", "ART", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schritt auf Schritt ihr nach! Wer w\u00fcrde s\u00e4umen!", "tokens": ["Schritt", "auf", "Schritt", "ihr", "nach", "!", "Wer", "w\u00fcr\u00b7de", "s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "PTKVZ", "$.", "PWS", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Ist sie nun am Ziel? Da ist die H\u00fctte!", "tokens": ["Ist", "sie", "nun", "am", "Ziel", "?", "Da", "ist", "die", "H\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ja, sie pocht. Man \u00f6ffnet ihr. Ich sp\u00e4he", "tokens": ["Ja", ",", "sie", "pocht", ".", "Man", "\u00f6ff\u00b7net", "ihr", ".", "Ich", "sp\u00e4\u00b7he"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$.", "PIS", "VVFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch den Ritz. Wer wei\u00df, was ihr gesch\u00e4he,", "tokens": ["Durch", "den", "Ritz", ".", "Wer", "wei\u00df", ",", "was", "ihr", "ge\u00b7sch\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PWS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ich nicht \u2013 \u2013 Ein Kreis! Sie in der Mitte!", "tokens": ["Wenn", "ich", "nicht", "\u2013", "\u2013", "Ein", "Kreis", "!", "Sie", "in", "der", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "$(", "$(", "ART", "NN", "$.", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.19": {"line.1": {"text": "Wie sie da steht, fast zum Schnee erbleichend,", "tokens": ["Wie", "sie", "da", "steht", ",", "fast", "zum", "Schnee", "er\u00b7blei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und die Alte, in der Ecke kauernd,", "tokens": ["Und", "die", "Al\u00b7te", ",", "in", "der", "E\u00b7cke", "kau\u00b7ernd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dreht ein Bild aus Wachs. Sie sieht es schauernd.", "tokens": ["Dreht", "ein", "Bild", "aus", "Wachs", ".", "Sie", "sieht", "es", "schau\u00b7ernd", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jetzt spricht die zu ihr, das Bild ihr reichend:", "tokens": ["Jetzt", "spricht", "die", "zu", "ihr", ",", "das", "Bild", "ihr", "rei\u00b7chend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "PPER", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.20": {"line.1": {"text": "Zieh dir nun die Nadel aus den Haaren,", "tokens": ["Zieh", "dir", "nun", "die", "Na\u00b7del", "aus", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rufe den Geliebten, laut und deutlich,", "tokens": ["Ru\u00b7fe", "den", "Ge\u00b7lieb\u00b7ten", ",", "laut", "und", "deut\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und durchstich dies Bild, dann wirst du br\u00e4utlich", "tokens": ["Und", "durch\u00b7stich", "dies", "Bild", ",", "dann", "wirst", "du", "br\u00e4ut\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PDS", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ihn umfangen und ihn dir bewahren.", "tokens": ["Ihn", "um\u00b7fan\u00b7gen", "und", "ihn", "dir", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.21": {"line.1": {"text": "Schweigt, ihr Donner! Pra\u00dfle noch nicht, Regen,", "tokens": ["Schweigt", ",", "ihr", "Don\u00b7ner", "!", "Pra\u00df\u00b7le", "noch", "nicht", ",", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "NN", "ADV", "PTKNEG", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich noch den Einen Laut vernehme,", "tokens": ["Da\u00df", "ich", "noch", "den", "Ei\u00b7nen", "Laut", "ver\u00b7neh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ART", "APPR", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ob er auch des Herzens Schlag mir l\u00e4hme", "tokens": ["Ob", "er", "auch", "des", "Her\u00b7zens", "Schlag", "mir", "l\u00e4h\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Pulse feuriges Bewegen!", "tokens": ["Und", "der", "Pul\u00b7se", "feu\u00b7ri\u00b7ges", "Be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.22": {"line.1": {"text": "Wie sie z\u00f6gert! Wie sie mit Err\u00f6then", "tokens": ["Wie", "sie", "z\u00f6\u00b7gert", "!", "Wie", "sie", "mit", "Er\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PWAV", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die Locken greift und eine Nadel", "tokens": ["In", "die", "Lo\u00b7cken", "greift", "und", "ei\u00b7ne", "Na\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auszieht auf der Alten stummen Tadel", "tokens": ["Aus\u00b7zieht", "auf", "der", "Al\u00b7ten", "stum\u00b7men", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und noch s\u00e4umt, als g\u00e4lte es, zu t\u00f6dten!", "tokens": ["Und", "noch", "s\u00e4umt", ",", "als", "g\u00e4l\u00b7te", "es", ",", "zu", "t\u00f6d\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.23": {"line.1": {"text": "Endlich z\u00fcckt sie die, und \u2013 meine Sinne", "tokens": ["End\u00b7lich", "z\u00fcckt", "sie", "die", ",", "und", "\u2013", "mei\u00b7ne", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "$,", "KON", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rei\u00dfen! \u2013 ruft \u2013 hinein! Zu ihren F\u00fc\u00dfen! \u2013", "tokens": ["Rei\u00b7\u00dfen", "!", "\u2013", "ruft", "\u2013", "hin\u00b7ein", "!", "Zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "VVFIN", "$(", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ruft mich selbst mit Worten, stammelnd-s\u00fc\u00dfen,", "tokens": ["Ruft", "mich", "selbst", "mit", "Wor\u00b7ten", ",", "stam\u00b7meln\u00b7ds\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als den Einen, den sie heimlich minne! \u2013", "tokens": ["Als", "den", "Ei\u00b7nen", ",", "den", "sie", "heim\u00b7lich", "min\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "Und dem Zagen kommt der Muth, behende", "tokens": ["Und", "dem", "Za\u00b7gen", "kommt", "der", "Muth", ",", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weicht die Th\u00fcr. Wer durfte sich erfrechen,", "tokens": ["Weicht", "die", "Th\u00fcr.", "Wer", "durf\u00b7te", "sich", "er\u00b7fre\u00b7chen", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PWS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ruft die Alte, und den Zauber brechen? \u2013", "tokens": ["Ruft", "die", "Al\u00b7te", ",", "und", "den", "Zau\u00b7ber", "bre\u00b7chen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ohne Furcht! Hier kommt nur, der ihn ende!", "tokens": ["Oh\u00b7ne", "Furcht", "!", "Hier", "kommt", "nur", ",", "der", "ihn", "en\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADV", "VVFIN", "ADV", "$,", "PRELS", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.25": {"line.1": {"text": "Sie entweicht mit holden Schaam-Geberden;", "tokens": ["Sie", "ent\u00b7weicht", "mit", "hol\u00b7den", "Schaam\u00b7Ge\u00b7ber\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da umschlie\u00dft er sie, und Glut und Sehnen", "tokens": ["Da", "um\u00b7schlie\u00dft", "er", "sie", ",", "und", "Glut", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "NN", "KON", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "L\u00f6s't bei Beiden sich in linden Thr\u00e4nen,", "tokens": ["L\u00f6s't", "bei", "Bei\u00b7den", "sich", "in", "lin\u00b7den", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Mensch nur einmal weint auf Erden.", "tokens": ["Die", "der", "Mensch", "nur", "ein\u00b7mal", "weint", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.26": {"line.1": {"text": "Und so steh'n sie, wechseln keine K\u00fcsse,", "tokens": ["Und", "so", "steh'n", "sie", ",", "wech\u00b7seln", "kei\u00b7ne", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Still ges\u00e4ttigt und in sich versunken,", "tokens": ["Still", "ge\u00b7s\u00e4t\u00b7tigt", "und", "in", "sich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schon berauscht, bevor sie noch getrunken,", "tokens": ["Schon", "be\u00b7rauscht", ",", "be\u00b7vor", "sie", "noch", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In der Ahnung d\u00e4mmernder Gen\u00fcsse.", "tokens": ["In", "der", "Ah\u00b7nung", "d\u00e4m\u00b7mern\u00b7der", "Ge\u00b7n\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.27": {"line.1": {"text": "Und auch drau\u00dfen l\u00f6s't sich jetzt die Schw\u00fcle,", "tokens": ["Und", "auch", "drau\u00b7\u00dfen", "l\u00f6s't", "sich", "jetzt", "die", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die zerri\u00dfnen Wolken, Regen schwanger,", "tokens": ["Die", "zer\u00b7ri\u00df\u00b7nen", "Wol\u00b7ken", ",", "Re\u00b7gen", "schwan\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sch\u00fctten ihn herab auf Hain und Anger,", "tokens": ["Sch\u00fct\u00b7ten", "ihn", "her\u00b7ab", "auf", "Hain", "und", "An\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und hinein zur H\u00fctte dringt die K\u00fchle.", "tokens": ["Und", "hin\u00b7ein", "zur", "H\u00fct\u00b7te", "dringt", "die", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.28": {"line.1": {"text": "Als nun auch der Regen ausgew\u00fcthet,", "tokens": ["Als", "nun", "auch", "der", "Re\u00b7gen", "aus\u00b7ge\u00b7w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wallen sie, die Alte gern verlassend,", "tokens": ["Wal\u00b7len", "sie", ",", "die", "Al\u00b7te", "gern", "ver\u00b7las\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kinderfromm sich an den H\u00e4nden fassend,", "tokens": ["Kin\u00b7der\u00b7fromm", "sich", "an", "den", "H\u00e4n\u00b7den", "fas\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wieder heim, von Engeln still beh\u00fctet.", "tokens": ["Wie\u00b7der", "heim", ",", "von", "En\u00b7geln", "still", "be\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.29": {"line.1": {"text": "Als sie aber scheiden will, da ziehen", "tokens": ["Als", "sie", "a\u00b7ber", "schei\u00b7den", "will", ",", "da", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gl\u00fchendhei\u00df die Nachtviolend\u00fcfte", "tokens": ["Gl\u00fc\u00b7hend\u00b7hei\u00df", "die", "Nacht\u00b7vi\u00b7o\u00b7len\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An ihm hin im sanften Spiel der L\u00fcfte,", "tokens": ["An", "ihm", "hin", "im", "sanf\u00b7ten", "Spiel", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nun k\u00fc\u00dft er sie noch im Entfliehen.", "tokens": ["Und", "nun", "k\u00fc\u00dft", "er", "sie", "noch", "im", "Ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}