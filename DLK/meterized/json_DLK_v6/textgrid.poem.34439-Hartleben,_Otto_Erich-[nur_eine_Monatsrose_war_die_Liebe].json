{"textgrid.poem.34439": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[nur eine Monatsrose war die Liebe]", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur eine Monatsrose war die Liebe,", "tokens": ["Nur", "ei\u00b7ne", "Mo\u00b7nats\u00b7ro\u00b7se", "war", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die deine Wangen f\u00e4rbte. \u2013 Ihr Verbl\u00fchn", "tokens": ["die", "dei\u00b7ne", "Wan\u00b7gen", "f\u00e4rb\u00b7te", ".", "\u2013", "Ihr", "Ver\u00b7bl\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "hab ich voll tiefen, tiefen Leids gesehn,", "tokens": ["hab", "ich", "voll", "tie\u00b7fen", ",", "tie\u00b7fen", "Leids", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "und meine Thr\u00e4nen netzten jedes Blatt,", "tokens": ["und", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "netz\u00b7ten", "je\u00b7des", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "das bald zu Boden sank und bald der Wind", "tokens": ["das", "bald", "zu", "Bo\u00b7den", "sank", "und", "bald", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "verwehte. \u2013 Meinen Augen hast du nun", "tokens": ["ver\u00b7weh\u00b7te", ".", "\u2013", "Mei\u00b7nen", "Au\u00b7gen", "hast", "du", "nun"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "den Schmuck geraubt, der ihnen wohlgethan,", "tokens": ["den", "Schmuck", "ge\u00b7raubt", ",", "der", "ih\u00b7nen", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und meinem Leben raubtest du das Ziel ...", "tokens": ["und", "mei\u00b7nem", "Le\u00b7ben", "raub\u00b7test", "du", "das", "Ziel", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}