{"textgrid.poem.39684": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00fcze, o sinnende Muse! mir gn\u00e4dig die \u00e4rmlichen Bl\u00e4tter!", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00fcze, o sinnende Muse! mir gn\u00e4dig die \u00e4rmlichen Bl\u00e4tter!", "tokens": ["Sch\u00fc\u00b7ze", ",", "o", "sin\u00b7nen\u00b7de", "Mu\u00b7se", "!", "mir", "gn\u00e4\u00b7dig", "die", "\u00e4rm\u00b7li\u00b7chen", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$.", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "F\u00fclle des Lorbeers bringt reichlich der lauere S\u00fcd,", "tokens": ["F\u00fcl\u00b7le", "des", "Lor\u00b7beers", "bringt", "reich\u00b7lich", "der", "lau\u00b7e\u00b7re", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Aber den Norden umziehn die St\u00fcrme und eisichte Regen;", "tokens": ["A\u00b7ber", "den", "Nor\u00b7den", "um\u00b7ziehn", "die", "St\u00fcr\u00b7me", "und", "ei\u00b7sich\u00b7te", "Re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sparsamer sprie\u00dfen empor Bl\u00fcthen aus d\u00fcrftiger Aue.", "tokens": ["Spar\u00b7sa\u00b7mer", "sprie\u00b7\u00dfen", "em\u00b7por", "Bl\u00fc\u00b7then", "aus", "d\u