{"textgrid.poem.52462": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "44. Pfl\u00fcgerlied", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arbeitsam und wacker", "tokens": ["Ar\u00b7beit\u00b7sam", "und", "wa\u00b7cker"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Pfl\u00fcgen wir den Acker,", "tokens": ["Pfl\u00fc\u00b7gen", "wir", "den", "A\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Singend, auf und ab.", "tokens": ["Sin\u00b7gend", ",", "auf", "und", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sorgsam trennen wollen", "tokens": ["Sorg\u00b7sam", "tren\u00b7nen", "wol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wir die lockern Schollen,", "tokens": ["Wir", "die", "lo\u00b7ckern", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unsrer Saaten Grab.", "tokens": ["Uns\u00b7rer", "Saa\u00b7ten", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Auf- und abw\u00e4rts ziehend", "tokens": ["Auf", "und", "ab\u00b7w\u00e4rts", "zie\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Furchen wir, stets fliehend", "tokens": ["Fur\u00b7chen", "wir", ",", "stets", "flie\u00b7hend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das erreichte Ziel.", "tokens": ["Das", "er\u00b7reich\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00fchl, o Pflugschar, w\u00fchle!", "tokens": ["W\u00fchl", ",", "o", "Pflug\u00b7schar", ",", "w\u00fch\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Au\u00dfen dr\u00fcckt die Schw\u00fcle,", "tokens": ["Au\u00b7\u00dfen", "dr\u00fcckt", "die", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Tief im Grund ist's k\u00fchl.", "tokens": ["Tief", "im", "Grund", "ist's", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Neigt den Blick zur Erde,", "tokens": ["Neigt", "den", "Blick", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lieb und heimlich werde", "tokens": ["Lieb", "und", "heim\u00b7lich", "wer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Uns ihr dunkler Scho\u00df:", "tokens": ["Uns", "ihr", "dunk\u00b7ler", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hier ist doch kein Bleiben;", "tokens": ["Hier", "ist", "doch", "kein", "Blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ausges\u00e4t zerst\u00e4uben", "tokens": ["Aus\u00b7ge\u00b7s\u00e4t", "zer\u00b7st\u00e4u\u00b7ben"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ist auch unser Los.", "tokens": ["Ist", "auch", "un\u00b7ser", "Los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "S\u00e4et, froh im Hoffen;", "tokens": ["S\u00e4et", ",", "froh", "im", "Hof\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gr\u00e4ber harren offen,", "tokens": ["Gr\u00e4\u00b7ber", "har\u00b7ren", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fluren sind bebaut;", "tokens": ["Flu\u00b7ren", "sind", "be\u00b7baut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Deckt mit Egg' und Spaten", "tokens": ["Deckt", "mit", "Eg\u00b7g'", "und", "Spa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die versenkten Saaten,", "tokens": ["Die", "ver\u00b7senk\u00b7ten", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und dann: Gott vertraut!", "tokens": ["Und", "dann", ":", "Gott", "ver\u00b7traut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Gottes Sonne leuchtet;", "tokens": ["Got\u00b7tes", "Son\u00b7ne", "leuch\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lauer Regen feuchtet", "tokens": ["Lau\u00b7er", "Re\u00b7gen", "feuch\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das entkeimte Gr\u00fcn.", "tokens": ["Das", "ent\u00b7keim\u00b7te", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Flock, o Schnee, und strecke", "tokens": ["Flock", ",", "o", "Schnee", ",", "und", "stre\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Deine Silberdecke", "tokens": ["Dei\u00b7ne", "Sil\u00b7berd\u00b7e\u00b7cke"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schirmend dr\u00fcber hin!", "tokens": ["Schir\u00b7mend", "dr\u00fc\u00b7ber", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ernten werden wanken,", "tokens": ["Ern\u00b7ten", "wer\u00b7den", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo nur K\u00f6rner sanken;", "tokens": ["Wo", "nur", "K\u00f6r\u00b7ner", "san\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mutter Erd' ist treu.", "tokens": ["Mut\u00b7ter", "Erd'", "ist", "treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nichts wird hier vernichtet,", "tokens": ["Nichts", "wird", "hier", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und Verwesung sichtet", "tokens": ["Und", "Ver\u00b7we\u00b7sung", "sich\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nur vom Keim die Spreu.", "tokens": ["Nur", "vom", "Keim", "die", "Spreu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die vor uns entschliefen,", "tokens": ["Die", "vor", "uns", "ent\u00b7schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schlummern, in die Tiefen", "tokens": ["Schlum\u00b7mern", ",", "in", "die", "Tie\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihrer Gruft ges\u00e4t;", "tokens": ["Ih\u00b7rer", "Gruft", "ge\u00b7s\u00e4t", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "L\u00e4nger wird es s\u00e4umen,", "tokens": ["L\u00e4n\u00b7ger", "wird", "es", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bis die Gr\u00e4ber keimen,", "tokens": ["Bis", "die", "Gr\u00e4\u00b7ber", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gottes Saat ersteht!", "tokens": ["Got\u00b7tes", "Saat", "er\u00b7steht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wer um Tote trauert,", "tokens": ["Wer", "um", "To\u00b7te", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Glaub' es, ewig dauert", "tokens": ["Glaub'", "es", ",", "e\u00b7wig", "dau\u00b7ert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht der Aussaat Zeit.", "tokens": ["Nicht", "der", "Aus\u00b7saat", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aus enth\u00fclster Schale", "tokens": ["Aus", "ent\u00b7h\u00fcls\u00b7ter", "Scha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Keimt im Todesthale", "tokens": ["Keimt", "im", "To\u00b7dest\u00b7ha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Frucht der Ewigkeit!", "tokens": ["Frucht", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}