{"textgrid.poem.39022": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich dich gr\u00fc\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich dich gr\u00fc\u00dfe", "tokens": ["Wenn", "ich", "dich", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du ernst holdselige Wildni\u00df,", "tokens": ["Du", "ernst", "hold\u00b7se\u00b7li\u00b7ge", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mein Auge von der H\u00f6he", "tokens": ["Und", "mein", "Au\u00b7ge", "von", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem dichten Pinienwalde unten ruht:", "tokens": ["Auf", "dem", "dich\u00b7ten", "Pi\u00b7ni\u00b7en\u00b7wal\u00b7de", "un\u00b7ten", "ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Von dort herauf ein heilig S\u00e4useln t\u00f6nt,", "tokens": ["Von", "dort", "her\u00b7auf", "ein", "hei\u00b7lig", "S\u00e4u\u00b7seln", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So f\u00fchl' ich Geister schweben,", "tokens": ["So", "f\u00fchl'", "ich", "Geis\u00b7ter", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wie auf regen Harfen", "tokens": ["Und", "wie", "auf", "re\u00b7gen", "Har\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Ahndungsvolle Saiten mit luftgen H\u00e4nden r\u00fchren.", "tokens": ["Ahn\u00b7dungs\u00b7vol\u00b7le", "Sai\u00b7ten", "mit", "luft\u00b7gen", "H\u00e4n\u00b7den", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "So blickt der ewge Dom m\u00e4chtig her\u00fcber,", "tokens": ["So", "blickt", "der", "ew\u00b7ge", "Dom", "m\u00e4ch\u00b7tig", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Von Michel Angelo gegr\u00fcndet,", "tokens": ["Von", "Mi\u00b7chel", "An\u00b7ge\u00b7lo", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und Natur, Geschichte und Geisterwelt", "tokens": ["Und", "Na\u00b7tur", ",", "Ge\u00b7schich\u00b7te", "und", "Geis\u00b7ter\u00b7welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "T\u00f6nen im wundersamen Chor zugleich.", "tokens": ["T\u00f6\u00b7nen", "im", "wun\u00b7der\u00b7sa\u00b7men", "Chor", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Was ist der Mensch,", "tokens": ["Was", "ist", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df er diese Sch\u00f6ne f\u00fchlt?", "tokens": ["Da\u00df", "er", "die\u00b7se", "Sch\u00f6\u00b7ne", "f\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie unerme\u00dflich die Liebe", "tokens": ["Wie", "un\u00b7er\u00b7me\u00df\u00b7lich", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die ihm die Unendlichkeit dieser Wonnen g\u00f6nnt!", "tokens": ["Die", "ihm", "die", "Un\u00b7end\u00b7lich\u00b7keit", "die\u00b7ser", "Won\u00b7nen", "g\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}