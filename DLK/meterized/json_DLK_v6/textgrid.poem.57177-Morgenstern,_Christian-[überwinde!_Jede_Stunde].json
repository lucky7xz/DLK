{"textgrid.poem.57177": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[\u00fcberwinde! Jede Stunde]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberwinde! Jede Stunde,", "tokens": ["\u00dc\u00b7berw\u00b7in\u00b7de", "!", "Je\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die du siegreich \u00fcberwindest,", "tokens": ["die", "du", "sieg\u00b7reich", "\u00fc\u00b7berw\u00b7in\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sei getrost, da\u00df du im Pfunde", "tokens": ["sei", "ge\u00b7trost", ",", "da\u00df", "du", "im", "Pfun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deines neuen Lebens findest.", "tokens": ["dei\u00b7nes", "neu\u00b7en", "Le\u00b7bens", "fin\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jede Schmach und jede Schande,", "tokens": ["Je\u00b7de", "Schmach", "und", "je\u00b7de", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jeder Schmerz und jedes Leiden", "tokens": ["je\u00b7der", "Schmerz", "und", "je\u00b7des", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wird bei richtigem Verstande", "tokens": ["wird", "bei", "rich\u00b7ti\u00b7gem", "Ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deinen Aufstieg mehr entscheiden.", "tokens": ["dei\u00b7nen", "Auf\u00b7stieg", "mehr", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ohne Erbschuld wirst du funkeln,", "tokens": ["Oh\u00b7ne", "Erb\u00b7schuld", "wirst", "du", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "abermals vor Enkeln rege,", "tokens": ["a\u00b7ber\u00b7mals", "vor", "En\u00b7keln", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ungez\u00e4hltem Volk im Dunkeln", "tokens": ["un\u00b7ge\u00b7z\u00e4hl\u00b7tem", "Volk", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "weist ein Sieger Sonnenwege.", "tokens": ["weist", "ein", "Sie\u00b7ger", "Son\u00b7nen\u00b7we\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}