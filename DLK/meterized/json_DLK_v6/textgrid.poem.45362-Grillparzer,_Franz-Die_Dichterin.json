{"textgrid.poem.45362": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die Dichterin", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dich \u00f6ffentlich entkleiden,", "tokens": ["Willst", "du", "dich", "\u00f6f\u00b7fent\u00b7lich", "ent\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie Phrynes Beispiel weist,", "tokens": ["Wie", "Phry\u00b7nes", "Bei\u00b7spiel", "weist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So pr\u00fcfe vorher dich bescheiden,", "tokens": ["So", "pr\u00fc\u00b7fe", "vor\u00b7her", "dich", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sch\u00f6n du etwa seist.", "tokens": ["Wie", "sch\u00f6n", "du", "et\u00b7wa", "seist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}