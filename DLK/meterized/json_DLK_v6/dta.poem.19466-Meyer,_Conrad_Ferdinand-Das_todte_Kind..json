{"dta.poem.19466": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das todte Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat den Garten sich zum Freund gemacht,", "tokens": ["Es", "hat", "den", "Gar\u00b7ten", "sich", "zum", "Freund", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann welkten es und er im Herbste sacht,", "tokens": ["Dann", "welk\u00b7ten", "es", "und", "er", "im", "Herbs\u00b7te", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne ging und es und er entschlief,", "tokens": ["Die", "Son\u00b7ne", "ging", "und", "es", "und", "er", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geh\u00fcllt in ", "tokens": ["Ge\u00b7h\u00fcllt", "in"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Jetzt ist der Garten unversehns erwacht,", "tokens": ["Jetzt", "ist", "der", "Gar\u00b7ten", "un\u00b7ver\u00b7sehns", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kleine schlummert fest in ihrer Nacht.", "tokens": ["Die", "Klei\u00b7ne", "schlum\u00b7mert", "fest", "in", "ih\u00b7rer", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo steckst du? summt es dort und summt es hier.", "tokens": ["Wo", "steckst", "du", "?", "summt", "es", "dort", "und", "summt", "es", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ganze Garten fr\u00e4gt nach ihr, nach ihr.", "tokens": ["Der", "gan\u00b7ze", "Gar\u00b7ten", "fr\u00e4gt", "nach", "ihr", ",", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die blaue Winde klettert schlank empor", "tokens": ["Die", "blau\u00b7e", "Win\u00b7de", "klet\u00b7tert", "schlank", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und blickt ins Haus: Komm hinterm Schrank hervor!", "tokens": ["Und", "blickt", "ins", "Haus", ":", "Komm", "hin\u00b7term", "Schrank", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo birgst du dich? Du thust dir's selbst zu leid!", "tokens": ["Wo", "birgst", "du", "dich", "?", "Du", "thust", "dir's", "selbst", "zu", "leid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "PIS", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was hast du f\u00fcr ein neues Sommerkleid?", "tokens": ["Was", "hast", "du", "f\u00fcr", "ein", "neu\u00b7es", "Som\u00b7mer\u00b7kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}