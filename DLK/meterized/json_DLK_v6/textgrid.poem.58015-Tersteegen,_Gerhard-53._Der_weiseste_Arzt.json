{"textgrid.poem.58015": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "53. Der weiseste Arzt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, \u00fcberla\u00df dich Gott, er ist so weis' und treu,", "tokens": ["Ei", ",", "\u00fc\u00b7berl\u00b7a\u00df", "dich", "Gott", ",", "er", "ist", "so", "weis'", "und", "treu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein S\u00fcndengift er selbst gebraucht zur Arzenei!", "tokens": ["Dein", "S\u00fcn\u00b7den\u00b7gift", "er", "selbst", "ge\u00b7braucht", "zur", "Ar\u00b7ze\u00b7nei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer aber seine S\u00fcnd' mit solcher Gnade decket,", "tokens": ["Wer", "a\u00b7ber", "sei\u00b7ne", "S\u00fcnd'", "mit", "sol\u00b7cher", "Gna\u00b7de", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wisse, da\u00df ", "tokens": ["Der", "wis\u00b7se", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS"], "meter": "-+-+", "measure": "iambic.di"}}}}}