{"dta.poem.1426": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Unschuld Christi Kleid/ mein Pathe/ legst du an/", "tokens": ["Der", "Un\u00b7schuld", "Chris\u00b7ti", "Kleid", "/", "mein", "Pa\u00b7the", "/", "legst", "du", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Schmuck/ der dich f\u00fcr GOtt und Menschen zieren", "tokens": ["Den", "Schmuck", "/", "der", "dich", "f\u00fcr", "Gott", "und", "Men\u00b7schen", "zie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kan/", "tokens": ["kan", "/"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Den keine Motte frist/ den keine Zeit verzehrt/", "tokens": ["Den", "kei\u00b7ne", "Mot\u00b7te", "frist", "/", "den", "kei\u00b7ne", "Zeit", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der dir/ was zeitlich n\u00fctz und ewig gutt/ gew\u00e4hrt.", "tokens": ["Der", "dir", "/", "was", "zeit\u00b7lich", "n\u00fctz", "und", "e\u00b7wig", "gutt", "/", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$(", "PWS", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dich kleide Gl\u00fcck und Heyl in langer Lebens-Zeit/", "tokens": ["Dich", "klei\u00b7de", "Gl\u00fcck", "und", "Heyl", "in", "lan\u00b7ger", "Le\u00b7bens\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dich kleide dieser Rock in selger Ewigkeit.", "tokens": ["Dich", "klei\u00b7de", "die\u00b7ser", "Rock", "in", "sel\u00b7ger", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}