{"textgrid.poem.59013": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "232. Meide das Lockaas!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder f\u00fcrcht't, was ihn verletzet,", "tokens": ["Ein", "je\u00b7der", "f\u00fcrcht't", ",", "was", "ihn", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Christe f\u00fcrcht't, was ihn erg\u00f6tzet;", "tokens": ["Ein", "Chris\u00b7te", "f\u00fcrcht't", ",", "was", "ihn", "er\u00b7g\u00f6t\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Suchst du des Geistes Fried' und Freud',", "tokens": ["Suchst", "du", "des", "Geis\u00b7tes", "Fried'", "und", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werde fremd der Sinnlichkeit!", "tokens": ["So", "wer\u00b7de", "fremd", "der", "Sinn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}