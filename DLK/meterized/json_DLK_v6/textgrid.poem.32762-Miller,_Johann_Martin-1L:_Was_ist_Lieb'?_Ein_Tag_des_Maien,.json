{"textgrid.poem.32762": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist Lieb'? Ein Tag des Maien,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Lieb'? Ein Tag des Maien,", "tokens": ["Was", "ist", "Lieb'", "?", "Ein", "Tag", "des", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in goldnem Glanz erwacht,", "tokens": ["Der", "in", "gold\u00b7nem", "Glanz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell auf froher Sch\u00e4fer Reihen", "tokens": ["Hell", "auf", "fro\u00b7her", "Sch\u00e4\u00b7fer", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom entw\u00f6lkten Himmel lacht.", "tokens": ["Vom", "ent\u00b7w\u00f6lk\u00b7ten", "Him\u00b7mel", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fl\u00f6ten locken zu den T\u00e4nzen", "tokens": ["Fl\u00f6\u00b7ten", "lo\u00b7cken", "zu", "den", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der vergn\u00fcgten M\u00e4dchen Schar;", "tokens": ["Der", "ver\u00b7gn\u00fcg\u00b7ten", "M\u00e4d\u00b7chen", "Schar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen sammeln sie zu Kr\u00e4nzen,", "tokens": ["Blu\u00b7men", "sam\u00b7meln", "sie", "zu", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fccken ihrer Sch\u00e4fer Haar.", "tokens": ["Schm\u00fc\u00b7cken", "ih\u00b7rer", "Sch\u00e4\u00b7fer", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schnell verd\u00fcstert \u00fcber ihnen", "tokens": ["Schnell", "ver\u00b7d\u00fcs\u00b7tert", "\u00fc\u00b7ber", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der schw\u00fclen Sonne Blick;", "tokens": ["Sich", "der", "schw\u00fc\u00b7len", "Son\u00b7ne", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schrecken blickt aus ihren Mienen;", "tokens": ["Schre\u00b7cken", "blickt", "aus", "ih\u00b7ren", "Mie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcchtern eilen sie zur\u00fcck.", "tokens": ["Sch\u00fcch\u00b7tern", "ei\u00b7len", "sie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Regeng\u00fcsse str\u00f6men nieder;", "tokens": ["Re\u00b7gen\u00b7g\u00fcs\u00b7se", "str\u00f6\u00b7men", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blum' und Wiese sind verheert;", "tokens": ["Blum'", "und", "Wie\u00b7se", "sind", "ver\u00b7heert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der frommen Freude Lieder", "tokens": ["Und", "der", "from\u00b7men", "Freu\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind in Trauerton verkehrt. \u2013", "tokens": ["Sind", "in", "Trau\u00b7er\u00b7ton", "ver\u00b7kehrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schau! der Friedensboge strahlet", "tokens": ["Schau", "!", "der", "Frie\u00b7dens\u00b7bo\u00b7ge", "strah\u00b7let"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ins erschrockne Thal herab;", "tokens": ["Ins", "er\u00b7schrock\u00b7ne", "Thal", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau! der Hoffnung Freude malet", "tokens": ["Schau", "!", "der", "Hoff\u00b7nung", "Freu\u00b7de", "ma\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich auf allen Wangen ab. \u2013", "tokens": ["Sich", "auf", "al\u00b7len", "Wan\u00b7gen", "ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gieb, o Gott der frommen Liebe,", "tokens": ["Gieb", ",", "o", "Gott", "der", "from\u00b7men", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns ein ruhiges Gem\u00fct,", "tokens": ["Uns", "ein", "ru\u00b7hi\u00b7ges", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das durch Wolken, schwarz und tr\u00fcbe,", "tokens": ["Das", "durch", "Wol\u00b7ken", ",", "schwarz", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ins Gefild der Hoffnung sieht!", "tokens": ["Ins", "Ge\u00b7fild", "der", "Hoff\u00b7nung", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}