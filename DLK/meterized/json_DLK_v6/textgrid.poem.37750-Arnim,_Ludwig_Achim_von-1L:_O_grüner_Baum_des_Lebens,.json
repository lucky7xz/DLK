{"textgrid.poem.37750": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O gr\u00fcner Baum des Lebens,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O gr\u00fcner Baum des Lebens,", "tokens": ["O", "gr\u00fc\u00b7ner", "Baum", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meiner Brust versteckt,", "tokens": ["In", "mei\u00b7ner", "Brust", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich nicht flehn vergebens!", "tokens": ["La\u00df", "mich", "nicht", "flehn", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich habe dich entdeckt.", "tokens": ["Ich", "ha\u00b7be", "dich", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O zeige mir die Wege", "tokens": ["O", "zei\u00b7ge", "mir", "die", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch diesen tiefen Schnee,", "tokens": ["Durch", "die\u00b7sen", "tie\u00b7fen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich den Fu\u00df bewege,", "tokens": ["Wenn", "ich", "den", "Fu\u00df", "be\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So gleit' ich von der H\u00f6h'.", "tokens": ["So", "gleit'", "ich", "von", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bliebe dir gern eigen,", "tokens": ["Ich", "blie\u00b7be", "dir", "gern", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich g\u00e4b' mich selber auf, \u2013", "tokens": ["Ich", "g\u00e4b'", "mich", "sel\u00b7ber", "auf", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst du den Weg mir zeigen,", "tokens": ["Willst", "du", "den", "Weg", "mir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll enden hier mein Lauf?", "tokens": ["Soll", "en\u00b7den", "hier", "mein", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Denken ist verschwunden,", "tokens": ["Mein", "Den\u00b7ken", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es schlief das Haupt mir ein,", "tokens": ["Es", "schlief", "das", "Haupt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es ist mein Herz entbunden", "tokens": ["Es", "ist", "mein", "Herz", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von der Erkenntni\u00df Schein.", "tokens": ["Von", "der", "Er\u00b7kennt\u00b7ni\u00df", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich werd' in Strahlen schwimmen,", "tokens": ["Ich", "werd'", "in", "Strah\u00b7len", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dieses Leibes Nacht,", "tokens": ["Aus", "die\u00b7ses", "Lei\u00b7bes", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin kein Mensch kann klimmen,", "tokens": ["Wo\u00b7hin", "kein", "Mensch", "kann", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit des Gedankens Macht.", "tokens": ["Mit", "des", "Ge\u00b7dan\u00b7kens", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ward mein Sinn erheitert,", "tokens": ["Es", "ward", "mein", "Sinn", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Welt mir aufgethan", "tokens": ["Die", "Welt", "mir", "auf\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Geist in Gott erweitert,", "tokens": ["Der", "Geist", "in", "Gott", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Unendlich ist die Bahn!", "tokens": ["Un\u00b7end\u00b7lich", "ist", "die", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}