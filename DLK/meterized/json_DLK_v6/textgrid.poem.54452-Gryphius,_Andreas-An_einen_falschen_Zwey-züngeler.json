{"textgrid.poem.54452": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "An einen falschen Zwey-z\u00fcngeler", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv falscher b\u00f6ser Mensch/ au\u00df dessen krummen Rachen", "tokens": ["Dv", "fal\u00b7scher", "b\u00f6\u00b7ser", "Mensch", "/", "au\u00df", "des\u00b7sen", "krum\u00b7men", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN", "$(", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die schwartzen Schlang\u1ebd sehn/ in dessen schlimmen Mund", "tokens": ["Die", "schwart\u00b7zen", "Schlang\u1ebd", "sehn", "/", "in", "des\u00b7sen", "schlim\u00b7men", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$(", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das natterzischen pfeifft/ Du mehr al\u00df tober Hund/", "tokens": ["Das", "nat\u00b7ter\u00b7zi\u00b7schen", "pfeifft", "/", "Du", "mehr", "al\u00df", "to\u00b7ber", "Hund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "ADV", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du gantz verschalckter Fuchs/ du Hau\u00df der grimmen Drachen.", "tokens": ["Du", "gantz", "ver\u00b7schalck\u00b7ter", "Fuchs", "/", "du", "Hau\u00df", "der", "grim\u00b7men", "Dra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NE", "$(", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wil Dir de\u00f1 nicht einmahl der Leib vor Gifft zukrachen?", "tokens": ["Wil", "Dir", "de\u00f1", "nicht", "ein\u00b7mahl", "der", "Leib", "vor", "Gifft", "zu\u00b7kra\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ja nichts an dir/ (Ich reds mit guttem Grund!)", "tokens": ["Es", "ist", "ja", "nichts", "an", "dir", "/", "(", "Ich", "reds", "mit", "gut\u00b7tem", "Grund", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "PPER", "$(", "$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von deinem L\u00e4sterkopff/ bi\u00df auff die Zeh gesund/", "tokens": ["Von", "dei\u00b7nem", "L\u00e4s\u00b7ter\u00b7kopff", "/", "bi\u00df", "auff", "die", "Zeh", "ge\u00b7sund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du bist so Teuffel schwartz/ al\u00df du dich wei\u00df kanst machen.", "tokens": ["Du", "bist", "so", "Teuf\u00b7fel", "schwartz", "/", "al\u00df", "du", "dich", "wei\u00df", "kanst", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADJD", "$(", "KOUS", "PPER", "PRF", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein jeder Wort au\u00df dir schmertzt als ein schneidig Schwert/", "tokens": ["Ein", "je\u00b7der", "Wort", "au\u00df", "dir", "schmertzt", "als", "ein", "schnei\u00b7dig", "Schwert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPER", "VVFIN", "KOKOM", "ART", "ADJD", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Du leichter Ehrendieb/ bist Rad vnd Fewres werth/", "tokens": ["Du", "leich\u00b7ter", "Eh\u00b7ren\u00b7dieb", "/", "bist", "Rad", "vnd", "Few\u00b7res", "werth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VAFIN", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja so Gerechtigkeit hier auff dem Erdkrei\u00df wohnet;", "tokens": ["Ja", "so", "Ge\u00b7rech\u00b7tig\u00b7keit", "hier", "auff", "dem", "Erd\u00b7krei\u00df", "woh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird es deiner Zung so vbel noch ergehn/", "tokens": ["So", "wird", "es", "dei\u00b7ner", "Zung", "so", "vbel", "noch", "er\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Da\u00df jeder der dich siht/ mit zittern sol gestehn/", "tokens": ["Da\u00df", "je\u00b7der", "der", "dich", "siht", "/", "mit", "zit\u00b7tern", "sol", "ge\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "PPER", "VVFIN", "$(", "APPR", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df der sonst sanffte Gott dir schrecklich abgelohnet.", "tokens": ["Da\u00df", "der", "sonst", "sanff\u00b7te", "Gott", "dir", "schreck\u00b7lich", "ab\u00b7ge\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "VVFIN", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}