{"textgrid.poem.53807": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Dichtkunst 1926", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was werden die Dichter heuer schreiben \u2013?", "tokens": ["Was", "wer\u00b7den", "die", "Dich\u00b7ter", "heu\u00b7er", "schrei\u00b7ben", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "VVINF", "$(", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das wird auch in diesem Jahre so bleiben:", "tokens": ["Das", "wird", "auch", "in", "die\u00b7sem", "Jah\u00b7re", "so", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn der Sekundaner, sanft erhitzt,", "tokens": ["Wenn", "der", "Se\u00b7kun\u00b7da\u00b7ner", ",", "sanft", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "vor einem nackerten Bilderbuch sitzt,", "tokens": ["vor", "ei\u00b7nem", "na\u00b7cker\u00b7ten", "Bil\u00b7der\u00b7buch", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "steigt ihm das Blut in die Gef\u00e4\u00dfe,", "tokens": ["steigt", "ihm", "das", "Blut", "in", "die", "Ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "er wackelt leise mit dem Ges\u00e4\u00dfe;", "tokens": ["er", "wa\u00b7ckelt", "lei\u00b7se", "mit", "dem", "Ge\u00b7s\u00e4\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "hochrot der Kopf, hochrot das Ohr,", "tokens": ["hoch\u00b7rot", "der", "Kopf", ",", "hoch\u00b7rot", "das", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "stellt er sich etwas Sch\u00f6nes vor:", "tokens": ["stellt", "er", "sich", "et\u00b7was", "Sch\u00f6\u00b7nes", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJA", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "eine z\u00fcngelnde und rundbauchige Fee \u2013", "tokens": ["ei\u00b7ne", "z\u00fcn\u00b7geln\u00b7de", "und", "rund\u00b7bau\u00b7chi\u00b7ge", "Fee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$("], "meter": "+-+---++--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "und dann spielt er mit seiner Lieblingsidee.", "tokens": ["und", "dann", "spielt", "er", "mit", "sei\u00b7ner", "Lieb\u00b7lings\u00b7i\u00b7dee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So auch der Deutsche.", "tokens": ["So", "auch", "der", "Deut\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Alle Knaben,", "tokens": ["Al\u00b7le", "Kna\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "die eine Schreibmaschine haben,", "tokens": ["die", "ei\u00b7ne", "Schreib\u00b7ma\u00b7schi\u00b7ne", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "verfassen heute radikal", "tokens": ["ver\u00b7fas\u00b7sen", "heu\u00b7te", "ra\u00b7di\u00b7kal"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Weltgeschichte noch einmal.", "tokens": ["die", "Welt\u00b7ge\u00b7schich\u00b7te", "noch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der alte Fritz sagt m\u00fcrrisch: \u00bbEr . . . !\u00ab", "tokens": ["Der", "al\u00b7te", "Fritz", "sagt", "m\u00fcr\u00b7risch", ":", "\u00bb", "Er", ".", ".", ".", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADJD", "$.", "$(", "PPER", "$.", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und plaudert mit dem Affen Voltaire;", "tokens": ["und", "plau\u00b7dert", "mit", "dem", "Af\u00b7fen", "Vol\u00b7tai\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "er kann zwar nicht richtig deutsch buchstabieren,", "tokens": ["er", "kann", "zwar", "nicht", "rich\u00b7tig", "deutsch", "buch\u00b7sta\u00b7bie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "doch das tut der Kr\u00fcckstock remplacieren \u2013", "tokens": ["doch", "das", "tut", "der", "Kr\u00fcck\u00b7stock", "rem\u00b7pla\u00b7cie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "davon leben die Biographen.", "tokens": ["da\u00b7von", "le\u00b7ben", "die", "Bio\u00b7gra\u00b7phen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Die Juden vom Film gehn mit Bismarck schlafen", "tokens": ["Die", "Ju\u00b7den", "vom", "Film", "gehn", "mit", "Bis\u00b7marck", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und stehn mit Moltken wieder auf \u2013", "tokens": ["und", "stehn", "mit", "Molt\u00b7ken", "wie\u00b7der", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das ist so der deutsche Lebenslauf.", "tokens": ["das", "ist", "so", "der", "deut\u00b7sche", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Arminius, der Gro\u00dfe Kurf\u00fcrst und Stein", "tokens": ["Ar\u00b7mi\u00b7nius", ",", "der", "Gro\u00b7\u00dfe", "Kur\u00b7f\u00fcrst", "und", "Stein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "spielen einen sch\u00f6nen Bierskat zu drein;", "tokens": ["spie\u00b7len", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Bier\u00b7skat", "zu", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKZU", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bl\u00fccher und Barbarossa mit Bart", "tokens": ["Bl\u00fc\u00b7cher", "und", "Bar\u00b7ba\u00b7ros\u00b7sa", "mit", "Bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "APPR", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "kiebitzen dazu auf deutsche Art;", "tokens": ["kie\u00b7bit\u00b7zen", "da\u00b7zu", "auf", "deut\u00b7sche", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und inmitten dieses gro\u00dfen Geschreis", "tokens": ["und", "in\u00b7mit\u00b7ten", "die\u00b7ses", "gro\u00b7\u00dfen", "Ge\u00b7schreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "steht Turnvater Jahn und riecht nach Schwei\u00df.", "tokens": ["steht", "Turn\u00b7va\u00b7ter", "Jahn", "und", "riecht", "nach", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und segnend schwebt \u00fcber alle diese", "tokens": ["Und", "seg\u00b7nend", "schwebt", "\u00fc\u00b7ber", "al\u00b7le", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PIS", "PDAT"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "die gute K\u00f6nigin Luise,", "tokens": ["die", "gu\u00b7te", "K\u00f6\u00b7ni\u00b7gin", "Lu\u00b7i\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "eine wahrhafte, echte, deutsche Frau.", "tokens": ["ei\u00b7ne", "wahr\u00b7haf\u00b7te", ",", "ech\u00b7te", ",", "deut\u00b7sche", "Frau", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "F\u00e4llt einem nichts ein, schreibt er: Gneisenau.", "tokens": ["F\u00e4llt", "ei\u00b7nem", "nichts", "ein", ",", "schreibt", "er", ":", "Gnei\u00b7se\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "PTKVZ", "$,", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auch die Operetten aus Wien", "tokens": ["Und", "auch", "die", "O\u00b7per\u00b7et\u00b7ten", "aus", "Wi\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "ben\u00f6tigen Landes-Dynastien", "tokens": ["be\u00b7n\u00f6\u00b7ti\u00b7gen", "Lan\u00b7des\u00b7Dy\u00b7nas\u00b7ti\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "(mit Renten).", "tokens": ["(", "mit", "Ren\u00b7ten", ")", "."], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So besinnt sich weit und breit", "tokens": ["So", "be\u00b7sinnt", "sich", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "der Deutsche auf seine Vergangenheit:", "tokens": ["der", "Deut\u00b7sche", "auf", "sei\u00b7ne", "Ver\u00b7gan\u00b7gen\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Hochrot der Kopf, hochrot das Ohr,", "tokens": ["Hoch\u00b7rot", "der", "Kopf", ",", "hoch\u00b7rot", "das", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "stellt er sich etwas Sch\u00f6nes vor", "tokens": ["stellt", "er", "sich", "et\u00b7was", "Sch\u00f6\u00b7nes", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJA", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "in B\u00fcchern, Theaterst\u00fccken, Journalen \u2013", "tokens": ["in", "B\u00fc\u00b7chern", ",", "The\u00b7a\u00b7ter\u00b7st\u00fc\u00b7cken", ",", "Jour\u00b7na\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "und dann spielt er mit seinem Nationalen.", "tokens": ["und", "dann", "spielt", "er", "mit", "sei\u00b7nem", "Na\u00b7ti\u00b7o\u00b7na\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Denn dieses Deutschtum mit Sonnenstich", "tokens": ["Denn", "die\u00b7ses", "Deutschtum", "mit", "Son\u00b7nen\u00b7stich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ist eine Besch\u00e4ftigung an und f\u00fcr sich.", "tokens": ["ist", "ei\u00b7ne", "Be\u00b7sch\u00e4f\u00b7ti\u00b7gung", "an", "und", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "KON", "APPR", "PRF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es gibt Leute, die statt Kinder zu zeugen, schreiben.", "tokens": ["Es", "gibt", "Leu\u00b7te", ",", "die", "statt", "Kin\u00b7der", "zu", "zeu\u00b7gen", ",", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "APPR", "NN", "PTKZU", "VVFIN", "$,", "VVFIN", "$."], "meter": "-++-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das wird auch im kommenden Jahre so bleiben.", "tokens": ["Das", "wird", "auch", "im", "kom\u00b7men\u00b7den", "Jah\u00b7re", "so", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}