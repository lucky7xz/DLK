{"dta.poem.3425": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Schutzgedicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Liliengewand, in abendrothem Glanze", "tokens": ["Im", "Li\u00b7li\u00b7en\u00b7ge\u00b7wand", ",", "in", "a\u00b7ben\u00b7dro\u00b7them", "Glan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sitzt hoch auf einem Regenbogenthron", "tokens": ["Sitzt", "hoch", "auf", "ei\u00b7nem", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Unschuld. Ihre Stirn, vom Amaranthenkranze", "tokens": ["Die", "Un\u00b7schuld", ".", "Ih\u00b7re", "Stirn", ",", "vom", "A\u00b7ma\u00b7ran\u00b7then\u00b7kran\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umschattet, blickt nicht Trotz noch Hohn.", "tokens": ["Um\u00b7schat\u00b7tet", ",", "blickt", "nicht", "Trotz", "noch", "Hohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PTKNEG", "APPR", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch blickt sie W\u00fcrd' und Ruh. Der Sp\u00fcrhund", "tokens": ["Doch", "blickt", "sie", "W\u00fcrd'", "und", "Ruh", ".", "Der", "Sp\u00fcr\u00b7hund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "KON", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Argwohn wittert,", "tokens": ["Arg\u00b7wohn", "wit\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die Dogge Klatschsucht bellet um sie her.", "tokens": ["Die", "Dog\u00b7ge", "Klatschsucht", "bel\u00b7let", "um", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihr Schneegewand bleibt weiss. Ihr Stuhl bleibt", "tokens": ["Ihr", "Schnee\u00b7ge\u00b7wand", "bleibt", "weiss", ".", "Ihr", "Stuhl", "bleibt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "unersch\u00fcttert,", "tokens": ["un\u00b7er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und ihre Stirne wolkenleer.", "tokens": ["Und", "ih\u00b7re", "Stir\u00b7ne", "wol\u00b7ken\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O selig, selig, wer in ihrem sichern Schirme", "tokens": ["O", "se\u00b7lig", ",", "se\u00b7lig", ",", "wer", "in", "ih\u00b7rem", "si\u00b7chern", "Schir\u00b7me"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "ADJD", "$,", "PWS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pfade wandelt, die die Pflicht ihm zeigt!", "tokens": ["Die", "Pfa\u00b7de", "wan\u00b7delt", ",", "die", "die", "Pflicht", "ihm", "zeigt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er bleibt im Sonnenschein, und im Gebr\u00fcll der St\u00fcrme", "tokens": ["Er", "bleibt", "im", "Son\u00b7nen\u00b7schein", ",", "und", "im", "Ge\u00b7br\u00fcll", "der", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich unverzagt und ungebeugt.", "tokens": ["Gleich", "un\u00b7ver\u00b7zagt", "und", "un\u00b7ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihm ist ein st\u00e4hlern Bollwerk, eine Demant-", "tokens": ["Ihm", "ist", "ein", "st\u00e4h\u00b7lern", "Boll\u00b7werk", ",", "ei\u00b7ne", "De\u00b7mant"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "mauer", "tokens": ["mau\u00b7er"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sein Selbstbewustseyn, seines Werths Gef\u00fchl.", "tokens": ["Sein", "Selbst\u00b7be\u00b7wust\u00b7seyn", ",", "sei\u00b7nes", "Werths", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm rauscht der Wolkenbruch wie Mayenregen-", "tokens": ["Ihm", "rauscht", "der", "Wol\u00b7ken\u00b7bruch", "wie", "Mayen\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "TRUNC"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "schauer,", "tokens": ["schau\u00b7er", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Ihm s\u00e4uselt Boreas wie Sommerabendk\u00fchl.", "tokens": ["Ihm", "s\u00e4u\u00b7selt", "Bo\u00b7re\u00b7as", "wie", "Som\u00b7mer\u00b7a\u00b7bend\u00b7k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihn regt des Gecken Spott, des L\u00e4strers Nattern-", "tokens": ["Ihn", "regt", "des", "Ge\u00b7cken", "Spott", ",", "des", "L\u00e4st\u00b7rers", "Nat\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "geifer,", "tokens": ["gei\u00b7fer", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Des Buben Hohngel\u00e4chter keinen Schmerz.", "tokens": ["Des", "Bu\u00b7ben", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "kei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit siebenfachem Erz umpanzert sch\u00f6ner Eifer", "tokens": ["Mit", "sie\u00b7ben\u00b7fa\u00b7chem", "Erz", "um\u00b7pan\u00b7zert", "sch\u00f6\u00b7ner", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein k\u00fchnes, kaltes, edles Herz \u2014 \u2014", "tokens": ["Sein", "k\u00fch\u00b7nes", ",", "kal\u00b7tes", ",", "ed\u00b7les", "Herz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr wenigen, die ich in dieses Eylands Mitte", "tokens": ["Ihr", "we\u00b7ni\u00b7gen", ",", "die", "ich", "in", "die\u00b7ses", "Ey\u00b7lands", "Mit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fernahnend sucht', und sorgsam sp\u00e4hend fand,", "tokens": ["Fer\u00b7nah\u00b7nend", "sucht'", ",", "und", "sorg\u00b7sam", "sp\u00e4\u00b7hend", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die unverschrobner Sinn und unverdorbne Sitte,", "tokens": ["Die", "un\u00b7ver\u00b7schrob\u00b7ner", "Sinn", "und", "un\u00b7ver\u00b7dorb\u00b7ne", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Herzenseinfalt mir verband;", "tokens": ["Und", "Her\u00b7zen\u00b7sein\u00b7falt", "mir", "ver\u00b7band", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die ihr mich kennt und liebt, und die ich", "tokens": ["Die", "ihr", "mich", "kennt", "und", "liebt", ",", "und", "die", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,", "KON", "PRELS", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "kenn' und liebe \u2014", "tokens": ["kenn'", "und", "lie\u00b7be"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nicht euch zu lehren, singt mein Schutzgedicht.", "tokens": ["Nicht", "euch", "zu", "leh\u00b7ren", ",", "singt", "mein", "Schutz\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr sahet dieses Herz. Ich barg euch seine Triebe,", "tokens": ["Ihr", "sa\u00b7het", "die\u00b7ses", "Herz", ".", "Ich", "barg", "euch", "sei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Dursten und sein Sehnen nicht.", "tokens": ["Sein", "Durs\u00b7ten", "und", "sein", "Seh\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Saht ihr ein Laster drin? Vom Strand der", "tokens": ["Saht", "ihr", "ein", "Las\u00b7ter", "drin", "?", "Vom", "Strand", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "freyen Hylde", "tokens": ["frey\u00b7en", "Hyl\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Kam ich zu euch mit ungebrochnem Muth.", "tokens": ["Kam", "ich", "zu", "euch", "mit", "un\u00b7ge\u00b7broch\u00b7nem", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch ungeb\u00e4ndigt war des J\u00fcnglings Trotz und Wilde,", "tokens": ["Noch", "un\u00b7ge\u00b7b\u00e4n\u00b7digt", "war", "des", "J\u00fcng\u00b7lings", "Trotz", "und", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch unverlodert seine Gluth.", "tokens": ["Noch", "un\u00b7ver\u00b7lo\u00b7dert", "sei\u00b7ne", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich irrt' umher und sucht' und las mir manche Blume", "tokens": ["Ich", "irrt'", "um\u00b7her", "und", "sucht'", "und", "las", "mir", "man\u00b7che", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In unsrer grossen Mutter mildem Schooss;", "tokens": ["In", "uns\u00b7rer", "gros\u00b7sen", "Mut\u00b7ter", "mil\u00b7dem", "Scho\u00b7oss", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieh! im Grazienhayn, in Eros Heiligthume", "tokens": ["Und", "sieh", "!", "im", "Gra\u00b7zi\u00b7en\u00b7hayn", ",", "in", "E\u00b7ros", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPRART", "NN", "$,", "APPR", "NE", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Spross mir die Eine hold und gross.", "tokens": ["Spross", "mir", "die", "Ei\u00b7ne", "hold", "und", "gross", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ART", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ich gewann sie lieb. Ich barg sie vor dem", "tokens": ["Und", "ich", "ge\u00b7wann", "sie", "lieb", ".", "Ich", "barg", "sie", "vor", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcthen", "tokens": ["W\u00fct\u00b7hen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Des Mittagstrahls, vor Frost und Hagelschlag.", "tokens": ["Des", "Mit\u00b7tags\u00b7trahls", ",", "vor", "Frost", "und", "Ha\u00b7gel\u00b7schlag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6her schoss ihr Halm, und tausendbl\u00e4ttrig m\u00fchten", "tokens": ["Und", "h\u00f6\u00b7her", "schoss", "ihr", "Halm", ",", "und", "tau\u00b7send\u00b7bl\u00e4tt\u00b7rig", "m\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich ihre Bl\u00fcthen an den Tag.", "tokens": ["Sich", "ih\u00b7re", "Bl\u00fc\u00b7then", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich hing mein Herz an sie. Doch fort ist meine", "tokens": ["Ich", "hing", "mein", "Herz", "an", "sie", ".", "Doch", "fort", "ist", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$.", "KON", "PTKVZ", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blume,", "tokens": ["Blu\u00b7me", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und ich verlasse nun den \u00f6den Ort.", "tokens": ["Und", "ich", "ver\u00b7las\u00b7se", "nun", "den", "\u00f6\u00b7den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Hain Uraniens, aus Eros Heiligthume", "tokens": ["Vom", "Hain", "U\u00b7ra\u00b7ni\u00b7ens", ",", "aus", "E\u00b7ros", "Hei\u00b7lig\u00b7thu\u00b7me"], "