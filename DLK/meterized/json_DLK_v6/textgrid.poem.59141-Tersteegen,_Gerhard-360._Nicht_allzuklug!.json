{"textgrid.poem.59141": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "360. Nicht allzuklug!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Such Schlangenklugheit nach der Schrift,", "tokens": ["Such", "Schlan\u00b7gen\u00b7klug\u00b7heit", "nach", "der", "Schrift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch denk: die Schlange hat auch Gift!", "tokens": ["Doch", "denk", ":", "die", "Schlan\u00b7ge", "hat", "auch", "Gift", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man ist Gott viel zu klug, zu gro\u00df;", "tokens": ["Man", "ist", "Gott", "viel", "zu", "klug", ",", "zu", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "ADV", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das T\u00e4ublein sitzt ihm nur im Scho\u00df.", "tokens": ["Das", "T\u00e4ub\u00b7lein", "sitzt", "ihm", "nur", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}