{"dta.poem.844": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.  Auff den Sontag de\u00df vor vns verbergenen  \n Helffers/ oder  Jubilate.  Johan. 16.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Kom' vnd schawe doch wie ich nun in lauter Threnenfliesse! ", "tokens": ["Kom'", "vnd", "scha\u00b7we", "doch", "wie", "ich", "nun", "in", "lau\u00b7ter", "Thre\u00b7nen\u00b7flies\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "ADV", "KOKOM", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Kom de\u00f1 ich nicht sehen kan/ kom mein schmertz nimt", "tokens": ["Kom", "de\u00f1", "ich", "nicht", "se\u00b7hen", "kan", "/", "kom", "mein", "schmertz", "nimt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "VVFIN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u00fcberhand ", "tokens": ["\u00fc\u00b7ber\u00b7hand"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Kom es hat ", "tokens": ["Kom", "es", "hat"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "sich gewand ", "tokens": ["sich", "ge\u00b7wand"], "token_info": ["word", "word"], "pos": ["PRF", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Kom eh\u2019 ich die grim\u0303e noth mit dem letzten Seuffzer schliesse!", "tokens": ["Kom", "eh'", "ich", "die", "grim\u0303e", "noth", "mit", "dem", "letz\u00b7ten", "Seuff\u00b7zer", "schlies\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Eh\u2019 ich den gepress\u2019ten Geist mit der Jammer klag au\u00dfgiesse.", "tokens": ["Eh'", "ich", "den", "ge\u00b7press'\u00b7ten", "Geist", "mit", "der", "Jam\u00b7mer", "klag", "au\u00df\u00b7gies\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Gleich wie ein hochschwanger Leib der die herbe zeit er-", "tokens": ["Gleich", "wie", "ein", "hoch\u00b7schwan\u00b7ger", "Leib", "der", "die", "her\u00b7be", "zeit", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ART", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "kant ", "tokens": ["kant"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die Jhm zu der arbeit rufft/ schmachtet in der weh-muth", "tokens": ["Die", "Jhm", "zu", "der", "ar\u00b7beit", "rufft", "/", "schmach\u00b7tet", "in", "der", "weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "band ", "tokens": ["band"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Also beb\u2019 ich! Kom mein ", "tokens": ["Al\u00b7so", "beb'", "ich", "!", "Kom", "mein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "PPOSAT"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "s\u00fcsse ", "tokens": ["s\u00fcs\u00b7se"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Dieses Wermutt herbe Creutz! diese Welt ist frewden voll", "tokens": ["Die\u00b7ses", "Wer\u00b7mutt", "her\u00b7be", "Creutz", "!", "die\u00b7se", "Welt", "ist", "frew\u00b7den", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN", "$.", "PDAT", "NN", "VAFIN", "ADJD", "ADJD"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Vnd weis schier nicht wie sie recht/ mich Verla\u00df\u2019nen h\u00f6h-", "tokens": ["Vnd", "weis", "schier", "nicht", "wie", "sie", "recht", "/", "mich", "Ver\u00b7la\u00df'\u00b7nen", "h\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "ADV", "PTKNEG", "PWAV", "PPER", "ADJD", "$(", "PPER", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nen soll ", "tokens": ["nen", "soll"], "token_info": ["word", "word"], "pos": ["VVINF", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Doch wird jhr kurtze lust sich in langes Leid verkehren", "tokens": ["Doch", "wird", "jhr", "kurt\u00b7ze", "lust", "sich", "in", "lan\u00b7ges", "Leid", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie sich aber nach der Noth ein Weib an der Frucht er-", "tokens": ["Wie", "sich", "a\u00b7ber", "nach", "der", "Noth", "ein", "Weib", "an", "der", "Frucht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "getzt ", "tokens": ["getzt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Also wird mein mattes Hertz/ dem die kurtze Qual zusetzt", "tokens": ["Al\u00b7so", "wird", "mein", "mat\u00b7tes", "Hertz", "/", "dem", "die", "kurt\u00b7ze", "Qual", "zu\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dich mit h\u00f6chster wonne schawn/ die in ewigkeit wird weh-", "tokens": ["Dich", "mit", "h\u00f6chs\u00b7ter", "won\u00b7ne", "schawn", "/", "die", "in", "e\u00b7wig\u00b7keit", "wird", "weh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$(", "ART", "APPR", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "ren. ", "tokens": ["ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}