{"textgrid.poem.45116": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[gott sagte nein]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.57", "da:0.14", "af:0.14", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott sagte nein,", "tokens": ["Gott", "sag\u00b7te", "nein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKANT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich aber sagte ja;", "tokens": ["Ich", "a\u00b7ber", "sag\u00b7te", "ja", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch als ich es ins Werk gesetzt,", "tokens": ["Doch", "als", "ich", "es", "ins", "Werk", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stand nur ein Nein mir da.", "tokens": ["Stand", "nur", "ein", "Nein", "mir", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}