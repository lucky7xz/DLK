{"dta.poem.22107": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "210. Je auffgegebner/ je G\u00f6ttlicher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Die Heilgen sind so vil von Gottes Gottheit trunken/", "tokens": ["Die", "Heil\u00b7gen", "sind", "so", "vil", "von", "Got\u00b7tes", "Got\u00b7theit", "trun\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So vil sie sind in jhm verlohren und versunken.", "tokens": ["So", "vil", "sie", "sind", "in", "jhm", "ver\u00b7loh\u00b7ren", "und", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}