{"textgrid.poem.41799": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Deinem Kerker klangen mir einst Lieder", "tokens": ["Aus", "Dei\u00b7nem", "Ker\u00b7ker", "klan\u00b7gen", "mir", "einst", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Liebeslust und wollten mich bereden,", "tokens": ["Voll", "Lie\u00b7bes\u00b7lust", "und", "woll\u00b7ten", "mich", "be\u00b7re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst hinter Eisengittern sei ein Eden,", "tokens": ["Selbst", "hin\u00b7ter", "Ei\u00b7sen\u00b7git\u00b7tern", "sei", "ein", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil ich Dir Rosen warf durch sie hernieder.", "tokens": ["Weil", "ich", "Dir", "Ro\u00b7sen", "warf", "durch", "sie", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als endlich wurde Dir die Freiheit wieder,", "tokens": ["Als", "end\u00b7lich", "wur\u00b7de", "Dir", "die", "Frei\u00b7heit", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ward ein Paradies von uns betreten,", "tokens": ["Da", "ward", "ein", "Pa\u00b7ra\u00b7dies", "von", "uns", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dem der Liebe holde Geister wehten,", "tokens": ["In", "dem", "der", "Lie\u00b7be", "hol\u00b7de", "Geis\u00b7ter", "weh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vereinigt klangen unsre Jubellieder.", "tokens": ["Ver\u00b7ei\u00b7nigt", "klan\u00b7gen", "uns\u00b7re", "Ju\u00b7bel\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vereinigt dienten wir des Hauses Laren,", "tokens": ["Ver\u00b7ei\u00b7nigt", "dien\u00b7ten", "wir", "des", "Hau\u00b7ses", "La\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In s\u00fc\u00dfer Liebe und im heil'gen Streben", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Lie\u00b7be", "und", "im", "heil'\u00b7gen", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vereinigt auch f\u00fcr eine Welt zu leben.", "tokens": ["Ver\u00b7ei\u00b7nigt", "auch", "f\u00fcr", "ei\u00b7ne", "Welt", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie wir im Ungl\u00fcck treu geblieben waren", "tokens": ["Wie", "wir", "im", "Un\u00b7gl\u00fcck", "treu", "ge\u00b7blie\u00b7ben", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Schwur: der Freiheit Fahne zu entfalten,", "tokens": ["Dem", "Schwur", ":", "der", "Frei\u00b7heit", "Fah\u00b7ne", "zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So haben wir ihn auch im Gl\u00fcck gehalten.", "tokens": ["So", "ha\u00b7ben", "wir", "ihn", "auch", "im", "Gl\u00fcck", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}