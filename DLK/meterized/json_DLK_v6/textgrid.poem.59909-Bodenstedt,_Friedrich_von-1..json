{"textgrid.poem.59909": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich glaub', was der Prophet verhie\u00df,", "tokens": ["Ich", "glaub'", ",", "was", "der", "Pro\u00b7phet", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Lohn f\u00fcr gutes Streben wird", "tokens": ["Da\u00df", "Lohn", "f\u00fcr", "gu\u00b7tes", "Stre\u00b7ben", "wird"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und uns dereinst im Paradies", "tokens": ["Und", "uns", "de\u00b7reinst", "im", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wunderbares Leben wird \u2013", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7res", "Le\u00b7ben", "wird", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch alles Sch\u00f6ne hier und dort", "tokens": ["Doch", "al\u00b7les", "Sch\u00f6\u00b7ne", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df man erkennen lernen,", "tokens": ["Mu\u00df", "man", "er\u00b7ken\u00b7nen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Will man es sicher immerfort", "tokens": ["Will", "man", "es", "si\u00b7cher", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Schlechten trennen lernen.", "tokens": ["Vom", "Schlech\u00b7ten", "tren\u00b7nen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drum \u00fcb' ich mich schon in der Zeit", "tokens": ["Drum", "\u00fcb'", "ich", "mich", "schon", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf den Genu\u00df der Ewigkeit.", "tokens": ["Auf", "den", "Ge\u00b7nu\u00df", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und sollte des Propheten Wort", "tokens": ["Und", "soll\u00b7te", "des", "Pro\u00b7phe\u00b7ten", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "(wer kann dar\u00fcber klar sein?)", "tokens": ["(", "wer", "kann", "da\u00b7r\u00fc\u00b7ber", "klar", "sein", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PAV", "ADJD", "VAINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Von ew'gen Himmelsfreuden dort", "tokens": ["Von", "ew'\u00b7gen", "Him\u00b7mels\u00b7freu\u00b7den", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nicht, wie wir hoffen, wahr sein,", "tokens": ["Nicht", ",", "wie", "wir", "hof\u00b7fen", ",", "wahr", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "So hab' ich doch schon in der Zeit", "tokens": ["So", "hab'", "ich", "doch", "schon", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein gutes Teil erkoren,", "tokens": ["Ein", "gu\u00b7tes", "Teil", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und die gew\u00fcnschte Seligkeit", "tokens": ["Und", "die", "ge\u00b7w\u00fcnschte", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.18": {"text": "Ging mir nicht ganz verloren!", "tokens": ["Ging", "mir", "nicht", "ganz", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}