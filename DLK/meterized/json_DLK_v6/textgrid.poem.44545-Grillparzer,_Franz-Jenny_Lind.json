{"textgrid.poem.44545": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Jenny Lind", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie nennen dich die Nachtigall", "tokens": ["Sie", "nen\u00b7nen", "dich", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit d\u00fcrftgem Bilderraube,", "tokens": ["Mit", "d\u00fcrft\u00b7gem", "Bil\u00b7der\u00b7rau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So s\u00fc\u00df auch deiner Lieder Schall,", "tokens": ["So", "s\u00fc\u00df", "auch", "dei\u00b7ner", "Lie\u00b7der", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nenn ich dich: die Taube.", "tokens": ["Doch", "nenn", "ich", "dich", ":", "die", "Tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$.", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und bist du Rose, wie dus bist,", "tokens": ["Und", "bist", "du", "Ro\u00b7se", ",", "wie", "dus", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NE", "$,", "PWAV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seis denn die Alpenrose,", "tokens": ["Seis", "denn", "die", "Al\u00b7pen\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, wo sich Schnee und Leben k\u00fc\u00dft,", "tokens": ["Die", ",", "wo", "sich", "Schnee", "und", "Le\u00b7ben", "k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufgl\u00fcht aus dunklem Moose.", "tokens": ["Auf\u00b7gl\u00fcht", "aus", "dunk\u00b7lem", "Moo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist nicht Farbe, bist das Licht,", "tokens": ["Du", "bist", "nicht", "Far\u00b7be", ",", "bist", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Farben erst verk\u00fcndet,", "tokens": ["Das", "Far\u00b7ben", "erst", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das, wenn sein Wei\u00df an Fremdem bricht,", "tokens": ["Das", ",", "wenn", "sein", "Wei\u00df", "an", "Frem\u00b7dem", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die bunte Pracht entz\u00fcndet.", "tokens": ["Die", "bun\u00b7te", "Pracht", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und spenden sie des Beifalls Lohn", "tokens": ["Und", "spen\u00b7den", "sie", "des", "Bei\u00b7falls", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wundern deiner Kehle,", "tokens": ["Den", "Wun\u00b7dern", "dei\u00b7ner", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier ist nicht K\u00f6rper, kaum noch Ton,", "tokens": ["Hier", "ist", "nicht", "K\u00f6r\u00b7per", ",", "kaum", "noch", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00f6re deine Seele.", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}