{"textgrid.poem.51374": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nymphe, weil ich werffe mich,", "tokens": ["Nym\u00b7phe", ",", "weil", "ich", "werf\u00b7fe", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir geb\u00fcckter vor die F\u00fcsse:", "tokens": ["Dir", "ge\u00b7b\u00fcck\u00b7ter", "vor", "die", "F\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Saum des Rockes K\u00fcsse,", "tokens": ["Und", "den", "Saum", "des", "Ro\u00b7ckes", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ey so la\u00df erbitten dich.", "tokens": ["Ey", "so", "la\u00df", "er\u00b7bit\u00b7ten", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar, du wilt mit deiner Hand", "tokens": ["Zwar", ",", "du", "wilt", "mit", "dei\u00b7ner", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Erden mich aufheben,", "tokens": ["Von", "der", "Er\u00b7den", "mich", "auf\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit ein sch\u00f6nes Antlitz geben:", "tokens": ["Mit", "ein", "sch\u00f6\u00b7nes", "Ant\u00b7litz", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dein Hertz ist unverwand.", "tokens": ["Doch", "dein", "Hertz", "ist", "un\u00b7ver\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun ein Circkel ist der Saum:", "tokens": ["Nun", "ein", "Cir\u00b7ckel", "ist", "der", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcber wolt ich gerne kommen,", "tokens": ["Dr\u00fc\u00b7ber", "wolt", "ich", "ger\u00b7ne", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Davon hast du nichts vernommen,", "tokens": ["Da\u00b7von", "hast", "du", "nichts", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drumb erlang ich da nicht Raum.", "tokens": ["Drumb", "er\u00b7lang", "ich", "da", "nicht", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}