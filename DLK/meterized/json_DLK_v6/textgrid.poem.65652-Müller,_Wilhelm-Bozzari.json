{"textgrid.poem.65652": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Bozzari", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frei flog seine Heldenseele aus des Busens offnen Wunden", "tokens": ["Frei", "flog", "sei\u00b7ne", "Hel\u00b7den\u00b7see\u00b7le", "aus", "des", "Bu\u00b7sens", "off\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In das Reich der Freiheit auf. Oder will sie noch verweilen", "tokens": ["In", "das", "Reich", "der", "Frei\u00b7heit", "auf", ".", "O\u00b7der", "will", "sie", "noch", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$.", "KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Unter uns und jeden Kampf mit den Erdenbr\u00fcdern theilen?", "tokens": ["Un\u00b7ter", "uns", "und", "je\u00b7den", "Kampf", "mit", "den", "Er\u00b7den\u00b7br\u00fc\u00b7dern", "thei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "O so sei gegr\u00fc\u00dft im Streite, sei gegr\u00fc\u00dft beim Siegesmahle!", "tokens": ["O", "so", "sei", "ge\u00b7gr\u00fc\u00dft", "im", "Strei\u00b7te", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "beim", "Sie\u00b7ges\u00b7mah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "VVPP", "APPRART", "NN", "$,", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wollen dir die ersten Tropfen aus dem sch\u00e4umenden Pokale", "tokens": ["Wol\u00b7len", "dir", "die", "ers\u00b7ten", "Trop\u00b7fen", "aus", "dem", "sch\u00e4u\u00b7men\u00b7den", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Auf den Grabesh\u00fcgel sch\u00fctten, und die ersten Lorbeerzweige", "tokens": ["Auf", "den", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", "sch\u00fct\u00b7ten", ",", "und", "die", "ers\u00b7ten", "Lor\u00b7beer\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Auf den nassen Rasen legen. Freier, sel'ger Geist, dann neige", "tokens": ["Auf", "den", "nas\u00b7sen", "Ra\u00b7sen", "le\u00b7gen", ".", "Frei\u00b7er", ",", "sel'\u00b7ger", "Geist", ",", "dann", "nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$.", "NN", "$,", "ADJA", "NN", "$,", "ADV", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Segnend dich herab und fache hell in uns empor die Gluthen,", "tokens": ["Seg\u00b7nend", "dich", "her\u00b7ab", "und", "fa\u00b7che", "hell", "in", "uns", "em\u00b7por", "die", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "KON", "ADJA", "ADJD", "APPR", "PPER", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Die auch mit des Heldenblutes letztem Tropfen nicht verbluten,", "tokens": ["Die", "auch", "mit", "des", "Hel\u00b7den\u00b7blu\u00b7tes", "letz\u00b7tem", "Trop\u00b7fen", "nicht", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Die noch heut' im Staube brennen unter Pyl\u00e4's heil'gen Gr\u00fcften,", "tokens": ["Die", "noch", "heut'", "im", "Stau\u00b7be", "bren\u00b7nen", "un\u00b7ter", "Py\u00b7l\u00e4's", "heil'\u00b7gen", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VVINF", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Die auf Marathons Gefilden ewig wehen in den L\u00fcften,", "tokens": ["Die", "auf", "Ma\u00b7ra\u00b7thons", "Ge\u00b7fil\u00b7den", "e\u00b7wig", "we\u00b7hen", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Die wir alle in uns trinken recht in vollen, hei\u00dfen Z\u00fcgen,", "tokens": ["Die", "wir", "al\u00b7le", "in", "uns", "trin\u00b7ken", "recht", "in", "vol\u00b7len", ",", "hei\u00b7\u00dfen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "APPR", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}