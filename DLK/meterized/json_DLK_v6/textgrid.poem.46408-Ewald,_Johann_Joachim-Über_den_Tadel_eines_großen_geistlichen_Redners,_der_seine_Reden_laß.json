{"textgrid.poem.46408": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber den Tadel eines gro\u00dfen geistlichen Redners, der seine Reden la\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K* will ein Redner seyn? er lie\u00dft ja was er spricht;", "tokens": ["K", "*", "will", "ein", "Red\u00b7ner", "seyn", "?", "er", "lie\u00dft", "ja", "was", "er", "spricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "VMFIN", "ART", "NN", "VAINF", "$.", "PPER", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So reden w\u00fcrd ich auch! So schreiben? Nein das nicht!", "tokens": ["So", "re\u00b7den", "w\u00fcrd", "ich", "auch", "!", "So", "schrei\u00b7ben", "?", "Nein", "das", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PPER", "ADV", "$.", "ADV", "VVINF", "$.", "PTKANT", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}