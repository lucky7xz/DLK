{"textgrid.poem.45733": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Einer Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeig Kopf und Herz einander ebenb\u00fcrtig,", "tokens": ["Zeig", "Kopf", "und", "Herz", "ein\u00b7an\u00b7der", "e\u00b7ben\u00b7b\u00fcr\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben hat dich dulden ja gelehrt.", "tokens": ["Das", "Le\u00b7ben", "hat", "dich", "dul\u00b7den", "ja", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Sohn war einer solchen Mutter w\u00fcrdig,", "tokens": ["Dein", "Sohn", "war", "ei\u00b7ner", "sol\u00b7chen", "Mut\u00b7ter", "w\u00fcr\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei du auch eines solchen Sohnes wert.", "tokens": ["Sei", "du", "auch", "ei\u00b7nes", "sol\u00b7chen", "Soh\u00b7nes", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}