{"dta.poem.21596": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Schuld und Schicksal \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Schuld und Schicksal schlagen ums Haupt des Menschen", "tokens": ["Schuld", "und", "Schick\u00b7sal", "schla\u00b7gen", "ums", "Haupt", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ihre lebensfeindlichen finstren Schwingen,", "tokens": ["Ih\u00b7re", "le\u00b7bens\u00b7feind\u00b7li\u00b7chen", "finst\u00b7ren", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Selten k\u00fc\u00dft ihn irgend ein Strahl der Freude", "tokens": ["Sel\u00b7ten", "k\u00fc\u00dft", "ihn", "ir\u00b7gend", "ein", "Strahl", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Fl\u00fcchtig begl\u00fcckend.", "tokens": ["Fl\u00fcch\u00b7tig", "be\u00b7gl\u00fc\u00b7ckend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber dennoch wittert und sp\u00fcrt die Seele", "tokens": ["A\u00b7ber", "den\u00b7noch", "wit\u00b7tert", "und", "sp\u00fcrt", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ueber all der lastenden Nacht der Schmerzen", "tokens": ["Ue\u00b7ber", "all", "der", "las\u00b7ten\u00b7den", "Nacht", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Eines reinen, nimmergetr\u00fcbten Himmels", "tokens": ["Ei\u00b7nes", "rei\u00b7nen", ",", "nim\u00b7mer\u00b7ge\u00b7tr\u00fcb\u00b7ten", "Him\u00b7mels"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "G\u00f6ttliche Klarheit! \u2014", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Harren stets und hoffen und aufw\u00e4rts blicken", "tokens": ["Har\u00b7ren", "stets", "und", "hof\u00b7fen", "und", "auf\u00b7w\u00e4rts", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "KON", "VVFIN", "KON", "ADV", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Mit der Sehnsucht gl\u00fchendem Seherauge", "tokens": ["Mit", "der", "Sehn\u00b7sucht", "gl\u00fc\u00b7hen\u00b7dem", "Se\u00b7her\u00b7au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Mu\u00df der Mensch. Zu bitter und herb entt\u00e4uschet", "tokens": ["Mu\u00df", "der", "Mensch", ".", "Zu", "bit\u00b7ter", "und", "herb", "ent\u00b7t\u00e4u\u00b7schet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "$.", "PTKA", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Leben und Welt ihn.", "tokens": ["Le\u00b7ben", "und", "Welt", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}