{"textgrid.poem.56906": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie wollen mich umgarnen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wollen mich umgarnen,", "tokens": ["Sie", "wol\u00b7len", "mich", "um\u00b7gar\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sie wollen mich fort rei\u00dfen \u2013", "tokens": ["sie", "wol\u00b7len", "mich", "fort", "rei\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "aber ich werfe mich", "tokens": ["a\u00b7ber", "ich", "wer\u00b7fe", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "an deine heilige Brust,", "tokens": ["an", "dei\u00b7ne", "hei\u00b7li\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mutter Erde ...", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mit weiten H\u00e4nden", "tokens": ["Mit", "wei\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "greif ich in deine Schollen,", "tokens": ["greif", "ich", "in", "dei\u00b7ne", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "mit tiefen Z\u00fcgen", "tokens": ["mit", "tie\u00b7fen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "schl\u00fcrf ich den herben Duft", "tokens": ["schl\u00fcrf", "ich", "den", "her\u00b7ben", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "deiner Kr\u00e4uter ...", "tokens": ["dei\u00b7ner", "Kr\u00e4u\u00b7ter", "..."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Nein, Du", "tokens": ["Nein", ",", "Du"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER"], "meter": "++", "measure": "spondeus"}, "line.12": {"text": "verl\u00e4ssest mich nicht,", "tokens": ["ver\u00b7l\u00e4s\u00b7sest", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "du n\u00e4hrst mich,", "tokens": ["du", "n\u00e4hrst", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "du st\u00e4rkst mich,", "tokens": ["du", "st\u00e4rkst", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "da\u00df die b\u00f6sen Geister", "tokens": ["da\u00df", "die", "b\u00f6\u00b7sen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "mich lassen m\u00fcssen,", "tokens": ["mich", "las\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "und ich hoch und heiter", "tokens": ["und", "ich", "hoch", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "wieder des Weges wandere,", "tokens": ["wie\u00b7der", "des", "We\u00b7ges", "wan\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "den ich mir kor.", "tokens": ["den", "ich", "mir", "kor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Daf\u00fcr will ich dich auch", "tokens": ["Da\u00b7f\u00fcr", "will", "ich", "dich", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "ohn End, ohn Ende", "tokens": ["ohn", "End", ",", "ohn", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "lieben und preisen ...", "tokens": ["lie\u00b7ben", "und", "prei\u00b7sen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Und wenn du mich einst", "tokens": ["Und", "wenn", "du", "mich", "einst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "vom Strahl der Sonne", "tokens": ["vom", "Strahl", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "zur\u00fcckheischst,", "tokens": ["zu\u00b7r\u00fcck\u00b7heischst", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "dann will ich", "tokens": ["dann", "will", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "mein Haupt still", "tokens": ["mein", "Haupt", "still"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "in deinen Scho\u00df betten ...", "tokens": ["in", "dei\u00b7nen", "Scho\u00df", "bet\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.29": {"text": "Und du wirst", "tokens": ["Und", "du", "wirst"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "meinen Schlummer beh\u00fcten", "tokens": ["mei\u00b7nen", "Schlum\u00b7mer", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "von Ewigkeit", "tokens": ["von", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "zu Ewigkeit.", "tokens": ["zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}