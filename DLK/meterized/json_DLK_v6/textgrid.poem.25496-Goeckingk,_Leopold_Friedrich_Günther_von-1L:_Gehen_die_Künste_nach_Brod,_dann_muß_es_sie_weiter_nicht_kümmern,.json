{"textgrid.poem.25496": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gehen die K\u00fcnste nach Brod, dann mu\u00df es sie weiter nicht k\u00fcmmern,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehen die K\u00fcnste nach Brod, dann mu\u00df es sie weiter nicht k\u00fcmmern,", "tokens": ["Ge\u00b7hen", "die", "K\u00fcns\u00b7te", "nach", "Brod", ",", "dann", "mu\u00df", "es", "sie", "wei\u00b7ter", "nicht", "k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,", "ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wen am Ziele der Ruhm mit Amaranten bekr\u00e4nzt.", "tokens": ["Wen", "am", "Zie\u00b7le", "der", "Ruhm", "mit", "A\u00b7ma\u00b7ran\u00b7ten", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weinend m\u00fcssen sie dann die Fl\u00fcgel des Genius binden,", "tokens": ["Wei\u00b7nend", "m\u00fcs\u00b7sen", "sie", "dann", "die", "Fl\u00fc\u00b7gel", "des", "Ge\u00b7nius", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Der, \u00fcberlassen sich selbst, sonst Ideale verfolgt,", "tokens": ["Der", ",", "\u00fc\u00b7ber\u00b7las\u00b7sen", "sich", "selbst", ",", "sonst", "I\u00b7dea\u00b7le", "ver\u00b7folgt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PRF", "ADV", "$,", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Die, von keinem erreicht, nur dem im d\u00fcnneren Aether", "tokens": ["Die", ",", "von", "kei\u00b7nem", "er\u00b7reicht", ",", "nur", "dem", "im", "d\u00fcn\u00b7ne\u00b7ren", "A\u00b7e\u00b7ther"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "PIS", "VVPP", "$,", "ADV", "ART", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Sichtbar werden, der hoch \u00fcber den P\u00f6bel sich schwingt.", "tokens": ["Sicht\u00b7bar", "wer\u00b7den", ",", "der", "hoch", "\u00fc\u00b7ber", "den", "P\u00f6\u00b7bel", "sich", "schwingt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Aber wenn er im Volk', gebunden, hier unten verweilet,", "tokens": ["A\u00b7ber", "wenn", "er", "im", "Vol\u00b7k'", ",", "ge\u00b7bun\u00b7den", ",", "hier", "un\u00b7ten", "ver\u00b7wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "$,", "VVPP", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Bringt er zur Erde uns nichts neues vom Himmel herab.", "tokens": ["Bringt", "er", "zur", "Er\u00b7de", "uns", "nichts", "neu\u00b7es", "vom", "Him\u00b7mel", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "PIS", "ADJA", "APPRART", "NN", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Was der reiche D\u00fcnkel ersinnt, sich selber zu schmeicheln,", "tokens": ["Was", "der", "rei\u00b7che", "D\u00fcn\u00b7kel", "er\u00b7sinnt", ",", "sich", "sel\u00b7ber", "zu", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "F\u00fchrt, verw\u00fcnschend sich selbst, str\u00e4ubend der Genius aus.", "tokens": ["F\u00fchrt", ",", "ver\u00b7w\u00fcn\u00b7schend", "sich", "selbst", ",", "str\u00e4u\u00b7bend", "der", "Ge\u00b7nius", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "PRF", "ADV", "$,", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Gehen die K\u00fcnste nach Ruf, dann mu\u00df es sie weiter nicht k\u00fcmmern,", "tokens": ["Ge\u00b7hen", "die", "K\u00fcns\u00b7te", "nach", "Ruf", ",", "dann", "mu\u00df", "es", "sie", "wei\u00b7ter", "nicht", "k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,", "ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Was erheischt die Natur, oder der Kritiker w\u00fcnscht.", "tokens": ["Was", "er\u00b7heischt", "die", "Na\u00b7tur", ",", "o\u00b7der", "der", "Kri\u00b7ti\u00b7ker", "w\u00fcnscht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dann so wird die Stadt zur Welt, die Zeitung zur Nachwelt;", "tokens": ["Dann", "so", "wird", "die", "Stadt", "zur", "Welt", ",", "die", "Zei\u00b7tung", "zur", "Nach\u00b7welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+--++", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gaffender Menge Geklatsch: Genius, Regel, Modell!", "tokens": ["Gaf\u00b7fen\u00b7der", "Men\u00b7ge", "Ge\u00b7klatsch", ":", "Ge\u00b7nius", ",", "Re\u00b7gel", ",", "Mo\u00b7dell", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+---+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ach! sie w\u00fcrdigen sich herab zu Putzmacherinnen;", "tokens": ["Ach", "!", "sie", "w\u00fcr\u00b7di\u00b7gen", "sich", "her\u00b7ab", "zu", "Putz\u00b7ma\u00b7che\u00b7rin\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Einer ", "tokens": ["Ei\u00b7ner"], "token_info": ["word"], "pos": ["PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Bildete nicht selbst ", "tokens": ["Bil\u00b7de\u00b7te", "nicht", "selbst"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "F\u00fcr der Familie Gold, ", "tokens": ["F\u00fcr", "der", "Fa\u00b7mi\u00b7lie", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und beh\u00e4ngte den Schulthei\u00df von ", "tokens": ["Und", "be\u00b7h\u00e4ng\u00b7te", "den", "Schult\u00b7hei\u00df", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.10": {"text": "Bunt mit Fahnen und Helm, Wapen und Orden und Speer?", "tokens": ["Bunt", "mit", "Fah\u00b7nen", "und", "Helm", ",", "Wa\u00b7pen", "und", "Or\u00b7den", "und", "Speer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Aber der Fremde, sch\u00fcttelt den Kopf, blickt l\u00e4chelnd und kalt hin,", "tokens": ["A\u00b7ber", "der", "Frem\u00b7de", ",", "sch\u00fct\u00b7telt", "den", "Kopf", ",", "blickt", "l\u00e4\u00b7chelnd", "und", "kalt", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+--+-+-+++", "measure": "iambic.octa.plus.invert"}, "line.12": {"text": "W\u00e4hrend der K\u00fcster ihm stolz Ahnen und Kosten summirt.", "tokens": ["W\u00e4h\u00b7rend", "der", "K\u00fcs\u00b7ter", "ihm", "stolz", "Ah\u00b7nen", "und", "Kos\u00b7ten", "sum\u00b7mirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Doch der Genius hat in einem bescheidenen Sandstein'", "tokens": ["Doch", "der", "Ge\u00b7nius", "hat", "in", "ei\u00b7nem", "be\u00b7schei\u00b7de\u00b7nen", "Sand\u00b7stein'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.14": {"text": "Kunst und K\u00fcnstler daf\u00fcr gleich auf der Stelle ger\u00e4cht.", "tokens": ["Kunst", "und", "K\u00fcnst\u00b7ler", "da\u00b7f\u00fcr", "gleich", "auf", "der", "Stel\u00b7le", "ge\u00b7r\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Schneller, erleichternd die M\u00fch', die wieder erwachende ", "tokens": ["Schnel\u00b7ler", ",", "er\u00b7leich\u00b7ternd", "die", "M\u00fch'", ",", "die", "wie\u00b7der", "er\u00b7wa\u00b7chen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "ART", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Aus dem Grabe zu ziehn, strecket des Reisenden Arm", "tokens": ["Aus", "dem", "Gra\u00b7be", "zu", "ziehn", ",", "stre\u00b7cket", "des", "Rei\u00b7sen\u00b7den", "Arm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unwillk\u00fchrlich sich ihr entgegen; aber sein Mitleid", "tokens": ["Un\u00b7will\u00b7k\u00fchr\u00b7lich", "sich", "ihr", "ent\u00b7ge\u00b7gen", ";", "a\u00b7ber", "sein", "Mit\u00b7leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PRF", "PPER", "PTKVZ", "$.", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wandelt, erkennend den Stein, schnell in Erstaunen sich um.", "tokens": ["Wan\u00b7delt", ",", "er\u00b7ken\u00b7nend", "den", "Stein", ",", "schnell", "in", "Er\u00b7stau\u00b7nen", "sich", "um", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "ART", "NN", "$,", "ADJD", "APPR", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "H\u00e4tt' auch ", "tokens": ["H\u00e4tt'", "auch"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Unsinn mit unter sogar, schon in der Prosa gef\u00fchlt,", "tokens": ["Un\u00b7sinn", "mit", "un\u00b7ter", "so\u00b7gar", ",", "schon", "in", "der", "Pro\u00b7sa", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADV", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nun so mu\u00dfte doch wohl bei so genannten Ges\u00e4ngen", "tokens": ["Nun", "so", "mu\u00df\u00b7te", "doch", "wohl", "bei", "so", "ge\u00b7nann\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ADV", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "--+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sein melodisches Ohr minder nicht leiden, als der,", "tokens": ["Sein", "me\u00b7lo\u00b7di\u00b7sches", "Ohr", "min\u00b7der", "nicht", "lei\u00b7den", ",", "als", "der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PTKNEG", "VVINF", "$,", "KOUS", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den im Trabe die Post auf einem Damme von Kn\u00fcppeln,", "tokens": ["Den", "im", "Tra\u00b7be", "die", "Post", "auf", "ei\u00b7nem", "Dam\u00b7me", "von", "Kn\u00fcp\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Packen und F\u00e4sser im Tanz', rasselnd mit Ketten, erreicht.", "tokens": ["Pa\u00b7cken", "und", "F\u00e4s\u00b7ser", "im", "Tanz'", ",", "ras\u00b7selnd", "mit", "Ket\u00b7ten", ",", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$,", "VVPP", "APPR", "NN", "$,", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Du, der weder um Gold, noch um das Bravo der Menge,", "tokens": ["Du", ",", "der", "we\u00b7der", "um", "Gold", ",", "noch", "um", "das", "Bra\u00b7vo", "der", "Men\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "KON", "APPR", "NN", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Je dich k\u00fcmmerst, o ", "tokens": ["Je", "dich", "k\u00fcm\u00b7merst", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "FM"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "(beide reichen bei dir sich wie Verlobte die H\u00e4nde,)", "tokens": ["(", "bei\u00b7de", "rei\u00b7chen", "bei", "dir", "sich", "wie", "Ver\u00b7lob\u00b7te", "die", "H\u00e4n\u00b7de", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VVFIN", "APPR", "PPER", "PRF", "KOKOM", "NN", "ART", "NN", "$,", "$("], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Nur um ihrer selbst willen zu lieben, beschwurst,", "tokens": ["Nur", "um", "ih\u00b7rer", "selbst", "wil\u00b7len", "zu", "lie\u00b7ben", ",", "be\u00b7schwurst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADV", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.15": {"text": "Und zw\u00f6lf Lustern lang den Schwur hast treulich gehalten:", "tokens": ["Und", "zw\u00f6lf", "Lus\u00b7tern", "lang", "den", "Schwur", "hast", "treu\u00b7lich", "ge\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Beide flechten sie einst deinen nicht welkenden Kranz!", "tokens": ["Bei\u00b7de", "flech\u00b7ten", "sie", "einst", "dei\u00b7nen", "nicht", "wel\u00b7ken\u00b7den", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PPOSAT", "PTKNEG", "PWAT", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer Zerstreuung sucht, ist immer begierig nach Neuem,", "tokens": ["Wer", "Zer\u00b7streu\u00b7ung", "sucht", ",", "ist", "im\u00b7mer", "be\u00b7gie\u00b7rig", "nach", "Neu\u00b7em", ","], "token_info": ["word", "word", "word", "punct", "word", "word",