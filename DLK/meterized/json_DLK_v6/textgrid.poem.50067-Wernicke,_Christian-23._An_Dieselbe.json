{"textgrid.poem.50067": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "23. An Dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr durch eure ", "tokens": ["Die", "ihr", "durch", "eu\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Durch die den ", "tokens": ["Durch", "die", "den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Durch diese der Gefahr die M\u00e4nner habt entzogen,", "tokens": ["Durch", "die\u00b7se", "der", "Ge\u00b7fahr", "die", "M\u00e4n\u00b7ner", "habt", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und aller Welt gezeigt, ", "tokens": ["Und", "al\u00b7ler", "Welt", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Legt eure B\u00fcrd' itzt ab, vertauscht die ", "tokens": ["Legt", "eu\u00b7re", "B\u00fcrd'", "itzt", "ab", ",", "ver\u00b7tauscht", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Legt sie ", "tokens": ["Legt", "sie"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Macht dass ein Wunderwerck das andre nach sich zieh',", "tokens": ["Macht", "dass", "ein", "Wun\u00b7der\u00b7werck", "das", "and\u00b7re", "nach", "sich", "zieh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "ART", "ADJA", "APPR", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Den solche Liebe st\u00e4rckt die ", "tokens": ["Den", "sol\u00b7che", "Lie\u00b7be", "st\u00e4rckt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}