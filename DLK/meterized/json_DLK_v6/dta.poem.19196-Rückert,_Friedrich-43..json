{"dta.poem.19196": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Onyx wird gesagt, da\u00df er, im Ring gefa\u00dft,", "tokens": ["Vom", "O\u00b7nyx", "wird", "ge\u00b7sagt", ",", "da\u00df", "er", ",", "im", "Ring", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht einen, der ihn tr\u00e4gt, in jedem Ding gefa\u00dft,", "tokens": ["Macht", "ei\u00b7nen", ",", "der", "ihn", "tr\u00e4gt", ",", "in", "je\u00b7dem", "Ding", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und, wem ein solcher Stein zur Erbschaft ist gelassen,", "tokens": ["Und", ",", "wem", "ein", "sol\u00b7cher", "Stein", "zur", "Erb\u00b7schaft", "ist", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "ART", "PIAT", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gl\u00fcck und Ungl\u00fcck ist er jederzeit gelassen.", "tokens": ["In", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "ist", "er", "je\u00b7der\u00b7zeit", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wol so gefa\u00dft zu seyn, ist eine sch\u00f6ne Fassung,", "tokens": ["Wol", "so", "ge\u00b7fa\u00dft", "zu", "seyn", ",", "ist", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Fas\u00b7sung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PTKZU", "VAINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und die Gelassenheit die reichste Hinterlassung.", "tokens": ["Und", "die", "Ge\u00b7las\u00b7sen\u00b7heit", "die", "reichs\u00b7te", "Hin\u00b7ter\u00b7las\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch la\u00df dir deuten recht die Hinterlassenschaft,", "tokens": ["Doch", "la\u00df", "dir", "deu\u00b7ten", "recht", "die", "Hin\u00b7ter\u00b7las\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fa\u00df in deinen Sinn der Fassung Wunderkraft!", "tokens": ["Und", "fa\u00df", "in", "dei\u00b7nen", "Sinn", "der", "Fas\u00b7sung", "Wun\u00b7der\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der so gefa\u00dfte Ring will an der Hand dir reichen", "tokens": ["Der", "so", "ge\u00b7fa\u00df\u00b7te", "Ring", "will", "an", "der", "Hand", "dir", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zeichen, da\u00df dein Herz sich fassen soll desgleichen.", "tokens": ["Ein", "Zei\u00b7chen", ",", "da\u00df", "dein", "Herz", "sich", "fas\u00b7sen", "soll", "des\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn wol zu jeder Frist gefa\u00dft wird seyn ein Mann,", "tokens": ["Denn", "wol", "zu", "je\u00b7der", "Frist", "ge\u00b7fa\u00dft", "wird", "seyn", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "VVPP", "VAFIN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nie vergi\u00dft, da\u00df er gefa\u00dft seyn soll und kann.", "tokens": ["Der", "nie", "ver\u00b7gi\u00dft", ",", "da\u00df", "er", "ge\u00b7fa\u00dft", "seyn", "soll", "und", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So trag den Stein, und trag das Leben fein gelassen,", "tokens": ["So", "trag", "den", "Stein", ",", "und", "trag", "das", "Le\u00b7ben", "fein", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie der das Leben trug, der dir den Stein gelassen.", "tokens": ["Wie", "der", "das", "Le\u00b7ben", "trug", ",", "der", "dir", "den", "Stein", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ja sei wie er, der nun, in Grabesrund gefa\u00dft,", "tokens": ["Ja", "sei", "wie", "er", ",", "der", "nun", ",", "in", "Gra\u00b7be\u00b7srund", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "KOKOM", "PPER", "$,", "PRELS", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt gelassen hat, gelassen und gefa\u00dft!", "tokens": ["Die", "Welt", "ge\u00b7las\u00b7sen", "hat", ",", "ge\u00b7las\u00b7sen", "und", "ge\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}