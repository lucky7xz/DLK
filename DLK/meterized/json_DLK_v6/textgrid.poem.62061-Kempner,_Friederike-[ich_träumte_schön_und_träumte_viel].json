{"textgrid.poem.62061": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ich tr\u00e4umte sch\u00f6n und tr\u00e4umte viel]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte sch\u00f6n und tr\u00e4umte viel,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "sch\u00f6n", "und", "tr\u00e4um\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Leben schien ein Kinderspiel,", "tokens": ["Das", "Le\u00b7ben", "schien", "ein", "Kin\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gute schien so federleicht,", "tokens": ["Das", "Gu\u00b7te", "schien", "so", "fe\u00b7der\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als h\u00e4tte man es bald erreicht!", "tokens": ["Als", "h\u00e4t\u00b7te", "man", "es", "bald", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Leben ist ein Kampfesspiel,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Kamp\u00b7fes\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bot der Wunden schwer und viel,", "tokens": ["Und", "bot", "der", "Wun\u00b7den", "schwer", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gute ach, ein goldner Traum,", "tokens": ["Das", "Gu\u00b7te", "ach", ",", "ein", "gold\u00b7ner", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "XY", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erreichbar selten oder kaum!", "tokens": ["Er\u00b7reich\u00b7bar", "sel\u00b7ten", "o\u00b7der", "kaum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}