{"textgrid.poem.53626": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Heimg'funden", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der B\u00fcrger zieht die vollen Hosen", "tokens": ["Der", "B\u00fcr\u00b7ger", "zieht", "die", "vol\u00b7len", "Ho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sich h\u00f6her rauf und eilt zur Wahl.", "tokens": ["sich", "h\u00f6\u00b7her", "rauf", "und", "eilt", "zur", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm ist nach der Revolte Tosen", "tokens": ["Ihm", "ist", "nach", "der", "Re\u00b7vol\u00b7te", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "alles ejal.", "tokens": ["al\u00b7les", "e\u00b7jal", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nach diesem Krieg, nach diesen Putschen,", "tokens": ["Nach", "die\u00b7sem", "Krieg", ",", "nach", "die\u00b7sen", "Put\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nach Kapp \u2013 nach Willys starker Hand:", "tokens": ["nach", "Kapp", "\u2013", "nach", "Wil\u00b7lys", "star\u00b7ker", "Hand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du siehst ihm Herz und B\u00fcchsen rutschen.", "tokens": ["du", "siehst", "ihm", "Herz", "und", "B\u00fcch\u00b7sen", "rut\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er denkt an seinen Barbestand.", "tokens": ["Er", "denkt", "an", "sei\u00b7nen", "Bar\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach diesem Preu\u00dfen, diesem Morden,", "tokens": ["Nach", "die\u00b7sem", "Preu\u00b7\u00dfen", ",", "die\u00b7sem", "Mor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dem Tod, den noch Hans Paasche fand \u2013", "tokens": ["dem", "Tod", ",", "den", "noch", "Hans", "Paa\u00b7sche", "fand", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nach bunten Soldateskahorden:", "tokens": ["nach", "bun\u00b7ten", "Sol\u00b7da\u00b7tes\u00b7ka\u00b7hor\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er denkt an seinen Barbestand.", "tokens": ["Er", "denkt", "an", "sei\u00b7nen", "Bar\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbflamm auf, du Volk!\u00ab Du liebe G\u00fcte!", "tokens": ["\u00bb", "flamm", "auf", ",", "du", "Volk", "!", "\u00ab", "Du", "lie\u00b7be", "G\u00fc\u00b7te", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "PPER", "NN", "$.", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt ihnen ruhig ins Gesicht.", "tokens": ["Tritt", "ih\u00b7nen", "ru\u00b7hig", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wackeln die Zylinderh\u00fcte,", "tokens": ["Es", "wa\u00b7ckeln", "die", "Zy\u00b7lin\u00b7der\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er will ja nicht.", "tokens": ["Er", "will", "ja", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Hebammen und die Professoren,", "tokens": ["He\u00b7bam\u00b7men", "und", "die", "Pro\u00b7fes\u00b7so\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Schieberbraut, der Referendar \u2013", "tokens": ["die", "Schie\u00b7ber\u00b7braut", ",", "der", "Re\u00b7fe\u00b7ren\u00b7dar", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "sie haben ihren Herrn verloren;", "tokens": ["sie", "ha\u00b7ben", "ih\u00b7ren", "Herrn", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "jedoch das Herz bleibt, wie es war.", "tokens": ["je\u00b7doch", "das", "Herz", "bleibt", ",", "wie", "es", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie war es denn?", "tokens": ["Wie", "war", "es", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bei dem Getue,", "tokens": ["Bei", "dem", "Ge\u00b7tue", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "bei Streik und bei Revoltenbrand \u2013", "tokens": ["bei", "Streik", "und", "bei", "Re\u00b7vol\u00b7ten\u00b7brand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie wollten Ruhe, Ruhe, Ruhe", "tokens": ["sie", "woll\u00b7ten", "Ru\u00b7he", ",", "Ru\u00b7he", ",", "Ru\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und ihren Polizeisergeant.", "tokens": ["und", "ih\u00b7ren", "Po\u00b7li\u00b7zei\u00b7ser\u00b7ge\u00b7ant", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "So heilt der Deutsche seine Wunden.", "tokens": ["So", "heilt", "der", "Deut\u00b7sche", "sei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Herz aus Wachs, Ges\u00e4\u00df aus Stahl . . .", "tokens": ["Ein", "Herz", "aus", "Wachs", ",", "Ge\u00b7s\u00e4\u00df", "aus", "Stahl", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der B\u00fcrger hat sich heimgefunden.", "tokens": ["Der", "B\u00fcr\u00b7ger", "hat", "sich", "heim\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm ist auch in den Schicksalsstunden", "tokens": ["Ihm", "ist", "auch", "in", "den", "Schick\u00b7sals\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "alles ejal \u2013 alles ejal!", "tokens": ["al\u00b7les", "e\u00b7jal", "\u2013", "al\u00b7les", "e\u00b7jal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$(", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}