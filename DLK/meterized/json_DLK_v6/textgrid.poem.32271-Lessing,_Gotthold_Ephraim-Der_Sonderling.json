{"textgrid.poem.32271": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Sonderling", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bald der Mensch sich kennt,", "tokens": ["So", "bald", "der", "Mensch", "sich", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sieht er, er sei ein Narr;", "tokens": ["Sieht", "er", ",", "er", "sei", "ein", "Narr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gleichwohl z\u00fcrnt der Narr,", "tokens": ["Und", "gleich\u00b7wohl", "z\u00fcrnt", "der", "Narr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn man ihn also nennt.", "tokens": ["Wenn", "man", "ihn", "al\u00b7so", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So bald der Mensch sich kennt,", "tokens": ["So", "bald", "der", "Mensch", "sich", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sieht er, er sei nicht klug;", "tokens": ["Sieht", "er", ",", "er", "sei", "nicht", "klug", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ists ihm lieb genug,", "tokens": ["Doch", "ists", "ihm", "lieb", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn man ihn weise nennt.", "tokens": ["Wenn", "man", "ihn", "wei\u00b7se", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein jeder, der mich kennt,", "tokens": ["Ein", "je\u00b7der", ",", "der", "mich", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Spricht: welcher Sonderling!", "tokens": ["Spricht", ":", "wel\u00b7cher", "Son\u00b7der\u00b7ling", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur diesem ists Ein Ding,", "tokens": ["Nur", "die\u00b7sem", "ists", "Ein", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie ihn die Welt auch nennt.", "tokens": ["Wie", "ihn", "die", "Welt", "auch", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}