{"dta.poem.19522": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "La R\u00f6se.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Als der Bernina Felsenthor", "tokens": ["Als", "der", "Ber\u00b7ni\u00b7na", "Fel\u00b7sen\u00b7thor"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NE", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Durchdonnerte der Wagen", "tokens": ["Durch\u00b7don\u00b7ner\u00b7te", "der", "Wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wir im S\u00fcden sahn empor", "tokens": ["Und", "wir", "im", "S\u00fc\u00b7den", "sahn", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Muschelberge ragen,", "tokens": ["Die", "Mu\u00b7schel\u00b7ber\u00b7ge", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Blies schmetternd auf dem R\u00f6\u00dflein vorn", "tokens": ["Blies", "schmet\u00b7ternd", "auf", "dem", "R\u00f6\u00df\u00b7lein", "vorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der in der Lederhose.", "tokens": ["Der", "in", "der", "Le\u00b7der\u00b7ho\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ewen gr\u00fc\u00dfest du mit deinem Horn?\u201c", "tokens": ["\u201e", "wen", "gr\u00fc\u00b7\u00dfest", "du", "mit", "dei\u00b7nem", "Horn", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201edie Rose, Herr, die Rose!\u201c", "tokens": ["\u201e", "die", "Ro\u00b7se", ",", "Herr", ",", "die", "Ro\u00b7se", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit flachem Dach ein S\u00e4ulenhaus,", "tokens": ["Mit", "fla\u00b7chem", "Dach", "ein", "S\u00e4u\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das erste welsche Bildni\u00df,", "tokens": ["Das", "ers\u00b7te", "wel\u00b7sche", "Bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schaut R\u00f6se weinumwunden aus", "tokens": ["Schaut", "R\u00f6\u00b7se", "wei\u00b7num\u00b7wun\u00b7den", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verworr'ner Gartenwildni\u00df \u2014", "tokens": ["Ver\u00b7worr'\u00b7ner", "Gar\u00b7ten\u00b7wild\u00b7ni\u00df"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist, als ob des Baches Flut", "tokens": ["Es", "ist", ",", "als", "ob", "des", "Ba\u00b7ches", "Flut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Melod'scher schon ertose,", "tokens": ["Me\u00b7lo\u00b7d'\u00b7scher", "schon", "er\u00b7to\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hell brennt in Abendsonnenglut", "tokens": ["Hell", "brennt", "in", "A\u00b7bend\u00b7son\u00b7nen\u00b7glut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Rose, ja, die Rose.", "tokens": ["Die", "Ro\u00b7se", ",", "ja", ",", "die", "Ro\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKANT", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun, Herz, beginnt die Wonnezeit", "tokens": ["Nun", ",", "Herz", ",", "be\u00b7ginnt", "die", "Won\u00b7ne\u00b7zeit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Wegen und auf Stegen,", "tokens": ["Auf", "We\u00b7gen", "und", "auf", "Ste\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir str\u00f6mt ein Hauch von Ueppigkeit", "tokens": ["Mir", "str\u00f6mt", "ein", "Hauch", "von", "Uep\u00b7pig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ew'gem Lenz entgegen \u2014", "tokens": ["Und", "ew'\u00b7gem", "Lenz", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir gaukelt um die helle Stirn", "tokens": ["Mir", "gau\u00b7kelt", "um", "die", "hel\u00b7le", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Falter mit Gekose \u2014", "tokens": ["Ein", "Fal\u00b7ter", "mit", "Ge\u00b7ko\u00b7se"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Wein bringt eine junge Dirn,", "tokens": ["Den", "Wein", "bringt", "ei\u00b7ne", "jun\u00b7ge", "Dirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Rose, ja, die Rose.", "tokens": ["Die", "Ro\u00b7se", ",", "ja", ",", "die", "Ro\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKANT", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch einmal darf in s\u00fcdlich Land", "tokens": ["Noch", "ein\u00b7mal", "darf", "in", "s\u00fcd\u00b7lich", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "APPR", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Nordgeborner wallen,", "tokens": ["Ich", "Nord\u00b7ge\u00b7bor\u00b7ner", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vertauschen meine Felsenwand", "tokens": ["Ver\u00b7tau\u00b7schen", "mei\u00b7ne", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit wei\u00dfen Marmorhallen.", "tokens": ["Mit", "wei\u00b7\u00dfen", "Mar\u00b7mo\u00b7rhal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gegr\u00fc\u00dft, Italien, Licht und Lust!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", ",", "I\u00b7ta\u00b7li\u00b7en", ",", "Licht", "und", "Lust", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich preise meine Loose!", "tokens": ["Ich", "prei\u00b7se", "mei\u00b7ne", "Loo\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du bist an unsrer Erde Brust", "tokens": ["Du", "bist", "an", "uns\u00b7rer", "Er\u00b7de", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Rose, ja die Rose!", "tokens": ["Die", "Ro\u00b7se", ",", "ja", "die", "Ro\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}