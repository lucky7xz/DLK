{"textgrid.poem.49583": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Neujahr bei Pastors", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mama sch\u00f6pft aus dem Punschgef\u00e4\u00dfe,", "tokens": ["Ma\u00b7ma", "sch\u00f6pft", "aus", "dem", "Punschge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der Vater l\u00fcftet das Ges\u00e4\u00dfe", "tokens": ["Der", "Va\u00b7ter", "l\u00fcf\u00b7tet", "das", "Ge\u00b7s\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und spricht: \u00bbJetzt sind es vier Minuten", "tokens": ["Und", "spricht", ":", "\u00bb", "Jetzt", "sind", "es", "vier", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur mehr bis zw\u00f6lfe, meine Guten.", "tokens": ["Nur", "mehr", "bis", "zw\u00f6l\u00b7fe", ",", "mei\u00b7ne", "Gu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "CARD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, da\u00df ihr mit mir empfindet,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "ihr", "mit", "mir", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dieses alte Jahr entschwindet,", "tokens": ["Wie", "die\u00b7ses", "al\u00b7te", "Jahr", "ent\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df ihr Gott in seinen Werken", "tokens": ["Und", "da\u00df", "ihr", "Gott", "in", "sei\u00b7nen", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Mama, den Punsch noch was verst\u00e4rken! \u2013", "tokens": ["\u2013", "Ma\u00b7ma", ",", "den", "Pun\u00b7sch", "noch", "was", "ver\u00b7st\u00e4r\u00b7ken", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "ART", "NN", "ADV", "PIS", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und da\u00df ihr Gott von Herzen danket,", "tokens": ["Und", "da\u00df", "ihr", "Gott", "von", "Her\u00b7zen", "dan\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch in der Liebe nimmer wanket,", "tokens": ["Auch", "in", "der", "Lie\u00b7be", "nim\u00b7mer", "wan\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil alles, was uns widerfahren", "tokens": ["Weil", "al\u00b7les", ",", "was", "uns", "wi\u00b7der\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Mama, nicht mit dem Arrak sparen! \u2013", "tokens": ["\u2013", "Ma\u00b7ma", ",", "nicht", "mit", "dem", "Ar\u00b7rak", "spa\u00b7ren", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil, was geschah, und was geschehen,", "tokens": ["Weil", ",", "was", "ge\u00b7schah", ",", "und", "was", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "VVFIN", "$,", "KON", "PWS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob wir es freilich nicht verstehen,", "tokens": ["Ob", "wir", "es", "frei\u00b7lich", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weise war, durch seine Gnade", "tokens": ["Doch", "wei\u00b7se", "war", ",", "durch", "sei\u00b7ne", "Gna\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Mama, er schmeckt noch immer fade! \u2013", "tokens": ["\u2013", "Ma\u00b7ma", ",", "er", "schmeckt", "noch", "im\u00b7mer", "fa\u00b7de", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In diesem Sinne meine Guten,", "tokens": ["In", "die\u00b7sem", "Sin\u00b7ne", "mei\u00b7ne", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es sind jetzt blo\u00df mehr zwei Minuten,", "tokens": ["Es", "sind", "jetzt", "blo\u00df", "mehr", "zwei", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem gl\u00e4ubig frommen Sinne", "tokens": ["In", "die\u00b7sem", "gl\u00e4u\u00b7big", "from\u00b7men", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Gie\u00df noch mal Rum in die Terrine! \u2013", "tokens": ["\u2013", "Gie\u00df", "noch", "mal", "Rum", "in", "die", "Ter\u00b7ri\u00b7ne", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "ADV", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Wir bitten Gott, da\u00df er uns helfe", "tokens": ["Wir", "bit\u00b7ten", "Gott", ",", "da\u00df", "er", "uns", "hel\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ferner \u2013 Wie? Es schl\u00e4gt schon zw\u00f6lfe?", "tokens": ["Auch", "fer\u00b7ner", "\u2013", "Wie", "?", "Es", "schl\u00e4gt", "schon", "zw\u00f6l\u00b7fe", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PWAV", "$.", "PPER", "VVFIN", "ADV", "CARD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann prosit! Prost an allen Tischen!", "tokens": ["Dann", "pro\u00b7sit", "!", "Prost", "an", "al\u00b7len", "Ti\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Ich will den Punsch mal selber mischen.\u00ab", "tokens": ["\u2013", "Ich", "will", "den", "Pun\u00b7sch", "mal", "sel\u00b7ber", "mi\u00b7schen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}