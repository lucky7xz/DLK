{"textgrid.poem.54435": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "In bibliothec\u00e3 & Effigiem Nobiliss. Excellentiss: Magnificentissq; DN. Georgii Schonborneri in Sch\u00f6nborn & Zissendorff", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der trawte Sitz/ den ", "tokens": ["Das", "ist", "der", "traw\u00b7te", "Sitz", "/", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dran Svada sich verliebt; der hohen Wei\u00dfheit Zelt;", "tokens": ["Dran", "Sva\u00b7da", "sich", "ver\u00b7liebt", ";", "der", "ho\u00b7hen", "Wei\u00df\u00b7heit", "Zelt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "PRF", "VVPP", "$.", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das aller K\u00fcnste Schaar in seinem Schrancken helt/", "tokens": ["Das", "al\u00b7ler", "K\u00fcns\u00b7te", "Schaar", "in", "sei\u00b7nem", "Schran\u00b7cken", "helt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd was ber\u00fchmbte Leut au\u00df jhrem Sinn gebohren:", "tokens": ["Vnd", "was", "be\u00b7r\u00fchmb\u00b7te", "Leut", "au\u00df", "jhrem", "Sinn", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Vier le\u00df Ich/ was vorl\u00e4ngst Gott seinem Volck geschworen/", "tokens": ["Vier", "le\u00df", "Ich", "/", "was", "vor\u00b7l\u00e4ngst", "Gott", "sei\u00b7nem", "Volck", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "$(", "PWS", "VVFIN", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier sind Gesetz vnd Recht/ hier wird die grosse Welt", "tokens": ["Hier", "sind", "Ge\u00b7setz", "vnd", "Recht", "/", "hier", "wird", "die", "gros\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In B\u00fcchern/ vnd was mehr in Bildern vorgestellt;", "tokens": ["In", "B\u00fc\u00b7chern", "/", "vnd", "was", "mehr", "in", "Bil\u00b7dern", "vor\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "PWS", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier ist die zeit/ die sich von Anfang her verlohren/", "tokens": ["Hier", "ist", "die", "zeit", "/", "die", "sich", "von", "An\u00b7fang", "her", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd was in der gethan; Hier lern ich/ was ein Geist/", "tokens": ["Vnd", "was", "in", "der", "ge\u00b7than", ";", "Hier", "lern", "ich", "/", "was", "ein", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "VVPP", "$.", "ADV", "VVFIN", "PPER", "$(", "PWS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hier seh ich was ein Leib/ vnnd sp\u00fcr was Tugend heist/", "tokens": ["Hier", "seh", "ich", "was", "ein", "Leib", "/", "vnnd", "sp\u00fcr", "was", "Tu\u00b7gend", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "$(", "KON", "APPR", "PRELS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schaw aller St\u00e4dte wei\u00df/ vnd wie man die regieret/", "tokens": ["Schaw", "al\u00b7ler", "St\u00e4d\u00b7te", "wei\u00df", "/", "vnd", "wie", "man", "die", "re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVFIN", "$(", "KON", "PWAV", "PIS", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit kurtzem/ die Natur in Sprachen mancher hand.", "tokens": ["Mit", "kurt\u00b7zem", "/", "die", "Na\u00b7tur", "in", "Spra\u00b7chen", "man\u00b7cher", "hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$(", "ART", "NN", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd wenn mein Aug auff dehn/ des dieses Bild/ gewand/", "tokens": ["Vnd", "wenn", "mein", "Aug", "auff", "dehn", "/", "des", "die\u00b7ses", "Bild", "/", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ART", "$(", "ART", "PDAT", "NN", "$(", "VAPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Befind Ich/ da\u00df Er sey mit allem dehm gezieret.", "tokens": ["Be\u00b7find", "Ich", "/", "da\u00df", "Er", "sey", "mit", "al\u00b7lem", "dehm", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "KOUS", "PPER", "VAFIN", "APPR", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}