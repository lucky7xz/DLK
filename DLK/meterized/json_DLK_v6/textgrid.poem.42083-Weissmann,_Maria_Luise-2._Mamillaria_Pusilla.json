{"textgrid.poem.42083": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "2. Mamillaria Pusilla", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie stehen fremd in einem reinen Rund,", "tokens": ["Sie", "ste\u00b7hen", "fremd", "in", "ei\u00b7nem", "rei\u00b7nen", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tief in sich eingeh\u00fcllt wie in Gefieder.", "tokens": ["Tief", "in", "sich", "ein\u00b7ge\u00b7h\u00fcllt", "wie", "in", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVPP", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie gehn in sanfter W\u00f6lbung auf und nieder,", "tokens": ["Sie", "gehn", "in", "sanf\u00b7ter", "W\u00f6l\u00b7bung", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie bergen Z\u00e4rtlichkeit in ihrem Grund,", "tokens": ["Sie", "ber\u00b7gen", "Z\u00e4rt\u00b7lich\u00b7keit", "in", "ih\u00b7rem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Der unergr\u00fcndlich ist: sie ragen nah", "tokens": ["Der", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", ":", "sie", "ra\u00b7gen", "nah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sind Entfernte, zauberisch bewehrt.", "tokens": ["Und", "sind", "Ent\u00b7fern\u00b7te", ",", "zau\u00b7be\u00b7risch", "be\u00b7wehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann pl\u00f6tzlich, so entfremdet abgekehrt,", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", ",", "so", "ent\u00b7frem\u00b7det", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Scheinen sie n\u00e4her und vertrauter da,", "tokens": ["Schei\u00b7nen", "sie", "n\u00e4\u00b7her", "und", "ver\u00b7trau\u00b7ter", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Rufen sie eine sanft verwehte Gier", "tokens": ["Ru\u00b7fen", "sie", "ei\u00b7ne", "sanft", "ver\u00b7weh\u00b7te", "Gier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nach Liebkosungen, darin sie erschlossen", "tokens": ["Nach", "Lieb\u00b7ko\u00b7sun\u00b7gen", ",", "da\u00b7rin", "sie", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PAV", "PPER", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sich \u00f6ffneten. Doch einem sch\u00f6nen Tier", "tokens": ["Sich", "\u00f6ff\u00b7ne\u00b7ten", ".", "Doch", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Tier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Unfa\u00dfbar gleich in ihrer stummen Ruh", "tokens": ["Un\u00b7fa\u00df\u00b7bar", "gleich", "in", "ih\u00b7rer", "stum\u00b7men", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Stehen sie unbewegt und ungenossen", "tokens": ["Ste\u00b7hen", "sie", "un\u00b7be\u00b7wegt", "und", "un\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und sie versagen sich so tief wie Du.", "tokens": ["Und", "sie", "ver\u00b7sa\u00b7gen", "sich", "so", "tief", "wie", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}