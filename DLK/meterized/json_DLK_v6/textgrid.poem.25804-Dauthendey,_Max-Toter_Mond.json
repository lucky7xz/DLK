{"textgrid.poem.25804": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Toter Mond", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer die eisendunkeln Pappeln rauschen.", "tokens": ["Schwer", "die", "ei\u00b7sen\u00b7dun\u00b7keln", "Pap\u00b7peln", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schw\u00fcl, ein hei\u00dfdunkler Violenkelch,", "tokens": ["Schw\u00fcl", ",", "ein", "hei\u00df\u00b7du\u00b7nk\u00b7ler", "Vi\u00b7o\u00b7len\u00b7kelch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Flammt der schwarze Himmel.", "tokens": ["Flammt", "der", "schwar\u00b7ze", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ohne Echo starrt die Nacht.", "tokens": ["Oh\u00b7ne", "E\u00b7cho", "starrt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Echo pocht mein Herz.", "tokens": ["Oh\u00b7ne", "E\u00b7cho", "pocht", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}