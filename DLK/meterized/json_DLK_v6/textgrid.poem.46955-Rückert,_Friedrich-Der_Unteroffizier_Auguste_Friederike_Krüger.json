{"textgrid.poem.46955": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Unteroffizier Auguste Friederike Kr\u00fcger", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser Unteroffizier,", "tokens": ["Die\u00b7ser", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, wie gef\u00e4llt er dir?", "tokens": ["M\u00e4d\u00b7chen", ",", "wie", "ge\u00b7f\u00e4llt", "er", "dir", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Farben stehn ihm gut,", "tokens": ["Sei\u00b7ne", "Far\u00b7ben", "stehn", "ihm", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein kriegerischer Hut;", "tokens": ["Und", "sein", "krie\u00b7ge\u00b7ri\u00b7scher", "Hut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er schaut so mutig drein:", "tokens": ["Und", "er", "schaut", "so", "mu\u00b7tig", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00e4dchen, hast ihn Lust zu frein?", "tokens": ["M\u00e4d\u00b7chen", ",", "hast", "ihn", "Lust", "zu", "frein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "PTKZU", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4dchen, la\u00df es bleiben.", "tokens": ["M\u00e4d\u00b7chen", ",", "la\u00df", "es", "blei\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dieser Unteroffizier,", "tokens": ["Die\u00b7ser", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Mann steht er allhier;", "tokens": ["Wie", "ein", "Mann", "steht", "er", "all\u00b7hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wenn er seinen Rock zieht aus,", "tokens": ["Wenn", "er", "sei\u00b7nen", "Rock", "zieht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird, o weh, ein M\u00e4dchen draus;", "tokens": ["Wird", ",", "o", "weh", ",", "ein", "M\u00e4d\u00b7chen", "draus", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "PTKVZ", "$,", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wer irgend ihn will frein,", "tokens": ["Und", "wer", "ir\u00b7gend", "ihn", "will", "frein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPER", "VMFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Darf f\u00fcrwahr kein M\u00e4dchen sein.", "tokens": ["Darf", "f\u00fcr\u00b7wahr", "kein", "M\u00e4d\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das sind Wunder Gottes.", "tokens": ["Das", "sind", "Wun\u00b7der", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dieser Unteroffizier", "tokens": ["Die\u00b7ser", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War ein M\u00e4dchen, so wie ihr;", "tokens": ["War", "ein", "M\u00e4d\u00b7chen", ",", "so", "wie", "ihr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADV", "KOKOM", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber als der Krieg begann,", "tokens": ["A\u00b7ber", "als", "der", "Krieg", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht' es sich zu einem Mann;", "tokens": ["Macht'", "es", "sich", "zu", "ei\u00b7nem", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil's die Schneiderei verstand,", "tokens": ["Weil's", "die", "Schnei\u00b7de\u00b7rei", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht' es sich ein Mannsgewand,", "tokens": ["Macht'", "es", "sich", "ein", "Manns\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zog als Mann zu Felde.", "tokens": ["Zog", "als", "Mann", "zu", "Fel\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dieser Unteroffizier", "tokens": ["Die\u00b7ser", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Focht mit rechter Mannsbegier,", "tokens": ["Focht", "mit", "rech\u00b7ter", "Manns\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat erfochten Wunden viel", "tokens": ["Hat", "er\u00b7foch\u00b7ten", "Wun\u00b7den", "viel"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein eisern Kreuz am Ziel,", "tokens": ["Und", "ein", "ei\u00b7sern", "Kreuz", "am", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andern Brautschatz auch, der klingt,", "tokens": ["An\u00b7dern", "Braut\u00b7schatz", "auch", ",", "der", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den zum Heiratsgut sie bringt", "tokens": ["Den", "zum", "Hei\u00b7rats\u00b7gut", "sie", "bringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem, der sie will freien.", "tokens": ["Dem", ",", "der", "sie", "will", "frei\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Dieser Unteroffizier,", "tokens": ["Die\u00b7ser", "Un\u00b7ter\u00b7of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer ihn frein will, glaubet mir,", "tokens": ["Wer", "ihn", "frein", "will", ",", "glau\u00b7bet", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ein t\u00fccht'ger Hauptmann sein,", "tokens": ["Mu\u00df", "ein", "t\u00fccht'\u00b7ger", "Haupt\u00b7mann", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Handel soll gedeihn.", "tokens": ["Wenn", "der", "Han\u00b7del", "soll", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, ein Hauptmann bringt ihn schon", "tokens": ["Ei", ",", "ein", "Haupt\u00b7mann", "bringt", "ihn", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zur Subordination,", "tokens": ["Zur", "Sub\u00b7or\u00b7di\u00b7na\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Trotz dem Kreuz am Halse.", "tokens": ["Trotz", "dem", "Kreuz", "am", "Hal\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}