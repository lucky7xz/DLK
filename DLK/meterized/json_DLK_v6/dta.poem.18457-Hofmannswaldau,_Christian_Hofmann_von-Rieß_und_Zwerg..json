{"dta.poem.18457": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Rie\u00df und Zwerg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein langer edelmann ging eins mit seinen s\u00f6hnen/", "tokens": ["Ein", "lan\u00b7ger", "e\u00b7del\u00b7mann", "ging", "eins", "mit", "sei\u00b7nen", "s\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zwerg begegnet ihm/ den fragt er/ ihn zu h\u00f6hnen/", "tokens": ["Ein", "Zwerg", "be\u00b7geg\u00b7net", "ihm", "/", "den", "fragt", "er", "/", "ihn", "zu", "h\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "VVFIN", "PPER", "$(", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie bist du doch so klein/ du kurtzer katzen-kater?", "tokens": ["Wie", "bist", "du", "doch", "so", "klein", "/", "du", "kurt\u00b7zer", "kat\u00b7zen\u00b7ka\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja Juncker/ sprach der Zwerg; Ich hab nur einen vater.", "tokens": ["Ja", "Jun\u00b7cker", "/", "sprach", "der", "Zwerg", ";", "Ich", "hab", "nur", "ei\u00b7nen", "va\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "