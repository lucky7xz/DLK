{"textgrid.poem.42834": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Das Parlament", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Parlament geht's zu.", "tokens": ["Im", "Par\u00b7la\u00b7ment", "geht's", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was die f\u00fcr Schn\u00e4bel haben, \u2013", "tokens": ["Was", "die", "f\u00fcr", "Schn\u00e4\u00b7bel", "ha\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ART", "APPR", "NN", "VAFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sind wir Waisenknaben", "tokens": ["Da", "sind", "wir", "Wai\u00b7sen\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dagegen, ich und du.", "tokens": ["Da\u00b7ge\u00b7gen", ",", "ich", "und", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Onkel Rolf aus R\u00fcgen,", "tokens": ["Mein", "On\u00b7kel", "Rolf", "aus", "R\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ist einmal hineingew\u00e4hlt.", "tokens": ["Der", "ist", "ein\u00b7mal", "hin\u00b7ein\u00b7ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er recht voll ist und erz\u00e4hlt,", "tokens": ["Wenn", "er", "recht", "voll", "ist", "und", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann merkt man, wie die l\u00fcgen.", "tokens": ["Dann", "merkt", "man", ",", "wie", "die", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich habe selber zugeschaut,", "tokens": ["Ich", "ha\u00b7be", "sel\u00b7ber", "zu\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie der das Volk vertrat.", "tokens": ["Wie", "der", "das", "Volk", "ver\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das geht auf keine Kuhhaut.", "tokens": ["Das", "geht", "auf", "kei\u00b7ne", "Kuh\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Man meint, die spielen Skat.", "tokens": ["Man", "meint", ",", "die", "spie\u00b7len", "Skat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur manchmal, wenn der Pr\u00e4sident", "tokens": ["Nur", "manch\u00b7mal", ",", "wenn", "der", "Pr\u00e4\u00b7si\u00b7dent"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut l\u00e4utet, gibt es Ruhe.", "tokens": ["Laut", "l\u00e4u\u00b7tet", ",", "gibt", "es", "Ru\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch alles, was im Parlament", "tokens": ["Doch", "al\u00b7les", ",", "was", "im", "Par\u00b7la\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschieht, ist nur Getue.", "tokens": ["Ge\u00b7schieht", ",", "ist", "nur", "Ge\u00b7tue", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie wollen sich in Wirklichkeit", "tokens": ["Sie", "wol\u00b7len", "sich", "in", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur gro\u00dftun und vertagen", "tokens": ["Nur", "gro\u00df\u00b7tun", "und", "ver\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und freun sich auf die Ferienzeit.", "tokens": ["Und", "freun", "sich", "auf", "die", "Fe\u00b7ri\u00b7en\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo wir die Steuern tragen.", "tokens": ["Wo", "wir", "die", "Steu\u00b7ern", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mir geht das ganz daneben.", "tokens": ["Mir", "geht", "das", "ganz", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin selbst im Gesangverein.", "tokens": ["Ich", "bin", "selbst", "im", "Ge\u00b7sang\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wolln halt auch beisammen sein.", "tokens": ["Die", "wolln", "halt", "auch", "bei\u00b7sam\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Mensch will leben.", "tokens": ["Und", "je\u00b7der", "Mensch", "will", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}