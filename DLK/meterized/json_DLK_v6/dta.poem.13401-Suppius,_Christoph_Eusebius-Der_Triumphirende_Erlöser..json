{"dta.poem.13401": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Triumphirende Erl\u00f6ser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Du Sieges-F\u00fcrst! hilf meinem Wissen,", "tokens": ["Du", "Sie\u00b7ges\u00b7F\u00fcrst", "!", "hilf", "mei\u00b7nem", "Wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn deiner Auferstehung Kraft", "tokens": ["Denn", "dei\u00b7ner", "Auf\u00b7er\u00b7ste\u00b7hung", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist eine grosse Wissenschaft,", "tokens": ["Ist", "ei\u00b7ne", "gros\u00b7se", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann mir alle M\u00fch vers\u00fcssen,", "tokens": ["Und", "kann", "mir", "al\u00b7le", "M\u00fch", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil mein Verstand nun nicht vergi\u00dft,", "tokens": ["Weil", "mein", "Ver\u00b7stand", "nun", "nicht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Du, o h\u00f6chste Weisheit! bist.", "tokens": ["Wo", "Du", ",", "o", "h\u00f6chs\u00b7te", "Weis\u00b7heit", "!", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "FM", "ADJA", "NN", "$.", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}