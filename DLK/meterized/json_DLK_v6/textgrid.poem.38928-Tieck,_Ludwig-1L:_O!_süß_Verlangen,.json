{"textgrid.poem.38928": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O! s\u00fc\u00df Verlangen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O! s\u00fc\u00df Verlangen,", "tokens": ["O", "!", "s\u00fc\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nun bin ich dein;", "tokens": ["Nun", "bin", "ich", "dein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich soll gefangen,", "tokens": ["Ich", "soll", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Verschlossen seyn.", "tokens": ["Ver\u00b7schlos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das holde Sehnen,", "tokens": ["Das", "hol\u00b7de", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "H\u00e4lt bei mir Wacht,", "tokens": ["H\u00e4lt", "bei", "mir", "Wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und weckt die Thr\u00e4nen,", "tokens": ["Und", "weckt", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So Tag als Nacht.", "tokens": ["So", "Tag", "als", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Giebst du mich nimmer,", "tokens": ["Giebst", "du", "mich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der Banden frei,", "tokens": ["Der", "Ban\u00b7den", "frei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Da\u00df ich im Schimmer", "tokens": ["Da\u00df", "ich", "im", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Zufrieden sei?", "tokens": ["Zu\u00b7frie\u00b7den", "sei", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Doch la\u00df mich wohnen", "tokens": ["Doch", "la\u00df", "mich", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "In Ketten hier,", "tokens": ["In", "Ket\u00b7ten", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Ich finde Kronen,", "tokens": ["Ich", "fin\u00b7de", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Ach, nur bei dir.", "tokens": ["Ach", ",", "nur", "bei", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}