{"dta.poem.10355": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  \n  An eine S\u00e4ngerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich denke noch der Zaubervollen,", "tokens": ["Ich", "den\u00b7ke", "noch", "der", "Zau\u00b7ber\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie zuerst mein Auge sah!", "tokens": ["Wie", "sie", "zu\u00b7erst", "mein", "Au\u00b7ge", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ihre T\u00f6ne lieblich klangen,", "tokens": ["Wie", "ih\u00b7re", "T\u00f6\u00b7ne", "lieb\u00b7lich", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und heimlich s\u00fc\u00df in's Herze drangen,", "tokens": ["Und", "heim\u00b7lich", "s\u00fc\u00df", "in's", "Her\u00b7ze", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entrollten Thr\u00e4nen meinen Wangen, \u2014", "tokens": ["Ent\u00b7roll\u00b7ten", "Thr\u00e4\u00b7nen", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wu\u00dfte nicht wie mir geschah.", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", "wie", "mir", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Traum war \u00fcber mich gekommen:", "tokens": ["Ein", "Traum", "war", "\u00fc\u00b7ber", "mich", "ge\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als sey ich noch ein frommes Kind,", "tokens": ["Als", "sey", "ich", "noch", "ein", "from\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und s\u00e4\u00dfe still, beim L\u00e4mpchenscheine,", "tokens": ["Und", "s\u00e4\u00b7\u00dfe", "still", ",", "beim", "L\u00e4mp\u00b7chen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Mutters warmem K\u00e4mmerleine,", "tokens": ["In", "Mut\u00b7ters", "war\u00b7mem", "K\u00e4m\u00b7mer\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und l\u00e4se M\u00e4hrchen wunderfeine,", "tokens": ["Und", "l\u00e4\u00b7se", "M\u00e4hr\u00b7chen", "wun\u00b7der\u00b7fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Derweilen drau\u00dfen Nacht und Wind.", "tokens": ["Der\u00b7wei\u00b7len", "drau\u00b7\u00dfen", "Nacht", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die M\u00e4hrchen fangen an zu leben,", "tokens": ["Die", "M\u00e4hr\u00b7chen", "fan\u00b7gen", "an", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ritter steigen aus der Gruft;", "tokens": ["Die", "Rit\u00b7ter", "stei\u00b7gen", "aus", "der", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Ronzisvall da giebt's ein Streiten,", "tokens": ["Bei", "Ron\u00b7zis\u00b7vall", "da", "giebt's", "ein", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da kommt Herr Roland herzureiten,", "tokens": ["Da", "kommt", "Herr", "Ro\u00b7land", "her\u00b7zu\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel k\u00fchne Degen ihn begleiten,", "tokens": ["Viel", "k\u00fch\u00b7ne", "De\u00b7gen", "ihn", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch leider Ganelon, der Schuft.", "tokens": ["Auch", "lei\u00b7der", "Ga\u00b7ne\u00b7lon", ",", "der", "Schuft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch den wird Roland schlimm gebettet;", "tokens": ["Durch", "den", "wird", "Ro\u00b7land", "schlimm", "ge\u00b7bet\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er schwimmt in Blut, und athmet kaum;", "tokens": ["Er", "schwimmt", "in", "Blut", ",", "und", "ath\u00b7met", "kaum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum mochte fern sein Jagdhornzeichen", "tokens": ["Kaum", "moch\u00b7te", "fern", "sein", "Jagd\u00b7horn\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ohr des gro\u00dfen Carls erreichen, \u2014", "tokens": ["Das", "Ohr", "des", "gro\u00b7\u00dfen", "Carls", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da mu\u00df der Ritter schon erbleichen, \u2014", "tokens": ["Da", "mu\u00df", "der", "Rit\u00b7ter", "schon", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit ihm stirbt zugleich mein Traum.", "tokens": ["Und", "mit", "ihm", "stirbt", "zu\u00b7gleich", "mein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das war ein laut verworr'nes Schallen,", "tokens": ["Das", "war", "ein", "laut", "ver\u00b7worr'\u00b7nes", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das mich aus meinem Tr\u00e4umen rief.", "tokens": ["Das", "mich", "aus", "mei\u00b7nem", "Tr\u00e4u\u00b7men", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verklungen war jetzt die Legende,", "tokens": ["Ver\u00b7klun\u00b7gen", "war", "jetzt", "die", "Le\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leute schlugen in die H\u00e4nde,", "tokens": ["Die", "Leu\u00b7te", "schlu\u00b7gen", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und riefen \u201eBravo\u201c ohne Ende;", "tokens": ["Und", "rie\u00b7fen", "\u201e", "Bra\u00b7vo", "\u201c", "oh\u00b7ne", "En\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ITJ", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die S\u00e4ngerin verneigt sich tief.", "tokens": ["Die", "S\u00e4n\u00b7ge\u00b7rin", "ver\u00b7neigt", "sich", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}