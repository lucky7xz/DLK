{"dta.poem.8664": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Hoffnung! la\u00df allein mich wallen,", "tokens": ["Hoff\u00b7nung", "!", "la\u00df", "al\u00b7lein", "mich", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gaukle nicht um meine Bahn!", "tokens": ["Gauk\u00b7le", "nicht", "um", "mei\u00b7ne", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Sterne sind gefallen,", "tokens": ["Dei\u00b7ne", "Ster\u00b7ne", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich t\u00e4uscht kein holder Wahn!", "tokens": ["Und", "mich", "t\u00e4uscht", "kein", "hol\u00b7der", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieser streckt nach einer Krone", "tokens": ["Die\u00b7ser", "streckt", "nach", "ei\u00b7ner", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Hand verwegen aus;", "tokens": ["Sei\u00b7ne", "Hand", "ver\u00b7we\u00b7gen", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihn sto\u00dft der Tod mit Hohne", "tokens": ["Doch", "ihn", "sto\u00dft", "der", "Tod", "mit", "Hoh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In sein enges, k\u00fchles Haus.", "tokens": ["In", "sein", "en\u00b7ges", ",", "k\u00fch\u00b7les", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein Andrer hat errungen,", "tokens": ["Und", "ein", "A\u00b7ndrer", "hat", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der Erste nur gewollt;", "tokens": ["Was", "der", "Ers\u00b7te", "nur", "ge\u00b7wollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "ADV", "VMPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat die h\u00f6chste H\u00f6h' erschwungen:", "tokens": ["Hat", "die", "h\u00f6chs\u00b7te", "H\u00f6h'", "er\u00b7schwun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Throne wanken, wenn er grollt.", "tokens": ["Thro\u00b7ne", "wan\u00b7ken", ",", "wenn", "er", "grollt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hoffnung! o warum entz\u00fcndest", "tokens": ["Hoff\u00b7nung", "!", "o", "wa\u00b7rum", "ent\u00b7z\u00fcn\u00b7dest"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "FM", "FM", "FM"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du sein Herz zum stolzen Plan,", "tokens": ["Du", "sein", "Herz", "zum", "stol\u00b7zen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da du schmeichelnd ihm verk\u00fcndest", "tokens": ["Da", "du", "schmei\u00b7chelnd", "ihm", "ver\u00b7k\u00fcn\u00b7dest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Welttheil unterthan?!", "tokens": ["Ei\u00b7nen", "Welt\u00b7theil", "un\u00b7ter\u00b7than", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ueber V\u00f6lkern klirrt die Kette,", "tokens": ["Ue\u00b7ber", "V\u00f6l\u00b7kern", "klirrt", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da sein Schritt nach Osten st\u00fcrmt;", "tokens": ["Da", "sein", "Schritt", "nach", "Os\u00b7ten", "st\u00fcrmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bang ruft eins dem andern: rette!", "tokens": ["Bang", "ruft", "eins", "dem", "an\u00b7dern", ":", "ret\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "ADJA", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Schreckensmacht umth\u00fcrmt.", "tokens": ["Von", "der", "Schre\u00b7ckens\u00b7macht", "umt\u00b7h\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun ergreift ihn sein Verh\u00e4ngni\u00df,", "tokens": ["Nun", "er\u00b7greift", "ihn", "sein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00dft ihm Kron' und Purpur ab,", "tokens": ["Rei\u00dft", "ihm", "Kron'", "und", "Pur\u00b7pur", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleudert ihn ins Meergef\u00e4ngni\u00df,", "tokens": ["Schleu\u00b7dert", "ihn", "ins", "Meer\u00b7ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald verschlingt ihn dort sein Grab. \u2014", "tokens": ["Bald", "ver\u00b7schlingt", "ihn", "dort", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In der N\u00e4chte stiller Feier", "tokens": ["In", "der", "N\u00e4ch\u00b7te", "stil\u00b7ler", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt der heiligen Natur", "tokens": ["Hebt", "der", "hei\u00b7li\u00b7gen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchn ein Forscher ihre Schleier,", "tokens": ["K\u00fchn", "ein", "For\u00b7scher", "ih\u00b7re", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verfolget Gottes Spur.", "tokens": ["Und", "ver\u00b7fol\u00b7get", "Got\u00b7tes", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn du l\u00e4ssest sch\u00f6n ergl\u00e4nzen", "tokens": ["Denn", "du", "l\u00e4s\u00b7sest", "sch\u00f6n", "er\u00b7gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm ein Mahl der Ewigkeit,", "tokens": ["Ihm", "ein", "Mahl", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Enkel seine Gruft bekr\u00e4nzen; \u2014", "tokens": ["En\u00b7kel", "sei\u00b7ne", "Gruft", "be\u00b7kr\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihn lohnt \u2014 Vergessenheit!", "tokens": ["Und", "ihn", "lohnt", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Nach der Liebe treuem Gl\u00fccke", "tokens": ["Nach", "der", "Lie\u00b7be", "treu\u00b7em", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das er nirgends finden soll,", "tokens": ["Das", "er", "nir\u00b7gends", "fin\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehrt ein Andrer seine Blicke,", "tokens": ["Kehrt", "ein", "A\u00b7ndrer", "sei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir vertrauend, sehnsuchtsvoll.", "tokens": ["Dir", "ver\u00b7trau\u00b7end", ",", "sehn\u00b7suchts\u00b7voll", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach, sie liebt ihn, der Entgl\u00fchte", "tokens": ["Ach", ",", "sie", "liebt", "ihn", ",", "der", "Ent\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt sie wonnevoll umstrickt;", "tokens": ["H\u00e4lt", "sie", "won\u00b7ne\u00b7voll", "um\u00b7strickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Liebe zarte Bl\u00fcthe", "tokens": ["Doch", "der", "Lie\u00b7be", "zar\u00b7te", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird im Rausche bald zerknickt! \u2014", "tokens": ["Wird", "im", "Rau\u00b7sche", "bald", "zer\u00b7knickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "All' dein Wort ist Windesf\u00e4cheln;", "tokens": ["All'", "dein", "Wort", "ist", "Win\u00b7des\u00b7f\u00e4\u00b7cheln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffnung! dann nur trau' ich dir,", "tokens": ["Hoff\u00b7nung", "!", "dann", "nur", "trau'", "ich", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weisest du mit Trostesl\u00e4cheln", "tokens": ["Wei\u00b7sest", "du", "mit", "Tros\u00b7tes\u00b7l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir des Todes Nachtrevier!", "tokens": ["Mir", "des", "To\u00b7des", "Nach\u00b7tre\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}