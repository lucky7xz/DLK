{"textgrid.poem.24298": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Unser Schlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte mich in einen tiefen Wald ...", "tokens": ["Ich", "tr\u00e4um\u00b7te", "mich", "in", "ei\u00b7nen", "tie\u00b7fen", "Wald", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich wanderte dem Lied der V\u00f6gel nach;", "tokens": ["Ich", "wan\u00b7der\u00b7te", "dem", "Lied", "der", "V\u00f6\u00b7gel", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf schmalen Wegen \u00fcber Wurzeln weg", "tokens": ["Auf", "schma\u00b7len", "We\u00b7gen", "\u00fc\u00b7ber", "Wur\u00b7zeln", "weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schritt ich und strauchelte doch nie; es war", "tokens": ["Schritt", "ich", "und", "strau\u00b7chel\u00b7te", "doch", "nie", ";", "es", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADV", "ADV", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Gehn ein Schweben. \u2013 Eine Stimme sang", "tokens": ["Im", "Gehn", "ein", "Schwe\u00b7ben", ".", "\u2013", "Ei\u00b7ne", "Stim\u00b7me", "sang"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ganz leise in mir: Siehe, heute noch", "tokens": ["Ganz", "lei\u00b7se", "in", "mir", ":", "Sie\u00b7he", ",", "heu\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$.", "VVIMP", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bist du zu Hause ... Immer gr\u00fcner ward", "tokens": ["Bist", "du", "zu", "Hau\u00b7se", "...", "Im\u00b7mer", "gr\u00fc\u00b7ner", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$(", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es rings um mich, und alles fiel von mir,", "tokens": ["Es", "rings", "um", "mich", ",", "und", "al\u00b7les", "fiel", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$,", "KON", "PIS", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das mich beb\u00fcrdet. Und der Welt Ger\u00e4usch", "tokens": ["Das", "mich", "be\u00b7b\u00fcr\u00b7det", ".", "Und", "der", "Welt", "Ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "$.", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verhallte hinter mir. Die V\u00f6gel selbst", "tokens": ["Ver\u00b7hall\u00b7te", "hin\u00b7ter", "mir", ".", "Die", "V\u00f6\u00b7gel", "selbst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verstummten. Nur das leise Wipfelwehn", "tokens": ["Ver\u00b7stumm\u00b7ten", ".", "Nur", "das", "lei\u00b7se", "Wip\u00b7fel\u00b7wehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Umrauschte mich: dies s\u00fc\u00dfe Schlummerlied", "tokens": ["Um\u00b7rauschte", "mich", ":", "dies", "s\u00fc\u00b7\u00dfe", "Schlum\u00b7mer\u00b7lied"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PDS", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der gro\u00dfen Stille, das die Tr\u00e4ume ruft,", "tokens": ["Der", "gro\u00b7\u00dfen", "Stil\u00b7le", ",", "das", "die", "Tr\u00e4u\u00b7me", "ruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die samtenen Nachtfalter: braun und schwarz", "tokens": ["Die", "sam\u00b7te\u00b7nen", "Nacht\u00b7fal\u00b7ter", ":", "braun", "und", "schwarz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Mit goldenen F\u00fchlern, die wie Palmen sind", "tokens": ["Mit", "gol\u00b7de\u00b7nen", "F\u00fch\u00b7lern", ",", "die", "wie", "Pal\u00b7men", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "KOKOM", "NN", "VAFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Aus seidenen Rispen, und mit blinden Augen,", "tokens": ["Aus", "sei\u00b7de\u00b7nen", "Ris\u00b7pen", ",", "und", "mit", "blin\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Die mehr erblicken, als jemals der Tag", "tokens": ["Die", "mehr", "er\u00b7bli\u00b7cken", ",", "als", "je\u00b7mals", "der", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "In seiner harten Grelle zeigt ... Da stand", "tokens": ["In", "sei\u00b7ner", "har\u00b7ten", "Grel\u00b7le", "zeigt", "...", "Da", "stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein kleines Schlo\u00df an einem Teich vor mir.", "tokens": ["Ein", "klei\u00b7nes", "Schlo\u00df", "an", "ei\u00b7nem", "Teich", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Drei gro\u00dfe schwarze Schw\u00e4ne glitten sanft", "tokens": ["Drei", "gro\u00b7\u00dfe", "schwar\u00b7ze", "Schw\u00e4\u00b7ne", "glit\u00b7ten", "sanft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Auf seinem Spiegel, drauf der Abendschein", "tokens": ["Auf", "sei\u00b7nem", "Spie\u00b7gel", ",", "drauf", "der", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Gelb lag gleich einem welken Rosenblatt.", "tokens": ["Gelb", "lag", "gleich", "ei\u00b7nem", "wel\u00b7ken", "Ro\u00b7sen\u00b7blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das Schlo\u00df war ganz aus ametystnem Quarz,", "tokens": ["Das", "Schlo\u00df", "war", "ganz", "aus", "a\u00b7me\u00b7tyst\u00b7nem", "Quarz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Violenblau, gold\u00e4derig, gebaut;", "tokens": ["Vi\u00b7o\u00b7len\u00b7blau", ",", "gol\u00b7d\u00e4\u00b7de\u00b7rig", ",", "ge\u00b7baut", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die T\u00fcren bronzen, gr\u00fcnlich-schwarz: als Schild", "tokens": ["Die", "T\u00fc\u00b7ren", "bron\u00b7zen", ",", "gr\u00fcn\u00b7lich\u00b7schwarz", ":", "als", "Schild"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "$.", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Das Bild der Sonne drauf: ", "tokens": ["Das", "Bild", "der", "Son\u00b7ne", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "(ich f\u00fchlt es nun) in diesen Zauber rief.", "tokens": ["(", "ich", "f\u00fchlt", "es", "nun", ")", "in", "die\u00b7sen", "Zau\u00b7ber", "rief", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u2013: Wo bist du? sagt ich leise vor mich hin.", "tokens": ["\u2013", ":", "Wo", "bist", "du", "?", "sagt", "ich", "lei\u00b7se", "vor", "mich", "hin", "."], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$.", "PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u2013: L\u00e4dst du mich ein in unser Gl\u00fcck, das wir,", "tokens": ["\u2013", ":", "L\u00e4dst", "du", "mich", "ein", "in", "un\u00b7ser", "Gl\u00fcck", ",", "das", "wir", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "$.", "VVFIN", "PPER", "PRF", "ART", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In unsrer Herzen Gleichklang wortelos", "tokens": ["In", "uns\u00b7rer", "Her\u00b7zen", "Gleich\u00b7klang", "wor\u00b7te\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns ganz verstehend, Tag f\u00fcr Tag", "tokens": ["Uns", "ganz", "ver\u00b7ste\u00b7hend", ",", "Tag", "f\u00fcr", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$,", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aufrecht im Glauben suchen: niemals ganz", "tokens": ["Auf\u00b7recht", "im", "Glau\u00b7ben", "su\u00b7chen", ":", "nie\u00b7mals", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVINF", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verzagend, ob auch manches Mal", "tokens": ["Ver\u00b7za\u00b7gend", ",", "ob", "auch", "man\u00b7ches", "Mal"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im D\u00fcster irrend: \u2013 hast du mir erbaut", "tokens": ["Im", "D\u00fcs\u00b7ter", "ir\u00b7rend", ":", "\u2013", "hast", "du", "mir", "er\u00b7baut"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$.", "$(", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dies Schlo\u00df aus hellem Gold und Veilchenblau?", "tokens": ["Dies", "Schlo\u00df", "aus", "hel\u00b7lem", "Gold", "und", "Veil\u00b7chen\u00b7blau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u2013 Da taten sich die Bronzefl\u00fcgel auf,", "tokens": ["\u2013", "Da", "ta\u00b7ten", "sich", "die", "Bron\u00b7ze\u00b7fl\u00fc\u00b7gel", "auf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den Sonnenschild zerteilend, und ", "tokens": ["Den", "Son\u00b7nen\u00b7schild", "zer\u00b7tei\u00b7lend", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Minerva mit dem Speere, im Geviert", "tokens": ["Mi\u00b7ner\u00b7va", "mit", "dem", "Spee\u00b7re", ",", "im", "Ge\u00b7viert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Des hohen Eingangs, aber l\u00e4chelnd wie", "tokens": ["Des", "ho\u00b7hen", "Ein\u00b7gangs", ",", "a\u00b7ber", "l\u00e4\u00b7chelnd", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Liebesg\u00f6ttin und die Mutter Gottes da:", "tokens": ["Die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tin", "und", "die", "Mut\u00b7ter", "Got\u00b7tes", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ihre Blicke \u00fcberstrahlten mich", "tokens": ["Und", "ih\u00b7re", "Bli\u00b7cke", "\u00fc\u00b7bers\u00b7trahl\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wie aller Menschenliebe Inbegriff.", "tokens": ["Wie", "al\u00b7ler", "Men\u00b7schen\u00b7lie\u00b7be", "In\u00b7be\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}