{"textgrid.poem.62910": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Iman schickte seine S\u00f6hne", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Iman schickte seine S\u00f6hne", "tokens": ["Ein", "I\u00b7man", "schick\u00b7te", "sei\u00b7ne", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Mekka zu des Sehers Grab;", "tokens": ["Nach", "Mek\u00b7ka", "zu", "des", "Se\u00b7hers", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie reisten wie die Diogene,", "tokens": ["Sie", "reis\u00b7ten", "wie", "die", "Dio\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Das hei\u00dft, zu Fu\u00df. Beym Abschied gab", "tokens": ["Das", "hei\u00dft", ",", "zu", "Fu\u00df", ".", "Beym", "Ab\u00b7schied", "gab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der fromme Greis, mit einer Thr\u00e4ne", "tokens": ["Der", "from\u00b7me", "Greis", ",", "mit", "ei\u00b7ner", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Segens, jedem einen Stab", "tokens": ["Des", "Se\u00b7gens", ",", "je\u00b7dem", "ei\u00b7nen", "Stab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sprach: la\u00dft diesen euch regieren.", "tokens": ["Und", "sprach", ":", "la\u00dft", "die\u00b7sen", "euch", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVIMP", "PDAT", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Gott gab im die Wunderkraft,", "tokens": ["Ein", "Gott", "gab", "im", "die", "Wun\u00b7der\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Euch stets den rechten Weg zu f\u00fchren.", "tokens": ["Euch", "stets", "den", "rech\u00b7ten", "Weg", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie traten ihre Pilgrimschaft", "tokens": ["Sie", "tra\u00b7ten", "ih\u00b7re", "Pil\u00b7grim\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Itzt muthig an. Einst rief im Gehen", "tokens": ["Itzt", "mut\u00b7hig", "an", ".", "Einst", "rief", "im", "Ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der j\u00fcngste Bruder: la\u00df doch sehen,", "tokens": ["Der", "j\u00fcngs\u00b7te", "Bru\u00b7der", ":", "la\u00df", "doch", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVIMP", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wer wohl den sch\u00f6nsten Stecken f\u00fchrt?", "tokens": ["Wer", "wohl", "den", "sch\u00f6ns\u00b7ten", "Ste\u00b7cken", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Stracks blieb die Caravane stehen.", "tokens": ["Stracks", "blieb", "die", "Ca\u00b7ra\u00b7va\u00b7ne", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die St\u00e4be werden recensiert,", "tokens": ["Die", "St\u00e4\u00b7be", "wer\u00b7den", "re\u00b7cen\u00b7siert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und in die L\u00e4ng und in die Quere", "tokens": ["Und", "in", "die", "L\u00e4ng", "und", "in", "die", "Que\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gedreht, gebogen, abvisiert,", "tokens": ["Ge\u00b7dreht", ",", "ge\u00b7bo\u00b7gen", ",", "ab\u00b7vi\u00b7siert", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und jeder schwur bey Gott und Ehre,", "tokens": ["Und", "je\u00b7der", "schwur", "bey", "Gott", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df seiner doch der sch\u00f6nste w\u00e4re.", "tokens": ["Da\u00df", "sei\u00b7ner", "doch", "der", "sch\u00f6ns\u00b7te", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Als man sich heiser demonstriert,", "tokens": ["Als", "man", "sich", "hei\u00b7ser", "de\u00b7monst\u00b7riert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "So kam es, wie in unsern Tagen,", "tokens": ["So", "kam", "es", ",", "wie", "in", "un\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zum Schelten und zuletzt zum Schlagen.", "tokens": ["Zum", "Schel\u00b7ten", "und", "zu\u00b7letzt", "zum", "Schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die St\u00f6cke zischten durch die Luft;", "tokens": ["Die", "St\u00f6\u00b7cke", "zischten", "durch", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Hier flog ein Ohr, dort eine Nase,", "tokens": ["Hier", "flog", "ein", "Ohr", ",", "dort", "ei\u00b7ne", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Hier sprang ein Zahn aus seiner Kluft,", "tokens": ["Hier", "sprang", "ein", "Zahn", "aus", "sei\u00b7ner", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dort lag ein scheeles Aug im Grase.", "tokens": ["Dort", "lag", "ein", "schee\u00b7les", "Aug", "im", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ein Derwisch, weis und fromm, wie du,", "tokens": ["Ein", "Der\u00b7wisch", ",", "weis", "und", "fromm", ",", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "KON", "ADJD", "$,", "PWAV", "PPER", "$,"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.28": {"text": "Freund, zog von ungef\u00e4hr die Stra\u00dfe;", "tokens": ["Freund", ",", "zog", "von", "un\u00b7ge\u00b7f\u00e4hr", "die", "Stra\u00b7\u00dfe", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJD", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Er lief auf die Athleten zu", "tokens": ["Er", "lief", "auf", "die", "Ath\u00b7le\u00b7ten", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKZU"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.30": {"text": "Und rief mit eines Seraphs Stimme:", "tokens": ["Und", "rief", "mit", "ei\u00b7nes", "Se\u00b7raphs", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "La\u00dft ab, Unsinnige, la\u00dft ab", "tokens": ["La\u00dft", "ab", ",", "Un\u00b7sin\u00b7ni\u00b7ge", ",", "la\u00dft", "ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJA", "$,", "VVIMP", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Von eurem m\u00f6rderischen Grimme!", "tokens": ["Von", "eu\u00b7rem", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Grim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der Vater gab euch diesen Stab", "tokens": ["Der", "Va\u00b7ter", "gab", "euch", "die\u00b7sen", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Um euch auf rechter Bahn zu leiten,", "tokens": ["Um", "euch", "auf", "rech\u00b7ter", "Bahn", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Und den gebraucht ihr, ihm zum Hohn,", "tokens": ["Und", "den", "ge\u00b7braucht", "ihr", ",", "ihm", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Als Werkzeug toller Streitigkeiten,", "tokens": ["Als", "Werk\u00b7zeug", "tol\u00b7ler", "Strei\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wie Christen die Religion.", "tokens": ["Wie", "Chris\u00b7ten", "die", "Re\u00b7li\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}