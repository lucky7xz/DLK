{"dta.poem.7922": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An das Vaterland .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Dir m\u00f6cht\u2019 ich diese Lieder weihen,", "tokens": ["Dir", "m\u00f6cht'", "ich", "die\u00b7se", "Lie\u00b7der", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebtes deutsches Vaterland!", "tokens": ["Ge\u00b7lieb\u00b7tes", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dir, dem neuerstandnen, freien,", "tokens": ["Denn", "dir", ",", "dem", "neu\u00b7er\u00b7stand\u00b7nen", ",", "frei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist all mein Sinnen zugewandt.", "tokens": ["Ist", "all", "mein", "Sin\u00b7nen", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch Heldenblut ist dir geflossen,", "tokens": ["Doch", "Hel\u00b7den\u00b7blut", "ist", "dir", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir sank der Jugend sch\u00f6nste Zier:", "tokens": ["Dir", "sank", "der", "Ju\u00b7gend", "sch\u00f6ns\u00b7te", "Zier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach solchen Opfern, heilig gro\u00dfen,", "tokens": ["Nach", "sol\u00b7chen", "Op\u00b7fern", ",", "hei\u00b7lig", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was g\u00e4lten diese Lieder dir?", "tokens": ["Was", "g\u00e4l\u00b7ten", "die\u00b7se", "Lie\u00b7der", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}