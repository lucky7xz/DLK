{"textgrid.poem.26831": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo ich sei, und wo mich hingewendet,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ich sei, und wo mich hingewendet,", "tokens": ["Wo", "ich", "sei", ",", "und", "wo", "mich", "hin\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als mein fl\u00fcchtger Schatte dir entschwebt?", "tokens": ["Als", "mein", "fl\u00fccht\u00b7ger", "Schat\u00b7te", "dir", "ent\u00b7schwebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hab ich nicht beschlossen und geendet,", "tokens": ["Hab", "ich", "nicht", "be\u00b7schlos\u00b7sen", "und", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hab ich nicht geliebet und gelebt?", "tokens": ["Hab", "ich", "nicht", "ge\u00b7lie\u00b7bet", "und", "ge\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Willst du nach den Nachtigallen fragen,", "tokens": ["Willst", "du", "nach", "den", "Nach\u00b7ti\u00b7gal\u00b7len", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die mit seelenvoller Melodie", "tokens": ["Die", "mit", "see\u00b7len\u00b7vol\u00b7ler", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dich entz\u00fcckten in des Lenzes Tagen?", "tokens": ["Dich", "ent\u00b7z\u00fcck\u00b7ten", "in", "des", "Len\u00b7zes", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur solang sie liebten, waren sie.", "tokens": ["Nur", "so\u00b7lang", "sie", "lieb\u00b7ten", ",", "wa\u00b7ren", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ob ich den Verlorenen gefunden?", "tokens": ["Ob", "ich", "den", "Ver\u00b7lo\u00b7re\u00b7nen", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Glaube mir, ich bin mit ihm vereint,", "tokens": ["Glau\u00b7be", "mir", ",", "ich", "bin", "mit", "ihm", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo sich nicht mehr trennt, was sich verbunden,", "tokens": ["Wo", "sich", "nicht", "mehr", "trennt", ",", "was", "sich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PTKNEG", "ADV", "VVFIN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dort, wo keine Tr\u00e4ne wird geweint.", "tokens": ["Dort", ",", "wo", "kei\u00b7ne", "Tr\u00e4\u00b7ne", "wird", "ge\u00b7weint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dorten wirst auch du uns wiederfinden,", "tokens": ["Dor\u00b7ten", "wirst", "auch", "du", "uns", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn dein Lieben unserm Lieben gleicht;", "tokens": ["Wenn", "dein", "Lie\u00b7ben", "un\u00b7serm", "Lie\u00b7ben", "gleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dort ist auch der Vater, frei von S\u00fcnden,", "tokens": ["Dort", "ist", "auch", "der", "Va\u00b7ter", ",", "frei", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Den der blutge Mord nicht mehr erreicht.", "tokens": ["Den", "der", "blut\u00b7ge", "Mord", "nicht", "mehr", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und er f\u00fchlt, da\u00df ihn kein Wahn betrogen,", "tokens": ["Und", "er", "f\u00fchlt", ",", "da\u00df", "ihn", "kein", "Wahn", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als er aufw\u00e4rts zu den Sternen sah;", "tokens": ["Als", "er", "auf\u00b7w\u00e4rts", "zu", "den", "Ster\u00b7nen", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn wie jeder w\u00e4gt, wird ihm gewogen,", "tokens": ["Denn", "wie", "je\u00b7der", "w\u00e4gt", ",", "wird", "ihm", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wer es glaubt, dem ist das Heilge nah.", "tokens": ["Wer", "es", "glaubt", ",", "dem", "ist", "das", "Heil\u00b7ge", "nah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wort gehalten wird in jenen R\u00e4umen", "tokens": ["Wort", "ge\u00b7hal\u00b7ten", "wird", "in", "je\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jedem sch\u00f6nen gl\u00e4ubigen Gef\u00fchl;", "tokens": ["Je\u00b7dem", "sch\u00f6\u00b7nen", "gl\u00e4u\u00b7bi\u00b7gen", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wage du, zu irren und zu tr\u00e4umen:", "tokens": ["Wa\u00b7ge", "du", ",", "zu", "ir\u00b7ren", "und", "zu", "tr\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hoher Sinn liegt oft in kindschem Spiel.", "tokens": ["Ho\u00b7her", "Sinn", "liegt", "oft", "in", "kind\u00b7schem", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}