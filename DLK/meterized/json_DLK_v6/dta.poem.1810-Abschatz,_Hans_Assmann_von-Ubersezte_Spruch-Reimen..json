{"dta.poem.1810": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wenig k\u00f6nnen doch das wahre Gutt erkennen?", "tokens": ["Wie", "we\u00b7nig", "k\u00f6n\u00b7nen", "doch", "das", "wah\u00b7re", "Gutt", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sieht uns dieses scheun und nach dem andern", "tokens": ["Man", "sieht", "uns", "die\u00b7ses", "scheun", "und", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PDS", "VVINF", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "rennen;", "tokens": ["ren\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "L\u00e4st sich der Fu\u00df hernach nicht mehr zur\u00fccke ziehn/", "tokens": ["L\u00e4st", "sich", "der", "Fu\u00df", "her\u00b7nach", "nicht", "mehr", "zu\u00b7r\u00fc\u00b7cke", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "PTKNEG", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So hat man eitle M\u00fch und Reue zum Gewinn.", "tokens": ["So", "hat", "man", "eit\u00b7le", "M\u00fch", "und", "Reu\u00b7e", "zum", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}