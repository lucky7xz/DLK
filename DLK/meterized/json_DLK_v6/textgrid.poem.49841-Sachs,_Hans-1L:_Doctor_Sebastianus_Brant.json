{"textgrid.poem.49841": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doctor Sebastianus Brant", "genre": "verse", "period": "N.A.", "pub_year": 1563, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doctor Sebastianus Brant", "tokens": ["Doc\u00b7tor", "Se\u00b7bas\u00b7ti\u00b7a\u00b7nus", "Brant"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "ein fabel schreibet mit verstant,", "tokens": ["ein", "fa\u00b7bel", "schrei\u00b7bet", "mit", "ver\u00b7stant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie das auf einem dorfe sas", "tokens": ["wie", "das", "auf", "ei\u00b7nem", "dor\u00b7fe", "sas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein baur, der faul und gfre\u00dfig was,", "tokens": ["ein", "baur", ",", "der", "faul", "und", "gfre\u00b7\u00dfig", "was", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "welchem sein vatter war gestorben,", "tokens": ["wel\u00b7chem", "sein", "vat\u00b7ter", "war", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "von dem het er sein hof erworben,", "tokens": ["von", "dem", "het", "er", "sein", "hof", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "darin von treit ein vorrat het,", "tokens": ["da\u00b7rin", "von", "treit", "ein", "vor\u00b7rat", "het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "ART", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "das er balt gen mark f\u00fcren tet", "tokens": ["das", "er", "balt", "gen", "mark", "f\u00fc\u00b7ren", "tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "ADV", "APPR", "NE", "VVINF", "VVFIN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.9": {"text": "und verkaufet das kurzer tag.", "tokens": ["und", "ver\u00b7kau\u00b7fet", "das", "kur\u00b7zer", "tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "mit dem gelt in der stat drin lag,", "tokens": ["mit", "dem", "gelt", "in", "der", "stat", "drin", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "APPR", "ART", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "in dem wirteshaus bei dem wein,", "tokens": ["in", "dem", "wir\u00b7tes\u00b7haus", "bei", "dem", "wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "het gar kein achtung auf das sein", "tokens": ["het", "gar", "kein", "ach\u00b7tung", "auf", "das", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPR", "PRELS", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "und saget oft: was sol ich sorgen,", "tokens": ["und", "sa\u00b7get", "oft", ":", "was", "sol", "ich", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PWS", "VMFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "ich hab gnug, sterb heut oder morgen,", "tokens": ["ich", "hab", "gnug", ",", "sterb", "heut", "o\u00b7der", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "wan ich wei\u00df gewis und f\u00fcrwar,", "tokens": ["wan", "ich", "wei\u00df", "ge\u00b7wis", "und", "f\u00fcr\u00b7war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "wenn ich noch lebet hundert jar,", "tokens": ["wenn", "ich", "noch", "le\u00b7bet", "hun\u00b7dert", "jar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "so hab ich hundert jar zu e\u00dfen.", "tokens": ["so", "hab", "ich", "hun\u00b7dert", "jar", "zu", "e\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "solcher wort trib er vil verme\u00dfen,", "tokens": ["sol\u00b7cher", "wort", "trib", "er", "vil", "ver\u00b7me\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.19": {"text": "ie feuler und heilloser wur.", "tokens": ["ie", "feu\u00b7ler", "und", "heil\u00b7lo\u00b7ser", "wur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "forthin nicht mer gen acker fur", "tokens": ["for\u00b7thin", "nicht", "mer", "gen", "ac\u00b7ker", "fur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "den ganzen herbst, auch seet nit.", "tokens": ["den", "gan\u00b7zen", "herbst", ",", "auch", "seet", "nit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "als es hinaus kam an den schnit,", "tokens": ["als", "es", "hin\u00b7aus", "kam", "an", "den", "schnit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APZR", "VVFIN", "APPR", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "das sein nachbaurn in dorfgemein", "tokens": ["das", "sein", "nach\u00b7baurn", "in", "dorf\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "schnitten und f\u00fcrten koren ein", "tokens": ["schnit\u00b7ten", "und", "f\u00fcr\u00b7ten", "ko\u00b7ren", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJA", "NN", "ART"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "und samleten ir kesten vol,", "tokens": ["und", "sam\u00b7le\u00b7ten", "ir", "kes\u00b7ten", "vol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "zu zeren mit ornlich und wol;", "tokens": ["zu", "ze\u00b7ren", "mit", "orn\u00b7lich", "und", "wol", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJD", "KON", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "aber der faule bauersman", "tokens": ["a\u00b7ber", "der", "fau\u00b7le", "bau\u00b7ers\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "der het im sommer nichtsen tan,", "tokens": ["der", "het", "im", "som\u00b7mer", "nicht\u00b7sen", "tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "den sein vetterlich erb an worn,", "tokens": ["den", "sein", "vet\u00b7ter\u00b7lich", "erb", "an", "worn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "ADJD", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "het gebaut weder weiz noch korn;", "tokens": ["het", "ge\u00b7baut", "we\u00b7der", "weiz", "noch", "korn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "NE", "ADV", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "als der winter kam nach den sachen,", "tokens": ["als", "der", "win\u00b7ter", "kam", "nach", "den", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "het er kein treit, im brot zu bachen.", "tokens": ["het", "er", "kein", "treit", ",", "im", "brot", "zu", "ba\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "als nun der hunger in anfacht,", "tokens": ["als", "nun", "der", "hun\u00b7ger", "in", "an\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "APPR", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "er im ein losen f\u00fcrschlag macht,", "tokens": ["er", "im", "ein", "lo\u00b7sen", "f\u00fcrsc\u00b7hlag", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "stach darnider das weidvich sein,", "tokens": ["stach", "dar\u00b7ni\u00b7der", "das", "weid\u00b7vich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJD", "VAINF", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.36": {"text": "schaf, rinder, b\u00f6ck, gei\u00df und die schwein,", "tokens": ["schaf", ",", "rin\u00b7der", ",", "b\u00f6ck", ",", "gei\u00df", "und", "die", "schwein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "$,", "ADJD", "$,", "ADJD", "KON", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "eins nach dem andern solcher ma\u00df,", "tokens": ["eins", "nach", "dem", "an\u00b7dern", "sol\u00b7cher", "ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "sie sut und briet, darnach sie fra\u00df.", "tokens": ["sie", "sut", "und", "briet", ",", "dar\u00b7nach", "sie", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "$,", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "nach dem er auch darnider schlug", "tokens": ["nach", "dem", "er", "auch", "dar\u00b7ni\u00b7der", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "die ochsen, so vor in dem pflug", "tokens": ["die", "och\u00b7sen", ",", "so", "vor", "in", "dem", "pflug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "zugen, und die einsalzen was", "tokens": ["zu\u00b7gen", ",", "und", "die", "ein\u00b7sal\u00b7zen", "was"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "PWS"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.42": {"text": "und einen nach dem andern fra\u00df.", "tokens": ["und", "ei\u00b7nen", "nach", "dem", "an\u00b7dern", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "als solichs sahen seine hunt,", "tokens": ["als", "so\u00b7lichs", "sa\u00b7hen", "sei\u00b7ne", "hunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "da hettens ein gesprech gar runt,", "tokens": ["da", "het\u00b7tens", "ein", "ge\u00b7sprech", "gar", "runt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "sprachen: schaut, der faul bauersman,", "tokens": ["spra\u00b7chen", ":", "schaut", ",", "der", "faul", "bau\u00b7ers\u00b7man", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$,", "PRELS", "ADJD", "PTKVZ", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.46": {"text": "so er sein erbteil hat vertan,", "tokens": ["so", "er", "sein", "erb\u00b7teil", "hat", "ver\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "hat er sein weidvich abgestochen,", "tokens": ["hat", "er", "sein", "weid\u00b7vich", "ab\u00b7ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJD", "VVPP", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.48": {"text": "le\u00dft das sieden, braten und kochen,", "tokens": ["le\u00dft", "das", "sie\u00b7den", ",", "bra\u00b7ten", "und", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "hat das gefre\u00dfen aus dem salz,", "tokens": ["hat", "das", "ge\u00b7fre\u00b7\u00dfen", "aus", "dem", "salz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "APPR", "ART", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.50": {"text": "von den er het kes, milch und schmalz,", "tokens": ["von", "den", "er", "het", "kes", ",", "milch", "und", "schmalz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "PIS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "die im vil nutz haben getragen,", "tokens": ["die", "im", "vil", "nutz", "ha\u00b7ben", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.52": {"text": "das hat er als in wint geschlagen.", "tokens": ["das", "hat", "er", "als", "in", "wint", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "KOUS", "APPR", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "nach dem sein ochsen nider schlug,", "tokens": ["nach", "dem", "sein", "och\u00b7sen", "ni\u00b7der", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "welche im zugen in dem pflug,", "tokens": ["wel\u00b7che", "im", "zu\u00b7gen", "in", "dem", "pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "APPRART", "ADJA", "APPR", "ART", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "lin