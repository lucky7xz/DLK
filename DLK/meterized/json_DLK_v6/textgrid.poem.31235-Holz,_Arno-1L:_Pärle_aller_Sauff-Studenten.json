{"textgrid.poem.31235": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: P\u00e4rle aller Sauff-Studenten/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "P\u00e4rle aller Sauff-Studenten/", "tokens": ["P\u00e4r\u00b7le", "al\u00b7ler", "Sauff\u00b7Stu\u00b7den\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da/ bek\u00fckk dir deinen Flau\u00df!", "tokens": ["da", "/", "be\u00b7k\u00fckk", "dir", "dei\u00b7nen", "Flau\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine g\u00f6ldnen Posamenten", "tokens": ["Sei\u00b7ne", "g\u00f6ld\u00b7nen", "Po\u00b7sa\u00b7men\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "herbergt lengst das J\u00fcden-Hau\u00df.", "tokens": ["her\u00b7bergt", "lengst", "das", "J\u00fc\u00b7den\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ab-gedreten die Kal\u00f6schen/", "tokens": ["Ab\u00b7ge\u00b7dre\u00b7ten", "die", "Ka\u00b7l\u00f6\u00b7schen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die Barukke sizzt f\u00fcr qwer;", "tokens": ["die", "Ba\u00b7ruk\u00b7ke", "sizzt", "f\u00fcr", "qwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "gestern noch an sechzehn Gr\u00f6schen/", "tokens": ["ge\u00b7stern", "noch", "an", "sech\u00b7zehn", "Gr\u00f6\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "CARD", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "heute keinen Heller mehr.", "tokens": ["heu\u00b7te", "kei\u00b7nen", "Hel\u00b7ler", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und darzu dan noch dih\u00df Bromsen/", "tokens": ["Und", "dar\u00b7zu", "dan", "noch", "dih\u00df", "Brom\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "al\u00df ob nichts wie Mukken somsen!", "tokens": ["al\u00df", "ob", "nichts", "wie", "Muk\u00b7ken", "som\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Weck/ verfluchter ", "tokens": ["Weck", "/", "ver\u00b7fluch\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "von itztab b\u00fcn ich dugendhafft!", "tokens": ["von", "itz\u00b7tab", "b\u00fcn", "ich", "du\u00b7gend\u00b7hafft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allzu willige Bluminde/", "tokens": ["All\u00b7zu", "wil\u00b7li\u00b7ge", "Blu\u00b7min\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bitte/ schleu\u00df dich nicht mehr auff/", "tokens": ["bit\u00b7te", "/", "schleu\u00df", "dich", "nicht", "mehr", "auff", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "weil ich mir dir itzt entwinde/", "tokens": ["weil", "ich", "mir", "dir", "itzt", "ent\u00b7win\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "ADV", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "denn nunmehro huhst ich drauff.", "tokens": ["denn", "nun\u00b7meh\u00b7ro", "huhst", "ich", "drauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach/ man mu\u00df euch/ Kindgens/ k\u00e4nnen/", "tokens": ["Ach", "/", "man", "mu\u00df", "euch", "/", "Kind\u00b7gens", "/", "k\u00e4n\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$(", "PIS", "VMFIN", "PPER", "$(", "NN", "$(", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die wie rohtes Feuer br\u00e4nnen/", "tokens": ["die", "wie", "roh\u00b7tes", "Feu\u00b7er", "br\u00e4n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "noch die allerz\u00e4hrtsten sind.", "tokens": ["noch", "die", "al\u00b7ler\u00b7z\u00e4hrts\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nachts/ wie die Boeten fabeln/", "tokens": ["Nachts", "/", "wie", "die", "Boe\u00b7ten", "fa\u00b7beln", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "reutten sie auff Ofen-Gabeln!", "tokens": ["reut\u00b7ten", "sie", "auff", "O\u00b7fen\u00b7Ga\u00b7beln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Weck/ verfluchte ", "tokens": ["Weck", "/", "ver\u00b7fluch\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "von itztab b\u00fcn ich dugendhafft!", "tokens": ["von", "itz\u00b7tab", "b\u00fcn", "ich", "du\u00b7gend\u00b7hafft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "hellt wie dr\u00f6stend mir den Sinn;", "tokens": ["hellt", "wie", "dr\u00f6s\u00b7tend", "mir", "den", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "du bist noch die eintzge Dame/", "tokens": ["du", "bist", "noch", "die", "eintz\u00b7ge", "Da\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der ich gantz ergeben bin.", "tokens": ["der", "ich", "gantz", "er\u00b7ge\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mit dir noch zu scharmiren/", "tokens": ["Nur", "mit", "dir", "noch", "zu", "schar\u00b7mi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "halt ich r\u00e4cht for meinen Zw\u00f6kk;", "tokens": ["halt", "ich", "r\u00e4cht", "for", "mei\u00b7nen", "Zw\u00f6kk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6gel-spihlen/ Billjardiren", "tokens": ["K\u00f6\u00b7gel\u00b7spihlen", "/", "Bill\u00b7jar\u00b7di\u00b7ren"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$(", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "k\u00f6mbt mir f\u00fcr wie Teuffels-Dr\u00f6kk.", "tokens": ["k\u00f6mbt", "mir", "f\u00fcr", "wie", "Teuf\u00b7fels\u00b7Dr\u00f6kk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "KOKOM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch die sp\u00e4htsten Folge-Zeiten", "tokens": ["Noch", "die", "sp\u00e4hts\u00b7ten", "Fol\u00b7ge\u00b7Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "werden mir dr\u00fcmb Ruhm bereiten!", "tokens": ["wer\u00b7den", "mir", "dr\u00fcmb", "Ruhm", "be\u00b7rei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wa\u00df man auch darg\u00e4gen klafft/", "tokens": ["Wa\u00df", "man", "auch", "dar\u00b7g\u00e4\u00b7gen", "klafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PAV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "von itztab b\u00fcn ich dugendhafft!", "tokens": ["von", "itz\u00b7tab", "b\u00fcn", "ich", "du\u00b7gend\u00b7hafft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}