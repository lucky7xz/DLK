{"textgrid.poem.43328": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Empfange mich, dem Gram geweihter Bach!", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Empfange mich, dem Gram geweihter Bach!", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "dem", "Gram", "ge\u00b7weih\u00b7ter", "Bach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Oft schallt mein traurig Lied dir sympathetisch nach.", "tokens": ["Oft", "schallt", "mein", "trau\u00b7rig", "Lied", "dir", "sym\u00b7pa\u00b7the\u00b7tisch", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn hier vor deiner Nymphe Klagen", "tokens": ["Wenn", "hier", "vor", "dei\u00b7ner", "Nym\u00b7phe", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wehm\u00fcth'ge Herzen st\u00e4rker schlagen;", "tokens": ["Weh\u00b7m\u00fcth'\u00b7ge", "Her\u00b7zen", "st\u00e4r\u00b7ker", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Zephyre hier deiner Nymphe Klagen", "tokens": ["Wenn", "Ze\u00b7phy\u00b7re", "hier", "dei\u00b7ner", "Nym\u00b7phe", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In schaudernd Laub sanfthallend \u00fcbertragen.", "tokens": ["In", "schau\u00b7dernd", "Laub", "sanft\u00b7hal\u00b7lend", "\u00fc\u00b7ber\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es horcht der Hayn auf der Najade Lied.", "tokens": ["Es", "horcht", "der", "Hayn", "auf", "der", "Na\u00b7ja\u00b7de", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und beugt vor der Najade Lied", "tokens": ["Und", "beugt", "vor", "der", "Na\u00b7ja\u00b7de", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sein Haupt, das finstrer Ernst umzieht:", "tokens": ["Sein", "Haupt", ",", "das", "finst\u00b7rer", "Ernst", "um\u00b7zieht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und herrscht den b\u00e4ngsten Gram in Eulen,", "tokens": ["Und", "herrscht", "den", "b\u00e4ngs\u00b7ten", "Gram", "in", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die f\u00fcrchterlich auf seinen Armen heulen.", "tokens": ["Die", "f\u00fcrch\u00b7ter\u00b7lich", "auf", "sei\u00b7nen", "Ar\u00b7men", "heu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst, Daphne, war die Halbg\u00f6ttinn", "tokens": ["Einst", ",", "Daph\u00b7ne", ",", "war", "die", "Halb\u00b7g\u00f6t\u00b7tinn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein M\u00e4dchen, eine Sch\u00e4ferinn,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "ei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des ganzen Thales Wunsch und Ehre:", "tokens": ["Des", "gan\u00b7zen", "Tha\u00b7les", "Wunsch", "und", "Eh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch stolz und wilder noch, als Meere,", "tokens": ["Doch", "stolz", "und", "wil\u00b7der", "noch", ",", "als", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJA", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wilder noch als du \u2013 wenn dieses m\u00f6glich w\u00e4re.", "tokens": ["Und", "wil\u00b7der", "noch", "als", "du", "\u2013", "wenn", "die\u00b7ses", "m\u00f6g\u00b7lich", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "KOUS", "PPER", "$(", "KOUS", "PDAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Selbst Hyas, der Adon der Flur,", "tokens": ["Selbst", "Hyas", ",", "der", "A\u00b7don", "der", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sch\u00f6n, wie die sch\u00f6nere Natur,", "tokens": ["Sch\u00f6n", ",", "wie", "die", "sch\u00f6\u00b7ne\u00b7re", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dessen sanfter Stirn, wie von des Amors Bogen,", "tokens": ["Von", "des\u00b7sen", "sanf\u00b7ter", "Stirn", ",", "wie", "von", "des", "A\u00b7mors", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In manche wei\u00dfe Brust der Liebe Pfeile flogen;", "tokens": ["In", "man\u00b7che", "wei\u00b7\u00dfe", "Brust", "der", "Lie\u00b7be", "Pfei\u00b7le", "flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auf den, bey jedem Reihentanz,", "tokens": ["Auf", "den", ",", "bey", "je\u00b7dem", "Rei\u00b7hen\u00b7tanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der feinste Strau\u00df, der buntste Kranz,", "tokens": ["Der", "feins\u00b7te", "Strau\u00df", ",", "der", "bunts\u00b7te", "Kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von Seufzern still begleitet, flogen:", "tokens": ["Von", "Seuf\u00b7zern", "still", "be\u00b7glei\u00b7tet", ",", "flo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Selbst Hyas liebte sie!", "tokens": ["Selbst", "Hyas", "lieb\u00b7te", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Mit wie viel Ehrfurcht liebt er sie!", "tokens": ["Mit", "wie", "viel", "Ehr\u00b7furcht", "liebt", "er", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wie r\u00fchrend klang sein z\u00e4rtlich Lied um sie!", "tokens": ["Wie", "r\u00fch\u00b7rend", "klang", "sein", "z\u00e4rt\u00b7lich", "Lied", "um", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "ADJD", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die fernste Flur empfand des Liedes Harmonie,", "tokens": ["Die", "ferns\u00b7te", "Flur", "emp\u00b7fand", "des", "Lie\u00b7des", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nur sie, nur sie empfand es nie.", "tokens": ["Nur", "sie", ",", "nur", "sie", "emp\u00b7fand", "es", "nie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie unaussprechlich ward sein Leiden!", "tokens": ["Wie", "un\u00b7aus\u00b7sprech\u00b7lich", "ward", "sein", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die Grausame! stets floh sie ihn!", "tokens": ["Die", "Grau\u00b7sa\u00b7me", "!", "stets", "floh", "sie", "ihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Grausame! wie konnte sie ihn fliehn? \u2013", "tokens": ["Die", "Grau\u00b7sa\u00b7me", "!", "wie", "konn\u00b7te", "sie", "ihn", "fliehn", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Beb, Eitle, beb! und f\u00fcrchte seine Leiden!", "tokens": ["Beb", ",", "Eit\u00b7le", ",", "beb", "!", "und", "f\u00fcrch\u00b7te", "sei\u00b7ne", "Lei\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "$.", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die Rache wird nicht stets verziehn.", "tokens": ["Die", "Ra\u00b7che", "wird", "nicht", "stets", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Kannst du am tiefsten Harm dein stolzes Auge weiden?", "tokens": ["Kannst", "du", "am", "tiefs\u00b7ten", "Harm", "dein", "stol\u00b7zes", "Au\u00b7ge", "wei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Einst bat er sie auf seinen Knien,", "tokens": ["Einst", "bat", "er", "sie", "auf", "sei\u00b7nen", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Thr\u00e4nen bat er sie, nicht ewig ihn zu fliehn:", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "bat", "er", "sie", ",", "nicht", "e\u00b7wig", "ihn", "zu", "fliehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$,", "PTKNEG", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Se floh: und sterben sah sie ihn.", "tokens": ["Se", "floh", ":", "und", "ster\u00b7ben", "sah", "sie", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "KON", "VVINF", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er starb, der Hirt, von dem in ihren Finsternissen", "tokens": ["Er", "starb", ",", "der", "Hirt", ",", "von", "dem", "in", "ih\u00b7ren", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Fabel einst gedichtet hat,", "tokens": ["Die", "Fa\u00b7bel", "einst", "ge\u00b7dich\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df eine L\u00f6winn ihn zerrissen \u2013", "tokens": ["Da\u00df", "ei\u00b7ne", "L\u00f6\u00b7winn", "ihn", "zer\u00b7ris\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie war die L\u00f6winn, die es that.", "tokens": ["Sie", "war", "die", "L\u00f6\u00b7winn", ",", "die", "es", "that", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie starrt und bebt. Der Reue Schmerzen", "tokens": ["Sie", "starrt", "und", "bebt", ".", "Der", "Reu\u00b7e", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ergreifen schnell das h\u00e4rteste der Herzen;", "tokens": ["Er\u00b7grei\u00b7fen", "schnell", "das", "h\u00e4r\u00b7tes\u00b7te", "der", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zeus sah des M\u00e4dchens Leid,", "tokens": ["Zeus", "sah", "des", "M\u00e4d\u00b7chens", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und gab in z\u00fcrnender Gerechtigkeit", "tokens": ["Und", "gab", "in", "z\u00fcr\u00b7nen\u00b7der", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr einer Nymph Unsterblichkeit,", "tokens": ["Ihr", "ei\u00b7ner", "Nymph", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Gab ihrem Gram die Ewigkeit.", "tokens": ["Gab", "ih\u00b7rem", "Gram", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die schwermuthsvolle der Najaden", "tokens": ["Die", "schwer\u00b7muths\u00b7vol\u00b7le", "der", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weint seit Jahrhunderten hier noch um ihren Freund;", "tokens": ["Weint", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "hier", "noch", "um", "ih\u00b7ren", "Freund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Und diesen Bach hat sie und die Hyaden", "tokens": ["Und", "die\u00b7sen", "Bach", "hat", "sie", "und", "die", "Hya\u00b7den"], "t