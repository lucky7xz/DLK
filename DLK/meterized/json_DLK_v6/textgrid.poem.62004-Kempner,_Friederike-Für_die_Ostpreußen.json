{"textgrid.poem.62004": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr die Ostpreu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00fcstre Nacht und lange Schatten", "tokens": ["D\u00fcst\u00b7re", "Nacht", "und", "lan\u00b7ge", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Land und \u00fcber Meer,", "tokens": ["Ue\u00b7ber", "Land", "und", "\u00fc\u00b7ber", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Vaterlandes Matten", "tokens": ["Auf", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleicht das Elend hin und her.", "tokens": ["Schleicht", "das", "E\u00b7lend", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "D\u00fcstre Nacht und lange Schatten", "tokens": ["D\u00fcst\u00b7re", "Nacht", "und", "lan\u00b7ge", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Land und \u00fcber Meer,", "tokens": ["Ue\u00b7ber", "Land", "und", "\u00fc\u00b7ber", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gestalten, bleichen, matten,", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", ",", "blei\u00b7chen", ",", "mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fccken immer, immer n\u00e4h'r! \u2013", "tokens": ["R\u00fc\u00b7cken", "im\u00b7mer", ",", "im\u00b7mer", "n\u00e4h'r", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da \u2013 ein Leuchten l\u00e4ngs des Meeres \u2013", "tokens": ["Da", "\u2013", "ein", "Leuch\u00b7ten", "l\u00e4ngs", "des", "Mee\u00b7res", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, der Liebe Sonnenschein,", "tokens": ["Ach", ",", "der", "Lie\u00b7be", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rker als die Macht des Heeres \u2013", "tokens": ["St\u00e4r\u00b7ker", "als", "die", "Macht", "des", "Hee\u00b7res", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fccket in die Nacht hinein.", "tokens": ["R\u00fc\u00b7cket", "in", "die", "Nacht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Spricht zum Elend: horch, ich lehr' es,", "tokens": ["Spricht", "zum", "E\u00b7lend", ":", "horch", ",", "ich", "lehr'", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "VVIMP", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df zuletzt der Sieg doch mein!", "tokens": ["Da\u00df", "zu\u00b7letzt", "der", "Sieg", "doch", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}