{"dta.poem.12124": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kannst nicht \u00e4u\u00dferlich die ganze Welt umfassen,", "tokens": ["Du", "kannst", "nicht", "\u00e4u\u00b7\u00dfer\u00b7lich", "die", "gan\u00b7ze", "Welt", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An innrer Ganzheit mu\u00dft du dir gen\u00fcgen lassen.", "tokens": ["An", "inn\u00b7rer", "Ganz\u00b7heit", "mu\u00dft", "du", "dir", "ge\u00b7n\u00fc\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt ist \u00fcberal ein ganzer Gottesglanz,", "tokens": ["Die", "Welt", "ist", "\u00fc\u00b7be\u00b7ral", "ein", "gan\u00b7zer", "Got\u00b7tes\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sie der Liebe Stral verschlingt um dich zum Glanz.", "tokens": ["Wo", "sie", "der", "Lie\u00b7be", "Stral", "ver\u00b7schlingt", "um", "dich", "zum", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "APPR", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da ist das Kleine gro\u00df, und nicht das Gro\u00dfe blo\u00df,", "tokens": ["Da", "ist", "das", "Klei\u00b7ne", "gro\u00df", ",", "und", "nicht", "das", "Gro\u00b7\u00dfe", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJD", "$,", "KON", "PTKNEG", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da siehst du Gro\u00df und Klein die Welt in Gottes Schoo\u00df.", "tokens": ["Da", "siehst", "du", "Gro\u00df", "und", "Klein", "die", "Welt", "in", "Got\u00b7tes", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "KON", "NN", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}