{"textgrid.poem.56160": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Irgendwo wohnt das Gold in der verw\u00f6hnenden Bank", "tokens": ["Ir\u00b7gend\u00b7wo", "wohnt", "das", "Gold", "in", "der", "ver\u00b7w\u00f6h\u00b7nen\u00b7den", "Bank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "und mit Tausenden tut es vertraulich. Doch jener", "tokens": ["und", "mit", "Tau\u00b7sen\u00b7den", "tut", "es", "ver\u00b7trau\u00b7lich", ".", "Doch", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$.", "KON", "PDS"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Blinde, der Bettler, ist selbst dem kupfernen Zehner", "tokens": ["Blin\u00b7de", ",", "der", "Bett\u00b7ler", ",", "ist", "selbst", "dem", "kup\u00b7fer\u00b7nen", "Zeh\u00b7ner"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "wie ein verlorener Ort, wie das staubige Eck unterm Schrank.", "tokens": ["wie", "ein", "ver\u00b7lo\u00b7re\u00b7ner", "Ort", ",", "wie", "das", "stau\u00b7bi\u00b7ge", "Eck", "un\u00b7term", "Schrank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+---+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Gesch\u00e4ften entlang ist das Geld wie zuhause", "tokens": ["In", "den", "Ge\u00b7sch\u00e4f\u00b7ten", "ent\u00b7lang", "ist", "das", "Geld", "wie", "zu\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPO", "VAFIN", "ART", "NN", "KOKOM", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "und verkleidet sich scheinbar in Seide, Nelken und Pelz.", "tokens": ["und", "ver\u00b7klei\u00b7det", "sich", "schein\u00b7bar", "in", "Sei\u00b7de", ",", "Nel\u00b7ken", "und", "Pelz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Er, der Schweigende, steht in der Atempause", "tokens": ["Er", ",", "der", "Schwei\u00b7gen\u00b7de", ",", "steht", "in", "der", "A\u00b7tem\u00b7pau\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "alles des wach oder schlafend atmenden Gelds.", "tokens": ["al\u00b7les", "des", "wach", "o\u00b7der", "schla\u00b7fend", "at\u00b7men\u00b7den", "Gelds", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "O wie mag sie sich schlie\u00dfen bei Nacht, diese immer offene Hand.", "tokens": ["O", "wie", "mag", "sie", "sich", "schlie\u00b7\u00dfen", "bei", "Nacht", ",", "die\u00b7se", "im\u00b7mer", "of\u00b7fe\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "PRF", "VVFIN", "APPR", "NN", "$,", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "--+--+--+--+-+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Morgen holt sie das Schicksal wieder, und t\u00e4glich", "tokens": ["Mor\u00b7gen", "holt", "sie", "das", "Schick\u00b7sal", "wie\u00b7der", ",", "und", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,", "KON", "ADJD"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "h\u00e4lt es sie hin: hell, elend, unendlich zerst\u00f6rbar.", "tokens": ["h\u00e4lt", "es", "sie", "hin", ":", "hell", ",", "e\u00b7lend", ",", "un\u00b7end\u00b7lich", "zer\u00b7st\u00f6r\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$.", "ADJD", "$,", "ADJD", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Da\u00df doch einer, ein Schauender, endlich ihren langen Bestand", "tokens": ["Da\u00df", "doch", "ei\u00b7ner", ",", "ein", "Schau\u00b7en\u00b7der", ",", "end\u00b7lich", "ih\u00b7ren", "lan\u00b7gen", "Be\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "$,", "ART", "NN", "$,", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+-+-+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "staunend begriffe und r\u00fchmte. Nur dem Aufsingenden s\u00e4glich.", "tokens": ["stau\u00b7nend", "be\u00b7grif\u00b7fe", "und", "r\u00fchm\u00b7te", ".", "Nur", "dem", "Auf\u00b7sin\u00b7gen\u00b7den", "s\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$.", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Nur dem G\u00f6ttlichen h\u00f6rbar.", "tokens": ["Nur", "dem", "G\u00f6tt\u00b7li\u00b7chen", "h\u00f6r\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}