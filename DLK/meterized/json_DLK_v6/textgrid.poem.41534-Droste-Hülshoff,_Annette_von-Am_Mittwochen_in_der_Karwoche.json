{"textgrid.poem.41534": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Mittwochen in der Karwoche", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl, so will ich vorw\u00e4rts gehen", "tokens": ["Wohl", ",", "so", "will", "ich", "vor\u00b7w\u00e4rts", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der schwergepre\u00dften Brust,", "tokens": ["Mit", "der", "schwer\u00b7ge\u00b7pre\u00df\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird doch alles mir bewu\u00dft,", "tokens": ["Wird", "doch", "al\u00b7les", "mir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Toten auferstehen.", "tokens": ["Wenn", "die", "To\u00b7ten", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so lange mu\u00df ich tragen,", "tokens": ["Und", "so", "lan\u00b7ge", "mu\u00df", "ich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist meine gr\u00f6\u00dfte Not,", "tokens": ["Dies", "ist", "mei\u00b7ne", "gr\u00f6\u00df\u00b7te", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "All die \u00fcbermut'gen Fragen,", "tokens": ["All", "die", "\u00fc\u00b7ber\u00b7mut'\u00b7gen", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mich dr\u00fccken in den Tod:", "tokens": ["Die", "mich", "dr\u00fc\u00b7cken", "in", "den", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Leib, der l\u00e4ngst entfaltet", "tokens": ["Wie", "ein", "Leib", ",", "der", "l\u00e4ngst", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Pflanze mildem Saft,", "tokens": ["Durch", "der", "Pflan\u00b7ze", "mil\u00b7dem", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In erneuter Lebenskraft", "tokens": ["In", "er\u00b7neu\u00b7ter", "Le\u00b7bens\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den zweiten Leib gestaltet,", "tokens": ["In", "den", "zwei\u00b7ten", "Leib", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie er wieder mag erscheinen,", "tokens": ["Wie", "er", "wie\u00b7der", "mag", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem andern unverwehrt,", "tokens": ["Von", "dem", "an\u00b7dern", "un\u00b7ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der ihn trug in den Gebeinen,", "tokens": ["Der", "ihn", "trug", "in", "den", "Ge\u00b7bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vom dritten l\u00e4ngst verzehrt;", "tokens": ["Und", "vom", "drit\u00b7ten", "l\u00e4ngst", "ver\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was vom Guten, was vom B\u00f6sen", "tokens": ["Was", "vom", "Gu\u00b7ten", ",", "was", "vom", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Seele mannigfalt;", "tokens": ["In", "der", "See\u00b7le", "man\u00b7nig\u00b7falt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die heiligste Gewalt", "tokens": ["Wie", "die", "hei\u00b7ligs\u00b7te", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sich in Erdenlust will l\u00f6sen;", "tokens": ["Sich", "in", "Er\u00b7den\u00b7lust", "will", "l\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df in jenen zarten Stunden,", "tokens": ["Da\u00df", "in", "je\u00b7nen", "zar\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo wir wie mit Gott vereint,", "tokens": ["Wo", "wir", "wie", "mit", "Gott", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOKOM", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns am schw\u00e4chsten oft gefunden", "tokens": ["Uns", "am", "schw\u00e4chs\u00b7ten", "oft", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jener ewig rege Feind;", "tokens": ["Je\u00b7ner", "e\u00b7wig", "re\u00b7ge", "Feind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und noch viele andre Dinge,", "tokens": ["Und", "noch", "vie\u00b7le", "and\u00b7re", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir nicht zu wissen not", "tokens": ["Die", "mir", "nicht", "zu", "wis\u00b7sen", "not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "PTKZU", "VVINF", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich dr\u00fccken in den Tod.", "tokens": ["Und", "mich", "dr\u00fc\u00b7cken", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, dem Frommen gar geringe!", "tokens": ["Ach", ",", "dem", "From\u00b7men", "gar", "ge\u00b7rin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch in meinem leeren Herzen", "tokens": ["Doch", "in", "mei\u00b7nem", "lee\u00b7ren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonder Wahrheit, sonder Rast,", "tokens": ["Son\u00b7der", "Wahr\u00b7heit", ",", "son\u00b7der", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lagern sie zu dumpfen Schmerzen", "tokens": ["La\u00b7gern", "sie", "zu", "dum\u00b7pfen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine spitze Felsenlast.", "tokens": ["Ei\u00b7ne", "spit\u00b7ze", "Fel\u00b7sen\u00b7last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herr, ich kann sie nicht verbannen,", "tokens": ["Herr", ",", "ich", "kann", "sie", "nicht", "ver\u00b7ban\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur verschlie\u00dfen, fest und treu;", "tokens": ["Nur", "ver\u00b7schlie\u00b7\u00dfen", ",", "fest", "und", "treu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Leben rauscht vorbei,", "tokens": ["Und", "das", "Le\u00b7ben", "rauscht", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Tag treibt sie von dannen!", "tokens": ["Und", "dein", "Tag", "treibt", "sie", "von", "dan\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sieh, so kann ich gl\u00e4ubig sagen,", "tokens": ["Sieh", ",", "so", "kann", "ich", "gl\u00e4u\u00b7big", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber meine Seele steht,", "tokens": ["A\u00b7ber", "mei\u00b7ne", "See\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der Tag von allen Tagen", "tokens": ["Wenn", "der", "Tag", "von", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Furchtbar mir vor\u00fcbergeht.", "tokens": ["Furcht\u00b7bar", "mir", "vor\u00b7\u00fc\u00b7ber\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie wenn in beklemmter Schw\u00fcle", "tokens": ["Wie", "wenn", "in", "be\u00b7klemm\u00b7ter", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine schwarze Wolkenmacht", "tokens": ["Ei\u00b7ne", "schwar\u00b7ze", "Wol\u00b7ken\u00b7macht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00e4rzer dunkelt durch die Nacht,", "tokens": ["Schw\u00e4r\u00b7zer", "dun\u00b7kelt", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir um des Wetters K\u00fchle", "tokens": ["Da\u00df", "wir", "um", "des", "Wet\u00b7ters", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flehn mit allen seinen Schrecken,", "tokens": ["Flehn", "mit", "al\u00b7len", "sei\u00b7nen", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt in deiner Ewigkeit,", "tokens": ["Liegt", "in", "dei\u00b7ner", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie ein hei\u00dfer dunkler Flecken,", "tokens": ["Wie", "ein", "hei\u00b7\u00dfer", "dunk\u00b7ler", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jene namenlose Zeit.", "tokens": ["Je\u00b7ne", "na\u00b7men\u00b7lo\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber wie mit Eisenketten", "tokens": ["A\u00b7ber", "wie", "mit", "Ei\u00b7sen\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00df' ich meine Augen fest,", "tokens": ["Schlie\u00df'", "ich", "mei\u00b7ne", "Au\u00b7gen", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die Felsenwand gepre\u00dft,", "tokens": ["An", "die", "Fel\u00b7sen\u00b7wand", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Schwindel mich zu retten,", "tokens": ["Vor", "dem", "Schwin\u00b7del", "mich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so will ich vorw\u00e4rts gehen", "tokens": ["Und", "so", "will", "ich", "vor\u00b7w\u00e4rts", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der schwerbeladnen Brust,", "tokens": ["Mit", "der", "schwer\u00b7be\u00b7lad\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn die Toten auferstehen,", "tokens": ["Wenn", "die", "To\u00b7ten", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird doch alles mir bewu\u00dft.", "tokens": ["Wird", "doch", "al\u00b7les", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}