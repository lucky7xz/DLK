{"textgrid.poem.31488": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der selbselbste Lebens-Safft / wird mit Milch getr\u00e4nket.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der selbselbste Lebens-Safft / wird mit Milch getr\u00e4nket.", "tokens": ["Der", "selb\u00b7selbs\u00b7te", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", "/", "wird", "mit", "Milch", "ge\u00b7tr\u00e4n\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der die weite Welt besiegt /", "tokens": ["Der", "die", "wei\u00b7te", "Welt", "be\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in der \u00e4ngen Krippen ligt.", "tokens": ["in", "der", "\u00e4n\u00b7gen", "Krip\u00b7pen", "ligt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunder! aller Himmel Herr sich der Erden schenket.", "tokens": ["Wun\u00b7der", "!", "al\u00b7ler", "Him\u00b7mel", "Herr", "sich", "der", "Er\u00b7den", "schen\u00b7ket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Der selbst ist das H\u00f6chste Gut / sich ins Elend senket.", "tokens": ["Der", "selbst", "ist", "das", "H\u00f6chs\u00b7te", "Gut", "/", "sich", "ins", "E\u00b7lend", "sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Bewegung wird gewiegt:", "tokens": ["Die", "Be\u00b7we\u00b7gung", "wird", "ge\u00b7wiegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "von der selbst der Himmel kriegt", "tokens": ["von", "der", "selbst", "der", "Him\u00b7mel", "kriegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "seines Lauffes Ordnung-Pflicht / und nach ihr sich lenket.", "tokens": ["sei\u00b7nes", "Lauf\u00b7fes", "Ord\u00b7nung\u00b7Pflicht", "/", "und", "nach", "ihr", "sich", "len\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "KON", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Hirten! lasst den Himmel stehn /", "tokens": ["Hir\u00b7ten", "!", "lasst", "den", "Him\u00b7mel", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "und uns in den Stall hingehn!", "tokens": ["und", "uns", "in", "den", "Stall", "hin\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "kam' euch jemals auch zu Ohren", "tokens": ["kam'", "euch", "je\u00b7mals", "auch", "zu", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "solch ein sonder-Wunderwerk /", "tokens": ["solch", "ein", "son\u00b7der\u00b7Wun\u00b7der\u00b7werk", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "da\u00df die Schwachheit hat die St\u00e4rk'", "tokens": ["da\u00df", "die", "Schwach\u00b7heit", "hat", "die", "St\u00e4rk'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "und ein Stern die Sonn gebohren?", "tokens": ["und", "ein", "Stern", "die", "Sonn", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}