{"textgrid.poem.37198": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Molly fehlt", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum reizt dieser Hain, warum die sch\u00f6ne Wiese,", "tokens": ["Wa\u00b7rum", "reizt", "die\u00b7ser", "Hain", ",", "wa\u00b7rum", "die", "sch\u00f6\u00b7ne", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die er bekr\u00e4nzt, uns halb nur? fragen wir.", "tokens": ["Die", "er", "be\u00b7kr\u00e4nzt", ",", "uns", "halb", "nur", "?", "fra\u00b7gen", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PPER", "ADJD", "ADV", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht viel entbehrt zu einem Paradiese", "tokens": ["Nicht", "viel", "ent\u00b7behrt", "zu", "ei\u00b7nem", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Lustgeb\u00fcsch \u2013 doch Molly fehlet hier.", "tokens": ["Dies", "Lust\u00b7ge\u00b7b\u00fcsch", "\u2013", "doch", "Mol\u00b7ly", "feh\u00b7let", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "KON", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Freude G\u00f6tterchen, die sie gedr\u00e4ngt umschlie\u00dfen,", "tokens": ["Der", "Freu\u00b7de", "G\u00f6t\u00b7ter\u00b7chen", ",", "die", "sie", "ge\u00b7dr\u00e4ngt", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Belauscht ich j\u00fcngst an diesem Ort.", "tokens": ["Be\u00b7lauscht", "ich", "j\u00fcngst", "an", "die\u00b7sem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihr allein, hie\u00df es, gilt jenes alte Wort:", "tokens": ["Von", "ihr", "al\u00b7lein", ",", "hie\u00df", "es", ",", "gilt", "je\u00b7nes", "al\u00b7te", "Wort", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie sehen oder sie vermissen!", "tokens": ["Sie", "se\u00b7hen", "o\u00b7der", "sie", "ver\u00b7mis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Entwickelt war in unsrem Kreise", "tokens": ["Ent\u00b7wi\u00b7ckelt", "war", "in", "uns\u00b7rem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Geistes Anmut, d\u00e4mmert wo und tagt", "tokens": ["Des", "Geis\u00b7tes", "An\u00b7mut", ",", "d\u00e4m\u00b7mert", "wo", "und", "tagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "PWAV", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Stral des Lichtes, strebt und ragt", "tokens": ["Ein", "Stral", "des", "Lich\u00b7tes", ",", "strebt", "und", "ragt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald hier bald da des Ausdrucks Ton und Weise,", "tokens": ["Bald", "hier", "bald", "da", "des", "Aus\u00b7drucks", "Ton", "und", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann spricht der lobende ganz leise:", "tokens": ["Dann", "spricht", "der", "lo\u00b7ben\u00b7de", "ganz", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Viel feiner, treffender und weniger gewagt", "tokens": ["Viel", "fei\u00b7ner", ",", "tref\u00b7fen\u00b7der", "und", "we\u00b7ni\u00b7ger", "ge\u00b7wagt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00e4tt unsre Molly das gesagt.", "tokens": ["H\u00e4tt", "uns\u00b7re", "Mol\u00b7ly", "das", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "PDS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Scherz, die Musen im Geleite", "tokens": ["Der", "Scherz", ",", "die", "Mu\u00b7sen", "im", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Huldg\u00f6ttinnen stehn ihr immerdar zur Hand.", "tokens": ["Der", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "stehn", "ihr", "im\u00b7mer\u00b7dar", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie leiht dem Spotte selbst ein attisches Gewand,", "tokens": ["Sie", "leiht", "dem", "Spot\u00b7te", "selbst", "ein", "at\u00b7ti\u00b7sches", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und liebensw\u00fcrdiger wird Tiefsinn und Verstand,", "tokens": ["Und", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7ger", "wird", "Tief\u00b7sinn", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erscheinen sie an ihrer Seite.", "tokens": ["Er\u00b7schei\u00b7nen", "sie", "an", "ih\u00b7rer", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}