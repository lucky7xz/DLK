{"textgrid.poem.52382": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "32. \u00dcmkihrt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'t is Middag, un oll Moses Abraham", "tokens": ["'t", "is", "Mid\u00b7dag", ",", "un", "oll", "Mo\u00b7ses", "Ab\u00b7ra\u00b7ham"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "FM", "NN", "$,", "FM", "FM", "FM", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "De sitt bi Disch. 'ne sch\u00f6ne Spickgaus steiht \u2013", "tokens": ["De", "sitt", "bi", "Disch", ".", "'ne", "sch\u00f6\u00b7ne", "Spick\u00b7gaus", "steiht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "De irst, de ut den Rok herute kamen deiht \u2013", "tokens": ["De", "irst", ",", "de", "ut", "den", "Rok", "he\u00b7ru\u00b7te", "ka\u00b7men", "deiht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "FM", "FM", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "V\u00f6r em, as Joel Itzig rinne kamm.", "tokens": ["V\u00f6r", "em", ",", "as", "Joel", "It\u00b7zig", "rin\u00b7ne", "kamm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "NE", "NE", "NE", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u00bbgu'n Morgen, Abraham! Ich w\u00fcnsche wohlgespeist.", "tokens": ["\u00bb", "gu'n", "Mor\u00b7gen", ",", "Ab\u00b7ra\u00b7ham", "!", "Ich", "w\u00fcn\u00b7sche", "wohl\u00b7ge\u00b7speist", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wos \u00fcs? Sieh doch einmol! Wo hai\u00dft?", "tokens": ["Wos", "\u00fcs", "?", "Sieh", "doch", "ein\u00b7mol", "!", "Wo", "hai\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$.", "NE", "ADV", "ADV", "$.", "PWAV", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Wohrhaft'gen Gott schon von die Gaus!", "tokens": ["Wohr\u00b7haft'\u00b7gen", "Gott", "schon", "von", "die", "Gaus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo? Hast du die schon aus dem Rauch heraus?\u00ab", "tokens": ["Wo", "?", "Hast", "du", "die", "schon", "aus", "dem", "Rauch", "he\u00b7raus", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "ART", "ADV", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Oll Abraham verfihrt sick niedertr\u00e4chtig,", "tokens": ["Oll", "Ab\u00b7ra\u00b7ham", "ver\u00b7fihrt", "sick", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "De Spickgaus smeckt em doch tau pr\u00e4chtig,", "tokens": ["De", "Spick\u00b7gaus", "smeckt", "em", "doch", "tau", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hei wull'e ungirn wat von missen.", "tokens": ["Hei", "wull'e", "un\u00b7girn", "wat", "von", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Indessen g\u00fcng dat doch nich an,", "tokens": ["In\u00b7des\u00b7sen", "g\u00fcng", "dat", "doch", "nich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hei m\u00fc\u00dft em n\u00f6d'gen: \u00bbK\u00fcmm! Setz dir heran!", "tokens": ["Hei", "m\u00fc\u00dft", "em", "n\u00f6d'\u00b7gen", ":", "\u00bb", "K\u00fcmm", "!", "Setz", "dir", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Perbier doch mol un e\u00df en Bissen!\u00ab", "tokens": ["Per\u00b7bier", "doch", "mol", "un", "e\u00df", "en", "Bis\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "\u00bbaih waih! Es tut mer leid; geessen hob ich schon,", "tokens": ["\u00bb", "aih", "waih", "!", "Es", "tut", "mer", "leid", ";", "gees\u00b7sen", "hob", "ich", "schon", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$.", "VVPP", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ich komm von Moses Salomon,", "tokens": ["Ich", "komm", "von", "Mo\u00b7ses", "Sa\u00b7lo\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wo ich geessen Eierkuchen.\u00ab", "tokens": ["Wo", "ich", "gees\u00b7sen", "Ei\u00b7er\u00b7ku\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVPP", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "\u00bbnu, setz dich hin! Man blo\u00df \u00fcm zu perbieren.\u00ab", "tokens": ["\u00bb", "nu", ",", "setz", "dich", "hin", "!", "Man", "blo\u00df", "\u00fcm", "zu", "per\u00b7bie\u00b7ren", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "PIS", "ADV", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbnu, da du mir doch tust so enfitieren,", "tokens": ["\u00bb", "nu", ",", "da", "du", "mir", "doch", "tust", "so", "en\u00b7fi\u00b7tie\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "So will ich's doch enmol versuchen,", "tokens": ["So", "will", "ich's", "doch", "en\u00b7mol", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "En bischen lutschen blo\u00df.\u00ab", "tokens": ["En", "bi\u00b7schen", "lut\u00b7schen", "blo\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Un Itzig lutscht nu los.", "tokens": ["Un", "It\u00b7zig", "lutscht", "nu", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.23": {"text": "De Spickgaus is ok gor tau saftig.", "tokens": ["De", "Spick\u00b7gaus", "is", "ok", "gor", "tau", "saf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Un in 'ne korte Tid, in'n Hand\u00fcmwenn'n,", "tokens": ["Un", "in", "'ne", "kor\u00b7te", "Tid", ",", "in'n", "Han\u00b7d\u00fcm\u00b7wenn'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Lutscht hei de Spickgaus up von Ur tau En'n.", "tokens": ["Lutscht", "hei", "de", "Spick\u00b7gaus", "up", "von", "Ur", "tau", "En'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "\u00bbdas nennt er lutschen! Nu wohrhaftig!\u00ab", "tokens": ["\u00bb", "das", "nennt", "er", "lut\u00b7schen", "!", "Nu", "wohr\u00b7haf\u00b7tig", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "VVINF", "$.", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Seggt Abraham, \u00bbnach diesem, sog ich dir,", "tokens": ["Seggt", "Ab\u00b7ra\u00b7ham", ",", "\u00bb", "nach", "die\u00b7sem", ",", "sog", "ich", "dir", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "$(", "APPR", "PDAT", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}