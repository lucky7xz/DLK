{"textgrid.poem.61129": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie war mir daheim am nordischen Herde die Freude", "tokens": ["O", "wie", "war", "mir", "da\u00b7heim", "am", "nor\u00b7di\u00b7schen", "Her\u00b7de", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ein willkommener zwar, aber ein seltener Gast!", "tokens": ["Ein", "will\u00b7kom\u00b7me\u00b7ner", "zwar", ",", "a\u00b7ber", "ein", "sel\u00b7te\u00b7ner", "Gast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn bald scheuchte der Nebel sie fort, der grau und verdrie\u00dflich", "tokens": ["Denn", "bald", "scheuch\u00b7te", "der", "Ne\u00b7bel", "sie", "fort", ",", "der", "grau", "und", "ver\u00b7drie\u00df\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "\u00dcber das lachende Tal, \u00fcber die Berge sich zog;", "tokens": ["\u00dc\u00b7ber", "das", "la\u00b7chen\u00b7de", "Tal", ",", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "sich", "zog", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Bald vertrieb sie der l\u00e4rmende Tag und das Dr\u00f6hnen des Marktes,", "tokens": ["Bald", "ver\u00b7trieb", "sie", "der", "l\u00e4r\u00b7men\u00b7de", "Tag", "und", "das", "Dr\u00f6h\u00b7nen", "des", "Mark\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Wo nur jeder sich selbst, keiner den S\u00e4nger vernahm.", "tokens": ["Wo", "nur", "je\u00b7der", "sich", "selbst", ",", "kei\u00b7ner", "den", "S\u00e4n\u00b7ger", "ver\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PRF", "ADV", "$,", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Auch den st\u00f6renden Schwarm der wilden Genossen vermied sie,", "tokens": ["Auch", "den", "st\u00f6\u00b7ren\u00b7den", "Schwarm", "der", "wil\u00b7den", "Ge\u00b7nos\u00b7sen", "ver\u00b7mied", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und sie entfloh dem Gelag, fand sie die Zither verstimmt.", "tokens": ["Und", "sie", "ent\u00b7floh", "dem", "Ge\u00b7lag", ",", "fand", "sie", "die", "Zi\u00b7ther", "ver\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Manchmal nur, wenn im Arm der Geliebten sinnend ich ruhte,", "tokens": ["Manch\u00b7mal", "nur", ",", "wenn", "im", "Arm", "der", "Ge\u00b7lieb\u00b7ten", "sin\u00b7nend", "ich", "ruh\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und ihr leuchtender Blick tief mir den Himmel erschlo\u00df,", "tokens": ["Und", "ihr", "leuch\u00b7ten\u00b7der", "Blick", "tief", "mir", "den", "Him\u00b7mel", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wenn wir in leisem Gespr\u00e4ch der rinnenden Stunden verga\u00dfen,", "tokens": ["Wenn", "wir", "in", "lei\u00b7sem", "Ge\u00b7spr\u00e4ch", "der", "rin\u00b7nen\u00b7den", "Stun\u00b7den", "ver\u00b7ga\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aug' in Auge versenkt, weilte die Liebliche gern.", "tokens": ["Aug'", "in", "Au\u00b7ge", "ver\u00b7senkt", ",", "weil\u00b7te", "die", "Lieb\u00b7li\u00b7che", "gern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Aber auch dann nur kurz. Bald kamen die schwatzenden Muhmen,", "tokens": ["A\u00b7ber", "auch", "dann", "nur", "kurz", ".", "Bald", "ka\u00b7men", "die", "schwat\u00b7zen\u00b7den", "Muh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Vor dem gesch\u00e4ftigen Wort floh das versch\u00fcchterte Kind.", "tokens": ["Vor", "dem", "ge\u00b7sch\u00e4f\u00b7ti\u00b7gen", "Wort", "floh", "das", "ver\u00b7sch\u00fcch\u00b7ter\u00b7te", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Wieder verstrichen darauf eint\u00f6nige Wochen und Monde,", "tokens": ["Wie\u00b7der", "ver\u00b7stri\u00b7chen", "da\u00b7rauf", "ein\u00b7t\u00f6\u00b7ni\u00b7ge", "Wo\u00b7chen", "und", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ADJA", "NN", "KON", "NE", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Und nach der G\u00f6ttlichen Gru\u00df blickte vergebens ich aus.", "tokens": ["Und", "nach", "der", "G\u00f6tt\u00b7li\u00b7chen", "Gru\u00df", "blick\u00b7te", "ver\u00b7ge\u00b7bens", "ich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Gl\u00fccklicher S\u00fcden, wie dank' ich es dir! Du hast die Entwichne", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "S\u00fc\u00b7den", ",", "wie", "dank'", "ich", "es", "dir", "!", "Du", "hast", "die", "Ent\u00b7wich\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PPER", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Neu mir vereint und sie ganz mir zur Vertrauten gemacht.", "tokens": ["Neu", "mir", "ver\u00b7eint", "und", "sie", "ganz", "mir", "zur", "Ver\u00b7trau\u00b7ten", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "KON", "PPER", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Schreit' ich hinaus ins Gebirg', so find' ich sie unter dem Lorbeer", "tokens": ["Schreit'", "ich", "hin\u00b7aus", "ins", "Ge\u00b7bir\u00b7g'", ",", "so", "find'", "ich", "sie", "un\u00b7ter", "dem", "Lor\u00b7beer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Mein schon harrend: sie schl\u00e4ft, sch\u00f6n wie ein M\u00e4dchen, am Quell.", "tokens": ["Mein", "schon", "har\u00b7rend", ":", "sie", "schl\u00e4ft", ",", "sch\u00f6n", "wie", "ein", "M\u00e4d\u00b7chen", ",", "am", "Quell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVPP", "$.", "PPER", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Aber sie h\u00f6rt des Nahenden Tritt, mit wehenden Locken", "tokens": ["A\u00b7ber", "sie", "h\u00f6rt", "des", "Na\u00b7hen\u00b7den", "Tritt", ",", "mit", "we\u00b7hen\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Springt sie empor, und zum Ku\u00df h\u00e4ngt an den Lippen sie mir.", "tokens": ["Springt", "sie", "em\u00b7por", ",", "und", "zum", "Ku\u00df", "h\u00e4ngt", "an", "den", "Lip\u00b7pen", "sie", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "PPER", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "An das Gestade des Meers, zu den heiligen Schatten des \u00d6lwalds", "tokens": ["An", "das", "Ge\u00b7sta\u00b7de", "des", "Meers", ",", "zu", "den", "hei\u00b7li\u00b7gen", "Schat\u00b7ten", "des", "\u00d6l\u00b7walds"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Leitet sie mich; sie besteigt mit mir den schwankenden Kahn;", "tokens": ["Lei\u00b7tet", "sie", "mich", ";", "sie", "be\u00b7steigt", "mit", "mir", "den", "schwan\u00b7ken\u00b7den", "Kahn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.25": {"text": "Leis auch f\u00fchrt sie den Hang mich empor zu den Tr\u00fcmmern des Tempels,", "tokens": ["Leis", "auch", "f\u00fchrt", "sie", "den", "Hang", "mich", "em\u00b7por", "zu", "den", "Tr\u00fcm\u00b7mern", "des", "Tem\u00b7pels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Wo noch das Marmorgesims \u00fcber den S\u00e4ulen ergl\u00e4nzt;", "tokens": ["Wo", "noch", "das", "Mar\u00b7mor\u00b7ge\u00b7sims", "\u00fc\u00b7ber", "den", "S\u00e4u\u00b7len", "er\u00b7gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und sie deutet mir dort die verwitterten Bilder, erg\u00e4nzend", "tokens": ["Und", "sie", "deu\u00b7tet", "mir", "dort", "die", "ver\u00b7wit\u00b7ter\u00b7ten", "Bil\u00b7der", ",", "er\u00b7g\u00e4n\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Mit lebendigem Wort, was die Barbaren zerst\u00f6rt.", "tokens": ["Mit", "le\u00b7ben\u00b7di\u00b7gem", "Wort", ",", "was", "die", "Bar\u00b7ba\u00b7ren", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.29": {"text": "Faunen erblick' ich im bakchischen Tanz und trunkne M\u00e4naden,", "tokens": ["Fau\u00b7nen", "er\u00b7blick'", "ich", "im", "bak\u00b7chi\u00b7schen", "Tanz", "und", "trunk\u00b7ne", "M\u00e4\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Hoch auf dem Panthergespann folgt mit dem Thyrsus der Gott;", "tokens": ["Hoch", "auf", "dem", "Pan\u00b7ther\u00b7ge\u00b7spann", "folgt", "mit", "dem", "Thyr\u00b7sus", "der", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Weiter verliert sich der taumelnde Zug; harmlosere Feste,", "tokens": ["Wei\u00b7ter", "ver\u00b7liert", "sich", "der", "tau\u00b7meln\u00b7de", "Zug", ";", "harm\u00b7lo\u00b7se\u00b7re", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Wie sie Demeter gebeut, zeigt der gebildete Stein;", "tokens": ["Wie", "sie", "De\u00b7me\u00b7ter", "ge\u00b7beut", ",", "zeigt", "der", "ge\u00b7bil\u00b7de\u00b7te", "Stein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Hirten, mit Blumen bekr\u00e4nzt, und Jungfraun f\u00fchren den Reigen,", "tokens": ["Hir\u00b7ten", ",", "mit", "Blu\u00b7men", "be\u00b7kr\u00e4nzt", ",", "und", "Jung\u00b7fraun", "f\u00fch\u00b7ren", "den", "Rei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVPP", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Und im gel\u00e4uterten Ma\u00df hebt sich und senkt sich der Fu\u00df;", "tokens": ["Und", "im", "ge\u00b7l\u00e4u\u00b7ter\u00b7ten", "Ma\u00df", "hebt", "sich", "und", "senkt", "sich", "der", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Sieh, dort st\u00fcrmen auch Rosse heran. Die st\u00e4ubende Rennbahn", "tokens": ["Sieh", ",", "dort", "st\u00fcr\u00b7men", "auch", "Ros\u00b7se", "he\u00b7ran", ".", "Die", "st\u00e4u\u00b7ben\u00b7de", "Renn\u00b7bahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "F\u00fcllt sich mit Wagen, es strebt jeder der Erste zu sein.", "tokens": ["F\u00fcllt", "sich", "mit", "Wa\u00b7gen", ",", "es", "strebt", "je\u00b7der", "der", "Ers\u00b7te", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "PPER", "VVFIN", "PIS", "ART", "ADJA", "PTKZU", "VAINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Lorbeern winken dem Sieger als Preis, doch sch\u00f6ner als Lorbeern", "tokens": ["Lor\u00b7beern", "win\u00b7ken", "dem", "Sie\u00b7ger", "als", "Preis", ",", "doch", "sch\u00f6\u00b7ner", "als", "Lor\u00b7beern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "KOUS", "NN", "$,", "ADV", "ADJD", "KOKOM", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Lohnt ihm des Dichters Gesang, der ihm Unsterblichkeit schenkt.", "tokens": ["Lohnt", "ihm", "des", "Dich\u00b7ters", "Ge\u00b7sang", ",", "der", "ihm", "U\u00b7nsterb\u00b7lich\u00b7keit", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "+--+--+--+---+", "measure": "dactylic.tetra.plus"}, "line.39": {"text": "Also deutet die Himmlische mir die Gebilde der K\u00fcnstler,", "tokens": ["Al\u00b7so", "deu\u00b7tet", "die", "Himm\u00b7li\u00b7sche", "mir", "die", "Ge\u00b7bil\u00b7de", "der", "K\u00fcnst\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.40": {"text": "Und ich erkenne, wie sch\u00f6n einst sie die V\u00f6lker regiert;", "tokens": ["Und", "ich", "er\u00b7ken\u00b7ne", ",", "wie", "sch\u00f6n", "einst", "sie", "die", "V\u00f6l\u00b7ker", "re\u00b7giert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Wie sie mit l\u00e4chelndem Blick die rohen Gewalten gez\u00fcgelt,", "tokens": ["Wie", "sie", "mit", "l\u00e4\u00b7cheln\u00b7dem", "Blick", "die", "ro\u00b7hen", "Ge\u00b7wal\u00b7ten", "ge\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Wie sie die sprossende Kraft stets auf das Gro\u00dfe gelenkt.", "tokens": ["Wie", "sie", "die", "spros\u00b7sen\u00b7de", "Kraft", "stets", "auf", "das", "Gro\u00b7\u00dfe", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "O da wird mir die Seele so weit, unendliche Sehnsucht", "tokens": ["O", "da", "wird", "mir", "die", "See\u00b7le", "so", "weit", ",", "un\u00b7end\u00b7li\u00b7che", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Fa\u00dft mich, mit bebendem Mund sprech' ich ein stilles Gebet:", "tokens": ["Fa\u00dft", "mich", ",", "mit", "be\u00b7ben\u00b7dem", "Mund", "sprech'", "ich", "ein", "stil\u00b7les", "Ge\u00b7bet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.45": {"text": "Weile bei mir, du Sch\u00f6nste von allen den T\u00f6chtern des Himmels,", "tokens": ["Wei\u00b7le", "bei", "mir", ",", "du", "Sch\u00f6ns\u00b7te", "von", "al\u00b7len", "den", "T\u00f6ch\u00b7tern", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "NN", "APPR", "PIAT", "ART", "NN", "ART", "NN", "$,"], "meter": "+----+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.46": {"text": "Mit sanft lenkender Hand f\u00fchre durchs Leben mich hin!", "tokens": ["Mit", "sanft", "len\u00b7ken\u00b7der", "Hand", "f\u00fch\u00b7re", "durchs", "Le\u00b7ben", "mich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.47": {"text": "Zeige bes\u00e4nftigend mir die rechten Bahnen und d\u00e4mpfe", "tokens": ["Zei\u00b7ge", "be\u00b7s\u00e4nf\u00b7ti\u00b7gend", "mir", "die", "rech\u00b7ten", "Bah\u00b7nen", "und", "d\u00e4mp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.48": {"text": "Weise die Glut, und wenn blind einst mich die Leidenschaft fa\u00dft,", "tokens": ["Wei\u00b7se", "die", "Glut", ",", "und", "wenn", "blind", "einst", "mich", "die", "Lei\u00b7den\u00b7schaft", "fa\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "KOUS", "ADJD", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.49": {"text": "O da k\u00fchle das brennende Haupt und kr\u00e4nz' es mit Rosen,", "tokens": ["O", "da", "k\u00fch\u00b7le", "das", "bren\u00b7nen\u00b7de", "Haupt", "und", "kr\u00e4nz'", "es", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.50": {"text": "Bis mich der z\u00f6gernde Gott still zu den Schatten entf\u00fchrt.", "tokens": ["Bis", "mich", "der", "z\u00f6\u00b7gern\u00b7de", "Gott", "still", "zu", "den", "Schat\u00b7ten", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}