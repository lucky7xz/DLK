{"dta.poem.8820": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Schilt mich stolz die Welt, so wei\u00dft du, da\u00df ich von", "tokens": ["Schilt", "mich", "stolz", "die", "Welt", ",", "so", "wei\u00dft", "du", ",", "da\u00df", "ich", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df ich scheu vor dir und sch\u00fcchtern gleich dem Reh,", "tokens": ["Da\u00df", "ich", "scheu", "vor", "dir", "und", "sch\u00fcch\u00b7tern", "gleich", "dem", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Schilt sie wortkarg mich, so wei\u00dft du, da\u00df ich f\u00e4hig", "tokens": ["Schilt", "sie", "wort\u00b7karg", "mich", ",", "so", "wei\u00dft", "du", ",", "da\u00df", "ich", "f\u00e4\u00b7hig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Auch des Sch\u00f6nsten, was die Sprache je vermocht zu", "tokens": ["Auch", "des", "Sch\u00f6ns\u00b7ten", ",", "was", "die", "Spra\u00b7che", "je", "ver\u00b7mocht", "zu"], "token_info": ["word", "word", "word", "punct", "w