{"textgrid.poem.40201": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ich schweigend immer sitze,", "tokens": ["Wa\u00b7rum", "ich", "schwei\u00b7gend", "im\u00b7mer", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wenn unter Euch die Rede kreist?", "tokens": ["wenn", "un\u00b7ter", "Euch", "die", "Re\u00b7de", "kreist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "kaum l\u00e4cheln kann zu euerm Witze,", "tokens": ["kaum", "l\u00e4\u00b7cheln", "kann", "zu", "eu\u00b7erm", "Wit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der doch die Andern mit sich rei\u00dft? \u2013", "tokens": ["der", "doch", "die", "An\u00b7dern", "mit", "sich", "rei\u00dft", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "APPR", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O glaubt mir! schwer genug bedr\u00fcckte", "tokens": ["O", "glaubt", "mir", "!", "schwer", "ge\u00b7nug", "be\u00b7dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich selber dieser stumme Bann,", "tokens": ["mich", "sel\u00b7ber", "die\u00b7ser", "stum\u00b7me", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "eh' mir des R\u00e4tsels Deutung gl\u00fcckte", "tokens": ["eh'", "mir", "des", "R\u00e4t\u00b7sels", "Deu\u00b7tung", "gl\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und meine Ruhe ich gewann.", "tokens": ["und", "mei\u00b7ne", "Ru\u00b7he", "ich", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie bangt' ich, noch ein froher Knabe,", "tokens": ["Wie", "bangt'", "ich", ",", "noch", "ein", "fro\u00b7her", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der ", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "wenn pl\u00f6tzlich mit dem Zauberstabe", "tokens": ["wenn", "pl\u00f6tz\u00b7lich", "mit", "dem", "Zau\u00b7ber\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dies Schweigen meine Zunge band!", "tokens": ["dies", "Schwei\u00b7gen", "mei\u00b7ne", "Zun\u00b7ge", "band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ri\u00df als J\u00fcngling die Genossen", "tokens": ["Wie", "ri\u00df", "als", "J\u00fcng\u00b7ling", "die", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zu toller Laune oft auch ich:", "tokens": ["zu", "tol\u00b7ler", "Lau\u00b7ne", "oft", "auch", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mich zu bet\u00e4uben durch die Possen,", "tokens": ["mich", "zu", "be\u00b7t\u00e4u\u00b7ben", "durch", "die", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wenn diese Schwermut mich beschlich! \u2013", "tokens": ["wenn", "die\u00b7se", "Schwer\u00b7mut", "mich", "be\u00b7schlich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dennoch wollte sich gew\u00f6hnen", "tokens": ["Und", "den\u00b7noch", "woll\u00b7te", "sich", "ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist an Eure Sprache nicht:", "tokens": ["Mein", "Geist", "an", "Eu\u00b7re", "Spra\u00b7che", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "denn Meine Sprache rang nach T\u00f6nen,", "tokens": ["denn", "Mei\u00b7ne", "Spra\u00b7che", "rang", "nach", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und was ich dachte, war Gedicht.", "tokens": ["und", "was", "ich", "dach\u00b7te", ",", "war", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}