{"textgrid.poem.24061": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Fieberlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieses Lebens Jammerthal", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht voll schwarzer Schmerzensrosen,", "tokens": ["Steht", "voll", "schwar\u00b7zer", "Schmer\u00b7zens\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die an grauem Dornenstrauche,", "tokens": ["Die", "an", "grau\u00b7em", "Dor\u00b7nen\u00b7strau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen scharfgezackten dunkel-", "tokens": ["Zwi\u00b7schen", "scharf\u00b7ge\u00b7zack\u00b7ten", "dun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fcnen Bl\u00e4ttern bl\u00fchn.", "tokens": ["Gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7tern", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gro\u00dfe, schwarze Schmerzensrosen", "tokens": ["Gro\u00b7\u00dfe", ",", "schwar\u00b7ze", "Schmer\u00b7zens\u00b7ro\u00b7sen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicken \u00fcber meinem Haupte", "tokens": ["Ni\u00b7cken", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und entsch\u00fctten ihrem Schoo\u00dfe", "tokens": ["Und", "ent\u00b7sch\u00fct\u00b7ten", "ih\u00b7rem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Giftig gelben Samenstaub.", "tokens": ["Gif\u00b7tig", "gel\u00b7ben", "Sa\u00b7men\u00b7staub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dicker, dumpfer Duft umschwillt mich", "tokens": ["Di\u00b7cker", ",", "dum\u00b7pfer", "Duft", "um\u00b7schwillt", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sichtbarlich in sammetblauer", "tokens": ["Sicht\u00b7bar\u00b7lich", "in", "sam\u00b7met\u00b7blau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Schw\u00fcler, feuchter Wetterwolke,", "tokens": ["Schw\u00fc\u00b7ler", ",", "feuch\u00b7ter", "Wet\u00b7ter\u00b7wol\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und von ferne h\u00f6r ich Geigen.", "tokens": ["Und", "von", "fer\u00b7ne", "h\u00f6r", "ich", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Geigen h\u00f6r ich ein wildes Lied.", "tokens": ["Gei\u00b7gen", "h\u00f6r", "ich", "ein", "wil\u00b7des", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Schmerzensschrill und voller Wollust,", "tokens": ["Schmer\u00b7zens\u00b7schrill", "und", "vol\u00b7ler", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Voller Gier und greller Helle,", "tokens": ["Vol\u00b7ler", "Gier", "und", "grel\u00b7ler", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und im Takte meines Herzschlags,", "tokens": ["Und", "im", "Tak\u00b7te", "mei\u00b7nes", "Herz\u00b7schlags", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sto\u00dfweis wechselnd, klingt das Lied.", "tokens": ["Sto\u00df\u00b7weis", "wech\u00b7selnd", ",", "klingt", "das", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Lullt mich ein zu Schlaf und schreckt mich", "tokens": ["Lullt", "mich", "ein", "zu", "Schlaf", "und", "schreckt", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "APPR", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "In ein atemloses Wachen,", "tokens": ["In", "ein", "a\u00b7tem\u00b7lo\u00b7ses", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dr\u00fcckt die Lider mir wie Bleilast,", "tokens": ["Dr\u00fcckt", "die", "Li\u00b7der", "mir", "wie", "Blei\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Rei\u00dft mein rot entz\u00fcndet Auge", "tokens": ["Rei\u00dft", "mein", "rot", "ent\u00b7z\u00fcn\u00b7det", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJD", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Auf in eine blutige Sonne \u2013", "tokens": ["Auf", "in", "ei\u00b7ne", "blu\u00b7ti\u00b7ge", "Son\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Und die schwarzen Schmerzensrosen", "tokens": ["Und", "die", "schwar\u00b7zen", "Schmer\u00b7zens\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Nicken \u00fcber mir ...", "tokens": ["Ni\u00b7cken", "\u00fc\u00b7ber", "mir", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}