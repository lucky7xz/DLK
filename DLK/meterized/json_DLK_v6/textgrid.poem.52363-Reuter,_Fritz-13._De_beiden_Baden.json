{"textgrid.poem.52363": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "13. De beiden Baden", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "As de Franzosen bi uns wiren,", "tokens": ["As", "de", "Fran\u00b7zo\u00b7sen", "bi", "uns", "wi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dunn m\u00fc\u00dft't in Tet'row mal passieren,", "tokens": ["Dunn", "m\u00fc\u00dft't", "in", "Tet'\u00b7row", "mal", "pas\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dat de wohll\u00f6blich Magistrat", "tokens": ["Dat", "de", "wohl\u00b7l\u00f6b\u00b7lich", "Ma\u00b7gist\u00b7rat"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "En Breif wull schicken nah Malchin", "tokens": ["En", "Breif", "wull", "schi\u00b7cken", "nah", "Mal\u00b7chin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "VVINF", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Hofrat L\u00fcdersen; un Bad'", "tokens": ["An", "Hof\u00b7rat", "L\u00fc\u00b7der\u00b7sen", ";", "un", "Bad'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "NN", "$.", "FM", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Ded in gew\u00f6hnlichen F\u00e4llen", "tokens": ["Ded", "in", "ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Oll Schauster Jehann Haken sin.", "tokens": ["Oll", "Schaus\u00b7ter", "Je\u00b7hann", "Ha\u00b7ken", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "De Kniper sall den Schauster nu bestellen.", "tokens": ["De", "Kni\u00b7per", "sall", "den", "Schaus\u00b7ter", "nu", "be\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "\u00bbje\u00ab, seggt de Fru, \u00bbhei is nich in,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "de", "Fru", ",", "\u00bb", "hei", "is", "nich", "in", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "NE", "NN", "$,", "$(", "FM", "FM", "PTKNEG", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ward woll en beten nahwern sin,", "tokens": ["Ward", "woll", "en", "be\u00b7ten", "nah\u00b7wern", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch k\u00fcmmt hei s\u00e4ker mitdewil.\u00ab", "tokens": ["Doch", "k\u00fcmmt", "hei", "s\u00e4\u00b7ker", "mit\u00b7de\u00b7wil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbje\u00ab, seggt de Kniper, \u00bbdit hett Il,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "de", "Kni\u00b7per", ",", "\u00bb", "dit", "hett", "Il", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "NE", "NN", "$,", "$(", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hei m\u00f6t sick spauden un sall denn", "tokens": ["Hei", "m\u00f6t", "sick", "spau\u00b7den", "un", "sall", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "KON"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Glik nah Malchin nah Hofrat L\u00fcdersen\u00ab,", "tokens": ["Glik", "nah", "Mal\u00b7chin", "nah", "Hof\u00b7rat", "L\u00fc\u00b7der\u00b7sen", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "NN", "ADJD", "VVFIN", "NN", "$(", "$,"], "meter": "+-+--+----", "measure": "trochaic.tri.relaxed"}, "line.15": {"text": "Un geiht un ward en annern halen.", "tokens": ["Un", "geiht", "un", "ward", "en", "an\u00b7nern", "ha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Oll Haken k\u00fcmmt tau Hus; sin Fru, de seggt:", "tokens": ["Oll", "Ha\u00b7ken", "k\u00fcmmt", "tau", "Hus", ";", "sin", "Fru", ",", "de", "seggt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "NE", "$.", "PPOSAT", "NN", "$,", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbnu mak di fix man up de Salen,", "tokens": ["\u00bb", "nu", "mak", "di", "fix", "man", "up", "de", "Sa\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "De Kniper, de was eben hir,", "tokens": ["De", "Kni\u00b7per", ",", "de", "was", "e\u00b7ben", "hir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "PWS", "ADV", "ADV", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Sallst nah Malchin, nah Hofrat L\u00fcdersen.\u00ab", "tokens": ["Sallst", "nah", "Mal\u00b7chin", ",", "nah", "Hof\u00b7rat", "L\u00fc\u00b7der\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "ADJD", "VVFIN", "NN", "$.", "$("], "meter": "+-+--+----", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "\u00bbna, hett hei keinen Breif nich br\u00f6cht?\u00ab", "tokens": ["\u00bb", "na", ",", "hett", "hei", "kei\u00b7nen", "Breif", "nich", "br\u00f6cht", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "ADJD", "PIAT", "NN", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbne, keinen Breif un kein Poppier;", "tokens": ["\u00bb", "ne", ",", "kei\u00b7nen", "Breif", "un", "kein", "Pop\u00b7pier", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PIAT", "NN", "FM", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hei s\u00e4d man blot, du s\u00fcllst dor hen.\u00ab", "tokens": ["Hei", "s\u00e4d", "man", "blot", ",", "du", "s\u00fcllst", "dor", "hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "PPER", "VVFIN", "ADV", "VVINF", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "\u00bbha, ha!\u00ab seggt hei, \u00bbick weit Bescheid;", "tokens": ["\u00bb", "ha", ",", "ha", "!", "\u00ab", "seggt", "hei", ",", "\u00bb", "ick", "weit", "Be\u00b7scheid", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ITJ", "$.", "$(", "VVFIN", "PTKVZ", "$,", "$(", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dit's wedder so'ne Heimlichkeit.", "tokens": ["Dit's", "wed\u00b7der", "so'\u00b7ne", "Heim\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "De Magistrat, de trut sick nich", "tokens": ["De", "Ma\u00b7gist\u00b7rat", ",", "de", "trut", "sick", "nich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ut Furcht v\u00f6r dat Franzosent\u00fcg,", "tokens": ["Ut", "Furcht", "v\u00f6r", "dat", "Fran\u00b7zo\u00b7sen\u00b7t\u00fcg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wat Schriftlichs von sick mi tau gewen.\u00ab", "tokens": ["Wat", "Schrift\u00b7lichs", "von", "sick", "mi", "tau", "ge\u00b7wen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Un nimmt den Haut un Stock un geiht.", "tokens": ["Un", "nimmt", "den", "Haut", "un", "Stock", "un", "geiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Oll Hofrat L\u00fcders, de sitt eben", "tokens": ["Oll", "Hof\u00b7rat", "L\u00fc\u00b7ders", ",", "de", "sitt", "e\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "$,", "NE", "VVFIN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit drei franz\u00f6sche Offezier", "tokens": ["Mit", "drei", "fran\u00b7z\u00f6\u00b7sche", "Of\u00b7fe\u00b7zier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un ett, dunn k\u00fcmmt oll Haken rin.", "tokens": ["Un", "ett", ",", "dunn", "k\u00fcmmt", "oll", "Ha\u00b7ken", "rin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADV", "VVFIN", "ADJD", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbgu'n Dag ok, Herr! \u2013 Ick b\u00fcn nu hir.\u00ab", "tokens": ["\u00bb", "gu'n", "Dag", "ok", ",", "Herr", "!", "\u2013", "Ick", "b\u00fcn", "nu", "hir", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$,", "NN", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbna\u00ab, seggt de Hofrat, \u00bbdat's mi leiw,", "tokens": ["\u00bb", "na", "\u00ab", ",", "seggt", "de", "Hof\u00b7rat", ",", "\u00bb", "dat's", "mi", "leiw", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "$,", "FM.la", "FM.la", "FM.la", "$,", "$(", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn gewen S' mi man Ehren Breif.\u00ab", "tokens": ["Denn", "ge\u00b7wen", "S'", "mi", "man", "Eh\u00b7ren", "Breif", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "NN", "NE", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "\u00bbje\u00ab, seggt de Oll un kikt verlegen,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "de", "Oll", "un", "kikt", "ver\u00b7le\u00b7gen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "FM", "FM", "FM", "FM", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bben Schriwen, Herr, heww ick nich kregen.\u00ab", "tokens": ["\u00bb", "en", "Schri\u00b7wen", ",", "Herr", ",", "heww", "ick", "nich", "kre\u00b7gen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Oll Hofrat L\u00fcders was en pfiff'gen Racker.", "tokens": ["Oll", "Hof\u00b7rat", "L\u00fc\u00b7ders", "was", "en", "pfiff'\u00b7gen", "Ra\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Entweder, denkt hei, he