{"textgrid.poem.31091": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "205.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann S. Andreas-Abend k\u00fcmt, pflegt ieder, der sich wil beweiben,", "tokens": ["Wann", "S.", "An\u00b7dre\u00b7as\u00b7A\u00b7bend", "k\u00fcmt", ",", "pflegt", "ie\u00b7der", ",", "der", "sich", "wil", "be\u00b7wei\u00b7ben", ","], "token_info": ["word", "abbreviation", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NE", "VVFIN", "$,", "VVFIN", "PIS", "$,", "PRELS", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Auch die, die sich bemannen wil, ein hitziges Gebet zu treiben.", "tokens": ["Auch", "die", ",", "die", "sich", "be\u00b7man\u00b7nen", "wil", ",", "ein", "hit\u00b7zi\u00b7ges", "Ge\u00b7bet", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Andreas, der sich nennt vom Manne, kan Weibern, glaub ich, rathen wol,", "tokens": ["A\u00b7ndre\u00b7as", ",", "der", "sich", "nennt", "vom", "Man\u00b7ne", ",", "kan", "Wei\u00b7bern", ",", "glaub", "ich", ",", "ra\u00b7then", "wol", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "$,", "VMFIN", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Wei\u00df aber nicht, wie seines gleichen, als wie ein Weib, er rathen soll.", "tokens": ["Wei\u00df", "a\u00b7ber", "nicht", ",", "wie", "sei\u00b7nes", "glei\u00b7chen", ",", "als", "wie", "ein", "Weib", ",", "er", "ra\u00b7then", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PPOSAT", "ADJA", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}