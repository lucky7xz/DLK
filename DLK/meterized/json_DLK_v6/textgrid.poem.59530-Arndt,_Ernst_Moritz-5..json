{"textgrid.poem.59530": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter Blumen spielen", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "spie\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gern die kleinen Kinder,", "tokens": ["Gern", "die", "klei\u00b7nen", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blumen sind s\u00fc\u00df und sch\u00f6n.", "tokens": ["Blu\u00b7men", "sind", "s\u00fc\u00df", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wie den Sonnenkindlein,", "tokens": ["Wie", "den", "Son\u00b7nen\u00b7kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie den bunten Blumen,", "tokens": ["Wie", "den", "bun\u00b7ten", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Soll den Kindern das Herzchen stehn.", "tokens": ["Soll", "den", "Kin\u00b7dern", "das", "Herz\u00b7chen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Denn die Blumen heben", "tokens": ["Denn", "die", "Blu\u00b7men", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gern die Liebes\u00e4uglein", "tokens": ["Gern", "die", "Lie\u00b7be\u00b7s\u00e4ug\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Liebend zum Lichte auf;", "tokens": ["Lie\u00b7bend", "zum", "Lich\u00b7te", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wann die Sonne sinket,", "tokens": ["Wann", "die", "Son\u00b7ne", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sinken sie in Schlummer,", "tokens": ["Sin\u00b7ken", "sie", "in", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Stehn zugleich mit der Sonne auf.", "tokens": ["Stehn", "zu\u00b7gleich", "mit", "der", "Son\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wi\u00dft ihr, kleine Kinder,", "tokens": ["Wi\u00dft", "ihr", ",", "klei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Droben hoch auf Sternen", "tokens": ["Dro\u00b7ben", "hoch", "auf", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fchen viel tausendmal", "tokens": ["Bl\u00fc\u00b7hen", "viel", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Tausend bunte Blumen,", "tokens": ["Tau\u00b7send", "bun\u00b7te", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und die Englein winden", "tokens": ["Und", "die", "En\u00b7glein", "win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kr\u00e4nze daraus im Himmelssaal.", "tokens": ["Kr\u00e4n\u00b7ze", "da\u00b7raus", "im", "Him\u00b7mels\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wann die Kinder schlafen,", "tokens": ["Wann", "die", "Kin\u00b7der", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00e4ngen sie die Kr\u00e4nze", "tokens": ["H\u00e4n\u00b7gen", "sie", "die", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihnen am Bettchen auf,", "tokens": ["Ih\u00b7nen", "am", "Bett\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und in goldnen Tr\u00e4umen", "tokens": ["Und", "in", "gold\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Tut der ganze Himmel", "tokens": ["Tut", "der", "gan\u00b7ze", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sich mit Sternen und Blumen auf.", "tokens": ["Sich", "mit", "Ster\u00b7nen", "und", "Blu\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}