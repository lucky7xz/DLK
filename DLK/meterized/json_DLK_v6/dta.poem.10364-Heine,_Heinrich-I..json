{"dta.poem.10364": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich tanz' nicht mit, ich r\u00e4uchre nicht den Kl\u00f6tzen,", "tokens": ["Ich", "tanz'", "nicht", "mit", ",", "ich", "r\u00e4uch\u00b7re", "nicht", "den", "Kl\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die au\u00dfen goldig sind, inwendig Sand,", "tokens": ["Die", "au\u00b7\u00dfen", "gol\u00b7dig", "sind", ",", "in\u00b7wen\u00b7dig", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schlag' nicht ein, reicht mir ein Bub die Hand,", "tokens": ["Ich", "schlag'", "nicht", "ein", ",", "reicht", "mir", "ein", "Bub", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der heimlich mir den Namen will zerfetzen.", "tokens": ["Der", "heim\u00b7lich", "mir", "den", "Na\u00b7men", "will", "zer\u00b7fet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich beug' mich nicht vor jenen h\u00fcbschen Metzen,", "tokens": ["Ich", "beug'", "mich", "nicht", "vor", "je\u00b7nen", "h\u00fcb\u00b7schen", "Met\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die schamlos prunken mit der eignen Schand,", "tokens": ["Die", "scham\u00b7los", "prun\u00b7ken", "mit", "der", "eig\u00b7nen", "Schand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich zieh' nicht mit, wenn sich der P\u00f6bel spannt", "tokens": ["Ich", "zieh'", "nicht", "mit", ",", "wenn", "sich", "der", "P\u00f6\u00b7bel", "spannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vor'n Siegeswagen seiner eiteln G\u00f6tzen.", "tokens": ["Vor'n", "Sie\u00b7ges\u00b7wa\u00b7gen", "sei\u00b7ner", "ei\u00b7teln", "G\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich wei\u00df es wohl, die Eiche mu\u00df erliegen,", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "die", "Ei\u00b7che", "mu\u00df", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Derweil das Rohr am Bach, durch schwankes Biegen,", "tokens": ["Der\u00b7weil", "das", "Rohr", "am", "Bach", ",", "durch", "schwan\u00b7kes", "Bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In Wind und Wetter stehn bleibt, nach wie vor.", "tokens": ["In", "Wind", "und", "Wet\u00b7ter", "stehn", "bleibt", ",", "nach", "wie", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VVFIN", "$,", "APPR", "KOKOM", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch sprich, wie weit bringt's wohl am End' solch Rohr?", "tokens": ["Doch", "sprich", ",", "wie", "weit", "bringt's", "wohl", "am", "End'", "solch", "Rohr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADJD", "VVFIN", "ADV", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Welch Gl\u00fcck! als ein Spazierstock dient's dem Stutzer,", "tokens": ["Welch", "Gl\u00fcck", "!", "als", "ein", "Spa\u00b7zier\u00b7stock", "dient's", "dem", "Stut\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Als Kleiderklopfer dient's dem Stiefelputzer.", "tokens": ["Als", "Klei\u00b7der\u00b7klop\u00b7fer", "dient's", "dem", "Stie\u00b7fel\u00b7put\u00b7zer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}