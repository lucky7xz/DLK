{"dta.poem.15056": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man liset von zwain menschen das", "tokens": ["Man", "li\u00b7set", "von", "zwain", "men\u00b7schen", "das"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ir hertz mit Minne verstriket was.", "tokens": ["Ir", "hertz", "mit", "Min\u00b7ne", "ver\u00b7stri\u00b7ket", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das was ain man und ouch sin wib,", "tokens": ["Das", "was", "ain", "man", "und", "ouch", "sin", "wib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PIS", "KON", "FM", "FM", "FM", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die hatt er lieb als sinen Lib.", "tokens": ["Die", "hatt", "er", "lieb", "als", "si\u00b7nen", "Lib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die starke minne schied der tod.", "tokens": ["Die", "star\u00b7ke", "min\u00b7ne", "schied", "der", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der man starb do kam in not", "tokens": ["Der", "man", "starb", "do", "kam", "in", "not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Frau und ouch die minne gro\u00df;", "tokens": ["Die", "Frau", "und", "ouch", "die", "min\u00b7ne", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Alles Trostes was si blos.", "tokens": ["Al\u00b7les", "Tros\u00b7tes", "was", "si", "blos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PWS", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Do si verlor ir lieben Man.", "tokens": ["Do", "si", "ver\u00b7lor", "ir", "lie\u00b7ben", "Man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Si wolt von niemand trost enpfan.", "tokens": ["Si", "wolt", "von", "nie\u00b7mand", "trost", "en\u00b7pfan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sy schray und wainet &#226;n underlas.", "tokens": ["Sy", "schray", "und", "wai\u00b7net", "&#226;", "n", "un\u00b7der\u00b7las", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "VVFIN", "$(", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ob im sy staetenklichen sa\u00df.", "tokens": ["Ob", "im", "sy", "stae\u00b7ten\u00b7kli\u00b7chen", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NE", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Do er ward in das Grab gelait,", "tokens": ["Do", "er", "ward", "in", "das", "Grab", "ge\u00b7lait", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "Do huob sich jammer und laid.", "tokens": ["Do", "huob", "sich", "jam\u00b7mer", "und", "laid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Sy wolt nit von dem Grab hindan,", "tokens": ["Sy", "wolt", "nit", "von", "dem", "Grab", "hin\u00b7dan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Sy clagt als iren lieben man.", "tokens": ["Sy", "clagt", "als", "i\u00b7ren", "lie\u00b7ben", "man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "VVINF", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sy schrai vil lut ach und we.", "tokens": ["Sy", "schrai", "vil", "lut", "ach", "und", "we", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "XY", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Weder regen und der schne", "tokens": ["We\u00b7der", "re\u00b7gen", "und", "der", "schne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "KON", "ART", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mocht sy geschaiden von dem grab", "tokens": ["Mocht", "sy", "ge\u00b7schai\u00b7den", "von", "dem", "grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sy lebt in grosser Vngehab", "tokens": ["Sy", "lebt", "in", "gros\u00b7ser", "Vn\u00b7ge\u00b7hab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bedi Nacht und den Tag,", "tokens": ["Be\u00b7di", "Nacht", "und", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Das si da anderst n&#250;t en pflag.", "tokens": ["Das", "si", "da", "an\u00b7derst", "n", "&#250;", "t", "en", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "------+-+", "measure": "unknown.measure.di"}, "line.23": {"text": "Ir Ruowe die was clain.", "tokens": ["Ir", "Ruo\u00b7we", "die", "was", "clain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "By dem grab sa\u00df sy allein,", "tokens": ["By", "dem", "grab", "sa\u00df", "sy", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Vnd wainet by dem F&#250;re.", "tokens": ["Vnd", "wai\u00b7net", "by", "dem", "F", "&#250;", "re", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "VVFIN", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Kurzwil was ir t&#250;re.", "tokens": ["Kurz\u00b7wil", "was", "ir", "t", "&#250;", "re", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.27": {"text": "Nu fuegt sich uf derselben vart", "tokens": ["Nu", "fuegt", "sich", "uf", "der\u00b7sel\u00b7ben", "vart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Das da ainer erhenket wart", "tokens": ["Das", "da", "ai\u00b7ner", "er\u00b7hen\u00b7ket", "wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Hin von dem grab &#250;ber ain veld,", "tokens": ["Hin", "von", "dem", "grab", "&#250;", "ber", "ain", "veld", ","], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.30": {"text": "Des huot ain man dem gab man gelt", "tokens": ["Des", "huot", "ain", "man", "dem", "gab", "man", "gelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "ART", "VVFIN", "PIS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Das er nit dannen solt kommen,", "tokens": ["Das", "er", "nit", "dan\u00b7nen", "solt", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.32": {"text": "Wurd ab dem Galgen abgenommen", "tokens": ["Wurd", "ab", "dem", "Gal\u00b7gen", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der tieb, das waer dem richter Zorn", "tokens": ["Der", "tieb", ",", "das", "waer", "dem", "rich\u00b7ter", "Zorn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "PRELS", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und muest er sin houpt han verlorn.", "tokens": ["Und", "mu\u00b7est", "er", "sin", "houpt", "han", "ver\u00b7lorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Do er das f&#250;r sach und das Wib", "tokens": ["Do", "er", "das", "f", "&#250;", "r", "sach", "und", "das", "Wib"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "ADV", "ADJD", "KON", "ART", "NN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Clagen hort ir mannes lib", "tokens": ["Cla\u00b7gen", "hort", "ir", "man\u00b7nes", "lib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Vil ser in t&#250;rsten began.", "tokens": ["Vil", "ser", "in", "t", "&#250;", "rs\u00b7ten", "be\u00b7gan", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Zu dem grab gieng er hindan", "tokens": ["Zu", "dem", "grab", "gieng", "er", "hin\u00b7dan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und sach die Frowen die was stoltz.", "tokens": ["Und", "sach", "die", "Fro\u00b7wen", "die", "was", "stoltz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "PWS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "An das f&#250;r bracht er ir holtz", "tokens": ["An", "das", "f", "&#250;", "r", "bracht", "er", "ir", "holtz"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "NE", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Das si vor frost wart behut.", "tokens": ["Das", "si", "vor", "frost", "wart", "be\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Er sprach Frow hand guten mut,", "tokens": ["Er", "sprach", "Frow", "hand", "gu\u00b7ten", "mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "Sid tod ist &#250;wer lieber man", "tokens": ["Sid", "tod", "ist", "&#250;", "wer", "lie\u00b7ber", "man"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "$(", "PWS", "ADV", "PIS"], "meter": "+-+-+-+", "measu