{"textgrid.poem.35843": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lerche hoch in L\u00fcften", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche hoch in L\u00fcften", "tokens": ["Die", "Ler\u00b7che", "hoch", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Preist Dich mit s\u00fc\u00dfer Stimme Klang;", "tokens": ["Preist", "Dich", "mit", "s\u00fc\u00b7\u00dfer", "Stim\u00b7me", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Veilchen lobt mit D\u00fcften", "tokens": ["Das", "Veil\u00b7chen", "lobt", "mit", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich still sein ganzes Leben lang.", "tokens": ["Dich", "still", "sein", "gan\u00b7zes", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Meer geht hoch in Wogen", "tokens": ["Das", "Meer", "geht", "hoch", "in", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und jauchzt Dir sein gewaltig Lied;", "tokens": ["Und", "jauchzt", "Dir", "sein", "ge\u00b7wal\u00b7tig", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein siebenfarbner Bogen", "tokens": ["Dein", "sie\u00b7ben\u00b7farb\u00b7ner", "Bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drob friedestrahlend niedersieht.", "tokens": ["Drob", "frie\u00b7des\u00b7trah\u00b7lend", "nie\u00b7der\u00b7sieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es fliegt die kleine Imme", "tokens": ["Es", "fliegt", "die", "klei\u00b7ne", "Im\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sammelt Wachs und s\u00fc\u00dfen Seim,", "tokens": ["Und", "sam\u00b7melt", "Wachs", "und", "s\u00fc\u00b7\u00dfen", "Seim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihres Summens Stimme", "tokens": ["Und", "ih\u00b7res", "Sum\u00b7mens", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Preist Dich, tr\u00e4gt sie die B\u00fcrde heim.", "tokens": ["Preist", "Dich", ",", "tr\u00e4gt", "sie", "die", "B\u00fcr\u00b7de", "heim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Die Sonne sendet Gluthen", "tokens": ["Die", "Son\u00b7ne", "sen\u00b7det", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lobet Dich in Flammenpracht;", "tokens": ["Und", "lo\u00b7bet", "Dich", "in", "Flam\u00b7men\u00b7pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Fischlein in den Fluthen", "tokens": ["Das", "Fisc\u00b7hlein", "in", "den", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Preist schwimmend, schwebend Deine Macht.", "tokens": ["Preist", "schwim\u00b7mend", ",", "schwe\u00b7bend", "Dei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir zirpt die kleine Grille", "tokens": ["Dir", "zirpt", "die", "klei\u00b7ne", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den immer gleichen leisen Klang;", "tokens": ["Den", "im\u00b7mer", "glei\u00b7chen", "lei\u00b7sen", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch meiner Kammer Stille", "tokens": ["Durch", "mei\u00b7ner", "Kam\u00b7mer", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwirrt froh ihr heis'rer Abendsang.", "tokens": ["Schwirrt", "froh", "ihr", "heis'\u00b7rer", "A\u00b7bend\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dich preist der St\u00fcrme Sausen,", "tokens": ["Dich", "preist", "der", "St\u00fcr\u00b7me", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Wetters Strahl, des Donners Graus;", "tokens": ["Des", "Wet\u00b7ters", "Strahl", ",", "des", "Don\u00b7ners", "Graus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir streckt aus enger Klausen", "tokens": ["Dir", "streckt", "aus", "en\u00b7ger", "Klau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Schnecklein froh die H\u00f6rner aus.", "tokens": ["Das", "Schnec\u00b7klein", "froh", "die", "H\u00f6r\u00b7ner", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des Mondes mildes Schimmern,", "tokens": ["Des", "Mon\u00b7des", "mil\u00b7des", "Schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Schnee'es wunderklares Wei\u00df,", "tokens": ["Des", "Schnee'\u00b7es", "wun\u00b7der\u00b7kla\u00b7res", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sternlein zuckend Flimmern:", "tokens": ["Der", "Stern\u00b7lein", "zu\u00b7ckend", "Flim\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schimmern, flimmern Dir zum Preis.", "tokens": ["Sie", "schim\u00b7mern", ",", "flim\u00b7mern", "Dir", "zum", "Preis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der B\u00e4ume gr\u00fcne Kronen,", "tokens": ["Der", "B\u00e4u\u00b7me", "gr\u00fc\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie strecken sich nach Dir, nach Dir,", "tokens": ["Sie", "stre\u00b7cken", "sich", "nach", "Dir", ",", "nach", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Nachtigallen wohnen", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len", "woh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihrer Hut und singen mir.", "tokens": ["In", "ih\u00b7rer", "Hut", "und", "sin\u00b7gen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es lehrt wohl s\u00fc\u00df're Weisen", "tokens": ["Es", "lehrt", "wohl", "s\u00fc\u00df'\u00b7re", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich ihrer reinen Stimme Schall.", "tokens": ["Mich", "ih\u00b7rer", "rei\u00b7nen", "Stim\u00b7me", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O lernt' auch ich Dich preisen,", "tokens": ["O", "lernt'", "auch", "ich", "Dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Herr, trotz Lerch' und Nachtigall!", "tokens": ["O", "Herr", ",", "trotz", "Ler\u00b7ch'", "und", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}