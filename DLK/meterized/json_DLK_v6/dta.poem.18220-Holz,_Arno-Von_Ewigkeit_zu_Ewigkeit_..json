{"dta.poem.18220": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Von Ewigkeit zu Ewigkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Entfacht noch st\u00fcndlich den Prometheusfunken", "tokens": ["Ent\u00b7facht", "noch", "st\u00fcnd\u00b7lich", "den", "Pro\u00b7me\u00b7theus\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch ist ihre goldne Bl\u00fcthezeit", "tokens": ["Und", "doch", "ist", "ih\u00b7re", "gold\u00b7ne", "Bl\u00fc\u00b7the\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon l\u00e4ngst ins Grab der Ewigkeit gesunken.", "tokens": ["Schon", "l\u00e4ngst", "ins", "Grab", "der", "E\u00b7wig\u00b7keit", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn jene Welt der Sagenpoesie", "tokens": ["Denn", "je\u00b7ne", "Welt", "der", "Sa\u00b7gen\u00b7po\u00b7e\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist nicht nur Traum, ist Wirklichkeit gewesen,", "tokens": ["Ist", "nicht", "nur", "Traum", ",", "ist", "Wirk\u00b7lich\u00b7keit", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "NN", "$,", "VAFIN", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wem das Schicksal Seherkraft verlieh,", "tokens": ["Und", "wem", "das", "Schick\u00b7sal", "Se\u00b7her\u00b7kraft", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kann das noch heute aus den Sternen lesen.", "tokens": ["Kann", "das", "noch", "heu\u00b7te", "aus", "den", "Ster\u00b7nen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer z\u00e4hlt die Sprossen, die zertr\u00fcmmert sind", "tokens": ["Wer", "z\u00e4hlt", "die", "Spros\u00b7sen", ",", "die", "zer\u00b7tr\u00fcm\u00b7mert", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus jener gotterbauten Himmelsleiter?", "tokens": ["Aus", "je\u00b7ner", "got\u00b7ter\u00b7bau\u00b7ten", "Him\u00b7mels\u00b7lei\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne gl\u00fcht und k\u00fchlend weht der Wind", "tokens": ["Die", "Son\u00b7ne", "gl\u00fcht", "und", "k\u00fch\u00b7lend", "weht", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und unaufhaltsam rollt das Rad sich weiter.", "tokens": ["Und", "un\u00b7auf\u00b7halt\u00b7sam", "rollt", "das", "Rad", "sich", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die leuchtend kreisen durch das dunkle All,", "tokens": ["Die", "leuch\u00b7tend", "krei\u00b7sen", "durch", "das", "dunk\u00b7le", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erhaben gro\u00df ist noch die Zahl der Welten;", "tokens": ["Er\u00b7ha\u00b7ben", "gro\u00df", "ist", "noch", "die", "Zahl", "der", "Wel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und kommt alln\u00e4chtlich eine auch zum Fall,", "tokens": ["Und", "kommt", "all\u00b7n\u00e4cht\u00b7lich", "ei\u00b7ne", "auch", "zum", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was kann dem Meere wohl ein Tropfen gelten?", "tokens": ["Was", "kann", "dem", "Mee\u00b7re", "wohl", "ein", "Trop\u00b7fen", "gel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wem sich das Geheimni\u00df der Natur", "tokens": ["Doch", "wem", "sich", "das", "Ge\u00b7heim\u00b7ni\u00df", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht unterm Sternenzelt mag offenbaren,", "tokens": ["Nicht", "un\u00b7term", "Ster\u00b7nen\u00b7zelt", "mag", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der wandle mit mir durch die Erdenflur,", "tokens": ["Der", "wand\u00b7le", "mit", "mir", "durch", "die", "Er\u00b7den\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wie sie war vor hunderttausend Jahren.", "tokens": ["So", "wie", "sie", "war", "vor", "hun\u00b7dert\u00b7tau\u00b7send", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch stritt kein Jason um das goldne Vlie\u00df,", "tokens": ["Noch", "stritt", "kein", "Ja\u00b7son", "um", "das", "gold\u00b7ne", "Vlie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die Menschheit knechtete kein Triumphator,", "tokens": ["Die", "Menschheit", "knech\u00b7te\u00b7te", "kein", "Tri\u00b7um\u00b7pha\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Doch endlos dehnte sich ein Paradies", "tokens": ["Doch", "end\u00b7los", "dehn\u00b7te", "sich", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vom Nordpol bis hinunter zum Aequator.", "tokens": ["Vom", "Nord\u00b7pol", "bis", "hin\u00b7un\u00b7ter", "zum", "A\u00b7e\u00b7qua\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+---", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo heute sich durch eisumstarrten Belt", "tokens": ["Wo", "heu\u00b7te", "sich", "durch", "ei\u00b7sum\u00b7starr\u00b7ten", "Belt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Walfischfahrer ihre Stra\u00dfe bahnen,", "tokens": ["Die", "Wal\u00b7fischfah\u00b7rer", "ih\u00b7re", "Stra\u00b7\u00dfe", "bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erhub sich ehmals eine Inselwelt,", "tokens": ["Er\u00b7hub", "sich", "eh\u00b7mals", "ei\u00b7ne", "In\u00b7sel\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bebl\u00fcht von \u00fcppig wuchernden Bananen.", "tokens": ["Be\u00b7bl\u00fcht", "von", "\u00fcp\u00b7pig", "wu\u00b7chern\u00b7den", "Ba\u00b7na\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und l\u00e4chelnd kr\u00e4nzte sich die Meeresfee", "tokens": ["Und", "l\u00e4\u00b7chelnd", "kr\u00e4nz\u00b7te", "sich", "die", "Mee\u00b7res\u00b7fee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit bunten Perlenmuscheln und Korallen,", "tokens": ["Mit", "bun\u00b7ten", "Per\u00b7len\u00b7mu\u00b7scheln", "und", "Ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo l\u00e4ngst verweht vom W\u00fcstenk\u00f6rnerschnee", "tokens": ["Wo", "l\u00e4ngst", "ver\u00b7weht", "vom", "W\u00fcs\u00b7ten\u00b7k\u00f6r\u00b7ner\u00b7schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Isistempel in sich selbst zerfallen.", "tokens": ["Die", "I\u00b7sis\u00b7tem\u00b7pel", "in", "sich", "selbst", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht tr\u00fcbte schon den funkelnden Azur", "tokens": ["Nicht", "tr\u00fcb\u00b7te", "schon", "den", "fun\u00b7keln\u00b7den", "A\u00b7zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Riesenschlote schmutzigfeuchter Brodem,", "tokens": ["Der", "Rie\u00b7sen\u00b7schlo\u00b7te", "schmut\u00b7zig\u00b7feuch\u00b7ter", "Bro\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn unentweiht noch tr\u00e4umte die Natur", "tokens": ["Denn", "un\u00b7ent\u00b7weiht", "noch", "tr\u00e4um\u00b7te", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Windhauch war ein Gottesodem.", "tokens": ["Und", "je\u00b7der", "Wind\u00b7hauch", "war", "ein", "Got\u00b7te\u00b7so\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein Erdgeborner f\u00fchlte sich entbrannt", "tokens": ["Kein", "Erd\u00b7ge\u00b7bor\u00b7ner", "f\u00fchl\u00b7te", "sich", "ent\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach fremden Wundern einer fremden Zone", "tokens": ["Nach", "frem\u00b7den", "Wun\u00b7dern", "ei\u00b7ner", "frem\u00b7den", "Zo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und brach mit seiner frevlen Menschenhand", "tokens": ["Und", "brach", "mit", "sei\u00b7ner", "frev\u00b7len", "Men\u00b7schen\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich Stein auf Stein aus Gottes Sch\u00f6pfungskrone.", "tokens": ["Sich", "Stein", "auf", "Stein", "aus", "Got\u00b7tes", "Sch\u00f6p\u00b7fungs\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch jede Zeit singt sich ihr eignes Lied", "tokens": ["Doch", "je\u00b7de", "Zeit", "singt", "sich", "ihr", "eig\u00b7nes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jenes Lied ist lange schon verklungen;", "tokens": ["Und", "je\u00b7nes", "Lied", "ist", "lan\u00b7ge", "schon", "ver\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Melodie, die heut die Welt durchzieht,", "tokens": ["Die", "Me\u00b7lo\u00b7die", ",", "die", "heut", "die", "Welt", "durch\u00b7zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00f6hnt die alten Ueberlieferungen.", "tokens": ["Ver\u00b7h\u00f6hnt", "die", "al\u00b7ten", "Ue\u00b7berl\u00b7ie\u00b7fe\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Menschheit hat sich zum Titanenkampf", "tokens": ["Die", "Menschheit", "hat", "sich", "zum", "Ti\u00b7ta\u00b7nen\u00b7kampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit ihrer Mutter, der Natur, ger\u00fcstet", "tokens": ["Mit", "ih\u00b7rer", "Mut\u00b7ter", ",", "der", "Na\u00b7tur", ",", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und denkt nur noch mit Eisen, Blut und Dampf,", "tokens": ["Und", "denkt", "nur", "noch", "mit", "Ei\u00b7sen", ",", "Blut", "und", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Weil sie's dem Sch\u00f6pfer ", "tokens": ["Weil", "sie's", "dem", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Erloschen ist der kindlichfromme Zug", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "der", "kind\u00b7lich\u00b7from\u00b7me", "Zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus ihres Angesichts versteinten Mienen,", "tokens": ["Aus", "ih\u00b7res", "An\u00b7ge\u00b7sichts", "ver\u00b7stein\u00b7ten", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unbek\u00fcmmert um den alten Fluch,", "tokens": ["Und", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "um", "den", "al\u00b7ten", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zwingt sie die E