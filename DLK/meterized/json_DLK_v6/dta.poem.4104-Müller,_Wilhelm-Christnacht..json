{"dta.poem.4104": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Christnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Goldengr\u00fcn und Kerzenschein,", "tokens": ["Gol\u00b7den\u00b7gr\u00fcn", "und", "Ker\u00b7zen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jauchzend h\u00f6r' ich durch die Laden", "tokens": ["Jauch\u00b7zend", "h\u00f6r'", "ich", "durch", "die", "La\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helle Kinderstimmen schrein.", "tokens": ["Hel\u00b7le", "Kin\u00b7der\u00b7stim\u00b7men", "schrein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schmetternde Posaunen schallen", "tokens": ["Schmet\u00b7tern\u00b7de", "Po\u00b7sau\u00b7nen", "schal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Kirchenthurm herab:", "tokens": ["Von", "dem", "Kir\u00b7chen\u00b7thurm", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lobt den Vater in der H\u00f6he,", "tokens": ["Lobt", "den", "Va\u00b7ter", "in", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der der Welt das Kindlein gab!", "tokens": ["Der", "der", "Welt", "das", "Kin\u00b7dlein", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herz, mein Herz, wie bist so selig?", "tokens": ["Herz", ",", "mein", "Herz", ",", "wie", "bist", "so", "se\u00b7lig", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, mein Herz, und so allein?", "tokens": ["Herz", ",", "mein", "Herz", ",", "und", "so", "al\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Gaben, unsre W\u00fcnsche,", "tokens": ["Uns\u00b7re", "Ga\u00b7ben", ",", "uns\u00b7re", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcrfen wir sie Keinem weihn?", "tokens": ["D\u00fcr\u00b7fen", "wir", "sie", "Kei\u00b7nem", "weihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine wei\u00df ich wohl zu finden,", "tokens": ["Ei\u00b7ne", "wei\u00df", "ich", "wohl", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich Vieles g\u00f6nnen mag;", "tokens": ["Der", "ich", "Vie\u00b7les", "g\u00f6n\u00b7nen", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Offen steht mir ihre Pforte,", "tokens": ["Of\u00b7fen", "steht", "mir", "ih\u00b7re", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es kennt mich ihr Gemach.", "tokens": ["Und", "es", "kennt", "mich", "ihr", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber in dem stillen Hause", "tokens": ["A\u00b7ber", "in", "dem", "stil\u00b7len", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brennt kein festlich helles Licht,", "tokens": ["Brennt", "kein", "fest\u00b7lich", "hel\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im schwarzen Wochenkleide", "tokens": ["Und", "im", "schwar\u00b7zen", "Wo\u00b7chen\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitzt sie da und freut sich nicht.", "tokens": ["Sitzt", "sie", "da", "und", "freut", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, ihr ist er nicht geboren,", "tokens": ["Ach", ",", "ihr", "ist", "er", "nicht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in dieser sel'gen Nacht", "tokens": ["Der", "in", "die\u00b7ser", "sel'\u00b7gen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freud' und Fried' und Wohlgefallen", "tokens": ["Freud'", "und", "Fried'", "und", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat zu uns herabgebracht.", "tokens": ["Hat", "zu", "uns", "her\u00b7ab\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seine Liebe, seine Leiden", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", ",", "sei\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringen nicht zu ihr hinein:", "tokens": ["Drin\u00b7gen", "nicht", "zu", "ihr", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber ihre zarte Seele", "tokens": ["Ue\u00b7ber", "ih\u00b7re", "zar\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrschet ein Gesetz von Stein.", "tokens": ["Herr\u00b7schet", "ein", "Ge\u00b7setz", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}