{"textgrid.poem.25422": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "An sein B\u00fcchlein", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es dann nun lauter eylen?", "tokens": ["Ist", "es", "dann", "nun", "lau\u00b7ter", "ey\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kanstu B\u00fcchlein nicht verweilen?", "tokens": ["Kans\u00b7tu", "B\u00fcch\u00b7lein", "nicht", "ver\u00b7wei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziht dann ein Magnet dich fort?", "tokens": ["Ziht", "dann", "ein", "Mag\u00b7net", "dich", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Ja ich mercke deine Possen/", "tokens": ["Ja", "ich", "mer\u00b7cke", "dei\u00b7ne", "Pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dier beliebt ein solcher Ort/", "tokens": ["Dier", "be\u00b7liebt", "ein", "sol\u00b7cher", "Ort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo du wirst aus Lieb' \u00fcmbschlossen.", "tokens": ["Wo", "du", "wirst", "aus", "Lieb'", "\u00fcmb\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du wilt in den edlen Zimmern", "tokens": ["Du", "wilt", "in", "den", "ed\u00b7len", "Zim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bey den sch\u00f6nen Jungfern schimmern;", "tokens": ["Bey", "den", "sch\u00f6\u00b7nen", "Jung\u00b7fern", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr Geth\u00f6n hat dich entz\u00fcckt/", "tokens": ["Ihr", "Ge\u00b7th\u00f6n", "hat", "dich", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du nun von ihren Zungen", "tokens": ["Da\u00df", "du", "nun", "von", "ih\u00b7ren", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch wilt werden angeschm\u00fcckt", "tokens": ["Auch", "wilt", "wer\u00b7den", "an\u00b7ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und mit Freuden hergesungen.", "tokens": ["und", "mit", "Freu\u00b7den", "her\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie? wann aber nicht gefiele/", "tokens": ["Wie", "?", "wann", "a\u00b7ber", "nicht", "ge\u00b7fie\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich hier ohn Anmuth spiele?", "tokens": ["Was", "ich", "hier", "ohn", "An\u00b7muth", "spie\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer es dann viel b\u00e4sser nicht/", "tokens": ["Wer", "es", "dann", "viel", "b\u00e4s\u00b7ser", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du werest bey mier blieben", "tokens": ["Da\u00df", "du", "we\u00b7rest", "bey", "mier", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und gescheut das Tagelicht/", "tokens": ["und", "ge\u00b7scheut", "das", "Ta\u00b7ge\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als da\u00df du Dich wilt betr\u00fcben?", "tokens": ["Als", "da\u00df", "du", "Dich", "wilt", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}