{"textgrid.poem.52003": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hutt steigt wie ein Thurn vom Sch\u00e4del in die H\u00f6h,", "tokens": ["Der", "Hutt", "steigt", "wie", "ein", "Thurn", "vom", "Sch\u00e4\u00b7del", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff welchen dir der Kranch drey Federn hat geliehen:", "tokens": ["Auff", "wel\u00b7chen", "dir", "der", "Kranch", "drey", "Fe\u00b7dern", "hat", "ge\u00b7lie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ART", "NN", "CARD", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht die Wangen ab des Bartes P\u00fcsche ziehen,", "tokens": ["Man", "sieht", "die", "Wan\u00b7gen", "ab", "des", "Bar\u00b7tes", "P\u00fc\u00b7sche", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der voller Zapffen h\u00e4ngt und grauer ist als Schnee.", "tokens": ["Der", "vol\u00b7ler", "Zapf\u00b7fen", "h\u00e4ngt", "und", "grau\u00b7er", "ist", "als", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Kragen umb den Hals kennt keine St\u00e4rcke nicht,", "tokens": ["Der", "Kra\u00b7gen", "umb", "den", "Hals", "kennt", "kei\u00b7ne", "St\u00e4r\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwimmt auff der Kappen her, wie die Magister tragen:", "tokens": ["Schwimmt", "auff", "der", "Kap\u00b7pen", "her", ",", "wie", "die", "Ma\u00b7gis\u00b7ter", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das andre Schweitzer Kleid ist um und um beschlagen,", "tokens": ["Das", "and\u00b7re", "Schweit\u00b7zer", "Kleid", "ist", "um", "und", "um", "be\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "APPR", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil es die Trauffe f\u00e4rbt und Kammerlauge bricht.", "tokens": ["Weil", "es", "die", "Trauf\u00b7fe", "f\u00e4rbt", "und", "Kam\u00b7mer\u00b7lau\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zun Knochen hastu dir von Filtz ein Haus gebaut,", "tokens": ["Zun", "Kno\u00b7chen", "has\u00b7tu", "dir", "von", "Filtz", "ein", "Haus", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "APPR", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du bi\u00df oben an mit Betten hast verbunden:", "tokens": ["Die", "du", "bi\u00df", "o\u00b7ben", "an", "mit", "Bet\u00b7ten", "hast", "ver\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn auff der Radwer du dich drauff zu uns gefunden,", "tokens": ["Wenn", "auff", "der", "Rad\u00b7wer", "du", "dich", "drauff", "zu", "uns", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PRF", "PAV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Liegt vorn und hinten Heu, da\u00df man nichts von dir schaut.", "tokens": ["Liegt", "vorn", "und", "hin\u00b7ten", "Heu", ",", "da\u00df", "man", "nichts", "von", "dir", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "NN", "$,", "KOUS", "PIS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mein lieber Ritters Mann, wie seltsam bist doch du:", "tokens": ["Mein", "lie\u00b7ber", "Rit\u00b7ters", "Mann", ",", "wie", "selt\u00b7sam", "bist", "doch", "du", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob der Sulpitia dein junges Weib zu gleichen,", "tokens": ["Ob", "der", "Sul\u00b7pi\u00b7tia", "dein", "jun\u00b7ges", "Weib", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Wird doch der Titul nicht des Hanrehs von dir weichen,", "tokens": ["Wird", "doch", "der", "Ti\u00b7tul", "nicht", "des", "Han\u00b7rehs", "von", "dir", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie fromm sie scheint und ist: Die Tracht macht dich darzu.", "tokens": ["Wie", "fromm", "sie", "scheint", "und", "ist", ":", "Die", "Tracht", "macht", "dich", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "KON", "VAFIN", "$.", "ART", "NN", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}