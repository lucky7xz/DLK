{"textgrid.poem.35694": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "18. Die Laube", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit des Jubels Donnerschl\u00e4gen", "tokens": ["Mit", "des", "Ju\u00b7bels", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab die Wolke Gottes Segen;", "tokens": ["Gab", "die", "Wol\u00b7ke", "Got\u00b7tes", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Fluren Opferduft", "tokens": ["Und", "der", "Flu\u00b7ren", "Op\u00b7fer\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallet lieblich durch die Luft.", "tokens": ["Wal\u00b7let", "lieb\u00b7lich", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Wolke steht, umzogen", "tokens": ["Und", "die", "Wol\u00b7ke", "steht", ",", "um\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Friedens hellem Bogen,", "tokens": ["Von", "des", "Frie\u00b7dens", "hel\u00b7lem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter dem die Flamme spielt,", "tokens": ["Un\u00b7ter", "dem", "die", "Flam\u00b7me", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der des Tages Glut gek\u00fchlt.", "tokens": ["Der", "des", "Ta\u00b7ges", "Glut", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Sonn' am blauen Himmel,", "tokens": ["Und", "die", "Sonn'", "am", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Rings umschwebt von Glanzgewimmel!", "tokens": ["Rings", "um\u00b7schwebt", "von", "Glanz\u00b7ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das gr\u00fcne Weizenthal,", "tokens": ["Und", "das", "gr\u00fc\u00b7ne", "Wei\u00b7zen\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberstr\u00f6mt vom mildern Strahl!", "tokens": ["\u00dc\u00b7ber\u00b7str\u00f6mt", "vom", "mil\u00b7dern", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie mit Brautgeschmeide, funkeln", "tokens": ["Wie", "mit", "Braut\u00b7ge\u00b7schmei\u00b7de", ",", "fun\u00b7keln"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mohne, Rosen und Ranunkeln,", "tokens": ["Moh\u00b7ne", ",", "Ro\u00b7sen", "und", "Ra\u00b7nun\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bienen suchen Honigseim,", "tokens": ["Bie\u00b7nen", "su\u00b7chen", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sumsen goldgefl\u00fcgelt heim!", "tokens": ["Sum\u00b7sen", "gold\u00b7ge\u00b7fl\u00fc\u00b7gelt", "heim", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alle Kreaturen loben,", "tokens": ["Al\u00b7le", "Kre\u00b7a\u00b7tu\u00b7ren", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachteln unten, Lerchen oben,", "tokens": ["Wach\u00b7teln", "un\u00b7ten", ",", "Ler\u00b7chen", "o\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Herd' am Bache springt,", "tokens": ["Und", "die", "Herd'", "am", "Ba\u00b7che", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und der frohe Bauer singt!", "tokens": ["Und", "der", "fro\u00b7he", "Bau\u00b7er", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Und da wandelt Ernestine", "tokens": ["Und", "da", "wan\u00b7delt", "Er\u00b7nes\u00b7ti\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Forschend durch des Gartens Gr\u00fcne,", "tokens": ["For\u00b7schend", "durch", "des", "Gar\u00b7tens", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Achtet nichts, erblickt mich hier", "tokens": ["Ach\u00b7tet", "nichts", ",", "er\u00b7blickt", "mich", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Laub', und fliegt zu mir!", "tokens": ["In", "der", "Laub'", ",", "und", "fliegt", "zu", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}