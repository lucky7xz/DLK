{"textgrid.poem.24757": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sprechen eine Sprach und verstehen einand", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sprechen eine Sprach und verstehen einand", "tokens": ["Wir", "spre\u00b7chen", "ei\u00b7ne", "Sprach", "und", "ver\u00b7ste\u00b7hen", "ei\u00b7nand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn Mund reimt sich auf Mund und Hand reimt sich auf Hand", "tokens": ["Denn", "Mund", "reimt", "sich", "auf", "Mund", "und", "Hand", "reimt", "sich", "auf", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lauberl, Lauberl, Lirilauberl", "tokens": ["Lau\u00b7berl", ",", "Lau\u00b7berl", ",", "Li\u00b7ri\u00b7lau\u00b7berl"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, Lauberl, Litumda.", "tokens": ["Lau\u00b7berl", ",", "Lau\u00b7berl", ",", "Li\u00b7tum\u00b7da", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Kuckuck ist nah und der Kuckuck ist weit", "tokens": ["Der", "Ku\u00b7ckuck", "ist", "nah", "und", "der", "Ku\u00b7ckuck", "ist", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wir reden recht dumm und wir k\u00fcssen gescheit", "tokens": ["Wir", "re\u00b7den", "recht", "dumm", "und", "wir", "k\u00fcs\u00b7sen", "ge\u00b7scheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lauberl, Lauberl, Lirilauberl", "tokens": ["Lau\u00b7berl", ",", "Lau\u00b7berl", ",", "Li\u00b7ri\u00b7lau\u00b7berl"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauberl, Lauberl, Litumda.", "tokens": ["Lau\u00b7berl", ",", "Lau\u00b7berl", ",", "Li\u00b7tum\u00b7da", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Mutter ist drau\u00dfen und wir sind herin", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "ist", "drau\u00b7\u00dfen", "und", "wir", "sind", "he\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "PPER", "VAFIN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und k\u00e4m sie herein, das h\u00e4tt gar keinen Sinn", "tokens": ["Und", "k\u00e4m", "sie", "her\u00b7ein", ",", "das", "h\u00e4tt", "gar", "kei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PDS", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lauberl, Lauberl, Lirilauberl", "to