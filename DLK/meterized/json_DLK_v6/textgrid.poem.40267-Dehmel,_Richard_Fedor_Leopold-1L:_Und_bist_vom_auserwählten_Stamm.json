{"textgrid.poem.40267": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und bist vom auserw\u00e4hlten Stamm", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und bist vom auserw\u00e4hlten Stamm", "tokens": ["Und", "bist", "vom", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und liebst dein Volk und uralt Blut,", "tokens": ["und", "liebst", "dein", "Volk", "und", "ur\u00b7alt", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und fast wie Ha\u00df ist deine Glut", "tokens": ["und", "fast", "wie", "Ha\u00df", "ist", "dei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "f\u00fcr deinen schwer gequ\u00e4lten Stamm;", "tokens": ["f\u00fcr", "dei\u00b7nen", "schwer", "ge\u00b7qu\u00e4l\u00b7ten", "Stamm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "und tr\u00e4umst von euerm Sinai", "tokens": ["und", "tr\u00e4umst", "von", "eu\u00b7erm", "Si\u00b7nai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "und der nur Euren Himmelsn\u00e4h',", "tokens": ["und", "der", "nur", "Eu\u00b7ren", "Him\u00b7mels\u00b7n\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und stehst wie Mose vor Jahw\u00e4h", "tokens": ["und", "stehst", "wie", "Mo\u00b7se", "vor", "Jah\u00b7w\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "NE", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und stehst und schw\u00f6rst: ich wanke nie.", "tokens": ["und", "stehst", "und", "schw\u00f6rst", ":", "ich", "wan\u00b7ke", "nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dennoch kam in deinen Mund", "tokens": ["Und", "den\u00b7noch", "kam", "in", "dei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Wort, das einst am Jordan klang;", "tokens": ["das", "Wort", ",", "das", "einst", "am", "Jor\u00b7dan", "klang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da rang ein ", "tokens": ["da", "rang", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "sich weinend ", "tokens": ["sich", "wei\u00b7nend"], "token_info": ["word", "word"], "pos": ["PRF", "VVPP"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "und sprach, inde\u00df sein blutend Herz", "tokens": ["und", "sprach", ",", "in\u00b7de\u00df", "sein", "blu\u00b7tend", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus Schauern schwer gen Himmel stieg:", "tokens": ["aus", "Schau\u00b7ern", "schwer", "gen", "Him\u00b7mel", "stieg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur dein Entsagen ist dein Sieg,", "tokens": ["Nur", "dein", "Ent\u00b7sa\u00b7gen", "ist", "dein", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und eins und gleich ist Aller Schmerz.", "tokens": ["und", "eins", "und", "gleich", "ist", "Al\u00b7ler", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KON", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie kam es doch, das Wort der Qual,", "tokens": ["Wie", "kam", "es", "doch", ",", "das", "Wort", "der", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des Menschensohnes Siegesnot,", "tokens": ["des", "Men\u00b7schen\u00b7soh\u00b7nes", "Sie\u00b7ges\u00b7not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auf Deine Zunge \u2013 sein Gebot:", "tokens": ["auf", "Dei\u00b7ne", "Zun\u00b7ge", "\u2013", "sein", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbsei stark, zerbrich den Trieb der Wahl!\u00ab", "tokens": ["\u00bb", "sei", "stark", ",", "zer\u00b7brich", "den", "Trieb", "der", "Wahl", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "ADJD", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "und liebst ein auserw\u00e4hltes Volk", "tokens": ["und", "liebst", "ein", "au\u00b7ser\u00b7w\u00e4hl\u00b7tes", "Volk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und fast wie Ha\u00df ist deine Glut?!", "tokens": ["und", "fast", "wie", "Ha\u00df", "ist", "dei\u00b7ne", "Glut", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und siehe, um dich rauscht Ein Blut:", "tokens": ["Und", "sie\u00b7he", ",", "um", "dich", "rauscht", "Ein", "Blut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "KOUI", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Menschen schwer gequ\u00e4ltes Volk ...", "tokens": ["der", "Men\u00b7schen", "schwer", "ge\u00b7qu\u00e4l\u00b7tes", "Volk", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}