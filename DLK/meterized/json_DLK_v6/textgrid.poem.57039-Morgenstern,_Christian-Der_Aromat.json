{"textgrid.poem.57039": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Aromat", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angeregt durch Korfs Geruchs-Sonaten,", "tokens": ["An\u00b7ge\u00b7regt", "durch", "Korfs", "Ge\u00b7ruchs\u00b7So\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "gr\u00fcnden Freunde einen \u00bbAromaten\u00ab.", "tokens": ["gr\u00fcn\u00b7den", "Freun\u00b7de", "ei\u00b7nen", "\u00bb", "A\u00b7ro\u00b7ma\u00b7ten", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ART", "$(", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Einen Raum, in welchem, kurz gesprochen,", "tokens": ["Ei\u00b7nen", "Raum", ",", "in", "wel\u00b7chem", ",", "kurz", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "nicht geschluckt wird, sondern nur gerochen.", "tokens": ["nicht", "ge\u00b7schluckt", "wird", ",", "son\u00b7dern", "nur", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "$,", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Gegen Einwurf kleiner M\u00fcnzen treten", "tokens": ["Ge\u00b7gen", "Ein\u00b7wurf", "klei\u00b7ner", "M\u00fcn\u00b7zen", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus der Wand balsamische Trompeten,", "tokens": ["aus", "der", "Wand", "bal\u00b7sa\u00b7mi\u00b7sche", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "die den G\u00e4sten in gebl\u00e4hte Nasen,", "tokens": ["die", "den", "G\u00e4s\u00b7ten", "in", "ge\u00b7bl\u00e4h\u00b7te", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "was sie w\u00fcnschen, leicht und lustig blasen.", "tokens": ["was", "sie", "w\u00fcn\u00b7schen", ",", "leicht", "und", "lus\u00b7tig", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und zugleich erscheint auf einem Schild", "tokens": ["Und", "zu\u00b7gleich", "er\u00b7scheint", "auf", "ei\u00b7nem", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "des Gerichtes wohlgetroffnes Bild", "tokens": ["des", "Ge\u00b7rich\u00b7tes", "wohl\u00b7ge\u00b7troff\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Viele Hunderte, um nicht zu l\u00fcgen,", "tokens": ["Vie\u00b7le", "Hun\u00b7der\u00b7te", ",", "um", "nicht", "zu", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "speisen nun erst wirklich mit Vergn\u00fcgen.", "tokens": ["spei\u00b7sen", "nun", "erst", "wirk\u00b7lich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}