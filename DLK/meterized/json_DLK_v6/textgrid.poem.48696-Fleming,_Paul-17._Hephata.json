{"textgrid.poem.48696": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. Hephata", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! sprich es auch zu mir, dein kr\u00e4ftigs: Tu dich auf!", "tokens": ["Ach", "!", "sprich", "es", "auch", "zu", "mir", ",", "dein", "kr\u00e4f\u00b7tigs", ":", "Tu", "dich", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "NE", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach! sprich es auch zu mir. Denn mir auch sind verschlossen", "tokens": ["Ach", "!", "sprich", "es", "auch", "zu", "mir", ".", "Denn", "mir", "auch", "sind", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$.", "KON", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ohr, Augen und der Mund. Viel Zeit ist hin verflossen,", "tokens": ["Ohr", ",", "Au\u00b7gen", "und", "der", "Mund", ".", "Viel", "Zeit", "ist", "hin", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "$.", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df ich so elend bin. Die Welt hat viel zu Kauf.", "tokens": ["da\u00df", "ich", "so", "e\u00b7lend", "bin", ".", "Die", "Welt", "hat", "viel", "zu", "Kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$.", "ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich folge, was sie r\u00e4t und werd nur \u00e4rger drauf.", "tokens": ["Ich", "fol\u00b7ge", ",", "was", "sie", "r\u00e4t", "und", "werd", "nur", "\u00e4r\u00b7ger", "drauf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lebt mein kranker Leib mit seinem Hausgenossen,", "tokens": ["So", "lebt", "mein", "kran\u00b7ker", "Leib", "mit", "sei\u00b7nem", "Haus\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zu allem Werke la\u00df, zu allem Tun verdrossen,", "tokens": ["zu", "al\u00b7lem", "Wer\u00b7ke", "la\u00df", ",", "zu", "al\u00b7lem", "Tun", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "PTKVZ", "$,", "APPR", "PIS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf ein Ding nur beherzt, zu enden seinen Lauf.", "tokens": ["auf", "ein", "Ding", "nur", "be\u00b7herzt", ",", "zu", "en\u00b7den", "sei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ists m\u00f6glich, da\u00df mir noch auf dieser b\u00f6sen Erden,", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "mir", "noch", "auf", "die\u00b7ser", "b\u00f6\u00b7sen", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "o Arzt, durch deine Hand soll ausgeholfen werden,", "tokens": ["o", "Arzt", ",", "durch", "dei\u00b7ne", "Hand", "soll", "aus\u00b7ge\u00b7hol\u00b7fen", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "APPR", "PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so zeuch mich nicht mehr auf. Hilf diesem \u00dcbel ab.", "tokens": ["so", "zeuch", "mich", "nicht", "mehr", "auf", ".", "Hilf", "die\u00b7sem", "\u00dc\u00b7bel", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKNEG", "ADV", "PTKVZ", "$.", "NE", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nim mein Beschweren hin, nach dem mein Geist so wacht.", "tokens": ["Nim", "mein", "Be\u00b7schwe\u00b7ren", "hin", ",", "nach", "dem", "mein", "Geist", "so", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "APPR", "ART", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tust du's, so soll dein Lob auch rufen aus mein Grab:", "tokens": ["Tust", "du's", ",", "so", "soll", "dein", "Lob", "auch", "ru\u00b7fen", "aus", "mein", "Grab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Alles machet wol, hat mirs auch wol gemacht!", "tokens": ["Der", "Al\u00b7les", "ma\u00b7chet", "wol", ",", "hat", "mirs", "auch", "wol", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "VAFIN", "NE", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}