{"textgrid.poem.38936": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es funkelt Gold in wilden Tr\u00fcmmern,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es funkelt Gold in wilden Tr\u00fcmmern,", "tokens": ["Es", "fun\u00b7kelt", "Gold", "in", "wil\u00b7den", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tief im verborgenen Gestein,", "tokens": ["Tief", "im", "ver\u00b7bor\u00b7ge\u00b7nen", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe ferne Sch\u00e4tze schimmern,", "tokens": ["Ich", "se\u00b7he", "fer\u00b7ne", "Sch\u00e4t\u00b7ze", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich lockt der r\u00e4thselhafte Schein.", "tokens": ["Mich", "lockt", "der", "r\u00e4th\u00b7sel\u00b7haf\u00b7te", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und hinter mir f\u00e4llt es zusammen,", "tokens": ["Und", "hin\u00b7ter", "mir", "f\u00e4llt", "es", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ha! um mich her ein enges Grab,", "tokens": ["Ha", "!", "um", "mich", "her", "ein", "en\u00b7ges", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUI", "PRF", "APZR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Welt, der Tag entflieht, die Flammen", "tokens": ["Die", "Welt", ",", "der", "Tag", "ent\u00b7flieht", ",", "die", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kerzen sinken, sterben ab.", "tokens": ["Der", "Ker\u00b7zen", "sin\u00b7ken", ",", "ster\u00b7ben", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Hand klopft zitternd an die W\u00e4nde,", "tokens": ["Die", "Hand", "klopft", "zit\u00b7ternd", "an", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unterird'sche Wandrer schaut", "tokens": ["Der", "un\u00b7ter\u00b7ird'\u00b7sche", "Wand\u00b7rer", "schaut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Licht und Rettung, ohne Ende", "tokens": ["Nach", "Licht", "und", "Ret\u00b7tung", ",", "oh\u00b7ne", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Dunkel! \u2013 Ihn erquickt kein Laut.", "tokens": ["Das", "Dun\u00b7kel", "!", "\u2013", "Ihn", "er\u00b7quickt", "kein", "Laut", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er h\u00e4mmert in den Felsgem\u00e4chern", "tokens": ["Er", "h\u00e4m\u00b7mert", "in", "den", "Fels\u00b7ge\u00b7m\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einer dumpfen Lebensgier,", "tokens": ["Mit", "ei\u00b7ner", "dum\u00b7pfen", "Le\u00b7bens\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gefangen von den dunkeln R\u00e4chern,", "tokens": ["Ge\u00b7fan\u00b7gen", "von", "den", "dun\u00b7keln", "R\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Strafe seiner Wi\u00dfbegier.", "tokens": ["Zur", "Stra\u00b7fe", "sei\u00b7ner", "Wi\u00df\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da \u00e4ugelt aus der fernsten Ritze", "tokens": ["Da", "\u00e4u\u00b7gelt", "aus", "der", "ferns\u00b7ten", "Rit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein blaues Lichtchen nach mir hin,", "tokens": ["Ein", "blau\u00b7es", "Licht\u00b7chen", "nach", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich krieche zu der schroffen Spitze,", "tokens": ["Ich", "krie\u00b7che", "zu", "der", "schrof\u00b7fen", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und taste mit entz\u00fcckten Sinn.", "tokens": ["Und", "tas\u00b7te", "mit", "ent\u00b7z\u00fcck\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ach, es ist das Goldgestein,", "tokens": ["Und", "ach", ",", "es", "ist", "das", "Gold\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mich zuerst hierher versucht,", "tokens": ["Das", "mich", "zu\u00b7erst", "hier\u00b7her", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun labt mich nicht der Flimmerschein,", "tokens": ["Nun", "labt", "mich", "nicht", "der", "Flim\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der boshaft mich zuerst versucht.", "tokens": ["Der", "bos\u00b7haft", "mich", "zu\u00b7erst", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es sehnt der Geist sich nach dem Bande,", "tokens": ["Es", "sehnt", "der", "Geist", "sich", "nach", "dem", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ihn mit zarter Fessel hielt,", "tokens": ["Das", "ihn", "mit", "zar\u00b7ter", "Fes\u00b7sel", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er sich wie im Vaterlande", "tokens": ["Als", "er", "sich", "wie", "im", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner stillen Brust gef\u00fchlt.", "tokens": ["In", "sei\u00b7ner", "stil\u00b7len", "Brust", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Fern liegt das heimische Gestade", "tokens": ["Fern", "liegt", "das", "hei\u00b7mi\u00b7sche", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um wilden Taurien verirrt,", "tokens": ["Um", "wil\u00b7den", "Tau\u00b7ri\u00b7en", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kniet er umsonst und flehet Gnade,", "tokens": ["Kniet", "er", "um\u00b7sonst", "und", "fle\u00b7het", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das blut'ge Opfermesser klirrt!", "tokens": ["Das", "blut'\u00b7ge", "Op\u00b7fer\u00b7mes\u00b7ser", "klirrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch Blumen bl\u00fchn in diesen Schrecken,", "tokens": ["Doch", "Blu\u00b7men", "bl\u00fchn", "in", "die\u00b7sen", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hell mit rothem Purpur gl\u00fchn,", "tokens": ["Die", "hell", "mit", "ro\u00b7them", "Pur\u00b7pur", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Todesschatten, die ihn decken,", "tokens": ["Die", "To\u00b7des\u00b7schat\u00b7ten", ",", "die", "ihn", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lassen pr\u00e4cht'ge Funken spr\u00fchn.", "tokens": ["Sie", "las\u00b7sen", "pr\u00e4cht'\u00b7ge", "Fun\u00b7ken", "spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Liegt alles nur im Sinnengl\u00fccke?", "tokens": ["Liegt", "al\u00b7les", "nur", "im", "Sin\u00b7nen\u00b7gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vereint sich jeder Ton zum Chor?", "tokens": ["Ver\u00b7eint", "sich", "je\u00b7der", "Ton", "zum", "Chor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr tausend Str\u00f6me eine Br\u00fccke?", "tokens": ["F\u00fcr", "tau\u00b7send", "Str\u00f6\u00b7me", "ei\u00b7ne", "Br\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gehn alle Pilger durch dies Thor?", "tokens": ["Gehn", "al\u00b7le", "Pil\u00b7ger", "durch", "dies", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So \u00f6ffnet mir die dunkeln Reiche,", "tokens": ["So", "\u00f6ff\u00b7net", "mir", "die", "dun\u00b7keln", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich ein Wandrer drinnen geh,", "tokens": ["Da\u00df", "ich", "ein", "Wand\u00b7rer", "drin\u00b7nen", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich nur einst das Ziel erreiche", "tokens": ["Da\u00df", "ich", "nur", "einst", "das", "Ziel", "er\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedes Wunder schnell versteh.", "tokens": ["Und", "je\u00b7des", "Wun\u00b7der", "schnell", "ver\u00b7steh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er\u00f6ffnet mir die finstern Pforten,", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "mir", "die", "fins\u00b7tern", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An denen schwarze W\u00e4chter stehn,", "tokens": ["An", "de\u00b7nen", "schwar\u00b7ze", "W\u00e4ch\u00b7ter", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft alle gr\u00e4\u00dflichen Kohorten,", "tokens": ["La\u00dft", "al\u00b7le", "gr\u00e4\u00df\u00b7li\u00b7chen", "Ko\u00b7hor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir durch jene Pfade gehn!", "tokens": ["Mit", "mir", "durch", "je\u00b7ne", "Pfa\u00b7de", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Je wildre Schrecken mich ergreifen,", "tokens": ["Je", "wild\u00b7re", "Schre\u00b7cken", "mich", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Je h\u00f6her mich der Wahnsinn hebt,", "tokens": ["Je", "h\u00f6\u00b7her", "mich", "der", "Wahn\u00b7sinn", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lauter alle St\u00fcrme pfeifen,", "tokens": ["So", "lau\u00b7ter", "al\u00b7le", "St\u00fcr\u00b7me", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Je \u00e4ngstlicher mein Busen bebt,", "tokens": ["Je", "\u00e4ngst\u00b7li\u00b7cher", "mein", "Bu\u00b7sen", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "So inniger hei\u00df ich willkommen", "tokens": ["So", "in\u00b7ni\u00b7ger", "hei\u00df", "ich", "will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was gr\u00e4\u00dflich sich mir n\u00e4her schleift,", "tokens": ["Was", "gr\u00e4\u00df\u00b7lich", "sich", "mir", "n\u00e4\u00b7her", "schleift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem ird'schen Leben abgenommen,", "tokens": ["Dem", "ird'\u00b7schen", "Le\u00b7ben", "ab\u00b7ge\u00b7nom\u00b7men", ","], "to