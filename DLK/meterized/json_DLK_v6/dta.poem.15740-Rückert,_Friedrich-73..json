{"dta.poem.15740": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst: Die Rose bl\u00fcht, es singt die Nachtigall;", "tokens": ["Du", "sagst", ":", "Die", "Ro\u00b7se", "bl\u00fcht", ",", "es", "singt", "die", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch siehst du hundert bl\u00fchn, h\u00f6rst hundertfachen Schall.", "tokens": ["Doch", "siehst", "du", "hun\u00b7dert", "bl\u00fchn", ",", "h\u00f6rst", "hun\u00b7dert\u00b7fa\u00b7chen", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "CARD", "VVINF", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch alle Rosen sind in einer dir verschlungen,", "tokens": ["Doch", "al\u00b7le", "Ro\u00b7sen", "sind", "in", "ei\u00b7ner", "dir", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ART", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nachtigallen all' in einer Kehl' erklungen.", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "all'", "in", "ei\u00b7ner", "Kehl'", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So f\u00fchlt die Poesie in sich ein Dichter ganz,", "tokens": ["So", "f\u00fchlt", "die", "Poe\u00b7sie", "in", "sich", "ein", "Dich\u00b7ter", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und alle Sch\u00f6nheit sieht die Lieb' in Einem Glanz.", "tokens": ["Und", "al\u00b7le", "Sch\u00f6n\u00b7heit", "sieht", "die", "Lieb'", "in", "Ei\u00b7nem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}