{"textgrid.poem.25917": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und der Regen f\u00e4llt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut, ums Haus herauf,", "tokens": ["Heut", ",", "ums", "Haus", "her\u00b7auf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Geht die Sonn' nicht auf.", "tokens": ["Geht", "die", "Sonn'", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Regen auf den Steinen,", "tokens": ["Re\u00b7gen", "auf", "den", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihre B\u00e4cklein weinen.", "tokens": ["Ih\u00b7re", "B\u00e4c\u00b7klein", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Berglein und die Hecken", "tokens": ["Berg\u00b7lein", "und", "die", "He\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Voller Tr\u00e4nen stecken.", "tokens": ["Vol\u00b7ler", "Tr\u00e4\u00b7nen", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kein Ding h\u00e4lt am Ort,", "tokens": ["Kein", "Ding", "h\u00e4lt", "am", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Nebel tr\u00e4gt es fort.", "tokens": ["Ne\u00b7bel", "tr\u00e4gt", "es", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Weinend kommt die Stund,", "tokens": ["Wei\u00b7nend", "kommt", "die", "Stund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Lebwohl sagt der Mund.", "tokens": ["Leb\u00b7wohl", "sagt", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und die Tr\u00e4n' sagt: bleib!", "tokens": ["Und", "die", "Tr\u00e4n'", "sagt", ":", "bleib", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Und das Herz im Leib", "tokens": ["Und", "das", "Herz", "im", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Dreht sich schluchzend um,", "tokens": ["Dreht", "sich", "schluch\u00b7zend", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Nur der Fu\u00df geht stumm.", "tokens": ["Nur", "der", "Fu\u00df", "geht", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Weiter rollt die Welt,", "tokens": ["Wei\u00b7ter", "rollt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Und der Regen f\u00e4llt.", "tokens": ["Und", "der", "Re\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}