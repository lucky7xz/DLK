{"dta.poem.2051": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Elephant.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist gleich dieses gro\u00dfe Thier selten nur bey uns zu", "tokens": ["Ist", "gleich", "die\u00b7ses", "gro\u00b7\u00dfe", "Thier", "sel\u00b7ten", "nur", "bey", "uns", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PDAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPER", "PTKZU"], "meter": "+-+-+-+---+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "sehen,", "tokens": ["se\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da es andern V\u00f6lkern dient; ist es doch besonders werth,", "tokens": ["Da", "es", "an\u00b7dern", "V\u00f6l\u00b7kern", "dient", ";", "ist", "es", "doch", "be\u00b7son\u00b7ders", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wegen seiner Gr\u00f6\u00df\u2019 und Klugheit, wodurch Wunder fast ge-", "tokens": ["We\u00b7gen", "sei\u00b7ner", "Gr\u00f6\u00df'", "und", "Klug\u00b7heit", ",", "wo\u00b7durch", "Wun\u00b7der", "fast", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,", "PWAV", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "schehen,", "tokens": ["sche\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Da\u00df man, wegen seiner Sch\u00f6pfung, dessen Sch\u00f6pfer r\u00fchmt", "tokens": ["Da\u00df", "man", ",", "we\u00b7gen", "sei\u00b7ner", "Sch\u00f6p\u00b7fung", ",", "des\u00b7sen", "Sch\u00f6p\u00b7fer", "r\u00fchmt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "APPR", "PPOSAT", "NN", "$,", "PRELAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "und ehrt.", "tokens": ["und", "ehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Wie von seines K\u00f6rpers Gr\u00f6\u00dfe alle andre Thier\u2019 auf", "tokens": ["Wie", "von", "sei\u00b7nes", "K\u00f6r\u00b7pers", "Gr\u00f6\u00b7\u00dfe", "al\u00b7le", "and\u00b7re", "Thier'", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "NN", "PIAT", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.9": {"text": "Erden,", "tokens": ["Er\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Sollen sie von seinem Geist gleichfalls, \u00fcbertroffen", "tokens": ["Sol\u00b7len", "sie", "von", "sei\u00b7nem", "Geist", "gleich\u00b7falls", ",", "\u00fc\u00b7ber\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$,", "VVPP"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "werden.", "tokens": ["wer\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Jhrer K\u00f6rper gro\u00dfer Bau, so wie man davon erz\u00e4hlt,", "tokens": ["Ih\u00b7rer", "K\u00f6r\u00b7per", "gro\u00b7\u00dfer", "Bau", ",", "so", "wie", "man", "da\u00b7von", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "ADV", "KOKOM", "PIS", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Gleichet, so an Gr\u00f6\u00df\u2019 als Schwere, gleichsam Bergen, die", "tokens": ["Glei\u00b7chet", ",", "so", "an", "Gr\u00f6\u00df'", "als", "Schwe\u00b7re", ",", "gleich\u00b7sam", "Ber\u00b7gen", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ADV", "APPR", "NE", "KOUS", "NN", "$,", "ADJD", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "beseelt.", "tokens": ["be\u00b7seelt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Wie so weit an ihrem Wuchs die Natur die Kr\u00e4fte dehne,", "tokens": ["Wie", "so", "weit", "an", "ih\u00b7rem", "Wuchs", "die", "Na\u00b7tur", "die", "Kr\u00e4f\u00b7te", "deh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Zeigt allein die L\u00e4ng\u2019 und Schwere vieler Elephanten-", "tokens": ["Zeigt", "al\u00b7lein", "die", "L\u00e4ng'", "und", "Schwe\u00b7re", "vie\u00b7ler", "E\u00b7le\u00b7phan\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN", "PIAT", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "z\u00e4hne,", "tokens": ["z\u00e4h\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Da derselben einige zehn Schuh lang, und, wie sie", "tokens": ["Da", "der\u00b7sel\u00b7ben", "ei\u00b7ni\u00b7ge", "zehn", "Schuh", "lang", ",", "und", ",", "wie", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PDAT", "PIAT", "CARD", "NN", "ADJD", "$,", "KON", "$,", "PWAV", "PPER"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "wollen,", "tokens": ["wol\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Weit mehr als dreyhundert Pfund am Gewichte halten", "tokens": ["Weit", "mehr", "als", "drey\u00b7hun\u00b7dert", "Pfund", "am", "Ge\u00b7wich\u00b7te", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PIAT", "KOKOM", "CARD", "NN", "APPRART", "NN", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.21": {"text": "sollen.", "tokens": ["sol\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Er soll sich zum Kampf, zur Arbeit, nutzbarlich gebrauchen", "tokens": ["Er", "soll", "sich", "zum", "Kampf", ",", "zur", "Ar\u00b7beit", ",", "nutz\u00b7bar\u00b7lich", "ge\u00b7brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "ADJD", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "lassen;", "tokens": ["las\u00b7sen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Er soll, wundersam gelehrig, tausend K\u00fcnste leichtlich", "tokens": ["Er", "soll", ",", "wun\u00b7der\u00b7sam", "ge\u00b7leh\u00b7rig", ",", "tau\u00b7send", "K\u00fcns\u00b7te", "leicht\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "ADJD", "$,", "CARD", "NN", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.25": {"text": "fassen,", "tokens": ["fas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Tapferkeit und Treu\u2019 besitzen, und fast wirklichen Ver-", "tokens": ["Tap\u00b7fer\u00b7keit", "und", "Treu'", "be\u00b7sit\u00b7zen", ",", "und", "fast", "wirk\u00b7li\u00b7chen", "Ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF", "$,", "KON", "ADV", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "stand.", "tokens": ["stand", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Sein gelenker schlanker R\u00fc\u00dfel dienet ihm anstatt der", "tokens": ["Sein", "ge\u00b7len\u00b7ker", "schlan\u00b7ker", "R\u00fc\u00b7\u00dfel", "die\u00b7net", "ihm", "an\u00b7statt", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Hand,", "tokens": ["Hand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}}}}}