{"dta.poem.21699": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Er.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehe, wie des Geisblatts sich", "tokens": ["Sie\u00b7he", ",", "wie", "des", "Geis\u00b7blatts", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "PRF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Die gewalt'ge Eich' erbarmt;", "tokens": ["Die", "ge\u00b7walt'\u00b7ge", "Eich'", "er\u00b7barmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Epheu br\u00fcnstiglich", "tokens": ["Wie", "der", "E\u00b7pheu", "br\u00fcns\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den erhab'nen Ulm umarmt.", "tokens": ["Den", "er\u00b7hab'\u00b7nen", "Ulm", "um\u00b7armt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebchen komm an meine Brust,", "tokens": ["Lieb\u00b7chen", "komm", "an", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis nicht bl\u00f6de! Bis nicht scheu!", "tokens": ["Bis", "nicht", "bl\u00f6\u00b7de", "!", "Bis", "nicht", "scheu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "$.", "APPR", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um uns jubeln Lieb' und Lust;", "tokens": ["Um", "uns", "ju\u00b7beln", "Lieb'", "und", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um uns scherzt je zwey und zwey.", "tokens": ["Um", "uns", "scherzt", "je", "zwey", "und", "zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ADV", "CARD", "KON", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seiner Sie hofirt der Spatz,", "tokens": ["Sei\u00b7ner", "Sie", "ho\u00b7firt", "der", "Spatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das T\u00e4ubchen kos't und girrt.", "tokens": ["Und", "das", "T\u00e4ub\u00b7chen", "kos't", "und", "girrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur das M\u00e4gdlein sey Niemands Schatz,", "tokens": ["Nur", "das", "M\u00e4gd\u00b7lein", "sey", "Nie\u00b7mands", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "NE", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Bis es beringt vom Priester wird.", "tokens": ["Bis", "es", "be\u00b7ringt", "vom", "Pries\u00b7ter", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Muthe mir nichts Schn\u00f6des an!", "tokens": ["Mu\u00b7the", "mir", "nichts", "Schn\u00f6\u00b7des", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemands Sch\u00e4tzchen will ich seyn,", "tokens": ["Nie\u00b7mands", "Sch\u00e4tz\u00b7chen", "will", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis mich trau't ein geweihter Mann.", "tokens": ["Bis", "mich", "trau't", "ein", "ge\u00b7weih\u00b7ter", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sprichst du Ja, so sprech ich Nein.", "tokens": ["Sprichst", "du", "Ja", ",", "so", "sprech", "ich", "Nein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}