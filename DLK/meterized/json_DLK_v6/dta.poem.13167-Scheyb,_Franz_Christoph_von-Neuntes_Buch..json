{"dta.poem.13167": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Inzwischen stellte sich ein stolzer Herold dar,", "tokens": ["I\u00b7nzwi\u00b7schen", "stell\u00b7te", "sich", "ein", "stol\u00b7zer", "He\u00b7rold", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wodurch die Gegenwart noch mehr erfreuet war.", "tokens": ["Wo\u00b7durch", "die", "Ge\u00b7gen\u00b7wart", "noch", "mehr", "er\u00b7freu\u00b7et", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er trat mit Ansehn auf, als wann er was gebiete;", "tokens": ["Er", "trat", "mit", "An\u00b7sehn", "auf", ",", "als", "wann", "er", "was", "ge\u00b7bie\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "KOUS", "PWAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er gieng mit schwerem Schritt noch nicht bi\u00df in die Mitte,", "tokens": ["Er", "gieng", "mit", "schwe\u00b7rem", "Schritt", "noch", "nicht", "bi\u00df", "in", "die", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Sieh da! welch neuer Glanz kam seiner Ankunft nach,", "tokens": ["Sieh", "da", "!", "welch", "neu\u00b7er", "Glanz", "kam", "sei\u00b7ner", "An\u00b7kunft", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PWAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "330Der jedes Herz ergriff, das Lispeln unterbrach.", "tokens": ["je\u00b7des", "Herz", "er\u00b7griff", ",", "das", "Lis\u00b7peln", "un\u00b7ter\u00b7brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "O Wunder-volle Schaar! man kann es nicht erachten,", "tokens": ["O", "Wun\u00b7der\u00b7vol\u00b7le", "Schaar", "!", "man", "kann", "es", "nicht", "e\u00b7rach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was Pomp und Majest\u00e4t, was Ansehn und was Prachten", "tokens": ["Was", "Pomp", "und", "Ma\u00b7jes\u00b7t\u00e4t", ",", "was", "An\u00b7sehn", "und", "was", "Prach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "$,", "PWS", "VVINF", "KON", "PWS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Hofs Vortrefflichkeit den Augen vorgestellt;", "tokens": ["Des", "Hofs", "Vor\u00b7treff\u00b7lich\u00b7keit", "den", "Au\u00b7gen", "vor\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was vor Annehmlichkeit dem Staat sich beygesellt.", "tokens": ["Was", "vor", "An\u00b7nehm\u00b7lich\u00b7keit", "dem", "Staat", "sich", "bey\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "335Ein Chor von z\u00e4rtlichstem geschm\u00fccktem Frauenzimmer", "tokens": ["Chor", "von", "z\u00e4rt\u00b7lichs\u00b7tem", "ge\u00b7schm\u00fcck\u00b7tem", "Frau\u00b7en\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Verdoppelte die Lust durch ihrer Kleider Schimmer:", "tokens": ["Ver\u00b7dop\u00b7pel\u00b7te", "die", "Lust", "durch", "ih\u00b7rer", "Klei\u00b7der", "Schim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drauf kam das Kronen-Haus, mit ihm ", "tokens": ["Drauf", "kam", "das", "Kro\u00b7nen\u00b7Haus", ",", "mit", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "An der man gleich den Werth der wahren Hoheit sah.", "tokens": ["An", "der", "man", "gleich", "den", "Werth", "der", "wah\u00b7ren", "Ho\u00b7heit", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}