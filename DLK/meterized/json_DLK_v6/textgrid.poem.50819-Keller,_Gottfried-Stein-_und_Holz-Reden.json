{"textgrid.poem.50819": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Stein- und Holz-Reden", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der L\u00fcneburger Heiden,", "tokens": ["Auf", "der", "L\u00fc\u00b7ne\u00b7bur\u00b7ger", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da steht ein alter Stein,", "tokens": ["Da", "steht", "ein", "al\u00b7ter", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dabei eine alte Eiche,", "tokens": ["Da\u00b7bei", "ei\u00b7ne", "al\u00b7te", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mag wohl tausendj\u00e4hrig sein!", "tokens": ["Die", "mag", "wohl", "tau\u00b7send\u00b7j\u00e4h\u00b7rig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ziehn vor\u00fcber Gesellen", "tokens": ["Es", "ziehn", "vor\u00b7\u00fc\u00b7ber", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zwei oder drei mit Sang,", "tokens": ["Zwei", "o\u00b7der", "drei", "mit", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die singen von deutscher Freiheit,", "tokens": ["Die", "sin\u00b7gen", "von", "deut\u00b7scher", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf weiter Heid' verhallt der Klang!", "tokens": ["Auf", "wei\u00b7ter", "Heid'", "ver\u00b7hallt", "der", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da spricht der Stein zur Eiche,", "tokens": ["Da", "spricht", "der", "Stein", "zur", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie erwacht vom Traum:", "tokens": ["Als", "wie", "er\u00b7wacht", "vom", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbging nicht die Freiheit vor\u00fcber?", "tokens": ["\u00bb", "ging", "nicht", "die", "Frei\u00b7heit", "vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wach auf! wach auf, du deutscher Baum!\u00ab", "tokens": ["Wach", "auf", "!", "wach", "auf", ",", "du", "deut\u00b7scher", "Baum", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und durch die Krone fahre", "tokens": ["Und", "durch", "die", "Kro\u00b7ne", "fah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein lauter Saus und Braus,", "tokens": ["Ein", "lau\u00b7ter", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schlagen die moosigen Zweige", "tokens": ["Es", "schla\u00b7gen", "die", "moo\u00b7si\u00b7gen", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In tausend gr\u00fcne Bl\u00e4tter aus.", "tokens": ["In", "tau\u00b7send", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Gesellen sind gezogen", "tokens": ["Die", "Ge\u00b7sel\u00b7len", "sind", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon fern durchs Heidekraut!", "tokens": ["Schon", "fern", "durchs", "Hei\u00b7de\u00b7kraut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die Eiche hat ihnen", "tokens": ["Und", "die", "Ei\u00b7che", "hat", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gar bang und traurig nachgeschaut!", "tokens": ["Gar", "bang", "und", "trau\u00b7rig", "nach\u00b7ge\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es kreischen ein paar M\u00f6wen", "tokens": ["Es", "krei\u00b7schen", "ein", "paar", "M\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verd\u00e4chtig hin und her,", "tokens": ["Ver\u00b7d\u00e4ch\u00b7tig", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die machen der grauen Eichen", "tokens": ["Die", "ma\u00b7chen", "der", "grau\u00b7en", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Herz so d\u00fcster und so schwer!", "tokens": ["Das", "Herz", "so", "d\u00fcs\u00b7ter", "und", "so", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbnun will ich wiederum schlafen\u00ab,", "tokens": ["\u00bb", "nun", "will", "ich", "wie\u00b7de\u00b7rum", "schla\u00b7fen", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$(", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Spricht sie zum alten Stein,", "tokens": ["Spricht", "sie", "zum", "al\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdu wunderlicher Tr\u00e4umer,", "tokens": ["\u00bb", "du", "wun\u00b7der\u00b7li\u00b7cher", "Tr\u00e4u\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sollst mir nun einmal ruhig sein!\u00ab", "tokens": ["Sollst", "mir", "nun", "ein\u00b7mal", "ru\u00b7hig", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}