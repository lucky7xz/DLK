{"textgrid.poem.61265": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4r' es eine Schuld nur, was uns trennte!", "tokens": ["O", "w\u00e4r'", "es", "ei\u00b7ne", "Schuld", "nur", ",", "was", "uns", "trenn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00fcnde vorn sie in der S\u00fcnden Reihen:", "tokens": ["Und", "st\u00fcn\u00b7de", "vorn", "sie", "in", "der", "S\u00fcn\u00b7den", "Rei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lieb' ist Gnad' und k\u00f6nnte sie verzeihen,", "tokens": ["Die", "Lieb'", "ist", "Gnad'", "und", "k\u00f6nn\u00b7te", "sie", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie im andern nur die Lieb' erkennte.", "tokens": ["Wenn", "sie", "im", "an\u00b7dern", "nur", "die", "Lieb'", "er\u00b7kenn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wo ist Feuer, das im Wasser brennte?", "tokens": ["Doch", "wo", "ist", "Feu\u00b7er", ",", "das", "im", "Was\u00b7ser", "brenn\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Wasser, das in Flammen mag gedeihen?", "tokens": ["Wo", "Was\u00b7ser", ",", "das", "in", "Flam\u00b7men", "mag", "ge\u00b7dei\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was uns f\u00fcr heut und immer mu\u00df entzweien,", "tokens": ["Was", "uns", "f\u00fcr", "heut", "und", "im\u00b7mer", "mu\u00df", "ent\u00b7zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADV", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Widerspruch wie der der Elemente.", "tokens": ["Ist", "Wi\u00b7der\u00b7spruch", "wie", "der", "der", "E\u00b7le\u00b7men\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KOKOM", "ART", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du folgtest deinen Sternen, ich den meinen -", "tokens": ["Du", "folg\u00b7test", "dei\u00b7nen", "Ster\u00b7nen", ",", "ich", "den", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "ART", "PPOSAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit man uns schied - im Glauben, Denken, Lieben.", "tokens": ["Seit", "man", "uns", "schied", "im", "Glau\u00b7ben", ",", "Den\u00b7ken", ",", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "$(", "APPRART", "NN", "$,", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, da\u00df die Sterne so verschieden scheinen!", "tokens": ["Ach", ",", "da\u00df", "die", "Ster\u00b7ne", "so", "ver\u00b7schie\u00b7den", "schei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun mu\u00df dein Wort mir, leerer Schall, verstieben,", "tokens": ["Nun", "mu\u00df", "dein", "Wort", "mir", ",", "lee\u00b7rer", "Schall", ",", "ver\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meines dir. Wir aber stehn und weinen,", "tokens": ["Und", "mei\u00b7nes", "dir", ".", "Wir", "a\u00b7ber", "stehn", "und", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PPER", "$.", "PPER", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nichts gemein uns als dies Leid geblieben.", "tokens": ["Da\u00df", "nichts", "ge\u00b7mein", "uns", "als", "dies", "Leid", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "KOKOM", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}