{"textgrid.poem.56522": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne hat ihn gesotten,", "tokens": ["Son\u00b7ne", "hat", "ihn", "ge\u00b7sot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wind hat ihn d\u00fcrr gemacht,", "tokens": ["Wind", "hat", "ihn", "d\u00fcrr", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kein Baum wollte ihn haben,", "tokens": ["Kein", "Baum", "woll\u00b7te", "ihn", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00dcberall fiel er ab.", "tokens": ["\u00dc\u00b7be\u00b7rall", "fiel", "er", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Nur eine Eberesche", "tokens": ["Nur", "ei\u00b7ne", "E\u00b7be\u00b7re\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit roten Beeren bespickt", "tokens": ["Mit", "ro\u00b7ten", "Bee\u00b7ren", "be\u00b7spickt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie mit feurigen Zungen,", "tokens": ["Wie", "mit", "feu\u00b7ri\u00b7gen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hat ihm Obdach gegeben.", "tokens": ["Hat", "ihm", "Ob\u00b7dach", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und da hing er mit Schweben,", "tokens": ["Und", "da", "hing", "er", "mit", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seine F\u00fc\u00dfe lagen im Gras.", "tokens": ["Sei\u00b7ne", "F\u00fc\u00b7\u00dfe", "la\u00b7gen", "im", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Abendsonne fuhr blutig", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "fuhr", "blu\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch die Rippen ihm na\u00df,", "tokens": ["Durch", "die", "Rip\u00b7pen", "ihm", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Schlug die \u00d6lw\u00e4lder alle", "tokens": ["Schlug", "die", "\u00d6l\u00b7w\u00e4l\u00b7der", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PIAT"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "\u00dcber der Landschaft herauf,", "tokens": ["\u00dc\u00b7ber", "der", "Land\u00b7schaft", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gott in dem wei\u00dfen Kleide", "tokens": ["Gott", "in", "dem", "wei\u00b7\u00dfen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Tat in den Wolken sich auf.", "tokens": ["Tat", "in", "den", "Wol\u00b7ken", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "In den blumigen Gr\u00fcnden", "tokens": ["In", "den", "blu\u00b7mi\u00b7gen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ringelte Schlangengez\u00fccht,", "tokens": ["Rin\u00b7gel\u00b7te", "Schlan\u00b7gen\u00b7ge\u00b7z\u00fccht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "In den silbernen H\u00e4lsen", "tokens": ["In", "den", "sil\u00b7ber\u00b7nen", "H\u00e4l\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zwitscherte d\u00fcnnes Ger\u00fccht.", "tokens": ["Zwit\u00b7scher\u00b7te", "d\u00fcn\u00b7nes", "Ge\u00b7r\u00fccht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Und sie zitterten alle", "tokens": ["Und", "sie", "zit\u00b7ter\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIS"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u00dcber dem Bl\u00e4tterreich,", "tokens": ["\u00dc\u00b7ber", "dem", "Bl\u00e4t\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "H\u00f6rend die H\u00e4nde des Vaters", "tokens": ["H\u00f6\u00b7rend", "die", "H\u00e4n\u00b7de", "des", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Im hellen Ge\u00e4der leicht.", "tokens": ["Im", "hel\u00b7len", "Ge\u00b7\u00e4\u00b7der", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}