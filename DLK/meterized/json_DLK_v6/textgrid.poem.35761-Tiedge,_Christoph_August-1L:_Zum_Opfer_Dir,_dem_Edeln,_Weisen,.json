{"textgrid.poem.35761": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Opfer Dir, dem Edeln, Weisen,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Opfer Dir, dem Edeln, Weisen,", "tokens": ["Zum", "Op\u00b7fer", "Dir", ",", "dem", "E\u00b7deln", ",", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den heut' in Emmas Hain ein sch\u00f6ner Altar ehrt,", "tokens": ["Den", "heut'", "in", "Em\u00b7mas", "Hain", "ein", "sch\u00f6\u00b7ner", "Al\u00b7tar", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Lieder, welche Dich Urania gelehrt,", "tokens": ["Und", "Lie\u00b7der", ",", "wel\u00b7che", "Dich", "U\u00b7ra\u00b7nia", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mehr, als des Freundes Lieder, preisen;", "tokens": ["Mehr", ",", "als", "des", "Freun\u00b7des", "Lie\u00b7der", ",", "prei\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dir, Freund, der zwischen zwei Unsterblichkeiten steht,", "tokens": ["Dir", ",", "Freund", ",", "der", "zwi\u00b7schen", "zwei", "U\u00b7nsterb\u00b7lich\u00b7kei\u00b7ten", "steht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PRELS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mit einem Kranz, den in geweihten Stunden,", "tokens": ["Mit", "ei\u00b7nem", "Kranz", ",", "den", "in", "ge\u00b7weih\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vom Hauche der Begeistrung angeweht,", "tokens": ["Vom", "Hau\u00b7che", "der", "Be\u00b7geis\u00b7trung", "an\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Muse Halladats um Deine Stirn gewunden;", "tokens": ["Die", "Mu\u00b7se", "Hal\u00b7la\u00b7dats", "um", "Dei\u00b7ne", "Stirn", "ge\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O Dir, Du S\u00e4nger Gottes, weiht", "tokens": ["O", "Dir", ",", "Du", "S\u00e4n\u00b7ger", "Got\u00b7tes", ",", "weiht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "$,", "PPER", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich dieses Lied von Gott und der Unsterblichkeit.", "tokens": ["Sich", "die\u00b7ses", "Lied", "von", "Gott", "und", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "NN", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}