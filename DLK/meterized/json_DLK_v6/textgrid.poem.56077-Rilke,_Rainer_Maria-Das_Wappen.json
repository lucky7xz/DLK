{"textgrid.poem.56077": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Wappen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Spiegel, der, von ferne tragend,", "tokens": ["Wie", "ein", "Spie\u00b7gel", ",", "der", ",", "von", "fer\u00b7ne", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "$,", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "lautlos in sich aufnahm, ist der Schild;", "tokens": ["laut\u00b7los", "in", "sich", "auf\u00b7nahm", ",", "ist", "der", "Schild", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "offen einstens, dann zusammenschlagend", "tokens": ["of\u00b7fen", "eins\u00b7tens", ",", "dann", "zu\u00b7sam\u00b7men\u00b7schla\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00fcber einem Spiegelbild", "tokens": ["\u00fc\u00b7ber", "ei\u00b7nem", "Spie\u00b7gel\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "jener Wesen, die in des Geschlechts", "tokens": ["je\u00b7ner", "We\u00b7sen", ",", "die", "in", "des", "Ge\u00b7schlechts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Weiten wohnen, nicht mehr zu bestreiten,", "tokens": ["Wei\u00b7ten", "woh\u00b7nen", ",", "nicht", "mehr", "zu", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "seiner Dinge, seiner Wirklichkeiten", "tokens": ["sei\u00b7ner", "Din\u00b7ge", ",", "sei\u00b7ner", "Wirk\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "(rechte links und linke rechts),", "tokens": ["(", "rech\u00b7te", "links", "und", "lin\u00b7ke", "rechts", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "die er eingesteht und sagt und zeigt.", "tokens": ["die", "er", "ein\u00b7ge\u00b7steht", "und", "sagt", "und", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Drauf, mit Ruhm und Dunkel ausgeschlagen,", "tokens": ["Drauf", ",", "mit", "Ruhm", "und", "Dun\u00b7kel", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ruht der Spangenhelm, verk\u00fcrzt,", "tokens": ["ruht", "der", "Span\u00b7gen\u00b7helm", ",", "ver\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "den das Fl\u00fcgelkleinod \u00fcbersteigt,", "tokens": ["den", "das", "Fl\u00fc\u00b7gel\u00b7klei\u00b7nod", "\u00fc\u00b7bers\u00b7teigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "w\u00e4hrend seine Decke, wie mit Klagen,", "tokens": ["w\u00e4h\u00b7rend", "sei\u00b7ne", "De\u00b7cke", ",", "wie", "mit", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "reich und aufgeregt herniederst\u00fcrzt.", "tokens": ["reich", "und", "auf\u00b7ge\u00b7regt", "her\u00b7nie\u00b7ders\u00b7t\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}