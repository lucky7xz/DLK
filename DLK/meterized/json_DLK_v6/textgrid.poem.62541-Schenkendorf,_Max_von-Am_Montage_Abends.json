{"textgrid.poem.62541": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Montage Abends", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dir leb' ich,", "tokens": ["Mit", "dir", "leb'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.2": {"text": "In dir sterb' ich,", "tokens": ["In", "dir", "sterb'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bei dir will ich ewig sein.", "tokens": ["Bei", "dir", "will", "ich", "e\u00b7wig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Sinnen", "tokens": ["Wenn", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mir zerrinnen", "tokens": ["Mir", "zer\u00b7rin\u00b7nen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Hilf mir in der letzten Pein.", "tokens": ["Hilf", "mir", "in", "der", "letz\u00b7ten", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gib, da\u00df ich in Frieden ende", "tokens": ["Gib", ",", "da\u00df", "ich", "in", "Frie\u00b7den", "en\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinen Pilgerlauf;", "tokens": ["Mei\u00b7nen", "Pil\u00b7ger\u00b7lauf", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und in deine treuen H\u00e4nde", "tokens": ["Und", "in", "dei\u00b7ne", "treu\u00b7en", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nimm die matte Seele auf.", "tokens": ["Nimm", "die", "mat\u00b7te", "See\u00b7le", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}