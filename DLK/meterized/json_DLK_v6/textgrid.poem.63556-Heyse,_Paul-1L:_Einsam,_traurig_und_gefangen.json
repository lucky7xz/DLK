{"textgrid.poem.63556": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam, traurig und gefangen", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam, traurig und gefangen", "tokens": ["Ein\u00b7sam", ",", "trau\u00b7rig", "und", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt der kleine Gott zu Haus,", "tokens": ["Sitzt", "der", "klei\u00b7ne", "Gott", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit na\u00dfgeweinten Wangen", "tokens": ["Und", "mit", "na\u00df\u00b7ge\u00b7wein\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rupft er sich die Federn aus;", "tokens": ["Rupft", "er", "sich", "die", "Fe\u00b7dern", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spitzt sie fein an seinen Pfeilen,", "tokens": ["Spitzt", "sie", "fein", "an", "sei\u00b7nen", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taucht sie in ein Tr\u00f6pfchen Blut,", "tokens": ["Taucht", "sie", "in", "ein", "Tr\u00f6pf\u00b7chen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schreibt damit entflammte Zeilen,", "tokens": ["Schreibt", "da\u00b7mit", "ent\u00b7flamm\u00b7te", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brief' und Lieder voller Glut.", "tokens": ["Brie\u00b7f'", "und", "Lie\u00b7der", "vol\u00b7ler", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ach, und kann's ihm denn gen\u00fcgen,", "tokens": ["Ach", ",", "und", "kann's", "ihm", "denn", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er lahm die Feder f\u00fchrt,", "tokens": ["Da\u00df", "er", "lahm", "die", "Fe\u00b7der", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da er einst in sel'gen Fl\u00fcgen", "tokens": ["Da", "er", "einst", "in", "sel'\u00b7gen", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zweier Schwingen Kraft gesp\u00fcrt?", "tokens": ["Zwei\u00b7er", "Schwin\u00b7gen", "Kraft", "ge\u00b7sp\u00fcrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heil'ge Venus, la\u00df geschwinde", "tokens": ["Heil'\u00b7ge", "Ve\u00b7nus", ",", "la\u00df", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingehn diese Mauserzeit,", "tokens": ["Hin\u00b7gehn", "die\u00b7se", "Mau\u00b7ser\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dem armen G\u00f6tterkinde", "tokens": ["Die", "dem", "ar\u00b7men", "G\u00f6t\u00b7ter\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sichtbar k\u00fcmmerlich gedeiht.", "tokens": ["Sicht\u00b7bar", "k\u00fcm\u00b7mer\u00b7lich", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Neu beschwing ihm das Gefieder,", "tokens": ["Neu", "be\u00b7schwing", "ihm", "das", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nun kriechend kritzeln mu\u00df:", "tokens": ["Das", "nun", "krie\u00b7chend", "krit\u00b7zeln", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blick und Wort statt Brief' und Lieder,", "tokens": ["Blick", "und", "Wort", "statt", "Brie\u00b7f'", "und", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Statt der Siegel Ku\u00df um Ku\u00df!", "tokens": ["Statt", "der", "Sie\u00b7gel", "Ku\u00df", "um", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}