{"dta.poem.18697": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Reitest du bey e\u2019nem Schmied vorbey,", "tokens": ["Rei\u00b7test", "du", "bey", "e'\u00b7nem", "Schmied", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Weisst nicht wann er dein Pferd beschl\u00e4gt;", "tokens": ["Weisst", "nicht", "wann", "er", "dein", "Pferd", "be\u00b7schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Siehst du eine H\u00fctte im Felde frey,", "tokens": ["Siehst", "du", "ei\u00b7ne", "H\u00fct\u00b7te", "im", "Fel\u00b7de", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weisst nicht ob sie dir ein Liebchen hegt;", "tokens": ["Weisst", "nicht", "ob", "sie", "dir", "ein", "Lieb\u00b7chen", "hegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Einem J\u00fcngling begegnest du sch\u00f6n und k\u00fchn,", "tokens": ["Ei\u00b7nem", "J\u00fcng\u00b7ling", "be\u00b7geg\u00b7nest", "du", "sch\u00f6n", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Er \u00fcberwindet dich k\u00fcnftig oder du ihn.", "tokens": ["Er", "\u00fc\u00b7berw\u00b7in\u00b7det", "dich", "k\u00fcnf\u00b7tig", "o\u00b7der", "du", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "PPER", "PPER", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Am sichersten kannst du vom Rebstock sagen", "tokens": ["Am", "si\u00b7chers\u00b7ten", "kannst", "du", "vom", "Reb\u00b7stock", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Er werde f\u00fcr dich was Gutes tragen.", "tokens": ["Er", "wer\u00b7de", "f\u00fcr", "dich", "was", "Gu\u00b7tes", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "So bist du denn der Welt empfohlen,", "tokens": ["So", "bist", "du", "denn", "der", "Welt", "emp\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Uebrige will ich nicht wiederholen.", "tokens": ["Das", "Ueb\u00b7ri\u00b7ge", "will", "ich", "nicht", "wie\u00b7der\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}