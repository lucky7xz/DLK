{"textgrid.poem.41109": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich auch noch so alt geworden,", "tokens": ["Bin", "ich", "auch", "noch", "so", "alt", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Starb doch die junge Liebe nicht,", "tokens": ["Starb", "doch", "die", "jun\u00b7ge", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gern, wie in der fr\u00fchsten Jugend,", "tokens": ["Und", "gern", ",", "wie", "in", "der", "fr\u00fchs\u00b7ten", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seh' ich dir noch ins Angesicht.", "tokens": ["Seh'", "ich", "dir", "noch", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja lieber noch: denn was uns freute,", "tokens": ["Ja", "lie\u00b7ber", "noch", ":", "denn", "was", "uns", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "$.", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und was uns schmerzte, liegt nun hier,", "tokens": ["Und", "was", "uns", "schmerz\u00b7te", ",", "liegt", "nun", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es singt nicht mehr blo\u00df Fr\u00fchlingsz\u00fcge,", "tokens": ["Es", "singt", "nicht", "mehr", "blo\u00df", "Fr\u00fch\u00b7lings\u00b7z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Und wie nach noch so vielen Wettern", "tokens": ["Und", "wie", "nach", "noch", "so", "vie\u00b7len", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Stern in gleichem Lichte scheint,", "tokens": ["Ein", "Stern", "in", "glei\u00b7chem", "Lich\u00b7te", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So blieb dein Aug' das alte, klare,", "tokens": ["So", "blieb", "dein", "Aug'", "das", "al\u00b7te", ",", "kla\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du's auch oftmals tr\u00fcb geweint.", "tokens": ["Hast", "du's", "auch", "oft\u00b7mals", "tr\u00fcb", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}