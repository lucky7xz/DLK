{"dta.poem.3415": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Von Ida .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen bl&#252;ht;", "tokens": ["Der", "Mor\u00b7gen", "bl", "&#252;", "ht", ";"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Osten gl\u00fcht;", "tokens": ["Der", "Os\u00b7ten", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es l\u00e4chelt aus dem d\u00fcnnen Flor", "tokens": ["Es", "l\u00e4\u00b7chelt", "aus", "dem", "d\u00fcn\u00b7nen", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne matt und krank hervor.", "tokens": ["Die", "Son\u00b7ne", "matt", "und", "krank", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ach, mein Liebling flieht!", "tokens": ["Denn", "ach", ",", "mein", "Lieb\u00b7ling", "flieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf welcher Flur,", "tokens": ["Auf", "wel\u00b7cher", "Flur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auf wessen Spur,", "tokens": ["Auf", "wes\u00b7sen", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So fern von Iden wallst du itzt,", "tokens": ["So", "fern", "von", "I\u00b7den", "wallst", "du", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O du, der ganz mein Herz besitzt,", "tokens": ["O", "du", ",", "der", "ganz", "mein", "Herz", "be\u00b7sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Liebling der Natur?", "tokens": ["Du", "Lieb\u00b7ling", "der", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vernimmst du auch", "tokens": ["Ver\u00b7nimmst", "du", "auch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im Morgenhauch", "tokens": ["Im", "Mor\u00b7gen\u00b7hauch"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Ach, das Idens Brust ent\u00e4chzt,", "tokens": ["Das", "Ach", ",", "das", "I\u00b7dens", "Brust", "en\u00b7t\u00e4chzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Sehnen, drin ihr Herz zerlechzt,", "tokens": ["Das", "Seh\u00b7nen", ",", "drin", "ihr", "Herz", "zer\u00b7lechzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im k\u00fchlen Morgenhauch?", "tokens": ["Im", "k\u00fch\u00b7len", "Mor\u00b7gen\u00b7hauch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was ahnest du,", "tokens": ["Was", "ah\u00b7nest", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Idens Ruh", "tokens": ["Der", "I\u00b7dens", "Ruh"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Idens Freuden mit sich nahm?", "tokens": ["Und", "I\u00b7dens", "Freu\u00b7den", "mit", "sich", "nahm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, ahnest du wohl Idens Gram,", "tokens": ["Ach", ",", "ah\u00b7nest", "du", "wohl", "I\u00b7dens", "Gram", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und flehst f\u00fcr Idens Ruh?", "tokens": ["Und", "flehst", "f\u00fcr", "I\u00b7dens", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O kehre um!", "tokens": ["O", "keh\u00b7re", "um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kehr um, kehr um!", "tokens": ["Kehr", "um", ",", "kehr", "um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu deiner Einsamtraurenden!", "tokens": ["Zu", "dei\u00b7ner", "Ein\u00b7samt\u00b7rau\u00b7ren\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zu deiner Ahnungschaurenden!", "tokens": ["Zu", "dei\u00b7ner", "Ah\u00b7nung\u00b7schau\u00b7ren\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Mein Einziger, kehr' um!", "tokens": ["Mein", "Ein\u00b7zi\u00b7ger", ",", "kehr'", "um", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}