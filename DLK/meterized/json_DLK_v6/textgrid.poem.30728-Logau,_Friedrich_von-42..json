{"textgrid.poem.30728": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man mahlt den blassen Neid mit Br\u00fcsten, die verschrumpen;", "tokens": ["Man", "mahlt", "den", "blas\u00b7sen", "Neid", "mit", "Br\u00fcs\u00b7ten", ",", "die", "ver\u00b7schrum\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es eyffert sich der Neid um Sachen, die sich lumpen.", "tokens": ["Es", "eyf\u00b7fert", "sich", "der", "Neid", "um", "Sa\u00b7chen", ",", "die", "sich", "lum\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}