{"textgrid.poem.34371": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Freundinn f\u00fchlen Sie die Wunde", "tokens": ["Wie", "Freun\u00b7dinn", "f\u00fch\u00b7len", "Sie", "die", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht dem Gatten blos, auch mir das Schicksal schlug!", "tokens": ["Die", "nicht", "dem", "Gat\u00b7ten", "blos", ",", "auch", "mir", "das", "Schick\u00b7sal", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "ADV", "$,", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir der nur Zeuge war von mancher frohen Stunde", "tokens": ["Mir", "der", "nur", "Zeu\u00b7ge", "war", "von", "man\u00b7cher", "fro\u00b7hen", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADV", "NN", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von jedem Wort aus ihrem Munde", "tokens": ["Von", "je\u00b7dem", "Wort", "aus", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das das Gepr\u00e4g der innern Gr\u00f6sse trug.", "tokens": ["Das", "das", "Ge\u00b7pr\u00e4g", "der", "in\u00b7nern", "Gr\u00f6s\u00b7se", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ganz von der armen Welt vergessen,", "tokens": ["Ganz", "von", "der", "ar\u00b7men", "Welt", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie oft hat sie begl\u00fcckt durch sich", "tokens": ["Wie", "oft", "hat", "sie", "be\u00b7gl\u00fcckt", "durch", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf seinem Schoo\u00df mit Siegerstolz gesessen!", "tokens": ["Auf", "sei\u00b7nem", "Schoo\u00df", "mit", "Sie\u00b7ger\u00b7stolz", "ge\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ach und ihr Blick erw\u00e4rmt' auch mich.", "tokens": ["Ach", "und", "ihr", "Blick", "er\u00b7w\u00e4rmt'", "auch", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PPOSAT", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auch ich auch ich im seeligsten Momente", "tokens": ["Auch", "ich", "auch", "ich", "im", "see\u00b7ligs\u00b7ten", "Mo\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Schlug eine z\u00e4rtliche Tangente", "tokens": ["Schlug", "ei\u00b7ne", "z\u00e4rt\u00b7li\u00b7che", "Tan\u00b7gen\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Zur grossen Harmonie in ihrem Herzen an", "tokens": ["Zur", "gros\u00b7sen", "Har\u00b7mo\u00b7nie", "in", "ih\u00b7rem", "Her\u00b7zen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit ihrem Bruder, ihrem Mann.", "tokens": ["Mit", "ih\u00b7rem", "Bru\u00b7der", ",", "ih\u00b7rem", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie hob mich das Gef\u00fchl auf Engelschwingen", "tokens": ["Wie", "hob", "mich", "das", "Ge\u00b7f\u00fchl", "auf", "En\u00b7gel\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Zu edlern Neigungen empor,", "tokens": ["Zu", "ed\u00b7lern", "Nei\u00b7gun\u00b7gen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Wie warnt' es mich bey allzu feinen Schlingen,", "tokens": ["Wie", "warnt'", "es", "mich", "bey", "all\u00b7zu", "fei\u00b7nen", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "APPR", "PTKA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df ich nie meinen Werth verlohr.", "tokens": ["Da\u00df", "ich", "nie", "mei\u00b7nen", "Werth", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mein Schutzgeist ist dahin, die Gottheit die mich f\u00fchrte", "tokens": ["Mein", "Schutz\u00b7geist", "ist", "da\u00b7hin", ",", "die", "Got\u00b7theit", "die", "mich", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV", "$,", "ART", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Am Rande jeglicher Gefahr,", "tokens": ["Am", "Ran\u00b7de", "jeg\u00b7li\u00b7cher", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und wenn mein Herz erstorben war", "tokens": ["Und", "wenn", "mein", "Herz", "ers\u00b7tor\u00b7ben", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Gottheit die es wieder r\u00fchrte.", "tokens": ["Die", "Got\u00b7theit", "die", "es", "wie\u00b7der", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ihr zart Gef\u00fchl das jeden Mi\u00dflaut sp\u00fchrte", "tokens": ["Ihr", "zart", "Ge\u00b7f\u00fchl", "das", "je\u00b7den", "Mi\u00df\u00b7laut", "sp\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Litt auch kein Wort, auch keinen Blick", "tokens": ["Litt", "auch", "kein", "Wort", ",", "auch", "kei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der nicht der Wahrheit Stempel f\u00fchrte.", "tokens": ["Der", "nicht", "der", "Wahr\u00b7heit", "Stem\u00b7pel", "f\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ach diese Streng' allein erh\u00e4lt das reinste Gl\u00fcck", "tokens": ["Ach", "die\u00b7se", "Streng'", "al\u00b7lein", "er\u00b7h\u00e4lt", "das", "reins\u00b7te", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PDAT", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ohne sie sind Freundschaftliche Triebe,", "tokens": ["Und", "oh\u00b7ne", "sie", "sind", "Freund\u00b7schaft\u00b7li\u00b7che", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ist selbst der h\u00f6chste Rausch der Liebe", "tokens": ["Ist", "selbst", "der", "h\u00f6chs\u00b7te", "Rausch", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Nur Mummerey die uns entehrt,", "tokens": ["Nur", "Mum\u00b7me\u00b7rey", "die", "uns", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Nicht ihres sch\u00f6nen Namens werth.", "tokens": ["Nicht", "ih\u00b7res", "sch\u00f6\u00b7nen", "Na\u00b7mens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie wenn ich itzt mein k\u00fcnftig Gl\u00fcck beschriebe?", "tokens": ["Wie", "wenn", "ich", "itzt", "mein", "k\u00fcnf\u00b7tig", "Gl\u00fcck", "be\u00b7schrie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie wenn mir das an Ihnen bliebe", "tokens": ["Wie", "wenn", "mir", "das", "an", "Ih\u00b7nen", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "PDS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrtrefliche! was ich an ihr verlor?", "tokens": ["F\u00fcr\u00b7tre\u00b7fli\u00b7che", "!", "was", "ich", "an", "ihr", "ver\u00b7lor", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wenn mir die Seelige in der Verkl\u00e4rten Chor", "tokens": ["Wenn", "mir", "die", "See\u00b7li\u00b7ge", "in", "der", "Ver\u00b7kl\u00e4r\u00b7ten", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Sie selber dazu auserkohr?", "tokens": ["Sie", "sel\u00b7ber", "da\u00b7zu", "au\u00b7ser\u00b7kohr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O womit dankt' ich ihr und Ihnen?", "tokens": ["O", "wo\u00b7mit", "dankt'", "ich", "ihr", "und", "Ih\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "PPER", "KON", "PPER", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Womit, womit k\u00f6nnt ich dies Gl\u00fcck verdienen,", "tokens": ["Wo\u00b7mit", ",", "wo\u00b7mit", "k\u00f6nnt", "ich", "dies", "Gl\u00fcck", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "VVFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Freundschaft unverd\u00e4chtig Gl\u00fcck,", "tokens": ["Der", "Freund\u00b7schaft", "un\u00b7ver\u00b7d\u00e4ch\u00b7tig", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die nur dem Werth den sie am andern kannte", "tokens": ["Die", "nur", "dem", "Werth", "den", "sie", "am", "an\u00b7dern", "kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ART", "PPER", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und seiner Dauer nur den liebevollen Blick", "tokens": ["Und", "sei\u00b7ner", "Dau\u00b7er", "nur", "den", "lie\u00b7be\u00b7vol\u00b7len", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und mit ihm Himmelsfreuden sandte.", "tokens": ["Und", "mit", "ihm", "Him\u00b7mels\u00b7freu\u00b7den", "sand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}