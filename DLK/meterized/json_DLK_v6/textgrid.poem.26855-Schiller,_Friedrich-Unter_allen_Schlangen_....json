{"textgrid.poem.26855": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unter allen Schlangen ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter allen Schlangen ist ", "tokens": ["Un\u00b7ter", "al\u00b7len", "Schlan\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Erden nicht gezeugt,", "tokens": ["Auf", "Er\u00b7den", "nicht", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der an Schnelle keine,", "tokens": ["Mit", "der", "an", "Schnel\u00b7le", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Wut sich keine vergleicht.", "tokens": ["An", "Wut", "sich", "kei\u00b7ne", "ver\u00b7gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PIAT", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie st\u00fcrzt mit furchtbarer Stimme", "tokens": ["Sie", "st\u00fcrzt", "mit", "furcht\u00b7ba\u00b7rer", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf ihren Raub sich los,", "tokens": ["Auf", "ih\u00b7ren", "Raub", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vertilgt in ", "tokens": ["Ver\u00b7tilgt", "in"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den Reiter und sein Ro\u00df.", "tokens": ["Den", "Rei\u00b7ter", "und", "sein", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie liebt die h\u00f6chsten Spitzen,", "tokens": ["Sie", "liebt", "die", "h\u00f6chs\u00b7ten", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht Schlo\u00df, nicht Riegel kann", "tokens": ["Nicht", "Schlo\u00df", ",", "nicht", "Rie\u00b7gel", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor ihrem Anfall sch\u00fctzen,", "tokens": ["Vor", "ih\u00b7rem", "An\u00b7fall", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Harnisch \u2013 lockt sie an.", "tokens": ["Der", "Har\u00b7nisch", "\u2013", "lockt", "sie", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie bricht wie d\u00fcnne Halmen", "tokens": ["Sie", "bricht", "wie", "d\u00fcn\u00b7ne", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den st\u00e4rksten Baum entzwei,", "tokens": ["Den", "st\u00e4rks\u00b7ten", "Baum", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kann das Erz zermalmen,", "tokens": ["Sie", "kann", "das", "Erz", "zer\u00b7mal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie dicht und fest es sei.", "tokens": ["Wie", "dicht", "und", "fest", "es", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und dieses Ungeheuer", "tokens": ["Und", "die\u00b7ses", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat zweimal nur gedroht \u2013", "tokens": ["Hat", "zwei\u00b7mal", "nur", "ge\u00b7droht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es stirbt im eignen Feuer,", "tokens": ["Es", "stirbt", "im", "eig\u00b7nen", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's t\u00f6tet, ist es tot!", "tokens": ["Wie's", "t\u00f6\u00b7tet", ",", "ist", "es", "tot", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Diese Schlange, der an Schnelle keine gleicht,", "tokens": ["Die\u00b7se", "Schlan\u00b7ge", ",", "der", "an", "Schnel\u00b7le", "kei\u00b7ne", "gleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "NN", "PIAT", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die aus der H\u00f6he schie\u00dft, die st\u00e4rksten Eichen", "tokens": ["Die", "aus", "der", "H\u00f6\u00b7he", "schie\u00dft", ",", "die", "st\u00e4rks\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie d\u00fcnnes Rohr zerbricht, durch Schlo\u00df und Riegel dringt,", "tokens": ["Wie", "d\u00fcn\u00b7nes", "Rohr", "zer\u00b7bricht", ",", "durch", "Schlo\u00df", "und", "Rie\u00b7gel", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor der kein Harnisch kann besch\u00fctzen,", "tokens": ["Vor", "der", "kein", "Har\u00b7nisch", "kann", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sich in eignem Feuer selbst verzehrt,", "tokens": ["Die", "sich", "in", "eig\u00b7nem", "Feu\u00b7er", "selbst", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u2013 Es ist der ", "tokens": ["\u2013", "Es", "ist", "der"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}