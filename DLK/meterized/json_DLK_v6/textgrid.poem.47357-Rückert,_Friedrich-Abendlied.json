{"textgrid.poem.47357": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr mit dem Odem linde", "tokens": ["Die", "ihr", "mit", "dem", "O\u00b7dem", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Bl\u00fcmchen k\u00fc\u00dft und gr\u00fc\u00dft,", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", "k\u00fc\u00dft", "und", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt mir, laue Abendwinde,", "tokens": ["Sagt", "mir", ",", "lau\u00b7e", "Ab\u00b7end\u00b7win\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ihr jetzt mein M\u00e4dchen k\u00fc\u00dft?", "tokens": ["Wo", "ihr", "jetzt", "mein", "M\u00e4d\u00b7chen", "k\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob im Spiegel eines Quelles", "tokens": ["Ob", "im", "Spie\u00b7gel", "ei\u00b7nes", "Quel\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich ihr klares Bildnis malt,", "tokens": ["Sich", "ihr", "kla\u00b7res", "Bild\u00b7nis", "malt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ob das Antlitz helles", "tokens": ["O\u00b7der", "ob", "das", "Ant\u00b7litz", "hel\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abendrot ihr \u00fcberstrahlt?", "tokens": ["A\u00b7ben\u00b7drot", "ihr", "\u00fc\u00b7bers\u00b7trahlt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob sie Nachtigallen gr\u00fc\u00dfen,", "tokens": ["Ob", "sie", "Nach\u00b7ti\u00b7gal\u00b7len", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie froh durch B\u00fcsche eilt,", "tokens": ["Wo", "sie", "froh", "durch", "B\u00fc\u00b7sche", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder neue Blumen sprie\u00dfen,", "tokens": ["O\u00b7der", "neu\u00b7e", "Blu\u00b7men", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ihr sanfter Fu\u00dftritt weilt?", "tokens": ["Wo", "ihr", "sanf\u00b7ter", "Fu\u00df\u00b7tritt", "weilt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flattert zu ihr, laue Winde,", "tokens": ["Flat\u00b7tert", "zu", "ihr", ",", "lau\u00b7e", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt ihr, da\u00df ich harre schon;", "tokens": ["Sagt", "ihr", ",", "da\u00df", "ich", "har\u00b7re", "schon", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr zum F\u00fchrer tragt geschwinde", "tokens": ["Ihr", "zum", "F\u00fch\u00b7rer", "tragt", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit euch meines Liedes Ton.", "tokens": ["Mit", "euch", "mei\u00b7nes", "Lie\u00b7des", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch die blauen L\u00fcfte webet", "tokens": ["Durch", "die", "blau\u00b7en", "L\u00fcf\u00b7te", "we\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abendd\u00e4mm'rung, ruhig, mild,", "tokens": ["A\u00b7bend\u00b7d\u00e4m\u00b7m'\u00b7rung", ",", "ru\u00b7hig", ",", "mild", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und vom Stern der Liebe bebet", "tokens": ["Und", "vom", "Stern", "der", "Lie\u00b7be", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanfter Schimmer aufs Gefild'.", "tokens": ["Sanf\u00b7ter", "Schim\u00b7mer", "aufs", "Ge\u00b7fild'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur wo mich ihr Arm umfasset,", "tokens": ["Nur", "wo", "mich", "ihr", "Arm", "um\u00b7fas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelt mir der sch\u00f6ne Stern,", "tokens": ["L\u00e4\u00b7chelt", "mir", "der", "sch\u00f6\u00b7ne", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein hellster Glanz erblasset,", "tokens": ["Und", "sein", "hells\u00b7ter", "Glanz", "er\u00b7blas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O Geliebte, bist du fern.", "tokens": ["O", "Ge\u00b7lieb\u00b7te", ",", "bist", "du", "fern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}