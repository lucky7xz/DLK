{"textgrid.poem.62194": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Wintergem\u00e4lde", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schneit im Wald", "tokens": ["Es", "schneit", "im", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Unheimlich kalt,", "tokens": ["Un\u00b7heim\u00b7lich", "kalt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Mann versinkt im Schnee;", "tokens": ["Ein", "Mann", "ver\u00b7sinkt", "im", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Ach, sein Weh,", "tokens": ["Sein", "Ach", ",", "sein", "Weh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Verhallet bald", "tokens": ["Ver\u00b7hal\u00b7let", "bald"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Im tiefen Wald.", "tokens": ["Im", "tie\u00b7fen", "Wald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Jagd, sie naht,", "tokens": ["Die", "Jagd", ",", "sie", "naht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zertritt die Saat;", "tokens": ["Zer\u00b7tritt", "die", "Saat", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein angeschossen Reh", "tokens": ["Ein", "an\u00b7ge\u00b7schos\u00b7sen", "Reh"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Versinkt im Schnee,", "tokens": ["Ver\u00b7sinkt", "im", "Schnee", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die B\u00fcchse knallt,", "tokens": ["Die", "B\u00fcch\u00b7se", "knallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Schu\u00df verhallt.", "tokens": ["Der", "Schu\u00df", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}