{"dta.poem.14091": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiwerber .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' einen Fremden wohl bei mir,", "tokens": ["Ich", "hab'", "ei\u00b7nen", "Frem\u00b7den", "wohl", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der lauert unten auf der Wacht,", "tokens": ["Der", "lau\u00b7ert", "un\u00b7ten", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bittet sch\u00f6n dich um Quartier,", "tokens": ["Der", "bit\u00b7tet", "sch\u00f6n", "dich", "um", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verschlaf'nes Kind, nimm dich in Acht!", "tokens": ["Ver\u00b7schlaf'\u00b7nes", "Kind", ",", "nimm", "dich", "in", "Acht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}