{"dta.poem.6915": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unser Geist die Vorw\u00fcrff' \u00fcberleget,", "tokens": ["Wenn", "un\u00b7ser", "Geist", "die", "Vor\u00b7w\u00fcrff'", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind sie ein Land, so keinen C\u00f6rper heget,", "tokens": ["Sind", "sie", "ein", "Land", ",", "so", "kei\u00b7nen", "C\u00f6r\u00b7per", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das er bewohnet, das er sieht.", "tokens": ["Das", "er", "be\u00b7woh\u00b7net", ",", "das", "er", "sieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist die Sonne nicht, die man an Himmel setzet,", "tokens": ["Es", "ist", "die", "Son\u00b7ne", "nicht", ",", "die", "man", "an", "Him\u00b7mel", "set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die eigentlich sich zeiget dem Gem\u00fcth;", "tokens": ["Die", "ei\u00b7gent\u00b7lich", "sich", "zei\u00b7get", "dem", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von einer andern Sonn\u2019, die in uns gl\u00fcht,", "tokens": ["Von", "ei\u00b7ner", "an\u00b7dern", "Sonn'", ",", "die", "in", "uns", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wird unser Geist, und nicht das Aug\u2019 erg\u00f6tzet.", "tokens": ["Wird", "un\u00b7ser", "Geist", ",", "und", "nicht", "das", "Aug'", "er\u00b7g\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Gegenw\u00fcrffe, die so fl\u00fcchtig seyn,", "tokens": ["Die", "Ge\u00b7gen\u00b7w\u00fcrf\u00b7fe", ",", "die", "so", "fl\u00fcch\u00b7tig", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die bunten Fl\u00e4chen voll von Klarheit, Glantz und Schein,", "tokens": ["Die", "bun\u00b7ten", "Fl\u00e4\u00b7chen", "voll", "von", "Klar\u00b7heit", ",", "Glantz", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Felder voller Bl\u00fcht, voll g\u00fcldner Aehren Spitzen,", "tokens": ["Die", "Fel\u00b7der", "vol\u00b7ler", "Bl\u00fcht", ",", "voll", "g\u00fcld\u00b7ner", "A\u00b7eh\u00b7ren", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Die Fluth, der Himmel und sein Blau,", "tokens": ["Die", "Fluth", ",", "der", "Him\u00b7mel", "und", "sein", "Blau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die sind die Sch\u00f6nheit nicht, die ich bezaubert schau.", "tokens": ["Die", "sind", "die", "Sch\u00f6n\u00b7heit", "nicht", ",", "die", "ich", "be\u00b7zau\u00b7bert", "schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von C\u00f6rpern gantz umringt, die unsern Geist besitzen,", "tokens": ["Von", "C\u00f6r\u00b7pern", "gantz", "um\u00b7ringt", ",", "die", "un\u00b7sern", "Geist", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sind sie es dennoch nicht, die wir erblicken.", "tokens": ["Sind", "sie", "es", "den\u00b7noch", "nicht", ",", "die", "wir", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wenn wir die gantze Welt betrachten und besehen,", "tokens": ["Wenn", "wir", "die", "gant\u00b7ze", "Welt", "be\u00b7trach\u00b7ten", "und", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Erkennen wir doch nichts, als unsere Jdeen.", "tokens": ["Er\u00b7ken\u00b7nen", "wir", "doch", "nichts", ",", "als", "un\u00b7se\u00b7re", "Jdeen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Es siehet jederman die Welt verschiedlich an.", "tokens": ["Es", "sie\u00b7het", "je\u00b7der\u00b7man", "die", "Welt", "ver\u00b7schied\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nachdem wir Witz, Verstand und Achtsamkeit empfangen,", "tokens": ["Nach\u00b7dem", "wir", "Witz", ",", "Ver\u00b7stand", "und", "Acht\u00b7sam\u00b7keit", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Empfinden wir auch mehr der Creaturen Prangen.", "tokens": ["Emp\u00b7fin\u00b7den", "wir", "auch", "mehr", "der", "Crea\u00b7tu\u00b7ren", "Pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}