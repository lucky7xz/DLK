{"textgrid.poem.64370": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "93. Lied", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ich als ein Pilger walle,", "tokens": ["Wo", "ich", "als", "ein", "Pil\u00b7ger", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "S\u00e4umet gern und oft mein Fu\u00df;", "tokens": ["S\u00e4u\u00b7met", "gern", "und", "oft", "mein", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in der Erinnrung Halle", "tokens": ["Denn", "in", "der", "E\u00b7rinn\u00b7rung", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Trag' ich fliehenden Genu\u00df;", "tokens": ["Trag'", "ich", "flie\u00b7hen\u00b7den", "Ge\u00b7nu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Tempel ist mir heilig,", "tokens": ["Die\u00b7ser", "Tem\u00b7pel", "ist", "mir", "hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Muse pfleget sein.", "tokens": ["Und", "die", "Mu\u00b7se", "pfle\u00b7get", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sei der Sohn der Sorge eilig;", "tokens": ["Sei", "der", "Sohn", "der", "Sor\u00b7ge", "ei\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der Freude Thr\u00e4ne weil' ich", "tokens": ["Mit", "der", "Freu\u00b7de", "Thr\u00e4\u00b7ne", "weil'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Vor des Heiligtumes Schrein.", "tokens": ["Vor", "des", "Hei\u00b7lig\u00b7tu\u00b7mes", "Schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meiner Jugend Blume bl\u00fchet", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Blu\u00b7me", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort vom Morgentaue frisch;", "tokens": ["Dort", "vom", "Mor\u00b7gen\u00b7tau\u00b7e", "frisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reif von Mittagssonne gl\u00fchet", "tokens": ["Reif", "von", "Mit\u00b7tags\u00b7son\u00b7ne", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe Frucht auf meinem Tisch!", "tokens": ["S\u00fc\u00b7\u00dfe", "Frucht", "auf", "mei\u00b7nem", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die immerjungen Horen", "tokens": ["Denn", "die", "im\u00b7mer\u00b7jun\u00b7gen", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollen mir gewogen sein;", "tokens": ["Wol\u00b7len", "mir", "ge\u00b7wo\u00b7gen", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als die Mutter mich geboren,", "tokens": ["Als", "die", "Mut\u00b7ter", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sangen sie vor zarten Ohren", "tokens": ["San\u00b7gen", "sie", "vor", "zar\u00b7ten", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ahndendes Gef\u00fchl mir ein.", "tokens": ["Ahn\u00b7den\u00b7des", "Ge\u00b7f\u00fchl", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie Aprilgew\u00f6lk den blauen", "tokens": ["Wie", "Ap\u00b7ril\u00b7ge\u00b7w\u00f6lk", "den", "blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Himmel birget und enth\u00fcllt,", "tokens": ["Him\u00b7mel", "bir\u00b7get", "und", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward von Wonn' und bangem Grauen", "tokens": ["Ward", "von", "Wonn'", "und", "ban\u00b7gem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da mein junges Herz erf\u00fcllt.", "tokens": ["Da", "mein", "jun\u00b7ges", "Herz", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die G\u00f6tter gehn und kommen,", "tokens": ["Wie", "die", "G\u00f6t\u00b7ter", "gehn", "und", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsichtbar dem \u00e4u\u00dfern Sinn,", "tokens": ["Un\u00b7sicht\u00b7bar", "dem", "\u00e4u\u00b7\u00dfern", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ungesehn, doch wahrgenommen,", "tokens": ["Un\u00b7ge\u00b7sehn", ",", "doch", "wahr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blickten scheidend auch die frommen", "tokens": ["Blick\u00b7ten", "schei\u00b7dend", "auch", "die", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Horen nach dem Knaben hin.", "tokens": ["Ho\u00b7ren", "nach", "dem", "Kna\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und da trat an meine Wiege", "tokens": ["Und", "da", "trat", "an", "mei\u00b7ne", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Eine junge Muse hin;", "tokens": ["Ei\u00b7ne", "jun\u00b7ge", "Mu\u00b7se", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich geh' und wo ich liege,", "tokens": ["Wo", "ich", "geh'", "und", "wo", "ich", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt sie her, und schwebt sie hin,", "tokens": ["Schwebt", "sie", "her", ",", "und", "schwebt", "sie", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist ein wunderbares M\u00e4dchen,", "tokens": ["Ist", "ein", "wun\u00b7der\u00b7ba\u00b7res", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt und gehet wie sie will,", "tokens": ["Kommt", "und", "ge\u00b7het", "wie", "sie", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sitzt an ihrem Zauberr\u00e4dchen,", "tokens": ["Sitzt", "an", "ih\u00b7rem", "Zau\u00b7berr\u00b7\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spinnet zarte goldne F\u00e4dchen,", "tokens": ["Spin\u00b7net", "zar\u00b7te", "gold\u00b7ne", "F\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber sitzet selten still.", "tokens": ["A\u00b7ber", "sit\u00b7zet", "sel\u00b7ten", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Frei, doch h\u00e4uslich, wie ein T\u00e4ubchen,", "tokens": ["Frei", ",", "doch", "h\u00e4us\u00b7lich", ",", "wie", "ein", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleugt sie aus, und fleugt sie ein;", "tokens": ["Fleugt", "sie", "aus", ",", "und", "fleugt", "sie", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4gt mir manches gr\u00fcne L\u00e4ubchen", "tokens": ["Tr\u00e4gt", "mir", "man\u00b7ches", "gr\u00fc\u00b7ne", "L\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Lebens Arch' hinein;", "tokens": ["In", "des", "Le\u00b7bens", "Ar\u00b7ch'", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "F\u00fcgte, als ich einsam weinen", "tokens": ["F\u00fcg\u00b7te", ",", "als", "ich", "ein\u00b7sam", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollte, sich in meinen Sinn,", "tokens": ["Woll\u00b7te", ",", "sich", "in", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Um mir wieder zu erscheinen;", "tokens": ["Um", "mir", "wie\u00b7der", "zu", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und als Engel trat vor meinen", "tokens": ["Und", "als", "En\u00b7gel", "trat", "vor", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NE", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tr\u00fcben Blick sie freundlich hin.", "tokens": ["Tr\u00fc\u00b7ben", "Blick", "sie", "freund\u00b7lich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bleib bei mir in meinem Leben,", "tokens": ["Bleib", "bei", "mir", "in", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelskind, verla\u00df mich nicht!", "tokens": ["Him\u00b7mel\u00b7skind", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollest freundlich mich umschweben,", "tokens": ["Wol\u00b7lest", "freund\u00b7lich", "mich", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mein Herz im Tode bricht!", "tokens": ["Wenn", "mein", "Herz", "im", "To\u00b7de", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re, was ich noch verlange!", "tokens": ["H\u00f6\u00b7re", ",", "was", "ich", "noch", "ver\u00b7lan\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann noch fl\u00fcstre mir ins Ohr,", "tokens": ["Dann", "noch", "fl\u00fcst\u00b7re", "mir", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df in heil'gem Schwanensange,", "tokens": ["Da\u00df", "in", "heil'\u00b7gem", "Schwa\u00b7nen\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Fl\u00fcgel Silberklange", "tokens": ["Und", "der", "Fl\u00fc\u00b7gel", "Sil\u00b7ber\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Meine Seele steig' empor!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "steig'", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}