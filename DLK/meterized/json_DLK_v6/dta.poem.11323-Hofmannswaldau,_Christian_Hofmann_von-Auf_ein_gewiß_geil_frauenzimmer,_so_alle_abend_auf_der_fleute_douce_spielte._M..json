{"dta.poem.11323": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein gewi\u00df geil frauenzimmer, so alle  \n abend auf der  fleute douce  spielte.  \n  M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vjel wusten nicht, warum die sch\u00f6ne Margaris", "tokens": ["Vjel", "wus\u00b7ten", "nicht", ",", "wa\u00b7rum", "die", "sch\u00f6\u00b7ne", "Mar\u00b7ga\u00b7ris"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jeder abend-stunde", "tokens": ["In", "je\u00b7der", "a\u00b7ben\u00b7dstun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrem s\u00fcssen munde", "tokens": ["Mit", "ih\u00b7rem", "s\u00fcs\u00b7sen", "mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einer fl\u00f6te blie\u00df;", "tokens": ["Auf", "ei\u00b7ner", "fl\u00f6\u00b7te", "blie\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Allein anitzo kan ein jeder sperling fingen,", "tokens": ["Al\u00b7lein", "a\u00b7nit\u00b7zo", "kan", "ein", "je\u00b7der", "sper\u00b7ling", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "PIS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was doch die ursach sey;", "tokens": ["Was", "doch", "die", "ur\u00b7sach", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn jeder f\u00e4llt uns bey:", "tokens": ["Denn", "je\u00b7der", "f\u00e4llt", "uns", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df man den hengsten pfeifft, so offt sie sollen springen.", "tokens": ["Da\u00df", "man", "den", "hengs\u00b7ten", "pfeifft", ",", "so", "offt", "sie", "sol\u00b7len", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}