{"textgrid.poem.61375": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Macht der Zeit verworrnes Stammeln,", "tokens": ["Macht", "der", "Zeit", "ver\u00b7worr\u00b7nes", "Stam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht ihr w\u00fcster Rausch dir Pein,", "tokens": ["Macht", "ihr", "w\u00fcs\u00b7ter", "Rausch", "dir", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehr', o Seele, dich zu sammeln,", "tokens": ["Kehr'", ",", "o", "See\u00b7le", ",", "dich", "zu", "sam\u00b7meln", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehre bei dir selber ein.", "tokens": ["Keh\u00b7re", "bei", "dir", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon ein heilig ernster Wille", "tokens": ["Schon", "ein", "hei\u00b7lig", "erns\u00b7ter", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht den Gott in deinen Kreis;", "tokens": ["Zieht", "den", "Gott", "in", "dei\u00b7nen", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist du fromm und bist du stille,", "tokens": ["Bist", "du", "fromm", "und", "bist", "du", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So vernimmst du sein Gehei\u00df.", "tokens": ["So", "ver\u00b7nimmst", "du", "sein", "Ge\u00b7hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mag dir dann der Markt nicht lauschen,", "tokens": ["Mag", "dir", "dann", "der", "Markt", "nicht", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df ihn st\u00fcrmen, la\u00df ihn rauscher", "tokens": ["La\u00df", "ihn", "st\u00fcr\u00b7men", ",", "la\u00df", "ihn", "rau\u00b7scher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In besinnungsloser Hast!", "tokens": ["In", "be\u00b7sin\u00b7nungs\u00b7lo\u00b7ser", "Hast", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mit gl\u00fccklicherm Geschlechte", "tokens": ["Doch", "mit", "gl\u00fcck\u00b7li\u00b7cherm", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sitzest du die sch\u00f6nen N\u00e4chte", "tokens": ["Sit\u00b7zest", "du", "die", "sch\u00f6\u00b7nen", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei der Zukunft schon zu Gast.", "tokens": ["Bei", "der", "Zu\u00b7kunft", "schon", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}