{"dta.poem.12599": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder! netzt die Zungen,", "tokens": ["Br\u00fc\u00b7der", "!", "netzt", "die", "Zun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Netzt und k\u00fchlt die Lungen", "tokens": ["Netzt", "und", "k\u00fchlt", "die", "Lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit dem besten Wein!", "tokens": ["Mit", "dem", "bes\u00b7ten", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auf, und schenket ein!", "tokens": ["Auf", ",", "und", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Seht! der Hundsstern gl\u00fchet,", "tokens": ["Seht", "!", "der", "Hunds\u00b7stern", "gl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Alles, was man siehet,", "tokens": ["Al\u00b7les", ",", "was", "man", "sie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Alles ist erhitzt,", "tokens": ["Al\u00b7les", "ist", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Alles durstet itzt.", "tokens": ["Al\u00b7les", "durs\u00b7tet", "itzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Sollten wir allein", "tokens": ["Soll\u00b7ten", "wir", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Denn nicht durstig seyn?", "tokens": ["Denn", "nicht", "durs\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}