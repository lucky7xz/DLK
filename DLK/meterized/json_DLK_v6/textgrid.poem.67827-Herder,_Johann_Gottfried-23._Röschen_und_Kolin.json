{"textgrid.poem.67827": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "23. R\u00f6schen und Kolin", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr gesehn eine Lilie,", "tokens": ["Habt", "ihr", "ge\u00b7sehn", "ei\u00b7ne", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die sinkt in Regenzeit?", "tokens": ["Die", "sinkt", "in", "Re\u00b7gen\u00b7zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach so schwand R\u00f6schen hin, sie schwand", "tokens": ["Ach", "so", "schwand", "R\u00f6\u00b7schen", "hin", ",", "sie", "schwand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "NN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Liebesherzeleid.", "tokens": ["Vor", "Lie\u00b7bes\u00b7her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als dreimal in der dunkeln Nacht", "tokens": ["Als", "drei\u00b7mal", "in", "der", "dun\u00b7keln", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Todtenglocke klang,", "tokens": ["Die", "Tod\u00b7ten\u00b7glo\u00b7cke", "klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dreimal die Eul' ans Fenster schlug,", "tokens": ["Drei\u00b7mal", "die", "Eul'", "ans", "Fens\u00b7ter", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und: \u00bbMit! Komm mit!\u00ab ihr sang;", "tokens": ["Und", ":", "\u00bb", "Mit", "!", "Komm", "mit", "!", "\u00ab", "ihr", "sang", ";"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "APPR", "$.", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Das liebe M\u00e4dchen wuste wohl,", "tokens": ["Das", "lie\u00b7be", "M\u00e4d\u00b7chen", "wus\u00b7te", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu wohl, da\u00df ihr das gilt;", "tokens": ["Zu", "wohl", ",", "da\u00df", "ihr", "das", "gilt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schwestern sassen ringsumher,", "tokens": ["Die", "Schwes\u00b7tern", "sas\u00b7sen", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und graus'ten eingeh\u00fcllt.", "tokens": ["Und", "graus'\u00b7ten", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbich h\u00f6r' ein' Stimm', ihr h\u00f6rt sie nicht,", "tokens": ["\u00bb", "ich", "h\u00f6r'", "ein'", "Stimm'", ",", "ihr", "h\u00f6rt", "sie", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die spricht: Komm mit mir fort!", "tokens": ["Die", "spricht", ":", "Komm", "mit", "mir", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh ein' Hand, ihr seht sie nicht,", "tokens": ["Ich", "seh", "ein'", "Hand", ",", "ihr", "seht", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die winkt mir, winkt mir dort!", "tokens": ["Die", "winkt", "mir", ",", "winkt", "mir", "dort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So wi\u00dft es denn, ein treulos Herz,", "tokens": ["So", "wi\u00dft", "es", "denn", ",", "ein", "treu\u00b7los", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Br\u00e4utgam t\u00f6dtet mich.", "tokens": ["Ein", "Br\u00e4ut\u00b7gam", "t\u00f6d\u00b7tet", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann ich daf\u00fcr, da\u00df seine Braut", "tokens": ["Kann", "ich", "da\u00b7f\u00fcr", ",", "da\u00df", "sei\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PAV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat dreimal mehr als ich?", "tokens": ["Hat", "drei\u00b7mal", "mehr", "als", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Kolin, gib ihr nicht dein Ja!", "tokens": ["O", "Ko\u00b7lin", ",", "gib", "ihr", "nicht", "dein", "Ja", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Dies Ja ist l\u00e4ngst schon mein.", "tokens": ["Dies", "Ja", "ist", "l\u00e4ngst", "schon", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und du, o Braut, nimm nicht den Ku\u00df!", "tokens": ["Und", "du", ",", "o", "Braut", ",", "nimm", "nicht", "den", "Ku\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VVIMP", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ku\u00df, er ist nicht dein.", "tokens": ["Der", "Ku\u00df", ",", "er", "ist", "nicht", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr schickt euch an zum Hochzeitfest,", "tokens": ["Ihr", "schickt", "euch", "an", "zum", "Hoch\u00b7zeit\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht morgen zum Altar;", "tokens": ["Geht", "mor\u00b7gen", "zum", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du armes M\u00e4dchen, falscher Mann.", "tokens": ["Du", "ar\u00b7mes", "M\u00e4d\u00b7chen", ",", "fal\u00b7scher", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch R\u00f6schen ist alldar!", "tokens": ["Auch", "R\u00f6\u00b7schen", "ist", "all\u00b7dar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Br\u00fcder, morgen tragt ihr mich,", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "mor\u00b7gen", "tragt", "ihr", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tragt mich an seiner Seit';", "tokens": ["Tragt", "mich", "an", "sei\u00b7ner", "Seit'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er zieht, geschm\u00fcckt als Br\u00e4utigam,", "tokens": ["Er", "zieht", ",", "ge\u00b7schm\u00fcckt", "als", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich schm\u00fcckt ein Leichenkleid.\u00ab", "tokens": ["Mich", "schm\u00fcckt", "ein", "Lei\u00b7chen\u00b7kleid", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie sprachs und starb. Man trug den Sarg,", "tokens": ["Sie", "sprachs", "und", "starb", ".", "Man", "trug", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trug ihn an seiner Seit';", "tokens": ["Trug", "ihn", "an", "sei\u00b7ner", "Seit'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er zog, geschm\u00fcckt als Br\u00e4utigam,", "tokens": ["Er", "zog", ",", "ge\u00b7schm\u00fcckt", "als", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schm\u00fcckt ein Leichenkleid.", "tokens": ["Sie", "schm\u00fcckt", "ein", "Lei\u00b7chen\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach Br\u00e4utigam, wie war dir da?", "tokens": ["Ach", "Br\u00e4u\u00b7ti\u00b7gam", ",", "wie", "war", "dir", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWAV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie war dir da, o Braut?", "tokens": ["Wie", "war", "dir", "da", ",", "o", "Braut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Brautreihn flog um R\u00f6schens Sarg,", "tokens": ["Der", "Brau\u00b7treihn", "flog", "um", "R\u00f6\u00b7schens", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ganze Dorf weint laut.", "tokens": ["Das", "gan\u00b7ze", "Dorf", "weint", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Verwirrung, Angst den Br\u00e4utgam fa\u00dft,", "tokens": ["Ver\u00b7wir\u00b7rung", ",", "Angst", "den", "Br\u00e4ut\u00b7gam", "fa\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzweiflung fasset ihn;", "tokens": ["Ver\u00b7zwei\u00b7flung", "fas\u00b7set", "ihn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon dunkelt Tod auf seiner Stirn,", "tokens": ["Schon", "dun\u00b7kelt", "Tod", "auf", "sei\u00b7ner", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er \u00e4chzt und sinket hin.", "tokens": ["Er", "\u00e4chzt", "und", "sin\u00b7ket", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und ach! du Braut, nun Braut nicht mehr,", "tokens": ["Und", "ach", "!", "du", "Braut", ",", "nun", "Braut", "nicht", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "NN", "$,", "ADV", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist dein Hochzeitroth?", "tokens": ["Wo", "ist", "dein", "Hoch\u00b7zeit\u00b7roth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh seine erste Liebe da,", "tokens": ["Sieh", "sei\u00b7ne", "ers\u00b7te", "Lie\u00b7be", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh deinen Br\u00e4utgam todt!", "tokens": ["Sieh", "dei\u00b7nen", "Br\u00e4ut\u00b7gam", "todt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Nachbarn-Sch\u00e4fer legten ihn", "tokens": ["Die", "Nach\u00b7barn\u00b7Sch\u00e4\u00b7fer", "leg\u00b7ten", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seines R\u00f6schens Gruft;", "tokens": ["In", "sei\u00b7nes", "R\u00f6\u00b7schens", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da liegt er nun, Ein Staub mit ihr,", "tokens": ["Da", "liegt", "er", "nun", ",", "Ein", "Staub", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis Gottes Stimme ruft.", "tokens": ["Bis", "Got\u00b7tes", "Stim\u00b7me", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und oft geht noch ans heil'ge Grab", "tokens": ["Und", "oft", "geht", "noch", "ans", "heil'\u00b7ge", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein treuverlobtes Paar,", "tokens": ["Ein", "treu\u00b7ver\u00b7lob\u00b7tes", "Paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und binden Liebesknoten sich,", "tokens": ["Und", "bin\u00b7den", "Lie\u00b7bes\u00b7kno\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringen Kr\u00e4nze dar.", "tokens": ["Und", "brin\u00b7gen", "Kr\u00e4n\u00b7ze", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Du aber, Falscher, sey gewarnt,", "tokens": ["Du", "a\u00b7ber", ",", "Fal\u00b7scher", ",", "sey", "ge\u00b7warnt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nah dich nicht herzu,", "tokens": ["Und", "nah", "dich", "nicht", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedenk an Kolin, fleuch und st\u00f6r'", "tokens": ["Ge\u00b7denk", "an", "Ko\u00b7lin", ",", "fleuch", "und", "st\u00f6r'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "$,", "NN", "KON", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ihn nicht aus seiner Ruh.", "tokens": ["Ihn", "nicht", "aus", "sei\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}