{"textgrid.poem.38279": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abl\u00f6sung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kukuk hat sich zu todt gefallen", "tokens": ["Ku\u00b7kuk", "hat", "sich", "zu", "todt", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "PTKA", "ADJD", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "An einer holen Weiden,", "tokens": ["An", "ei\u00b7ner", "ho\u00b7len", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer soll uns diesen Sommer lang", "tokens": ["Wer", "soll", "uns", "die\u00b7sen", "Som\u00b7mer", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PDAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit und Weil vertreiben.", "tokens": ["Die", "Zeit", "und", "Weil", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ey das soll thun Frau Nachtigall,", "tokens": ["Ey", "das", "soll", "thun", "Frau", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VMFIN", "VVINF", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Die sitzt auf gr\u00fcnem Zweige;", "tokens": ["Die", "sitzt", "auf", "gr\u00fc\u00b7nem", "Zwei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie singt und springt, ist allzeit froh,", "tokens": ["Sie", "singt", "und", "springt", ",", "ist", "all\u00b7zeit", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn andre V\u00f6gel schweigen.", "tokens": ["Wenn", "and\u00b7re", "V\u00f6\u00b7gel", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}