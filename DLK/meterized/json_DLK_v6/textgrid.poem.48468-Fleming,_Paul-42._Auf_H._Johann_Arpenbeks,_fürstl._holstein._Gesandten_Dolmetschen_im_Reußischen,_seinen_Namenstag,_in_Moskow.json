{"textgrid.poem.48468": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "42. Auf H. Johann Arpenbeks, f\u00fcrstl. holstein. Gesandten Dolmetschen im Reu\u00dfischen, seinen Namenstag, in Moskow", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indessen, da\u00df du bist der sch\u00f6nen Stadt entzogen,", "tokens": ["In\u00b7des\u00b7sen", ",", "da\u00df", "du", "bist", "der", "sch\u00f6\u00b7nen", "Stadt", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die deiner allzeit hat als ihres Sohns gepflogen,", "tokens": ["die", "dei\u00b7ner", "all\u00b7zeit", "hat", "als", "ih\u00b7res", "Sohns", "ge\u00b7pflo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADV", "VAFIN", "KOKOM", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so, da\u00df die Liebste dich nicht selbsten binden kan,", "tokens": ["so", ",", "da\u00df", "die", "Liebs\u00b7te", "dich", "nicht", "selbs\u00b7ten", "bin\u00b7den", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so nimm an ihrer Statt von mir di\u00df Brieflein an!", "tokens": ["so", "nimm", "an", "ih\u00b7rer", "Statt", "von", "mir", "di\u00df", "Brie\u00b7flein", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "APPR", "PPER", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm dieses Brieflein an und meine hierbeineben,", "tokens": ["Nimm", "die\u00b7ses", "Brie\u00b7flein", "an", "und", "mei\u00b7ne", "hier\u00b7bei\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "PTKVZ", "KON", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "als h\u00e4tte sie dirs selbst von Hand zu Hand gegeben,", "tokens": ["als", "h\u00e4t\u00b7te", "sie", "dirs", "selbst", "von", "Hand", "zu", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "PIS", "ADV", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da doch das sch\u00f6ne Kind nun weit, weit von uns ist,", "tokens": ["da", "doch", "das", "sch\u00f6\u00b7ne", "Kind", "nun", "weit", ",", "weit", "von", "uns", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "ADJD", "$,", "ADJD", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nach dem du kaum halb froh mit feuchten Augen siehst!", "tokens": ["nach", "dem", "du", "kaum", "halb", "froh", "mit", "feuch\u00b7ten", "Au\u00b7gen", "siehst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zwar, sie wird tausentmal und tausentmal gedenken:", "tokens": ["Zwar", ",", "sie", "wird", "tau\u00b7sent\u00b7mal", "und", "tau\u00b7sent\u00b7mal", "ge\u00b7den\u00b7ken", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach! solt' ich diesen Tag ihm dieses B\u00e4ndlein schenken!", "tokens": ["Ach", "!", "solt'", "ich", "die\u00b7sen", "Tag", "ihm", "die\u00b7ses", "B\u00e4n\u00b7dlein", "schen\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PDAT", "NN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "dem sie vom Wert' und Kunst gab einen lieben Schein,", "tokens": ["dem", "sie", "vom", "Wert'", "und", "Kunst", "gab", "ei\u00b7nen", "lie\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "mehr, da\u00df ihr g\u00fcldnes Haar steht mit geflochten ein.", "tokens": ["mehr", ",", "da\u00df", "ihr", "g\u00fcld\u00b7nes", "Haar", "steht", "mit", "ge\u00b7floch\u00b7ten", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df aber hast du, Freund, ink\u00fcnftig zu empfangen", "tokens": ["Di\u00df", "a\u00b7ber", "hast", "du", ",", "Freund", ",", "in\u00b7k\u00fcnf\u00b7tig", "zu", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und sie auch selbst darzu; itzt ma\u00dfe dein Verlangen!", "tokens": ["und", "sie", "auch", "selbst", "dar\u00b7zu", ";", "itzt", "ma\u00b7\u00dfe", "dein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "PAV", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denk' nicht zu sehr an ihr und harre jener Zeit,", "tokens": ["Denk'", "nicht", "zu", "sehr", "an", "ihr", "und", "har\u00b7re", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PTKA", "ADV", "APPR", "PPER", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "die allzeit dich mit ihr und sie mit dir erfreut!", "tokens": ["die", "all\u00b7zeit", "dich", "mit", "ihr", "und", "sie", "mit", "dir", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "L\u00f6s' heute dieses Band, mit dem wir heute binden", "tokens": ["L\u00f6s'", "heu\u00b7te", "die\u00b7ses", "Band", ",", "mit", "dem", "wir", "heu\u00b7te", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "das Herz' und nicht die Hand! Du wei\u00dft wol Rat zu finden,", "tokens": ["das", "Her\u00b7z'", "und", "nicht", "die", "Hand", "!", "Du", "wei\u00dft", "wol", "Rat", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PTKNEG", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.19": {"text": "wir sorgen nicht darf\u00fcr. Nur das sei alles klar,", "tokens": ["wir", "sor\u00b7gen", "nicht", "dar\u00b7f\u00fcr", ".", "Nur", "das", "sei", "al\u00b7les", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PAV", "$.", "ADV", "PDS", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "wenn ich dir spreche zu und eine lange Schar,", "tokens": ["wenn", "ich", "dir", "spre\u00b7che", "zu", "und", "ei\u00b7ne", "lan\u00b7ge", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "die nicht kan traurig sein. Doch aber, ist es Sache,", "tokens": ["die", "nicht", "kan", "trau\u00b7rig", "sein", ".", "Doch", "a\u00b7ber", ",", "ist", "es", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$.", "KON", "ADV", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "da\u00df etwan jener Freund uns heut' ein Gastmahl mache,", "tokens": ["da\u00df", "et\u00b7wan", "je\u00b7ner", "Freund", "uns", "heut'", "ein", "Gast\u00b7mahl", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDAT", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "der auch hei\u00dft so wie du, so sei mit dem vergn\u00fcgt,", "tokens": ["der", "auch", "hei\u00dft", "so", "wie", "du", ",", "so", "sei", "mit", "dem", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "KOKOM", "PPER", "$,", "ADV", "VAFIN", "APPR", "ART", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "da\u00df sich ein ieder denn auf Morgen zu dir f\u00fcgt!", "tokens": ["da\u00df", "sich", "ein", "ie\u00b7der", "denn", "auf", "Mor\u00b7gen", "zu", "dir", "f\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "KON", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}