{"dta.poem.8793": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Was nie du fassen wirst, verschone nur!", "tokens": ["Was", "nie", "du", "fas\u00b7sen", "wirst", ",", "ver\u00b7scho\u00b7ne", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVINF", "VAFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der K\u00e4fer hier beschmuzt den reinen Quell,", "tokens": ["Der", "K\u00e4\u00b7fer", "hier", "be\u00b7schmuzt", "den", "rei\u00b7nen", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch er ertrinkt, er hat's zum Lohne nur!", "tokens": ["Doch", "er", "er\u00b7trinkt", ",", "er", "hat's", "zum", "Loh\u00b7ne", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es h\u00e4ngen Tropfen an die Tulpe sich,", "tokens": ["Es", "h\u00e4n\u00b7gen", "Trop\u00b7fen", "an", "die", "Tul\u00b7pe", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch sie versch\u00f6nern ihre Krone nur!", "tokens": ["Doch", "sie", "ver\u00b7sch\u00f6\u00b7nern", "ih\u00b7re", "Kro\u00b7ne", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Schilf erklang, der Hirte schnitt es ab,", "tokens": ["Das", "Schilf", "er\u00b7klang", ",", "der", "Hir\u00b7te", "schnitt", "es", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als Fl\u00f6te scholl's mit s\u00fc\u00dferm Tone nur!", "tokens": ["Als", "Fl\u00f6\u00b7te", "scholl's", "mit", "s\u00fc\u00b7\u00dferm", "To\u00b7ne", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}