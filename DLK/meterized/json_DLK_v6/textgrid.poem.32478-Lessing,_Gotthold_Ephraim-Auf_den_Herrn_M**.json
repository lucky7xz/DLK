{"textgrid.poem.32478": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Herrn M**", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der mathematsche Theolog,", "tokens": ["Der", "ma\u00b7the\u00b7mat\u00b7sche", "Theo\u00b7log", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der sich und andre nie betrog,", "tokens": ["Der", "sich", "und", "and\u00b7re", "nie", "be\u00b7trog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KON", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sa\u00df zwischen zweimal zweien W\u00e4nden,", "tokens": ["Sa\u00df", "zwi\u00b7schen", "zwei\u00b7mal", "zwei\u00b7en", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit archimedscher D\u00fcsternheit,", "tokens": ["Mit", "ar\u00b7chi\u00b7med\u00b7scher", "D\u00fcs\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hatte \u2013 \u2013 welche Kleinigkeit!", "tokens": ["Und", "hat\u00b7te", "\u2013", "\u2013", "wel\u00b7che", "Klei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "$(", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Zirkels Vierung unter H\u00e4nden.", "tokens": ["Des", "Zir\u00b7kels", "Vie\u00b7rung", "un\u00b7ter", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00fchn schm\u00e4ht er auf das x + z", "tokens": ["K\u00fchn", "schm\u00e4ht", "er", "auf", "das", "x", "+", "z"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "XY", "XY", "XY"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "(denn was ist leichter als geschm\u00e4ht?)", "tokens": ["(", "denn", "was", "ist", "leich\u00b7ter", "als", "ge\u00b7schm\u00e4ht", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWS", "VAFIN", "ADJD", "KOKOM", "VVPP", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Als ihn der Hochmut sacht und sachte", "tokens": ["Als", "ihn", "der", "Hoch\u00b7mut", "sacht", "und", "sach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bei seinen Zahlen drehend machte.", "tokens": ["Bei", "sei\u00b7nen", "Zah\u00b7len", "dre\u00b7hend", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So wie auf einem Fu\u00df der Bube", "tokens": ["So", "wie", "auf", "ei\u00b7nem", "Fu\u00df", "der", "Bu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sich dreht, und dreht sich endlich dumm,", "tokens": ["Sich", "dreht", ",", "und", "dreht", "sich", "end\u00b7lich", "dumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "KON", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So ging die tetragonsche Stube,", "tokens": ["So", "ging", "die", "tet\u00b7ra\u00b7gon\u00b7sche", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und Stuhl und Tisch mit ihm herum.", "tokens": ["Und", "Stuhl", "und", "Tisch", "mit", "ihm", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "O Wunder, schrie er, o Natur!", "tokens": ["O", "Wun\u00b7der", ",", "schrie", "er", ",", "o", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da hab ich sie, des Zirkels Quadratur.", "tokens": ["Da", "hab", "ich", "sie", ",", "des", "Zir\u00b7kels", "Quad\u00b7ra\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}