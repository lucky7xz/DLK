{"dta.poem.23393": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gute Wille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gern will ich sein ein Rather,", "tokens": ["Gern", "will", "ich", "sein", "ein", "Ra\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlangt nur keine That \u2014", "tokens": ["Ver\u00b7langt", "nur", "kei\u00b7ne", "That"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin Familienvater", "tokens": ["Ich", "bin", "Fa\u00b7mi\u00b7li\u00b7en\u00b7va\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und auch Geheimerrath.", "tokens": ["Und", "auch", "Ge\u00b7hei\u00b7mer\u00b7rath", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja freilich, beides bin ich,", "tokens": ["Ja", "frei\u00b7lich", ",", "bei\u00b7des", "bin", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PIS", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das macht mir viele Pein \u2014", "tokens": ["Das", "macht", "mir", "vie\u00b7le", "Pein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin gewi\u00df freisinnig,", "tokens": ["Ich", "bin", "ge\u00b7wi\u00df", "frei\u00b7sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's einer nur kann sein.", "tokens": ["Wie's", "ei\u00b7ner", "nur", "kann", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt' ich nicht Frau und Kinder,", "tokens": ["H\u00e4tt'", "ich", "nicht", "Frau", "und", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da w\u00e4r's mir einerlei,", "tokens": ["Da", "w\u00e4r's", "mir", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vorsichtig w\u00e4r' ich minder,", "tokens": ["Vor\u00b7sich\u00b7tig", "w\u00e4r'", "ich", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spr\u00e4ch auch noch mal so frei.", "tokens": ["Spr\u00e4ch", "auch", "noch", "mal", "so", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch ein Familienvater \u2014", "tokens": ["Doch", "ein", "Fa\u00b7mi\u00b7li\u00b7en\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Punkt ist delicat,", "tokens": ["Der", "Punkt", "ist", "de\u00b7li\u00b7cat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und noch viel delicater", "tokens": ["Und", "noch", "viel", "de\u00b7li\u00b7ca\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist ein Geheimerrath.", "tokens": ["Ist", "ein", "Ge\u00b7hei\u00b7mer\u00b7rath", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}