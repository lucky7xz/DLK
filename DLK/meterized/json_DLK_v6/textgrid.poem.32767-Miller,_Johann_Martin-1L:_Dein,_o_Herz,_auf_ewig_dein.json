{"textgrid.poem.32767": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein, o Herz, auf ewig dein", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein, o Herz, auf ewig dein", "tokens": ["Dein", ",", "o", "Herz", ",", "auf", "e\u00b7wig", "dein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "$,", "FM", "NN", "$,", "APPR", "ADJD", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll der Engel Gottes sein!", "tokens": ["Soll", "der", "En\u00b7gel", "Got\u00b7tes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ich fass' es, fass' es kaum,", "tokens": ["Ach", "!", "ich", "fass'", "es", ",", "fass'", "es", "kaum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halt's f\u00fcr T\u00e4uschung nur und Traum!", "tokens": ["Halt's", "f\u00fcr", "T\u00e4u\u00b7schung", "nur", "und", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieser Arm umfa\u00dfte Sie!", "tokens": ["Die\u00b7ser", "Arm", "um\u00b7fa\u00df\u00b7te", "Sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese H\u00e4nde dr\u00fcckte Sie,", "tokens": ["Die\u00b7se", "H\u00e4n\u00b7de", "dr\u00fcck\u00b7te", "Sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fc\u00dfte mich mit hei\u00dfen Mund,", "tokens": ["K\u00fc\u00df\u00b7te", "mich", "mit", "hei\u00b7\u00dfen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "That mein Gl\u00fcck mir stammelnd kund!", "tokens": ["That", "mein", "Gl\u00fcck", "mir", "stam\u00b7melnd", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, ihr Thr\u00e4nen, st\u00fcrzet hin,", "tokens": ["Ach", ",", "ihr", "Thr\u00e4\u00b7nen", ",", "st\u00fcr\u00b7zet", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dankt der Wonnegeberin!", "tokens": ["Dankt", "der", "Won\u00b7ne\u00b7ge\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holde, nimm die Thr\u00e4nen an,", "tokens": ["Hol\u00b7de", ",", "nimm", "die", "Thr\u00e4\u00b7nen", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Mund nicht danken kann!", "tokens": ["Wenn", "der", "Mund", "nicht", "dan\u00b7ken", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freud' und Leben k\u00f6mmt mit dir;", "tokens": ["Freud'", "und", "Le\u00b7ben", "k\u00f6mmt", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Golden lacht die Sch\u00f6pfung mir;", "tokens": ["Gol\u00b7den", "lacht", "die", "Sch\u00f6p\u00b7fung", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Tag, mit Heil geziert,", "tokens": ["Je\u00b7der", "Tag", ",", "mit", "Heil", "ge\u00b7ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird von dir mir zugef\u00fchrt!", "tokens": ["Wird", "von", "dir", "mir", "zu\u00b7ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "K\u00fcss', o Engel, k\u00fcsse mich!", "tokens": ["K\u00fcss'", ",", "o", "En\u00b7gel", ",", "k\u00fcs\u00b7se", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel werd' ich auch durch dich!", "tokens": ["En\u00b7gel", "werd'", "ich", "auch", "durch", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, an dieser lieben Brust", "tokens": ["O", ",", "an", "die\u00b7ser", "lie\u00b7ben", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrb' ich gern vor Liebeslust!", "tokens": ["St\u00fcrb'", "ich", "gern", "vor", "Lie\u00b7bes\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dein, o Wonnereiche, dein", "tokens": ["Dein", ",", "o", "Won\u00b7ne\u00b7rei\u00b7che", ",", "dein"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "$,", "FM", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll dies ganze Leben sein!", "tokens": ["Soll", "dies", "gan\u00b7ze", "Le\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Tr\u00f6pfchen meiner Zeit", "tokens": ["Je\u00b7des", "Tr\u00f6pf\u00b7chen", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei nur dir, nur dir geweiht!", "tokens": ["Sei", "nur", "dir", ",", "nur", "dir", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Teilt sie nicht mein Herz mit dir,", "tokens": ["Teilt", "sie", "nicht", "mein", "Herz", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmecke keine Freude mir!", "tokens": ["Schme\u00b7cke", "kei\u00b7ne", "Freu\u00b7de", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Naht dir je ein Kummer sich,", "tokens": ["Naht", "dir", "je", "ein", "Kum\u00b7mer", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O so leg' ihn Gott auf mich!", "tokens": ["O", "so", "leg'", "ihn", "Gott", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "K\u00fcss', o Holde, k\u00fcsse mich!", "tokens": ["K\u00fcss'", ",", "o", "Hol\u00b7de", ",", "k\u00fcs\u00b7se", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrb ich, Engel, doch f\u00fcr dich!", "tokens": ["St\u00fcrb", "ich", ",", "En\u00b7gel", ",", "doch", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott, wie dank' ich, dank' ich dir!", "tokens": ["Gott", ",", "wie", "dank'", "ich", ",", "dank'", "ich", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ein M\u00e4dchen gabst du mir!", "tokens": ["Welch", "ein", "M\u00e4d\u00b7chen", "gabst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}