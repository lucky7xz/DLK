{"textgrid.poem.53589": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Revolutions-R\u00fcckblick", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schau zur\u00fcck. Die Pressegenerale", "tokens": ["Ich", "schau", "zu\u00b7r\u00fcck", ".", "Die", "Pres\u00b7se\u00b7ge\u00b7ne\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ergriff vor einem Jahr der gro\u00dfe Schreck.", "tokens": ["er\u00b7griff", "vor", "ei\u00b7nem", "Jahr", "der", "gro\u00b7\u00dfe", "Schreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die O H L verstummt mit einem Male.", "tokens": ["Die", "O", "H", "L", "ver\u00b7stummt", "mit", "ei\u00b7nem", "Ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "XY", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vorbei. Die Phrase lag im Dreck.", "tokens": ["Vor\u00b7bei", ".", "Die", "Phra\u00b7se", "lag", "im", "Dreck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vorbei die Pl\u00e4ne und die dicken Thesen,", "tokens": ["Vor\u00b7bei", "die", "Pl\u00e4\u00b7ne", "und", "die", "di\u00b7cken", "The\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "vorbei die plumpen Renommisterein \u2013", "tokens": ["vor\u00b7bei", "die", "plum\u00b7pen", "Re\u00b7nom\u00b7mis\u00b7ter\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beh\u00fct dich Gott, es w\u00e4r zu sch\u00f6n gewesen,", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", ",", "es", "w\u00e4r", "zu", "sch\u00f6n", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "beh\u00fct dich Gott, es hat nicht sollen sein!", "tokens": ["be\u00b7h\u00fct", "dich", "Gott", ",", "es", "hat", "nicht", "sol\u00b7len", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soldaten vor! Der Kaiser hat verzichtet.", "tokens": ["Sol\u00b7da\u00b7ten", "vor", "!", "Der", "Kai\u00b7ser", "hat", "ver\u00b7zich\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun wolltet ihr alleine weiter sehn.", "tokens": ["Nun", "woll\u00b7tet", "ihr", "al\u00b7lei\u00b7ne", "wei\u00b7ter", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist im Leben h\u00e4\u00dflich eingerichtet,", "tokens": ["Das", "ist", "im", "Le\u00b7ben", "h\u00e4\u00df\u00b7lich", "ein\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df bei den Eberts gleich die Noskes stehn.", "tokens": ["da\u00df", "bei", "den", "E\u00b7berts", "gleich", "die", "Nos\u00b7kes", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kaum ist das Land von einer Pest genesen,", "tokens": ["Kaum", "ist", "das", "Land", "von", "ei\u00b7ner", "Pest", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "f\u00e4llt es mit Grazie in die n\u00e4chste rein \u2013", "tokens": ["f\u00e4llt", "es", "mit", "Gra\u00b7zie", "in", "die", "n\u00e4chs\u00b7te", "rein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Beh\u00fct dich Gott, es w\u00e4r zu sch\u00f6n gewesen,", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", ",", "es", "w\u00e4r", "zu", "sch\u00f6n", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "beh\u00fct dich Gott, es hat nicht sollen sein!", "tokens": ["be\u00b7h\u00fct", "dich", "Gott", ",", "es", "hat", "nicht", "sol\u00b7len", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir dachten schon: Jetzt gilts den Offizieren!", "tokens": ["Wir", "dach\u00b7ten", "schon", ":", "Jetzt", "gilts", "den", "Of\u00b7fi\u00b7zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir dachten schon: Hier wird nun Ernst gemacht.", "tokens": ["Wir", "dach\u00b7ten", "schon", ":", "Hier", "wird", "nun", "Ernst", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "VAFIN", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir dachten schon: Man wird sich nicht genieren,", "tokens": ["Wir", "dach\u00b7ten", "schon", ":", "Man", "wird", "sich", "nicht", "ge\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PIS", "VAFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Feuer brennt einmal . . . es ist entfacht . . .", "tokens": ["das", "Feu\u00b7er", "brennt", "ein\u00b7mal", ".", ".", ".", "es", "ist", "ent\u00b7facht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "$.", "$.", "PPER", "VAFIN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir dachten schon: Nun kommt der Eisenbesen . . .", "tokens": ["Wir", "dach\u00b7ten", "schon", ":", "Nun", "kommt", "der", "Ei\u00b7sen\u00b7be\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch weicht der Deutsche sich die Hosen ein \u2013", "tokens": ["Doch", "weicht", "der", "Deut\u00b7sche", "sich", "die", "Ho\u00b7sen", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beh\u00fct dich Gott, es w\u00e4r zu sch\u00f6n gewesen,", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", ",", "es", "w\u00e4r", "zu", "sch\u00f6n", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "beh\u00fct dich Gott, es hat nicht sollen sein!", "tokens": ["be\u00b7h\u00fct", "dich", "Gott", ",", "es", "hat", "nicht", "sol\u00b7len", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kommt diesem Lande niemals denn ein Retter?", "tokens": ["Kommt", "die\u00b7sem", "Lan\u00b7de", "nie\u00b7mals", "denn", "ein", "Ret\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die graue Regenluft weht na\u00df und fahl.", "tokens": ["Die", "grau\u00b7e", "Re\u00b7gen\u00b7luft", "weht", "na\u00df", "und", "fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Abschiednehmen just das rechte Wetter:", "tokens": ["Zum", "Ab\u00b7schied\u00b7neh\u00b7men", "just", "das", "rech\u00b7te", "Wet\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fahr wohl, fahr wohl, November-Ideal!", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", ",", "No\u00b7vem\u00b7ber\u00b7Ide\u00b7al", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn erstens kostest du zu hohe Spesen,", "tokens": ["Denn", "ers\u00b7tens", "kos\u00b7test", "du", "zu", "ho\u00b7he", "Spe\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und zweitens singt ihr noch die Wacht am Rhein \u2013", "tokens": ["und", "zwei\u00b7tens", "singt", "ihr", "noch", "die", "Wacht", "am", "Rhein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Tat\u00fc-tata \u2013 es w\u00e4r zu sch\u00f6n gewesen,", "tokens": ["Ta\u00b7t\u00fc\u00b7ta\u00b7ta", "\u2013", "es", "w\u00e4r", "zu", "sch\u00f6n", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "beh\u00fct dich Gott, es hat nicht sollen sein!", "tokens": ["be\u00b7h\u00fct", "dich", "Gott", ",", "es", "hat", "nicht", "sol\u00b7len", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}