{"textgrid.poem.47657": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Erde, deine Mutter]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde, deine Mutter,", "tokens": ["Die", "Er\u00b7de", ",", "dei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie leidet Hack' und Spaten,", "tokens": ["Sie", "lei\u00b7det", "Hack'", "und", "Spa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lohnt in ihrer G\u00fcte", "tokens": ["Und", "lohnt", "in", "ih\u00b7rer", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit tausendfacher Bl\u00fcthe,", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7cher", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihrer Frucht, so lind,", "tokens": ["Mit", "ih\u00b7rer", "Frucht", ",", "so", "lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die allerschlimmsten Thaten \u2013", "tokens": ["Die", "al\u00b7ler\u00b7schlimms\u00b7ten", "Tha\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hab' auch ein solch Gem\u00fcthe,", "tokens": ["Hab'", "auch", "ein", "solch", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Du ausgeartet Kind!", "tokens": ["Du", "aus\u00b7ge\u00b7ar\u00b7tet", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}