{"textgrid.poem.65227": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Alexander und Diogenes", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringt mir die liebe Jugend fort", "tokens": ["Bringt", "mir", "die", "lie\u00b7be", "Ju\u00b7gend", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrem Saus und Braus!", "tokens": ["Mit", "ih\u00b7rem", "Saus", "und", "Braus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ziemet sich ein kluges Wort", "tokens": ["Es", "zie\u00b7met", "sich", "ein", "klu\u00b7ges", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem guten Schmaus.", "tokens": ["Zu", "ei\u00b7nem", "gu\u00b7ten", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drum setzet Einen zu mir her,", "tokens": ["Drum", "set\u00b7zet", "Ei\u00b7nen", "zu", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der \u00e4lter ist, als ich,", "tokens": ["Der", "\u00e4l\u00b7ter", "ist", ",", "als", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weitgereist durch Land und Meer:", "tokens": ["Und", "weit\u00b7ge\u00b7reist", "durch", "Land", "und", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach diesem d\u00fcrstet mich!", "tokens": ["Nach", "die\u00b7sem", "d\u00fcrs\u00b7tet", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der in dem stillen dunkeln Fa\u00df", "tokens": ["Der", "in", "dem", "stil\u00b7len", "dun\u00b7keln", "Fa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Jahre lang gedacht,", "tokens": ["Viel", "Jah\u00b7re", "lang", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df gewi\u00dflich dies und das,", "tokens": ["Er", "wei\u00df", "ge\u00b7wi\u00df\u00b7lich", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was uns auch weiser macht.", "tokens": ["Was", "uns", "auch", "wei\u00b7ser", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Diogenes sei er genannt,", "tokens": ["Dio\u00b7ge\u00b7nes", "sei", "er", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Der Herr Philosophus,", "tokens": ["Der", "Herr", "Phi\u00b7lo\u00b7so\u00b7phus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4r' ich Herr von Griechenland,", "tokens": ["Und", "w\u00e4r'", "ich", "Herr", "von", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich b\u00f6t' ihm meinen Gru\u00df.", "tokens": ["Ich", "b\u00f6t'", "ihm", "mei\u00b7nen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und spr\u00e4che: Wenn ich Ich nicht w\u00e4r'", "tokens": ["Und", "spr\u00e4\u00b7che", ":", "Wenn", "ich", "Ich", "nicht", "w\u00e4r'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "PPER", "PTKNEG", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich nicht tr\u00e4nke dich,", "tokens": ["Und", "ich", "nicht", "tr\u00e4n\u00b7ke", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wollt' ich Du sein ohn' Beschwer,", "tokens": ["So", "wollt'", "ich", "Du", "sein", "ohn'", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PPOSAT", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du, du tr\u00e4nkest mich!", "tokens": ["Und", "du", ",", "du", "tr\u00e4n\u00b7kest", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}