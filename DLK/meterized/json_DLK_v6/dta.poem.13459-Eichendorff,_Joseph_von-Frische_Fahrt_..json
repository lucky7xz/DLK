{"dta.poem.13459": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frische Fahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Laue Luft kommt blau geflossen,", "tokens": ["Lau\u00b7e", "Luft", "kommt", "blau", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling, Fr\u00fchling soll es seyn!", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "soll", "es", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waldw\u00e4rts H\u00f6rnerklang geschossen,", "tokens": ["Wald\u00b7w\u00e4rts", "H\u00f6r\u00b7ner\u00b7klang", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Muth'ger Augen lichter Schein;", "tokens": ["Muth'\u00b7ger", "Au\u00b7gen", "lich\u00b7ter", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Wirren bunt und bunter", "tokens": ["Und", "das", "Wir\u00b7ren", "bunt", "und", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird ein magisch wilder Flu\u00df,", "tokens": ["Wird", "ein", "ma\u00b7gisch", "wil\u00b7der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In die sch\u00f6ne Welt hinunter", "tokens": ["In", "die", "sch\u00f6\u00b7ne", "Welt", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPO"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lockt dich dieses Stromes Gru\u00df.", "tokens": ["Lockt", "dich", "die\u00b7ses", "Stro\u00b7mes", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich mag mich nicht bewahren!", "tokens": ["Und", "ich", "mag", "mich", "nicht", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit von Euch treibt mich der Wind,", "tokens": ["Weit", "von", "Euch", "treibt", "mich", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Strome will ich fahren,", "tokens": ["Auf", "dem", "Stro\u00b7me", "will", "ich", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Glanze selig blind!", "tokens": ["Von", "dem", "Glan\u00b7ze", "se\u00b7lig", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend Stimmen lockend schlagen,", "tokens": ["Tau\u00b7send", "Stim\u00b7men", "lo\u00b7ckend", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch Aurora flammend weht,", "tokens": ["Hoch", "Au\u00b7ro\u00b7ra", "flam\u00b7mend", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fahre zu! ich mag nicht fragen,", "tokens": ["Fah\u00b7re", "zu", "!", "ich", "mag", "nicht", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Fahrt zu Ende geht!", "tokens": ["Wo", "die", "Fahrt", "zu", "En\u00b7de", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}