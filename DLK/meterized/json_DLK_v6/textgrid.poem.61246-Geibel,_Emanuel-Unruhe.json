{"textgrid.poem.61246": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Unruhe", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Wunden, schweren,", "tokens": ["An", "Wun\u00b7den", ",", "schwe\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Langsam verbluten,", "tokens": ["Lang\u00b7sam", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In heimlichen Gluten", "tokens": ["In", "heim\u00b7li\u00b7chen", "Glu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Still sich verzehren,", "tokens": ["Still", "sich", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "T\u00e4glich voll Reue", "tokens": ["T\u00e4g\u00b7lich", "voll", "Reu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Wahnsinn verschw\u00f6ren.", "tokens": ["Den", "Wahn\u00b7sinn", "ver\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "T\u00e4glich aufs neue", "tokens": ["T\u00e4g\u00b7lich", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sich wieder bet\u00f6ren,", "tokens": ["Sich", "wie\u00b7der", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ewig zum Meiden", "tokens": ["E\u00b7wig", "zum", "Mei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die Schritte wenden", "tokens": ["Die", "Schrit\u00b7te", "wen\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und doch nicht scheiden -", "tokens": ["Und", "doch", "nicht", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "O Lieb', o Leiden,", "tokens": ["O", "Lieb'", ",", "o", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Wann wirst du enden!", "tokens": ["Wann", "wirst", "du", "en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}