{"dta.poem.14392": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "id:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und manchmal da drehet", "tokens": ["Und", "manch\u00b7mal", "da", "dre\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Fackel er um \u2014", "tokens": ["Die", "Fa\u00b7ckel", "er", "um"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tiefschauernd vergehet", "tokens": ["Tief\u00b7schau\u00b7ernd", "ver\u00b7ge\u00b7het"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Welt und wird stumm.", "tokens": ["Die", "Welt", "und", "wird", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}