{"dta.poem.10090": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Pflicht-m\u00e4ssige Aufmercksamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Worin kann Adams Herrlichkeit", "tokens": ["Wo\u00b7rin", "kann", "A\u00b7dams", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Paradiese sonst bestanden haben,", "tokens": ["Jm", "Pa\u00b7ra\u00b7die\u00b7se", "sonst", "be\u00b7stan\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als da\u00df er sich an der Vollkommenheit", "tokens": ["Als", "da\u00df", "er", "sich", "an", "der", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Creaturen k\u00f6nnen laben?", "tokens": ["Der", "Crea\u00b7tu\u00b7ren", "k\u00f6n\u00b7nen", "la\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als da\u00df er seiner Seelen Kr\u00e4ffte", "tokens": ["Als", "da\u00df", "er", "sei\u00b7ner", "See\u00b7len", "Kr\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinen Sinnen so verbunden,", "tokens": ["Mit", "sei\u00b7nen", "Sin\u00b7nen", "so", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er dadurch, (o seliges Gesch\u00e4ffte)", "tokens": ["Da\u00df", "er", "da\u00b7durch", ",", "(", "o", "se\u00b7li\u00b7ges", "Ge\u00b7sch\u00e4ff\u00b7te", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "$,", "$(", "FM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In aller Creaturen Pracht", "tokens": ["In", "al\u00b7ler", "Crea\u00b7tu\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Des grossen Sch\u00f6pfers weise Macht,", "tokens": ["Des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "wei\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Voll stetiger Bewunderung, gefunden?", "tokens": ["Voll", "ste\u00b7ti\u00b7ger", "Be\u00b7wun\u00b7de\u00b7rung", ",", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "H\u00e4tt\u2019 er sie, wie wir sie betrachten, auch betrachtet,", "tokens": ["H\u00e4tt'", "er", "sie", ",", "wie", "wir", "sie", "be\u00b7trach\u00b7ten", ",", "auch", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sie nicht seines Blicks noch denckens werth geachtet;", "tokens": ["Und", "sie", "nicht", "sei\u00b7nes", "Blicks", "noch", "den\u00b7ckens", "werth", "ge\u00b7ach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So h\u00e4tt\u2019 er, so wie wir, auch keine Lust empfunden.", "tokens": ["So", "h\u00e4tt'", "er", ",", "so", "wie", "wir", ",", "auch", "kei\u00b7ne", "Lust", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das gantze Paradies, mit allen seinen Sch\u00e4tzen,", "tokens": ["Das", "gant\u00b7ze", "Pa\u00b7ra\u00b7dies", ",", "mit", "al\u00b7len", "sei\u00b7nen", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "H\u00e4tt\u2019 ihm so wenig Lust, Vergn\u00fcgen und Ergetzen", "tokens": ["H\u00e4tt'", "ihm", "so", "we\u00b7nig", "Lust", ",", "Ver\u00b7gn\u00fc\u00b7gen", "und", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In seiner Seel\u2019 erwecket und erregt,", "tokens": ["In", "sei\u00b7ner", "Seel'", "er\u00b7we\u00b7cket", "und", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Als leider ietzt die Welt uns zu erregen pflegt.", "tokens": ["Als", "lei\u00b7der", "ietzt", "die", "Welt", "uns", "zu", "er\u00b7re\u00b7gen", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Woraus denn kl\u00e4rlich zu erweisen,", "tokens": ["Wo\u00b7raus", "denn", "kl\u00e4r\u00b7lich", "zu", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, wo wir unser dencken", "tokens": ["Da\u00df", ",", "wo", "wir", "un\u00b7ser", "den\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht auf die Creaturen lencken,", "tokens": ["Nicht", "auf", "die", "Crea\u00b7tu\u00b7ren", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und in derselbigen Genu\u00df den Sch\u00f6pfer preisen,", "tokens": ["Und", "in", "der\u00b7sel\u00b7bi\u00b7gen", "Ge\u00b7nu\u00df", "den", "Sch\u00f6p\u00b7fer", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir, weder unsers Sch\u00f6pfers Willen,", "tokens": ["Wir", ",", "we\u00b7der", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der uns aus Lieb\u2019 ergetzen will, erf\u00fcllen,", "tokens": ["Der", "uns", "aus", "Lieb'", "er\u00b7get\u00b7zen", "will", ",", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Noch ie in einem Stand\u2019 auf Erden", "tokens": ["Noch", "ie", "in", "ei\u00b7nem", "Stand'", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergn\u00fcgt und gl\u00fccklich k\u00f6nnen werden.", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "gl\u00fcck\u00b7lich", "k\u00f6n\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hingegen, weil darin gewi\u00df was G\u00f6ttlichs steckt,", "tokens": ["Hin\u00b7ge\u00b7gen", ",", "weil", "da\u00b7rin", "ge\u00b7wi\u00df", "was", "G\u00f6tt\u00b7lichs", "steckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PAV", "ADV", "PWS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird dem, der GOTT zum Ruhm, sieht, h\u00f6ret, riecht und", "tokens": ["Wird", "dem", ",", "der", "GoTT", "zum", "Ruhm", ",", "sieht", ",", "h\u00f6\u00b7ret", ",", "riecht", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "$,", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Solch eine Art von Lust darin entdeckt,", "tokens": ["Solch", "ei\u00b7ne", "Art", "von", "Lust", "da\u00b7rin", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die wahrlich alle Lust der Welt,", "tokens": ["Die", "wahr\u00b7lich", "al\u00b7le", "Lust", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die man sich iemahls vorgestellt,", "tokens": ["Die", "man", "sich", "ie\u00b7mahls", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey weitem \u00fcbertrifft. Ich hab\u2019 es dann und wann", "tokens": ["Bey", "wei\u00b7tem", "\u00fc\u00b7bert\u00b7rifft", ".", "Ich", "hab'", "es", "dann", "und", "wann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und man auch dieses sich nicht selber geben kann)", "tokens": ["Und", "man", "auch", "die\u00b7ses", "sich", "nicht", "sel\u00b7ber", "ge\u00b7ben", "kann", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "PDAT", "PRF", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich sage dann und wann, versp\u00fcrt,", "tokens": ["Ich", "sa\u00b7ge", "dann", "und", "wann", ",", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PWAV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da mich ein solcher Strahl von Lust ger\u00fchrt,", "tokens": ["Da", "mich", "ein", "sol\u00b7cher", "Strahl", "von", "Lust", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df ich mit einem K\u00f6nigreich", "tokens": ["Da\u00df", "ich", "mit", "ei\u00b7nem", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie wahrlich nicht vertauschet h\u00e4tte.", "tokens": ["Sie", "wahr\u00b7lich", "nicht", "ver\u00b7tau\u00b7schet", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach grosser GOTT! weil alles Dein,", "tokens": ["Ach", "gros\u00b7ser", "GoTT", "!", "weil", "al\u00b7les", "Dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "KOUS", "PIS", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So g\u00f6nne mir, und vielen neben mir,", "tokens": ["So", "g\u00f6n\u00b7ne", "mir", ",", "und", "vie\u00b7len", "ne\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PIAT", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wir, \u00fcm Dir mit unsrer Lust zu dienen,", "tokens": ["Da\u00df", "wir", ",", "\u00fcm", "Dir", "mit", "uns\u00b7rer", "Lust", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Dein Gesch\u00f6pf empfindlich seyn!", "tokens": ["F\u00fcr", "Dein", "Ge\u00b7sch\u00f6pf", "emp\u00b7find\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach gieb, da\u00df mich nebst ihnen", "tokens": ["Ach", "gieb", ",", "da\u00df", "mich", "nebst", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein herrlich Werck mag \u00f6ffters r\u00fchren.", "tokens": ["Dein", "herr\u00b7lich", "Werck", "mag", "\u00f6ff\u00b7ters", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df solchen Ausbruch s\u00fcsser Lust,", "tokens": ["La\u00df", "sol\u00b7chen", "Aus\u00b7bruch", "s\u00fcs\u00b7ser", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In unsrer durch die Welt ger\u00fchrten Brust,", "tokens": ["In", "uns\u00b7rer", "durch", "die", "Welt", "ge\u00b7r\u00fchr\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zu Deinem Ruhm, uns offt versp\u00fcren!", "tokens": ["Zu", "Dei\u00b7nem", "Ruhm", ",", "uns", "offt", "ver\u00b7sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}