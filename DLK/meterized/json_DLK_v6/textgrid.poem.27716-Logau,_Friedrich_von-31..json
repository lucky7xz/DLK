{"textgrid.poem.27716": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat durch unser Land sich alles umgekehret;", "tokens": ["Es", "hat", "durch", "un\u00b7ser", "Land", "sich", "al\u00b7les", "um\u00b7ge\u00b7keh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum wundert mich der Brauch, da\u00df der so lange wehret,", "tokens": ["Drum", "wun\u00b7dert", "mich", "der", "Brauch", ",", "da\u00df", "der", "so", "lan\u00b7ge", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Dienstags noch und nicht man Freytags Hochzeit macht,", "tokens": ["Da\u00df", "Diens\u00b7tags", "noch", "und", "nicht", "man", "Frey\u00b7tags", "Hoch\u00b7zeit", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KON", "PTKNEG", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mehr als g\u00f6ldnes frey das schwere dienen acht'.", "tokens": ["Und", "mehr", "als", "g\u00f6ld\u00b7nes", "frey", "das", "schwe\u00b7re", "die\u00b7nen", "acht'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJA", "NN", "ART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es gienge zwar noch hin de\u00df Dienstags Hochzeit haben,", "tokens": ["Es", "gien\u00b7ge", "zwar", "noch", "hin", "de\u00df", "Diens\u00b7tags", "Hoch\u00b7zeit", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Freytages aber doch bald tod, bald seyn begraben.", "tokens": ["Frey\u00b7ta\u00b7ges", "a\u00b7ber", "doch", "bald", "tod", ",", "bald", "seyn", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "NN", "$,", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}