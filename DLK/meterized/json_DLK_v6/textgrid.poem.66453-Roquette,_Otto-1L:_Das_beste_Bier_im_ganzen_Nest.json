{"textgrid.poem.66453": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das beste Bier im ganzen Nest", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das beste Bier im ganzen Nest", "tokens": ["Das", "bes\u00b7te", "Bier", "im", "gan\u00b7zen", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schenkt Margreth am Thore,", "tokens": ["Das", "schenkt", "Mar\u00b7greth", "am", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Derweil das frisch den Gaumen n\u00e4\u00dft", "tokens": ["Der\u00b7weil", "das", "frisch", "den", "Gau\u00b7men", "n\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht hold Margreth zum Ohre.", "tokens": ["Spricht", "hold", "Mar\u00b7greth", "zum", "Oh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Steht vor der Th\u00fcr ein Lindenbaum,", "tokens": ["Steht", "vor", "der", "Th\u00fcr", "ein", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da schenkt sie mir den k\u00fchlen Schaum,", "tokens": ["Da", "schenkt", "sie", "mir", "den", "k\u00fch\u00b7len", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Margreth, Margreth am Thore.", "tokens": ["Mar\u00b7greth", ",", "Mar\u00b7greth", "am", "Tho\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst n\u00e4chtens hatt' ich keine Ruh,", "tokens": ["J\u00fcngst", "n\u00e4ch\u00b7tens", "hatt'", "ich", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir war so weh, so bange,", "tokens": ["Mir", "war", "so", "weh", ",", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKVZ", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wandert' ich der Linde zu,", "tokens": ["Da", "wan\u00b7dert'", "ich", "der", "Lin\u00b7de", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leiden w\u00e4hrt' nicht lange!", "tokens": ["Mein", "Lei\u00b7den", "w\u00e4hrt'", "nicht", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mond ging auf so wundersam \u2013", "tokens": ["Der", "Mond", "ging", "auf", "so", "wun\u00b7der\u00b7sam", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Margreth, steh' auf! Margreth sie kam,", "tokens": ["Mar\u00b7greth", ",", "steh'", "auf", "!", "Mar\u00b7greth", "sie", "kam", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$.", "NE", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Margreth, Margreth am Thore!", "tokens": ["Mar\u00b7greth", ",", "Mar\u00b7greth", "am", "Tho\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Und wandr' ich einstens wiedrum aus,", "tokens": ["Und", "wandr'", "ich", "eins\u00b7tens", "wie\u00b7drum", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ganze Nest vergess' ich,", "tokens": ["Das", "gan\u00b7ze", "Nest", "ver\u00b7ge\u00b7ss'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Margrethlein hold im Lindenhaus,", "tokens": ["Mar\u00b7gret\u00b7hlein", "hold", "im", "Lin\u00b7den\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dein denk' ich unabl\u00e4ssig!", "tokens": ["Dein", "denk'", "ich", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mond, dazu die goldnen Stern',", "tokens": ["Der", "Mond", ",", "da\u00b7zu", "die", "gold\u00b7nen", "Stern'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach k\u00f6nnten sie's, sie sagten's gern,", "tokens": ["Ach", "k\u00f6nn\u00b7ten", "sie's", ",", "sie", "sag\u00b7ten's", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PIS", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Margreth, Margreth am Thore!", "tokens": ["Mar\u00b7greth", ",", "Mar\u00b7greth", "am", "Tho\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}