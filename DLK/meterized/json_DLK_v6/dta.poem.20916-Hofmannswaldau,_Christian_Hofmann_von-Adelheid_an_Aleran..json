{"dta.poem.20916": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Adelheid an Aleran.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ach! wie reimt sich ach/ und Liebe doch", "tokens": ["Ach", "ach", "!", "wie", "reimt", "sich", "ach", "/", "und", "Lie\u00b7be", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "PWAV", "VVFIN", "PRF", "XY", "$(", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zusammen?", "tokens": ["zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Was aber reimt sich nicht/ wann Zeit und", "tokens": ["Was", "a\u00b7ber", "reimt", "sich", "nicht", "/", "wann", "Zeit", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PRF", "PTKNEG", "$(", "PWAV", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Himmel schafft/", "tokens": ["Him\u00b7mel", "schafft", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der Furchte dickes Ei\u00df bestrickt die Liebes Flammer", "tokens": ["Der", "Furch\u00b7te", "di\u00b7ckes", "Ei\u00df", "be\u00b7strickt", "die", "Lie\u00b7bes", "Flam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich werde durch die Hand der Aengsten hingeraft.", "tokens": ["Ich", "wer\u00b7de", "durch", "die", "Hand", "der", "A\u00b7engs\u00b7ten", "hin\u00b7ge\u00b7raft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ich b\u00f6be wie ein Laub bewegt durch Kummer Winde/", "tokens": ["Ich", "b\u00f6\u00b7be", "wie", "ein", "Laub", "be\u00b7wegt", "durch", "Kum\u00b7mer", "Win\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es plaget meinen Geist Verlust und auch Gewinn/", "tokens": ["Es", "pla\u00b7get", "mei\u00b7nen", "Geist", "Ver\u00b7lust", "und", "auch", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "KON", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich werd\u2019 aus bleicher Noth zu einem rechten Kinde/", "tokens": ["Ich", "werd'", "aus", "blei\u00b7cher", "Noth", "zu", "ei\u00b7nem", "rech\u00b7ten", "Kin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach da\u00df ich nicht als Kind vorl\u00e4ngst gestorben bin.", "tokens": ["Ach", "da\u00df", "ich", "nicht", "als", "Kind", "vor\u00b7l\u00e4ngst", "ge\u00b7stor\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PTKNEG", "KOUS", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Aeltern Nahmen ist in meinen d\u00fcnnen Ohren/", "tokens": ["Der", "A\u00b7el\u00b7tern", "Nah\u00b7men", "ist", "in", "mei\u00b7nen", "d\u00fcn\u00b7nen", "Oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Wie ein Beschwerungs Wort und wie ein Donner-", "tokens": ["Wie", "ein", "Be\u00b7schwe\u00b7rungs", "Wort", "und", "wie", "ein", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "PWAV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "schlag/", "tokens": ["schlag", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Ach wer\u2019 ich nur zuvor gestorben/ als gebohren!", "tokens": ["Ach", "wer'", "ich", "nur", "zu\u00b7vor", "ge\u00b7stor\u00b7ben", "/", "als", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PPER", "ADV", "ADV", "VVPP", "$(", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df ich doch nicht alsbald ein Unding werden mag;", "tokens": ["Da\u00df", "ich", "doch", "nicht", "als\u00b7bald", "ein", "Un\u00b7ding", "wer\u00b7den", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mich deucht/ der gantze Hoff erkent was ich begangen/", "tokens": ["Mich", "deucht", "/", "der", "gant\u00b7ze", "Hoff", "er\u00b7kent", "was", "ich", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "VVPP", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mich deucht/ ein ieder Mensch verweist mir meine", "tokens": ["Mich", "deucht", "/", "ein", "ie\u00b7der", "Mensch", "ver\u00b7weist", "mir", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ART", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "That/", "tokens": ["That", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Die Bluhmen wachsen noch aus Scham auf meinen", "tokens": ["Die", "Bluh\u00b7men", "wach\u00b7sen", "noch", "aus", "Scham", "auf", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wangen/", "tokens": ["Wan\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Die sonst mein schwacher Leib aus Lust verlohren hat.", "tokens": ["Die", "sonst", "mein", "schwa\u00b7cher", "Leib", "aus", "Lust", "ver\u00b7loh\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich bin der Perle gleich/ die Flecke hat bekommen/", "tokens": ["Ich", "bin", "der", "Per\u00b7le", "gleich", "/", "die", "Fle\u00b7cke", "hat", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$(", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und von des Keysers Haubt an schlechte H\u00e4lse mu\u00df/", "tokens": ["Und", "von", "des", "Key\u00b7sers", "Haubt", "an", "schlech\u00b7te", "H\u00e4l\u00b7se", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mir ist nunmehr mein Glantz und auch mein Werth", "tokens": ["Mir", "ist", "nun\u00b7mehr", "mein", "Glantz", "und", "auch", "mein", "Werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "entnommen/", "tokens": ["ent\u00b7nom\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Und dieses alles f\u00e4llt durch einen s\u00fcssen Ku\u00df.", "tokens": ["Und", "die\u00b7ses", "al\u00b7les", "f\u00e4llt", "durch", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Di\u00df schwer\u2019 ich/ da\u00df mein Leib ein Garten ist gewesen/", "tokens": ["Di\u00df", "schwer'", "ich", "/", "da\u00df", "mein", "Leib", "ein", "Gar\u00b7ten", "ist", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der stets verschlossen war als wie das Paradies/", "tokens": ["Der", "stets", "ver\u00b7schlos\u00b7sen", "war", "als", "wie", "das", "Pa\u00b7ra\u00b7dies", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "KOUS", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich wei\u00df das keine Hand hier Bluhmen hat gelesen/", "tokens": ["Ich", "wei\u00df", "das", "kei\u00b7ne", "Hand", "hier", "Bluh\u00b7men", "hat", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "ADV", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df kein geiler Wind durch meine Bl\u00e4tter blie\u00df;", "tokens": ["Und", "da\u00df", "kein", "gei\u00b7ler", "Wind", "durch", "mei\u00b7ne", "Bl\u00e4t\u00b7ter", "blie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was hilft uns aber doch zuseyn und nicht zubleiben/", "tokens": ["Was", "hilft", "uns", "a\u00b7ber", "doch", "zu\u00b7seyn", "und", "nicht", "zu\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KON", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Verflossen Wasser mahlt doch keine K\u00f6rner nicht;", "tokens": ["Ver\u00b7flos\u00b7sen", "Was\u00b7ser", "mahlt", "doch", "kei\u00b7ne", "K\u00f6r\u00b7ner", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es wird die Affter Welt nur meinen Fall beschreiben/", "tokens": ["Es", "wird", "die", "Aff\u00b7ter", "Welt", "nur", "mei\u00b7nen", "Fall", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und was ich guts gestift schaut nicht das Tage Licht", "tokens": ["Und", "was", "ich", "guts", "ge\u00b7stift", "schaut", "nicht", "das", "Ta\u00b7ge", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "NN", "VVPP", "VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Menschen Urthel Spruch vergleichet sich den Flie-", "tokens": ["Der", "Men\u00b7schen", "Ur\u00b7thel", "Spruch", "ver\u00b7glei\u00b7chet", "sich", "den", "Flie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.37": {"text": "Sie fallen nur Geschw\u00fcr und Eyter Beulen an/", "tokens": ["Sie", "fal\u00b7len", "nur", "Ge\u00b7schw\u00fcr", "und", "Ey\u00b7ter", "Beu\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Fehler unser Zeit/ die werden nicht verschwiegen/", "tokens": ["Die", "Feh\u00b7ler", "un\u00b7ser", "Zeit", "/", "die", "wer\u00b7den", "nicht", "ver\u00b7schwie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "PDS", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nur di\u00df bleibt unbekant was man hat guts gethan.", "tokens": ["Nur", "di\u00df", "bleibt", "un\u00b7be\u00b7kant", "was", "man", "hat", "guts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ADJD", "PWS", "PIS", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mein fromseyn machte mich zum Ph\u00f6nix in dem Lan-", "tokens": ["Mein", "from\u00b7seyn", "mach\u00b7te", "mich", "zum", "Ph\u00f6\u00b7nix", "in", "dem", "Lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "de/", "tokens": ["de", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.42": {"text": "Nach dem ich aber mich in b\u00f6ser Gluth verbrennt/", "tokens": ["Nach", "dem", "ich", "a\u00b7ber", "mich", "in", "b\u00f6\u00b7ser", "Gluth", "ver\u00b7brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So giebt die Asche nichts als Eulen voller Schande/", "tokens": ["So", "giebt", "die", "A\u00b7sche", "nichts", "als", "Eu\u00b7len", "vol\u00b7ler", "Schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIS", "KOKOM", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ach da\u00df ein keusches Weib noch meinen Namen nen\u0303t.", "tokens": ["Ach", "da\u00df", "ein", "keu\u00b7sches", "Weib", "noch", "mei\u00b7nen", "Na\u00b7men", "ne\u00f1t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Der Ungarn weites Landt wird ungern h\u00f6ren m\u00fcssen/", "tokens": ["Der", "Un\u00b7garn", "wei\u00b7tes", "Landt", "wird", "un\u00b7gern", "h\u00f6\u00b7ren", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df nicht die Crone mir kan auf den Wirbel stehn/", "tokens": ["Da\u00df", "nicht", "die", "Cro\u00b7ne", "mir", "kan", "auf", "den", "Wir\u00b7bel", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", 