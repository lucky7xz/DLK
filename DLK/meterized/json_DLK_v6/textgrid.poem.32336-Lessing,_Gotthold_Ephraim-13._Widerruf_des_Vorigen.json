{"textgrid.poem.32336": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "13. Widerruf des Vorigen", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte so ein Gut nicht haben.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "so", "ein", "Gut", "nicht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn sollt' ich auch die sechste drauf begraben:", "tokens": ["Denn", "sollt'", "ich", "auch", "die", "sechs\u00b7te", "drauf", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "ADJA", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6nnt' ich doch leicht \u2013 nicht wahr, Pompil? \u2013", "tokens": ["K\u00f6nnt'", "ich", "doch", "leicht", "\u2013", "nicht", "wahr", ",", "Pom\u00b7pil", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "$(", "PTKNEG", "ADJD", "$,", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sechs gute Tage nur erlebet haben.", "tokens": ["Sechs", "gu\u00b7te", "Ta\u00b7ge", "nur", "er\u00b7le\u00b7bet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}