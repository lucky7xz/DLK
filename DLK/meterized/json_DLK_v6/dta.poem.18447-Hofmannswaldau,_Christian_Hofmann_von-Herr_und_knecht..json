{"dta.poem.18447": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Herr und knecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein herr versprach dem knecht/ da\u00df er sein kleid soll erben/", "tokens": ["Ein", "herr", "ver\u00b7sprach", "dem", "knecht", "/", "da\u00df", "er", "sein", "kleid", "soll", "er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der knecht grief fort darnach/ und wolt es nehmen hin.", "tokens": ["Der", "knecht", "grief", "fort", "dar\u00b7nach", "/", "und", "wolt", "es", "neh\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PAV", "$(", "KON", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein/ sprach der herr/ verzieh/ bi\u00df ich gestorben bin/", "tokens": ["Nein", "/", "sprach", "der", "herr", "/", "ver\u00b7zieh", "/", "bi\u00df", "ich", "ge\u00b7stor\u00b7ben", "bin", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "ART", "NN", "$(", "VVFIN", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So k\u00f6nnt ihr/ sprach der knecht/ hent oder morgen sterben.", "tokens": ["So", "k\u00f6nnt", "ihr", "/", "sprach", "der", "knecht", "/", "hent", "o\u00b7der"