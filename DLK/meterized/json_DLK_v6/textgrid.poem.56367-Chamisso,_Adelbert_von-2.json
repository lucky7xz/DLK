{"textgrid.poem.56367": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein Gott, mein Gott, so hast du mich verlassen!\u00ab", "tokens": ["\u00bb", "mein", "Gott", ",", "mein", "Gott", ",", "so", "hast", "du", "mich", "ver\u00b7las\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Dome ward zu Nacht der Ruf vernommen;", "tokens": ["Im", "Do\u00b7me", "ward", "zu", "Nacht", "der", "Ruf", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer ihn erhob? sie wu\u00dften's nicht zu fassen.", "tokens": ["Wer", "ihn", "er\u00b7hob", "?", "sie", "wu\u00df\u00b7ten's", "nicht", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Hochaltar, worauf ein Licht geglommen,", "tokens": ["Am", "Hoc\u00b7hal\u00b7tar", ",", "wo\u00b7rauf", "ein", "Licht", "ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bewegte sich gespenstisch die Gestalt,", "tokens": ["Be\u00b7weg\u00b7te", "sich", "ge\u00b7spens\u00b7tisch", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus deren Mund der Schmerzensschrei gekommen.", "tokens": ["Aus", "de\u00b7ren", "Mund", "der", "Schmer\u00b7zens\u00b7schrei", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie warf sich dann zur Erde, mit Gewalt", "tokens": ["Sie", "warf", "sich", "dann", "zur", "Er\u00b7de", ",", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Stirne schlagend an des Estrichs Steine,", "tokens": ["Die", "Stir\u00b7ne", "schla\u00b7gend", "an", "des", "Est\u00b7richs", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die W\u00f6lbung hat vom Schalle widerhallt.", "tokens": ["Die", "W\u00f6l\u00b7bung", "hat", "vom", "Schal\u00b7le", "wi\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dann war's, als ob sie unaufhaltsam weine,", "tokens": ["Dann", "wa\u00b7r's", ",", "als", "ob", "sie", "un\u00b7auf\u00b7halt\u00b7sam", "wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Und in den Tr\u00e4nen Linderung gefunden;", "tokens": ["Und", "in", "den", "Tr\u00e4\u00b7nen", "Lin\u00b7de\u00b7rung", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie st\u00f6hnte bei der Kerze letztem Scheine.", "tokens": ["Sie", "st\u00f6hn\u00b7te", "bei", "der", "Ker\u00b7ze", "letz\u00b7tem", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und als der Nacht unheimlich bange Stunden", "tokens": ["Und", "als", "der", "Nacht", "un\u00b7heim\u00b7lich", "ban\u00b7ge", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Verflossen und der Morgen sich erhellt,", "tokens": ["Ver\u00b7flos\u00b7sen", "und", "der", "Mor\u00b7gen", "sich", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "War's still, und die Erscheinung war verschwunden.", "tokens": ["Wa\u00b7r's", "still", ",", "und", "die", "Er\u00b7schei\u00b7nung", "war", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Nun eilt zum Kirchgang die erwachte Welt,", "tokens": ["Nun", "eilt", "zum", "Kirch\u00b7gang", "die", "er\u00b7wach\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Es dr\u00e4ngen sich die Chorherrn zum Altar;", "tokens": ["Es", "dr\u00e4n\u00b7gen", "sich", "die", "Chor\u00b7herrn", "zum", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Drauf ragt ein Kruzifix, erst aufgestellt. \u2013", "tokens": ["Drauf", "ragt", "ein", "Kru\u00b7zi\u00b7fix", ",", "erst", "auf\u00b7ge\u00b7stellt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Gnadenbild, wie nie noch eines war;", "tokens": ["Ein", "Gna\u00b7den\u00b7bild", ",", "wie", "nie", "noch", "ei\u00b7nes", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So hat der Gott den Todeskampf gerungen,", "tokens": ["So", "hat", "der", "Gott", "den", "To\u00b7des\u00b7kampf", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "So bracht er sich f\u00fcr uns zum Opfer dar.", "tokens": ["So", "bracht", "er", "sich", "f\u00fcr", "uns", "zum", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Es sehend, schreit der S\u00fcnder reudurchdrungen", "tokens": ["Es", "se\u00b7hend", ",", "schreit", "der", "S\u00fcn\u00b7der", "reu\u00b7durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Zu dem, der S\u00fcndern auch das Heil gebracht,", "tokens": ["Zu", "dem", ",", "der", "S\u00fcn\u00b7dern", "auch", "das", "Heil", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und: \u00bbChrist' eleison!\u00ab schallt von allen Zungen.", "tokens": ["Und", ":", "\u00bb", "Christ'", "e\u00b7lei\u00b7son", "!", "\u00ab", "schallt", "von", "al\u00b7len", "Zun\u00b7gen", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "FM.xy", "FM.xy", "$.", "$(", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Nicht scheint das Werk von Menschenhand gemacht;", "tokens": ["Nicht", "scheint", "das", "Werk", "von", "Men\u00b7schen\u00b7hand", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wer m\u00f6chte so das G\u00f6ttliche gestalten?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "so", "das", "G\u00f6tt\u00b7li\u00b7che", "ge\u00b7stal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wie seltsam stieg es auf im Scho\u00df der Nacht? \u2013", "tokens": ["Wie", "selt\u00b7sam", "stieg", "es", "auf", "im", "Scho\u00df", "der", "Nacht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Des Meisters ist es, der uns hingehalten", "tokens": ["Des", "Meis\u00b7ters", "ist", "es", ",", "der", "uns", "hin\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Mit Ausflucht lange z\u00f6gernd, zweifelsohne", "tokens": ["Mit", "Aus\u00b7flucht", "lan\u00b7ge", "z\u00f6\u00b7gernd", ",", "zwei\u00b7fel\u00b7soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Das \u00c4u\u00dferste der Kunst noch zu entfalten. \u2013", "tokens": ["Das", "\u00c4u\u00b7\u00dfers\u00b7te", "der", "Kunst", "noch", "zu", "ent\u00b7fal\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Was bringen wir dem Trefflichen zum Lohne?", "tokens": ["Was", "brin\u00b7gen", "wir", "dem", "Treff\u00b7li\u00b7chen", "zum", "Loh\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Es ist das Gold, das schlechte, nicht genug;", "tokens": ["Es", "ist", "das", "Gold", ",", "das", "schlech\u00b7te", ",", "nicht", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Geb\u00fchrt dem Edlen nicht die Lorbeerkrone?", "tokens": ["Ge\u00b7b\u00fchrt", "dem", "Ed\u00b7len", "nicht", "die", "Lor\u00b7beer\u00b7kro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und bald geordnet ward ein Ehrenzug,", "tokens": ["Und", "bald", "ge\u00b7ord\u00b7net", "ward", "ein", "Eh\u00b7ren\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "An welchem Lai und Priester Anteil nahmen;", "tokens": ["An", "wel\u00b7chem", "Lai", "und", "Pries\u00b7ter", "An\u00b7teil", "nah\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Voran ging, der den gr\u00fcnen Lorbeer trug.", "tokens": ["Vo\u00b7ran", "ging", ",", "der", "den", "gr\u00fc\u00b7nen", "Lor\u00b7beer", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und wie sie vor des Meisters Wohnung kamen,", "tokens": ["Und", "wie", "sie", "vor", "des", "Meis\u00b7ters", "Woh\u00b7nung", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "War weitge\u00f6ffnet, aber still das Haus,", "tokens": ["War", "weit\u00b7ge\u00f6ff\u00b7net", ",", "a\u00b7ber", "still", "das", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.39": {"text": "Auch still beim Widerhall von seinem Namen.", "tokens": ["Auch", "still", "beim", "Wi\u00b7der\u00b7hall", "von", "sei\u00b7nem", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Wohl schallten Pauk und Cymbeln mit Gebraus", "tokens": ["Wohl", "schall\u00b7ten", "Pauk", "und", "Cym\u00b7beln", "mit", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Zu der Drommeten gellend hellem Ton,", "tokens": ["Zu", "der", "Drom\u00b7me\u00b7ten", "gel\u00b7lend", "hel\u00b7lem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Doch niemand kam zum Festempfang heraus.", "tokens": ["Doch", "nie\u00b7mand", "kam", "zum", "Fes\u00b7temp\u00b7fang", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Ver\u00f6det war das Haus am Morgen schon,", "tokens": ["Ver\u00b7\u00f6\u00b7det", "war", "das", "Haus", "am", "Mor\u00b7gen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Aus dem ein Nachbar sich entfernen nur", "tokens": ["Aus", "dem", "ein", "Nach\u00b7bar", "sich", "ent\u00b7fer\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Sah pilgernd einen schlichten Menschensohn.", "tokens": ["Sah", "pil\u00b7gernd", "ei\u00b7nen", "schlich\u00b7ten", "Men\u00b7schen\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Die Herren traten sp\u00e4hend auf den Flur,", "tokens": ["Die", "Her\u00b7ren", "tra\u00b7ten", "sp\u00e4\u00b7hend", "auf", "den", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Sie brachen sich durch w\u00fcste Zimmer Bahn,", "tokens": ["Sie", "bra\u00b7chen", "sich", "durch", "w\u00fcs\u00b7te", "Zim\u00b7mer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Sie trafen nicht auf eines Menschen Spur;", "tokens": ["Sie", "tra\u00b7fen", "nicht", "auf", "ei\u00b7nes", "Men\u00b7schen", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Sie riefen, ohne Antwort zu empfahn,", "tokens": ["Sie", "rie\u00b7fen", ",", "oh\u00b7ne", "Ant\u00b7wort", "zu", "em\u00b7pfahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Und h\u00f6rten leer die R\u00e4ume widerhallen;", "tokens": ["Und", "h\u00f6r\u00b7ten", "leer", "die", "R\u00e4u\u00b7me", "wi\u00b7der\u00b7hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Sie drangen in die Werkstatt: was sie sahn \u2013", "tokens": ["Sie", "dran\u00b7gen", "in", "die", "Werk\u00b7statt", ":", "was", "sie", "sahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Dar\u00fcber l\u00e4\u00dft das Lied den Schleier fallen.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "l\u00e4\u00dft", "das", "Lied", "den", "Schlei\u00b7er", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}