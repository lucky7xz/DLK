{"dta.poem.18660": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Phaenomen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn zu der Regenwand", "tokens": ["Wenn", "zu", "der", "Re\u00b7gen\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Phoebus sich gattet,", "tokens": ["Phoe\u00b7bus", "sich", "gat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gleich steht ein Bogenrand", "tokens": ["Gleich", "steht", "ein", "Bo\u00b7gen\u00b7rand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Farbig beschattet.", "tokens": ["Far\u00b7big", "be\u00b7schat\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Nebel gleichen Kreis", "tokens": ["Im", "Ne\u00b7bel", "glei\u00b7chen", "Kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seh ich gezogen,", "tokens": ["Seh", "ich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zwar ist der Bogen weiss,", "tokens": ["Zwar", "ist", "der", "Bo\u00b7gen", "weiss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch Himmelsbogen.", "tokens": ["Doch", "Him\u00b7mels\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So sollst du, muntrer Greis,", "tokens": ["So", "sollst", "du", ",", "mun\u00b7trer", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dich nicht betr\u00fcben,", "tokens": ["Dich", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sind gleich die Haare weiss,", "tokens": ["Sind", "gleich", "die", "Haa\u00b7re", "weiss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wirst du lieben.", "tokens": ["Doch", "wirst", "du", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}