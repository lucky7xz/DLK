{"textgrid.poem.67543": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: O schwarze Nacht! wer hat ihn, Deinen Schleier,", "genre": "verse", "period": "N.A.", "pub_year": 1768, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schwarze Nacht! wer hat ihn, Deinen Schleier,", "tokens": ["O", "schwar\u00b7ze", "Nacht", "!", "wer", "hat", "ihn", ",", "Dei\u00b7nen", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWS", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Je aufgedeckt?", "tokens": ["Je", "auf\u00b7ge\u00b7deckt", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du warst einst All; da kam ein Funke Feuer", "tokens": ["Du", "warst", "einst", "All", ";", "da", "kam", "ein", "Fun\u00b7ke", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hat den Weltschein aufgeweckt,", "tokens": ["Und", "hat", "den", "Welt\u00b7schein", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der jetzt noch ist. In ew'gem Wechselkreise", "tokens": ["Der", "jetzt", "noch", "ist", ".", "In", "ew'\u00b7gem", "Wech\u00b7sel\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VAFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Tag und Nacht", "tokens": ["Mit", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Rollt er hinweg! mir, bis ich meine Reise,", "tokens": ["Rollt", "er", "hin\u00b7weg", "!", "mir", ",", "bis", "ich", "mei\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die kurze Reise, bald vollbracht;", "tokens": ["Die", "kur\u00b7ze", "Rei\u00b7se", ",", "bald", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann geb' ich Euch, die Ihr ihn gabet, wieder,", "tokens": ["Dann", "geb'", "ich", "Euch", ",", "die", "Ihr", "ihn", "ga\u00b7bet", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nacht oder Licht!", "tokens": ["Nacht", "o\u00b7der", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Dem Weltgeist meinen Geist, und sinke nieder,", "tokens": ["Dem", "Welt\u00b7geist", "mei\u00b7nen", "Geist", ",", "und", "sin\u00b7ke", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sei ich dann, oder sei ich nicht!", "tokens": ["Sei", "ich", "dann", ",", "o\u00b7der", "sei", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}