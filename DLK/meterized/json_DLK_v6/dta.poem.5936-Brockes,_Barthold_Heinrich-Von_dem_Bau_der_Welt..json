{"dta.poem.5936": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit solchen zierlichen Beschreibungen war immer", "tokens": ["Mit", "sol\u00b7chen", "zier\u00b7li\u00b7chen", "Be\u00b7schrei\u00b7bun\u00b7gen", "war", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+---++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Die alte Poesie", "tokens": ["Die", "al\u00b7te", "Poe\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Besch\u00e4fftigt und geschm\u00fcckt; und die Philosophie", "tokens": ["Be\u00b7sch\u00e4ff\u00b7tigt", "und", "ge\u00b7schm\u00fcckt", ";", "und", "die", "Phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bem\u00fchete sich stets, und unterlie\u00df es nimmer,", "tokens": ["Be\u00b7m\u00fc\u00b7he\u00b7te", "sich", "stets", ",", "und", "un\u00b7ter\u00b7lie\u00df", "es", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In ihren Lehren", "tokens": ["In", "ih\u00b7ren", "Leh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die tieffen Wunder zu erkl\u00e4ren.", "tokens": ["Die", "tief\u00b7fen", "Wun\u00b7der", "zu", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein fast verwegner Trieb bracht Epicur dahin,", "tokens": ["Ein", "fast", "ver\u00b7weg\u00b7ner", "Trieb", "bracht", "E\u00b7pi\u00b7cur", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er sich millionen Welte", "tokens": ["Da\u00df", "er", "sich", "mil\u00b7li\u00b7o\u00b7nen", "Wel\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In seinen Sinn,", "tokens": ["In", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und zwar ohn\u2019 End und Ziel, vor Augen stellte.", "tokens": ["Und", "zwar", "ohn'", "End", "und", "Ziel", ",", "vor", "Au\u00b7gen", "stell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}