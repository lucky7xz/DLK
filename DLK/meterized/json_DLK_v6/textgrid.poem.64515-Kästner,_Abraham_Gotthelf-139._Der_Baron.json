{"textgrid.poem.64515": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "139. Der Baron", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Sch\u00f6nen, wie Johann ihm von der Stra\u00dfe brachte,", "tokens": ["Mit", "Sch\u00f6\u00b7nen", ",", "wie", "Jo\u00b7hann", "ihm", "von", "der", "Stra\u00b7\u00dfe", "brach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "NE", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertrieb sich der Baron zuweilen seine Zeit;", "tokens": ["Ver\u00b7trieb", "sich", "der", "Ba\u00b7ron", "zu\u00b7wei\u00b7len", "sei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wirthinn, eine Frau, die klug und edel dachte,", "tokens": ["Die", "Wirt\u00b7hinn", ",", "ei\u00b7ne", "Frau", ",", "die", "klug", "und", "e\u00b7del", "dach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schalt ihn: Durch den Besuch wird ja mein Haus entweiht!", "tokens": ["Schalt", "ihn", ":", "Durch", "den", "Be\u00b7such", "wird", "ja", "mein", "Haus", "ent\u00b7weiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "APPR", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "\u00bbmadam, wollt' ich mein Geld an ihre T\u00f6chter wenden,", "tokens": ["\u00bb", "ma\u00b7dam", ",", "wollt'", "ich", "mein", "Geld", "an", "ih\u00b7re", "T\u00f6ch\u00b7ter", "wen\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kriegt man sie ja nie aus der Franzosen H\u00e4nden.\u00ab", "tokens": ["So", "kriegt", "man", "sie", "ja", "nie", "aus", "der", "Fran\u00b7zo\u00b7sen", "H\u00e4n\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}