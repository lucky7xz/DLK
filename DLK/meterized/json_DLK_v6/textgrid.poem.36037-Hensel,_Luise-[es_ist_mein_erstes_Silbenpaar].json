{"textgrid.poem.36037": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[es ist mein erstes Silbenpaar]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mein erstes Silbenpaar", "tokens": ["Es", "ist", "mein", "ers\u00b7tes", "Sil\u00b7ben\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So spiegelhell und silberklar.", "tokens": ["So", "spie\u00b7gel\u00b7hell", "und", "sil\u00b7ber\u00b7klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4r's uns nicht gegeben,", "tokens": ["Und", "w\u00e4r's", "uns", "nicht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer m\u00f6chte dann wohl leben?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "dann", "wohl", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch Freund, die dritte Silbe mein", "tokens": ["Doch", "Freund", ",", "die", "drit\u00b7te", "Sil\u00b7be", "mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann dir bedenklich werden,", "tokens": ["Kann", "dir", "be\u00b7denk\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum achte auf die Tritte dein,", "tokens": ["Drum", "ach\u00b7te", "auf", "die", "Trit\u00b7te", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Pilgersmann auf Erden.", "tokens": ["Du", "Pil\u00b7gers\u00b7mann", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du wirst's gewi\u00df mit bitt'rem Leid,", "tokens": ["Du", "wir\u00b7st's", "ge\u00b7wi\u00df", "mit", "bitt'\u00b7rem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So du's getan, bedauern,", "tokens": ["So", "du's", "ge\u00b7tan", ",", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mancher wird in Ewigkeit", "tokens": ["Und", "man\u00b7cher", "wird", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergeblich mich betrauern.", "tokens": ["Ver\u00b7geb\u00b7lich", "mich", "be\u00b7trau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn dein Aug', dein lauschend Ohr", "tokens": ["Und", "wenn", "dein", "Aug'", ",", "dein", "lau\u00b7schend", "Ohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ganzen dich erfreuet,", "tokens": ["Am", "Gan\u00b7zen", "dich", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So heb' dein Herz zu dem empor,", "tokens": ["So", "heb'", "dein", "Herz", "zu", "dem", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der solche Pracht verleihet.", "tokens": ["Der", "sol\u00b7che", "Pracht", "ver\u00b7lei\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}