{"dta.poem.23604": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein ist das Recht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Herr in meinem Rechte,", "tokens": ["Ich", "bin", "ein", "Herr", "in", "mei\u00b7nem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dieses Recht, es ist mein Haus:", "tokens": ["Und", "die\u00b7ses", "Recht", ",", "es", "ist", "mein", "Haus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wagt es, welcher Knecht der Knechte,", "tokens": ["Wer", "wagt", "es", ",", "wel\u00b7cher", "Knecht", "der", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und will vertreiben mich daraus?", "tokens": ["Und", "will", "ver\u00b7trei\u00b7ben", "mich", "da\u00b7raus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PRF", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr k\u00f6nnt's belagern, k\u00f6nnt's berennen \u2014", "tokens": ["Ihr", "k\u00f6nnt's", "be\u00b7la\u00b7gern", ",", "k\u00f6nnt's", "be\u00b7ren\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber weiche nicht daraus;", "tokens": ["Ich", "a\u00b7ber", "wei\u00b7che", "nicht", "da\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr k\u00f6nnt's zertr\u00fcmmern, k\u00f6nnt's verbrennen \u2014", "tokens": ["Ihr", "k\u00f6nnt's", "zer\u00b7tr\u00fcm\u00b7mern", ",", "k\u00f6nnt's", "ver\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein ist das Recht, das Recht mein Haus!", "tokens": ["Mein", "ist", "das", "Recht", ",", "das", "Recht", "mein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}