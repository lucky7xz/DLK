{"textgrid.poem.46451": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Weib bist Schale nur und Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Weib bist Schale nur und Spiegel", "tokens": ["Du", "Weib", "bist", "Scha\u00b7le", "nur", "und", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Taten, die mein Hirn erzeugt,", "tokens": ["Der", "Ta\u00b7ten", ",", "die", "mein", "Hirn", "er\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn nur durch meines Kopfes Tiegel", "tokens": ["Denn", "nur", "durch", "mei\u00b7nes", "Kop\u00b7fes", "Tie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das All nach Immer-Formung keucht.", "tokens": ["Das", "All", "nach", "Im\u00b7mer\u00b7For\u00b7mung", "keucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin der Mann, ich bin der Wille,", "tokens": ["Ich", "bin", "der", "Mann", ",", "ich", "bin", "der", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und du bist Weib und bist die Stille \u2013", "tokens": ["Und", "du", "bist", "Weib", "und", "bist", "die", "Stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du bist die Ader, bist die Stirn,", "tokens": ["Du", "bist", "die", "A\u00b7der", ",", "bist", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch ich bin Blut und hei\u00dfes Hirn,", "tokens": ["Doch", "ich", "bin", "Blut", "und", "hei\u00b7\u00dfes", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich bin der Keim in deinem Scho\u00df:", "tokens": ["Ich", "bin", "der", "Keim", "in", "dei\u00b7nem", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich sprenge ihn ganz mitleidlos.", "tokens": ["Ich", "spren\u00b7ge", "ihn", "ganz", "mit\u00b7leid\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch:", "tokens": ["Doch", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Bin ich auch Schrei und du nur Kehle,", "tokens": ["Bin", "ich", "auch", "Schrei", "und", "du", "nur", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bin ich Orkan und du nur Ruh:", "tokens": ["Bin", "ich", "Or\u00b7kan", "und", "du", "nur", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So bist du Leib doch meiner Seele:", "tokens": ["So", "bist", "du", "Leib", "doch", "mei\u00b7ner", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und Mensch bin ich und Mensch bist du!", "tokens": ["Und", "Mensch", "bin", "ich", "und", "Mensch", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "KON", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und nur als Eins sind wir Vollendung:", "tokens": ["Und", "nur", "als", "Eins", "sind", "wir", "Vol\u00b7len\u00b7dung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "So w\u00e4chst in unserm Einen Scho\u00df", "tokens": ["So", "w\u00e4chst", "in", "un\u00b7serm", "Ei\u00b7nen", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mein Sch\u00f6pferdrang in Taten gro\u00df:", "tokens": ["Mein", "Sch\u00f6p\u00b7fer\u00b7drang", "in", "Ta\u00b7ten", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und du bist Werk und Alles-Endung.", "tokens": ["Und", "du", "bist", "Werk", "und", "Al\u00b7les\u00b7En\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}