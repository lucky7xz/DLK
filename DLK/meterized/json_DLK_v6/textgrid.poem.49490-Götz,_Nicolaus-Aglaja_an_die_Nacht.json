{"textgrid.poem.49490": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Aglaja an die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1747, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einer Bank von Moos, an eines H\u00fcgels Fu\u00df", "tokens": ["Auf", "ei\u00b7ner", "Bank", "von", "Moos", ",", "an", "ei\u00b7nes", "H\u00fc\u00b7gels", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sa\u00df ich, und Athamas, o Nacht in deinem Schatten,", "tokens": ["Sa\u00df", "ich", ",", "und", "A\u00b7tha\u00b7mas", ",", "o", "Nacht", "in", "dei\u00b7nem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "NE", "$,", "FM", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seufzten unsre Pein dem nahen Wasserflu\u00df,", "tokens": ["Und", "seufz\u00b7ten", "uns\u00b7re", "Pein", "dem", "na\u00b7hen", "Was\u00b7ser\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der stillen Einsamkeit und den geweihten Matten,", "tokens": ["Der", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", "und", "den", "ge\u00b7weih\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da rief von einem Ast ein V\u00f6gelchen dem Gatten,", "tokens": ["Da", "rief", "von", "ei\u00b7nem", "Ast", "ein", "V\u00f6\u00b7gel\u00b7chen", "dem", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lern, holder Gatte, hier, wie man recht lieben mu\u00df.", "tokens": ["Lern", ",", "hol\u00b7der", "Gat\u00b7te", ",", "hier", ",", "wie", "man", "recht", "lie\u00b7ben", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADV", "$,", "PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}