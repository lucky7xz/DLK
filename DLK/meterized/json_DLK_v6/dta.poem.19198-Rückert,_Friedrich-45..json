{"dta.poem.19198": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der G\u00e4rtner liefert mir zum Schmuck in meinem Zimmer", "tokens": ["Der", "G\u00e4rt\u00b7ner", "lie\u00b7fert", "mir", "zum", "Schmuck", "in", "mei\u00b7nem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blumen von Zeit zu Zeit, neu bl\u00fchnde Blumen immer.", "tokens": ["Blu\u00b7men", "von", "Zeit", "zu", "Zeit", ",", "neu", "bl\u00fchn\u00b7de", "Blu\u00b7men", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$,", "ADJD", "ADJA", "NN", "ADV", "$."], "meter": "+--+-+-+-+-++", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text": "Da stehn sie denn solang als sie in Bl\u00fcte stehn,", "tokens": ["Da", "stehn", "sie", "denn", "so\u00b7lang", "als", "sie", "in", "Bl\u00fc\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "KOKOM", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00fc\u00dfen abgebl\u00fcht zur\u00fcck zum G\u00e4rtner gehn.", "tokens": ["Und", "m\u00fc\u00b7\u00dfen", "ab\u00b7ge\u00b7bl\u00fcht", "zu\u00b7r\u00fcck", "zum", "G\u00e4rt\u00b7ner", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich habe den Genu\u00df, der G\u00e4rtner hat die M\u00fchen,", "tokens": ["Ich", "ha\u00b7be", "den", "Ge\u00b7nu\u00df", ",", "der", "G\u00e4rt\u00b7ner", "hat", "die", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur bl\u00fchen seh' ich sie, er sorget da\u00df sie bl\u00fchen.", "tokens": ["Nur", "bl\u00fc\u00b7hen", "seh'", "ich", "sie", ",", "er", "sor\u00b7get", "da\u00df", "sie", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was mir der G\u00e4rtner ist, das ist der Dichter euch,", "tokens": ["Was", "mir", "der", "G\u00e4rt\u00b7ner", "ist", ",", "das", "ist", "der", "Dich\u00b7ter", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VAFIN", "$,", "PDS", "VAFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Bl\u00fct' und Blume zieht am kahlen Weltgestr\u00e4uch.", "tokens": ["Der", "Bl\u00fct'", "und", "Blu\u00b7me", "zieht", "am", "kah\u00b7len", "Welt\u00b7ge\u00b7str\u00e4uch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr habt den Augenblick des Aufgehns zu genie\u00dfen,", "tokens": ["Ihr", "habt", "den", "Au\u00b7gen\u00b7blick", "des", "Auf\u00b7gehns", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch das ist seine Lust stets neue zu erschlie\u00dfen.", "tokens": ["Doch", "das", "ist", "sei\u00b7ne", "Lust", "stets", "neu\u00b7e", "zu", "er\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}