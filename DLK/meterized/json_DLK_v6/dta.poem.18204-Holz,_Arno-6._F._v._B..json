{"dta.poem.18204": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "6.  \n  F. v. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.85", "af:0.14"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die schlanke Nase k\u00fchn gekurvt", "tokens": ["Die", "schlan\u00b7ke", "Na\u00b7se", "k\u00fchn", "ge\u00b7kurvt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die gedankenhohle Stirn", "tokens": ["Und", "die", "ge\u00b7dan\u00b7ken\u00b7hoh\u00b7le", "Stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedankenvoll \u201egefaltenwurft\u201c:", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "\u201e", "ge\u00b7fal\u00b7ten\u00b7wurft", "\u201c", ":"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "$(", "VVPP", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So seh ich ihn, verblichnen Airs,", "tokens": ["So", "seh", "ich", "ihn", ",", "ver\u00b7blich\u00b7nen", "Airs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den alten goldbebrillten Knaben \u2014", "tokens": ["Den", "al\u00b7ten", "gold\u00b7be\u00b7brill\u00b7ten", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O, F. v. B., das Beste w\u00e4r's,", "tokens": ["O", ",", "F.", "v.", "B.", ",", "das", "Bes\u00b7te", "w\u00e4r's", ","], "token_info": ["word", "punct", "abbreviation", "abbreviation", "abbreviation", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Du lie\u00dfest endlich Dich begraben!", "tokens": ["Du", "lie\u00b7\u00dfest", "end\u00b7lich", "Dich", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Begnadge Feder und Papier", "tokens": ["Be\u00b7gnad\u00b7ge", "Fe\u00b7der", "und", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ziehe endlich die Moral,", "tokens": ["Und", "zie\u00b7he", "end\u00b7lich", "die", "Mo\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst, ich mein es gut mit Dir", "tokens": ["Du", "siehst", ",", "ich", "mein", "es", "gut", "mit", "Dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PPOSAT", "PPER", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin wie immer radikal.", "tokens": ["Und", "bin", "wie", "im\u00b7mer", "ra\u00b7di\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hast Du um die Zeit der Noth", "tokens": ["Was", "hast", "Du", "um", "die", "Zeit", "der", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch heut in dieser Welt zu suchen?", "tokens": ["Auch", "heut", "in", "die\u00b7ser", "Welt", "zu", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir Dichter schrein nur noch nach Brot", "tokens": ["Wir", "Dich\u00b7ter", "schrein", "nur", "noch", "nach", "Brot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nicht wie Du nach Kaffeekuchen!", "tokens": ["Und", "nicht", "wie", "Du", "nach", "Kaf\u00b7fee\u00b7ku\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Mensch ist mehr zuleikatoll,", "tokens": ["Kein", "Mensch", "ist", "mehr", "zu\u00b7lei\u00b7ka\u00b7toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein B\u00fclb\u00fclschwindel ist verkracht,", "tokens": ["Dein", "B\u00fcl\u00b7b\u00fcl\u00b7schwin\u00b7del", "ist", "ver\u00b7kracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein entsetzlich tiefer Groll", "tokens": ["Und", "ein", "ent\u00b7setz\u00b7lich", "tie\u00b7fer", "Groll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist j\u00e4hlings mit uns aufgewacht.", "tokens": ["Ist", "j\u00e4h\u00b7lings", "mit", "uns", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum gecke weiter, alter Geck,", "tokens": ["Drum", "ge\u00b7cke", "wei\u00b7ter", ",", "al\u00b7ter", "Geck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schw\u00e4rme vom Medschidscheorden,", "tokens": ["Und", "schw\u00e4r\u00b7me", "vom", "Med\u00b7schid\u00b7sche\u00b7or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wir \u2014 schreiten \u00fcber Dich hinweg,", "tokens": ["Wir", "schrei\u00b7ten", "\u00fc\u00b7ber", "Dich", "hin\u00b7weg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "APPR", "PRF", "APZR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn anders ist die Welt geworden!", "tokens": ["Denn", "an\u00b7ders", "ist", "die", "Welt", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schwelgt nicht mehr \u201ean Baches Strand\u201c", "tokens": ["Sie", "schwelgt", "nicht", "mehr", "\u201e", "an", "Ba\u00b7ches", "Strand", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sucht verz\u00fcckt das Bl\u00fcmlein \u201eBlau\u201c,", "tokens": ["Und", "sucht", "ver\u00b7z\u00fcckt", "das", "Bl\u00fcm\u00b7lein", "\u201e", "Blau", "\u201c", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "NN", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat sich endlich selbst erkannt", "tokens": ["Sie", "hat", "sich", "end\u00b7lich", "selbst", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und plant den gro\u00dfen Zukunftsbau.", "tokens": ["Und", "plant", "den", "gro\u00b7\u00dfen", "Zu\u00b7kunfts\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Factum macht sie die Idee", "tokens": ["Zum", "Fac\u00b7tum", "macht", "sie", "die", "I\u00b7dee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und lacht der Schw\u00e4rmer hinterm Ofen \u2014", "tokens": ["Und", "lacht", "der", "Schw\u00e4r\u00b7mer", "hin\u00b7term", "O\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was sollen ", "tokens": ["Was", "sol\u00b7len"], "token_info": ["word", "word"], "pos": ["PWS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Was sollen ", "tokens": ["Was", "sol\u00b7len"], "token_info": ["word", "word"], "pos": ["PWS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Ein Musterst\u00fcck f\u00fcr Versdressur,", "tokens": ["Ein", "Mus\u00b7ter\u00b7st\u00fcck", "f\u00fcr", "Vers\u00b7dres\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein farblos Nichts, das bunt lakirt,", "tokens": ["Ein", "farb\u00b7los", "Nichts", ",", "das", "bunt", "la\u00b7kirt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergleichbar einer Kinderuhr,", "tokens": ["Ver\u00b7gleich\u00b7bar", "ei\u00b7ner", "Kin\u00b7de\u00b7ruhr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u201efingerdick mit Gold beschmiert\u201c \u2014", "tokens": ["Die", "\u201e", "fin\u00b7ger\u00b7dick", "mit", "Gold", "be\u00b7schmiert", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$(", "VVIMP", "APPR", "NN", "VVPP", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ungef\u00e4hr als Mann von Fach", "tokens": ["So", "un\u00b7ge\u00b7f\u00e4hr", "als", "Mann", "von", "Fach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd ich den Mischmasch kritisiren;", "tokens": ["W\u00fcrd", "ich", "den", "Mischmasch", "kri\u00b7ti\u00b7si\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch nein, auch das ist noch zu schwach,", "tokens": ["Doch", "nein", ",", "auch", "das", "ist", "noch", "zu", "schwach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ADV", "PDS", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Witz ist ledern zum Crepiren!", "tokens": ["Dein", "Witz", "ist", "le\u00b7dern", "zum", "Cre\u00b7pi\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum noch einmal: Streu Sand aufs Blatt", "tokens": ["Drum", "noch", "ein\u00b7mal", ":", "Streu", "Sand", "aufs", "Blatt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "$.", "NE", "NN", "APPRART", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schreibe endlich \u201ePunktum\u201c drauf!", "tokens": ["Und", "schrei\u00b7be", "end\u00b7lich", "\u201e", "Punk\u00b7tum", "\u201c", "drauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "ITJ", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind den alten Krimskrams satt", "tokens": ["Wir", "sind", "den", "al\u00b7ten", "Krims\u00b7krams", "satt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und athmen t\u00e4glich freier auf.", "tokens": ["Und", "ath\u00b7men", "t\u00e4g\u00b7lich", "frei\u00b7er", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir w\u00fcnschen Dir, weil Du ergraut,", "tokens": ["Wir", "w\u00fcn\u00b7schen", "Dir", ",", "weil", "Du", "er\u00b7graut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch schlie\u00dflich noch ein langes Leben;", "tokens": ["Auch", "schlie\u00df\u00b7lich", "noch", "ein", "lan\u00b7ges", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur darfst Du nie, was Du verdaut,", "tokens": ["Nur", "darfst", "Du", "nie", ",", "was", "Du", "ver\u00b7daut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Versen wieder von Dir geben!!!", "tokens": ["In", "Ver\u00b7sen", "wie\u00b7der", "von", "Dir", "ge\u00b7ben", "!!!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Denn traurig ist's mit anzuschaun,", "tokens": ["Denn", "trau\u00b7rig", "ist's", "mit", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ein zerbrochner Hampelmann", "tokens": ["Wenn", "ein", "zer\u00b7broch\u00b7ner", "Ham\u00b7pel\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch immer thun will wie ein Faun", "tokens": ["Noch", "im\u00b7mer", "thun", "will", "wie", "ein", "Faun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch nicht kann, o Gott, nicht kann!", "tokens": ["Und", "doch", "nicht", "kann", ",", "o", "Gott", ",", "nicht", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VMFIN", "$,", "FM", "NN", "$,", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann zuckt's mir durch das Herz: Er weint!", "tokens": ["Dann", "zuckt's", "mir", "durch", "das", "Herz", ":", "Er", "weint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gespenstisch d\u00e4ucht mir seine Glatze,", "tokens": ["Ge\u00b7spens\u00b7tisch", "d\u00e4ucht", "mir", "sei\u00b7ne", "Glat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn die Sonne dr\u00fcber scheint,", "tokens": ["Und", "wenn", "die", "Son\u00b7ne", "dr\u00fc\u00b7ber", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verkl\u00e4rt sie golden \u2014 eine Fratze!", "tokens": ["Ver\u00b7kl\u00e4rt", "sie", "gol\u00b7den", "ei\u00b7ne", "Frat\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}