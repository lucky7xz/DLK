{"textgrid.poem.44865": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Das korrigierte Gesuch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Strichen und mit Zeichen allerhand", "tokens": ["Mit", "Stri\u00b7chen", "und", "mit", "Zei\u00b7chen", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Habt mein Gesuch ihr rings bemalt, beschrieben,", "tokens": ["Habt", "mein", "Ge\u00b7such", "ihr", "rings", "be\u00b7malt", ",", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gleicht jetzt einem gr\u00fcnen Wiesenland,", "tokens": ["Es", "gleicht", "jetzt", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Wie\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch das man eine Ochsenschar getrieben.", "tokens": ["Durch", "das", "man", "ei\u00b7ne", "Och\u00b7sen\u00b7schar", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}