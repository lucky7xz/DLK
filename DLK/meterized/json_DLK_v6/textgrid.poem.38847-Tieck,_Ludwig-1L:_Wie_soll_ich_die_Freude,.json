{"textgrid.poem.38847": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie soll ich die Freude,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich die Freude,", "tokens": ["Wie", "soll", "ich", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Die Wonne denn tragen?", "tokens": ["Die", "Won\u00b7ne", "denn", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df unter dem Schlagen", "tokens": ["Da\u00df", "un\u00b7ter", "dem", "Schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Herzens die Seele nicht scheide?", "tokens": ["Des", "Her\u00b7zens", "die", "See\u00b7le", "nicht", "schei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn nun die Stunden", "tokens": ["Und", "wenn", "nun", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Liebe verschwunden,", "tokens": ["Der", "Lie\u00b7be", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wozu das Gel\u00fcste,", "tokens": ["Wo\u00b7zu", "das", "Ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In trauriger W\u00fcste", "tokens": ["In", "trau\u00b7ri\u00b7ger", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Noch weiter ein lustleeres Leben zu ziehn,", "tokens": ["Noch", "wei\u00b7ter", "ein", "lust\u00b7lee\u00b7res", "Le\u00b7ben", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Wenn nirgend dem Ufer mehr Blumen entbl\u00fchn?", "tokens": ["Wenn", "nir\u00b7gend", "dem", "U\u00b7fer", "mehr", "Blu\u00b7men", "ent\u00b7bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wie geht mit bleibehangnen F\u00fc\u00dfen", "tokens": ["Wie", "geht", "mit", "blei\u00b7be\u00b7hang\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit bed\u00e4chtig Schritt vor Schritt!", "tokens": ["Die", "Zeit", "be\u00b7d\u00e4ch\u00b7tig", "Schritt", "vor", "Schritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich werde scheiden m\u00fcssen,", "tokens": ["Und", "wenn", "ich", "wer\u00b7de", "schei\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie federleicht fliegt dann ihr Tritt!", "tokens": ["Wie", "fe\u00b7der\u00b7leicht", "fliegt", "dann", "ihr", "Tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlage, sehns\u00fcchtige Gewalt,", "tokens": ["Schla\u00b7ge", ",", "sehn\u00b7s\u00fcch\u00b7ti\u00b7ge", "Ge\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In tiefer treuer Brust!", "tokens": ["In", "tie\u00b7fer", "treu\u00b7er", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Lautenton vor\u00fcber hallt,", "tokens": ["Wie", "Lau\u00b7ten\u00b7ton", "vor\u00b7\u00fc\u00b7ber", "hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entflieht des Lebens sch\u00f6nste Lust.", "tokens": ["Ent\u00b7flieht", "des", "Le\u00b7bens", "sch\u00f6ns\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! wie bald", "tokens": ["Ach", "!", "wie", "bald"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Bin ich in der Wonne mir kaum noch bewu\u00dft.", "tokens": ["Bin", "ich", "in", "der", "Won\u00b7ne", "mir", "kaum", "noch", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Rausche, rausche weiter fort,", "tokens": ["Rau\u00b7sche", ",", "rau\u00b7sche", "wei\u00b7ter", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefer Strom der Zeit,", "tokens": ["Tie\u00b7fer", "Strom", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wandelst bald aus Morgen Heut,", "tokens": ["Wan\u00b7delst", "bald", "aus", "Mor\u00b7gen", "Heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gehst von Ort zu Ort;", "tokens": ["Gehst", "von", "Ort", "zu", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hast du mich bisher getragen,", "tokens": ["Hast", "du", "mich", "bis\u00b7her", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lustig bald, dann still,", "tokens": ["Lus\u00b7tig", "bald", ",", "dann", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Will es nun auch weiter wagen,", "tokens": ["Will", "es", "nun", "auch", "wei\u00b7ter", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie es werden will.", "tokens": ["Wie", "es", "wer\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Darf mich doch nicht elend achten,", "tokens": ["Darf", "mich", "doch", "nicht", "e\u00b7lend", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da die Einzge winkt,", "tokens": ["Da", "die", "Einz\u00b7ge", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Liebe l\u00e4\u00dft mich nicht verschmachten,", "tokens": ["Lie\u00b7be", "l\u00e4\u00dft", "mich", "nicht", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis dies Leben sinkt;", "tokens": ["Bis", "dies", "Le\u00b7ben", "sinkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nein, der Strom wird immer breiter,", "tokens": ["Nein", ",", "der", "Strom", "wird", "im\u00b7mer", "brei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmel bleibt mir immer heiter,", "tokens": ["Him\u00b7mel", "bleibt", "mir", "im\u00b7mer", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00f6hlichen Ruderschlags fahr ich hinab,", "tokens": ["Fr\u00f6h\u00b7li\u00b7chen", "Ru\u00b7der\u00b7schlags", "fahr", "ich", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Bring Liebe und Leben zugleich an das Grab.", "tokens": ["Bring", "Lie\u00b7be", "und", "Le\u00b7ben", "zu\u00b7gleich", "an", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}