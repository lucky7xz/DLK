{"dta.poem.21055": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Fredegunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von jener S&#228;ule, wo sie entschlummert schwebt,               ", "tokens": ["Von", "je\u00b7ner", "S", "&#228;", "u\u00b7le", ",", "wo", "sie", "ent\u00b7schlum\u00b7mert", "schwebt", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NE", "$(", "NE", "$,", "PWAV", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ihre goldnen Saiten der Mond begl\u00e4nzt,", "tokens": ["Wo", "ih\u00b7re", "gold\u00b7nen", "Sai\u00b7ten", "der", "Mond", "be\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo ihr Verm\u00f6gen schwirrt im Winde,", "tokens": ["Wo", "ihr", "Ver\u00b7m\u00f6\u00b7gen", "schwirrt", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reiche die Harfe mir, Fredegunde.", "tokens": ["Rei\u00b7che", "die", "Har\u00b7fe", "mir", ",", "Fre\u00b7de\u00b7gun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}