{"dta.poem.11434": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gefallenen stern-gucker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der vor die sternen ma\u00df, der mi\u00dft itzund die erde,", "tokens": ["Der", "vor", "die", "ster\u00b7nen", "ma\u00df", ",", "der", "mi\u00dft", "it\u00b7zund", "die", "er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum zeugni\u00df: Da\u00df ein fall offt unser meister werde.", "tokens": ["Zum", "zeug\u00b7ni\u00df", ":", "Da\u00df", "ein", "fall", "offt", "un\u00b7ser", "meis\u00b7ter", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "KOUS", "ART", "NN", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}