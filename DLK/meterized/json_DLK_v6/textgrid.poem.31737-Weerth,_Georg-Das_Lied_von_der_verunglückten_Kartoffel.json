{"textgrid.poem.31737": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied von der verungl\u00fcckten Kartoffel", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Nacht auf ihrem Lager lag", "tokens": ["Zur", "Nacht", "auf", "ih\u00b7rem", "La\u00b7ger", "lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eine arme, kranke Kartoffel.", "tokens": ["Ei\u00b7ne", "ar\u00b7me", ",", "kran\u00b7ke", "Kar\u00b7tof\u00b7fel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie hob sich matt empor und sprach,", "tokens": ["Sie", "hob", "sich", "matt", "em\u00b7por", "und", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sprach zu dem armen Stoffel:", "tokens": ["Sie", "sprach", "zu", "dem", "ar\u00b7men", "Stof\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbo Stoffel, ungl\u00fccklicher Mann,", "tokens": ["\u00bb", "o", "Stof\u00b7fel", ",", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "Mann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ich f\u00fchl's, da\u00df ich sterben werde!", "tokens": ["Ich", "f\u00fchl's", ",", "da\u00df", "ich", "ster\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schon kommt der Tod, der schlimme, heran", "tokens": ["Schon", "kommt", "der", "Tod", ",", "der", "schlim\u00b7me", ",", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und rafft mich von der Erde.", "tokens": ["Und", "rafft", "mich", "von", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar frag ich nach mir selber nicht,", "tokens": ["Zwar", "frag", "ich", "nach", "mir", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht will ich mich bedauern.", "tokens": ["Nicht", "will", "ich", "mich", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn ich schaue dein bleich Gesicht,", "tokens": ["Doch", "wenn", "ich", "schau\u00b7e", "dein", "bleich", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da mu\u00df ich trauern und trauern.", "tokens": ["Da", "mu\u00df", "ich", "trau\u00b7ern", "und", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dir bl\u00fcht kein Wein und Weizen nicht,", "tokens": ["Dir", "bl\u00fcht", "kein", "Wein", "und", "Wei\u00b7zen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast weder Ochs noch Rinder,", "tokens": ["Hast", "we\u00b7der", "Ochs", "noch", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Stoffel, bist ein armer Wicht,", "tokens": ["O", "Stof\u00b7fel", ",", "bist", "ein", "ar\u00b7mer", "Wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast nur hungrige Kinder.", "tokens": ["Du", "hast", "nur", "hung\u00b7ri\u00b7ge", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Was wird aus deinen Kindern nun,", "tokens": ["Was", "wird", "aus", "dei\u00b7nen", "Kin\u00b7dern", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fr\u00f6hlich waren noch gestern,", "tokens": ["Die", "fr\u00f6h\u00b7lich", "wa\u00b7ren", "noch", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich bald werde im Grabe ruhn", "tokens": ["Wenn", "ich", "bald", "wer\u00b7de", "im", "Gra\u00b7be", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit all meinen lieblichen Schwestern?", "tokens": ["Mit", "all", "mei\u00b7nen", "lieb\u00b7li\u00b7chen", "Schwes\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie starben in Ober- und Niederland,", "tokens": ["Sie", "star\u00b7ben", "in", "O\u00b7ber", "und", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "TRUNC", "KON", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie starben mit Weh und Gewinsel,", "tokens": ["Sie", "star\u00b7ben", "mit", "Weh", "und", "Ge\u00b7win\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie starben an Englands wei\u00dfem Strand", "tokens": ["Sie", "star\u00b7ben", "an", "En\u00b7glands", "wei\u00b7\u00dfem", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und auf der smaragdenen Insel.", "tokens": ["Und", "auf", "der", "sma\u00b7rag\u00b7de\u00b7nen", "In\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Sie starben, und ach, ich folg ihnen nach!\u00ab", "tokens": ["Sie", "star\u00b7ben", ",", "und", "ach", ",", "ich", "folg", "ih\u00b7nen", "nach", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "XY", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So sprach die kranke Kartoffel.", "tokens": ["So", "sprach", "die", "kran\u00b7ke", "Kar\u00b7tof\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schwieg, und das Herz, das Herz ihr brach \u2013", "tokens": ["Sie", "schwieg", ",", "und", "das", "Herz", ",", "das", "Herz", "ihr", "brach", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aufschluchzte der arme Stoffel", "tokens": ["Auf\u00b7schluchz\u00b7te", "der", "ar\u00b7me", "Stof\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und weinte die Nacht mit Weib und Kind,", "tokens": ["Und", "wein\u00b7te", "die", "Nacht", "mit", "Weib", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und der Hunger, der wollte nicht weichen.", "tokens": ["Und", "der", "Hun\u00b7ger", ",", "der", "woll\u00b7te", "nicht", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dumpf brauste der kalte Novemberwind", "tokens": ["Dumpf", "braus\u00b7te", "der", "kal\u00b7te", "No\u00b7vem\u00b7ber\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In den pr\u00e4chtigen deutschen Eichen.", "tokens": ["In", "den", "pr\u00e4ch\u00b7ti\u00b7gen", "deut\u00b7schen", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}