", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbso bringt mir drei Jungfrauen, die Knaben f\u00fchret fort!\u00ab", "tokens": ["\u00bb", "so", "bringt", "mir", "drei", "Jung\u00b7frau\u00b7en", ",", "die", "Kna\u00b7ben", "f\u00fch\u00b7ret", "fort", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "CARD", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drei M\u00e4gdlein, jung, unschuldig, f\u00fchrt man herbei bekr\u00e4nzt,", "tokens": ["Drei", "M\u00e4gd\u00b7lein", ",", "jung", ",", "un\u00b7schul\u00b7dig", ",", "f\u00fchrt", "man", "her\u00b7bei", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie neigen sich geduldig, nur ihre Thr\u00e4ne gl\u00e4nzt.", "tokens": ["Sie", "nei\u00b7gen", "sich", "ge\u00b7dul\u00b7dig", ",", "nur", "ih\u00b7re", "Thr\u00e4\u00b7ne", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbla\u00dft ab, la\u00dft ab!\u00ab ruft wieder der K\u00f6nig zagend aus:", "tokens": ["\u00bb", "la\u00dft", "ab", ",", "la\u00dft", "ab", "!", "\u00ab", "ruft", "wie\u00b7der", "der", "K\u00f6\u00b7nig", "za\u00b7gend", "aus", ":"], "token_in