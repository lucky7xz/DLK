{"textgrid.poem.39875": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Opfer", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Reich gen Morgen da gl\u00fchte der Sonne Brand,", "tokens": ["In", "ei\u00b7nem", "Reich", "gen", "Mor\u00b7gen", "da", "gl\u00fch\u00b7te", "der", "Son\u00b7ne", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da schaut in schweren Sorgen der K\u00f6nig auf sein Land:", "tokens": ["Da", "schaut", "in", "schwe\u00b7ren", "Sor\u00b7gen", "der", "K\u00f6\u00b7nig", "auf", "sein", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbes lechzen alle Felder, versiegen geht der Flu\u00df,", "tokens": ["\u00bb", "es", "lech\u00b7zen", "al\u00b7le", "Fel\u00b7der", ",", "ver\u00b7sie\u00b7gen", "geht", "der", "Flu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$,", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es dorren ab die W\u00e4lder, weh, da\u00df ich es schauen mu\u00df!", "tokens": ["Es", "dor\u00b7ren", "ab", "die", "W\u00e4l\u00b7der", ",", "weh", ",", "da\u00df", "ich", "es", "schau\u00b7en", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Was hilft mir Scepter tragen? Kann ich zum Strome: Fleu\u00df!", "tokens": ["Was", "hilft", "mir", "Scep\u00b7ter", "tra\u00b7gen", "?", "Kann", "ich", "zum", "Stro\u00b7me", ":", "Fleu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "VVINF", "$.", "VMFIN", "PPER", "APPRART", "NN", "$.", "NN", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Kann ich zur Wolke sagen: Die k\u00fchle Flut ergeu\u00df?\u00ab", "tokens": ["Kann", "ich", "zur", "Wol\u00b7ke", "sa\u00b7gen", ":", "Die", "k\u00fch\u00b7le", "Flut", "er\u00b7geu\u00df", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$.", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So hat er lang in Kummer von Tag' zu Tage gedacht,", "tokens": ["So", "hat", "er", "lang", "in", "Kum\u00b7mer", "von", "Tag'", "zu", "Ta\u00b7ge", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So seufzt' er, ohne Schlummer, von Nacht zu hei\u00dfer Nacht.", "tokens": ["So", "seufzt'", "er", ",", "oh\u00b7ne", "Schlum\u00b7mer", ",", "von", "Nacht", "zu", "hei\u00b7\u00dfer", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "NN", "$,", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und als nun ohne Wolke sechs Monden gl\u00e4nzte die Luft,", "tokens": ["Und", "als", "nun", "oh\u00b7ne", "Wol\u00b7ke", "sechs", "Mon\u00b7den", "gl\u00e4nz\u00b7te", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPR", "NN", "CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-++-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Tritt er hinaus zum Volke, das zu den G\u00f6ttern ruft.", "tokens": ["Tritt", "er", "hin\u00b7aus", "zum", "Vol\u00b7ke", ",", "das", "zu", "den", "G\u00f6t\u00b7tern", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Es schallten Trauerpsalme, davon kein Strauch genas,", "tokens": ["Es", "schall\u00b7ten", "Trau\u00b7er\u00b7psal\u00b7me", ",", "da\u00b7von", "kein", "Strauch", "ge\u00b7nas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PAV", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und welk stand jede Palme, als w\u00e4re sie junges Gras.", "tokens": ["Und", "welk", "stand", "je\u00b7de", "Pal\u00b7me", ",", "als", "w\u00e4\u00b7re", "sie", "jun\u00b7ges", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "$,", "KOKOM", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die fetten Aecker darben, kein Dampf steigt aus dem Kraut,", "tokens": ["Die", "fet\u00b7ten", "A\u00b7e\u00b7cker", "dar\u00b7ben", ",", "kein", "Dampf", "steigt", "aus", "dem", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verbl\u00fcht stehn, ohne Farben, die Blumen, wohin er schaut.", "tokens": ["Ver\u00b7bl\u00fcht", "stehn", ",", "oh\u00b7ne", "Far\u00b7ben", ",", "die", "Blu\u00b7men", ",", "wo\u00b7hin", "er", "schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUI", "NN", "$,", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht weht ein Strom von D\u00fcften aus den Gew\u00fcrzen mehr,", "tokens": ["Nicht", "weht", "ein", "Strom", "von", "D\u00fcf\u00b7ten", "aus", "den", "Ge\u00b7w\u00fcr\u00b7zen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht singt mehr in den L\u00fcften der bunten V\u00f6gel Heer.", "tokens": ["Nicht", "singt", "mehr", "in", "den", "L\u00fcf\u00b7ten", "der", "bun\u00b7ten", "V\u00f6\u00b7gel", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und unter den Zelten lagen die Menschen krank und matt,", "tokens": ["Und", "un\u00b7ter", "den", "Zel\u00b7ten", "la\u00b7gen", "die", "Men\u00b7schen", "krank", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von gl\u00fch'nder Pest geschlagen auf schw\u00fcler Lagerstatt.", "tokens": ["Von", "gl\u00fch'n\u00b7der", "Pest", "ge\u00b7schla\u00b7gen", "auf", "schw\u00fc\u00b7ler", "La\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und war die Sonne gesunken nach langem, hei\u00dfem Lauf,", "tokens": ["Und", "war", "die", "Son\u00b7ne", "ge\u00b7sun\u00b7ken", "nach", "lan\u00b7gem", ",", "hei\u00b7\u00dfem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So spr\u00fchten die tr\u00fcben Funken der Scheiterhaufen auf.", "tokens": ["So", "spr\u00fch\u00b7ten", "die", "tr\u00fc\u00b7ben", "Fun\u00b7ken", "der", "Schei\u00b7ter\u00b7hau\u00b7fen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Da deckte mit beiden H\u00e4nden der K\u00f6nig sein Gesicht:", "tokens": ["Da", "deck\u00b7te", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", "der", "K\u00f6\u00b7nig", "sein", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-++-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "\u00bbihr G\u00f6tter, kann ich wenden vom Volke den Jammer nicht?", "tokens": ["\u00bb", "ihr", "G\u00f6t\u00b7ter", ",", "kann", "ich", "wen\u00b7den", "vom", "Vol\u00b7ke", "den", "Jam\u00b7mer", "nicht", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gebt mir ein gn\u00e4dig Zeichen, vor keiner Last will ich,", "tokens": ["Gebt", "mir", "ein", "gn\u00e4\u00b7dig", "Zei\u00b7chen", ",", "vor", "kei\u00b7ner", "Last", "will", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJD", "NN", "$,", "APPR", "PIAT", "NN", "VMFIN", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vor keiner Schmach erbleichen, nur, eh'rner Himmel, sprich!\u00ab", "tokens": ["Vor", "kei\u00b7ner", "Schmach", "er\u00b7blei\u00b7chen", ",", "nur", ",", "eh'r\u00b7ner", "Him\u00b7mel", ",", "sprich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,", "ADV", "$,", "ADJA", "NN", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Da sprachen zu ihm die G\u00f6tter durch seiner Priester Mund:", "tokens": ["Da", "spra\u00b7chen", "zu", "ihm", "die", "G\u00f6t\u00b7ter", "durch", "sei\u00b7ner", "Pries\u00b7ter", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbdu wirst des Landes Retter, und schleu\u00dfst mit uns den Bund,", "tokens": ["\u00bb", "du", "wirst", "des", "Lan\u00b7des", "Ret\u00b7ter", ",", "und", "schleu\u00df\u00b7st", "mit", "uns", "den", "Bund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn zu des Volkes Heile das Opfer du gestellt,", "tokens": ["Wenn", "zu", "des", "Vol\u00b7kes", "Hei\u00b7le", "das", "Op\u00b7fer", "du", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das unter des Priesters Beile uns recht willkommen f\u00e4llt!\u00ab", "tokens": ["Das", "un\u00b7ter", "des", "Pries\u00b7ters", "Bei\u00b7le", "uns", "recht", "will\u00b7kom\u00b7men", "f\u00e4llt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Er l\u00e4\u00dft Alt\u00e4re zieren, der Hundert f\u00fchrt man drei", "tokens": ["Er", "l\u00e4\u00dft", "Al\u00b7t\u00e4\u00b7re", "zie\u00b7ren", ",", "der", "Hun\u00b7dert", "f\u00fchrt", "man", "drei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PIS", "CARD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von Schafen und von Stieren, die stattlichsten herbei.", "tokens": ["Von", "Scha\u00b7fen", "und", "von", "Stie\u00b7ren", ",", "die", "statt\u00b7lichs\u00b7ten", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein Hauch vom Berge wehet, keine Wolk' am Himmel stand,", "tokens": ["Kein", "Hauch", "vom", "Ber\u00b7ge", "we\u00b7het", ",", "kei\u00b7ne", "Wol\u00b7k'", "am", "Him\u00b7mel", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Mit lautem Schalle flehet der K\u00f6nig und sein Land.", "tokens": ["Mit", "lau\u00b7tem", "Schal\u00b7le", "fle\u00b7het", "der", "K\u00f6\u00b7nig", "und", "sein", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Doch als die Priester hoben den blanken Opferstahl,", "tokens": ["Doch", "als", "die", "Pries\u00b7ter", "ho\u00b7ben", "den", "blan\u00b7ken", "Op\u00b7fer\u00b7stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Thiere begannen zu toben und starben in Wut und Qual.", "tokens": ["Die", "Thie\u00b7re", "be\u00b7gan\u00b7nen", "zu", "to\u00b7ben", "und", "star\u00b7ben", "in", "Wut", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VVINF", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Es schaut auf das Gewimmel und auf das Blut, das flo\u00df,", "tokens": ["Es", "schaut", "auf", "das", "Ge\u00b7wim\u00b7mel", "und", "auf", "das", "Blut", ",", "das", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit blauem Auge der Himmel hernieder erbarmungslos.", "tokens": ["Mit", "blau\u00b7em", "Au\u00b7ge", "der", "Him\u00b7mel", "her\u00b7nie\u00b7der", "er\u00b7bar\u00b7mungs\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Der K\u00f6nig in tiefer Trauer ging wieder in sein Haus,", "tokens": ["Der", "K\u00f6\u00b7nig", "in", "tie\u00b7fer", "Trau\u00b7er", "ging", "wie\u00b7der", "in", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.h