_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch Bart und graue Locken der Strom dem K\u00f6nig quillt,", "tokens": ["Durch", "Bart", "und", "grau\u00b7e", "Lo\u00b7cken", "der", "Strom", "dem", "K\u00f6\u00b7nig", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein Auge bleibt nicht trocken, von sel'ger Thr\u00e4n' es schwillt.", "tokens": ["Sein", "Au\u00b7ge", "bleibt", "nicht", "tro\u00b7cken", ",", "von", "sel'\u00b7ger", "Thr\u00e4n'", "es", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "$,", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.20": {"line.1": {"text": "Die V\u00f6gel fangen zu singen, die Kr\u00e4uter zu duften an,", "tokens": ["Die", "V\u00f6\u00b7gel", "fan\u00b7gen", "zu", "sin\u00b7gen", ",", "die", "Kr\u00e4u\u00b7ter", "zu", "duf\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VVINF", "$,", "ART", "NN", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Flu\u00df sich zu schwellen, zu schlingen in seiner alten Bahn.", "tokens": ["Der", "Flu\u00df", "sich", "zu", "schwel\u00b7len", ",", "zu", "schlin\u00b7gen", "in", "sei\u00b7ner", "al\u00b7ten", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Es t\u00f6nen der Priester Lieder, der Dichter Harfe klingt,", "tokens": ["Es", "t\u00f6\u00b7nen", "der", "Pries\u00b7ter", "Lie\u00b7der", ",", "der", "Dich\u00b7ter", "Har\u00b7fe", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das Volk, es wirft sich nieder, den Scepter der K\u00f6nig schwingt.", "tokens": ["Das", "Volk", ",", "es", "wirft", "sich", "nie\u00b7der", ",", "den", "Scep\u00b7ter", "der", "K\u00f6\u00b7nig", "schwingt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}}}}