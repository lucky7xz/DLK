{"dta.poem.12801": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Buddeens, dem bisher der weisen V\u00e4ter rath", "tokens": ["Bud\u00b7deens", ",", "dem", "bis\u00b7her", "der", "wei\u00b7sen", "V\u00e4\u00b7ter", "rath"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Den scepter anvertrauet hat,", "tokens": ["Den", "scep\u00b7ter", "an\u00b7ver\u00b7trau\u00b7et", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Musen-s\u00f6hne recht zu f\u00fchren,", "tokens": ["Die", "Mu\u00b7sen\u00b7s\u00f6h\u00b7ne", "recht", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dieses Saal-Athen mit fried und ruh zu zieren.", "tokens": ["Und", "die\u00b7ses", "Saa\u00b7lA\u00b7then", "mit", "fried", "und", "ruh", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das ist es, was bisher Buddeens witz gethan.", "tokens": ["Das", "ist", "es", ",", "was", "bis\u00b7her", "Bud\u00b7deens", "witz", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ADV", "NE", "NE", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er hat das recht vern\u00fcnfftig abgewogen,", "tokens": ["Er", "hat", "das", "recht", "ver\u00b7n\u00fcnff\u00b7tig", "ab\u00b7ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Indem kein eigen-nutz die schaalen ausgef\u00fcllt.", "tokens": ["In\u00b7dem", "kein", "ei\u00b7gen\u00b7nutz", "die", "schaa\u00b7len", "aus\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}