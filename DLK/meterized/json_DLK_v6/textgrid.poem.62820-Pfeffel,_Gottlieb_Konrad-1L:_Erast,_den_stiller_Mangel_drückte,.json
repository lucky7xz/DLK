{"textgrid.poem.62820": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erast, den stiller Mangel dr\u00fcckte,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erast, den stiller Mangel dr\u00fcckte,", "tokens": ["E\u00b7rast", ",", "den", "stil\u00b7ler", "Man\u00b7gel", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df einst im dunkeln Lindenhayn,", "tokens": ["Sa\u00df", "einst", "im", "dun\u00b7keln", "Lin\u00b7den\u00b7hayn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der seines F\u00fcrsten Garten schm\u00fcckte,", "tokens": ["Der", "sei\u00b7nes", "F\u00fcrs\u00b7ten", "Gar\u00b7ten", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlte schlummernd noch die Pein", "tokens": ["Und", "f\u00fchl\u00b7te", "schlum\u00b7mernd", "noch", "die", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der schw\u00e4rzern Zukunft, die ihn schreckte,", "tokens": ["Der", "schw\u00e4r\u00b7zern", "Zu\u00b7kunft", ",", "die", "ihn", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als ihn im n\u00e4chsten Bogengang", "tokens": ["Als", "ihn", "im", "n\u00e4chs\u00b7ten", "Bo\u00b7gen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Aechzen eines M\u00e4dchens weckte.", "tokens": ["Das", "A\u00b7ech\u00b7zen", "ei\u00b7nes", "M\u00e4d\u00b7chens", "weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Er lauscht; mit lauten Schluchzern rang", "tokens": ["Er", "lauscht", ";", "mit", "lau\u00b7ten", "Schluch\u00b7zern", "rang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die dumpfe Stimme: Gott wie lang", "tokens": ["Die", "dump\u00b7fe", "Stim\u00b7me", ":", "Gott", "wie", "lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verbirgst du dich! Du hast geh\u00f6ret", "tokens": ["Ver\u00b7birgst", "du", "dich", "!", "Du", "hast", "ge\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was dieser Reiche f\u00fcr das Brod,", "tokens": ["Was", "die\u00b7ser", "Rei\u00b7che", "f\u00fcr", "das", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wodurch er meines Vaters Noth", "tokens": ["Wo\u00b7durch", "er", "mei\u00b7nes", "Va\u00b7ters", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Erleichtern will, von mir begehret.", "tokens": ["Er\u00b7leich\u00b7tern", "will", ",", "von", "mir", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Erasten schwoll das Herz, er zog", "tokens": ["E\u00b7ras\u00b7ten", "schwoll", "das", "Herz", ",", "er", "zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sein letztes Gold heraus und flog,", "tokens": ["Sein", "letz\u00b7tes", "Gold", "he\u00b7raus", "und", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von s\u00fc\u00dfem Mitleid angefeuert,", "tokens": ["Von", "s\u00fc\u00b7\u00dfem", "Mit\u00b7leid", "an\u00b7ge\u00b7feu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zur Fremden hin; sie war verschleyert.", "tokens": ["Zur", "Frem\u00b7den", "hin", ";", "sie", "war", "ver\u00b7schley\u00b7ert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Nimm, sprach er, Edle, die\u00df zum Pfand", "tokens": ["Nimm", ",", "sprach", "er", ",", "Ed\u00b7le", ",", "die\u00df", "zum", "Pfand"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "ADJA", "$,", "PDS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Ehrfurcht, nicht zum Tugendlohne;", "tokens": ["Der", "Ehr\u00b7furcht", ",", "nicht", "zum", "Tu\u00b7gend\u00b7loh\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Es ist gering, doch rein die Hand", "tokens": ["Es", "ist", "ge\u00b7ring", ",", "doch", "rein", "die", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des Gebers. \u2013 Himmel! rief Theone,", "tokens": ["Des", "Ge\u00b7bers", ".", "\u2013", "Him\u00b7mel", "!", "rief", "The\u00b7o\u00b7ne", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "NN", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er ists, mein Vater! \u2013 Wie? mein Kind!", "tokens": ["Er", "ists", ",", "mein", "Va\u00b7ter", "!", "\u2013", "Wie", "?", "mein", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$.", "$(", "PWAV", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sie wars. Er k\u00fcsset jede Thr\u00e4ne,", "tokens": ["Sie", "wars", ".", "Er", "k\u00fcs\u00b7set", "je\u00b7de", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die von der Tochter Wange rinnt", "tokens": ["Die", "von", "der", "Toch\u00b7ter", "Wan\u00b7ge", "rinnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Entz\u00fcckt hinweg. O feyrt die Scene,", "tokens": ["Ent\u00b7z\u00fcckt", "hin\u00b7weg", ".", "O", "feyrt", "die", "Sce\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ihr Engel, sie ist euer werth!", "tokens": ["Ihr", "En\u00b7gel", ",", "sie", "ist", "eu\u00b7er", "werth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Doch pl\u00f6tzlich wurden sie gest\u00f6rt.", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "wur\u00b7den", "sie", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Philint, der Schmuck der Erdens\u00f6hne,", "tokens": ["Phi\u00b7lint", ",", "der", "Schmuck", "der", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der alles gierig angeh\u00f6rt,", "tokens": ["Der", "al\u00b7les", "gie\u00b7rig", "an\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sprang aus dem Busch: Erhabne Seele!", "tokens": ["Sprang", "aus", "dem", "Busch", ":", "Er\u00b7hab\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "NN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Sprach er mit gl\u00fchendem Gesicht", "tokens": ["Sprach", "er", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Zur Sch\u00f6nen: meine Hand, wo nicht,", "tokens": ["Zur", "Sch\u00f6\u00b7nen", ":", "mei\u00b7ne", "Hand", ",", "wo", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPOSAT", "NN", "$,", "PWAV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die H\u00e4lfte meiner G\u00fcter \u2013 w\u00e4hle!", "tokens": ["Die", "H\u00e4lf\u00b7te", "mei\u00b7ner", "G\u00fc\u00b7ter", "\u2013", "w\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "VVIMP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, die Theonens Geist beseelt,", "tokens": ["Du", ",", "die", "Theo\u00b7nens", "Geist", "be\u00b7seelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Serena, reich an edeln Thaten,", "tokens": ["Se\u00b7re\u00b7na", ",", "reich", "an", "e\u00b7deln", "Tha\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dein Herz wird ohne mich errathen,", "tokens": ["Dein", "Herz", "wird", "oh\u00b7ne", "mich", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was sie gef\u00fchlet und gew\u00e4hlt.", "tokens": ["Was", "sie", "ge\u00b7f\u00fch\u00b7let", "und", "ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}