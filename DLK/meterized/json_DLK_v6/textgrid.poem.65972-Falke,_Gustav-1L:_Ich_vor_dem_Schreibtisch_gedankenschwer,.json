{"textgrid.poem.65972": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich vor dem Schreibtisch gedankenschwer,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich vor dem Schreibtisch gedankenschwer,", "tokens": ["Ich", "vor", "dem", "Schreib\u00b7tisch", "ge\u00b7dan\u00b7ken\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du vor dem Herd im hin und her,", "tokens": ["Du", "vor", "dem", "Herd", "im", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPRART", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sorgen wir beide den Boden zu n\u00e4hren.", "tokens": ["Sor\u00b7gen", "wir", "bei\u00b7de", "den", "Bo\u00b7den", "zu", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Heimlich reifen unsere \u00c4hren.", "tokens": ["Heim\u00b7lich", "rei\u00b7fen", "un\u00b7se\u00b7re", "\u00c4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ruhen die H\u00e4nde und halt' ich dich fest", "tokens": ["Ru\u00b7hen", "die", "H\u00e4n\u00b7de", "und", "halt'", "ich", "dich", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Abends, du Gute, ans Herz gepresst,", "tokens": ["A\u00b7bends", ",", "du", "Gu\u00b7te", ",", "ans", "Herz", "ge\u00b7presst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ist mir's, als h\u00f6rt' ich ein Rauschen und Regen:", "tokens": ["Ist", "mir's", ",", "als", "h\u00f6rt'", "ich", "ein", "Rau\u00b7schen", "und", "Re\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Feld an Feld in bl\u00fchendstem Segen.", "tokens": ["Feld", "an", "Feld", "in", "bl\u00fc\u00b7hends\u00b7tem", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}