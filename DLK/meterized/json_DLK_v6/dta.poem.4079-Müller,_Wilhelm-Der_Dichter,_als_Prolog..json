{"dta.poem.4079": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter, als Prolog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und die ihr h\u00f6rt und schaut was Gutes gern,", "tokens": ["Und", "die", "ihr", "h\u00f6rt", "und", "schaut", "was", "Gu\u00b7tes", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "KON", "VVFIN", "PIS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu einem funkelnagelneuen Spiel", "tokens": ["Zu", "ei\u00b7nem", "fun\u00b7kel\u00b7na\u00b7gel\u00b7neu\u00b7en", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im allerfunkelnagelneusten Styl;", "tokens": ["Im", "al\u00b7ler\u00b7fun\u00b7kel\u00b7na\u00b7gel\u00b7neus\u00b7ten", "Styl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schlicht ausgedrechselt, kunstlos zugestutzt,", "tokens": ["Schlicht", "aus\u00b7ge\u00b7drech\u00b7selt", ",", "kunst\u00b7los", "zu\u00b7ge\u00b7stutzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit edler deutscher Rohheit aufgeputzt,", "tokens": ["Mit", "ed\u00b7ler", "deut\u00b7scher", "Roh\u00b7heit", "auf\u00b7ge\u00b7putzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Keck wie ein Bursch' im Stadtsoldatenstrau\u00df,", "tokens": ["Keck", "wie", "ein", "Bur\u00b7sch'", "im", "Stadt\u00b7sol\u00b7da\u00b7ten\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Dazu wohl auch ein wenig fromm f\u00fcr's Haus:", "tokens": ["Da\u00b7zu", "wohl", "auch", "ein", "we\u00b7nig", "fromm", "f\u00fcr's", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "ART", "PIS", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das mag genug mir zur Empfehlung sein,", "tokens": ["Das", "mag", "ge\u00b7nug", "mir", "zur", "Emp\u00b7feh\u00b7lung", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPER", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wem die behagt, der trete nur herein.", "tokens": ["Wem", "die", "be\u00b7hagt", ",", "der", "tre\u00b7te", "nur", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erhoffe, weil es grad' ist Winterzeit,", "tokens": ["Er\u00b7hof\u00b7fe", ",", "weil", "es", "grad'", "ist", "Win\u00b7ter\u00b7zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Thut euch ein St\u00fcndlein hier im Gr\u00fcn nicht Leid;", "tokens": ["Thut", "euch", "ein", "St\u00fcnd\u00b7lein", "hier", "im", "Gr\u00fcn", "nicht", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wi\u00dft es nur, da\u00df heut' in meinem Lied", "tokens": ["Denn", "wi\u00dft", "es", "nur", ",", "da\u00df", "heut'", "in", "mei\u00b7nem", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Lenz mit allen seinen Blumen bl\u00fcht.", "tokens": ["Der", "Lenz", "mit", "al\u00b7len", "sei\u00b7nen", "Blu\u00b7men", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Freien geht die freie Handlung vor,", "tokens": ["Im", "Frei\u00b7en", "geht", "die", "frei\u00b7e", "Hand\u00b7lung", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In reiner Luft, weit aus der St\u00e4dte Thor,", "tokens": ["In", "rei\u00b7ner", "Luft", ",", "weit", "aus", "der", "St\u00e4d\u00b7te", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch Wald und Feld, in Gr\u00fcnden, auf den H\u00f6h'n;", "tokens": ["Durch", "Wald", "und", "Feld", ",", "in", "Gr\u00fcn\u00b7den", ",", "auf", "den", "H\u00f6h'n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und was nur in vier W\u00e4nden darf geschehn,", "tokens": ["Und", "was", "nur", "in", "vier", "W\u00e4n\u00b7den", "darf", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "CARD", "NN", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das schaut ihr halb durch's offne Fenster an,", "tokens": ["Das", "schaut", "ihr", "halb", "durch's", "off\u00b7ne", "Fens\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So ist der Kunst und euch genug gethan.", "tokens": ["So", "ist", "der", "Kunst", "und", "euch", "ge\u00b7nug", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wenn ihr nach des Spiels Personen fragt,", "tokens": ["Doch", "wenn", "ihr", "nach", "des", "Spiels", "Per\u00b7so\u00b7nen", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So kann ich euch, den Musen sei's geklagt,", "tokens": ["So", "kann", "ich", "euch", ",", "den", "Mu\u00b7sen", "sei's", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das ist ein junger blonder M\u00fcllersknecht.", "tokens": ["Das", "ist", "ein", "jun\u00b7ger", "blon\u00b7der", "M\u00fcl\u00b7ler\u00b7sknecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn, ob der Bach zuletzt ein Wort auch spricht,", "tokens": ["Denn", ",", "ob", "der", "Bach", "zu\u00b7letzt", "ein", "Wort", "auch", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wird ein Bach deshalb Person noch nicht.", "tokens": ["So", "wird", "ein", "Bach", "des\u00b7halb", "Per\u00b7son", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum nehmt nur heut das Monodram vorlieb:", "tokens": ["Drum", "nehmt", "nur", "heut", "das", "Mo\u00b7no\u00b7dram", "vor\u00b7lieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer mehr giebt, als er hat, der hei\u00dft ein Dieb.", "tokens": ["Wer", "mehr", "giebt", ",", "als", "er", "hat", ",", "der", "hei\u00dft", "ein", "Dieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Auch ist daf\u00fcr die Scene reich geziert,", "tokens": ["Auch", "ist", "da\u00b7f\u00fcr", "die", "Sce\u00b7ne", "reich", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit gr\u00fcnem Sammet unten tapeziert,", "tokens": ["Mit", "gr\u00fc\u00b7nem", "Sam\u00b7met", "un\u00b7ten", "ta\u00b7pe\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ist mit tausend Blumen bunt gestickt,", "tokens": ["Der", "ist", "mit", "tau\u00b7send", "Blu\u00b7men", "bunt", "ge\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "CARD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Weg und Steg dar\u00fcber ausgedr\u00fcckt.", "tokens": ["Und", "Weg", "und", "Steg", "da\u00b7r\u00fc\u00b7ber", "aus\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne strahlt von oben hell herein", "tokens": ["Die", "Son\u00b7ne", "strahlt", "von", "o\u00b7ben", "hell", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und bricht in Thau und Thr\u00e4nen ihren Schein,", "tokens": ["Und", "bricht", "in", "Thau", "und", "Thr\u00e4\u00b7nen", "ih\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und auch der Mond blickt aus der Wolken Flor", "tokens": ["Und", "auch", "der", "Mond", "blickt", "aus", "der", "Wol\u00b7ken", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schwerm\u00fcthig, wie's die Mode will, hervor.", "tokens": ["Schwer\u00b7m\u00fct\u00b7hig", ",", "wie's", "die", "Mo\u00b7de", "will", ",", "her\u00b7vor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "VMFIN", "$,", "PTKVZ", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.9": {"text": "Den Hintergrund umkr\u00e4nzt ein hoher Wald,", "tokens": ["Den", "Hin\u00b7ter\u00b7grund", "um\u00b7kr\u00e4nzt", "ein", "ho\u00b7her", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Hund schl\u00e4gt an, das muntre Jagdhorn schallt;", "tokens": ["Der", "Hund", "schl\u00e4gt", "an", ",", "das", "mun\u00b7tre", "Jagd\u00b7horn", "schallt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Hier st\u00fcrzt vom schroffen Fels der junge Quell", "tokens": ["Hier", "st\u00fcrzt", "vom", "schrof\u00b7fen", "Fels", "der", "jun\u00b7ge", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und flie\u00dft im Thal als B\u00e4chlein silberhell;", "tokens": ["Und", "flie\u00dft", "im", "Thal", "als", "B\u00e4ch\u00b7lein", "sil\u00b7ber\u00b7hell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KOUS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das M\u00fchlrad braust, die Werke klappern drein,", "tokens": ["Das", "M\u00fchl\u00b7rad", "braust", ",", "die", "Wer\u00b7ke", "klap\u00b7pern", "drein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Man h\u00f6rt die V\u00f6glein kaum im nahen Hain.", "tokens": ["Man", "h\u00f6rt", "die", "V\u00f6\u00b7glein", "kaum", "im", "na\u00b7hen", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum denkt, wenn euch zu rauh manch Lied-", "tokens": ["Drum", "denkt", ",", "wenn", "euch", "zu", "rauh", "manch", "Lie\u00b7d"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "chen klingt,", "tokens": ["chen", "klingt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da\u00df das Lokal es also mit sich bringt.", "tokens": ["Da\u00df", "das", "Lo\u00b7kal", "es", "al\u00b7so", "mit", "sich", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Doch, was das Sch\u00f6nste in der M\u00fchle ist,", "tokens": ["Doch", ",", "was", "das", "Sch\u00f6ns\u00b7te", "in", "der", "M\u00fch\u00b7le", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das wird euch sagen mein Monodramist;", "tokens": ["Das", "wird", "euch", "sa\u00b7gen", "mein", "Mo\u00b7no\u00b7dra\u00b7mist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verrieth' ich's euch, verd\u00fcrb' ich ihm das Spiel:", "tokens": ["Ver\u00b7ri\u00b7e\u00b7th'", "ich's", "euch", ",", "ver\u00b7d\u00fcrb'", "ich", "ihm", "das", "Spiel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gehabt euch wohl und am\u00fcsirt euch viel!", "tokens": ["Ge\u00b7habt", "euch", "wohl", "und", "am\u00b7\u00fc\u00b7sirt", "euch", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}