{"dta.poem.12314": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Erster J\u00e4ger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schwing mein Horn ins Jammerthal,               ", "tokens": ["Ich", "schwing", "mein", "Horn", "ins", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Freud ist mir verschwunden,", "tokens": ["Mein", "Freud", "ist", "mir", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab gejagt, mu\u00df abelahn,", "tokens": ["Ich", "hab", "ge\u00b7jagt", ",", "mu\u00df", "a\u00b7be\u00b7lahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wild lauft vor den Hunden,", "tokens": ["Das", "Wild", "lauft", "vor", "den", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein edel Thier in diesem Feld", "tokens": ["Ein", "e\u00b7del", "Thier", "in", "die\u00b7sem", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4tt ich mir auserkohren,", "tokens": ["H\u00e4tt", "ich", "mir", "au\u00b7ser\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das schied von mir als ich es meld,", "tokens": ["Das", "schied", "von", "mir", "als", "ich", "es", "meld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Jagen ist verloren.", "tokens": ["Mein", "Ja\u00b7gen", "ist", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fahr hin Gewild in Waldes-Lust,", "tokens": ["Fahr", "hin", "Ge\u00b7wild", "in", "Wal\u00b7des\u00b7Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will dich nimmer schrecken,", "tokens": ["Ich", "will", "dich", "nim\u00b7mer", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jagen dein schneeweisse Brust,", "tokens": ["Und", "ja\u00b7gen", "dein", "schnee\u00b7weis\u00b7se", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein ander mu\u00df dich wecken,", "tokens": ["Ein", "an\u00b7der", "mu\u00df", "dich", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Jagdgeschrey, und Hundebi\u00df,", "tokens": ["Mit", "Jagd\u00b7ge\u00b7schrey", ",", "und", "Hun\u00b7de\u00b7bi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du kaum m\u00f6gst entrinnen:", "tokens": ["Da\u00df", "du", "kaum", "m\u00f6gst", "ent\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Halt dich in Hut, sch\u00f6ns Maidlein gut,", "tokens": ["Halt", "dich", "in", "Hut", ",", "sch\u00f6ns", "Maid\u00b7lein", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Leid scheid ich von hinnen.", "tokens": ["Mit", "Leid", "scheid", "ich", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Hochgewild ich fahen kann,", "tokens": ["Kein", "Hoch\u00b7ge\u00b7wild", "ich", "fa\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mu\u00df ich oft entgelten;", "tokens": ["Das", "mu\u00df", "ich", "oft", "ent\u00b7gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch halt ich stets auf J\u00e4gers-Bahn,", "tokens": ["Noch", "halt", "ich", "stets", "auf", "J\u00e4\u00b7ger\u00b7sBahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wiewohl mir Gl\u00fcck kommt selten:", "tokens": ["Wie\u00b7wohl", "mir", "Gl\u00fcck", "kommt", "sel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mag ich nicht han ein Hochwild sch\u00f6n,", "tokens": ["Mag", "ich", "nicht", "han", "ein", "Hoch\u00b7wild", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df ich mich begn\u00fcgen,", "tokens": ["So", "la\u00df", "ich", "mich", "be\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Am Hasenfleisch, nichts mehr ich wei\u00df,", "tokens": ["Am", "Ha\u00b7sen\u00b7fleisch", ",", "nichts", "mehr", "ich", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das mag mich nicht betr\u00fcgen.", "tokens": ["Das", "mag", "mich", "nicht", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}