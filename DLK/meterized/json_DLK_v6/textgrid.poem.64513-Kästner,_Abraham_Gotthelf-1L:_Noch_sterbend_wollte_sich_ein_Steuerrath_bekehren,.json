{"textgrid.poem.64513": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch sterbend wollte sich ein Steuerrath bekehren,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch sterbend wollte sich ein Steuerrath bekehren,", "tokens": ["Noch", "ster\u00b7bend", "woll\u00b7te", "sich", "ein", "Steu\u00b7er\u00b7rath", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lie\u00df der Armen Zahl, die er gemacht, ern\u00e4hren:", "tokens": ["Und", "lie\u00df", "der", "Ar\u00b7men", "Zahl", ",", "die", "er", "ge\u00b7macht", ",", "er\u00b7n\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Thoren, welche ", "tokens": ["An", "Tho\u00b7ren", ",", "wel\u00b7che"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hat er im Tode noch ein Liebeswerk ver\u00fcbt,", "tokens": ["Hat", "er", "im", "To\u00b7de", "noch", "ein", "Lie\u00b7bes\u00b7werk", "ver\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch dein Gewissen kann, o ", "tokens": ["Auch", "dein", "Ge\u00b7wis\u00b7sen", "kann", ",", "o"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "$,", "FM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Reicht dein Verm\u00f6gen zu, zwo Stiftungen zu machen?", "tokens": ["Reicht", "dein", "Ver\u00b7m\u00f6\u00b7gen", "zu", ",", "zwo", "Stif\u00b7tun\u00b7gen", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "CARD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}