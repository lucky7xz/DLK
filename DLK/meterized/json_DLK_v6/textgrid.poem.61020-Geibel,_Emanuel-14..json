{"textgrid.poem.61020": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus zerri\u00dfnen Wolkenmassen", "tokens": ["Aus", "zer\u00b7ri\u00df\u00b7nen", "Wol\u00b7ken\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt ins Blau der goldne Mond", "tokens": ["Steigt", "ins", "Blau", "der", "gold\u00b7ne", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und begl\u00e4nzt den Bergesgipfel,", "tokens": ["Und", "be\u00b7gl\u00e4nzt", "den", "Ber\u00b7ges\u00b7gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Burgruine thront.", "tokens": ["Wo", "die", "Bur\u00b7gru\u00b7i\u00b7ne", "thront", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Am bemoosten Turme steh' ich,", "tokens": ["Am", "be\u00b7moos\u00b7ten", "Tur\u00b7me", "steh'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelsw\u00e4rts das Angesicht,", "tokens": ["Him\u00b7mels\u00b7w\u00e4rts", "das", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich horche, und ich lausche,", "tokens": ["Und", "ich", "hor\u00b7che", ",", "und", "ich", "lau\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Mond herniederspricht.", "tokens": ["Was", "der", "Mond", "her\u00b7nie\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von viel tausend M\u00e4dchenaugen", "tokens": ["Von", "viel", "tau\u00b7send", "M\u00e4d\u00b7chen\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's ein wunderbares Lied,", "tokens": ["Ist's", "ein", "wun\u00b7der\u00b7ba\u00b7res", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von viel tausend roten K\u00fcssen,", "tokens": ["Von", "viel", "tau\u00b7send", "ro\u00b7ten", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er in den Talen sieht.", "tokens": ["Die", "er", "in", "den", "Ta\u00b7len", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und schon will er mir erz\u00e4hlen", "tokens": ["Und", "schon", "will", "er", "mir", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem fernen blonden Kind -", "tokens": ["Von", "dem", "fer\u00b7nen", "blon\u00b7den", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, da kommen dunkle Wolken,", "tokens": ["Ach", ",", "da", "kom\u00b7men", "dunk\u00b7le", "Wol\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Lied verweht im Wind.", "tokens": ["Und", "das", "Lied", "ver\u00b7weht", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}