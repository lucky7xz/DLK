{"dta.poem.1049": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 5. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Derwegen will es sich geb\u00fchren/", "tokens": ["Der\u00b7we\u00b7gen", "will", "es", "sich", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrstin gleicher sch\u00f6n vnd from\u0303keit/", "tokens": ["F\u00fcrs\u00b7tin", "glei\u00b7cher", "sch\u00f6n", "vnd", "from\u0303keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJD", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines Vaterlands s\u00fcssigkeit", "tokens": ["Dei\u00b7nes", "Va\u00b7ter\u00b7lands", "s\u00fcs\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Nicht zuvil zu gem\u00fcht zuf\u00fchren:", "tokens": ["Nicht", "zu\u00b7vil", "zu", "ge\u00b7m\u00fcht", "zu\u00b7f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sondern nu bey deinem gemahl", "tokens": ["Son\u00b7dern", "nu", "bey", "dei\u00b7nem", "ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "(dessen hertz gro\u00df zu allen stunden", "tokens": ["(", "des\u00b7sen", "hertz", "gro\u00df", "zu", "al\u00b7len", "stun\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELAT", "NN", "ADJD", "APPR", "PIAT", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nicht konte werden \u00fcberwunden", "tokens": ["Nicht", "kon\u00b7te", "wer\u00b7den", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dan durch deiner Zierden anzahl)", "tokens": ["Dan", "durch", "dei\u00b7ner", "Zier\u00b7den", "an\u00b7zahl", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dein gem\u00fcht nit mehr zubekr\u00e4ncken/", "tokens": ["Dein", "ge\u00b7m\u00fcht", "nit", "mehr", "zu\u00b7be\u00b7kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Noch deiner Eltern zugedencken:", "tokens": ["Noch", "dei\u00b7ner", "El\u00b7tern", "zu\u00b7ge\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wanschon dein volkomner anfang", "tokens": ["Wan\u00b7schon", "dein", "vol\u00b7kom\u00b7ner", "an\u00b7fang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kam von zwifach gecr\u00f6ntem rang", "tokens": ["Kam", "von", "zwi\u00b7fach", "ge\u00b7cr\u00f6n\u00b7tem", "rang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Solcher K\u00f6nig vnd Potentaten/", "tokens": ["Sol\u00b7cher", "K\u00f6\u00b7nig", "vnd", "Po\u00b7ten\u00b7ta\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Welche streitbar/ gerecht vnd milt", "tokens": ["Wel\u00b7che", "streit\u00b7bar", "/", "ge\u00b7recht", "vnd", "milt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "$(", "ADJD", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Mit frids vnd kriegs l\u00f6blichen thaten", "tokens": ["Mit", "frids", "vnd", "kriegs", "l\u00f6b\u00b7li\u00b7chen", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVINF", "VVFIN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Des Vmbkraises vier eck erf\u00fclt.", "tokens": ["Des", "Vmb\u00b7krai\u00b7ses", "vier", "eck", "er\u00b7f\u00fclt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}