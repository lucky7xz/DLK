{"textgrid.poem.41063": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4he der Fernen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Licht und Dunkel,", "tokens": ["Durch", "Licht", "und", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durch Weh und Lust,", "tokens": ["Durch", "Weh", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Trag' ich dich stille", "tokens": ["Trag'", "ich", "dich", "stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In meiner Brust.", "tokens": ["In", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es trennen Meere", "tokens": ["Es", "tren\u00b7nen", "Mee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mich wohl von dir,", "tokens": ["Mich", "wohl", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch mein' ich, schw\u00f6r' ich,", "tokens": ["Doch", "mein'", "ich", ",", "schw\u00f6r'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du seist bei mir!", "tokens": ["Du", "seist", "bei", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "F\u00fchl' dich so innig", "tokens": ["F\u00fchl'", "dich", "so", "in\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In mir, in mir!", "tokens": ["In", "mir", ",", "in", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ach! dies Herze", "tokens": ["Und", "ach", "!", "dies", "Her\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$.", "PDS", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Will nicht von hier,", "tokens": ["Will", "nicht", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Will sich nur legen", "tokens": ["Will", "sich", "nur", "le\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So mit dir, ach!", "tokens": ["So", "mit", "dir", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tief in die Erde,", "tokens": ["Tief", "in", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ins Brautgemach.", "tokens": ["Ins", "Braut\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}