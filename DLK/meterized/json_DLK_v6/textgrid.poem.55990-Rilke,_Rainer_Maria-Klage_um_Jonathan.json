{"textgrid.poem.55990": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Klage um Jonathan", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach sind auch K\u00f6nige nicht von Bestand", "tokens": ["Ach", "sind", "auch", "K\u00f6\u00b7ni\u00b7ge", "nicht", "von", "Be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "NN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und d\u00fcrfen hingehn wie gemeine Dinge,", "tokens": ["und", "d\u00fcr\u00b7fen", "hin\u00b7gehn", "wie", "ge\u00b7mei\u00b7ne", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "obwohl ihr Druck wie der der Siegelringe", "tokens": ["ob\u00b7wohl", "ihr", "Druck", "wie", "der", "der", "Sie\u00b7gel\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KOKOM", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sich widerbildet in das weiche Land.", "tokens": ["sich", "wi\u00b7der\u00b7bil\u00b7det", "in", "das", "wei\u00b7che", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie aber konntest du, so angefangen", "tokens": ["Wie", "a\u00b7ber", "konn\u00b7test", "du", ",", "so", "an\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit deines Herzens Initial,", "tokens": ["mit", "dei\u00b7nes", "Her\u00b7zens", "I\u00b7ni\u00b7ti\u00b7al", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "aufh\u00f6ren pl\u00f6tzlich: W\u00e4rme meiner Wangen.", "tokens": ["auf\u00b7h\u00f6\u00b7ren", "pl\u00f6tz\u00b7lich", ":", "W\u00e4r\u00b7me", "mei\u00b7ner", "Wan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O da\u00df dich einer noch einmal", "tokens": ["O", "da\u00df", "dich", "ei\u00b7ner", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ART", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "erzeugte, wenn sein Samen in ihm gl\u00e4nzt.", "tokens": ["er\u00b7zeug\u00b7te", ",", "wenn", "sein", "Sa\u00b7men", "in", "ihm", "gl\u00e4nzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Irgend ein Fremder sollte dich zerst\u00f6ren,", "tokens": ["Ir\u00b7gend", "ein", "Frem\u00b7der", "soll\u00b7te", "dich", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "und der dir innig war, ist nichts dabei", "tokens": ["und", "der", "dir", "in\u00b7nig", "war", ",", "ist", "nichts", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "PIS", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und mu\u00df sich halten und die Botschaft h\u00f6ren;", "tokens": ["und", "mu\u00df", "sich", "hal\u00b7ten", "und", "die", "Bot\u00b7schaft", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie wunde Tiere auf den Lagern l\u00f6hren,", "tokens": ["wie", "wun\u00b7de", "Tie\u00b7re", "auf", "den", "La\u00b7gern", "l\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "m\u00f6cht ich mich legen mit Geschrei:", "tokens": ["m\u00f6cht", "ich", "mich", "le\u00b7gen", "mit", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "denn da und da, an meinen scheusten Orten,", "tokens": ["denn", "da", "und", "da", ",", "an", "mei\u00b7nen", "scheus\u00b7ten", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bist du mir ausgerissen wie das Haar,", "tokens": ["bist", "du", "mir", "aus\u00b7ge\u00b7ris\u00b7sen", "wie", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das in den Achselh\u00f6hlen w\u00e4chst und dorten,", "tokens": ["das", "in", "den", "Ach\u00b7sel\u00b7h\u00f6h\u00b7len", "w\u00e4chst", "und", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wo ich ein Spiel f\u00fcr Frauen war,", "tokens": ["wo", "ich", "ein", "Spiel", "f\u00fcr", "Frau\u00b7en", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "bevor du meine dort verfitzten Sinne", "tokens": ["be\u00b7vor", "du", "mei\u00b7ne", "dort", "ver\u00b7fitz\u00b7ten", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aufstr\u00e4hntest wie man einen Knaul entflicht;", "tokens": ["auf\u00b7str\u00e4hn\u00b7test", "wie", "man", "ei\u00b7nen", "Knaul", "ent\u00b7flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da sah ich auf und wurde deiner inne: \u2013", "tokens": ["da", "sah", "ich", "auf", "und", "wur\u00b7de", "dei\u00b7ner", "in\u00b7ne", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "PPOSAT", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt aber gehst du mir aus dem Gesicht.", "tokens": ["Jetzt", "a\u00b7ber", "gehst", "du", "mir", "aus", "dem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}