{"textgrid.poem.36507": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. Der Esel und der M\u00fcller", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Eselchen, ein gutes Vieh,", "tokens": ["Ein", "E\u00b7sel\u00b7chen", ",", "ein", "gu\u00b7tes", "Vieh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Brach aus in bittre Klagen:", "tokens": ["Brach", "aus", "in", "bitt\u00b7re", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der dumme Knappe da, was braucht er mich zu schlagen?", "tokens": ["Der", "dum\u00b7me", "Knap\u00b7pe", "da", ",", "was", "braucht", "er", "mich", "zu", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weit mehr, als er, hab' ich Genie,", "tokens": ["Weit", "mehr", ",", "als", "er", ",", "hab'", "ich", "Ge\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PPER", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Den schweren Sack zu tragen!", "tokens": ["Den", "schwe\u00b7ren", "Sack", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was braucht er mich zu schlagen?", "tokens": ["Was", "braucht", "er", "mich", "zu", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Meister M\u00fcller h\u00f6rt's", "tokens": ["Der", "Meis\u00b7ter", "M\u00fcl\u00b7ler", "h\u00f6rt's"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und spricht zum Eselchen: Ja wohl! Erfahrung lehrt's,", "tokens": ["Und", "spricht", "zum", "E\u00b7sel\u00b7chen", ":", "Ja", "wohl", "!", "Er\u00b7fah\u00b7rung", "lehrt's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "PTKANT", "ADV", "$.", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du hast Genie, den Sack zu tragen,", "tokens": ["Du", "hast", "Ge\u00b7nie", ",", "den", "Sack", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und still zu stehn!", "tokens": ["Und", "still", "zu", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Allein, Genie den Sack zu tragen und zu gehn,", "tokens": ["Al\u00b7lein", ",", "Ge\u00b7nie", "den", "Sack", "zu", "tra\u00b7gen", "und", "zu", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das hast du nicht! Dazu mu\u00df dich der Knappe schlagen!", "tokens": ["Das", "hast", "du", "nicht", "!", "Da\u00b7zu", "mu\u00df", "dich", "der", "Knap\u00b7pe", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "$.", "PAV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}