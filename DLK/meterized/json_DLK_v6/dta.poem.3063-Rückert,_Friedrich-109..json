{"dta.poem.3063": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schon \u00f6fter hab' ich dir in R\u00e4thseln vorgetragen", "tokens": ["Schon", "\u00f6f\u00b7ter", "hab'", "ich", "dir", "in", "R\u00e4th\u00b7seln", "vor\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Antworten, die sich gibt die Seel' auf Zweifelsfragen,", "tokens": ["Ant\u00b7wor\u00b7ten", ",", "die", "sich", "gibt", "die", "Seel'", "auf", "Zwei\u00b7fels\u00b7fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf Fragen, die sie an sich selbst thut \u00fcber sich:", "tokens": ["Auf", "Fra\u00b7gen", ",", "die", "sie", "an", "sich", "selbst", "thut", "\u00fc\u00b7ber", "sich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "ADV", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woher, woraus, wovon, wof\u00fcr, wozu bin ich?", "tokens": ["Wo\u00b7her", ",", "wo\u00b7raus", ",", "wo\u00b7von", ",", "wo\u00b7f\u00fcr", ",", "wo\u00b7zu", "bin", "ich", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "PWAV", "$,", "PWAV", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wozu kam ich hieher? von welchem Trieb getrieben?", "tokens": ["Wo\u00b7zu", "kam", "ich", "hie\u00b7her", "?", "von", "wel\u00b7chem", "Trieb", "ge\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PAV", "$.", "APPR", "PWAT", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und warum bin ich nicht dort wo ich war, geblieben?", "tokens": ["Und", "wa\u00b7rum", "bin", "ich", "nicht", "dort", "wo", "ich", "war", ",", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "PWAV", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bin ich herabgesandt? bin ich herabgebannt?", "tokens": ["Bin", "ich", "her\u00b7ab\u00b7ge\u00b7sandt", "?", "bin", "ich", "her\u00b7ab\u00b7ge\u00b7bannt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab' ich, und wei\u00df nicht mehr, mich frei herabgewandt?", "tokens": ["Hab'", "ich", ",", "und", "wei\u00df", "nicht", "mehr", ",", "mich", "frei", "her\u00b7ab\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "$,", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Herabgeflogen wol? villeicht herabgestiegen?", "tokens": ["Her\u00b7ab\u00b7ge\u00b7flo\u00b7gen", "wol", "?", "vil\u00b7leicht", "her\u00b7ab\u00b7ge\u00b7stie\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herabgefallen gar? am besten w\u00e4re Fliegen.", "tokens": ["Her\u00b7ab\u00b7ge\u00b7fal\u00b7len", "gar", "?", "am", "bes\u00b7ten", "w\u00e4\u00b7re", "Flie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PTKA", "ADJD", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn ich herab einst flog, werd' ich hinauf einst fliegen;", "tokens": ["Wenn", "ich", "her\u00b7ab", "einst", "flog", ",", "werd'", "ich", "hin\u00b7auf", "einst", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich herunter fiel, wie lange soll ich liegen?", "tokens": ["Wenn", "ich", "her\u00b7un\u00b7ter", "fiel", ",", "wie", "lan\u00b7ge", "soll", "ich", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "PWAV", "ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das, Seelchen, sag' ich dir: du bist gewis geflogen,", "tokens": ["Das", ",", "Seel\u00b7chen", ",", "sag'", "ich", "dir", ":", "du", "bist", "ge\u00b7wis", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn als ein Vogel nicht, doch wie ein Pfeil vom Bogen.", "tokens": ["Wenn", "als", "ein", "Vo\u00b7gel", "nicht", ",", "doch", "wie", "ein", "Pfeil", "vom", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "PTKNEG", "$,", "ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Vernimm den Ernst von mir: Zwei Schwingen dienten dir,", "tokens": ["Ver\u00b7nimm", "den", "Ernst", "von", "mir", ":", "Zwei", "Schwin\u00b7gen", "dien\u00b7ten", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPER", "$.", "CARD", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eine Langeweil, die andre Neubegier.", "tokens": ["Die", "ei\u00b7ne", "Lan\u00b7ge\u00b7weil", ",", "die", "and\u00b7re", "Neu\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Langweile war es m\u00fcd' im ew'gen Chor zu schweben,", "tokens": ["Lang\u00b7wei\u00b7le", "war", "es", "m\u00fcd'", "im", "ew'\u00b7gen", "Chor", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VMFIN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Neugierde f\u00fchlte Lust was andres zu erleben.", "tokens": ["Neu\u00b7gier\u00b7de", "f\u00fchl\u00b7te", "Lust", "was", "and\u00b7res", "zu", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PWS", "PIS", "PTKZU", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.10": {"line.1": {"text": "So trugen sie dich her, zu b\u00fc\u00dfen ihre Lust,", "tokens": ["So", "tru\u00b7gen", "sie", "dich", "her", ",", "zu", "b\u00fc\u00b7\u00dfen", "ih\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer f\u00fchlst du noch die beiden an der Brust.", "tokens": ["Und", "im\u00b7mer", "f\u00fchlst", "du", "noch", "die", "bei\u00b7den", "an", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "PIAT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ihr Nagen in der Brust f\u00fchlst du mit Unbehagen,", "tokens": ["Ihr", "Na\u00b7gen", "in", "der", "Brust", "f\u00fchlst", "du", "mit", "Un\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnschest da\u00df sie dich nur immer weiter tragen.", "tokens": ["Und", "w\u00fcn\u00b7schest", "da\u00df", "sie", "dich", "nur", "im\u00b7mer", "wei\u00b7ter", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Ich rathe dir, wann du kommst einmal heim zu ruhn,", "tokens": ["Ich", "ra\u00b7the", "dir", ",", "wann", "du", "kommst", "ein\u00b7mal", "heim", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die beiden Schwingen ganz und gar dann abzuthun.", "tokens": ["Die", "bei\u00b7den", "Schwin\u00b7gen", "ganz", "und", "gar", "dann", "ab\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "KON", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Doch, bleibt noch Trieb in dir, wird er sie wieder treiben,", "tokens": ["Doch", ",", "bleibt", "noch", "Trieb", "in", "dir", ",", "wird", "er", "sie", "wie\u00b7der", "trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ADV", "NN", "APPR", "PPER", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wieder wirst du dort nicht lange k\u00f6nnen bleiben.", "tokens": ["Und", "wie\u00b7der", "wirst", "du", "dort", "nicht", "lan\u00b7ge", "k\u00f6n\u00b7nen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "So fleug denn, weil du mu\u00dft! Ich aber, wenn Gefieder", "tokens": ["So", "fleug", "denn", ",", "weil", "du", "mu\u00dft", "!", "Ich", "a\u00b7ber", ",", "wenn", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$.", "PPER", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir spro\u00dfte, fl\u00f6g' ich auf, und nie herunter wieder.", "tokens": ["Mir", "spro\u00df\u00b7te", ",", "fl\u00f6g'", "ich", "auf", ",", "und", "nie", "her\u00b7un\u00b7ter", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Denn, ob ich es zur Zier sag' oder Schande mir:", "tokens": ["Denn", ",", "ob", "ich", "es", "zur", "Zier", "sag'", "o\u00b7der", "Schan\u00b7de", "mir", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Langeweile fehlt mir auch die Neubegier.", "tokens": ["Mit", "Lan\u00b7ge\u00b7wei\u00b7le", "fehlt", "mir", "auch", "die", "Neu\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Ich bliebe fort und fort gar gern an einem Ort,", "tokens": ["Ich", "blie\u00b7be", "fort", "und", "fort", "gar", "gern", "an", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang es seyn soll, hier, und wann es seyn kann, dort.", "tokens": ["So\u00b7lang", "es", "seyn", "soll", ",", "hier", ",", "und", "wann", "es", "seyn", "kann", ",", "dort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VAINF", "VMFIN", "$,", "ADV", "$,", "KON", "PWAV", "PPER", "VAINF", "VMFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}