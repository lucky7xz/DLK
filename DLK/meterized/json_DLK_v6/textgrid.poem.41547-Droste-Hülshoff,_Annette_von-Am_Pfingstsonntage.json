{"textgrid.poem.41547": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Pfingstsonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still war der Tag, die Sonne stand", "tokens": ["Still", "war", "der", "Tag", ",", "die", "Son\u00b7ne", "stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So klar an unbefleckten Domeshallen;", "tokens": ["So", "klar", "an", "un\u00b7be\u00b7fleck\u00b7ten", "Do\u00b7mes\u00b7hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Luft in Orientes Brand", "tokens": ["Die", "Luft", "in", "O\u00b7ri\u00b7en\u00b7tes", "Brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ausgedorrt, lie\u00df matt die Fl\u00fcgel fallen.", "tokens": ["Wie", "aus\u00b7ge\u00b7dorrt", ",", "lie\u00df", "matt", "die", "Fl\u00fc\u00b7gel", "fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein H\u00e4uflein sieh, so Mann als Greis,", "tokens": ["Ein", "H\u00e4uf\u00b7lein", "sieh", ",", "so", "Mann", "als", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch Frauen knieend, keine Worte hallen,", "tokens": ["Auch", "Frau\u00b7en", "kni\u00b7e\u00b7end", ",", "kei\u00b7ne", "Wor\u00b7te", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie beten leis.", "tokens": ["Sie", "be\u00b7ten", "leis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo bleibt der Tr\u00f6ster, treuer Hort,", "tokens": ["Wo", "bleibt", "der", "Tr\u00f6s\u00b7ter", ",", "treu\u00b7er", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den scheidend doch verhei\u00dfen du den Deinen?", "tokens": ["Den", "schei\u00b7dend", "doch", "ver\u00b7hei\u00b7\u00dfen", "du", "den", "Dei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht zagen sie; fest steht dein Wort,", "tokens": ["Nicht", "za\u00b7gen", "sie", ";", "fest", "steht", "dein", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$.", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch bang und tr\u00fcbe mu\u00df die Zeit wohl scheinen.", "tokens": ["Doch", "bang", "und", "tr\u00fc\u00b7be", "mu\u00df", "die", "Zeit", "wohl", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJA", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Stunde schleicht; schon vierzig Tag'", "tokens": ["Die", "Stun\u00b7de", "schleicht", ";", "schon", "vier\u00b7zig", "Tag'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und N\u00e4chte harrten sie in stillem Weinen,", "tokens": ["Und", "N\u00e4ch\u00b7te", "harr\u00b7ten", "sie", "in", "stil\u00b7lem", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sahn dir nach.", "tokens": ["Und", "sahn", "dir", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wo bleibt er? wo nur? Stund' an Stund',", "tokens": ["Wo", "bleibt", "er", "?", "wo", "nur", "?", "Stund'", "an", "Stund'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWAV", "ADV", "$.", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Minute will sich reihen an Minuten.", "tokens": ["Mi\u00b7nu\u00b7te", "will", "sich", "rei\u00b7hen", "an", "Mi\u00b7nu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo bleibt er denn? \u2013 und schweigt der Mund:", "tokens": ["Wo", "bleibt", "er", "denn", "?", "\u2013", "und", "schweigt", "der", "Mund", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$.", "$(", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele spricht es unter leisem Bluten.", "tokens": ["Die", "See\u00b7le", "spricht", "es", "un\u00b7ter", "lei\u00b7sem", "Blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Wirbel st\u00e4ubt, der Tiger \u00e4chzt", "tokens": ["Der", "Wir\u00b7bel", "st\u00e4ubt", ",", "der", "Ti\u00b7ger", "\u00e4chzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4lzt sich keuchend durch die sand'gen Fluten,", "tokens": ["Und", "w\u00e4lzt", "sich", "keu\u00b7chend", "durch", "die", "san\u00b7d'\u00b7gen", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Schlange lechzt.", "tokens": ["Die", "Schlan\u00b7ge", "lechzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Da horch! ein S\u00e4useln hebt sich leicht!", "tokens": ["Da", "horch", "!", "ein", "S\u00e4u\u00b7seln", "hebt", "sich", "leicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schwillt und schwillt und steigt zu Sturmes Rauschen.", "tokens": ["Es", "schwillt", "und", "schwillt", "und", "steigt", "zu", "Stur\u00b7mes", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gr\u00e4ser stehen ungebeugt;", "tokens": ["Die", "Gr\u00e4\u00b7ser", "ste\u00b7hen", "un\u00b7ge\u00b7beugt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Palme starr und staunend scheint zu lauschen.", "tokens": ["Die", "Pal\u00b7me", "starr", "und", "stau\u00b7nend", "scheint", "zu", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was zittert durch die fromme Schar,", "tokens": ["Was", "zit\u00b7tert", "durch", "die", "from\u00b7me", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was l\u00e4\u00dft sie bang' und gl\u00fche Blicke tauschen?", "tokens": ["Was", "l\u00e4\u00dft", "sie", "bang'", "und", "gl\u00fc\u00b7he", "Bli\u00b7cke", "tau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schaut auf! nehmt wahr!", "tokens": ["Schaut", "auf", "!", "nehmt", "wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Er ist's, er ist's; die Flamme zuckt", "tokens": ["Er", "ist's", ",", "er", "ist's", ";", "die", "Flam\u00b7me", "zuckt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob jedem Haupt; welch wunderbares Kreisen,", "tokens": ["Ob", "je\u00b7dem", "Haupt", ";", "welch", "wun\u00b7der\u00b7ba\u00b7res", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$.", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was durch die Adern quillt und ruckt!", "tokens": ["Was", "durch", "die", "A\u00b7dern", "quillt", "und", "ruckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zukunft bricht, es \u00f6ffnen sich die Schleusen,", "tokens": ["Die", "Zu\u00b7kunft", "bricht", ",", "es", "\u00f6ff\u00b7nen", "sich", "die", "Schleu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und unaufhaltsam str\u00f6mt das Wort", "tokens": ["Und", "un\u00b7auf\u00b7halt\u00b7sam", "str\u00f6mt", "das", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald Heroldsruf und bald im flehend leisen", "tokens": ["Bald", "He\u00b7rolds\u00b7ruf", "und", "bald", "im", "fle\u00b7hend", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADV", "APPRART", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gefl\u00fcster fort.", "tokens": ["Ge\u00b7fl\u00fcs\u00b7ter", "fort", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "O Licht, o Tr\u00f6ster, bist du, ach!", "tokens": ["O", "Licht", ",", "o", "Tr\u00f6s\u00b7ter", ",", "bist", "du", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "VAFIN", "PPER", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur jener Zeit, nur jener Schar verk\u00fcndet?", "tokens": ["Nur", "je\u00b7ner", "Zeit", ",", "nur", "je\u00b7ner", "Schar", "ver\u00b7k\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht uns, nicht \u00fcberall, wo wach", "tokens": ["Nicht", "uns", ",", "nicht", "\u00fc\u00b7be\u00b7rall", ",", "wo", "wach"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PPER", "$,", "PTKNEG", "ADV", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trostesbar sich eine Seele findet?", "tokens": ["Und", "tros\u00b7tes\u00b7bar", "sich", "ei\u00b7ne", "See\u00b7le", "fin\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich schmachte in der schw\u00fclen Nacht,", "tokens": ["Ich", "schmach\u00b7te", "in", "der", "schw\u00fc\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O leuchte, eh das Auge ganz erblindet;", "tokens": ["O", "leuch\u00b7te", ",", "eh", "das", "Au\u00b7ge", "ganz", "er\u00b7blin\u00b7det", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es weint und wacht!", "tokens": ["Es", "weint", "und", "wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}