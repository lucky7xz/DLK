{"textgrid.poem.39278": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du mit Hand und Herzen sein mein eigen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du mit Hand und Herzen sein mein eigen,", "tokens": ["Willst", "du", "mit", "Hand", "und", "Her\u00b7zen", "sein", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VAINF", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bin ich dein, mit meinem tiefsten Minnen,", "tokens": ["So", "bin", "ich", "dein", ",", "mit", "mei\u00b7nem", "tiefs\u00b7ten", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit meinem Denken, meinem Liedersinnen,", "tokens": ["Mit", "mei\u00b7nem", "Den\u00b7ken", ",", "mei\u00b7nem", "Lie\u00b7der\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nie, beim Himmel! soll dies Minnen schweigen.", "tokens": ["Und", "nie", ",", "beim", "Him\u00b7mel", "!", "soll", "dies", "Min\u00b7nen", "schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$.", "VMFIN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht kann ich Gold und Lieblichkeit dir zeigen,", "tokens": ["Nicht", "kann", "ich", "Gold", "und", "Lieb\u00b7lich\u00b7keit", "dir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Prunk und Worte, die das Herz gewinnen,", "tokens": ["Nicht", "Prunk", "und", "Wor\u00b7te", ",", "die", "das", "Herz", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch bis des Blutes Tropfen all' verrinnen,", "tokens": ["Doch", "bis", "des", "Blu\u00b7tes", "Trop\u00b7fen", "all'", "ver\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich dein Ritter, bis zum Sternenreigen.", "tokens": ["Bin", "ich", "dein", "Rit\u00b7ter", ",", "bis", "zum", "Ster\u00b7nen\u00b7rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht kann ich mild, huldflehend vor dir knien,", "tokens": ["Nicht", "kann", "ich", "mild", ",", "huld\u00b7fle\u00b7hend", "vor", "dir", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADJD", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn nimmer noch hat sich dies Knie gebeugt,", "tokens": ["Denn", "nim\u00b7mer", "noch", "hat", "sich", "dies", "Knie", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PRF", "PDS", "NN", "VVPP", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als nur dem Herrn, der mir das Lied gegeben.", "tokens": ["Als", "nur", "dem", "Herrn", ",", "der", "mir", "das", "Lied", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durch Herz und Saite soll dein Bild nur ziehn,", "tokens": ["Durch", "Herz", "und", "Sai\u00b7te", "soll", "dein", "Bild", "nur", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch wenn dein Herz in meines sich verzweigt,", "tokens": ["Doch", "wenn", "dein", "Herz", "in", "mei\u00b7nes", "sich", "ver\u00b7zweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darfst du es nimmer von mir heben.", "tokens": ["Darfst", "du", "es", "nim\u00b7mer", "von", "mir", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}