{"dta.poem.23735": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6nheit und Stolz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Du lobest Chloen? nennst sie sch\u00f6n?", "tokens": ["Du", "lo\u00b7best", "Chloen", "?", "nennst", "sie", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "O sieh doch mir erst ins Gesicht!", "tokens": ["O", "sieh", "doch", "mir", "erst", "ins", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie ich, das mu\u00dft du mir gestehn,", "tokens": ["Wie", "ich", ",", "das", "mu\u00dft", "du", "mir", "ge\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PDS", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n ist Chloe nicht.", "tokens": ["So", "sch\u00f6n", "ist", "Chloe", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NE", "PTKNEG", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ja, Phillis, da\u00df du sch\u00f6ner bist,", "tokens": ["Ja", ",", "Phil\u00b7lis", ",", "da\u00df", "du", "sch\u00f6\u00b7ner", "bist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesteh ich dir gar gerne zu:", "tokens": ["Ge\u00b7steh", "ich", "dir", "gar", "ger\u00b7ne", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist sie nicht so sch\u00f6n, so ist", "tokens": ["Doch", "ist", "sie", "nicht", "so", "sch\u00f6n", ",", "so", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie nicht so stolz, als du.", "tokens": ["Sie", "nicht", "so", "stolz", ",", "als", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}