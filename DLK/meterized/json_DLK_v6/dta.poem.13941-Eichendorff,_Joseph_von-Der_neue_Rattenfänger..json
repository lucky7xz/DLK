{"dta.poem.13941": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Rattenf\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das alte Lied das spiel' ich neu,", "tokens": ["Das", "al\u00b7te", "Lied", "das", "spiel'", "ich", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da tanzen alle Leute,", "tokens": ["Da", "tan\u00b7zen", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die Vaterl\u00e4nderei,", "tokens": ["Das", "ist", "die", "Va\u00b7ter\u00b7l\u00e4n\u00b7de\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Herr, mach' uns gescheute! \u2014", "tokens": ["O", "Herr", ",", "mach'", "uns", "ge\u00b7scheu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}