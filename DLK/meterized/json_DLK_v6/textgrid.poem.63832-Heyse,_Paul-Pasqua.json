{"textgrid.poem.63832": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Pasqua", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Last von schwarzen Haaren,", "tokens": ["Die\u00b7se", "Last", "von", "schwar\u00b7zen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Augen dunkle Glut,", "tokens": ["Die\u00b7ser", "Au\u00b7gen", "dunk\u00b7le", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so scheu und unerfahren,", "tokens": ["Und", "so", "scheu", "und", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses M\u00fcndchen rot wie Blut;", "tokens": ["Die\u00b7ses", "M\u00fcnd\u00b7chen", "rot", "wie", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Runde, zarte Kinderwangen,", "tokens": ["Run\u00b7de", ",", "zar\u00b7te", "Kin\u00b7der\u00b7wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glatt und wei\u00df wie feiner Samt,", "tokens": ["Glatt", "und", "wei\u00df", "wie", "fei\u00b7ner", "Samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in Schreck und Scham und Bangen", "tokens": ["Die", "in", "Schreck", "und", "Scham", "und", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosenschimmer \u00fcberflammt \u2013", "tokens": ["Ro\u00b7sen\u00b7schim\u00b7mer", "\u00fc\u00b7berf\u00b7lammt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, mit deinen Reizen allen", "tokens": ["Ach", ",", "mit", "dei\u00b7nen", "Rei\u00b7zen", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du einem niedren Loos,", "tokens": ["Bist", "du", "ei\u00b7nem", "nied\u00b7ren", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleine Pasqua, doch verfallen,", "tokens": ["Klei\u00b7ne", "Pas\u00b7qua", ",", "doch", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Heute noch so ahnungslos.", "tokens": ["Heu\u00b7te", "noch", "so", "ah\u00b7nungs\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4tte dich in goldner Wiege", "tokens": ["H\u00e4t\u00b7te", "dich", "in", "gold\u00b7ner", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weich geschaukelt je das Gl\u00fcck,", "tokens": ["Weich", "ge\u00b7schau\u00b7kelt", "je", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spr\u00e4che wohl von manchem Siege", "tokens": ["Spr\u00e4\u00b7che", "wohl", "von", "man\u00b7chem", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser spr\u00fch'nde Feuerblick.", "tokens": ["Die\u00b7ser", "spr\u00fch'n\u00b7de", "Feu\u00b7er\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt in \u00f6dem Haus vergeht dir", "tokens": ["Jetzt", "in", "\u00f6\u00b7dem", "Haus", "ver\u00b7geht", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Karg das Leben, gl\u00fcckverwaist.", "tokens": ["Karg", "das", "Le\u00b7ben", ",", "gl\u00fcck\u00b7ver\u00b7waist", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein Malerblick verr\u00e4t dir", "tokens": ["Nur", "ein", "Ma\u00b7ler\u00b7blick", "ver\u00b7r\u00e4t", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann und wann, wie sch\u00f6n du seist.", "tokens": ["Dann", "und", "wann", ",", "wie", "sch\u00f6n", "du", "seist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PWAV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und du d\u00fcnkst dich hochbeseligt,", "tokens": ["Und", "du", "d\u00fcnkst", "dich", "hoch\u00b7be\u00b7se\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn, bevor das Alter winkt,", "tokens": ["Wenn", ",", "be\u00b7vor", "das", "Al\u00b7ter", "winkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich ein grober Bauer ehlicht,", "tokens": ["Dich", "ein", "gro\u00b7ber", "Bau\u00b7er", "eh\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der nicht t\u00e4glich sich betrinkt.", "tokens": ["Der", "nicht", "t\u00e4g\u00b7lich", "sich", "be\u00b7trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Manchmal blickst du wohl mit Neide", "tokens": ["Manch\u00b7mal", "blickst", "du", "wohl", "mit", "Nei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer stolzen Dame nach,", "tokens": ["Ei\u00b7ner", "stol\u00b7zen", "Da\u00b7me", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in Pelz und Samt und Seide", "tokens": ["Die", "in", "Pelz", "und", "Samt", "und", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein par Worte mit dir sprach.", "tokens": ["Ein", "par", "Wor\u00b7te", "mit", "dir", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber wei\u00dft du, ob der Sch\u00f6nen,", "tokens": ["A\u00b7ber", "wei\u00dft", "du", ",", "ob", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die besitzt, was dir versagt,", "tokens": ["Die", "be\u00b7sitzt", ",", "was", "dir", "ver\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zu Haus mit tausend Tr\u00e4nen", "tokens": ["Nicht", "zu", "Haus", "mit", "tau\u00b7send", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfer Gram am Herzen nagt", "tokens": ["Hei\u00b7\u00dfer", "Gram", "am", "Her\u00b7zen", "nagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Um den Mann, er sie umschmeichelt,", "tokens": ["Um", "den", "Mann", ",", "er", "sie", "um\u00b7schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis er werbend sie bet\u00f6rt,", "tokens": ["Bis", "er", "wer\u00b7bend", "sie", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun l\u00e4ngst schon nicht mehr heuchelt,", "tokens": ["Und", "nun", "l\u00e4ngst", "schon", "nicht", "mehr", "heu\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df sein Herz nur ihr geh\u00f6rt?", "tokens": ["Da\u00df", "sein", "Herz", "nur", "ihr", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fr\u00fch mu\u00df sich die Blum' entbl\u00e4ttern,", "tokens": ["Fr\u00fch", "mu\u00df", "sich", "die", "Blum'", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauf zu hei\u00df das Licht gegl\u00fcht.", "tokens": ["Drauf", "zu", "hei\u00df", "das", "Licht", "ge\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKA", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleine Pasqua, dank den G\u00f6ttern,", "tokens": ["Klei\u00b7ne", "Pas\u00b7qua", ",", "dank", "den", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da\u00df im Schatten du erbl\u00fcht!", "tokens": ["Da\u00df", "im", "Schat\u00b7ten", "du", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}