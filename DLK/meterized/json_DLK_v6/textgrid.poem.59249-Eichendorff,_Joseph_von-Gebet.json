{"textgrid.poem.59249": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich, auf Gott nur bauend,", "tokens": ["Was", "soll", "ich", ",", "auf", "Gott", "nur", "bau\u00b7end", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schlechter sein, als all die andern,", "tokens": ["Schlech\u00b7ter", "sein", ",", "als", "all", "die", "an\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "KOUS", "PIAT", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, so wohlbehaglich schauend,", "tokens": ["Die", ",", "so", "wohl\u00b7be\u00b7hag\u00b7lich", "schau\u00b7end", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh dem eignen Nichts vertrauend,", "tokens": ["Froh", "dem", "eig\u00b7nen", "Nichts", "ver\u00b7trau\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die gemeine Stra\u00dfe wandern?", "tokens": ["Die", "ge\u00b7mei\u00b7ne", "Stra\u00b7\u00dfe", "wan\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum gabst du mir die G\u00fcte,", "tokens": ["Wa\u00b7rum", "gabst", "du", "mir", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gedanken himmelw\u00e4rts,", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und ein ritterlich Gem\u00fcte,", "tokens": ["Und", "ein", "rit\u00b7ter\u00b7lich", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das die Treue heilig h\u00fcte", "tokens": ["Das", "die", "Treu\u00b7e", "hei\u00b7lig", "h\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Zeit treulosem Scherz?", "tokens": ["In", "der", "Zeit", "treu\u00b7lo\u00b7sem", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was hast du mich blank ger\u00fcstet,", "tokens": ["Was", "hast", "du", "mich", "blank", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn mein Volk mich nicht begehrt,", "tokens": ["Wenn", "mein", "Volk", "mich", "nicht", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen mehr nach Freiheit l\u00fcstet,", "tokens": ["Kei\u00b7nen", "mehr", "nach", "Frei\u00b7heit", "l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein Herz, betr\u00fcbt, verw\u00fcstet,", "tokens": ["Da\u00df", "mein", "Herz", ",", "be\u00b7tr\u00fcbt", ",", "ver\u00b7w\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur dem Grabe zugekehrt? \u2013", "tokens": ["Nur", "dem", "Gra\u00b7be", "zu\u00b7ge\u00b7kehrt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df die Ketten mich zerschlagen,", "tokens": ["La\u00df", "die", "Ket\u00b7ten", "mich", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei zum sch\u00f6nen Gottesstreit", "tokens": ["Frei", "zum", "sch\u00f6\u00b7nen", "Got\u00b7tes\u00b7streit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine hellen Waffen tragen,", "tokens": ["Dei\u00b7ne", "hel\u00b7len", "Waf\u00b7fen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00f6hlich beten, herrlich wagen,", "tokens": ["Fr\u00f6h\u00b7lich", "be\u00b7ten", ",", "herr\u00b7lich", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib zur Kraft die Freudigkeit!", "tokens": ["Gib", "zur", "Kraft", "die", "Freu\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}