{"textgrid.poem.61196": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Kinder weinen, pflegt's nicht lang zu w\u00e4hren,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Kinder weinen, pflegt's nicht lang zu w\u00e4hren,", "tokens": ["Wenn", "Kin\u00b7der", "wei\u00b7nen", ",", "pflegt's", "nicht", "lang", "zu", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "VVFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getr\u00f6stet sind sie bald mit bunten Flittern,", "tokens": ["Ge\u00b7tr\u00f6s\u00b7tet", "sind", "sie", "bald", "mit", "bun\u00b7ten", "Flit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Tr\u00e4nen, die in M\u00e4dchenaugen zittern,", "tokens": ["Und", "Tr\u00e4\u00b7nen", ",", "die", "in", "M\u00e4d\u00b7chen\u00b7au\u00b7gen", "zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind Perlen, die die Sch\u00f6nheit nur verkl\u00e4ren.", "tokens": ["Sind", "Per\u00b7len", ",", "die", "die", "Sch\u00f6n\u00b7heit", "nur", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch anders ist es mit des Mannes Z\u00e4hren;", "tokens": ["Doch", "an\u00b7ders", "ist", "es", "mit", "des", "Man\u00b7nes", "Z\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Schmerz erpre\u00dft, vom langgen\u00e4hrten, bittern,", "tokens": ["Vom", "Schmerz", "er\u00b7pre\u00dft", ",", "vom", "lang\u00b7ge\u00b7n\u00e4hr\u00b7ten", ",", "bit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind sie den Tropfen gleich, die vor Gewittern,", "tokens": ["Sind", "sie", "den", "Trop\u00b7fen", "gleich", ",", "die", "vor", "Ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unheilverk\u00fcndend spr\u00fchn auf Laub und \u00c4hren.", "tokens": ["Un\u00b7heil\u00b7ver\u00b7k\u00fcn\u00b7dend", "spr\u00fchn", "auf", "Laub", "und", "\u00c4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "O b\u00f6se Zeit, wo solch ein hei\u00dfer Regen", "tokens": ["O", "b\u00f6\u00b7se", "Zeit", ",", "wo", "solch", "ein", "hei\u00b7\u00dfer", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An tausend Wimpern h\u00e4ngt, da\u00df wir mit Zagen", "tokens": ["An", "tau\u00b7send", "Wim\u00b7pern", "h\u00e4ngt", ",", "da\u00df", "wir", "mit", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allst\u00fcndlich schaun dem Wetterschlag entgegen!", "tokens": ["All\u00b7st\u00fcnd\u00b7lich", "schaun", "dem", "Wet\u00b7ter\u00b7schlag", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Donner raunen fern, die Wolken jagen;", "tokens": ["Die", "Don\u00b7ner", "rau\u00b7nen", "fern", ",", "die", "Wol\u00b7ken", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wogt auch heute noch der Felder Segen:", "tokens": ["Und", "wogt", "auch", "heu\u00b7te", "noch", "der", "Fel\u00b7der", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was morgen \u00fcbrig ist, wer mag es sagen!", "tokens": ["Was", "mor\u00b7gen", "\u00fcb\u00b7rig", "ist", ",", "wer", "mag", "es", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}