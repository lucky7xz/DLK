{"textgrid.poem.52711": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es krachet der Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es krachet der Wald,", "tokens": ["Es", "kra\u00b7chet", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Aeste brechen,", "tokens": ["Die", "A\u00b7es\u00b7te", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es brauset und schallt", "tokens": ["Es", "brau\u00b7set", "und", "schallt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In gie\u00dfenden B\u00e4chen.", "tokens": ["In", "gie\u00b7\u00dfen\u00b7den", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und hoch \u00fcber mir", "tokens": ["Und", "hoch", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER"], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Grollet langsam,", "tokens": ["Grol\u00b7let", "lang\u00b7sam", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und wiederum pl\u00f6tzlich", "tokens": ["Und", "wie\u00b7de\u00b7rum", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In rasenden Wirbeln", "tokens": ["In", "ra\u00b7sen\u00b7den", "Wir\u00b7beln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Geller Donner", "tokens": ["Gel\u00b7ler", "Don\u00b7ner"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Der Sturm peitschet die gro\u00dfe Fichte,", "tokens": ["Der", "Sturm", "peit\u00b7schet", "die", "gro\u00b7\u00dfe", "Fich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df sie morsch auseinanderst\u00fcrzt,", "tokens": ["Da\u00df", "sie", "morsch", "aus\u00b7ein\u00b7an\u00b7der\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und hundert Keime knickt.", "tokens": ["Und", "hun\u00b7dert", "Kei\u00b7me", "knickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Es br\u00fcllen, es l\u00e4rmen", "tokens": ["Es", "br\u00fcl\u00b7len", ",", "es", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Die jagenden Wolken,", "tokens": ["Die", "ja\u00b7gen\u00b7den", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "In scheuen Schw\u00e4rmen", "tokens": ["In", "scheu\u00b7en", "Schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Suchet Schutz das vergeisterte Wild.", "tokens": ["Su\u00b7chet", "Schutz", "das", "ver\u00b7geis\u00b7ter\u00b7te", "Wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Und wiederum nachher", "tokens": ["Und", "wie\u00b7de\u00b7rum", "nach\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ein sanftes volles Rauschen", "tokens": ["Ein", "sanf\u00b7tes", "vol\u00b7les", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Geht durch die Wipfel", "tokens": ["Geht", "durch", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Und Stille folgt ihm.", "tokens": ["Und", "Stil\u00b7le", "folgt", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Aber nicht lange so ziehet herauf", "tokens": ["A\u00b7ber", "nicht", "lan\u00b7ge", "so", "zie\u00b7het", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "VVFIN", "PTKVZ"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.22": {"text": "Ein zweites Wetter,", "tokens": ["Ein", "zwei\u00b7tes", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Voll Blitz und Regengu\u00df", "tokens": ["Voll", "Blitz", "und", "Re\u00b7gen\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Und schrecklichem Donnern.", "tokens": ["Und", "schreck\u00b7li\u00b7chem", "Don\u00b7nern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Was rennt dort ein Weib,", "tokens": ["Was", "rennt", "dort", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Ein sch\u00f6nes, verzweifelndes?", "tokens": ["Ein", "sch\u00f6\u00b7nes", ",", "ver\u00b7zwei\u00b7feln\u00b7des", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.27": {"text": "Im Arm ein Neugeborenes", "tokens": ["Im", "Arm", "ein", "Neu\u00b7ge\u00b7bo\u00b7re\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.28": {"text": "Krampfig haltend", "tokens": ["Kramp\u00b7fig", "hal\u00b7tend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Und schauernd.", "tokens": ["Und", "schau\u00b7ernd", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Ich frage, sie flieht mich,", "tokens": ["Ich", "fra\u00b7ge", ",", "sie", "flieht", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Ich rufe, sie eilet,", "tokens": ["Ich", "ru\u00b7fe", ",", "sie", "ei\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Ich folge, sie jammert:", "tokens": ["Ich", "fol\u00b7ge", ",", "sie", "jam\u00b7mert", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "O du bist auch ein Menschengesicht,", "tokens": ["O", "du", "bist", "auch", "ein", "Men\u00b7schen\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.34": {"text": "Und schlecht sind die Menschen!", "tokens": ["Und", "schlecht", "sind", "die", "Men\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Wer jagt dich in die Wildni\u00df,", "tokens": ["Wer", "jagt", "dich", "in", "die", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Ungl\u00fcckselige?", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ge", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+----", "measure": "dactylic.init"}, "line.37": {"text": "Wer mich jagt in die Wildni\u00df,", "tokens": ["Wer", "mich", "jagt", "in", "die", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Das ist Vater und Mutter.", "tokens": ["Das", "ist", "Va\u00b7ter", "und", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.39": {"text": "Ich h\u00fclflos herzlos Verlassene", "tokens": ["Ich", "h\u00fcl\u00b7flos", "herz\u00b7los", "Ver\u00b7las\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Von Vater und Mutter", "tokens": ["Von", "Va\u00b7ter", "und", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Und vom stolzen Geliebten!", "tokens": ["Und", "vom", "stol\u00b7zen", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.42": {"text": "So komme mit mir.", "tokens": ["So", "kom\u00b7me", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.43": {"text": "Ich will dich sch\u00fctzen,", "tokens": ["Ich", "will", "dich", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.44": {"text": "Ich will dich f\u00fchren", "tokens": ["Ich", "will", "dich", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Unter wirthliches Obdach.", "tokens": ["Un\u00b7ter", "wirth\u00b7li\u00b7ches", "Ob\u00b7dach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Denn Du bist eine Heilige,", "tokens": ["Denn", "Du", "bist", "ei\u00b7ne", "Hei\u00b7li\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Wenn Vater und Mutter", "tokens": ["Wenn", "Va\u00b7ter", "und", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Und ein Geliebter,", "tokens": ["Und", "ein", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.49": {"text": "Bei solchem Sturm dich jagen in die Wildni\u00df", "tokens": ["Bei", "sol\u00b7chem", "Sturm", "dich", "ja\u00b7gen", "in", "die", "Wild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Also sprach der Zigeuner.", "tokens": ["Al\u00b7so", "sprach", "der", "Zi\u00b7geu\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.51": {"text": "Bist du der Heiland?", "tokens": ["Bist", "du", "der", "Hei\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.52": {"text": "Fr\u00e4gt sie mit gro\u00dfen", "tokens": ["Fr\u00e4gt", "sie", "mit", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.53": {"text": "Strahlenden Augen.", "tokens": ["Strah\u00b7len\u00b7den", "Au\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "Ein Fremder bist du", "tokens": ["Ein", "Frem\u00b7der", "bist", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.55": {"text": "Und b\u00fcrdest dir Last auf,", "tokens": ["Und", "b\u00fcr\u00b7dest", "dir", "Last", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.56": {"text": "Unerquicklichste.", "tokens": ["Un\u00b7er\u00b7quick\u00b7lichs\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.57": {"text": "Ja ich bin der Heiland,", "tokens": ["Ja", "ich", "bin", "der", "Hei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.58": {"text": "Denn ewig lebt er", "tokens": ["Denn", "e\u00b7wig", "lebt", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.59": {"text": "Ewig ein Solcher.", "tokens": ["E\u00b7wig", "ein", "Sol\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.60": {"text": "Und wo du ihn suchest,", "tokens": ["Und", "wo", "du", "ihn", "su\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.61": {"text": "Da ist er.", "tokens": ["Da", "ist", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.62": {"text": "Niemals stirbt das Geschlecht aus", "tokens": ["Nie\u00b7mals", "stirbt", "das", "Ge\u00b7schlecht", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.63": {"text": "Der H\u00fclfreichen.", "tokens": ["Der", "H\u00fclf\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.64": {"text": "Und weh, wenn wir lebten", "tokens": ["Und", "weh", ",", "wenn", "wir", "leb\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.65": {"text": "Ohne diese Gewi\u00dfheit!", "tokens": ["Oh\u00b7ne", "die\u00b7se", "Ge\u00b7wi\u00df\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.66": {"text": "Entr\u00fcstet hast du,", "tokens": ["Ent\u00b7r\u00fcs\u00b7tet", "hast", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.67": {"text": "Schmerzreiche Mutter,", "tokens": ["Schmer\u00b7zrei\u00b7che", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.68": {"text": "Gl\u00e4ubige Christen durch menschliche That.", "tokens": ["Gl\u00e4u\u00b7bi\u00b7ge", "Chris\u00b7ten", "durch", "menschli\u00b7che", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.69": {"text": "Jetzt wo z\u00fcrnet der Himmel", "tokens": ["Jetzt", "wo", "z\u00fcr\u00b7net", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.70": {"text": "Und ihnen Angst ins Herz fri\u00dft,", "tokens": ["Und", "ih\u00b7nen", "Angst", "ins", "Herz", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Entschuldgen sich diese", "tokens": ["Ent\u00b7schuld\u00b7gen", "sich", "die\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "PDAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.72": {"text": "Mit des Gebetes Wollust \u2013", "tokens": ["Mit", "des", "Ge\u00b7be\u00b7tes", "Wol\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.73": {"text": "Aber drau\u00dfen im Sturmwind", "tokens": ["A\u00b7ber", "drau\u00b7\u00dfen", "im", "Sturm\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.74": {"text": "Irret ein Menschenkind,", "tokens": ["Ir\u00b7ret", "ein", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.75": {"text": "Dem sie fluchen ob menschlicher That.", "tokens": ["Dem", "sie", "flu\u00b7chen", "ob", "menschli\u00b7cher", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KOUS", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.76": {"text": "Ihnen war 