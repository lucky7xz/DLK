{"textgrid.poem.56636": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1. Dat Hus", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verst\u0119ken mank Eschen dar steit en l\u00fctt Hus,", "tokens": ["Ver\u00b7st\u0119ken", "mank", "E\u00b7schen", "dar", "steit", "en", "l\u00fctt", "Hus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da's Abends so sachen, dar r\u00f6hrt sik keen Mus,", "tokens": ["Da's", "A\u00b7bends", "so", "sa\u00b7chen", ",", "dar", "r\u00f6hrt", "sik", "ke\u00b7en", "Mus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVINF", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dar schint ut de Bl\u0153der en Licht rein so blank,", "tokens": ["Dar", "schint", "ut", "de", "Bl\u0153\u00b7der", "en", "Licht", "rein", "so", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "FM", "FM", "FM", "FM", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "En Olsche in L\u0153hnstohl, un Se op de Bank.", "tokens": ["En", "Ol\u00b7sche", "in", "L\u0153hn\u00b7stohl", ",", "un", "Se", "op", "de", "Bank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dat schint mi in Ogen, dat treckt mi in Sinn,", "tokens": ["Dat", "schint", "mi", "in", "O\u00b7gen", ",", "dat", "treckt", "mi", "in", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "APPR", "NN", "$,", "PAV", "VVFIN", "NE", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dat treckt mi in Schummern so heemli derhin,", "tokens": ["Dat", "treckt", "mi", "in", "Schum\u00b7mern", "so", "heem\u00b7li", "der\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So warm un so luri, weet s\u00fclm ni wos\u00fcck \u2013", "tokens": ["So", "warm", "un", "so", "lu\u00b7ri", ",", "weet", "s\u00fclm", "ni", "wo\u00b7s\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ik sta \u00fcnnert Finster un frei mi un kik.", "tokens": ["Ik", "sta", "\u00fcn\u00b7nert", "Fins\u00b7ter", "un", "frei", "mi", "un", "kik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Un sitt wi to snacken umt Licht oppe Bank,", "tokens": ["Un", "sitt", "wi", "to", "sna\u00b7cken", "umt", "Licht", "op\u00b7pe", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVINF", "VVFIN", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So schint mi \u0119r Backen noch eenmal so blank,", "tokens": ["So", "schint", "mi", "\u0119r", "Ba\u00b7cken", "noch", "e\u00b7en\u00b7mal", "so", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "FM", "FM", "NN", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Denn is dat so ruhi, denn r\u00f6hrt sik keen Mus:", "tokens": ["Denn", "is", "dat", "so", "ru\u00b7hi", ",", "denn", "r\u00f6hrt", "sik", "ke\u00b7en", "Mus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "ART", "ADV", "VVFIN", "$,", "KON", "VVFIN", "PPER", "VVFIN", "NE", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "O, kunn ik der bliben, un muss ni to Hus!", "tokens": ["O", ",", "kunn", "ik", "der", "bli\u00b7ben", ",", "un", "muss", "ni", "to", "Hus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "ART", "ADJA", "$,", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}