{"dta.poem.6543": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es legt sich deutlich hell und klar", "tokens": ["Es", "legt", "sich", "deut\u00b7lich", "hell", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eine leichte Probe dar,", "tokens": ["Durch", "ei\u00b7ne", "leich\u00b7te", "Pro\u00b7be", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wunderbar", "tokens": ["Wie", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das Licht sich mildere, sich breche,", "tokens": ["Das", "Licht", "sich", "mil\u00b7de\u00b7re", ",", "sich", "bre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJA", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich f\u00e4rbe, st\u00e4rck\u2019 und schw\u00e4che,", "tokens": ["Sich", "f\u00e4r\u00b7be", ",", "st\u00e4rck", "und", "schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie bringt von dem, was eine Farbe sey", "tokens": ["Sie", "bringt", "von", "dem", ",", "was", "ei\u00b7ne", "Far\u00b7be", "sey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "$,", "PRELS", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Uns die Empfindung bey.", "tokens": ["Uns", "die", "Emp\u00b7fin\u00b7dung", "bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wir sehen, da\u00df des Lichtes Schein", "tokens": ["Wir", "se\u00b7hen", ",", "da\u00df", "des", "Lich\u00b7tes", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erzeuge Roth, und Gelb, und Blau", "tokens": ["Er\u00b7zeu\u00b7ge", "Roth", ",", "und", "Gelb", ",", "und", "Blau"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}