{"textgrid.poem.41616": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir weihe ich mein Lied, da\u00df, wenn zum blassen Strand", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir weihe ich mein Lied, da\u00df, wenn zum blassen Strand", "tokens": ["Dir", "wei\u00b7he", "ich", "mein", "Lied", ",", "da\u00df", ",", "wenn", "zum", "blas\u00b7sen", "Strand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der fernsten Zeiten sich mein Name einst gefunden", "tokens": ["Der", "ferns\u00b7ten", "Zei\u00b7ten", "sich", "mein", "Na\u00b7me", "einst", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Menschen tr\u00e4umen macht in abendlichen Stunden,", "tokens": ["Und", "Men\u00b7schen", "tr\u00e4u\u00b7men", "macht", "in", "a\u00b7bend\u00b7li\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Schiff, vom gro\u00dfen Wehn des Nords dahingesandt,", "tokens": ["Ein", "Schiff", ",", "vom", "gro\u00b7\u00dfen", "Wehn", "des", "Nords", "da\u00b7hin\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Angedenken gleich verblichnen Fabelkunden,", "tokens": ["Dein", "An\u00b7ge\u00b7den\u00b7ken", "gleich", "ver\u00b7blich\u00b7nen", "Fa\u00b7bel\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie einer Trommel Klang, den m\u00fcden Leser bannt,", "tokens": ["Wie", "ei\u00b7ner", "Trom\u00b7mel", "Klang", ",", "den", "m\u00fc\u00b7den", "Le\u00b7ser", "bannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch ein geheimnisvoll und br\u00fcderliches Band", "tokens": ["Durch", "ein", "ge\u00b7heim\u00b7nis\u00b7voll", "und", "br\u00fc\u00b7der\u00b7li\u00b7ches", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An meinen stolzen Reim auf immerdar gebunden;", "tokens": ["An", "mei\u00b7nen", "stol\u00b7zen", "Reim", "auf", "im\u00b7mer\u00b7dar", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verworfner Geist, zu dem vom h\u00f6chsten Lichtrevier", "tokens": ["Ver\u00b7worf\u00b7ner", "Geist", ",", "zu", "dem", "vom", "h\u00f6chs\u00b7ten", "Licht\u00b7re\u00b7vier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis in die tiefste Nacht nichts redet au\u00dfer mir!", "tokens": ["Bis", "in", "die", "tiefs\u00b7te", "Nacht", "nichts", "re\u00b7det", "au\u00b7\u00dfer", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O du, der schattengleich, mit Spuren, die verflie\u00dfen,", "tokens": ["O", "du", ",", "der", "schat\u00b7ten\u00b7gleich", ",", "mit", "Spu\u00b7ren", ",", "die", "ver\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADJD", "$,", "APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Leichtf\u00fc\u00dfig niedertrittst, im Blicke hellen Schein,", "tokens": ["Leicht\u00b7f\u00fc\u00b7\u00dfig", "nie\u00b7der\u00b7trittst", ",", "im", "Bli\u00b7cke", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die stumpfen Menschen, die im Groll dich bitter hie\u00dfen,", "tokens": ["Die", "stump\u00b7fen", "Men\u00b7schen", ",", "die", "im", "Groll", "dich", "bit\u00b7ter", "hie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gesch\u00f6pf mit ehrner Stirn und Augen von Gestein!", "tokens": ["Ge\u00b7sch\u00f6pf", "mit", "ehr\u00b7ner", "Stirn", "und", "Au\u00b7gen", "von", "Ge\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}