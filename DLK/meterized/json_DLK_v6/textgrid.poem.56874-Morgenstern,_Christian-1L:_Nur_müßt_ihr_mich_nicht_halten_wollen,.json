{"textgrid.poem.56874": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur m\u00fc\u00dft ihr mich nicht halten wollen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur m\u00fc\u00dft ihr mich nicht halten wollen,", "tokens": ["Nur", "m\u00fc\u00dft", "ihr", "mich", "nicht", "hal\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wenn die Rosse der Phantasie", "tokens": ["wenn", "die", "Ros\u00b7se", "der", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "vor meiner Gei\u00dfel dahinrasen!", "tokens": ["vor", "mei\u00b7ner", "Gei\u00b7\u00dfel", "da\u00b7hin\u00b7ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wehe dem Schurken,", "tokens": ["We\u00b7he", "dem", "Schur\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "der mir in die Z\u00fcgel f\u00e4llt, \u2013", "tokens": ["der", "mir", "in", "die", "Z\u00fc\u00b7gel", "f\u00e4llt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "siebenmal schleif ich ihn", "tokens": ["sie\u00b7ben\u00b7mal", "schleif", "ich", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "um den Bezirk", "tokens": ["um", "den", "Be\u00b7zirk"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "meiner Welt.", "tokens": ["mei\u00b7ner", "Welt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wehe vor allem dem Rezensenten,", "tokens": ["We\u00b7he", "vor", "al\u00b7lem", "dem", "Re\u00b7zen\u00b7sen\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "der mir", "tokens": ["der", "mir"], "token_info": ["word", "word"], "pos": ["ART", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "mit h\u00f6chst ungriechischem Feuer", "tokens": ["mit", "h\u00f6chst", "un\u00b7grie\u00b7chi\u00b7schem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "den Weg bedr\u00e4ut.", "tokens": ["den", "Weg", "be\u00b7dr\u00e4ut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Meine Peitsche ist l\u00e4nger noch", "tokens": ["Mei\u00b7ne", "Peit\u00b7sche", "ist", "l\u00e4n\u00b7ger", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "als seine Ohren,", "tokens": ["als", "sei\u00b7ne", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "von st\u00e4rkerem Leder", "tokens": ["von", "st\u00e4r\u00b7ke\u00b7rem", "Le\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "als seine Hirnhaut,", "tokens": ["als", "sei\u00b7ne", "Hirn\u00b7haut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "die Schnur noch gespaltner", "tokens": ["die", "Schnur", "noch", "ge\u00b7spalt\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "als seine Zunge.", "tokens": ["als", "sei\u00b7ne", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Bahn frei!", "tokens": ["Bahn", "frei", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Kurz ist zur Fahrt die Zeit.", "tokens": ["Kurz", "ist", "zur", "Fahrt", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Springt mit herauf,", "tokens": ["Springt", "mit", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "wenns euch l\u00fcstet!", "tokens": ["wenns", "euch", "l\u00fcs\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Tausend gew\u00e4hr ich Platz,", "tokens": ["Tau\u00b7send", "ge\u00b7w\u00e4hr", "ich", "Platz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.24": {"text": "hier an den M\u00e4hnen,", "tokens": ["hier", "an", "den", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "hier an den Schweifen,", "tokens": ["hier", "an", "den", "Schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "hier auf den R\u00fccken der Rosse,", "tokens": ["hier", "auf", "den", "R\u00fc\u00b7cken", "der", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "und hier oben bei mir", "tokens": ["und", "hier", "o\u00b7ben", "bei", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.28": {"text": "auf dem Wagen", "tokens": ["auf", "dem", "Wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "weiteren tausend.", "tokens": ["wei\u00b7te\u00b7ren", "tau\u00b7send", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "CARD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Herauf, Freunde!", "tokens": ["Her\u00b7auf", ",", "Freun\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Sturm um die Stirn,", "tokens": ["Sturm", "um", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.32": {"text": "Sonnen im Aug,", "tokens": ["Son\u00b7nen", "im", "Aug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "so la\u00dft uns jauchzend", "tokens": ["so", "la\u00dft", "uns", "jauch\u00b7zend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "die tausendundein Weltwege", "tokens": ["die", "tau\u00b7sen\u00b7dund\u00b7ein", "Welt\u00b7we\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.35": {"text": "durchbrausen.", "tokens": ["durch\u00b7brau\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}