{"textgrid.poem.26320": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mein Ohr horcht hin auf jeden Schritt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4ume laut im Dunkeln rauschen,", "tokens": ["Die", "B\u00e4u\u00b7me", "laut", "im", "Dun\u00b7keln", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind nimmt mich zur Ferne mit.", "tokens": ["Der", "Wind", "nimmt", "mich", "zur", "Fer\u00b7ne", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df noch nachts der Sehnsucht lauschen,", "tokens": ["Ich", "mu\u00df", "noch", "nachts", "der", "Sehn\u00b7sucht", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Ohr horcht hin auf jeden Schritt.", "tokens": ["Mein", "Ohr", "horcht", "hin", "auf", "je\u00b7den", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sehnsucht ist es nie genug.", "tokens": ["Der", "Sehn\u00b7sucht", "ist", "es", "nie", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume reden schnell im Winde.", "tokens": ["Die", "B\u00e4u\u00b7me", "re\u00b7den", "schnell", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schmerzen, die ich t\u00e4glich trug,", "tokens": ["Und", "Schmer\u00b7zen", ",", "die", "ich", "t\u00e4g\u00b7lich", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich nachts noch sp\u00e4t am Wege finde.", "tokens": ["Ich", "nachts", "noch", "sp\u00e4t", "am", "We\u00b7ge", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}