{"dta.poem.14284": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ich ruht' \u00fcberwoben,", "tokens": ["Und", "ich", "ruht'", "\u00fc\u00b7ber\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du s\u00e4ngest immerzu,", "tokens": ["Du", "s\u00e4n\u00b7gest", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Linde sch\u00fcttelt oben", "tokens": ["Die", "Lin\u00b7de", "sch\u00fct\u00b7telt", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Laub und deckt mich zu.", "tokens": ["Ihr", "Laub", "und", "deckt", "mich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}