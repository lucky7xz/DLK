{"dta.poem.11916": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst sprach ein frommer Mann, der stets im Geiste lebte,", "tokens": ["Einst", "sprach", "ein", "from\u00b7mer", "Mann", ",", "der", "stets", "im", "Geis\u00b7te", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in der Nacht bald rang und bald im Lichte schwebte:", "tokens": ["Der", "in", "der", "Nacht", "bald", "rang", "und", "bald", "im", "Lich\u00b7te", "schweb\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Herr, wenn dir immer so, wie mir zuweilen, ist,", "tokens": ["Herr", ",", "wenn", "dir", "im\u00b7mer", "so", ",", "wie", "mir", "zu\u00b7wei\u00b7len", ",", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "ADV", "$,", "VAFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn in dir selber du so s\u00fc\u00df als in mir bist;", "tokens": ["Wenn", "in", "dir", "sel\u00b7ber", "du", "so", "s\u00fc\u00df", "als", "in", "mir", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "PPER", "ADV", "ADJD", "KOKOM", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So f\u00fchrest du ein alzuwonnigliches Leben.", "tokens": ["So", "f\u00fch\u00b7rest", "du", "ein", "al\u00b7zu\u00b7won\u00b7nig\u00b7li\u00b7ches", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darauf hat Gott in ihm die Antwort ihm gegeben:", "tokens": ["Da\u00b7rauf", "hat", "Gott", "in", "ihm", "die", "Ant\u00b7wort", "ihm", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "APPR", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "W\u00e4rst du im Kampfe nicht, du schmecktest nicht den Frieden,", "tokens": ["W\u00e4rst", "du", "im", "Kamp\u00b7fe", "nicht", ",", "du", "schmeck\u00b7test", "nicht", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch S\u00fc\u00dfes, w\u00e4re nicht auch Bittres dir beschieden.", "tokens": ["Noch", "S\u00fc\u00b7\u00dfes", ",", "w\u00e4\u00b7re", "nicht", "auch", "Bitt\u00b7res", "dir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich bin die Seligkeit, und f\u00fchlte selbst in mir", "tokens": ["Ich", "bin", "die", "Se\u00b7lig\u00b7keit", ",", "und", "f\u00fchl\u00b7te", "selbst", "in", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seligwerdung nicht, f\u00fchlt' ich sie nicht in dir.", "tokens": ["Die", "Se\u00b7lig\u00b7wer\u00b7dung", "nicht", ",", "f\u00fchlt'", "ich", "sie", "nicht", "in", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}