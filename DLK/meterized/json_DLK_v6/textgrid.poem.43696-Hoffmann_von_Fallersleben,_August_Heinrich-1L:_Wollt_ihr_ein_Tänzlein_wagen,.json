{"textgrid.poem.43696": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wollt ihr ein T\u00e4nzlein wagen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr ein T\u00e4nzlein wagen,", "tokens": ["Wollt", "ihr", "ein", "T\u00e4nz\u00b7lein", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr d\u00fcrft's nur sagen,", "tokens": ["Ihr", "d\u00fcrft's", "nur", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich spiel' euch auf.", "tokens": ["Ich", "spiel'", "euch", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Im Walde drau\u00dfen im Buchenschatten", "tokens": ["Im", "Wal\u00b7de", "drau\u00b7\u00dfen", "im", "Bu\u00b7chen\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf gr\u00fcnen Matten", "tokens": ["Auf", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Tanzt es sich gut.", "tokens": ["Tanzt", "es", "sich", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Werd' ich zum Tanze den Takt euch schlagen,", "tokens": ["Werd'", "ich", "zum", "Tan\u00b7ze", "den", "Takt", "euch", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann sollt ihr sagen:", "tokens": ["Dann", "sollt", "ihr", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Der Kuckuck kann's!", "tokens": ["Der", "Ku\u00b7ckuck", "kann's", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kommt, dreht euch alle im Kreise!", "tokens": ["Kommt", ",", "dreht", "euch", "al\u00b7le", "im", "Krei\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach meiner Weise", "tokens": ["Nach", "mei\u00b7ner", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dreht euch herum!", "tokens": ["Dreht", "euch", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Weil ich ber\u00fchmet als Musikant bin", "tokens": ["Weil", "ich", "be\u00b7r\u00fch\u00b7met", "als", "Mu\u00b7si\u00b7kant", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KOUS", "NN", "VAFIN"], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Durch jedes Land hin,", "tokens": ["Durch", "je\u00b7des", "Land", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "D\u00fcrft ihr mir trau'n!", "tokens": ["D\u00fcrft", "ihr", "mir", "trau'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Werd' ich zum Tanze den Takt euch schlagen,", "tokens": ["Werd'", "ich", "zum", "Tan\u00b7ze", "den", "Takt", "euch", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann sollt ihr sagen:", "tokens": ["Dann", "sollt", "ihr", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Der Kuckuck kann's!", "tokens": ["Der", "Ku\u00b7ckuck", "kann's", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}