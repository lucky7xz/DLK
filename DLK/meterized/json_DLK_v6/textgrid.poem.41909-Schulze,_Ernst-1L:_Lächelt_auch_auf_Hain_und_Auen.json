{"textgrid.poem.41909": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4chelt auch auf Hain und Auen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4chelt auch auf Hain und Auen", "tokens": ["L\u00e4\u00b7chelt", "auch", "auf", "Hain", "und", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hold dein lieblich laues Licht,", "tokens": ["Hold", "dein", "lieb\u00b7lich", "lau\u00b7es", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchling, ach, wer darf dir trauen", "tokens": ["Fr\u00fch\u00b7ling", ",", "ach", ",", "wer", "darf", "dir", "trau\u00b7en"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ITJ", "$,", "PWS", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dein freundlich Angesicht,", "tokens": ["Auf", "dein", "freund\u00b7lich", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn dich Thr\u00e4nen doch befeuchten,", "tokens": ["Wenn", "dich", "Thr\u00e4\u00b7nen", "doch", "be\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das traurende Gem\u00fcth", "tokens": ["Und", "das", "trau\u00b7ren\u00b7de", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Finstrer nur in deinem Leuchten", "tokens": ["Finst\u00b7rer", "nur", "in", "dei\u00b7nem", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen eignen Kummer sieht!", "tokens": ["Sei\u00b7nen", "eig\u00b7nen", "Kum\u00b7mer", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wohl ist's ein banges Sehnen,", "tokens": ["Ach", ",", "wohl", "ist's", "ein", "ban\u00b7ges", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was verlangend in dir schwillt,", "tokens": ["Was", "ver\u00b7lan\u00b7gend", "in", "dir", "schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von s\u00fc\u00dfen Wehmuthsthr\u00e4nen", "tokens": ["Und", "von", "s\u00fc\u00b7\u00dfen", "Weh\u00b7muthst\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Bl\u00fcthen wohl erf\u00fcllt;", "tokens": ["Sind", "die", "Bl\u00fc\u00b7then", "wohl", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch verkl\u00e4rt vom Liebesglanze,", "tokens": ["Doch", "ver\u00b7kl\u00e4rt", "vom", "Lie\u00b7bes\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Hoffnung lichtem Gr\u00fcn,", "tokens": ["Von", "der", "Hoff\u00b7nung", "lich\u00b7tem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollt' in deinem Blumenkranze", "tokens": ["Sollt'", "in", "dei\u00b7nem", "Blu\u00b7men\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst die Trauer lieblich bl\u00fchn.", "tokens": ["Selbst", "die", "Trau\u00b7er", "lieb\u00b7lich", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vieles mu\u00df das Herz ertragen", "tokens": ["Vie\u00b7les", "mu\u00df", "das", "Herz", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey des Zufalls blindem Spiel;", "tokens": ["Bey", "des", "Zu\u00b7falls", "blin\u00b7dem", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, selbst in begl\u00fcckten Tagen", "tokens": ["Ach", ",", "selbst", "in", "be\u00b7gl\u00fcck\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat es, Thr\u00e4nen nie zu viel!", "tokens": ["Hat", "es", ",", "Thr\u00e4\u00b7nen", "nie", "zu", "viel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "ADV", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den gold'nen Wolkens\u00e4umen", "tokens": ["In", "den", "gold'\u00b7nen", "Wol\u00b7ken\u00b7s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauscht das dunkle Wetter gern;", "tokens": ["Lauscht", "das", "dunk\u00b7le", "Wet\u00b7ter", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hoffen darfst du nur und tr\u00e4umen,", "tokens": ["Hof\u00b7fen", "darfst", "du", "nur", "und", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn das Gl\u00fcck bleibt ewig fern.", "tokens": ["Denn", "das", "Gl\u00fcck", "bleibt", "e\u00b7wig", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber auch wenn oft hienieden", "tokens": ["A\u00b7ber", "auch", "wenn", "oft", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Tr\u00e4ume schnell verwehn,", "tokens": ["Uns\u00b7re", "Tr\u00e4u\u00b7me", "schnell", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward die Treu' uns nur beschieden,", "tokens": ["Ward", "die", "Treu'", "uns", "nur", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag die Freude gern vergehn.", "tokens": ["Mag", "die", "Freu\u00b7de", "gern", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Treue h\u00e4lt mit starken Ketten,", "tokens": ["Treu\u00b7e", "h\u00e4lt", "mit", "star\u00b7ken", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was uns fl\u00fcchtig einst erfreut;", "tokens": ["Was", "uns", "fl\u00fcch\u00b7tig", "einst", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kannst du auch die Lust nicht retten,", "tokens": ["Kannst", "du", "auch", "die", "Lust", "nicht", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00fc\u00df ist um die Lust das Leid.", "tokens": ["S\u00fc\u00df", "ist", "um", "die", "Lust", "das", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}