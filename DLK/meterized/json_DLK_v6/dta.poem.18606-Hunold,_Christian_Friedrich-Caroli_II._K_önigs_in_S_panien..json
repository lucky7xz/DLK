{"dta.poem.18606": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Caroli II.   K \u00f6nigs in  S panien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kind und ein Monarch liegt in der Grufft begraben/", "tokens": ["Ein", "Kind", "und", "ein", "Mon\u00b7arch", "liegt", "in", "der", "Grufft", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der in den Leben weint/ und nach den Tode brennt.", "tokens": ["Der", "in", "den", "Le\u00b7ben", "weint", "/", "und", "nach", "den", "To\u00b7de", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Mars und Venus nicht zum Sohne kondten haben/", "tokens": ["Den", "Mars", "und", "Ve\u00b7nus", "nicht", "zum", "Soh\u00b7ne", "kond\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "APPRART", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der nichts mehr gemacht/ als nur ein Testament;", "tokens": ["Und", "der", "nichts", "mehr", "ge\u00b7macht", "/", "als", "nur", "ein", "Tes\u00b7ta\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADV", "VVPP", "$(", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der/ wer nicht Erben soll/ zum Erben eingesetzet.", "tokens": ["Der", "/", "wer", "nicht", "Er\u00b7ben", "soll", "/", "zum", "Er\u00b7ben", "ein\u00b7ge\u00b7set\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PTKNEG", "NN", "VMFIN", "$(", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schlieset jenen aus/ der Erbe solte seyn/", "tokens": ["Und", "schlie\u00b7set", "je\u00b7nen", "aus", "/", "der", "Er\u00b7be", "sol\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKVZ", "$(", "ART", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Warum? Die weil man Carln vor unverm\u00f6gend sch\u00e4tzet/", "tokens": ["Wa\u00b7rum", "?", "Die", "weil", "man", "Carln", "vor", "un\u00b7ver\u00b7m\u00f6\u00b7gend", "sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "KOUS", "PIS", "NN", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So stimmet auch sein Mu\u00df mit Franckreichs Willen ein.", "tokens": ["So", "stim\u00b7met", "auch", "sein", "Mu\u00df", "mit", "Fran\u00b7ck\u00b7reichs", "Wil\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VAINF", "VMFIN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}