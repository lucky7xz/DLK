{"dta.poem.23638": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh in Frankreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sehn' ich mich nach deinen Bergen wieder,", "tokens": ["Wie", "sehn'", "ich", "mich", "nach", "dei\u00b7nen", "Ber\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach deinem Schatten, deinem Sonnenschein!", "tokens": ["Nach", "dei\u00b7nem", "Schat\u00b7ten", ",", "dei\u00b7nem", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach deutschen Herzen voller Sang und Lieder,", "tokens": ["Nach", "deut\u00b7schen", "Her\u00b7zen", "vol\u00b7ler", "Sang", "und", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach deutscher Freud' und Lust, nach deutschem Wein!", "tokens": ["Nach", "deut\u00b7scher", "Freud'", "und", "Lust", ",", "nach", "deut\u00b7schem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt' ich den Wolken meine H\u00e4nde reichen,", "tokens": ["K\u00f6nnt'", "ich", "den", "Wol\u00b7ken", "mei\u00b7ne", "H\u00e4n\u00b7de", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich fl\u00f6ge windesschnell zu dir hinein;", "tokens": ["Ich", "fl\u00f6\u00b7ge", "win\u00b7des\u00b7schnell", "zu", "dir", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6nnt' ich dem Adler und dem Lichtstrahl gleichen,", "tokens": ["K\u00f6nnt'", "ich", "dem", "Ad\u00b7ler", "und", "dem", "Licht\u00b7strahl", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Gedanke wollt' ich bei dir sein!", "tokens": ["Wie", "ein", "Ge\u00b7dan\u00b7ke", "wollt'", "ich", "bei", "dir", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Fremde macht mich still und ernst und traurig;", "tokens": ["Die", "Frem\u00b7de", "macht", "mich", "still", "und", "ernst", "und", "trau\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verk\u00fcmmern mu\u00df mein frisches junges Herz.", "tokens": ["Ver\u00b7k\u00fcm\u00b7mern", "mu\u00df", "mein", "fri\u00b7sches", "jun\u00b7ges", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben hier, wie ist es bang' und schaurig,", "tokens": ["Das", "Le\u00b7ben", "hier", ",", "wie", "ist", "es", "bang'", "und", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was es beut, ist nur der Sehnsucht Schmerz.", "tokens": ["Und", "was", "es", "beut", ",", "ist", "nur", "der", "Sehn\u00b7sucht", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Vaterland, und wenn ich nichts mehr habe,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "und", "wenn", "ich", "nichts", "mehr", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "KOUS", "PPER", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begleitet treu noch diese Sehnsucht mich;", "tokens": ["Be\u00b7glei\u00b7tet", "treu", "noch", "die\u00b7se", "Sehn\u00b7sucht", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PDAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00fcrde selbst die Fremde mir zum Grabe,", "tokens": ["Und", "w\u00fcr\u00b7de", "selbst", "die", "Frem\u00b7de", "mir", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gern sterb' ich, denn ich lebte nur f\u00fcr dich.", "tokens": ["Gern", "sterb'", "ich", ",", "denn", "ich", "leb\u00b7te", "nur", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}