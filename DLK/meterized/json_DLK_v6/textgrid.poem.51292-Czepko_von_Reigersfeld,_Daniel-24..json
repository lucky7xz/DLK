{"textgrid.poem.51292": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie das Wa\u00dfer bald den Wirbel in sich schluckt,", "tokens": ["Gleich\u00b7wie", "das", "Wa\u00b7\u00dfer", "bald", "den", "Wir\u00b7bel", "in", "sich", "schluckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den aus gelenckter Hand ein blatter Stein gedruckt,", "tokens": ["Den", "aus", "ge\u00b7lenck\u00b7ter", "Hand", "ein", "blat\u00b7ter", "Stein", "ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie durch ihr Schwellen man die Blase sieht zerspringen,", "tokens": ["Wie", "durch", "ihr", "Schwel\u00b7len", "man", "die", "Bla\u00b7se", "sieht", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "PIS", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann Sie die leichten Wind' aus gelben R\u00f6hren bringen,", "tokens": ["Wann", "Sie", "die", "leich\u00b7ten", "Wind'", "aus", "gel\u00b7ben", "R\u00f6h\u00b7ren", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gleichwie die na\u00dfe Furch' an einem Blicke klebt,", "tokens": ["Gleich\u00b7wie", "die", "na\u00b7\u00dfe", "Furch'", "an", "ei\u00b7nem", "Bli\u00b7cke", "klebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die in das blaue Saltz der hole Balcke gr\u00e4bt:", "tokens": ["Die", "in", "das", "blau\u00b7e", "Saltz", "der", "ho\u00b7le", "Bal\u00b7cke", "gr\u00e4bt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie sich zusammenrollt ein leichter Dampff im Lentzen,", "tokens": ["Wie", "sich", "zu\u00b7sam\u00b7men\u00b7rollt", "ein", "leich\u00b7ter", "Dampff", "im", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann er die Sonne sieht umb die Gebirge gl\u00e4ntzen.", "tokens": ["Wann", "er", "die", "Son\u00b7ne", "sieht", "umb", "die", "Ge\u00b7bir\u00b7ge", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleichwie die f\u00fcge Lufft, wann sie der Adler theilt,", "tokens": ["Gleich\u00b7wie", "die", "f\u00fc\u00b7ge", "Lufft", ",", "wann", "sie", "der", "Ad\u00b7ler", "theilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch seiner Fl\u00fcgelmacht zusammen fl\u00fcchtig eilt:", "tokens": ["Durch", "sei\u00b7ner", "Fl\u00fc\u00b7gel\u00b7macht", "zu\u00b7sam\u00b7men", "fl\u00fcch\u00b7tig", "eilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So ist das Gl\u00fccke nechst bey mir vorbey gezogen,", "tokens": ["So", "ist", "das", "Gl\u00fc\u00b7cke", "nechst", "bey", "mir", "vor\u00b7bey", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Indem ich nach ihm grieff, so war es fort geflogen.", "tokens": ["In\u00b7dem", "ich", "nach", "ihm", "grieff", ",", "so", "war", "es", "fort", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O da\u00df es mir doch br\u00e4cht' es sey auch, wo es sey,", "tokens": ["O", "da\u00df", "es", "mir", "doch", "br\u00e4cht'", "es", "sey", "auch", ",", "wo", "es", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Stracks Wirbel, Blase, Dampff, Balck, Adler wieder bey.", "tokens": ["Stracks", "Wir\u00b7bel", ",", "Bla\u00b7se", ",", "Dampff", ",", "Balck", ",", "Ad\u00b7ler", "wie\u00b7der", "bey", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}