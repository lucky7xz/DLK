{"textgrid.poem.67564": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blitzegeborner Bacchus,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blitzegeborner Bacchus,", "tokens": ["Blit\u00b7ze\u00b7ge\u00b7bor\u00b7ner", "Bac\u00b7chus", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Im Feuer gezeugt, an Quellen erzogen,", "tokens": ["Im", "Feu\u00b7er", "ge\u00b7zeugt", ",", "an", "Quel\u00b7len", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Nymphen gepflegt, hast Nymphen geliebet,", "tokens": ["Von", "Nym\u00b7phen", "ge\u00b7pflegt", ",", "hast", "Nym\u00b7phen", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Blitzegeborener Bacchus!", "tokens": ["Blit\u00b7ze\u00b7ge\u00b7bo\u00b7re\u00b7ner", "Bac\u00b7chus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "So will ich denn auch unschuldiger Nymphe", "tokens": ["So", "will", "ich", "denn", "auch", "un\u00b7schul\u00b7di\u00b7ger", "Nym\u00b7phe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dich freundlich verm\u00e4hlen.", "tokens": ["Dich", "freund\u00b7lich", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}