{"textgrid.poem.38912": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin rennt ihr, liebe Wogen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin rennt ihr, liebe Wogen,", "tokens": ["Wo\u00b7hin", "rennt", "ihr", ",", "lie\u00b7be", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Uebereilt euch im Gedr\u00e4nge,", "tokens": ["Ue\u00b7be\u00b7reilt", "euch", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohin wird die volle Menge", "tokens": ["Wo\u00b7hin", "wird", "die", "vol\u00b7le", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dieser Wellen denn gezogen? \u2013", "tokens": ["Die\u00b7ser", "Wel\u00b7len", "denn", "ge\u00b7zo\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "KON", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbquellen haben uns erzogen", "tokens": ["\u00bb", "quel\u00b7len", "ha\u00b7ben", "uns", "er\u00b7zo\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVINF", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Strom hat uns genommen,", "tokens": ["Und", "der", "Strom", "hat", "uns", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Etwas haben wir vernommen", "tokens": ["Et\u00b7was", "ha\u00b7ben", "wir", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von den heil'gen Meerestiefen,", "tokens": ["Von", "den", "heil'\u00b7gen", "Mee\u00b7res\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo uralte Wunder schliefen,", "tokens": ["Wo", "ur\u00b7al\u00b7te", "Wun\u00b7der", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "W\u00e4ren wir dort angekommen!\u00ab", "tokens": ["W\u00e4\u00b7ren", "wir", "dort", "an\u00b7ge\u00b7kom\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohin Seufzer, Liebesblicke?", "tokens": ["Wo\u00b7hin", "Seuf\u00b7zer", ",", "Lie\u00b7bes\u00b7bli\u00b7cke", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohin aus der rothen Pforte", "tokens": ["Wo\u00b7hin", "aus", "der", "ro\u00b7then", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zartgefl\u00fcgelt Liebesworte? \u2013", "tokens": ["Zart\u00b7ge\u00b7fl\u00fc\u00b7gelt", "Lie\u00b7bes\u00b7wor\u00b7te", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbkeiner halte uns zur\u00fccke,", "tokens": ["\u00bb", "kei\u00b7ner", "hal\u00b7te", "uns", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, es giebt ein ewig Gl\u00fccke", "tokens": ["Ach", ",", "es", "giebt", "ein", "e\u00b7wig", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unergr\u00fcndlich, aus dem Bronnen", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7lich", ",", "aus", "dem", "Bron\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sprangen Sterne, Mond und Sonnen,", "tokens": ["Spran\u00b7gen", "Ster\u00b7ne", ",", "Mond", "und", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses sehnende Verlangen", "tokens": ["Die\u00b7ses", "seh\u00b7nen\u00b7de", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hat vom Liebesgeist empfangen,", "tokens": ["Hat", "vom", "Lie\u00b7bes\u00b7geist", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Welt als Kind gewonnen.\u00ab", "tokens": ["Und", "die", "Welt", "als", "Kind", "ge\u00b7won\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "KOUS", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}