{"textgrid.poem.45049": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fragt ihr mich, was das ", "tokens": ["Fragt", "ihr", "mich", ",", "was", "das"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PRELS", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Seht zu, ob ichs verfehle!", "tokens": ["Seht", "zu", ",", "ob", "ichs", "ver\u00b7feh\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gleichnis beut die ", "tokens": ["Ein", "Gleich\u00b7nis", "beut", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Es geht vom K\u00f6rper aus, gleich ihr,", "tokens": ["Es", "geht", "vom", "K\u00f6r\u00b7per", "aus", ",", "gleich", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und endigt in der Seele.", "tokens": ["Und", "en\u00b7digt", "in", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}