{"textgrid.poem.46214": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist, weltwerter held, w\u00fcrdiglich hochgeachtet", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist, weltwerter held, w\u00fcrdiglich hochgeachtet", "tokens": ["Du", "bist", ",", "welt\u00b7wer\u00b7ter", "held", ",", "w\u00fcr\u00b7dig\u00b7lich", "hoch\u00b7ge\u00b7ach\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "VVFIN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der wahren dapferkeit ein wahrer erb und sohn;", "tokens": ["der", "wah\u00b7ren", "dap\u00b7fer\u00b7keit", "ein", "wah\u00b7rer", "erb", "und", "sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du bist die blum, der ruhm der teutschen nation,", "tokens": ["du", "bist", "die", "blum", ",", "der", "ruhm", "der", "teut\u00b7schen", "na\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nur von dem, der got selbs verachten darf, verachtet.", "tokens": ["nur", "von", "dem", ",", "der", "got", "selbs", "ver\u00b7ach\u00b7ten", "darf", ",", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "$,", "ART", "NN", "ADV", "VVFIN", "VMFIN", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Dein schwert, so wie der strahl und des tods s\u00e4gis, schlachtet", "tokens": ["Dein", "schwert", ",", "so", "wie", "der", "strahl", "und", "des", "tods", "s\u00e4\u00b7gis", ",", "schlach\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "VVFIN", "$,", "ADV", "KOKOM", "ART", "NN", "KON", "ART", "FM", "FM", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "was dir zuwider ist, ist der gotlosen lohn,", "tokens": ["was", "dir", "zu\u00b7wi\u00b7der", "ist", ",", "ist", "der", "got\u00b7lo\u00b7sen", "lohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der angefochtnen trost und der vertribnen wohn,", "tokens": ["der", "an\u00b7ge\u00b7focht\u00b7nen", "trost", "und", "der", "ver\u00b7trib\u00b7nen", "wohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ja, auch der pracht des reichs, das sunst aus scham verschmachtet.", "tokens": ["ja", ",", "auch", "der", "pracht", "des", "reichs", ",", "das", "sunst", "aus", "scham", "ver\u00b7schmach\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ART", "ADJA", "$,", "PRELS", "ADV", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wan C\u00e4sar sehen solt von deinen streichen schwer", "tokens": ["Wan", "C\u00e4\u00b7sar", "se\u00b7hen", "solt", "von", "dei\u00b7nen", "strei\u00b7chen", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVINF", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "die Donau und den Rhein so oft in scharlach flie\u00dfen,", "tokens": ["die", "Do\u00b7nau", "und", "den", "Rhein", "so", "oft", "in", "schar\u00b7lach", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NE", "ADV", "ADV", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und deinen geist, herz, mund so reich an ehr, macht, lehr;", "tokens": ["und", "dei\u00b7nen", "geist", ",", "herz", ",", "mund", "so", "reich", "an", "ehr", ",", "macht", ",", "lehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$,", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "W\u00fcrd er, sich neigend bald zu seines sigers f\u00fc\u00dfen,", "tokens": ["W\u00fcrd", "er", ",", "sich", "nei\u00b7gend", "bald", "zu", "sei\u00b7nes", "si\u00b7gers", "f\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "bekennen, da\u00df dir, held, nichts kaiserliches mehr", "tokens": ["be\u00b7ken\u00b7nen", ",", "da\u00df", "dir", ",", "held", ",", "nichts", "kai\u00b7ser\u00b7li\u00b7ches", "mehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "$,", "VVFIN", "$,", "PIS", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ermanglet dan der nam, und dich selbs C\u00e4sar gr\u00fc\u00dfen.", "tokens": ["er\u00b7mang\u00b7let", "dan", "der", "nam", ",", "und", "dich", "selbs", "C\u00e4\u00b7sar", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVFIN", "$,", "KON", "PPER", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}