"], "pos": ["PDS", "$,", "ADV", "ADJD", "$,", "VVFIN", "PDS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hoch und niedre, und die letzten tragens", "tokens": ["Hoch", "und", "nied\u00b7re", ",", "und", "die", "letz\u00b7ten", "tra\u00b7gens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "PIS", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Himmelan.", "tokens": ["Him\u00b7me\u00b7lan", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Hell und rein, dann tief und voll ert\u00f6net", "tokens": ["Hell", "und", "rein", ",", "dann", "tief", "und", "voll", "er\u00b7t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Berg um Berg, bald wie Gesang des Mannes \u2013", "tokens": ["Berg", "um", "Berg", ",", "bald", "wie", "Ge\u00b7sang", "des", "Man\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "KOKOM", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dorther, wo ein Fels den Gipfel kr\u00f6net \u2013", "tokens": ["Dor\u00b7ther", ",", "wo", "ein", "Fels", "den", "Gip\u00b7fel", "kr\u00f6\u00b7net", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wieder bald wie Jungfraustimme, wann es", "tokens": ["Wie\u00b7der", "bald", "wie", "Jung\u00b7fraus\u00b7tim\u00b7me", ",", "wann", 