{"textgrid.poem.52644": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Dahner Thal", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem Himmel rauschet rascher Regen,", "tokens": ["Von", "dem", "Him\u00b7mel", "rau\u00b7schet", "ra\u00b7scher", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwer und tr\u00fcb durchstreicht der Wind die R\u00e4ume,", "tokens": ["Schwer", "und", "tr\u00fcb", "durch\u00b7streicht", "der", "Wind", "die", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nebel raubt des Lichtes goldnen Segen,", "tokens": ["Ne\u00b7bel", "raubt", "des", "Lich\u00b7tes", "gold\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um der Tannenberge dunkle S\u00e4ume", "tokens": ["Um", "der", "Tan\u00b7nen\u00b7ber\u00b7ge", "dunk\u00b7le", "S\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lagert Dunst.", "tokens": ["La\u00b7gert", "Dunst", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "F\u00fcrder schritt ich, \u00f6fter wars ein Waten,", "tokens": ["F\u00fcr\u00b7der", "schritt", "ich", ",", "\u00f6f\u00b7ter", "wars", "ein", "Wa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem losen Wege, \u00fcber Tr\u00fcmmer;", "tokens": ["In", "dem", "lo\u00b7sen", "We\u00b7ge", ",", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hingeschmettert sah ich hohe Saaten,", "tokens": ["Hin\u00b7ge\u00b7schmet\u00b7tert", "sah", "ich", "ho\u00b7he", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Felsen draufgerollet; steiler immer", "tokens": ["Fel\u00b7sen", "drauf\u00b7ge\u00b7rol\u00b7let", ";", "stei\u00b7ler", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "VVFIN", "ADV"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ward der Berg.", "tokens": ["Ward", "der", "Berg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Welchem Zauber ist das Thal erlegen?", "tokens": ["Wel\u00b7chem", "Zau\u00b7ber", "ist", "das", "Thal", "er\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welcher Bann hat seinen Reiz getroffen?", "tokens": ["Wel\u00b7cher", "Bann", "hat", "sei\u00b7nen", "Reiz", "ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Herz erbebt in Wonneschl\u00e4gen", "tokens": ["Wie", "ein", "Herz", "er\u00b7bebt", "in", "Won\u00b7ne\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bebte sonst in tr\u00e4umerischem Hoffen", "tokens": ["Beb\u00b7te", "sonst", "in", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hier Natur.", "tokens": ["Hier", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Haine t\u00f6nten, s\u00fc\u00dfe Schlummerfr\u00fche", "tokens": ["Hai\u00b7ne", "t\u00f6n\u00b7ten", ",", "s\u00fc\u00b7\u00dfe", "Schlum\u00b7mer\u00b7fr\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War gebreitet \u00fcber Berg und Auen,", "tokens": ["War", "ge\u00b7brei\u00b7tet", "\u00fc\u00b7ber", "Berg", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eh die Sonne hei\u00dfe Tagesm\u00fche", "tokens": ["Eh", "die", "Son\u00b7ne", "hei\u00b7\u00dfe", "Ta\u00b7ges\u00b7m\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Land und Leuten brachte unterm blauen", "tokens": ["Land", "und", "Leu\u00b7ten", "brach\u00b7te", "un\u00b7term", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Himmelszelt.", "tokens": ["Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Heute schweigt der einst so heitre Morgen,", "tokens": ["Heu\u00b7te", "schweigt", "der", "einst", "so", "heit\u00b7re", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wolken beugen um die feuchten H\u00fcgel,", "tokens": ["Wol\u00b7ken", "beu\u00b7gen", "um", "die", "feuch\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und Gesichter voll Verdru\u00df und Sorgen", "tokens": ["Und", "Ge\u00b7sich\u00b7ter", "voll", "Ver\u00b7dru\u00df", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schneiden sie herunter in den Spiegel", "tokens": ["Schnei\u00b7den", "sie", "her\u00b7un\u00b7ter", "in", "den", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Seichter Seen.", "tokens": ["Seich\u00b7ter", "Seen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Trauerselig von dem Walle nicken", "tokens": ["Trau\u00b7er\u00b7se\u00b7lig", "von", "dem", "Wal\u00b7le", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ritterburgen, alter Herrlichkeiten", "tokens": ["Rit\u00b7ter\u00b7bur\u00b7gen", ",", "al\u00b7ter", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "D\u00fcstre Zeugen, Wehmuthsgr\u00fc\u00dfe schicken", "tokens": ["D\u00fcst\u00b7re", "Zeu\u00b7gen", ",", "Weh\u00b7muths\u00b7gr\u00fc\u00b7\u00dfe", "schi\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie zu Thal, gedenkend be\u00dfrer Zeiten", "tokens": ["Sie", "zu", "Thal", ",", "ge\u00b7den\u00b7kend", "be\u00df\u00b7rer", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "$,", "VVPP", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eisenglanz.", "tokens": ["Ei\u00b7sen\u00b7glanz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Herrlich wohl und schrecklich ist gewesen,", "tokens": ["Herr\u00b7lich", "wohl", "und", "schreck\u00b7lich", "ist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJD", "VAFIN", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als die Ungewitter sie umrollten!", "tokens": ["Als", "die", "Un\u00b7ge\u00b7wit\u00b7ter", "sie", "um\u00b7roll\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Donner schlug die Mauer, wankend l\u00f6sen", "tokens": ["Don\u00b7ner", "schlug", "die", "Mau\u00b7er", ",", "wan\u00b7kend", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von dem Fels die Schl\u00f6sser sich, als wollten", "tokens": ["Von", "dem", "Fels", "die", "Schl\u00f6s\u00b7ser", "sich", ",", "als", "woll\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF", "$,", "KOUS", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sie vergehn.", "tokens": ["Sie", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Sieh! ein neues Wetter h\u00e4ngt im Osten,", "tokens": ["Sieh", "!", "ein", "neu\u00b7es", "Wet\u00b7ter", "h\u00e4ngt", "im", "Os\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fernab grollt es, grelle Blitze zucken,", "tokens": ["Fern\u00b7ab", "grollt", "es", ",", "grel\u00b7le", "Blit\u00b7ze", "zu\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bang erzittern wieder die verschlo\u00dften", "tokens": ["Bang", "er\u00b7zit\u00b7tern", "wie\u00b7der", "die", "ver\u00b7schlo\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Saatgefilde, sieh, die Halme ducken", "tokens": ["Saat\u00b7ge\u00b7fil\u00b7de", ",", "sieh", ",", "die", "Hal\u00b7me", "du\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sich mit Hast.", "tokens": ["Sich", "mit", "Hast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Mitleid weckt ihr s\u00e4uselndes Gewimmer,", "tokens": ["Mit\u00b7leid", "weckt", "ihr", "s\u00e4u\u00b7seln\u00b7des", "Ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und vor\u00fcber saust das stolze Wetter.", "tokens": ["Und", "vor\u00b7\u00fc\u00b7ber", "saust", "das", "stol\u00b7ze", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welcher Sturmwind, hinter ihm der Schimmer,", "tokens": ["Wel\u00b7cher", "Sturm\u00b7wind", ",", "hin\u00b7ter", "ihm", "der", "Schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wirft die Nebel, gleich unn\u00fctzem Volke,", "tokens": ["Wirft", "die", "Ne\u00b7bel", ",", "gleich", "un\u00b7n\u00fct\u00b7zem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Thalhinaus!", "tokens": ["Thal\u00b7hin\u00b7aus", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Welche Bl\u00e4ue! milden Tages Helle!", "tokens": ["Wel\u00b7che", "Bl\u00e4u\u00b7e", "!", "mil\u00b7den", "Ta\u00b7ges", "Hel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Himmelsfarbe, keusche, seligreine,", "tokens": ["Him\u00b7mels\u00b7far\u00b7be", ",", "keu\u00b7sche", ",", "se\u00b7li\u00b7grei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sei gegr\u00fc\u00dft mir, lichte Aetherwelle!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "mir", ",", "lich\u00b7te", "A\u00b7e\u00b7ther\u00b7wel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sei gegr\u00fc\u00dft mit deinem Sonnenscheine", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "mit", "dei\u00b7nem", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Maientag!", "tokens": ["Mai\u00b7en\u00b7tag", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Nieder, nieder auf den heilgen Boden!", "tokens": ["Nie\u00b7der", ",", "nie\u00b7der", "auf", "den", "heil\u00b7gen", "Bo\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort wohin das Gottesauge blickte!", "tokens": ["Dort", "wo\u00b7hin", "das", "Got\u00b7tes\u00b7au\u00b7ge", "blick\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wecken m\u00fc\u00dft es einen Starren, Todten,", "tokens": ["We\u00b7cken", "m\u00fc\u00dft", "es", "ei\u00b7nen", "Star\u00b7ren", ",", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVINF", "VMFIN", "PPER", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn die k\u00fchle Erde ihn nicht dr\u00fcckte", "tokens": ["Wenn", "die", "k\u00fch\u00b7le", "Er\u00b7de", "ihn", "nicht", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Allzutief!", "tokens": ["All\u00b7zu\u00b7tief", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Dr\u00fcben bl\u00fcht ein Kirchhof! Sie verscharrten", "tokens": ["Dr\u00fc\u00b7ben", "bl\u00fcht", "ein", "Kirch\u00b7hof", "!", "Sie", "ver\u00b7scharr\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFI