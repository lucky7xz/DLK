{"textgrid.poem.31640": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "12. Es m\u00fcssen alle meine Feinde zu Schanden werden!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freylich m\u00fcssen unsre Feind ganz und gar zu schanden werden:", "tokens": ["Frey\u00b7lich", "m\u00fcs\u00b7sen", "uns\u00b7re", "Feind", "ganz", "und", "gar", "zu", "schan\u00b7den", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "KON", "ADV", "PTKA", "ADJD", "VAINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "weil derselbig vor uns streit / dessen Wort erschuff die Erden /", "tokens": ["weil", "der\u00b7sel\u00b7big", "vor", "uns", "streit", "/", "des\u00b7sen", "Wort", "er\u00b7schuff", "die", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "VVFIN", "$(", "PRELAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+----+-+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "der den Fel\u00df in Brunnen wandelt / und das Meer in hohe M\u00e4ur /", "tokens": ["der", "den", "Fel\u00df", "in", "Brun\u00b7nen", "wan\u00b7delt", "/", "und", "das", "Meer", "in", "ho\u00b7he", "M\u00e4ur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$(", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "der die Sonn zurucke rucket / der vom Himmel schickt das Feur.", "tokens": ["der", "die", "Sonn", "zu\u00b7ru\u00b7cke", "ru\u00b7cket", "/", "der", "vom", "Him\u00b7mel", "schickt", "das", "Feur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVFIN", "$(", "ART", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wie die Syrer / wird er sie eilends in die Flucht verjagen /", "tokens": ["Wie", "die", "Sy\u00b7rer", "/", "wird", "er", "sie", "ei\u00b7lends", "in", "die", "Flucht", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$(", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "und mit seinem Engel-Heer / ohne unsre Schwerder / schlagen.", "tokens": ["und", "mit", "sei\u00b7nem", "En\u00b7gel\u00b7Heer", "/", "oh\u00b7ne", "uns\u00b7re", "Schwer\u00b7der", "/", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}}}}}