{"textgrid.poem.37600": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Sammler", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der beste Sammler, das ist Gott,", "tokens": ["Der", "bes\u00b7te", "Samm\u00b7ler", ",", "das", "ist", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht ihm nichts verloren,", "tokens": ["Es", "geht", "ihm", "nichts", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sammelt Leben aus dem Tod,", "tokens": ["Er", "sam\u00b7melt", "Le\u00b7ben", "aus", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird ihm neugeboren.", "tokens": ["So", "wird", "ihm", "neu\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in verherrlichter Gestalt", "tokens": ["Und", "in", "ver\u00b7herr\u00b7lich\u00b7ter", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erscheint was hier mi\u00dflungen,", "tokens": ["Er\u00b7scheint", "was", "hier", "mi\u00df\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wort, das einsam hier verhallt,", "tokens": ["Das", "Wort", ",", "das", "ein\u00b7sam", "hier", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ihm als Lied gesungen.", "tokens": ["Ist", "ihm", "als", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir folgen ihm, er steht uns bei,", "tokens": ["Wir", "fol\u00b7gen", "ihm", ",", "er", "steht", "uns", "bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kloster-B\u00fcchersaale,", "tokens": ["Im", "Klos\u00b7ter\u00b7B\u00fc\u00b7cher\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo ein Geist vergessen sei,", "tokens": ["Und", "wo", "ein", "Geist", "ver\u00b7ges\u00b7sen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ladet ihn zum Mahle.", "tokens": ["Er", "la\u00b7det", "ihn", "zum", "Mah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kommt ein Strahl vom Abendschein", "tokens": ["Da", "kommt", "ein", "Strahl", "vom", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch's Fenster, dich erschrecket,", "tokens": ["Durch's", "Fens\u00b7ter", ",", "dich", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und richtet Aug' und H\u00e4nde dein,", "tokens": ["Und", "rich\u00b7tet", "Aug'", "und", "H\u00e4n\u00b7de", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo noch ein Buch verstecket.", "tokens": ["Wo", "noch", "ein", "Buch", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So sammelt er die Freunde auch,", "tokens": ["So", "sam\u00b7melt", "er", "die", "Freun\u00b7de", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gleiche Wege gehen,", "tokens": ["Die", "glei\u00b7che", "We\u00b7ge", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie gr\u00fc\u00dfen sich nach altem Brauch,", "tokens": ["Sie", "gr\u00fc\u00b7\u00dfen", "sich", "nach", "al\u00b7tem", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gest\u00e4rket weitergehen.", "tokens": ["Ge\u00b7st\u00e4r\u00b7ket", "wei\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}