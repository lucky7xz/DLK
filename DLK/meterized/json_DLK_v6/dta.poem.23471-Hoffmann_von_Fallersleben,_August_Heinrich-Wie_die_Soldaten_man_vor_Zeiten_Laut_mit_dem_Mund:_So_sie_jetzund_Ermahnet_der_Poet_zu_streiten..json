{"dta.poem.23471": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie die Soldaten man vor Zeiten  \n Laut mit dem Mund:  \n So sie jetzund  \n Ermahnet der Poet zu streiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, ihr tapfere Soldaten!", "tokens": ["Frisch", "auf", ",", "ihr", "tap\u00b7fe\u00b7re", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr, die ihr noch mit deutschem Blut,", "tokens": ["Ihr", ",", "die", "ihr", "noch", "mit", "deut\u00b7schem", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr die ihr noch mit frischem Muth", "tokens": ["Ihr", "die", "ihr", "noch", "mit", "fri\u00b7schem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Belebet, suchet gro\u00dfe Thaten!", "tokens": ["Be\u00b7le\u00b7bet", ",", "su\u00b7chet", "gro\u00b7\u00dfe", "Tha\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Landsleut, ihr Landsknecht, frisch auf!", "tokens": ["Ihr", "Lands\u00b7leut", ",", "ihr", "Lands\u00b7knecht", ",", "frisch", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Das Land, die Freiheit sich verlieret,", "tokens": ["Das", "Land", ",", "die", "Frei\u00b7heit", "sich", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wann ihr nicht muthig schlaget drauf", "tokens": ["Wann", "ihr", "nicht", "mut\u00b7hig", "schla\u00b7get", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u00fcberwindend triumphieret.", "tokens": ["Und", "\u00fc\u00b7berw\u00b7in\u00b7dend", "tri\u00b7um\u00b7phie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der ist ein Deutscher wohlgeboren,", "tokens": ["Der", "ist", "ein", "Deut\u00b7scher", "wohl\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, von Betrug und Falschheit frei,", "tokens": ["Der", ",", "von", "Be\u00b7trug", "und", "Falschheit", "frei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat weder Redlichkeit noch Treu", "tokens": ["Hat", "we\u00b7der", "Red\u00b7lich\u00b7keit", "noch", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Glauben noch Freiheit verloren;", "tokens": ["Noch", "Glau\u00b7ben", "noch", "Frei\u00b7heit", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der ist ein Deutscher ehrenwerth,", "tokens": ["Der", "ist", "ein", "Deut\u00b7scher", "eh\u00b7ren\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wacker, herzhaft, unverzaget", "tokens": ["Der", "wa\u00b7cker", ",", "herz\u00b7haft", ",", "un\u00b7ver\u00b7za\u00b7get"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJD", "$,", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr die Freiheit mit seinem Schwert", "tokens": ["F\u00fcr", "die", "Frei\u00b7heit", "mit", "sei\u00b7nem", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "In einige Gefahr sich waget.", "tokens": ["In", "ei\u00b7ni\u00b7ge", "Ge\u00b7fahr", "sich", "wa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohlan derhalb, ihr wahre Deutschen,", "tokens": ["Wo\u00b7hlan", "der\u00b7halb", ",", "ihr", "wah\u00b7re", "Deut\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deutscher Faust, mit deutschem Muth", "tokens": ["Mit", "deut\u00b7scher", "Faust", ",", "mit", "deut\u00b7schem", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "D\u00e4mpfet nu der Tyrannen Wuth!", "tokens": ["D\u00e4mp\u00b7fet", "nu", "der", "Ty\u00b7ran\u00b7nen", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Zerbrechet ihr Joch, Band und Beutschen!", "tokens": ["Zer\u00b7bre\u00b7chet", "ihr", "Joch", ",", "Band", "und", "Beut\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "VVINF", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Un\u00fcberwindlich r\u00fchmet sie", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "r\u00fch\u00b7met", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Titul, Thorheit und Stolzieren;", "tokens": ["Ihr", "Ti\u00b7tul", ",", "Thor\u00b7heit", "und", "Stol\u00b7zie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aber ihr Heer mit schlechter M\u00fch", "tokens": ["A\u00b7ber", "ihr", "Heer", "mit", "schlech\u00b7ter", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mag (\u00fcberwindlich) bald verlieren.", "tokens": ["Mag", "(", "\u00fc\u00b7berw\u00b7ind\u00b7lich", ")", "bald", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$(", "ADJD", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ha! fallet in sie! ihre Fahnen", "tokens": ["Ha", "!", "fal\u00b7let", "in", "sie", "!", "ih\u00b7re", "Fah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zittern aus Furcht: sie trennen sich,", "tokens": ["Zit\u00b7tern", "aus", "Furcht", ":", "sie", "tren\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "PRF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr b\u00f6se Sach h\u00e4lt nicht den Stich,", "tokens": ["Ihr", "b\u00f6\u00b7se", "Sach", "h\u00e4lt", "nicht", "den", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum zu der Flucht sie sich schon mahnen;", "tokens": ["Drum", "zu", "der", "Flucht", "sie", "sich", "schon", "mah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gro\u00df ist ihr Heer, klein ist ihr Glaub;", "tokens": ["Gro\u00df", "ist", "ihr", "Heer", ",", "klein", "ist", "ihr", "Glaub", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gut ist ihr Zeug, b\u00f6s ihr Gewissen;", "tokens": ["Gut", "ist", "ihr", "Zeug", ",", "b\u00f6s", "ihr", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Frisch auf! sie zittern wie das Laub", "tokens": ["Frisch", "auf", "!", "sie", "zit\u00b7tern", "wie", "das", "Laub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und w\u00e4ren schon gern ausgerissen.", "tokens": ["Und", "w\u00e4\u00b7ren", "schon", "gern", "aus\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ha! schlaget auf sie, liebe Br\u00fcder!", "tokens": ["Ha", "!", "schla\u00b7get", "auf", "sie", ",", "lie\u00b7be", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist die M\u00fch gro\u00df, so ist nicht schlecht", "tokens": ["Ist", "die", "M\u00fch", "gro\u00df", ",", "so", "ist", "nicht", "schlecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sieg und Beut; und wohl und recht", "tokens": ["Der", "Sieg", "und", "Beut", ";", "und", "wohl", "und", "recht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$.", "KON", "ADV", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu thun seind sie, dann ihr, viel m\u00fcder.", "tokens": ["Zu", "thun", "seind", "sie", ",", "dann", "ihr", ",", "viel", "m\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "$,", "ADV", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So straf, o deutsches Herz und Hand,", "tokens": ["So", "straf", ",", "o", "deut\u00b7sches", "Herz", "und", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nu die Tyrannen und die B\u00f6sen:", "tokens": ["Nu", "die", "Ty\u00b7ran\u00b7nen", "und", "die", "B\u00f6\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Freiheit und das Vaterland", "tokens": ["Die", "Frei\u00b7heit", "und", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00dft du auf diese Weis erl\u00f6sen.", "tokens": ["Mu\u00dft", "du", "auf", "die\u00b7se", "Weis", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}