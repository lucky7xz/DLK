{"textgrid.poem.58379": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "417. Zu Gott den Sinn durch alles hin!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was man hier leidet und genie\u00dfet,", "tokens": ["Was", "man", "hier", "lei\u00b7det", "und", "ge\u00b7nie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ver\u00e4ndert sich und schnell verflie\u00dfet;", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "sich", "und", "schnell", "ver\u00b7flie\u00b7\u00dfet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum halte dich bei gar nichts auf,", "tokens": ["Drum", "hal\u00b7te", "dich", "bei", "gar", "nichts", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur blo\u00df zu Gott richt deinen Lauf!", "tokens": ["Nur", "blo\u00df", "zu", "Gott", "richt", "dei\u00b7nen", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}