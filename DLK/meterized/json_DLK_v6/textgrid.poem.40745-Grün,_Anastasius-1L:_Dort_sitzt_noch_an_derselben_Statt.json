{"textgrid.poem.40745": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort sitzt noch an derselben Statt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort sitzt noch an derselben Statt", "tokens": ["Dort", "sitzt", "noch", "an", "der\u00b7sel\u00b7ben", "Statt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alte Trommler wieder;", "tokens": ["Der", "al\u00b7te", "Tromm\u00b7ler", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Hand entfiel das Zeitungsblatt", "tokens": ["Der", "Hand", "ent\u00b7fiel", "das", "Zei\u00b7tungs\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und glitt zu Boden nieder.", "tokens": ["Und", "glitt", "zu", "Bo\u00b7den", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbmagenta! \u2013 Solferino!\u00ab st\u00f6hnt", "tokens": ["\u00bb", "ma\u00b7gen\u00b7ta", "!", "\u2013", "Sol\u00b7fe\u00b7ri\u00b7no", "!", "\u00ab", "st\u00f6hnt"], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "punct", "word"], "pos": ["$(", "ITJ", "$.", "$(", "NE", "$.", "$(", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im Selbstgespr\u00e4ch er heute,", "tokens": ["Im", "Selbst\u00b7ge\u00b7spr\u00e4ch", "er", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und seiner Stimme Nachhall t\u00f6nt", "tokens": ["Und", "sei\u00b7ner", "Stim\u00b7me", "Nach\u00b7hall", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie traurig Grabgel\u00e4ute.", "tokens": ["Wie", "trau\u00b7rig", "Grab\u00b7ge\u00b7l\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbund drum zehn Jahre Tag und Nacht", "tokens": ["\u00bb", "und", "drum", "zehn", "Jah\u00b7re", "Tag", "und", "Nacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PAV", "CARD", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getrommelt und geblasen!", "tokens": ["Ge\u00b7trom\u00b7melt", "und", "ge\u00b7bla\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Drillschritt uns mit Heeresmacht", "tokens": ["Im", "Drill\u00b7schritt", "uns", "mit", "Hee\u00b7res\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zertreten Saat und Rasen!", "tokens": ["Zer\u00b7tre\u00b7ten", "Saat", "und", "Ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wer nicht bunten Kragen trug", "tokens": ["Und", "wer", "nicht", "bun\u00b7ten", "Kra\u00b7gen", "trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Knecht betre\u00dfter Massen!", "tokens": ["Ein", "Knecht", "be\u00b7tre\u00df\u00b7ter", "Mas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verwaist die Werkstatt und der Pflug", "tokens": ["Ver\u00b7waist", "die", "Werk\u00b7statt", "und", "der", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leer gest\u00fcrzt die Kassen!", "tokens": ["Und", "leer", "ge\u00b7st\u00fcrzt", "die", "Kas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch jetzt! auch nicht ein kleinster Sieg", "tokens": ["Doch", "jetzt", "!", "auch", "nicht", "ein", "kleins\u00b7ter", "Sieg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Herzen aufzufrischen!", "tokens": ["Die", "Her\u00b7zen", "auf\u00b7zu\u00b7fri\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Krieg, der schreit nach neuem Krieg,", "tokens": ["Ein", "Krieg", ",", "der", "schreit", "nach", "neu\u00b7em", "Krieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Brandmal zu verwischen!", "tokens": ["Das", "Brand\u00b7mal", "zu", "ver\u00b7wi\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der einstudirte Schwertertanz", "tokens": ["Der", "ein\u00b7stu\u00b7dir\u00b7te", "Schwer\u00b7ter\u00b7tanz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So grauenvoll mi\u00dfrathen!", "tokens": ["So", "grau\u00b7en\u00b7voll", "mi\u00df\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einst rissen aus des Korsen Kranz", "tokens": ["Einst", "ris\u00b7sen", "aus", "des", "Kor\u00b7sen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Manch Blatt doch ", "tokens": ["Manch", "Blatt", "doch"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Sein Finger trommelt auf den Tisch", "tokens": ["Sein", "Fin\u00b7ger", "trom\u00b7melt", "auf", "den", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kriegsmarsch l\u00e4ngst verklungen;", "tokens": ["Den", "Kriegs\u00b7marsch", "l\u00e4ngst", "ver\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Alten macht er tr\u00e4umerisch,", "tokens": ["Den", "Al\u00b7ten", "macht", "er", "tr\u00e4u\u00b7me\u00b7risch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch fremd klingt er den Jungen.", "tokens": ["Doch", "fremd", "klingt", "er", "den", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Jetzt horcht er auf: was soll das sein?", "tokens": ["Jetzt", "horcht", "er", "auf", ":", "was", "soll", "das", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VMFIN", "ART", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Freudenruf und Klagen!", "tokens": ["Ein", "Freu\u00b7den\u00b7ruf", "und", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da tritt sein j\u00fcngster Enkel ein", "tokens": ["Da", "tritt", "sein", "j\u00fcngs\u00b7ter", "En\u00b7kel", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In buntem Rock und Kragen;", "tokens": ["In", "bun\u00b7tem", "Rock", "und", "Kra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Haupt gesenkt, das Herz so schwer,", "tokens": ["Das", "Haupt", "ge\u00b7senkt", ",", "das", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Arm in schwarzer Binde:", "tokens": ["Den", "Arm", "in", "schwar\u00b7zer", "Bin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbvon Solferino komm' ich her,", "tokens": ["\u00bb", "von", "Sol\u00b7fe\u00b7ri\u00b7no", "komm'", "ich", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein dunkler Wort ich finde!\u00ab", "tokens": ["Kein", "dunk\u00b7ler", "Wort", "ich", "fin\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da spielt ums Greisenhaupt ein Licht,", "tokens": ["Da", "spielt", "ums", "Grei\u00b7sen\u00b7haupt", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ebnet manche Falte,", "tokens": ["Das", "eb\u00b7net", "man\u00b7che", "Fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und milden Blicks zum Enkel spricht", "tokens": ["Und", "mil\u00b7den", "Blicks", "zum", "En\u00b7kel", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sanften Tons der Alte:", "tokens": ["Und", "sanf\u00b7ten", "Tons", "der", "Al\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbaus dunklem Schacht steigt helles Erz,", "tokens": ["\u00bb", "aus", "dunk\u00b7lem", "Schacht", "steigt", "hel\u00b7les", "Erz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus schwarzem Grund die Rose;", "tokens": ["Aus", "schwar\u00b7zem", "Grund", "die", "Ro\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob echt und recht ein Kriegerherz", "tokens": ["Ob", "echt", "und", "recht", "ein", "Krie\u00b7ger\u00b7herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befrag' ich Ungl\u00fccksloose.", "tokens": ["Be\u00b7frag'", "ich", "Un\u00b7gl\u00fccks\u00b7loo\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Das war das alte Schlachtro\u00df noch,", "tokens": ["Das", "war", "das", "al\u00b7te", "Schlach\u00b7tro\u00df", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nicht Radetzky's Z\u00fcgel!", "tokens": ["Doch", "nicht", "Ra\u00b7detz\u00b7ky's", "Z\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll Mark und Kampflust war's, \u2013 jedoch", "tokens": ["Voll", "Mark", "und", "Kampf\u00b7lust", "wa\u00b7r's", ",", "\u2013", "je\u00b7doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADJD", "NN", "KON", "NN", "VAFIN", "$,", "$(", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Andrer sa\u00df im B\u00fcgel.", "tokens": ["Ein", "A\u00b7ndrer", "sa\u00df", "im", "B\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die G\u00f6tter, die f\u00fcr Lorbeerglanz", "tokens": ["Die", "G\u00f6t\u00b7ter", ",", "die", "f\u00fcr", "Lor\u00b7beer\u00b7glanz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Feldherrnhaupt nicht finden,", "tokens": ["Ein", "Feld\u00b7herrn\u00b7haupt", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollen Euch mit vollem Kranz", "tokens": ["Sie", "wol\u00b7len", "Euch", "mit", "vol\u00b7lem", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die tapfre Faust umwinden.", "tokens": ["Die", "tapf\u00b7re", "Faust", "um\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie jener R\u00f6mer sprang zum Schlund,", "tokens": ["Wie", "je\u00b7ner", "R\u00f6\u00b7mer", "sprang", "zum", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem G\u00f6tterzorn sich weihend,", "tokens": ["Dem", "G\u00f6t\u00b7ter\u00b7zorn", "sich", "wei\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sankt Ihr auf der Wahlstatt Grund,", "tokens": ["So", "sankt", "Ihr", "auf", "der", "Wahl\u00b7statt", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Volk daheim befreiend.", "tokens": ["Das", "Volk", "da\u00b7heim", "be\u00b7frei\u00b7end", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Mein Oesterreich, gar manchen Sohn", "tokens": ["Mein", "O\u00b7es\u00b7ter\u00b7reich", ",", "gar", "man\u00b7chen", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als S\u00fchne sahst du bluten,", "tokens": ["Als", "S\u00fch\u00b7ne", "sahst", "du", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Schmerz und Schmach doch f\u00fchlst du's schon:", "tokens": ["In", "Schmerz", "und", "Schmach