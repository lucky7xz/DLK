{"dta.poem.12833": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein junges m\u00e4gdgen, so sich an  \n einen alten soldaten hienge.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Maegdgen! deine jungferschafft ist kaum heute jung ge-", "tokens": ["Maegd\u00b7gen", "!", "dei\u00b7ne", "jung\u00b7fer\u00b7schafft", "ist", "kaum", "heu\u00b7te", "jung", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "worden,", "tokens": ["wor\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VAPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ach du kauffest dich zu fr\u00fch in den welcken weiber-orden.", "tokens": ["Ach", "du", "kauf\u00b7fest", "dich", "zu", "fr\u00fch", "in", "den", "wel\u00b7cken", "wei\u00b7ber\u00b7or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "APPR", "ART", "NN", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Sage doch, was dienet dir ein so steiffer krieges-mann,", "tokens": ["Sa\u00b7ge", "doch", ",", "was", "die\u00b7net", "dir", "ein", "so", "steif\u00b7fer", "krie\u00b7ge\u00b7smann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWS", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Dem ein exercirtes heer kaum die lantze brechen kan?", "tokens": ["Dem", "ein", "ex\u00b7er\u00b7cir\u00b7tes", "heer", "kaum", "die", "lant\u00b7ze", "bre\u00b7chen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}