{"textgrid.poem.41866": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n ist es dort, wo k\u00fchne Adler bauen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist es dort, wo k\u00fchne Adler bauen,", "tokens": ["Sch\u00f6n", "ist", "es", "dort", ",", "wo", "k\u00fch\u00b7ne", "Ad\u00b7ler", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf hohem Fels mit stiller Kraft zu stehn,", "tokens": ["Auf", "ho\u00b7hem", "Fels", "mit", "stil\u00b7ler", "Kraft", "zu", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unverzagt durch finstres Wolkengrauen", "tokens": ["Und", "un\u00b7ver\u00b7zagt", "durch", "finst\u00b7res", "Wol\u00b7ken\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und durch's Gebiet des Blitzes hinzugehn;", "tokens": ["Und", "durch's", "Ge\u00b7biet", "des", "Blit\u00b7zes", "hin\u00b7zu\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch lieblich auch, zu ruhn auf weichen Auen", "tokens": ["Doch", "lieb\u00b7lich", "auch", ",", "zu", "ruhn", "auf", "wei\u00b7chen", "Au\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "$,", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Am leisen Quell, in linder L\u00fcfte Wehn,", "tokens": ["Am", "lei\u00b7sen", "Quell", ",", "in", "lin\u00b7der", "L\u00fcf\u00b7te", "Wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Lust und Leid des Lebens zu empfinden,", "tokens": ["Und", "Lust", "und", "Leid", "des", "Le\u00b7bens", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Kr\u00e4nze sich, die schnell verbl\u00fchn, zu winden.", "tokens": ["Und", "Kr\u00e4n\u00b7ze", "sich", ",", "die", "schnell", "ver\u00b7bl\u00fchn", ",", "zu", "win\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "$,", "PRELS", "ADJD", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So kann nicht stets mit ernsten Harfent\u00f6nen", "tokens": ["So", "kann", "nicht", "stets", "mit", "erns\u00b7ten", "Har\u00b7fen\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der S\u00e4nger sich den hohen Musen nahn.", "tokens": ["Der", "S\u00e4n\u00b7ger", "sich", "den", "ho\u00b7hen", "Mu\u00b7sen", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gern folgt er oft des Herzens weicherm Sehnen", "tokens": ["Gern", "folgt", "er", "oft", "des", "Her\u00b7zens", "wei\u00b7cherm", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wandelt still auf duft'ger Wiesenbahn,", "tokens": ["Und", "wan\u00b7delt", "still", "auf", "duft'\u00b7ger", "Wie\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit zartem Schmuck der Liebsten Bild zu kr\u00f6nen,", "tokens": ["Mit", "zar\u00b7tem", "Schmuck", "der", "Liebs\u00b7ten", "Bild", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im s\u00fc\u00dfen Traum das Leben zu umfahn,", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Traum", "das", "Le\u00b7ben", "zu", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit leiserm Klang das Sch\u00f6ne zu begr\u00fc\u00dfen", "tokens": ["Mit", "lei\u00b7serm", "Klang", "das", "Sch\u00f6\u00b7ne", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Lust und Leid in Liedern zu ergie\u00dfen.", "tokens": ["Und", "Lust", "und", "Leid", "in", "Lie\u00b7dern", "zu", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So hab' ich jetzt in unbelauschten Stunden,", "tokens": ["So", "hab'", "ich", "jetzt", "in", "un\u00b7be\u00b7lauschten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo l\u00e4chelnd mir dein holdes Bild erschien,", "tokens": ["Wo", "l\u00e4\u00b7chelnd", "mir", "dein", "hol\u00b7des", "Bild", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Blumenkranz der Lieder dir gewunden,", "tokens": ["Den", "Blu\u00b7men\u00b7kranz", "der", "Lie\u00b7der", "dir", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die leicht entstehn, kurz duften, bald verbl\u00fchn.", "tokens": ["Die", "leicht", "ent\u00b7stehn", ",", "kurz", "duf\u00b7ten", ",", "bald", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "ADJD", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht prangt, was rasch das gl\u00fch'nde Herz empfunden,", "tokens": ["Nicht", "prangt", ",", "was", "rasch", "das", "gl\u00fch'n\u00b7de", "Herz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PRELS", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im ew'gen Schmuck von frischem Immergr\u00fcn;", "tokens": ["Im", "ew'\u00b7gen", "Schmuck", "von", "fri\u00b7schem", "Im\u00b7mer\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aufwallend will's im Liede wiederhallen", "tokens": ["Auf\u00b7wal\u00b7lend", "will's", "im", "Lie\u00b7de", "wie\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und fl\u00fcchtig nur, so lang es klingt, gefallen.", "tokens": ["Und", "fl\u00fcch\u00b7tig", "nur", ",", "so", "lang", "es", "klingt", ",", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl kr\u00e4useln sich die leichtbewegten Fluthen,", "tokens": ["Wohl", "kr\u00e4u\u00b7seln", "sich", "die", "leicht\u00b7be\u00b7weg\u00b7ten", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und irrend schweift der Strahl im Wogentanz;", "tokens": ["Und", "ir\u00b7rend", "schweift", "der", "Strahl", "im", "Wo\u00b7gen\u00b7tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn vom Spiel die glatten Wellen ruhten,", "tokens": ["Doch", "wenn", "vom", "Spiel", "die", "glat\u00b7ten", "Wel\u00b7len", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann lacht' im Meer der Sonne stiller Glanz.", "tokens": ["Dann", "lacht'", "im", "Meer", "der", "Son\u00b7ne", "stil\u00b7ler", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So z\u00e4hm' ich jetzt des Herzens rasche Gluthen", "tokens": ["So", "z\u00e4hm'", "ich", "jetzt", "des", "Her\u00b7zens", "ra\u00b7sche", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und blick' empor zum nie verbl\u00fch'nden Kranz.", "tokens": ["Und", "blick'", "em\u00b7por", "zum", "nie", "ver\u00b7bl\u00fch'n\u00b7den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wohl ist es schwer, dem Spiele zu entsagen,", "tokens": ["Wohl", "ist", "es", "schwer", ",", "dem", "Spie\u00b7le", "zu", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch herrlich auch, Unsterbliches zu wagen.", "tokens": ["Doch", "herr\u00b7lich", "auch", ",", "U\u00b7nsterb\u00b7li\u00b7ches", "zu", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und lauter soll die Harfe wieder klingen;", "tokens": ["Und", "lau\u00b7ter", "soll", "die", "Har\u00b7fe", "wie\u00b7der", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Licht und Nacht, durch Kampf und Lust und Leid", "tokens": ["Durch", "Licht", "und", "Nacht", ",", "durch", "Kampf", "und", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will ich getrost den steilen Pfad vollbringen,", "tokens": ["Will", "ich", "ge\u00b7trost", "den", "stei\u00b7len", "Pfad", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Liebe mich, dem mich der Tod geweiht,", "tokens": ["Dem", "Lie\u00b7be", "mich", ",", "dem", "mich", "der", "Tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon rauscht und naht mit seinen lichten Schwingen", "tokens": ["Schon", "rauscht", "und", "naht", "mit", "sei\u00b7nen", "lich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das seel'ge Bild, das mir die Palme beut!", "tokens": ["Das", "seel'\u00b7ge", "Bild", ",", "das", "mir", "die", "Pal\u00b7me", "beut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du, l\u00e4chle mild herab auf meine T\u00f6ne,", "tokens": ["Du", ",", "l\u00e4ch\u00b7le", "mild", "her\u00b7ab", "auf", "mei\u00b7ne", "T\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Euch und mich der ew'ge Lorbeer kr\u00f6ne.", "tokens": ["Da\u00df", "Euch", "und", "mich", "der", "ew'\u00b7ge", "Lor\u00b7beer", "kr\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}