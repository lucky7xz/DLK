{"textgrid.poem.26137": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und Orgelpfeifen sind die Eichen und Buchen im Wind", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Walde einer am Wege sitzt von Mittagssonne erhitzt;", "tokens": ["Im", "Wal\u00b7de", "ei\u00b7ner", "am", "We\u00b7ge", "sitzt", "von", "Mit\u00b7tags\u00b7son\u00b7ne", "er\u00b7hitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "APPRART", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Sieht der Buschbirke zu, die mit den Blattspiegeln blitzt,", "tokens": ["Sieht", "der", "Buschbir\u00b7ke", "zu", ",", "die", "mit", "den", "Blatt\u00b7spie\u00b7geln", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Und horcht der Windorgel nach; die gibt der Waldtiefe keine Ruh.", "tokens": ["Und", "horcht", "der", "Win\u00b7dor\u00b7gel", "nach", ";", "die", "gibt", "der", "Wald\u00b7tie\u00b7fe", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Es spielt im Wald die Orgel erst leise, dann laut bald,", "tokens": ["Es", "spielt", "im", "Wald", "die", "Or\u00b7gel", "erst", "lei\u00b7se", ",", "dann", "laut", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und immer den Anfang ohn' Ende derselben Weise,", "tokens": ["Und", "im\u00b7mer", "den", "An\u00b7fang", "ohn'", "En\u00b7de", "der\u00b7sel\u00b7ben", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Und Orgelpfeifen sind Eichen und Buchen im Wind.", "tokens": ["Und", "Or\u00b7gel\u00b7pfei\u00b7fen", "sind", "Ei\u00b7chen", "und", "Bu\u00b7chen", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie w\u00fcnschen dem Wind, der auf ewiger Reise,", "tokens": ["Sie", "w\u00fcn\u00b7schen", "dem", "Wind", ",", "der", "auf", "e\u00b7wi\u00b7ger", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df er gleich der Liebe das Ende nie find',", "tokens": ["Da\u00df", "er", "gleich", "der", "Lie\u00b7be", "das", "En\u00b7de", "nie", "find'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Damit ihre Lieder unsterblich sind.", "tokens": ["Da\u00b7mit", "ih\u00b7re", "Lie\u00b7der", "uns\u00b7terb\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}