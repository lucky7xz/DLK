{"textgrid.poem.52610": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sitz ich am Morgen,", "tokens": ["So", "sitz", "ich", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bei Tag und Nacht,", "tokens": ["Bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und geb auf die fl\u00fcchtigen", "tokens": ["Und", "geb", "auf", "die", "fl\u00fcch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wellen Acht.", "tokens": ["Wel\u00b7len", "Acht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "CARD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die Wellen, sie kommen,", "tokens": ["Die", "Wel\u00b7len", ",", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie bleiben, sie gehn,", "tokens": ["Sie", "blei\u00b7ben", ",", "sie", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch keine l\u00e4\u00dft wieder", "tokens": ["Doch", "kei\u00b7ne", "l\u00e4\u00dft", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Freund mich sehn.", "tokens": ["Den", "Freund", "mich", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und bin ich zu Hause,", "tokens": ["Und", "bin", "ich", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So treibts immer mehr", "tokens": ["So", "treibts", "im\u00b7mer", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zum Platze, dem grausigen,", "tokens": ["Zum", "Plat\u00b7ze", ",", "dem", "grau\u00b7si\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aermste mich her.", "tokens": ["A\u00b7erms\u00b7te", "mich", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Die Wellen, sie wandern,", "tokens": ["Die", "Wel\u00b7len", ",", "sie", "wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie kommen und nahn,", "tokens": ["Sie", "kom\u00b7men", "und", "nahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es bringet mir keine", "tokens": ["Es", "brin\u00b7get", "mir", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Freund heran.", "tokens": ["Den", "Freund", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "O bin ich so einsam,", "tokens": ["O", "bin", "ich", "so", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Verlassen allein!", "tokens": ["Ver\u00b7las\u00b7sen", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Liebster mu\u00df ewig", "tokens": ["Mein", "Liebs\u00b7ter", "mu\u00df", "e\u00b7wig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verloren sein.", "tokens": ["Ver\u00b7lo\u00b7ren", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Es br\u00fcllte der Donner,", "tokens": ["Es", "br\u00fcll\u00b7te", "der", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es tobte der Bach,", "tokens": ["Es", "tob\u00b7te", "der", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Br\u00fcck ist geborsten,", "tokens": ["Die", "Br\u00fcck", "ist", "ge\u00b7bors\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er st\u00fcrzte so jach.", "tokens": ["Er", "st\u00fcrz\u00b7te", "so", "jach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "So ist er versunken", "tokens": ["So", "ist", "er", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ins nasse Grab,", "tokens": ["Ins", "nas\u00b7se", "Grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch mu\u00df er noch kommen", "tokens": ["Doch", "mu\u00df", "er", "noch", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu mir herab.", "tokens": ["Zu", "mir", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Nun schau ich ins Wasser,", "tokens": ["Nun", "schau", "ich", "ins", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sitz hin und harr,", "tokens": ["Sitz", "hin", "und", "harr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wann kommt sie die Leiche", "tokens": ["Wann", "kommt", "sie", "die", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So bleich und starr?", "tokens": ["So", "bleich", "und", "starr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "O sch\u00e4umet ihr Wasser,", "tokens": ["O", "sch\u00e4u\u00b7met", "ihr", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nimm auf mich du Bach,", "tokens": ["Nimm", "auf", "mich", "du", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PPER", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und treibt mich dem Freunde,", "tokens": ["Und", "treibt", "mich", "dem", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dem liebenden nach!", "tokens": ["Dem", "lie\u00b7ben\u00b7den", "nach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}