{"textgrid.poem.31549": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht / der im Adler Thron der Scepterf\u00fchrer ist /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht / der im Adler Thron der Scepterf\u00fchrer ist /", "tokens": ["Nicht", "/", "der", "im", "Ad\u00b7ler", "Thron", "der", "Scep\u00b7ter\u00b7f\u00fch\u00b7rer", "ist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$(", "ART", "APPRART", "NN", "NN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nicht stolze Helden auch / noch Sternen-Hochgelehrte /", "tokens": ["nicht", "stol\u00b7ze", "Hel\u00b7den", "auch", "/", "noch", "Ster\u00b7nen\u00b7Hoch\u00b7ge\u00b7lehr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "$(", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auch Wei\u00dfe Grei\u00dfen nicht / noch Geistlich-H\u00f6chstgeehrte /", "tokens": ["auch", "Wei\u00b7\u00dfe", "Grei\u00b7\u00dfen", "nicht", "/", "noch", "Geist\u00b7lich\u00b7H\u00f6chst\u00b7ge\u00b7ehr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "$(", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sind zu der hohen Ehr / der Vrst\u00e4nd fast erkiest.", "tokens": ["sind", "zu", "der", "ho\u00b7hen", "Ehr", "/", "der", "Vr\u00b7st\u00e4nd", "fast", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den schwachen Weibern du erschienen bist / Herr Christ!", "tokens": ["Den", "schwa\u00b7chen", "Wei\u00b7bern", "du", "er\u00b7schie\u00b7nen", "bist", "/", "Herr", "Christ", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "VAFIN", "$(", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der Himmel diese Gnad nur unserm Volck bescher /", "tokens": ["der", "Him\u00b7mel", "die\u00b7se", "Gnad", "nur", "un\u00b7serm", "Volck", "be\u00b7scher", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ADV", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df es die Wunder-That mit Lob und Ruhm vermehr /", "tokens": ["da\u00df", "es", "die", "Wun\u00b7der\u00b7That", "mit", "Lob", "und", "Ruhm", "ver\u00b7mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "erw\u00e4hlend reine Treu / vor arge Wei\u00dfheit-List.", "tokens": ["er\u00b7w\u00e4h\u00b7lend", "rei\u00b7ne", "Treu", "/", "vor", "ar\u00b7ge", "Wei\u00df\u00b7heit\u00b7List", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil unsre Einfalt dir / O Wei\u00dfheit-Brunn / gefallen:", "tokens": ["Weil", "uns\u00b7re", "Ein\u00b7falt", "dir", "/", "O", "Wei\u00df\u00b7heit\u00b7Brunn", "/", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "$(", "NE", "NE", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so la\u00df aus meinem Mund dein Sieges-Lob erschallen.", "tokens": ["so", "la\u00df", "aus", "mei\u00b7nem", "Mund", "dein", "Sie\u00b7ges\u00b7Lob", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gott / Mensch / Liecht / Leben / Heil / kurz / alles gut ersteht.", "tokens": ["Gott", "/", "Mensch", "/", "Liecht", "/", "Le\u00b7ben", "/", "Heil", "/", "kurz", "/", "al\u00b7les", "gut", "er\u00b7steht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "ADJD", "$(", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das ganze Himmel-Reich aus diesem Grab hergeht.", "tokens": ["Das", "gan\u00b7ze", "Him\u00b7mel\u00b7Reich", "aus", "die\u00b7sem", "Grab", "her\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gott / sein vereintes Seyn in seinem Tod belebet", "tokens": ["Gott", "/", "sein", "ver\u00b7ein\u00b7tes", "Seyn", "in", "sei\u00b7nem", "Tod", "be\u00b7le\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "auf Himmels Heimlichkeit / und herrlich / jetzt erhebet.", "tokens": ["auf", "Him\u00b7mels", "Heim\u00b7lich\u00b7keit", "/", "und", "herr\u00b7lich", "/", "jetzt", "er\u00b7he\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "KON", "ADJD", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}