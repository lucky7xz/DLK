{"textgrid.poem.49320": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Heuchler", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der kluge Satyrus der konte nicht erleiden", "tokens": ["Der", "klu\u00b7ge", "Sa\u00b7ty\u00b7rus", "der", "kon\u00b7te", "nicht", "er\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des falschen Bauren Maul, das fertig war in beiden,", "tokens": ["Des", "fal\u00b7schen", "Bau\u00b7ren", "Maul", ",", "das", "fer\u00b7tig", "war", "in", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "ADJD", "VAFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Versteh', in kalt und warm; er wuste, da\u00df zween Brei", "tokens": ["Ver\u00b7steh'", ",", "in", "kalt", "und", "warm", ";", "er", "wus\u00b7te", ",", "da\u00df", "zween", "Brei"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKVZ", "$,", "APPR", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "$,", "KOUS", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In einem Topf gekocht ein seltzam Fressen sei.", "tokens": ["In", "ei\u00b7nem", "Topf", "ge\u00b7kocht", "ein", "selt\u00b7zam", "Fres\u00b7sen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O, wie viel seglen noch hinweg mit allen Winden!", "tokens": ["O", ",", "wie", "viel", "seg\u00b7len", "noch", "hin\u00b7weg", "mit", "al\u00b7len", "Win\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PIS", "VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie viel, wie m\u00e4chtig viel sind hie und da zu finden,", "tokens": ["Wie", "viel", ",", "wie", "m\u00e4ch\u00b7tig", "viel", "sind", "hie", "und", "da", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWAV", "ADJD", "ADV", "VAFIN", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die krumm und h\u00f6ckricht sein, bald wiedrum gleich und schlecht;", "tokens": ["Die", "krumm", "und", "h\u00f6c\u00b7kricht", "sein", ",", "bald", "wie\u00b7drum", "gleich", "und", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAINF", "$,", "ADV", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den solcher leichten Bur\u00df sind alle S\u00e4ttel recht.", "tokens": ["Den", "sol\u00b7cher", "leich\u00b7ten", "Bur\u00df", "sind", "al\u00b7le", "S\u00e4t\u00b7tel", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}