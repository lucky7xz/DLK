{"dta.poem.645": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eines im Bette erstickten H\u00fcndgens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mir hat die Eyfersucht den Lebens-Drat zerschnitten/", "tokens": ["Mir", "hat", "die", "Ey\u00b7fer\u00b7sucht", "den", "Le\u00b7bens\u00b7Drat", "zer\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bette mu\u00df mein Tod/ und ich ersticket seyn/", "tokens": ["Ein", "Bet\u00b7te", "mu\u00df", "mein", "Tod", "/", "und", "ich", "er\u00b7sti\u00b7cket", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "$(", "KON", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn als ein altes Weib sah/ da\u00df ich mehr gelitten", "tokens": ["Denn", "als", "ein", "al\u00b7tes", "Weib", "sah", "/", "da\u00df", "ich", "mehr", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als sie war/ so must ich ins schwartze Grab hinein.", "tokens": ["Als", "sie", "war", "/", "so", "must", "ich", "ins", "schwart\u00b7ze", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$(", "ADV", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}