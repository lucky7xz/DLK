{"textgrid.poem.51132": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schifflein ruhn, und schimmernd ausgebreitet", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schifflein ruhn, und schimmernd ausgebreitet", "tokens": ["Die", "Schif\u00b7flein", "ruhn", ",", "und", "schim\u00b7mernd", "aus\u00b7ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erfreut das Auge der Gewebe Schwall;", "tokens": ["Er\u00b7freut", "das", "Au\u00b7ge", "der", "Ge\u00b7we\u00b7be", "Schwall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Hammer schweigt, doch mit dem Lichte streitet", "tokens": ["Der", "Ham\u00b7mer", "schweigt", ",", "doch", "mit", "dem", "Lich\u00b7te", "strei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In tausend Formen das Metall.", "tokens": ["In", "tau\u00b7send", "For\u00b7men", "das", "Me\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus tausend Stoffen hat Gestalt gewonnen,", "tokens": ["Aus", "tau\u00b7send", "Stof\u00b7fen", "hat", "Ge\u00b7stalt", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was Not und Lust der Welt ersonnen;", "tokens": ["Was", "Not", "und", "Lust", "der", "Welt", "er\u00b7son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit heil'gem Ernst, mit heiterm Tand", "tokens": ["Mit", "heil'\u00b7gem", "Ernst", ",", "mit", "hei\u00b7term", "Tand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umdr\u00e4ngt uns das Gebild der Hand.", "tokens": ["Um\u00b7dr\u00e4ngt", "uns", "das", "Ge\u00b7bild", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es will sich zeigen Wehr und Lehre", "tokens": ["Es", "will", "sich", "zei\u00b7gen", "Wehr", "und", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und er, der mit der Scholle ringt,", "tokens": ["Und", "er", ",", "der", "mit", "der", "Schol\u00b7le", "ringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.11": {"text": "Der Mann im Kampf um Brot und Ehre", "tokens": ["Der", "Mann", "im", "Kampf", "um", "Brot", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Des Feldes Frucht zum Feste bringt.", "tokens": ["Des", "Fel\u00b7des", "Frucht", "zum", "Fes\u00b7te", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Kr\u00e4fte, die da schliefen,", "tokens": ["Al\u00b7le", "Kr\u00e4f\u00b7te", ",", "die", "da", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden Flei\u00df, der schaffend wacht", "tokens": ["Je\u00b7den", "Flei\u00df", ",", "der", "schaf\u00b7fend", "wacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den H\u00f6hen, in den Tiefen,", "tokens": ["Auf", "den", "H\u00f6\u00b7hen", ",", "in", "den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehen wir zu Tag gebracht.", "tokens": ["Se\u00b7hen", "wir", "zu", "Tag", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein ganzes Volk will tagen,", "tokens": ["Und", "ein", "gan\u00b7zes", "Volk", "will", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind und J\u00fcngling, Mann und Frau", "tokens": ["Kind", "und", "J\u00fcng\u00b7ling", ",", "Mann", "und", "Frau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringen hoffend hergetragen", "tokens": ["Brin\u00b7gen", "hof\u00b7fend", "her\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer H\u00e4nde Werk zur Schau.", "tokens": ["Ih\u00b7rer", "H\u00e4n\u00b7de", "Werk", "zur", "Schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gro\u00dfe St\u00e4dte, Nationen", "tokens": ["Gro\u00b7\u00dfe", "St\u00e4d\u00b7te", ",", "Na\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eifern lang schon im Verein \u2013", "tokens": ["Ei\u00b7fern", "lang", "schon", "im", "Ver\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wo wir Kleinsten wohnen,", "tokens": ["A\u00b7ber", "wo", "wir", "Kleins\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf die M\u00fch nicht kleiner sein!", "tokens": ["Darf", "die", "M\u00fch", "nicht", "klei\u00b7ner", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleich st\u00fcrmender Wolken geschlossenen Scharen,", "tokens": ["Gleich", "st\u00fcr\u00b7men\u00b7der", "Wol\u00b7ken", "ge\u00b7schlos\u00b7se\u00b7nen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So reihn sich die V\u00f6lker und dr\u00e4ngen voran;", "tokens": ["So", "reihn", "sich", "die", "V\u00f6l\u00b7ker", "und", "dr\u00e4n\u00b7gen", "vo\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da gilt es zu stehn und sich regend zu wahren:", "tokens": ["Da", "gilt", "es", "zu", "stehn", "und", "sich", "re\u00b7gend", "zu", "wah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wer rastet, geht unter im Staube der Bahn!", "tokens": ["Wer", "ras\u00b7tet", ",", "geht", "un\u00b7ter", "im", "Stau\u00b7be", "der", "Bahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "In steter Bewegung ern\u00e4hrt sich die Kraft,", "tokens": ["In", "ste\u00b7ter", "Be\u00b7we\u00b7gung", "er\u00b7n\u00e4hrt", "sich", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Ruh liegt im Herzen dem Manne, der schafft!", "tokens": ["Die", "Ruh", "liegt", "im", "Her\u00b7zen", "dem", "Man\u00b7ne", ",", "der", "schafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Arbeit ist das w\u00e4rmste Hemde,", "tokens": ["Ar\u00b7beit", "ist", "das", "w\u00e4rms\u00b7te", "Hem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frischer Quell im W\u00fcstensand,", "tokens": ["Fri\u00b7scher", "Quell", "im", "W\u00fcs\u00b7ten\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stab und Zelt in weiter Fremde", "tokens": ["Stab", "und", "Zelt", "in", "wei\u00b7ter", "Frem\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das beste Heimatland!", "tokens": ["Und", "das", "bes\u00b7te", "Hei\u00b7mat\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Vaterland, ja du mu\u00dft siegen,", "tokens": ["Va\u00b7ter\u00b7land", ",", "ja", "du", "mu\u00dft", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Welt an Ehren gleich!", "tokens": ["Al\u00b7ler", "Welt", "an", "Eh\u00b7ren", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die Spreu von dannen fliegen \u2013", "tokens": ["La\u00df", "die", "Spreu", "von", "dan\u00b7nen", "flie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur durch Arbeit wirst du reich!", "tokens": ["Nur", "durch", "Ar\u00b7beit", "wirst", "du", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}