{"dta.poem.19869": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Des  \n  armen Suschens Traum .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte, wie um Mitternacht               ", "tokens": ["Ich", "tr\u00e4um\u00b7te", ",", "wie", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Falscher mir erschien.", "tokens": ["Mein", "Fal\u00b7scher", "mir", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fast schw\u00fcr\u2019 ich, da\u00df ich hell gewacht,", "tokens": ["Fast", "schw\u00fcr'", "ich", ",", "da\u00df", "ich", "hell", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hell erblikt\u2019 ich ihn.", "tokens": ["So", "hell", "er\u00b7blikt'", "ich", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er zog den Treuring von der Hand", "tokens": ["Er", "zog", "den", "Treu\u00b7ring", "von", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ach! zerbrach ihn mir.", "tokens": ["Und", "ach", "!", "zer\u00b7brach", "ihn", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein wasserhelles Perlenband", "tokens": ["Ein", "was\u00b7ser\u00b7hel\u00b7les", "Per\u00b7len\u00b7band"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warf er mir hin daf\u00fcr.", "tokens": ["Warf", "er", "mir", "hin", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Drauf ging ich wol ans Gartenbeet,", "tokens": ["Drauf", "ging", "ich", "wol", "ans", "Gar\u00b7ten\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu schaun mein Myrtenreis,", "tokens": ["Zu", "schaun", "mein", "Myr\u00b7ten\u00b7reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ich zum Kr\u00e4nzchen pflanzen th\u00e4t,", "tokens": ["Das", "ich", "zum", "Kr\u00e4nz\u00b7chen", "pflan\u00b7zen", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pflegen th\u00e4t mit Fleis.", "tokens": ["Und", "pfle\u00b7gen", "th\u00e4t", "mit", "Fleis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da ris entzwei mein Perlenband,", "tokens": ["Da", "ris", "ent\u00b7zwei", "mein", "Per\u00b7len\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eh ich\u2019s mich versah,", "tokens": ["Und", "eh", "ich's", "mich", "ver\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entrollten all\u2019 in Erd\u2019 und Sand,", "tokens": ["Ent\u00b7roll\u00b7ten", "all'", "in", "Erd'", "und", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine war mehr da.", "tokens": ["Und", "kei\u00b7ne", "war", "mehr", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich suchte wol mit Angst und Schweis;", "tokens": ["Ich", "such\u00b7te", "wol", "mit", "Angst", "und", "Schweis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fand keine mehr! Da schien", "tokens": ["Fand", "kei\u00b7ne", "mehr", "!", "Da", "schien"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "PIS", "$.", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verwandelt mein geliebtes Reis", "tokens": ["Ver\u00b7wan\u00b7delt", "mein", "ge\u00b7lieb\u00b7tes", "Reis"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dunkeln Rosmarin. \u2014", "tokens": ["In", "dun\u00b7keln", "Ros\u00b7ma\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erf\u00fclt ist l\u00e4ngst das Nachtgesicht,", "tokens": ["Er\u00b7f\u00fclt", "ist", "l\u00e4ngst", "das", "Nacht\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! l\u00e4ngst erf\u00fclt genau.", "tokens": ["Ach", "!", "l\u00e4ngst", "er\u00b7f\u00fclt", "ge\u00b7nau", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Traumbuch frag ich weiter nicht,", "tokens": ["Kein", "Traum\u00b7buch", "frag", "ich", "wei\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine weise Frau.", "tokens": ["Und", "kei\u00b7ne", "wei\u00b7se", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun brich, o Herz, der Ring ist hin!", "tokens": ["Nun", "brich", ",", "o", "Herz", ",", "der", "Ring", "ist", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Perlen sind geweint!", "tokens": ["Die", "Per\u00b7len", "sind", "ge\u00b7weint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stat Myrth\u2019 erwuchs dir Rosmarin!", "tokens": ["Stat", "Myr\u00b7th'", "er\u00b7wuchs", "dir", "Ros\u00b7ma\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der Traum hat Tod gemeint.", "tokens": ["Der", "Traum", "hat", "Tod", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Brich, armes Herz! Zur Todtenkron\u2019", "tokens": ["Brich", ",", "ar\u00b7mes", "Herz", "!", "Zur", "Tod\u00b7ten\u00b7kron'"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwuchs dir Rosmarin.", "tokens": ["Er\u00b7wuchs", "dir", "Ros\u00b7ma\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verweint sind deine Perlen schon.", "tokens": ["Ver\u00b7weint", "sind", "dei\u00b7ne", "Per\u00b7len", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ring, der Ring ist hin!", "tokens": ["Der", "Ring", ",", "der", "Ring", "ist", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}