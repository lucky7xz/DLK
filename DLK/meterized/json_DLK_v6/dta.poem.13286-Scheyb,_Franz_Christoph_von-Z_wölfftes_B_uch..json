{"dta.poem.13286": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum h\u00f6rt' ich diesen Saz, so fiel mir wieder bey", "tokens": ["Kaum", "h\u00f6rt'", "ich", "die\u00b7sen", "Saz", ",", "so", "fiel", "mir", "wie\u00b7der", "bey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ich schon gestern sang: da\u00df die\u00df der Anfang sey", "tokens": ["Was", "ich", "schon", "ge\u00b7stern", "sang", ":", "da\u00df", "die\u00df", "der", "An\u00b7fang", "sey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$.", "KOUS", "PDS", "ART", "NN", "VAFIN"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bey dem Thalia mich und meine Leyer st\u00f6rte,", "tokens": ["Bey", "dem", "Tha\u00b7lia", "mich", "und", "mei\u00b7ne", "Le\u00b7yer", "st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Als ich zum ersten Mahl derselben Zuruff h\u00f6rte.", "tokens": ["Als", "ich", "zum", "ers\u00b7ten", "Mahl", "der\u00b7sel\u00b7ben", "Zu\u00b7ruff", "h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}