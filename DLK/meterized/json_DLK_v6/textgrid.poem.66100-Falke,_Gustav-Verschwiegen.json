{"textgrid.poem.66100": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Verschwiegen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine liebliche Stunde", "tokens": ["Ei\u00b7ne", "lieb\u00b7li\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Stand vor mir, den Finger am Munde.", "tokens": ["Stand", "vor", "mir", ",", "den", "Fin\u00b7ger", "am", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Gro\u00dfe, klare Augen sagten", "tokens": ["Gro\u00b7\u00dfe", ",", "kla\u00b7re", "Au\u00b7gen", "sag\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Gedanken, die nicht hervor sich wagten.", "tokens": ["Von", "Ge\u00b7dan\u00b7ken", ",", "die", "nicht", "her\u00b7vor", "sich", "wag\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "PTKVZ", "PRF", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Rede nur, winkt ich, aber mit feinen", "tokens": ["Re\u00b7de", "nur", ",", "winkt", "ich", ",", "a\u00b7ber", "mit", "fei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "ADJA"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mienen wu\u00dfte sie zu verneinen.", "tokens": ["Mie\u00b7nen", "wu\u00df\u00b7te", "sie", "zu", "ver\u00b7nei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Stand nur immer und sah mich an,", "tokens": ["Stand", "nur", "im\u00b7mer", "und", "sah", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Eigen an,", "tokens": ["Ei\u00b7gen", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Mit dem Finger am Munde \u2013", "tokens": ["Mit", "dem", "Fin\u00b7ger", "am", "Mun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Eine liebliche, m\u00e4rchensch\u00f6ne Stunde.", "tokens": ["Ei\u00b7ne", "lieb\u00b7li\u00b7che", ",", "m\u00e4r\u00b7chen\u00b7sch\u00f6\u00b7ne", "Stun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}