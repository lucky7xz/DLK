{"dta.poem.1333": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Seel-Ewig/ la\u00df der Welt ihr Schw\u00e4rmen/ H\u00fcpffen/ Gei-\ngen/", "tokens": ["Seel\u00b7Ewig", "/", "la\u00df", "der", "Welt", "ihr", "Schw\u00e4r\u00b7men", "/", "H\u00fcpf\u00b7fen", "/", "Gei", "gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$(", "NN", "$(", "TRUNC", "APPR", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Du solt in stillem Geist mit mir den Oel-Berg steigen.", "tokens": ["Du", "solt", "in", "stil\u00b7lem", "Geist", "mit", "mir", "den", "O\u00b7el\u00b7Berg", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.3": {"text": "La\u00df jen\u2019 auff Rosen gehn/ tritt die bedornte Bahn.", "tokens": ["La\u00df", "jen'", "auff", "Ro\u00b7sen", "gehn", "/", "tritt", "die", "be\u00b7dorn\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "APPR", "NN", "VVINF", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie eilen unterwerts/ wir klimmen Himmel an.", "tokens": ["Sie", "ei\u00b7len", "un\u00b7ter\u00b7werts", "/", "wir", "klim\u00b7men", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was irrdisch ist gesinnt/ gedenckt auff lauter Freuden/", "tokens": ["Was", "irr\u00b7disch", "ist", "ge\u00b7sinnt", "/", "ge\u00b7denckt", "auff", "lau\u00b7ter", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "VVPP", "$(", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dein Erl\u00f6ser sagt von nichts als lauter Leyden.", "tokens": ["Und", "dein", "Er\u00b7l\u00f6\u00b7ser", "sagt", "von", "nichts", "als", "lau\u00b7ter", "Ley\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PIS", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gieb jene willig auff/ und folge diesem nach/", "tokens": ["Gieb", "je\u00b7ne", "wil\u00b7lig", "auff", "/", "und", "fol\u00b7ge", "die\u00b7sem", "nach", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "ADJD", "APPR", "$(", "KON", "VVFIN", "PDAT", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es f\u00fchrt kein ander Weg ins Himmels Lust-Gemach.", "tokens": ["Es", "f\u00fchrt", "kein", "an\u00b7der", "Weg", "ins", "Him\u00b7mels", "Lust\u00b7Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPRART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn F\u00fcllerey und Tantz das blinde Volck erhitzen/", "tokens": ["Wenn", "F\u00fcl\u00b7le\u00b7rey", "und", "Tantz", "das", "blin\u00b7de", "Volck", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schau du voller Blut den Lebens-F\u00fcrsten schwitzen.", "tokens": ["So", "schau", "du", "vol\u00b7ler", "Blut", "den", "Le\u00b7bens\u00b7F\u00fcrs\u00b7ten", "schwit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(des H\u00f6chsten Zorn-Kelch ist ihm voll geschencket ein/)", "tokens": ["(", "des", "H\u00f6chs\u00b7ten", "Zorn\u00b7Kelch", "ist", "ihm", "voll", "ge\u00b7schen\u00b7cket", "ein", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVFIN", "ART", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was kan dir lieblicher als diese Tropffen seyn.", "tokens": ["Was", "kan", "dir", "lieb\u00b7li\u00b7cher", "als", "die\u00b7se", "Tropf\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "KOKOM", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es jauchzt der nasse Mund mit \u00fcppigem Geth\u00f6ne;", "tokens": ["Es", "jauchzt", "der", "nas\u00b7se", "Mund", "mit", "\u00fcp\u00b7pi\u00b7gem", "Ge\u00b7th\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Heyland seuffzt/ damit er GOttes Grimm vers\u00f6hne;", "tokens": ["Dein", "Hey\u00b7land", "seuffzt", "/", "da\u00b7mit", "er", "Got\u00b7tes", "Grimm", "ver\u00b7s\u00f6h\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man bringt die lange Nacht in schnoder Kurtzweil zu/", "tokens": ["Man", "bringt", "die", "lan\u00b7ge", "Nacht", "in", "schno\u00b7der", "Kurt\u00b7zweil", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein JEsus hat f\u00fcr Angst und Feinden keine Ruh.", "tokens": ["Dein", "Je\u00b7sus", "hat", "f\u00fcr", "Angst", "und", "Fein\u00b7den", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "APPR", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es dient ein fremdes Kleid Gel\u00e4chter zu erwecken/", "tokens": ["Es", "dient", "ein", "frem\u00b7des", "Kleid", "Ge\u00b7l\u00e4ch\u00b7ter", "zu", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein JEsus lie\u00df sich auch mit fremden Lumpen decken/", "tokens": ["Dein", "Je\u00b7sus", "lie\u00df", "sich", "auch", "mit", "frem\u00b7den", "Lum\u00b7pen", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein Unschuld und Verdienst sey deiner Bl\u00f6sse Kleid/", "tokens": ["Sein", "Un\u00b7schuld", "und", "Ver\u00b7dienst", "sey", "dei\u00b7ner", "Bl\u00f6s\u00b7se", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und bringe dich geschm\u00fcckt ins Reich der Ewigkeit!", "tokens": ["Und", "brin\u00b7ge", "dich", "ge\u00b7schm\u00fcckt", "ins", "Reich", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}