{"textgrid.poem.48772": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "59. Auf Herrn Christian Schimlers sein Christi sanguine lotus sum", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie h\u00e4\u00dflich sahst du doch, du nun so reine Seele!", "tokens": ["Wie", "h\u00e4\u00df\u00b7lich", "sahst", "du", "doch", ",", "du", "nun", "so", "rei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du warest um und an ein arger Wust und Kot,", "tokens": ["Du", "wa\u00b7rest", "um", "und", "an", "ein", "ar\u00b7ger", "Wust", "und", "Kot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "KON", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um so viel desto mehr erb\u00e4rmlicher als tot,", "tokens": ["um", "so", "viel", "des\u00b7to", "mehr", "er\u00b7b\u00e4rm\u00b7li\u00b7cher", "als", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "je mehr du lebtest stets. Ach da\u00df ichs noch erz\u00e4hle!", "tokens": ["je", "mehr", "du", "leb\u00b7test", "stets", ".", "Ach", "da\u00df", "ichs", "noch", "er\u00b7z\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ADV", "$.", "ITJ", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir half kein Waschen nicht, kein Balsam, Salb' und Oele,", "tokens": ["Dir", "half", "kein", "Wa\u00b7schen", "nicht", ",", "kein", "Bal\u00b7sam", ",", "Salb'", "und", "O\u00b7e\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKNEG", "$,", "PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "bis dein Erl\u00f6ser kam und half dir aus der Not.", "tokens": ["bis", "dein", "Er\u00b7l\u00f6\u00b7ser", "kam", "und", "half", "dir", "aus", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er go\u00df sein Blut auf dich, das ihm so wei\u00df und rot", "tokens": ["Er", "go\u00df", "sein", "Blut", "auf", "dich", ",", "das", "ihm", "so", "wei\u00df", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit reichem Strome flo\u00df ans seiner Seiten H\u00f6le.", "tokens": ["mit", "rei\u00b7chem", "Stro\u00b7me", "flo\u00df", "ans", "sei\u00b7ner", "Sei\u00b7ten", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPRART", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gesegne dir dein Bad, der dich so sauber machte,", "tokens": ["Ge\u00b7seg\u00b7ne", "dir", "dein", "Bad", ",", "der", "dich", "so", "sau\u00b7ber", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NE", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "selbst Meister und selbst Bad. Indem ich di\u00df betrachte,", "tokens": ["selbst", "Meis\u00b7ter", "und", "selbst", "Bad", ".", "In\u00b7dem", "ich", "di\u00df", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NE", "$.", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "beschau ich mich in mir und werde himlisch ganz", "tokens": ["be\u00b7schau", "ich", "mich", "in", "mir", "und", "wer\u00b7de", "him\u00b7lisch", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "PRF", "APPR", "PPER", "KON", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "noch auf der Erden hier. Ihr englischen Gesichter,", "tokens": ["noch", "auf", "der", "Er\u00b7den", "hier", ".", "Ihr", "eng\u00b7li\u00b7schen", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verzeiht mirs, eure Zier hat weit nicht solchen Glanz.", "tokens": ["ver\u00b7zeiht", "mirs", ",", "eu\u00b7re", "Zier", "hat", "weit", "nicht", "sol\u00b7chen", "Glanz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel sie teurer ist, so viel ist meine lichter.", "tokens": ["So", "viel", "sie", "teu\u00b7rer", "ist", ",", "so", "viel", "ist", "mei\u00b7ne", "lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den j. Herbstmonatstag zu Astrachan in der Tartarei.", "tokens": ["Den", "j.", "Herbst\u00b7mo\u00b7nats\u00b7tag", "zu", "A\u00b7strac\u00b7han", "in", "der", "Tar\u00b7ta\u00b7rei", "."], "token_info": ["word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}