{"dta.poem.10687": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die gewaffnete Venus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Als Venus Helm vnd Schild hat ohn gefehr genommen/", "tokens": ["Als", "Ve\u00b7nus", "Helm", "vnd", "Schild", "hat", "ohn", "ge\u00b7fehr", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "KON", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach Pallas: streit mit mir/ jetzund mag Paris kommen/", "tokens": ["Sprach", "Pal\u00b7las", ":", "streit", "mit", "mir", "/", "je\u00b7tzund", "mag", "Pa\u00b7ris", "kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADV", "APPR", "PPER", "$(", "ADV", "VMFIN", "NE", "VVINF", "$("], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Venus sagt: ich darff kein Wassen gantz vnd gar/", "tokens": ["Die", "Ve\u00b7nus", "sagt", ":", "ich", "darff", "kein", "Was\u00b7sen", "gantz", "vnd", "gar", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "PIAT", "NN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ich dich vberwandt/ da ich doch nackend war.", "tokens": ["Weil", "ich", "dich", "vber\u00b7wandt", "/", "da", "ich", "doch", "na\u00b7ckend", "war", "."], "token_info": ["word", "word