{"textgrid.poem.38822": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Anfang war das Wort. Die ewgen Tiefen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Anfang war das Wort. Die ewgen Tiefen", "tokens": ["Im", "An\u00b7fang", "war", "das", "Wort", ".", "Die", "ew\u00b7gen", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entz\u00fcndeten sich br\u00fcnstig im Verlangen,", "tokens": ["Ent\u00b7z\u00fcn\u00b7de\u00b7ten", "sich", "br\u00fcns\u00b7tig", "im", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Die Liebe nahm das Wort in Lust gefangen,", "tokens": ["Die", "Lie\u00b7be", "nahm", "das", "Wort", "in", "Lust", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aufschlugen hell die Augen, welche schliefen,", "tokens": ["Auf\u00b7schlu\u00b7gen", "hell", "die", "Au\u00b7gen", ",", "wel\u00b7che", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sehns\u00fcchtge Angst, das Freudezittern, riefen", "tokens": ["Sehn\u00b7s\u00fccht\u00b7ge", "Angst", ",", "das", "Freu\u00b7de\u00b7zit\u00b7tern", ",", "rie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die seelgen Thr\u00e4nen auf die heilgen Wangen,", "tokens": ["Die", "seel\u00b7gen", "Thr\u00e4\u00b7nen", "auf", "die", "heil\u00b7gen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df alle Kr\u00e4fte wollustreich erklangen,", "tokens": ["Da\u00df", "al\u00b7le", "Kr\u00e4f\u00b7te", "wol\u00b7lust\u00b7reich", "er\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begierig, in sich selbst sich zu vertiefen.", "tokens": ["Be\u00b7gie\u00b7rig", ",", "in", "sich", "selbst", "sich", "zu", "ver\u00b7tie\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PRF", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da brachen sich die Leiden an den Freuden,", "tokens": ["Da", "bra\u00b7chen", "sich", "die", "Lei\u00b7den", "an", "den", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wonne suchte sich im stillen Innern,", "tokens": ["Die", "Won\u00b7ne", "such\u00b7te", "sich", "im", "stil\u00b7len", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Wort empfand die Engel, welche schufen;", "tokens": ["Das", "Wort", "emp\u00b7fand", "die", "En\u00b7gel", ",", "wel\u00b7che", "schu\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie gingen aus, entz\u00fcckend war ihr Scheiden.", "tokens": ["Sie", "gin\u00b7gen", "aus", ",", "ent\u00b7z\u00fc\u00b7ckend", "war", "ihr", "Schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf, Gottes Bildni\u00df, de\u00df dich zu erinnern", "tokens": ["Auf", ",", "Got\u00b7tes", "Bild\u00b7ni\u00df", ",", "de\u00df", "dich", "zu", "e\u00b7rin\u00b7nern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "NN", "$,", "ART", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernimm, wie meine heilgen T\u00f6ne rufen.", "tokens": ["Ver\u00b7nimm", ",", "wie", "mei\u00b7ne", "heil\u00b7gen", "T\u00f6\u00b7ne", "ru\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}