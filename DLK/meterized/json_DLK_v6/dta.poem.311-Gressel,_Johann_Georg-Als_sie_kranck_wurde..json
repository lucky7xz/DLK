{"dta.poem.311": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie kranck wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So sind die Stunden", "tokens": ["So", "sind", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Lust verschwunden", "tokens": ["Der", "Lust", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Sorgen-Nacht", "tokens": ["Die", "Sor\u00b7gen\u00b7Nacht"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bezieht mit Macht", "tokens": ["Be\u00b7zieht", "mit", "Macht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Den Himmel der Gedancken.", "tokens": ["Den", "Him\u00b7mel", "der", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An statt der Freuden", "tokens": ["An", "statt", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Findt sich jetzt Leyden/", "tokens": ["Findt", "sich", "jetzt", "Ley\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das Trauer-", "tokens": ["Das", "Trau\u00b7e\u00b7r"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Erschallt ins Ohr", "tokens": ["Er\u00b7schallt", "ins", "Ohr"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vor fr\u00f6lich Jubel-schreyen.", "tokens": ["Vor", "fr\u00f6\u00b7lich", "Ju\u00b7bel\u00b7schreyen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die weissen Br\u00fcste/", "tokens": ["Die", "weis\u00b7sen", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Platz der L\u00fcste/", "tokens": ["Ein", "Platz", "der", "L\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die seuffzen sehr/", "tokens": ["Die", "seuff\u00b7zen", "sehr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df sie nicht mehr", "tokens": ["Da\u00df", "sie", "nicht", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sich steiff auf schwellen k\u00f6nnen.", "tokens": ["Sich", "steiff", "auf", "schwel\u00b7len", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin betr\u00fcbet", "tokens": ["Ich", "bin", "be\u00b7tr\u00fc\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Weil die mich liebet", "tokens": ["Weil", "die", "mich", "lie\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch Kranckheit schwach/", "tokens": ["Durch", "Kran\u00b7ck\u00b7heit", "schwach", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da\u00df sie nicht mag", "tokens": ["Da\u00df", "sie", "nicht", "mag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch Anmuth mich erfreuen", "tokens": ["Durch", "An\u00b7muth", "mich", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So spielt das Gl\u00fccke", "tokens": ["So", "spielt", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durch falsche T\u00fccke", "tokens": ["Durch", "fal\u00b7sche", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df wenn man lacht", "tokens": ["Da\u00df", "wenn", "man", "lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es pl\u00f6tzlich macht", "tokens": ["Es", "pl\u00f6tz\u00b7lich", "macht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Augen \u00fcberfliessen.", "tokens": ["Die", "Au\u00b7gen", "\u00fc\u00b7berf\u00b7lies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die kurtze Wonne", "tokens": ["Die", "kurt\u00b7ze", "Won\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die gleicht der Sonne/", "tokens": ["Die", "gleicht", "der", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die bald vergeht", "tokens": ["Die", "bald", "ver\u00b7geht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und drauf entsteht", "tokens": ["Und", "drauf", "ent\u00b7steht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein hefftig Ungewitter.", "tokens": ["Ein", "heff\u00b7tig", "Un\u00b7ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es sind ", "tokens": ["Es", "sind"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Die mich ert\u00f6dten/", "tokens": ["Die", "mich", "er\u00b7t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und durch den Schmertz", "tokens": ["Und", "durch", "den", "Schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mein traurges Hertz", "tokens": ["Mein", "traur\u00b7ges", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zu Boden nieder reissen.", "tokens": ["Zu", "Bo\u00b7den", "nie\u00b7der", "reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wer kan wol lachen", "tokens": ["Wer", "kan", "wol", "la\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bey Donner-Krachen?", "tokens": ["Bey", "Don\u00b7ner\u00b7Kra\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Werd\u2019 ich sie sehn", "tokens": ["Werd'", "ich", "sie", "sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "VVINF"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Jm Sarge stehn/", "tokens": ["Jm", "Sar\u00b7ge", "stehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So mu\u00df ich gleichfals sterben.", "tokens": ["So", "mu\u00df", "ich", "gleich\u00b7fals", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}