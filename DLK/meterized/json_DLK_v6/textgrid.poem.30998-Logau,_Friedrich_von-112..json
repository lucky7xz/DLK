{"textgrid.poem.30998": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ich lebte, kunt ich Beine wol so hoch als Fleisch verkauffen;", "tokens": ["Weil", "ich", "leb\u00b7te", ",", "kunt", "ich", "Bei\u00b7ne", "wol", "so", "hoch", "als", "Fleisch", "ver\u00b7kauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "ADV", "ADV", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "W\u00fcrmen schenck ich ietzt, was fleischicht; Beine bleiben \u00fcberm Hauffen.", "tokens": ["W\u00fcr\u00b7men", "schenck", "ich", "ietzt", ",", "was", "flei\u00b7schicht", ";", "Bei\u00b7ne", "blei\u00b7ben", "\u00fc\u00b7berm", "Hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "PWS", "VVFIN", "$.", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}