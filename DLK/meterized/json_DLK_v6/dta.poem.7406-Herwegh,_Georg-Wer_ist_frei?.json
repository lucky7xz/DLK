{"dta.poem.7406": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Wer ist frei?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der ist allein ein freier Mann,", "tokens": ["Der", "ist", "al\u00b7lein", "ein", "frei\u00b7er", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seiner sei gedacht,", "tokens": ["Und", "sei\u00b7ner", "sei", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sie sich ", "tokens": ["Der", "sie", "sich"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Die Freiheit in der Schlacht,", "tokens": ["Die", "Frei\u00b7heit", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der mit der eignen Klinge", "tokens": ["Der", "mit", "der", "eig\u00b7nen", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie holt herbei,", "tokens": ["Sie", "holt", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Mann ist's, den ich singe,", "tokens": ["Der", "Mann", "ist's", ",", "den", "ich", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wehe, wer dem Franken traut", "tokens": ["O", "we\u00b7he", ",", "wer", "dem", "Fran\u00b7ken", "traut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PWS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihn zu froh begr\u00fc\u00dft;", "tokens": ["Und", "ihn", "zu", "froh", "be\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er bringt uns immer unsre Braut,", "tokens": ["Er", "bringt", "uns", "im\u00b7mer", "uns\u00b7re", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Er sie satt gek\u00fc\u00dft.", "tokens": ["Wenn", "Er", "sie", "satt", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch gibt's in ", "tokens": ["Noch", "gibt's", "in"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR"], "meter": "++-", "measure": "unknown.measure.di"}, "line.6": {"text": "Pulver und Blei \u2014", "tokens": ["Pul\u00b7ver", "und", "Blei"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Drum la\u00dft uns selber freien,", "tokens": ["Drum", "la\u00dft", "uns", "sel\u00b7ber", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So sind wir frei!", "tokens": ["So", "sind", "wir", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Freiheit wohnt am Don und Belt,", "tokens": ["Die", "Frei\u00b7heit", "wohnt", "am", "Don", "und", "Belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie trinkt aus unsrem Rhein,", "tokens": ["Sie", "trinkt", "aus", "uns\u00b7rem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freiheit schl\u00e4ft im W\u00fcstenzelt", "tokens": ["Die", "Frei\u00b7heit", "schl\u00e4ft", "im", "W\u00fcs\u00b7ten\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gl\u00e4nzt im Sternenschein;", "tokens": ["Und", "gl\u00e4nzt", "im", "Ster\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch mu\u00df man um sie werben,", "tokens": ["Doch", "mu\u00df", "man", "um", "sie", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo 's immer sei,", "tokens": ["Wo", "'s", "im\u00b7mer", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Doch mu\u00df man f\u00fcr sie sterben,", "tokens": ["Doch", "mu\u00df", "man", "f\u00fcr", "sie", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dann wird man frei!", "tokens": ["Dann", "wird", "man", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Noch hat der Deutsche eine Hand", "tokens": ["Noch", "hat", "der", "Deut\u00b7sche", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine starke Wehr,", "tokens": ["Und", "ei\u00b7ne", "star\u00b7ke", "Wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gibt keinen Schritt vom Vaterland", "tokens": ["Gibt", "kei\u00b7nen", "Schritt", "vom", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst f\u00fcr die Freiheit her;", "tokens": ["Selbst", "f\u00fcr", "die", "Frei\u00b7heit", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und die mit uns erheben", "tokens": ["Und", "die", "mit", "uns", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Solch Feldgeschrei,", "tokens": ["Solch", "Feld\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die sollen alle leben,", "tokens": ["Die", "sol\u00b7len", "al\u00b7le", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn sie sind frei!", "tokens": ["Denn", "sie", "sind", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Viel tausend Funken, Eine Glut,", "tokens": ["Viel", "tau\u00b7send", "Fun\u00b7ken", ",", "Ei\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Herzen und Ein Schlag,", "tokens": ["Viel", "Her\u00b7zen", "und", "Ein", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So harren wir gar wohlgemut", "tokens": ["So", "har\u00b7ren", "wir", "gar", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an den j\u00fcngsten Tag;", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Einheit mu\u00df verschlingen", "tokens": ["Die", "Ein\u00b7heit", "mu\u00df", "ver\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die b\u00f6se Zwei,", "tokens": ["Die", "b\u00f6\u00b7se", "Zwei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dann soll es donnernd klingen:", "tokens": ["Dann", "soll", "es", "don\u00b7nernd", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Deutschland ist frei!", "tokens": ["Deutschland", "ist", "frei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}