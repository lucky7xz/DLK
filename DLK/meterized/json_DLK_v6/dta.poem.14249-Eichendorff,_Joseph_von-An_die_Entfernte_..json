{"dta.poem.14249": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4\u00dft der Morgen von den H\u00f6hen", "tokens": ["L\u00e4\u00dft", "der", "Mor\u00b7gen", "von", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit die rothen Fahnen wehen,", "tokens": ["Weit", "die", "ro\u00b7then", "Fah\u00b7nen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiederhall in allen L\u00fcften,", "tokens": ["Wie\u00b7der\u00b7hall", "in", "al\u00b7len", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Losgerissen aus den Kl\u00fcften", "tokens": ["Los\u00b7ge\u00b7ris\u00b7sen", "aus", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Silberner die Str\u00f6me gehen:", "tokens": ["Sil\u00b7ber\u00b7ner", "die", "Str\u00f6\u00b7me", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Sp\u00fcrt der Mann die frischen Geister,", "tokens": ["Sp\u00fcrt", "der", "Mann", "die", "fri\u00b7schen", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drau\u00dfen auf dem Feld, zu Pferde", "tokens": ["Drau\u00b7\u00dfen", "auf", "dem", "Feld", ",", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Aengsten keck zerrei\u00dft er,", "tokens": ["Al\u00b7le", "A\u00b7engs\u00b7ten", "keck", "zer\u00b7rei\u00dft", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Dampfend unter ihm die Erde,", "tokens": ["Damp\u00b7fend", "un\u00b7ter", "ihm", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fchlt er hier sich Herr und Meister.", "tokens": ["F\u00fchlt", "er", "hier", "sich", "Herr", "und", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}