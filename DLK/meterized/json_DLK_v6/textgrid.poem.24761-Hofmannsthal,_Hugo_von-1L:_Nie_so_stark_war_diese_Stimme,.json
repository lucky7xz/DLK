{"textgrid.poem.24761": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nie so stark war diese Stimme,", "genre": "verse", "period": "N.A.", "pub_year": 1904, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie so stark war diese Stimme,", "tokens": ["Nie", "so", "stark", "war", "die\u00b7se", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie so funkelnd dieser Schrei!", "tokens": ["Nie", "so", "fun\u00b7kelnd", "die\u00b7ser", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ich einsam hier des Zauber-", "tokens": ["Da", "ich", "ein\u00b7sam", "hier", "des", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Berges dunklen Hang erklimme,", "tokens": ["Ber\u00b7ges", "dunk\u00b7len", "Hang", "er\u00b7klim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stumm durch \u00f6de Buchten schwimme,", "tokens": ["Stumm", "durch", "\u00f6\u00b7de", "Buch\u00b7ten", "schwim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stumm im Nebelfrost ergrimme,", "tokens": ["Stumm", "im", "Ne\u00b7bel\u00b7frost", "er\u00b7grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dr\u00f6hnend zieht es mich herbei.", "tokens": ["Dr\u00f6h\u00b7nend", "zieht", "es", "mich", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie so stark war dieses Rufen!", "tokens": ["Nie", "so", "stark", "war", "die\u00b7ses", "Ru\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt es vorw\u00e4rts? t\u00f6nts zur\u00fcck?", "tokens": ["T\u00f6nt", "es", "vor\u00b7w\u00e4rts", "?", "t\u00f6nts", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gedichte, die Gesichte,", "tokens": ["Die", "Ge\u00b7dich\u00b7te", ",", "die", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die wir schufen, die uns schufen,", "tokens": ["Die", "wir", "schu\u00b7fen", ",", "die", "uns", "schu\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dunkle Tore, dunkle Stufen,", "tokens": ["Dunk\u00b7le", "To\u00b7re", ",", "dunk\u00b7le", "Stu\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00f6hnen zauberhafter Hufen:", "tokens": ["Dr\u00f6h\u00b7nen", "zau\u00b7ber\u00b7haf\u00b7ter", "Hu\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hier; und nirgends Glut und Gl\u00fcck!", "tokens": ["Hier", ";", "und", "nir\u00b7gends", "Glut", "und", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}