{"textgrid.poem.48616": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ists der nicht, den wir so begehret,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists der nicht, den wir so begehret,", "tokens": ["Ists", "der", "nicht", ",", "den", "wir", "so", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "der oft gew\u00fcndschte liebe Tag?", "tokens": ["der", "oft", "ge\u00b7w\u00fcnd\u00b7schte", "lie\u00b7be", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, ja, er ists! Uns wird gew\u00e4hret,", "tokens": ["Ja", ",", "ja", ",", "er", "ists", "!", "Uns", "wird", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "was Ieder so zu hoffen pflag.", "tokens": ["was", "Ie\u00b7der", "so", "zu", "hof\u00b7fen", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, ja, er ists! Drum la\u00dft uns freuen!", "tokens": ["Ja", ",", "ja", ",", "er", "ists", "!", "Drum", "la\u00dft", "uns", "freu\u00b7en", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "$.", "PAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ists, der erste von dem Maien.", "tokens": ["Er", "ists", ",", "der", "ers\u00b7te", "von", "dem", "Mai\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Zephyr bult noch mit der Floren.", "tokens": ["Der", "Ze\u00b7phyr", "bult", "noch", "mit", "der", "Flo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Venus sucht Anchisen itzt,", "tokens": ["Die", "Ve\u00b7nus", "sucht", "An\u00b7chi\u00b7sen", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "ADV", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "der sich vorl\u00e4ngst von ihr verloren:", "tokens": ["der", "sich", "vor\u00b7l\u00e4ngst", "von", "ihr", "ver\u00b7lo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "schaut, wie die sch\u00f6ne Frau doch schwitzt!", "tokens": ["schaut", ",", "wie", "die", "sch\u00f6\u00b7ne", "Frau", "doch", "schwitzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "So viel' der Tropfen von ihr flie\u00dfen,", "tokens": ["So", "viel'", "der", "Trop\u00b7fen", "von", "ihr", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "so viel' sieht man der Blumen sprie\u00dfen.", "tokens": ["so", "viel'", "sieht", "man", "der", "Blu\u00b7men", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Herr, dieser Kranz wird nicht verwelken", "tokens": ["Herr", ",", "die\u00b7ser", "Kranz", "wird", "nicht", "ver\u00b7wel\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PDAT", "NN", "VAFIN", "PTKNEG", "VVINF"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "den wir euch winden in das Haar.", "tokens": ["den", "wir", "euch", "win\u00b7den", "in", "das", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Klee, kein Eiswig, keine Nelken,", "tokens": ["Kein", "Klee", ",", "kein", "Eis\u00b7wig", ",", "kei\u00b7ne", "Nel\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ganz keine von der Blumenschaar,", "tokens": ["ganz", "kei\u00b7ne", "von", "der", "Blu\u00b7men\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die kaum so lange tauren k\u00f6nnen,", "tokens": ["die", "kaum", "so", "lan\u00b7ge", "tau\u00b7ren", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die dienen euren gr\u00fcnen Sinnen.", "tokens": ["die", "die\u00b7nen", "eu\u00b7ren", "gr\u00fc\u00b7nen", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seid frisch am Geist' und auch am Leibe,", "tokens": ["Seid", "frisch", "am", "Geist'", "und", "auch", "am", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "seid gl\u00fcckhaft allzeit, wie ihr seid,", "tokens": ["seid", "gl\u00fcck\u00b7haft", "all\u00b7zeit", ",", "wie", "ihr", "seid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df man ink\u00fcnftig von euch schreibe:", "tokens": ["da\u00df", "man", "in\u00b7k\u00fcnf\u00b7tig", "von", "euch", "schrei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Man erfuhre ganz kein Leid!", "tokens": ["der", "Man", "er\u00b7fuh\u00b7re", "ganz", "kein", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was wir sonst mehr Gutes denken,", "tokens": ["Und", "was", "wir", "sonst", "mehr", "Gu\u00b7tes", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "das wird euch unser Himmel schenken.", "tokens": ["das", "wird", "euch", "un\u00b7ser", "Him\u00b7mel", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}