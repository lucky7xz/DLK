{"dta.poem.13621": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frisch auf !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df am Schreibtisch bleich und krumm,", "tokens": ["Ich", "sa\u00df", "am", "Schreib\u00b7tisch", "bleich", "und", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es war mir in meinem Kopf ganz dumm", "tokens": ["Es", "war", "mir", "in", "mei\u00b7nem", "Kopf", "ganz", "dumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor Dichten, wie ich alle die Sachen", "tokens": ["Vor", "Dich\u00b7ten", ",", "wie", "ich", "al\u00b7le", "die", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "PIS", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sollte auf's allerbeste machen.", "tokens": ["Soll\u00b7te", "auf's", "al\u00b7ler\u00b7bes\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da guckt am Fenster im Morgenlicht", "tokens": ["Da", "guckt", "am", "Fens\u00b7ter", "im", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Durch's Weinlaub ein wundersch\u00f6nes Gesicht,", "tokens": ["Durch's", "Wein\u00b7laub", "ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Guckt und lacht, kommt ganz herein", "tokens": ["Guckt", "und", "lacht", ",", "kommt", "ganz", "her\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "$,", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und kramt mir unter den Bl\u00e4ttern mein.", "tokens": ["Und", "kramt", "mir", "un\u00b7ter", "den", "Bl\u00e4t\u00b7tern", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich, ganz verwundert: \u201eIch sollt' dich kennen\u201c \u2014", "tokens": ["Ich", ",", "ganz", "ver\u00b7wun\u00b7dert", ":", "\u201e", "Ich", "sollt'", "dich", "ken\u00b7nen", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "ADV", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$(", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sie aber, statt ihren Namen zu nennen:", "tokens": ["Sie", "a\u00b7ber", ",", "statt", "ih\u00b7ren", "Na\u00b7men", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "\u201epfui in dem Schlafrock, siehst ja aus", "tokens": ["\u201e", "pfui", "in", "dem", "Schla\u00b7frock", ",", "siehst", "ja", "aus"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Wie ein verfallenes Schilderhaus!", "tokens": ["Wie", "ein", "ver\u00b7fal\u00b7le\u00b7nes", "Schil\u00b7der\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Willst du denn hier in der Tinte sitzen,", "tokens": ["Willst", "du", "denn", "hier", "in", "der", "Tin\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Schau, wie die Felder da drau\u00dfen blitzen!\u201c", "tokens": ["Schau", ",", "wie", "die", "Fel\u00b7der", "da", "drau\u00b7\u00dfen", "blit\u00b7zen", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "So dr\u00e4ngt sie mich fort unter Lachen und Streit,", "tokens": ["So", "dr\u00e4ngt", "sie", "mich", "fort", "un\u00b7ter", "La\u00b7chen", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Mir that's um die sch\u00f6ne Zeit nur Leid.", "tokens": ["Mir", "that's", "um", "die", "sch\u00f6\u00b7ne", "Zeit", "nur", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Drunten aber unter den B\u00e4umen", "tokens": ["Drun\u00b7ten", "a\u00b7ber", "un\u00b7ter", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Stand ein Ro\u00df mit funkelnden Z\u00e4umen.", "tokens": ["Stand", "ein", "Ro\u00df", "mit", "fun\u00b7keln\u00b7den", "Z\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Sie schwang sich lustig mit mir hinauf,", "tokens": ["Sie", "schwang", "sich", "lus\u00b7tig", "mit", "mir", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Die Sonne drau\u00dfen ging eben auf,", "tokens": ["Die", "Son\u00b7ne", "drau\u00b7\u00dfen", "ging", "e\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Und eh' ich mich konnte bedenken und fassen,", "tokens": ["Und", "eh'", "ich", "mich", "konn\u00b7te", "be\u00b7den\u00b7ken", "und", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.22": {"text": "Ritten wir rasch durch die stillen Gassen,", "tokens": ["Rit\u00b7ten", "wir", "rasch", "durch", "die", "stil\u00b7len", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Und als wir kamen vor die Stadt,", "tokens": ["Und", "als", "wir", "ka\u00b7men", "vor", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Das Ro\u00df auf einmal zwei Fl\u00fcgel hatt',", "tokens": ["Das", "Ro\u00df", "auf", "ein\u00b7mal", "zwei", "Fl\u00fc\u00b7gel", "hatt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Mir schauerte es recht durch alle Glieder:", "tokens": ["Mir", "schau\u00b7er\u00b7te", "es", "recht", "durch", "al\u00b7le", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "\u201emein Gott, ist's denn schon Fr\u00fchling wieder?\u201c \u2014", "tokens": ["\u201e", "mein", "Gott", ",", "ist's", "denn", "schon", "Fr\u00fch\u00b7ling", "wie\u00b7der", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ADV", "NN", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Sie aber wies mir, wie wir so zogen,", "tokens": ["Sie", "a\u00b7ber", "wies", "mir", ",", "wie", "wir", "so", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Die L\u00e4nder, die unten vor\u00fcberflogen,", "tokens": ["Die", "L\u00e4n\u00b7der", ",", "die", "un\u00b7ten", "vor\u00b7\u00fc\u00b7berf\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}