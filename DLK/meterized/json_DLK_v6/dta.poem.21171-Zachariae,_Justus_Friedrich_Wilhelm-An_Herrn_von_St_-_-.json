{"dta.poem.21171": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn von St - -", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "St - -, warum ietzt das gl\u00e4nzende Feld an der krieg-\nrischen Donau", "tokens": ["St", ",", "wa\u00b7rum", "ietzt", "das", "gl\u00e4n\u00b7zen\u00b7de", "Feld", "an", "der", "krieg", "ri\u00b7schen", "Do\u00b7nau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "$(", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "TRUNC", "ADJA", "NE"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Unter dem streifenden Ungar entflieht;", "tokens": ["Un\u00b7ter", "dem", "strei\u00b7fen\u00b7den", "Un\u00b7gar", "ent\u00b7flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Oder der eisengeharnischte Reuter, der wilde Pandure,", "tokens": ["O\u00b7der", "der", "ei\u00b7sen\u00b7ge\u00b7har\u00b7nischte", "Reu\u00b7ter", ",", "der", "wil\u00b7de", "Pan\u00b7du\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Zu der Jablunka Gebirge sich dr\u00e4ngt;", "tokens": ["Zu", "der", "Jab\u00b7lun\u00b7ka", "Ge\u00b7bir\u00b7ge", "sich", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Was geheim in der Seele der grosse Friedrich beschlies-", "tokens": ["Was", "ge\u00b7heim", "in", "der", "See\u00b7le", "der", "gros\u00b7se", "Fried\u00b7rich", "be\u00b7schlies"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NE", "TRUNC"], "meter": "--+--+--+----+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wenn er vor Legionen sich stellt,", "tokens": ["Wenn", "er", "vor", "Le\u00b7gi\u00b7o\u00b7nen", "sich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die, wie ein schweres Gewitter am langsam donnern-", "tokens": ["Die", ",", "wie", "ein", "schwe\u00b7res", "Ge\u00b7wit\u00b7ter", "am", "lang\u00b7sam", "don\u00b7nern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "ART", "ADJA", "NN", "PTKA", "ADJD", "TRUNC"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Schrecklich und dunkel zum Schlachtfelde ziehn;", "tokens": ["Schreck\u00b7lich", "und", "dun\u00b7kel", "zum", "Schlacht\u00b7fel\u00b7de", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "St - -, dies la\u00df uns nicht forschen. Wir brauchen zur", "tokens": ["St", ",", "dies", "la\u00df", "uns", "nicht", "for\u00b7schen", ".", "Wir", "brau\u00b7chen", "zur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "$(", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPER", "VVFIN", "APPRART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Oesterreichs Schwerdt nicht, nicht Galliens Heer.", "tokens": ["O\u00b7es\u00b7ter\u00b7reichs", "Schwerdt", "nicht", ",", "nicht", "Gal\u00b7li\u00b7ens", "Heer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "$,", "PTKNEG", "NN", "NN", "$."], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach! wie entflieht uns so schnell die leichte heitere Ju-", "tokens": ["Ach", "!", "wie", "ent\u00b7flieht", "uns", "so", "schnell", "die", "leich\u00b7te", "hei\u00b7te\u00b7re", "Ju"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mit ihr die Freude, die Liebe, der Scherz!", "tokens": ["Mit", "ihr", "die", "Freu\u00b7de", ",", "die", "Lie\u00b7be", ",", "der", "Scherz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Ph\u00f6be lachet nicht immer mit hellem Gesicht aus den", "tokens": ["Ph\u00f6\u00b7be", "la\u00b7chet", "nicht", "im\u00b7mer", "mit", "hel\u00b7lem", "Ge\u00b7sicht", "aus", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "APPR", "ART"], "meter": "+-+-+-+-+--+--", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jmmer nicht lacht uns der bl\u00fchende Lenz.", "tokens": ["Jm\u00b7mer", "nicht", "lacht", "uns", "der", "bl\u00fc\u00b7hen\u00b7de", "Lenz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wird nicht die Locke schon grau? La\u00df dann die Sorge", "tokens": ["Wird", "nicht", "die", "Lo\u00b7cke", "schon", "grau", "?", "La\u00df", "dann", "die", "Sor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$.", "VVIMP", "ADV", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und uns die Freude, den Freund, und den Wein.", "tokens": ["Und", "uns", "die", "Freu\u00b7de", ",", "den", "Freund", ",", "und", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Warum wollen wir nicht in laubichten Lindengew\u00f6lben,", "tokens": ["Wa\u00b7rum", "wol\u00b7len", "wir", "nicht", "in", "lau\u00b7bich\u00b7ten", "Lin\u00b7den\u00b7ge\u00b7w\u00f6l\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oder hier unter dem Ulmenbaum ruhn?", "tokens": ["O\u00b7der", "hier", "un\u00b7ter", "dem", "Ul\u00b7men\u00b7baum", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Uns mit Rosen bekr\u00e4nzen, und mit der Burgundischen", "tokens": ["Uns", "mit", "Ro\u00b7sen", "be\u00b7kr\u00e4n\u00b7zen", ",", "und", "mit", "der", "Bur\u00b7gun\u00b7di\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VVINF", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Weil wir noch leben, die Herzen erfreun?", "tokens": ["Weil", "wir", "noch", "le\u00b7ben", ",", "die", "Her\u00b7zen", "er\u00b7freun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Vor dem berauschenden Nektar entfliehen die nagenden", "tokens": ["Vor", "dem", "be\u00b7rau\u00b7schen\u00b7den", "Nek\u00b7tar", "ent\u00b7flie\u00b7hen", "die", "na\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+--+--+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch die verha\u00dfte Melancholey flieht.", "tokens": ["Auch", "die", "ver\u00b7ha\u00df\u00b7te", "Me\u00b7lan\u00b7cho\u00b7ley", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "K\u00fchl uns, o Knabe, den Wein in diesem silbernen", "tokens": ["K\u00fchl", "uns", ",", "o", "Kna\u00b7be", ",", "den", "Wein", "in", "die\u00b7sem", "sil\u00b7ber\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Welcher von schallenden Felsen sich gie\u00dft.", "tokens": ["Wel\u00b7cher", "von", "schal\u00b7len\u00b7den", "Fel\u00b7sen", "sich", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}