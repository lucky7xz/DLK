{"textgrid.poem.62214": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[des Abends letztes Gold]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Abends letztes Gold,", "tokens": ["Des", "A\u00b7bends", "letz\u00b7tes", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es spiegelt sich im Rhein,", "tokens": ["Es", "spie\u00b7gelt", "sich", "im", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Still kniet das M\u00e4gdelein", "tokens": ["Still", "kniet", "das", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Ufer, wunderhold!", "tokens": ["Am", "U\u00b7fer", ",", "wun\u00b7der\u00b7hold", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Haar, so licht wie Gold,", "tokens": ["Ihr", "Haar", ",", "so", "licht", "wie", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Aug' so himmelsrein,", "tokens": ["Ihr", "Aug'", "so", "him\u00b7mels\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was kniest Du so allein,", "tokens": ["Was", "kniest", "Du", "so", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Komm Maid, das Wetter grollt! \u2013", "tokens": ["Komm", "Maid", ",", "das", "Wet\u00b7ter", "grollt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Still winkt die Jungfrau mir:", "tokens": ["Still", "winkt", "die", "Jung\u00b7frau", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbein Opfer ruhet hier,", "tokens": ["\u00bb", "ein", "Op\u00b7fer", "ru\u00b7het", "hier", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einem Grab sind wir;\u00ab", "tokens": ["Auf", "ei\u00b7nem", "Grab", "sind", "wir", ";", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lieblosigkeit ist Mord, \u2013", "tokens": ["Lieb\u00b7lo\u00b7sig\u00b7keit", "ist", "Mord", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Entfliehe diesem Ort,", "tokens": ["Ent\u00b7flie\u00b7he", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sprich ein segnend Wort!", "tokens": ["Doch", "sprich", "ein", "seg\u00b7nend", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}