{"textgrid.poem.32813": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Crispus kauft und baut Pall\u00e4ste,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Crispus kauft und baut Pall\u00e4ste,", "tokens": ["Cris\u00b7pus", "kauft", "und", "baut", "Pal\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebet herrlich, gro\u00df und reich;", "tokens": ["Le\u00b7bet", "herr\u00b7lich", ",", "gro\u00df", "und", "reich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt Maitressen, feyert Feste,", "tokens": ["H\u00e4lt", "Mai\u00b7tres\u00b7sen", ",", "fe\u00b7yert", "Fes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und traktirt den F\u00fcrsten gleich:", "tokens": ["Und", "trak\u00b7tirt", "den", "F\u00fcrs\u00b7ten", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dennoch sagt die karge Stadt,", "tokens": ["Den\u00b7noch", "sagt", "die", "kar\u00b7ge", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er nicht viel \u00fcbrig hat.", "tokens": ["Da\u00df", "er", "nicht", "viel", "\u00fcb\u00b7rig", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mops z\u00e4hlt seinen Eheseegen", "tokens": ["Mops", "z\u00e4hlt", "sei\u00b7nen", "E\u00b7he\u00b7see\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns in funfzehn Kindern her;", "tokens": ["Uns", "in", "funf\u00b7zehn", "Kin\u00b7dern", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man r\u00fchmet allerwegen,", "tokens": ["Und", "man", "r\u00fch\u00b7met", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie kl\u00fcger sind, als Er:", "tokens": ["Da\u00df", "sie", "kl\u00fc\u00b7ger", "sind", ",", "als", "Er", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch sagt die b\u00f6se Stadt,", "tokens": ["Den\u00b7noch", "sagt", "die", "b\u00f6\u00b7se", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er sich verz\u00e4hlet hat.", "tokens": ["Da\u00df", "er", "sich", "ver\u00b7z\u00e4h\u00b7let", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Agnes schl\u00e4gt die Augen nieder,", "tokens": ["Ag\u00b7nes", "schl\u00e4gt", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hasset Tanz, Musik und Spiel,", "tokens": ["Has\u00b7set", "Tanz", ",", "Mu\u00b7sik", "und", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singet stets Bekehrungslieder,", "tokens": ["Sin\u00b7get", "stets", "Be\u00b7keh\u00b7rungs\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und h\u00e4lt auf den Cubach viel:", "tokens": ["Und", "h\u00e4lt", "auf", "den", "Cu\u00b7bach", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch sagt die verbuhlte Stadt,", "tokens": ["Doch", "sagt", "die", "ver\u00b7buhl\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df sie Fleisch und Blut noch hat.", "tokens": ["Da\u00df", "sie", "Fleisch", "und", "Blut", "noch", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ueberall verfolgt Selinden", "tokens": ["Ue\u00b7be\u00b7rall", "ver\u00b7folgt", "Se\u00b7lin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein gepudert Stutzerheer;", "tokens": ["Ein", "ge\u00b7pu\u00b7dert", "Stut\u00b7zer\u00b7heer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeden weis sie zu entz\u00fcnden,", "tokens": ["Je\u00b7den", "weis", "sie", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PTKVZ", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr wird kein Sieg zu schwer:", "tokens": ["Und", "ihr", "wird", "kein", "Sieg", "zu", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch sagt die h\u00f6hnsche Stadt,", "tokens": ["Den\u00b7noch", "sagt", "die", "h\u00f6hn\u00b7sche", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie keine Freyer hat.", "tokens": ["Da\u00df", "sie", "kei\u00b7ne", "Frey\u00b7er", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Stax beweist aus vielen F\u00e4llen", "tokens": ["Stax", "be\u00b7weist", "aus", "vie\u00b7len", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen g\u00f6ttlichen Verstand,", "tokens": ["Sei\u00b7nen", "g\u00f6tt\u00b7li\u00b7chen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus seinen Ehrenstellen", "tokens": ["Und", "aus", "sei\u00b7nen", "Eh\u00b7ren\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Verdienst ums Vaterland:", "tokens": ["Sein", "Ver\u00b7dienst", "ums", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch sagt die verme\u00dfne Stadt,", "tokens": ["Doch", "sagt", "die", "ver\u00b7me\u00df\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df er keins von beyden hat.", "tokens": ["Da\u00df", "er", "keins", "von", "bey\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "APPR", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit dem vollen Federhute", "tokens": ["Mit", "dem", "vol\u00b7len", "Fe\u00b7der\u00b7hu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Prangt der g\u00fcldne G\u00e4nserich,", "tokens": ["Prangt", "der", "g\u00fcld\u00b7ne", "G\u00e4n\u00b7se\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seinem edlen Blute", "tokens": ["Und", "von", "sei\u00b7nem", "ed\u00b7len", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberzeugt sein Wappen mich:", "tokens": ["Ue\u00b7ber\u00b7zeugt", "sein", "Wap\u00b7pen", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch sagt die plumpe Stadt,", "tokens": ["Den\u00b7noch", "sagt", "die", "plum\u00b7pe", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df kein Kutscher Ahnen hat.", "tokens": ["Da\u00df", "kein", "Kut\u00b7scher", "Ah\u00b7nen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Crantor richtet alle Sachen", "tokens": ["Cran\u00b7tor", "rich\u00b7tet", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der strengsten Billigkeit,", "tokens": ["Nach", "der", "strengs\u00b7ten", "Bil\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man wird ihn b\u00f6se machen,", "tokens": ["Und", "man", "wird", "ihn", "b\u00f6\u00b7se", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man ihm Geschenke beut:", "tokens": ["Wenn", "man", "ihm", "Ge\u00b7schen\u00b7ke", "beut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch sagt die verwegne Stadt,", "tokens": ["Doch", "sagt", "die", "ver\u00b7weg\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df er sie betrogen hat.", "tokens": ["Da\u00df", "er", "sie", "be\u00b7tro\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}