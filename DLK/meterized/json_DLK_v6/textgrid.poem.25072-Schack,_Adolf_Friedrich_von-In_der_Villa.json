{"textgrid.poem.25072": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "In der Villa", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Jahren, die mir tr\u00fcb geschwunden,", "tokens": ["Nach", "Jah\u00b7ren", ",", "die", "mir", "tr\u00fcb", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Neu trat ich in das Gartenthor,", "tokens": ["Neu", "trat", "ich", "in", "das", "Gar\u00b7ten\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder stiegen sel'ge Stunden,", "tokens": ["Und", "wie\u00b7der", "stie\u00b7gen", "sel'\u00b7ge", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier s\u00fc\u00df genossen, mir empor.", "tokens": ["Hier", "s\u00fc\u00df", "ge\u00b7nos\u00b7sen", ",", "mir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun \u00f6de und mit Spinngewebe", "tokens": ["Nun", "\u00f6\u00b7de", "und", "mit", "Spinn\u00b7ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hausaltane \u00fcberdeckt!", "tokens": ["Die", "Haus\u00b7al\u00b7ta\u00b7ne", "\u00fc\u00b7berd\u00b7eckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerfallen des Gel\u00e4nders St\u00e4be,", "tokens": ["Zer\u00b7fal\u00b7len", "des", "Ge\u00b7l\u00e4n\u00b7ders", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pfad in Unkraut tief versteckt!", "tokens": ["Der", "Pfad", "in", "Un\u00b7kraut", "tief", "ver\u00b7steckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich warf am Teich bei der Cypresse", "tokens": ["Ich", "warf", "am", "Teich", "bei", "der", "Cyp\u00b7res\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich nieder an den morschen Stamm,", "tokens": ["Mich", "nie\u00b7der", "an", "den", "mor\u00b7schen", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo neben mir in Leichenbl\u00e4sse", "tokens": ["Wo", "ne\u00b7ben", "mir", "in", "Lei\u00b7chen\u00b7bl\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond auf gelben Wellen schwamm;", "tokens": ["Der", "Mond", "auf", "gel\u00b7ben", "Wel\u00b7len", "schwamm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4hrend an des Fensters Gittern", "tokens": ["Und", "w\u00e4h\u00b7rend", "an", "des", "Fens\u00b7ters", "Git\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir festgebannt das Auge hing,", "tokens": ["Mir", "fest\u00b7ge\u00b7bannt", "das", "Au\u00b7ge", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt' ich, wie ein unheimlich Zittern", "tokens": ["H\u00f6rt'", "ich", ",", "wie", "ein", "un\u00b7heim\u00b7lich", "Zit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin l\u00e4ngs der \u00f6den Mauern ging.", "tokens": ["Hin", "l\u00e4ngs", "der", "\u00f6\u00b7den", "Mau\u00b7ern", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Auf den Balkon sah ich ", "tokens": ["Auf", "den", "Bal\u00b7kon", "sah", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihr Schleier weh'nd in Abendluft,", "tokens": ["Ihr", "Schlei\u00b7er", "weh'nd", "in", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rings quoll von den Gartenbeeten", "tokens": ["Und", "rings", "quoll", "von", "den", "Gar\u00b7ten\u00b7bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entgegen ihr ein matter Duft.", "tokens": ["Ent\u00b7ge\u00b7gen", "ihr", "ein", "mat\u00b7ter", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Halb wieder stieg aus der Font\u00e4ne", "tokens": ["Halb", "wie\u00b7der", "stieg", "aus", "der", "Fon\u00b7t\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der lang versiegte Wasserstrahl; \u2013", "tokens": ["Der", "lang", "ver\u00b7sieg\u00b7te", "Was\u00b7ser\u00b7strahl", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchlte, wie sich eine Thr\u00e4ne", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "wie", "sich", "ei\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meinem Auge bebend stahl.", "tokens": ["Aus", "mei\u00b7nem", "Au\u00b7ge", "be\u00b7bend", "stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bald wieder alles tot; mir starrten", "tokens": ["Bald", "wie\u00b7der", "al\u00b7les", "tot", ";", "mir", "starr\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADJD", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blicke noch zum Fenster bang,", "tokens": ["Die", "Bli\u00b7cke", "noch", "zum", "Fens\u00b7ter", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als in den w\u00fcst-verfallnen Garten", "tokens": ["Als", "in", "den", "w\u00fcst\u00b7ver\u00b7fall\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Morgens fahler Schimmer drang.", "tokens": ["Des", "Mor\u00b7gens", "fah\u00b7ler", "Schim\u00b7mer", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Einst Sitz von Wonnen ohnegleichen,", "tokens": ["Einst", "Sitz", "von", "Won\u00b7nen", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum \u00f6den Friedhof ward er nun!", "tokens": ["Zum", "\u00f6\u00b7den", "Fried\u00b7hof", "ward", "er", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum, mein Herz, noch \u00fcber Leichen", "tokens": ["Wa\u00b7rum", ",", "mein", "Herz", ",", "noch", "\u00fc\u00b7ber", "Lei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachtwandeln? \u2013 Geh auch du, zu ruhn!", "tokens": ["Nacht\u00b7wan\u00b7deln", "?", "\u2013", "Geh", "auch", "du", ",", "zu", "ruhn", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NE", "ADV", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}