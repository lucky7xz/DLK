{"textgrid.poem.59107": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "326. Liebe, des Gesetzes Erf\u00fcllung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch, der keine Liebe hat,", "tokens": ["Ein", "Mensch", ",", "der", "kei\u00b7ne", "Lie\u00b7be", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fragt viel nach Pflicht, Gebot und Rat;", "tokens": ["Fragt", "viel", "nach", "Pflicht", ",", "Ge\u00b7bot", "und", "Rat", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann Jesu Liebe dich durchflie\u00dfen,", "tokens": ["Kann", "Je\u00b7su", "Lie\u00b7be", "dich", "durch\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann tust du's alles ohne M\u00fcssen.", "tokens": ["Dann", "tust", "du's", "al\u00b7les", "oh\u00b7ne", "M\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}