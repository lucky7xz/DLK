{"textgrid.poem.41070": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das Verbrennen alter Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Mensch, ein faulend Aas,", "tokens": ["Wenn", "der", "Mensch", ",", "ein", "fau\u00b7lend", "Aas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieget unter Erd' und Gras,", "tokens": ["Lie\u00b7get", "un\u00b7ter", "Erd'", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In und auf ihm W\u00fcrmer, K\u00e4fer,", "tokens": ["In", "und", "auf", "ihm", "W\u00fcr\u00b7mer", ",", "K\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "KON", "APPR", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagen sie: Der m\u00fcde Schl\u00e4fer", "tokens": ["Sa\u00b7gen", "sie", ":", "Der", "m\u00fc\u00b7de", "Schl\u00e4\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruht nun s\u00fc\u00df im Erdenscho\u00df!", "tokens": ["Ruht", "nun", "s\u00fc\u00df", "im", "Er\u00b7den\u00b7scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich doch sage: herbes Los!", "tokens": ["Ich", "doch", "sa\u00b7ge", ":", "her\u00b7bes", "Los", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und die Leiche, die ins Meer", "tokens": ["Und", "die", "Lei\u00b7che", ",", "die", "ins", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Man gesenket, treibt umher", "tokens": ["Man", "ge\u00b7sen\u00b7ket", ",", "treibt", "um\u00b7her"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVPP", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Haien, Wasserschlangen,", "tokens": ["Un\u00b7ter", "Hai\u00b7en", ",", "Was\u00b7ser\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Magen sie empfangen.", "tokens": ["De\u00b7ren", "Ma\u00b7gen", "sie", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oben spricht ein dummer Mund:", "tokens": ["O\u00b7ben", "spricht", "ein", "dum\u00b7mer", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der ruht s\u00fc\u00df im stillen Grund!", "tokens": ["Der", "ruht", "s\u00fc\u00df", "im", "stil\u00b7len", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Abscheu auch der F\u00fcrstengruft,", "tokens": ["Ab\u00b7scheu", "auch", "der", "F\u00fcrs\u00b7ten\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ein Leib voll Moderduft", "tokens": ["Wo", "ein", "Leib", "voll", "Mo\u00b7der\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt gekr\u00f6nt im Sarkophage,", "tokens": ["Liegt", "ge\u00b7kr\u00f6nt", "im", "Sar\u00b7ko\u00b7pha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er noch am J\u00fcngsten Tage", "tokens": ["Da\u00df", "er", "noch", "am", "J\u00fcng\u00b7sten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Engeln Gottes Zeuge sei", "tokens": ["En\u00b7geln", "Got\u00b7tes", "Zeu\u00b7ge", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Menschlicher Alfanserei.", "tokens": ["Menschli\u00b7cher", "Al\u00b7fan\u00b7se\u00b7rei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Glaubt, am sch\u00f6nsten w\u00e4r' noch heut", "tokens": ["Glaubt", ",", "am", "sch\u00f6ns\u00b7ten", "w\u00e4r'", "noch", "heut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPRART", "ADJA", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Verbrennen alter Zeit,", "tokens": ["Das", "Ver\u00b7bren\u00b7nen", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuer l\u00e4\u00dft zur\u00fccke keine", "tokens": ["Feu\u00b7er", "l\u00e4\u00dft", "zu\u00b7r\u00fc\u00b7cke", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Totenk\u00f6pf' und Totenbeine,", "tokens": ["To\u00b7ten\u00b7k\u00f6pf'", "und", "To\u00b7ten\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was als Asche kam zur Welt,", "tokens": ["Was", "als", "A\u00b7sche", "kam", "zur", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flugs in Asche niederf\u00e4llt.", "tokens": ["Flugs", "in", "A\u00b7sche", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und zum Trotz dem kalten Tod", "tokens": ["Und", "zum", "Trotz", "dem", "kal\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00fcht ein hei\u00dfes Morgenrot,", "tokens": ["Gl\u00fcht", "ein", "hei\u00b7\u00dfes", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Solches tr\u00e4gt in Himmels L\u00fcfte", "tokens": ["Sol\u00b7ches", "tr\u00e4gt", "in", "Him\u00b7mels", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "\u00dcber Moder, \u00fcber Gr\u00fcfte", "tokens": ["\u00dc\u00b7ber", "Mo\u00b7der", ",", "\u00fc\u00b7ber", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Eines Menschen letzten Rest \u2013", "tokens": ["Ei\u00b7nes", "Men\u00b7schen", "letz\u00b7ten", "Rest", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Das ist Tod nicht, \u2013 ist ein Fest.", "tokens": ["Das", "ist", "Tod", "nicht", ",", "\u2013", "ist", "ein", "Fest", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PTKNEG", "$,", "$(", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}