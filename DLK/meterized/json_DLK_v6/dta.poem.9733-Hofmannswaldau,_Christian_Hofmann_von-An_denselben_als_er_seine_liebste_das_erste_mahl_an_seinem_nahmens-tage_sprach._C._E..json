{"dta.poem.9733": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An denselben/ als er seine liebste das erste  \n mahl an seinem nahmens-tage sprach.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wolgang/ der gang ist wohl und gl\u00fccklich unterfangen/", "tokens": ["Wol\u00b7gang", "/", "der", "gang", "ist", "wohl", "und", "gl\u00fcck\u00b7lich", "un\u00b7ter\u00b7fan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "VAFIN", "ADV", "KON", "ADJD", "VVPP", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da du das erste mahl zu deiner braut gegangen.", "tokens": ["Da", "du", "das", "ers\u00b7te", "mahl", "zu", "dei\u00b7ner", "braut", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach die zusammenkunfft war frembd und ungemein/", "tokens": ["Ach", "die", "zu\u00b7sam\u00b7men\u00b7kunfft", "war", "frembd", "und", "un\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du traffest einen tag/ der deinen nahmen f\u00fchrte/", "tokens": ["Du", "traf\u00b7fest", "ei\u00b7nen", "tag", "/", "der", "dei\u00b7nen", "nah\u00b7men", "f\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wie der himmel selbst das gantze spiel regierte/", "tokens": ["Und", "wie", "der", "him\u00b7mel", "selbst", "das", "gant\u00b7ze", "spiel", "re\u00b7gier\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So muste dieser tag auch gar ein Freytag seyn.", "tokens": ["So", "mus\u00b7te", "die\u00b7ser", "tag", "auch", "gar", "ein", "Frey\u00b7tag", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Welch ein geheimni\u00df l\u00e4st in diesem fall sich finden?", "tokens": ["Welch", "ein", "ge\u00b7heim\u00b7ni\u00df", "l\u00e4st", "in", "die\u00b7sem", "fall", "sich", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du soltest dich mit ihr befreyen und verbinden/", "tokens": ["Du", "sol\u00b7test", "dich", "mit", "ihr", "be\u00b7fre\u00b7yen", "und", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der au\u00dfgang w\u00fcrde wol und gut von statten gehn.", "tokens": ["Der", "au\u00df\u00b7gang", "w\u00fcr\u00b7de", "wol", "und", "gut", "von", "stat\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "ADJD", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ja das verh\u00e4ngnis hat dein nahmens-fest erkohren/", "tokens": ["Ja", "das", "ver\u00b7h\u00e4ng\u00b7nis", "hat", "dein", "nah\u00b7mens\u00b7fest", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Damit der schlu\u00df gewi\u00df und sicher k\u00f6nte stehn:", "tokens": ["Da\u00b7mit", "der", "schlu\u00df", "ge\u00b7wi\u00df", "und", "si\u00b7cher", "k\u00f6n\u00b7te", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du seyst f\u00fcr sie benennt/ wie sie f\u00fcr dich gebohren.", "tokens": ["Du", "seyst", "f\u00fcr", "sie", "be\u00b7nennt", "/", "wie", "sie", "f\u00fcr", "dich", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "$(", "PWAV", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}