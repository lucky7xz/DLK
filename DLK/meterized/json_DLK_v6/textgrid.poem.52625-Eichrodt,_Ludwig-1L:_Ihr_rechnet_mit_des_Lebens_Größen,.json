{"textgrid.poem.52625": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr rechnet mit des Lebens Gr\u00f6\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr rechnet mit des Lebens Gr\u00f6\u00dfen,", "tokens": ["Ihr", "rech\u00b7net", "mit", "des", "Le\u00b7bens", "Gr\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihr gelanget nur zum Schmerz.", "tokens": ["Und", "ihr", "ge\u00b7lan\u00b7get", "nur", "zum", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An unvermeidlichen Verst\u00f6\u00dfen", "tokens": ["An", "un\u00b7ver\u00b7meid\u00b7li\u00b7chen", "Ver\u00b7st\u00f6\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verblutet bald des Menschen Herz.", "tokens": ["Ver\u00b7blu\u00b7tet", "bald", "des", "Men\u00b7schen", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was bleibt ihm \u00fcbrig, als zu fl\u00fcchten", "tokens": ["Was", "bleibt", "ihm", "\u00fcb\u00b7rig", ",", "als", "zu", "fl\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In das Mysterium des Weins?", "tokens": ["In", "das", "Mys\u00b7te\u00b7ri\u00b7um", "des", "Weins", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Mit Seelengr\u00f6\u00dfe zu verzichten,", "tokens": ["Mit", "See\u00b7len\u00b7gr\u00f6\u00b7\u00dfe", "zu", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beim goldnen Flascheneinmaleins.", "tokens": ["Beim", "gold\u00b7nen", "Fla\u00b7schen\u00b7ein\u00b7mal\u00b7eins", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Freunde m\u00fcssen uns verlassen,", "tokens": ["Die", "Freun\u00b7de", "m\u00fcs\u00b7sen", "uns", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schicksal treibt sie alle fort;", "tokens": ["Das", "Schick\u00b7sal", "treibt", "sie", "al\u00b7le", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Menschen, die den Wein nicht hassen,", "tokens": ["Doch", "Men\u00b7schen", ",", "die", "den", "Wein", "nicht", "has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat jede Zeit, hat jeder Ort.", "tokens": ["Hat", "je\u00b7de", "Zeit", ",", "hat", "je\u00b7der", "Ort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn die Menschheit selber gl\u00fccklich", "tokens": ["Und", "wenn", "die", "Menschheit", "sel\u00b7ber", "gl\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sich heute nicht zu nennen wagt,", "tokens": ["Sich", "heu\u00b7te", "nicht", "zu", "nen\u00b7nen", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ist das Wort schon unerquicklich,", "tokens": ["So", "ist", "das", "Wort", "schon", "un\u00b7er\u00b7quick\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das auch den Becher mir verklagt.", "tokens": ["Das", "auch", "den", "Be\u00b7cher", "mir", "ver\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was war dem Weisesten der Weisen", "tokens": ["Was", "war", "dem", "Wei\u00b7ses\u00b7ten", "der", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Werth der ganzen Wissenschaft?", "tokens": ["Der", "Werth", "der", "gan\u00b7zen", "Wis\u00b7sen\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Tod ein Trinker noch zu hei\u00dfen,", "tokens": ["Im", "Tod", "ein", "Trin\u00b7ker", "noch", "zu", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trank er mit Lust den Schierlingssaft.", "tokens": ["Trank", "er", "mit", "Lust", "den", "Schier\u00b7lings\u00b7saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und Noah, der schon am Ertrinken,", "tokens": ["Und", "Noah", ",", "der", "schon", "am", "Er\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Blieb doch dem Trinken freundlich nur;", "tokens": ["Blieb", "doch", "dem", "Trin\u00b7ken", "freund\u00b7lich", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch Doktor Luther, will mich d\u00fcnken,", "tokens": ["Auch", "Dok\u00b7tor", "Lu\u00b7ther", ",", "will", "mich", "d\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "$,", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Begriff des Kelches Heilnatur.", "tokens": ["Be\u00b7griff", "des", "Kel\u00b7ches", "Heil\u00b7na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vergebens singen wir die Erben", "tokens": ["Ver\u00b7ge\u00b7bens", "sin\u00b7gen", "wir", "die", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sklaverei im Liebesnetz,", "tokens": ["Der", "Skla\u00b7ve\u00b7rei", "im", "Lie\u00b7bes\u00b7netz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe aber kann auch sterben,", "tokens": ["Die", "Lie\u00b7be", "a\u00b7ber", "kann", "auch", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und untreu werden kann sie stets.", "tokens": ["Und", "un\u00b7treu", "wer\u00b7den", "kann", "sie", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Becher k\u00f6nnen sie nicht nehmen,", "tokens": ["Den", "Be\u00b7cher", "k\u00f6n\u00b7nen", "sie", "nicht", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lang dies Ich sich nicht verliert,", "tokens": ["So", "lang", "dies", "Ich", "sich", "nicht", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lang sie diesen Geist nicht l\u00e4hmen,", "tokens": ["So", "lang", "sie", "die\u00b7sen", "Geist", "nicht", "l\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dieser Leib nicht m\u00fcde wird!", "tokens": ["Und", "die\u00b7ser", "Leib", "nicht", "m\u00fc\u00b7de", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bleibt mir der Becher nur, der volle,", "tokens": ["Bleibt", "mir", "der", "Be\u00b7cher", "nur", ",", "der", "vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So mi\u00df ich keinen Erdenreiz,", "tokens": ["So", "mi\u00df", "ich", "kei\u00b7nen", "Er\u00b7den\u00b7reiz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fluch ich jedem eitlen Grolle,", "tokens": ["So", "fluch", "ich", "je\u00b7dem", "eit\u00b7len", "Grol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So spott ich alles eitlen Leids.", "tokens": ["So", "spott", "ich", "al\u00b7les", "eit\u00b7len", "Leids", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gelassen will ich stets erscheinen,", "tokens": ["Ge\u00b7las\u00b7sen", "will", "ich", "stets", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn jedes Auge feucht erscheint,", "tokens": ["Wenn", "je\u00b7des", "Au\u00b7ge", "feucht", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nur noch mit der Rebe weinen,", "tokens": ["Und", "nur", "noch", "mit", "der", "Re\u00b7be", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die \u00e4chte Freudenthr\u00e4nen weint!", "tokens": ["Die", "\u00e4ch\u00b7te", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "weint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}