{"textgrid.poem.63669": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod im Baum", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Nebelduft am Stra\u00dfensaum", "tokens": ["Im", "Ne\u00b7bel\u00b7duft", "am", "Stra\u00b7\u00dfen\u00b7saum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da steht ein Ebereschenbaum.", "tokens": ["Da", "steht", "ein", "E\u00b7be\u00b7re\u00b7schen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fr\u00fcchte schimmern blutigrot,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "schim\u00b7mern", "blu\u00b7ti\u00b7grot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im kahlen Wipfel hockt der Tod.", "tokens": ["Im", "kah\u00b7len", "Wip\u00b7fel", "hockt", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Fiedel h\u00e4lt die Knochenhand,", "tokens": ["Die", "Fie\u00b7del", "h\u00e4lt", "die", "Kno\u00b7chen\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Menschensehnen bleich bespannt.", "tokens": ["Mit", "Men\u00b7schen\u00b7seh\u00b7nen", "bleich", "be\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sch\u00e4del, der wie Silber gl\u00e4nzt,", "tokens": ["Den", "Sch\u00e4\u00b7del", ",", "der", "wie", "Sil\u00b7ber", "gl\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kranz von Vogelbeeren kr\u00e4nzt.", "tokens": ["Ein", "Kranz", "von", "Vo\u00b7gel\u00b7bee\u00b7ren", "kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Kiefer blank die Z\u00e4hne zeigt,", "tokens": ["Der", "Kie\u00b7fer", "blank", "die", "Z\u00e4h\u00b7ne", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er grinst vergn\u00fcgt und singt und geigt.", "tokens": ["Er", "grinst", "ver\u00b7gn\u00fcgt", "und", "singt", "und", "geigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "KON", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus schwarzer Ackerfurch' zu hauf", "tokens": ["Aus", "schwar\u00b7zer", "A\u00b7cke\u00b7rfurch'", "zu", "hauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schwarm von Kr\u00e4hen flattert auf.", "tokens": ["Ein", "Schwarm", "von", "Kr\u00e4\u00b7hen", "flat\u00b7tert", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Singsang des Gerippleins gellt:", "tokens": ["Der", "Sings\u00b7ang", "des", "Ge\u00b7rip\u00b7pleins", "gellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnun bist du mein, du weite Welt!", "tokens": ["\u00bb", "nun", "bist", "du", "mein", ",", "du", "wei\u00b7te", "Welt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPOSAT", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schwarzen V\u00f6gel h\u00f6r' ich schrein,", "tokens": ["Die", "schwar\u00b7zen", "V\u00f6\u00b7gel", "h\u00f6r'", "ich", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sollt die Totengr\u00e4ber sein.", "tokens": ["Ihr", "sollt", "die", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was je gebl\u00fcht, was je gelacht,", "tokens": ["Was", "je", "ge\u00b7bl\u00fcht", ",", "was", "je", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird nun ins kalte Grab gebracht.", "tokens": ["Wird", "nun", "ins", "kal\u00b7te", "Grab", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ringsum liegt tot und stumm \u2013", "tokens": ["Die", "Welt", "ring\u00b7sum", "liegt", "tot", "und", "stumm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was aber klingt dort f\u00fcr Gesumm?\u00ab", "tokens": ["Was", "a\u00b7ber", "klingt", "dort", "f\u00fcr", "Ge\u00b7summ", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein B\u00fcblein kommt des Wegs daher,", "tokens": ["Ein", "B\u00fcb\u00b7lein", "kommt", "des", "Wegs", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Schule tr\u00e4gt's sein R\u00e4nzel schwer.", "tokens": ["Zur", "Schu\u00b7le", "tr\u00e4gt's", "sein", "R\u00e4n\u00b7zel", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ostwind pfeift ihm ins Gesicht,", "tokens": ["Der", "Ost\u00b7wind", "pfeift", "ihm", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kleinen Mann bek\u00fcmmert's nicht.", "tokens": ["Den", "klei\u00b7nen", "Mann", "be\u00b7k\u00fcm\u00b7mert's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und wie er tapfer f\u00fcrba\u00df zieht,", "tokens": ["Und", "wie", "er", "tap\u00b7fer", "f\u00fcr\u00b7ba\u00df", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er summt ein lieblich Weihnachtslied.", "tokens": ["Er", "summt", "ein", "lieb\u00b7lich", "Weih\u00b7nachts\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tod im Baume lauscht voll Grimm,", "tokens": ["Der", "Tod", "im", "Bau\u00b7me", "lauscht", "voll", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADJD", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht schweigen gern die Kinderstimm'.", "tokens": ["M\u00f6cht", "schwei\u00b7gen", "gern", "die", "Kin\u00b7der\u00b7stimm'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er wirft den Kranz ihm an den Kopf,", "tokens": ["Er", "wirft", "den", "Kranz", "ihm", "an", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da lacht hinauf der muntre Tropf:", "tokens": ["Da", "lacht", "hin\u00b7auf", "der", "mun\u00b7tre", "Tropf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sch\u00f6ne Kr\u00e4nzel heb' ich auf! \u2013", "tokens": ["Das", "sch\u00f6\u00b7ne", "Kr\u00e4n\u00b7zel", "heb'", "ich", "auf", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schrein entschwirrt der Kr\u00e4henhauf.", "tokens": ["Mit", "Schrein", "ent\u00b7schwirrt", "der", "Kr\u00e4\u00b7hen\u00b7hauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}