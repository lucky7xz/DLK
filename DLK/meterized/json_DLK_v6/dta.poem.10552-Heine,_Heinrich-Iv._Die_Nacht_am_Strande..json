{"dta.poem.10552": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Die Nacht am Strande.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sternlos und kalt ist die Nacht,", "tokens": ["Stern\u00b7los", "und", "kalt", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Es g\u00e4hrt das Meer;", "tokens": ["Es", "g\u00e4hrt", "das", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und \u00fcber dem Meer', platt auf dem Bauch,", "tokens": ["Und", "\u00fc\u00b7ber", "dem", "Meer'", ",", "platt", "auf", "dem", "Bauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUI", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Liegt der ungestaltete Nordwind,", "tokens": ["Liegt", "der", "un\u00b7ge\u00b7stal\u00b7te\u00b7te", "Nord\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und heimlich, mit \u00e4chzend ged\u00e4mpfter Stimme,", "tokens": ["Und", "heim\u00b7lich", ",", "mit", "\u00e4ch\u00b7zend", "ge\u00b7d\u00e4mpf\u00b7ter", "Stim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wie'n st\u00f6rriger Griesgram, der gutgelaunt wird,", "tokens": ["Wie'n", "st\u00f6r\u00b7ri\u00b7ger", "Gries\u00b7gram", ",", "der", "gut\u00b7ge\u00b7launt", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Schwatzt er in's Wasser hinein,", "tokens": ["Schwatzt", "er", "in's", "Was\u00b7ser", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Und erz\u00e4hlt viel tolle Geschichten,", "tokens": ["Und", "er\u00b7z\u00e4hlt", "viel", "tol\u00b7le", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Riesenm\u00e4hrchen, todtschlaglaunig,", "tokens": ["Rie\u00b7sen\u00b7m\u00e4hr\u00b7chen", ",", "todt\u00b7schlag\u00b7lau\u00b7nig", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Uralte Sagen aus Norweg,", "tokens": ["Ur\u00b7al\u00b7te", "Sa\u00b7gen", "aus", "Nor\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.11": {"text": "Und dazwischen, weitschallend, lacht er und heult er", "tokens": ["Und", "da\u00b7zwi\u00b7schen", ",", "weit\u00b7schal\u00b7lend", ",", "lacht", "er", "und", "heult", "er"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "$,", "VVPP", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Beschw\u00f6rungslieder der Edda,", "tokens": ["Be\u00b7schw\u00f6\u00b7rungs\u00b7lie\u00b7der", "der", "Ed\u00b7da", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Graue Runenspr\u00fcche,", "tokens": ["Grau\u00b7e", "Ru\u00b7nen\u00b7spr\u00fc\u00b7che", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "So dunkeltrotzig und zaubergewaltig,", "tokens": ["So", "dun\u00b7kel\u00b7trot\u00b7zig", "und", "zau\u00b7ber\u00b7ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Da\u00df die wei\u00dfen Meerkinder", "tokens": ["Da\u00df", "die", "wei\u00b7\u00dfen", "Me\u00b7er\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hochaufspringen und jauchzen,", "tokens": ["Hoch\u00b7auf\u00b7sprin\u00b7gen", "und", "jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.17": {"text": "Uebermuth-berauscht.", "tokens": ["Ue\u00b7ber\u00b7muth\u00b7be\u00b7rauscht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Derweilen, am flachen Gestade,", "tokens": ["Der\u00b7wei\u00b7len", ",", "am", "fla\u00b7chen", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ueber den fluthbefeuchteten Sand,", "tokens": ["Ue\u00b7ber", "den", "fluth\u00b7be\u00b7feuch\u00b7te\u00b7ten", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schreitet ein Fremdling, mit einem Herzen,", "tokens": ["Schrei\u00b7tet", "ein", "Fremd\u00b7ling", ",", "mit", "ei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das wilder noch als Wind und Wellen;", "tokens": ["Das", "wil\u00b7der", "noch", "als", "Wind", "und", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo es hintritt,", "tokens": ["Wo", "es", "hin\u00b7tritt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Spr\u00fchen Funken und knistern die Muscheln,", "tokens": ["Spr\u00fc\u00b7hen", "Fun\u00b7ken", "und", "knis\u00b7tern", "die", "Mu\u00b7scheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Und er h\u00fcllt sich fest in den grauen Mantel,", "tokens": ["Und", "er", "h\u00fcllt", "sich", "fest", "in", "den", "grau\u00b7en", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schreitet rasch durch die wehende Nacht;", "tokens": ["Und", "schrei\u00b7tet", "rasch", "durch", "die", "we\u00b7hen\u00b7de", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sicher geleitet vom kleinen Lichte,", "tokens": ["Si\u00b7cher", "ge\u00b7lei\u00b7tet", "vom", "klei\u00b7nen", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Das lockend und lieblich schimmert", "tokens": ["Das", "lo\u00b7ckend", "und", "lieb\u00b7lich", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVPP", "KON", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Aus einsamer Fischerh\u00fctte.", "tokens": ["Aus", "ein\u00b7sa\u00b7mer", "Fi\u00b7scher\u00b7h\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vater und Bruder sind auf der See,", "tokens": ["Va\u00b7ter", "und", "Bru\u00b7der", "sind", "auf", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und mutterseelallein blieb dort", "tokens": ["Und", "mut\u00b7ter\u00b7see\u00b7lal\u00b7lein", "blieb", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der H\u00fctte die Fischertochter,", "tokens": ["In", "der", "H\u00fct\u00b7te", "die", "Fi\u00b7scher\u00b7toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die wundersch\u00f6ne Fischertochter.", "tokens": ["Die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Fi\u00b7scher\u00b7toch\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Am Heerde sitzt sie", "tokens": ["Am", "Heer\u00b7de", "sitzt", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und horcht auf des Wasserkessels", "tokens": ["Und", "horcht", "auf", "des", "Was\u00b7ser\u00b7kes\u00b7sels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ahnungss\u00fc\u00dfes, heimliches Summen,", "tokens": ["Ah\u00b7nungs\u00b7s\u00fc\u00b7\u00dfes", ",", "heim\u00b7li\u00b7ches", "Sum\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und sch\u00fcttet knisterndes Reisig in's Feuer,", "tokens": ["Und", "sch\u00fct\u00b7tet", "knis\u00b7tern\u00b7des", "Rei\u00b7sig", "in's", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und bl\u00e4\u00dft hinein,", "tokens": ["Und", "bl\u00e4\u00dft", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Da\u00df die flackernd rothen Lichter", "tokens": ["Da\u00df", "die", "fla\u00b7ckernd", "ro\u00b7then", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Zauberlieblich wiederstrahlen", "tokens": ["Zau\u00b7berl\u00b7ieb\u00b7lich", "wie\u00b7der\u00b7strah\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf das bl\u00fchende Antlitz,", "tokens": ["Auf", "das", "bl\u00fc\u00b7hen\u00b7de", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Auf die zarte, wei\u00dfe Schulter,", "tokens": ["Auf", "die", "zar\u00b7te", ",", "wei\u00b7\u00dfe", "Schul\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die r\u00fchrend hervorlauscht", "tokens": ["Die", "r\u00fch\u00b7rend", "her\u00b7vor\u00b7lauscht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aus dem groben, grauen Hemde,", "tokens": ["Aus", "dem", "gro\u00b7ben", ",", "grau\u00b7en", "Hem\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf die kleine, sorgsame Hand,", "tokens": ["Und", "auf", "die", "klei\u00b7ne", ",", "sorg\u00b7sa\u00b7me", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die das Unterr\u00f6ckchen fester bindet", "tokens": ["Die", "das", "Un\u00b7ter\u00b7r\u00f6ck\u00b7chen", "fes\u00b7ter", "bin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um die feine H\u00fcfte.", "tokens": ["Um", "die", "fei\u00b7ne", "H\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aber pl\u00f6tzlich, die Th\u00fcr springt auf,", "tokens": ["A\u00b7ber", "pl\u00f6tz\u00b7lich", ",", "die", "Th\u00fcr", "springt", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und es tritt herein der n\u00e4chtige Fremdling;", "tokens": ["Und", "es", "tritt", "her\u00b7ein", "der", "n\u00e4ch\u00b7ti\u00b7ge", "Fremd\u00b7ling", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Liebesicher ruht sein Auge", "tokens": ["Lie\u00b7be\u00b7si\u00b7cher", "ruht", "sein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem wei\u00dfen, schlanken M\u00e4dchen,", "tokens": ["Auf", "dem", "wei\u00b7\u00dfen", ",", "schlan\u00b7ken", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das schauernd vor ihm steht,", "tokens": ["Das", "schau\u00b7ernd", "vor", "ihm", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gleich einer erschrockenen Lilie;", "tokens": ["Gleich", "ei\u00b7ner", "er\u00b7schro\u00b7cke\u00b7nen", "Li\u00b7lie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und er wirft den Mantel zur Erde,", "tokens": ["Und", "er", "wirft", "den", "Man\u00b7tel", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und lacht und spricht:", "tokens": ["Und", "lacht", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Siehst du, mein Kind, ich halte Wort,", "tokens": ["Siehst", "du", ",", "mein", "Kind", ",", "ich", "hal\u00b7te", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich komme, und mit mir kommt", "tokens": ["Und", "ich", "kom\u00b7me", ",", "und", "mit", "mir", "kommt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "APPR", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die alte Zeit, wo die G\u00f6tter des Himmels", "tokens": ["Die", "al\u00b7te", "Zeit", ",", "wo", "die", "G\u00f6t\u00b7ter", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Niederstiegen zu T\u00f6chtern der Menschen,", "tokens": ["Nie\u00b7ders\u00b7tie\u00b7gen", "zu", "T\u00f6ch\u00b7tern", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und die T\u00f6chter der Menschen umarmten,", "tokens": ["Und", "die", "T\u00f6ch\u00b7ter", "der", "Men\u00b7schen", "um\u00b7arm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und mit ihnen zeugten", "tokens": ["Und", "mit", "ih\u00b7nen", "zeug\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zeptertragende K\u00f6nigsgeschlechter", "tokens": ["Zep\u00b7ter\u00b7tra\u00b7gen\u00b7de", "K\u00f6\u00b7nigs\u00b7ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und Helden, Wunder der Welt.", "tokens": ["Und", "Hel\u00b7den", ",", "Wun\u00b7der", "der", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Doch staune, mein Kind, nicht l\u00e4nger", "tokens": ["Doch", "stau\u00b7ne", ",", "mein", "Kind", ",", "nicht", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ob meiner G\u00f6ttlichkeit,", "tokens": ["Ob", "mei\u00b7ner", "G\u00f6tt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und ich bitte dich, koche mir Thee mit Rum,", "tokens": ["Und", "ich", "bit\u00b7te", "dich", ",", "ko\u00b7che", "mir", "Thee", "mit", "Rum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "NN", "APPR", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Denn drau\u00dfen war's kalt,", "tokens": ["Denn", "drau\u00b7\u00dfen", "wa\u00b7r's", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und bei solcher Nachtluft", "tokens": ["Und", "bei", "sol\u00b7cher", "Nacht\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-++", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Frieren auch wir, wir ewigen G\u00f6tter,", "tokens": ["Frie\u00b7ren", "auch", "wir", ",", "wir", "e\u00b7wi\u00b7gen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und kriegen wir leicht den g\u00f6ttlichsten Schnupfen,", "tokens": ["Und", "krie\u00b7gen", "wir", "leicht", "den", "g\u00f6tt\u00b7lichs\u00b7ten",