{"textgrid.poem.66076": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein liebes Buch h\u00e4lt meine Hand,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein liebes Buch h\u00e4lt meine Hand,", "tokens": ["Ein", "lie\u00b7bes", "Buch", "h\u00e4lt", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darin ein herrlicher Poet,", "tokens": ["Da\u00b7rin", "ein", "herr\u00b7li\u00b7cher", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was er an seltnen Sch\u00e4tzen fand,", "tokens": ["Was", "er", "an", "selt\u00b7nen", "Sch\u00e4t\u00b7zen", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus reicher F\u00fclle um sich s\u00e4t.", "tokens": ["Aus", "rei\u00b7cher", "F\u00fcl\u00b7le", "um", "sich", "s\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In allen guten T\u00f6nen sang", "tokens": ["In", "al\u00b7len", "gu\u00b7ten", "T\u00f6\u00b7nen", "sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gottbegabte Liedermund", "tokens": ["Der", "gott\u00b7be\u00b7gab\u00b7te", "Lie\u00b7der\u00b7mund"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchrte mich im Eifergang", "tokens": ["Und", "f\u00fchr\u00b7te", "mich", "im", "Ei\u00b7fer\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich um ein ganzes Erdenrund.", "tokens": ["Gleich", "um", "ein", "gan\u00b7zes", "Er\u00b7den\u00b7rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und f\u00fchrte mich durch tiefes Tal", "tokens": ["Und", "f\u00fchr\u00b7te", "mich", "durch", "tie\u00b7fes", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber hohen Zackenschroff,", "tokens": ["Und", "\u00fc\u00b7ber", "ho\u00b7hen", "Za\u00b7cken\u00b7schroff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch manchen freudenhellen Saal", "tokens": ["Durch", "man\u00b7chen", "freu\u00b7den\u00b7hel\u00b7len", "Saal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und G\u00e4rten, wos von D\u00fcften troff.", "tokens": ["Und", "G\u00e4r\u00b7ten", ",", "wos", "von", "D\u00fcf\u00b7ten", "troff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und meinem wackern Schweizersmann,", "tokens": ["Und", "mei\u00b7nem", "wa\u00b7ckern", "Schwei\u00b7zers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sie auf Gottfried einst getauft,", "tokens": ["Den", "sie", "auf", "Gott\u00b7fried", "einst", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Hab ich ein feurig Ruhmgespann", "tokens": ["Hab", "ich", "ein", "feu\u00b7rig", "Ruhm\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In eitel Dankbarkeit gekauft.", "tokens": ["In", "ei\u00b7tel", "Dank\u00b7bar\u00b7keit", "ge\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch neben mir am Tischchen steht", "tokens": ["Doch", "ne\u00b7ben", "mir", "am", "Tischchen", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Stift und einem Briefpapier", "tokens": ["Mit", "Stift", "und", "ei\u00b7nem", "Brief\u00b7pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein freilich kleinerer Poet,", "tokens": ["Ein", "frei\u00b7lich", "klei\u00b7ne\u00b7rer", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drei K\u00e4se hoch, vielleicht auch vier.", "tokens": ["Drei", "K\u00e4\u00b7se", "hoch", ",", "viel\u00b7leicht", "auch", "vier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,", "ADV", "ADV", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der malt in Runen wundersam,", "tokens": ["Der", "malt", "in", "Ru\u00b7nen", "wun\u00b7der\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was seine junge Seele tr\u00e4umt,", "tokens": ["Was", "sei\u00b7ne", "jun\u00b7ge", "See\u00b7le", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn die Schrift zu Rande kam,", "tokens": ["Und", "wenn", "die", "Schrift", "zu", "Ran\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschreibt den Tisch er unges\u00e4umt.", "tokens": ["Be\u00b7schreibt", "den", "Tisch", "er", "un\u00b7ge\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf einmal zerrt er mich am Rock,", "tokens": ["Auf", "ein\u00b7mal", "zerrt", "er", "mich", "am", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Anstandsformen nicht genau,", "tokens": ["In", "An\u00b7stands\u00b7for\u00b7men", "nicht", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und reicht mir seinen Schreibestock:", "tokens": ["Und", "reicht", "mir", "sei\u00b7nen", "Schrei\u00b7be\u00b7stock", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Papa, ach bitte, ein Wauwau!", "tokens": ["Pa\u00b7pa", ",", "ach", "bit\u00b7te", ",", "ein", "Wau\u00b7wau", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKANT", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und l\u00e4\u00dft nicht nach und qu\u00e4lt und r\u00fchrt,", "tokens": ["Und", "l\u00e4\u00dft", "nicht", "nach", "und", "qu\u00e4lt", "und", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis ich in unge\u00fcbtem Tun", "tokens": ["Bis", "ich", "in", "un\u00b7ge\u00b7\u00fcb\u00b7tem", "Tun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Stift aufs wei\u00dfe Blatt gef\u00fchrt,", "tokens": ["Den", "Stift", "aufs", "wei\u00b7\u00dfe", "Blatt", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halb wards ein Hund und halb ein Huhn.", "tokens": ["Halb", "wards", "ein", "Hund", "und", "halb", "ein", "Huhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Papa, ein Pferd. Papa, ein Hahn.", "tokens": ["Pa\u00b7pa", ",", "ein", "Pferd", ".", "Pa\u00b7pa", ",", "ein", "Hahn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$.", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er will das ganze Tierreich sehn,", "tokens": ["Er", "will", "das", "gan\u00b7ze", "Tier\u00b7reich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht in seinem sch\u00f6nen Wahn", "tokens": ["Und", "sieht", "in", "sei\u00b7nem", "sch\u00f6\u00b7nen", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00f6pfung neu durch mich entstehn.", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "neu", "durch", "mich", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch bald, so schwerer Kunst erlahmt,", "tokens": ["Doch", "bald", ",", "so", "schwe\u00b7rer", "Kunst", "er\u00b7lahmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leg ich das Blatt in seine Hand,", "tokens": ["Leg", "ich", "das", "Blatt", "in", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selig hat er nachgeahmt,", "tokens": ["Und", "se\u00b7lig", "hat", "er", "nach\u00b7ge\u00b7ahmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was dort an krausen Wundern stand.", "tokens": ["Was", "dort", "an", "krau\u00b7sen", "Wun\u00b7dern", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich aber greif aufs neu zur\u00fcck", "tokens": ["Ich", "a\u00b7ber", "greif", "aufs", "neu", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach meines Z\u00fcrchers Perlenschrein.", "tokens": ["Nach", "mei\u00b7nes", "Z\u00fcr\u00b7chers", "Per\u00b7len\u00b7schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier Meisterst\u00fcck, dort Kindergl\u00fcck,", "tokens": ["Hier", "Meis\u00b7ter\u00b7st\u00fcck", ",", "dort", "Kin\u00b7der\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Poeten gro\u00df, Poeten klein.", "tokens": ["Po\u00b7et\u00b7en", "gro\u00df", ",", "Po\u00b7et\u00b7en", "klein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}