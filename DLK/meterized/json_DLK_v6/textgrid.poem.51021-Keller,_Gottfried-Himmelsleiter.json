{"textgrid.poem.51021": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Himmelsleiter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcde sa\u00df ich in der D\u00e4mmrung", "tokens": ["M\u00fc\u00b7de", "sa\u00df", "ich", "in", "der", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Tages L\u00e4rm und Staube,", "tokens": ["Von", "des", "Ta\u00b7ges", "L\u00e4rm", "und", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingelullt vom Abends\u00e4useln,", "tokens": ["Ein\u00b7ge\u00b7lullt", "vom", "A\u00b7bend\u00b7s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummernd in der Rebenlaube;", "tokens": ["Schlum\u00b7mernd", "in", "der", "Re\u00b7ben\u00b7lau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da begann von Licht und Blumen", "tokens": ["Da", "be\u00b7gann", "von", "Licht", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar ein seltsam schimmernd Weben", "tokens": ["Gar", "ein", "selt\u00b7sam", "schim\u00b7mernd", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Spielen vor den Augen", "tokens": ["Und", "ein", "Spie\u00b7len", "vor", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gleich dem Ranken goldner Reben.", "tokens": ["Gleich", "dem", "Ran\u00b7ken", "gold\u00b7ner", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rote Rosen, wei\u00dfe Rosen,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "wei\u00b7\u00dfe", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Primeln, Tulpen und Narzissen,", "tokens": ["Pri\u00b7meln", ",", "Tul\u00b7pen", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne, Kelche hundertfarbig", "tokens": ["Ster\u00b7ne", ",", "Kel\u00b7che", "hun\u00b7dert\u00b7far\u00b7big"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah ich durcheinander sprie\u00dfen.", "tokens": ["Sah", "ich", "durch\u00b7ein\u00b7an\u00b7der", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Purpur, Gold, Azur und Silber", "tokens": ["Pur\u00b7pur", ",", "Gold", ",", "A\u00b7zur", "und", "Sil\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flimmerten in Wechselt\u00f6nen,", "tokens": ["Flim\u00b7mer\u00b7ten", "in", "Wech\u00b7sel\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lila, Rosa, zartes Laubgr\u00fcn", "tokens": ["Li\u00b7la", ",", "Ro\u00b7sa", ",", "zar\u00b7tes", "Laub\u00b7gr\u00fcn"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dften Glanz mit Glanz vers\u00f6hnen.", "tokens": ["Mu\u00df\u00b7ten", "Glanz", "mit", "Glanz", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oh, das war ein sch\u00f6ner Reigen,", "tokens": ["Oh", ",", "das", "war", "ein", "sch\u00f6\u00b7ner", "Rei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Farben all ihn tanzten,", "tokens": ["Wie", "die", "Far\u00b7ben", "all", "ihn", "tanz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Bl\u00fctenstern' und -glocken", "tokens": ["Wie", "die", "Bl\u00fc\u00b7ten\u00b7stern'", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kreisend sich in Beete pflanzten!", "tokens": ["Krei\u00b7send", "sich", "in", "Bee\u00b7te", "pflanz\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber in den Wundergarten", "tokens": ["A\u00b7ber", "in", "den", "Wun\u00b7der\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Senkte eine Jakobsleiter", "tokens": ["Senk\u00b7te", "ei\u00b7ne", "Ja\u00b7kobs\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von zwei Strahlen sanft sich nieder,", "tokens": ["Von", "zwei", "Strah\u00b7len", "sanft", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus zwei Sternen bl\u00e4ulich heiter!", "tokens": ["Aus", "zwei", "Ster\u00b7nen", "bl\u00e4u\u00b7lich", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kleine blonde Liebesengel", "tokens": ["Klei\u00b7ne", "blon\u00b7de", "Lie\u00b7be\u00b7sen\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebten daran auf und nieder,", "tokens": ["Schweb\u00b7ten", "da\u00b7ran", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiegen in den blauen Himmel,", "tokens": ["Stie\u00b7gen", "in", "den", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrten in mein Herze wieder,", "tokens": ["Kehr\u00b7ten", "in", "mein", "Her\u00b7ze", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weckten andre Engelknaben,", "tokens": ["Weck\u00b7ten", "and\u00b7re", "En\u00b7gel\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche tr\u00e4umend drinnen schliefen", "tokens": ["Wel\u00b7che", "tr\u00e4u\u00b7mend", "drin\u00b7nen", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und darauf, mit jenen spielend,", "tokens": ["Und", "da\u00b7rauf", ",", "mit", "je\u00b7nen", "spie\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "APPR", "PDAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kosend durch die Blumen liefen.", "tokens": ["Ko\u00b7send", "durch", "die", "Blu\u00b7men", "lie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die aus dem Himmel kamen,", "tokens": ["Und", "die", "aus", "dem", "Him\u00b7mel", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wollten meines Herzens Kinder", "tokens": ["Woll\u00b7ten", "mei\u00b7nes", "Her\u00b7zens", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringend mit sich aufw\u00e4rts ziehen;", "tokens": ["Rin\u00b7gend", "mit", "sich", "auf\u00b7w\u00e4rts", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber diese auch nicht minder", "tokens": ["A\u00b7ber", "die\u00b7se", "auch", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hielten stand und k\u00e4mpften wacker,", "tokens": ["Hiel\u00b7ten", "stand", "und", "k\u00e4mpf\u00b7ten", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis sie jene bald umschlangen,", "tokens": ["Bis", "sie", "je\u00b7ne", "bald", "um\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hielten sie in meines Herzens", "tokens": ["Hiel\u00b7ten", "sie", "in", "mei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beiden K\u00e4mmerlein gefangen.", "tokens": ["Bei\u00b7den", "K\u00e4m\u00b7mer\u00b7lein", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Oben auf der Himmelsleiter", "tokens": ["O\u00b7ben", "auf", "der", "Him\u00b7mels\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine klare Seele schwebte,", "tokens": ["Ei\u00b7ne", "kla\u00b7re", "See\u00b7le", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die halb scheltend, halb mit L\u00e4cheln", "tokens": ["Die", "halb", "schel\u00b7tend", ",", "halb", "mit", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "$,", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie zur\u00fcckzulocken strebte;", "tokens": ["Sie", "zu\u00b7r\u00fcck\u00b7zu\u00b7lo\u00b7cken", "streb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch es schien mir im Gef\u00e4ngnis", "tokens": ["Doch", "es", "schien", "mir", "im", "Ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihnen leidlich zu gefallen,", "tokens": ["Ih\u00b7nen", "leid\u00b7lich", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich sah, der Herrin trotzend,", "tokens": ["Denn", "ich", "sah", ",", "der", "Her\u00b7rin", "trot\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bunt sie durcheinanderwallen.", "tokens": ["Bunt", "sie", "durch\u00b7ein\u00b7an\u00b7der\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sie mu\u00dfte sich bequemen,", "tokens": ["Und", "sie", "mu\u00df\u00b7te", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich selbst herabzusteigen,", "tokens": ["End\u00b7lich", "selbst", "her\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sich pl\u00f6tzlich bang umschlossen", "tokens": ["Sah", "sich", "pl\u00f6tz\u00b7lich", "bang", "um\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitten in dem frohen Reigen.", "tokens": ["Mit\u00b7ten", "in", "dem", "fro\u00b7hen", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch f\u00fcr all den Kinderjubel", "tokens": ["Doch", "f\u00fcr", "all", "den", "Kin\u00b7der\u00b7ju\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward das Herz zu eng und nieder:", "tokens": ["Ward", "das", "Herz", "zu", "eng", "und", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKA", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Klingend sprangen auf die Pforten,", "tokens": ["Klin\u00b7gend", "spran\u00b7gen", "auf", "die", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprangen auf die Augenlider.", "tokens": ["Spran\u00b7gen", "auf", "die", "Au\u00b7gen\u00b7li\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieh! da standest du, auf meine", "tokens": ["Sieh", "!", "da", "stan\u00b7dest", "du", ",", "auf", "mei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "$,", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4feraugen schweigsam schauend,", "tokens": ["Schl\u00e4\u00b7fe\u00b7rau\u00b7gen", "schweig\u00b7sam", "schau\u00b7end", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorgeneigt und unbefangen,", "tokens": ["Vor\u00b7ge\u00b7neigt", "und", "un\u00b7be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den festen Schlaf vertrauend;", "tokens": ["Auf", "den", "fes\u00b7ten", "Schlaf", "ver\u00b7trau\u00b7end", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wurdest rot und flohst vor\u00fcber,", "tokens": ["Wur\u00b7dest", "rot", "und", "flohst", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fast wie Schwalbenfl\u00fcgel summend", "tokens": ["Fast", "wie", "Schwal\u00b7ben\u00b7fl\u00fc\u00b7gel", "sum\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vergeblich dein Geheimnis", "tokens": ["Und", "ver\u00b7geb\u00b7lich", "dein", "Ge\u00b7heim\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der D\u00e4mmerung vermummend!", "tokens": ["In", "der", "D\u00e4m\u00b7me\u00b7rung", "ver\u00b7mum\u00b7mend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Fliehe nur, verratne Seele,", "tokens": ["Flie\u00b7he", "nur", ",", "ver\u00b7rat\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trostlos durch des Gartens Bl\u00fcten!", "tokens": ["Trost\u00b7los", "durch", "des", "Gar\u00b7tens", "Bl\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Suche st\u00e4rkre Zauberdrachen,", "tokens": ["Su\u00b7che", "st\u00e4r\u00b7kre", "Zau\u00b7ber\u00b7dra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Busens Schatz zu h\u00fcten!", "tokens": ["Dei\u00b7nes", "Bu\u00b7sens", "Schatz", "zu", "h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00f6richt Kind! nun magst du immer", "tokens": ["T\u00f6\u00b7richt", "Kind", "!", "nun", "magst", "du", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreifach deinen Mund verschlie\u00dfen,", "tokens": ["Drei\u00b7fach", "dei\u00b7nen", "Mund", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unerbittlich aus den Augen", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "aus", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seh ich Liebesengel gr\u00fc\u00dfen!", "tokens": ["Seh", "ich", "Lie\u00b7be\u00b7sen\u00b7gel", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}