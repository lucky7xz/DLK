{"textgrid.poem.59912": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gl\u00fccklich ist, der ist auch gut,", "tokens": ["Wer", "gl\u00fcck\u00b7lich", "ist", ",", "der", "ist", "auch", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das zeigt auf jedem Schritt sich;", "tokens": ["Das", "zeigt", "auf", "je\u00b7dem", "Schritt", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn wer auf Erden B\u00f6ses tut,", "tokens": ["Denn", "wer", "auf", "Er\u00b7den", "B\u00f6\u00b7ses", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt seine Strafe mit sich!", "tokens": ["Tr\u00e4gt", "sei\u00b7ne", "Stra\u00b7fe", "mit", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, der in deiner frommen Wut", "tokens": ["Du", ",", "der", "in", "dei\u00b7ner", "from\u00b7men", "Wut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Zorns und Hasses Sklave,", "tokens": ["Des", "Zorns", "und", "Has\u00b7ses", "Skla\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist nicht gl\u00fccklich, bist nicht gut:", "tokens": ["Du", "bist", "nicht", "gl\u00fcck\u00b7lich", ",", "bist", "nicht", "gut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Ha\u00df ist deine Strafe!", "tokens": ["Dein", "Ha\u00df", "ist", "dei\u00b7ne", "Stra\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}