{"textgrid.poem.55163": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Zeitma\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eros, wie seh ich dich hier! In jeglichem H\u00e4ndchen die Sanduhr!", "tokens": ["E\u00b7ros", ",", "wie", "seh", "ich", "dich", "hier", "!", "In", "jeg\u00b7li\u00b7chem", "H\u00e4nd\u00b7chen", "die", "San\u00b7duhr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "$.", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "---+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie? Leichtsinniger Gott, missest du doppelt die Zeit?", "tokens": ["Wie", "?", "Leicht\u00b7sin\u00b7ni\u00b7ger", "Gott", ",", "mis\u00b7sest", "du", "dop\u00b7pelt", "die", "Zeit", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "NN", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bblangsam rinnen aus einer die Stunden entfernter Geliebten;", "tokens": ["\u00bb", "lang\u00b7sam", "rin\u00b7nen", "aus", "ei\u00b7ner", "die", "Stun\u00b7den", "ent\u00b7fern\u00b7ter", "Ge\u00b7lieb\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPR", "ART", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Gegenw\u00e4rtigen flie\u00dft eilig die zweite herab.\u00ab", "tokens": ["Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", "flie\u00dft", "ei\u00b7lig", "die", "zwei\u00b7te", "her\u00b7ab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "ADV", "$.", "$("], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}}}}}