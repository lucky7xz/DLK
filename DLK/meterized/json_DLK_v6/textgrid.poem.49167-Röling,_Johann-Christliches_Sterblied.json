{"textgrid.poem.49167": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Christliches Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wird es endlich mit der Welt,", "tokens": ["Was", "wird", "es", "end\u00b7lich", "mit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die t\u00e4glich mehr danieder f\u00e4llt", "tokens": ["Die", "t\u00e4g\u00b7lich", "mehr", "da\u00b7nie\u00b7der", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrem blut'gen Kriegsget\u00fcmmel?", "tokens": ["In", "ih\u00b7rem", "blut'\u00b7gen", "Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gottfurcht macht mehr keine Scheu,", "tokens": ["Die", "Gott\u00b7furcht", "macht", "mehr", "kei\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ewigkeit ist Dichterei,", "tokens": ["Die", "E\u00b7wig\u00b7keit", "ist", "Dich\u00b7te\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts ist die H\u00f6lle, nichts der Himmel;", "tokens": ["Nichts", "ist", "die", "H\u00f6l\u00b7le", ",", "nichts", "der", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "$,", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wen nicht das Leben irrig macht,", "tokens": ["Wen", "nicht", "das", "Le\u00b7ben", "ir\u00b7rig", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der hat gewi\u00df es hoch gebracht.", "tokens": ["Der", "hat", "ge\u00b7wi\u00df", "es", "hoch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was k\u00f6nnen b\u00f6s' Exempel nicht?", "tokens": ["Was", "k\u00f6n\u00b7nen", "b\u00f6s'", "Ex\u00b7em\u00b7pel", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man der Bosheit Ehrenlicht,", "tokens": ["Sieht", "man", "der", "Bos\u00b7heit", "Eh\u00b7ren\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ist der Unschuld gern beflissen?", "tokens": ["Wer", "ist", "der", "Un\u00b7schuld", "gern", "be\u00b7flis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bet\u00fcnchet sich die falsche Lehr',", "tokens": ["Be\u00b7t\u00fcn\u00b7chet", "sich", "die", "fal\u00b7sche", "Lehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie leicht giebt einer ihr Geh\u00f6r,", "tokens": ["Wie", "leicht", "giebt", "ei\u00b7ner", "ihr", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der gern vor Andern was will wissen,", "tokens": ["Der", "gern", "vor", "An\u00b7dern", "was", "will", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "PWS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und kehrt man bei der Wollust ein,", "tokens": ["Und", "kehrt", "man", "bei", "der", "Wol\u00b7lust", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wem schmecket Kummer, M\u00fch' und Pein?", "tokens": ["Wem", "schme\u00b7cket", "Kum\u00b7mer", ",", "M\u00fch'", "und", "Pein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach liebster Gott, dies siehest du,", "tokens": ["Ach", "liebs\u00b7ter", "Gott", ",", "dies", "sie\u00b7hest", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deswegen eilest du zur Ruh'", "tokens": ["Des\u00b7we\u00b7gen", "ei\u00b7lest", "du", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen Freunden von der Erden,", "tokens": ["Mit", "dei\u00b7nen", "Freun\u00b7den", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihre dir geweihte Seel'", "tokens": ["Da\u00df", "ih\u00b7re", "dir", "ge\u00b7weih\u00b7te", "Seel'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht des besorgten Weges fehl'", "tokens": ["Nicht", "des", "be\u00b7sorg\u00b7ten", "We\u00b7ges", "fehl'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch mit verkehret m\u00f6ge werden.", "tokens": ["Noch", "mit", "ver\u00b7keh\u00b7ret", "m\u00f6\u00b7ge", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Herz, das stets zu dir gericht,", "tokens": ["Ihr", "Herz", ",", "das", "stets", "zu", "dir", "ge\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verg\u00f6nnst du keinem Andern nicht.", "tokens": ["Ver\u00b7g\u00f6nnst", "du", "kei\u00b7nem", "An\u00b7dern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer fr\u00fch dein Friedensreich bezieht,", "tokens": ["Wer", "fr\u00fch", "dein", "Frie\u00b7dens\u00b7reich", "be\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist hier\u00fcber unbem\u00fcht,", "tokens": ["Der", "ist", "hier\u00b7\u00fc\u00b7ber", "un\u00b7be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat zwar hier wenig k\u00f6nnen werden,", "tokens": ["Hat", "zwar", "hier", "we\u00b7nig", "k\u00f6n\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIS", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch so auch wenig Schuld gemacht,", "tokens": ["Doch", "so", "auch", "we\u00b7nig", "Schuld", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was ist das vor dir geacht,", "tokens": ["Und", "was", "ist", "das", "vor", "dir", "ge\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-----+", "measure": "dactylic.init"}, "line.6": {"text": "Was einer dient der faulen Erden,", "tokens": ["Was", "ei\u00b7ner", "dient", "der", "fau\u00b7len", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die, wie sie selbst voll M\u00e4ngel ist,", "tokens": ["Die", ",", "wie", "sie", "selbst", "voll", "M\u00e4n\u00b7gel", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch hiernach unsre Arbeit mi\u00dft?", "tokens": ["Auch", "hier\u00b7nach", "uns\u00b7re", "Ar\u00b7beit", "mi\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bei dir ist unsre Herrlichkeit,", "tokens": ["Bei", "dir", "ist", "uns\u00b7re", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ehre, die Vollkommenheit,", "tokens": ["Die", "Eh\u00b7re", ",", "die", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wozu wir sind erschaffen worden.", "tokens": ["Wo\u00b7zu", "wir", "sind", "er\u00b7schaf\u00b7fen", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer denn fr\u00fchzeitig geht von hie,", "tokens": ["Wer", "denn", "fr\u00fch\u00b7zei\u00b7tig", "geht", "von", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der wird bei dir vollkommen fr\u00fch", "tokens": ["Der", "wird", "bei", "dir", "voll\u00b7kom\u00b7men", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dem vollkommnen Engelorden", "tokens": ["In", "dem", "voll\u00b7komm\u00b7nen", "En\u00b7ge\u00b7lor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hat das Alterthum erreicht,", "tokens": ["Und", "hat", "das", "Al\u00b7ter\u00b7thum", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem auch Methusalem selbst weicht.", "tokens": ["Dem", "auch", "Me\u00b7thu\u00b7sa\u00b7lem", "selbst", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "ADV", "VVFIN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Was ein Gottloser lebet hier,", "tokens": ["Was", "ein", "Gott\u00b7lo\u00b7ser", "le\u00b7bet", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist kein Leben, Herr, vor dir,", "tokens": ["Das", "ist", "kein", "Le\u00b7ben", ",", "Herr", ",", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er graut und hat doch nicht gelebet.", "tokens": ["Er", "graut", "und", "hat", "doch", "nicht", "ge\u00b7le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sich dein Leben stellet f\u00fcr", "tokens": ["Wer", "sich", "dein", "Le\u00b7ben", "stel\u00b7let", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "PPOSAT", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und folget nach, der lebt in dir", "tokens": ["Und", "fol\u00b7get", "nach", ",", "der", "lebt", "in", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PRELS", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Leben, das kein Tod kann heben,", "tokens": ["Das", "Le\u00b7ben", ",", "das", "kein", "Tod", "kann", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er f\u00e4llt nur, da\u00df er sich erheb'", "tokens": ["Er", "f\u00e4llt", "nur", ",", "da\u00df", "er", "sich", "er\u00b7heb'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stirbet, da\u00df er ewig leb'.", "tokens": ["Und", "stir\u00b7bet", ",", "da\u00df", "er", "e\u00b7wig", "leb'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Thu, Vater, deinen Kindern wol", "tokens": ["Thu", ",", "Va\u00b7ter", ",", "dei\u00b7nen", "Kin\u00b7dern", "wol"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mach' uns deines Geistes voll,", "tokens": ["Und", "mach'", "uns", "dei\u00b7nes", "Geis\u00b7tes", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein heil'ges Leben zu erf\u00fcllen.", "tokens": ["Dein", "heil'\u00b7ges", "Le\u00b7ben", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Halt' uns zur Heimfahrt fertig hie,", "tokens": ["Halt'", "uns", "zur", "Heim\u00b7fahrt", "fer\u00b7tig", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sei denn langsam oder fr\u00fch,", "tokens": ["Es", "sei", "denn", "lang\u00b7sam", "o\u00b7der", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das steht in deinem freien Willen;", "tokens": ["Das", "steht", "in", "dei\u00b7nem", "frei\u00b7en", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wer vor Andern dir gef\u00e4llt,", "tokens": ["Doch", "wer", "vor", "An\u00b7dern", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den nimmst du zeitig von der Welt.", "tokens": ["Den", "nimmst", "du", "zei\u00b7tig", "von", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}