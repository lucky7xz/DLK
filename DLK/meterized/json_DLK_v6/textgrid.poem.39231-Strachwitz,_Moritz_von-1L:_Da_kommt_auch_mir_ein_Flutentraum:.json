{"textgrid.poem.39231": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da kommt auch mir ein Flutentraum:", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kommt auch mir ein Flutentraum:", "tokens": ["Da", "kommt", "auch", "mir", "ein", "Flu\u00b7ten\u00b7traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe dich, o F\u00fcrst der See,", "tokens": ["Ich", "se\u00b7he", "dich", ",", "o", "F\u00fcrst", "der", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hingehn durch spritzenden Wogenschaum,", "tokens": ["Hin\u00b7gehn", "durch", "sprit\u00b7zen\u00b7den", "Wo\u00b7gen\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Von Kopf zu Fu\u00df in lauter Schnee.", "tokens": ["Von", "Kopf", "zu", "Fu\u00df", "in", "lau\u00b7ter", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit wei\u00dfem Mantel angetan,", "tokens": ["Mit", "wei\u00b7\u00dfem", "Man\u00b7tel", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die D\u00e4nenflagge hoch am Bord,", "tokens": ["Die", "D\u00e4\u00b7nen\u00b7flag\u00b7ge", "hoch", "am", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So pilgerst du die W\u00fcstenbahn,", "tokens": ["So", "pil\u00b7gerst", "du", "die", "W\u00fcs\u00b7ten\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein rotbekreuzter Templer, fort.", "tokens": ["Ein", "rot\u00b7be\u00b7kreuz\u00b7ter", "Temp\u00b7ler", ",", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wind ist frisch, die See ist weit;", "tokens": ["Der", "Wind", "ist", "frisch", ",", "die", "See", "ist", "weit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gradaus, als wie ein Stern, der schie\u00dft,", "tokens": ["Gra\u00b7daus", ",", "als", "wie", "ein", "Stern", ",", "der", "schie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gehst du dahin in Herrlichkeit,", "tokens": ["Gehst", "du", "da\u00b7hin", "in", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weh' dem, der dir die Bahn verschlie\u00dft!", "tokens": ["Weh'", "dem", ",", "der", "dir", "die", "Bahn", "ver\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Lebensatem ringsumher,", "tokens": ["Und", "Le\u00b7ben\u00b7sa\u00b7tem", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Lebenswelle weit und breit!", "tokens": ["Und", "Le\u00b7bens\u00b7wel\u00b7le", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O hohes Meer, o heil'ges Meer,", "tokens": ["O", "ho\u00b7hes", "Meer", ",", "o", "heil'\u00b7ges", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein bester K\u00e4mpfer geht zum Streit!", "tokens": ["Dein", "bes\u00b7ter", "K\u00e4mp\u00b7fer", "geht", "zum", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sturmbeherzt und donnerfroh \u2013", "tokens": ["Und", "sturm\u00b7be\u00b7herzt", "und", "don\u00b7ner\u00b7froh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinfuhr der Segler im Triumph,", "tokens": ["Hin\u00b7fuhr", "der", "Seg\u00b7ler", "im", "Tri\u00b7umph", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da floh der Traum, das Meer entfloh,", "tokens": ["Da", "floh", "der", "Traum", ",", "das", "Meer", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch im Hafen lag der Rumpf.", "tokens": ["Und", "noch", "im", "Ha\u00b7fen", "lag", "der", "Rumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er lag, ein schwarzer Sarkophag,", "tokens": ["Er", "lag", ",", "ein", "schwar\u00b7zer", "Sar\u00b7ko\u00b7phag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weit, weit von Wind und Wellenku\u00df,", "tokens": ["Weit", ",", "weit", "von", "Wind", "und", "Wel\u00b7len\u00b7ku\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber sagte: Fluch dem Tag,", "tokens": ["Ich", "a\u00b7ber", "sag\u00b7te", ":", "Fluch", "dem", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo solch ein Schiff verfaulen mu\u00df.", "tokens": ["Wo", "solch", "ein", "Schiff", "ver\u00b7fau\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}