{"textgrid.poem.29697": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Porcia schont ihrer Augen; einen kleinen schlechten Mann", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Porcia schont ihrer Augen; einen kleinen schlechten Mann", "tokens": ["Por\u00b7cia", "schont", "ih\u00b7rer", "Au\u00b7gen", ";", "ei\u00b7nen", "klei\u00b7nen", "schlech\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Sihet sie nur \u00fcber Achsel, siht sie mit Verachtung an;", "tokens": ["Si\u00b7het", "sie", "nur", "\u00fc\u00b7ber", "Ach\u00b7sel", ",", "siht", "sie", "mit", "Ver\u00b7ach\u00b7tung", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Kleine Schrifft vexirt die Augen, da\u00df man \u00fcbler sehen kan.", "tokens": ["Klei\u00b7ne", "Schrifft", "ve\u00b7xirt", "die", "Au\u00b7gen", ",", "da\u00df", "man", "\u00fcb\u00b7ler", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "ART", "NN", "$,", "KOUS", "PIS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}