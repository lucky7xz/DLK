{"dta.poem.4344": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um mich anitzt der weissen Welt, so wie zur Sommer-Zeit", "tokens": ["Um", "mich", "a\u00b7nitzt", "der", "weis\u00b7sen", "Welt", ",", "so", "wie", "zur", "Som\u00b7mer\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "der gr\u00fcnen,", "tokens": ["der", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Zu GOttes Ehr\u2019, und meiner Lust, mit Ueberlegung zu", "tokens": ["Zu", "Got\u00b7tes", "Ehr'", ",", "und", "mei\u00b7ner", "Lust", ",", "mit", "Ue\u00b7ber\u00b7le\u00b7gung", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "APPR", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "bedienen;", "tokens": ["be\u00b7die\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Fuhr ich im Schlitten auf das Feld, um, wie sich viele Oerter", "tokens": ["Fuhr", "ich", "im", "Schlit\u00b7ten", "auf", "das", "Feld", ",", "um", ",", "wie", "sich", "vie\u00b7le", "O\u00b7er\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$,", "KOUI", "$,", "PWAV", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.6": {"text": "schm\u00fccken,", "tokens": ["schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit noch vermehrter Lust, Betrachtung, und mit Bewun-", "tokens": ["Mit", "noch", "ver\u00b7mehr\u00b7ter", "Lust", ",", "Be\u00b7trach\u00b7tung", ",", "und", "mit", "Be\u00b7wun"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "NN", "$,", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "drung, zu erblicken.", "tokens": ["drung", ",", "zu", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die B\u00e4ume schienen, da\u00df sie uns von B\u00e4umen nur allein", "tokens": ["Die", "B\u00e4u\u00b7me", "schie\u00b7nen", ",", "da\u00df", "sie", "uns", "von", "B\u00e4u\u00b7men", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "die Schatten,", "tokens": ["die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In d\u00fcnnen und durchsicht\u2019gen Zweigen, die dunkel, hin-", "tokens": ["In", "d\u00fcn\u00b7nen", "und", "durch\u00b7sicht'\u00b7gen", "Zwei\u00b7gen", ",", "die", "dun\u00b7kel", ",", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "terlassen hatten.", "tokens": ["ter\u00b7las\u00b7sen", "hat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Es siehet itzo wirklich kaum", "tokens": ["Es", "sie\u00b7het", "it\u00b7zo", "wirk\u00b7lich", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein bl\u00e4tterloser brauner Baum", "tokens": ["Ein", "bl\u00e4t\u00b7ter\u00b7lo\u00b7ser", "brau\u00b7ner", "Baum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Belaubten gr\u00fcnen B\u00e4umen gleich. Sieht man, da es", "tokens": ["Be\u00b7laub\u00b7ten", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", "gleich", ".", "Sieht", "man", ",", "da", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADV", "$.", "VVFIN", "PIS", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "geschneit, nun zwischen", "tokens": ["ge\u00b7schneit", ",", "nun", "zwi\u00b7schen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Dergleichen braunen Schatten-B\u00fcschen", "tokens": ["Derg\u00b7lei\u00b7chen", "brau\u00b7nen", "Schat\u00b7ten\u00b7B\u00fc\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bald hie, bald dort von Scheun- und H\u00fctten beschneite", "tokens": ["Bald", "hie", ",", "bald", "dort", "von", "Scheun", "und", "H\u00fct\u00b7ten", "be\u00b7schnei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "APPR", "TRUNC", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "weisse D\u00e4cher liegen;", "tokens": ["weis\u00b7se", "D\u00e4\u00b7cher", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "So ist dadurch, nicht ohn\u2019 Vergn\u00fcgen,", "tokens": ["So", "ist", "da\u00b7durch", ",", "nicht", "ohn'", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Schnee noch eins so wei\u00df, noch eins so sch\u00f6n,", "tokens": ["Der", "Schnee", "noch", "eins", "so", "wei\u00df", ",", "noch", "eins", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "ADV", "VVFIN", "$,", "ADV", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Jm braunen Gegensatz, zu sehn.", "tokens": ["Jm", "brau\u00b7nen", "Ge\u00b7gen\u00b7satz", ",", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Aecker scheinen itzt gefrorne kleine Wellen,", "tokens": ["Die", "A\u00b7e\u00b7cker", "schei\u00b7nen", "itzt", "ge\u00b7fror\u00b7ne", "klei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.16": {"text": "In Tief- und H\u00f6hen, vorzustellen.", "tokens": ["In", "Tie\u00b7f", "und", "H\u00f6\u00b7hen", ",", "vor\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,", "VVIZU", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wovon jedoch nicht so, wie auf der Fluht,", "tokens": ["Wo\u00b7von", "je\u00b7doch", "nicht", "so", ",", "wie", "auf", "der", "Fluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Der Schaum sich auf der H\u00f6he zeiget,", "tokens": ["Der", "Schaum", "sich", "auf", "der", "H\u00f6\u00b7he", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Als welche braun. Der Schnee, der Luft-Schaum, ruht", "tokens": ["Als", "wel\u00b7che", "braun", ".", "Der", "Schnee", ",", "der", "Luft\u00b7Schaum", ",", "ruht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$.", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Nur da, wo sich die Furche neiget.", "tokens": ["Nur", "da", ",", "wo", "sich", "die", "Fur\u00b7che", "nei\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch die\u00df Gemisch von Dunkelheit und Licht,", "tokens": ["Durch", "die\u00df", "Ge\u00b7misch", "von", "Dun\u00b7kel\u00b7heit", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wodurch des Schnees weisser Schein,", "tokens": ["Wo\u00b7durch", "des", "Schnees", "weis\u00b7ser", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Der sonsten allgemein,", "tokens": ["Der", "sons\u00b7ten", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In einer Art von D\u00e4mmerung sich bricht,", "tokens": ["In", "ei\u00b7ner", "Art", "von", "D\u00e4m\u00b7me\u00b7rung", "sich", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Legt dem betrachtenden Gesicht", "tokens": ["Legt", "dem", "be\u00b7trach\u00b7ten\u00b7den", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein nicht unangenehmes Grau,", "tokens": ["Ein", "nicht", "un\u00b7an\u00b7ge\u00b7neh\u00b7mes", "Grau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Jm weissen Schimmer, sich zur Schau,", "tokens": ["Jm", "weis\u00b7sen", "Schim\u00b7mer", ",", "sich", "zur", "Schau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In welchen sich die langen weissen Graben,", "tokens": ["In", "wel\u00b7chen", "sich", "die", "lan\u00b7gen", "weis\u00b7sen", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PRF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Von Schnee gef\u00fcllt, in fast nicht abzusehnden Strichen,", "tokens": ["Von", "Schnee", "ge\u00b7f\u00fcllt", ",", "in", "fast", "nicht", "ab\u00b7zu\u00b7sehn\u00b7den", "Stri\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als Rahmen, die dem Silber glichen,", "tokens": ["Als", "Rah\u00b7men", ",", "die", "dem", "Sil\u00b7ber", "gli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Symmetrisch eingefasset haben.", "tokens": ["Sym\u00b7met\u00b7risch", "ein\u00b7ge\u00b7fas\u00b7set", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Worauf bald hie, bald dort bestrahlten Eises Spitzen,", "tokens": ["Wo\u00b7rauf", "bald", "hie", ",", "bald", "dort", "be\u00b7strahl\u00b7ten", "Ei\u00b7ses", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "$,", "ADV", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als w\u00e4ren sie darauf durch Kunst gefasset, blitzen.", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "da\u00b7rauf", "durch", "Kunst", "ge\u00b7fas\u00b7set", ",", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PAV", "APPR", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die\u00df alles siehet ein Gem\u00fcht,", "tokens": ["Die\u00df", "al\u00b7les", "sie\u00b7het", "ein", "Ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das es mit Ueberlegung sieht,", "tokens": ["Das", "es", "mit", "Ue\u00b7ber\u00b7le\u00b7gung", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "(indem, was nicht zu leugnen, ja", "tokens": ["(", "in\u00b7dem", ",", "was", "nicht", "zu", "leug\u00b7nen", ",", "ja"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "KOUS", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die Farben wirklich da)", "tokens": ["Die", "Far\u00b7ben", "wirk\u00b7lich", "da", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Mit einem billigen Vergn\u00fcgen,", "tokens": ["Mit", "ei\u00b7nem", "bil\u00b7li\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Weil Formen, Farben, Glanz und Licht,", "tokens": ["Weil", "For\u00b7men", ",", "Far\u00b7ben", ",", "Glanz", "und", "Licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Zusammt dem es betrachtenden Gesicht,", "tokens": ["Zu\u00b7sammt", "dem", "es", "be\u00b7trach\u00b7ten\u00b7den", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wenn man es recht erwegt, in unserm Leben", "tokens": ["Wenn", "man", "es", "recht", "er\u00b7wegt", ",", "in", "un\u00b7serm", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVPP", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Gewi\u00df uns nicht umsonst gegeben.", "tokens": ["Ge\u00b7wi\u00df", "uns", "nicht", "um\u00b7sonst", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Man mu\u00df denn die verachtete Gestalten,", "tokens": ["Man", "mu\u00df", "denn", "die", "ver\u00b7ach\u00b7te\u00b7te", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die uns der Sch\u00f6pfer auch zur Winter-Zeit beschehrt,", "tokens": ["Die", "uns", "der", "Sch\u00f6p\u00b7fer", "auch", "zur", "Win\u00b7ter\u00b7Zeit", "be\u00b7schehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Weil man in ihnen auch, in unsrer Lust, Jhn ehrt,", "tokens": ["Weil", "man", "in", "ih\u00b7nen", "auch", ",", "in", "uns\u00b7rer", "Lust", ",", "Jhn", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hinf\u00fchro unsers Anblicks wehrt,", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "un\u00b7sers", "An\u00b7blicks", "wehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Und ferner, wie bisher, nicht f\u00fcr ver\u00e4chtlich halten.", "tokens": ["Und", "fer\u00b7ner", ",", "wie", "bis\u00b7her", ",", "nicht", "f\u00fcr", "ver\u00b7\u00e4cht\u00b7lich", "hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "$,", "PTKNEG", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}