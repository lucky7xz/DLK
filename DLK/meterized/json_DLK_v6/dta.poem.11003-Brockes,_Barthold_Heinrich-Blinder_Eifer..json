{"dta.poem.11003": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Blinder Eifer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nicht die Obrigkeit gelindre Triebe n\u00e4hrte,", "tokens": ["Wenn", "nicht", "die", "Ob\u00b7rig\u00b7keit", "ge\u00b7lind\u00b7re", "Trie\u00b7be", "n\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vielen Geistlichen den heilgen Eifer wehrte;", "tokens": ["Und", "vie\u00b7len", "Geist\u00b7li\u00b7chen", "den", "heil\u00b7gen", "Ei\u00b7fer", "wehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es w\u00fcrde, dem zur Ehr, der diese Welt gemacht,", "tokens": ["Es", "w\u00fcr\u00b7de", ",", "dem", "zur", "Ehr", ",", "der", "die\u00b7se", "Welt", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPRART", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der gr\u00f6\u00dfte Theil der Welt gelassen umgebracht.", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Theil", "der", "Welt", "ge\u00b7las\u00b7sen", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}