{"dta.poem.8589": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf das braune, vertrocknete Laub um die Tiergartenseee", "tokens": ["Auf", "das", "brau\u00b7ne", ",", "ver\u00b7trock\u00b7ne\u00b7te", "Laub", "um", "die", "Tier\u00b7gar\u00b7ten\u00b7see\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "scheint die Novembersonne.", "tokens": ["scheint", "die", "No\u00b7vem\u00b7ber\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Mit schillernden K\u00f6pfchen aus verzaubertem Gr\u00fcn", "tokens": ["Mit", "schil\u00b7lern\u00b7den", "K\u00f6pf\u00b7chen", "aus", "ver\u00b7zau\u00b7ber\u00b7tem", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "w\u00e4rmen sich in ihr die Enten.", "tokens": ["w\u00e4r\u00b7men", "sich", "in", "ihr", "die", "En\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "In stilles,", "tokens": ["In", "stil\u00b7les", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "blaues Wasser mit Wolken", "tokens": ["blau\u00b7es", "Was\u00b7ser",