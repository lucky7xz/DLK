{"dta.poem.20737": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  eine Freundin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies Tantalussische Verlangen,               ", "tokens": ["Dies", "Tan\u00b7ta\u00b7lus\u00b7si\u00b7sche", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hei\u00dfe Fieberdurst in mir", "tokens": ["Der", "hei\u00b7\u00dfe", "Fie\u00b7ber\u00b7durst", "in", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nun, dem Himmel Dank! vergangen.", "tokens": ["Ist", "nun", ",", "dem", "Him\u00b7mel", "Dank", "!", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "ART", "NN", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun, meine Freundin! kann ichs Dir", "tokens": ["Nun", ",", "mei\u00b7ne", "Freun\u00b7din", "!", "kann", "ichs", "Dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "VMFIN", "PIS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl sagen froh und unverholen,", "tokens": ["Wohl", "sa\u00b7gen", "froh", "und", "un\u00b7ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun gl\u00fcht mir Tag und Nacht der Mund", "tokens": ["Nun", "gl\u00fcht", "mir", "Tag", "und", "Nacht", "der", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht mehr wie angeflammte Kohlen,", "tokens": ["Nicht", "mehr", "wie", "an\u00b7ge\u00b7flamm\u00b7te", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Seitdem mir Milon hat befohlen:", "tokens": ["Seit\u00b7dem", "mir", "Mi\u00b7lon", "hat", "be\u00b7foh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "\u201ebleib ruhig, bleib gesund \u2014", "tokens": ["\u201e", "bleib", "ru\u00b7hig", ",", "bleib", "ge\u00b7sund"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "\u201esonst kr\u00e4nkst du mich\u201e \u2014", "tokens": ["\u201e", "sonst", "kr\u00e4nkst", "du", "mich", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Er sprachs, und l\u00e4\u00dft mich denken:", "tokens": ["Er", "sprachs", ",", "und", "l\u00e4\u00dft", "mich", "den\u00b7ken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ihn, meinen Wunsch, mein Augenmerk,", "tokens": ["Ihn", ",", "mei\u00b7nen", "Wunsch", ",", "mein", "Au\u00b7gen\u00b7merk", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihn, meinen Abgott! nicht zu kr\u00e4nken,", "tokens": ["Ihn", ",", "mei\u00b7nen", "Ab\u00b7gott", "!", "nicht", "zu", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$.", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "That die Natur ein Wunderwerk \u2014", "tokens": ["That", "die", "Na\u00b7tur", "ein", "Wun\u00b7der\u00b7werk"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}