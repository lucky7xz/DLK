{"dta.poem.8086": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Die drei K\u00f6nige zu Heimsen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Drei K\u00f6nige zu Heimsen, wer h\u00e4tt\u2019 es je gedacht!", "tokens": ["Drei", "K\u00f6\u00b7ni\u00b7ge", "zu", "Heim\u00b7sen", ",", "wer", "h\u00e4tt'", "es", "je", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Rittern und mit Rossen, in Herrlichkeit und Pracht!", "tokens": ["Mit", "Rit\u00b7tern", "und", "mit", "Ros\u00b7sen", ",", "in", "Herr\u00b7lich\u00b7keit", "und", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es sind die hohen H\u00e4upter der Schlegelbr\u00fcderschaft,", "tokens": ["Es", "sind", "die", "ho\u00b7hen", "H\u00e4up\u00b7ter", "der", "Schle\u00b7gel\u00b7br\u00fc\u00b7der\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sich K\u00f6nige zu nennen, das gibt der Sache Kraft.", "tokens": ["Sich", "K\u00f6\u00b7ni\u00b7ge", "zu", "nen\u00b7nen", ",", "das", "gibt", "der", "Sa\u00b7che", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "PTKZU", "VVINF", "$,", "PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da thronen sie beisammen und halten eifrig Rath,", "tokens": ["Da", "thro\u00b7nen", "sie", "bei\u00b7sam\u00b7men", "und", "hal\u00b7ten", "eif\u00b7rig", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bedenken und besprechen gewalt\u2019ge Waffenthat:", "tokens": ["Be\u00b7den\u00b7ken", "und", "be\u00b7spre\u00b7chen", "ge\u00b7walt'\u00b7ge", "Waf\u00b7fent\u00b7hat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie man den stolzen Greiner mit Kriegsheer \u00fcberf\u00e4llt", "tokens": ["Wie", "man", "den", "stol\u00b7zen", "Grei\u00b7ner", "mit", "Kriegs\u00b7heer", "\u00fc\u00b7berf\u00b7\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und, besser als im Bade, ihm jeden Schlich verstellt.", "tokens": ["Und", ",", "bes\u00b7ser", "als", "im", "Ba\u00b7de", ",", "ihm", "je\u00b7den", "Schlich", "ver\u00b7stellt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KOKOM", "APPRART", "NN", "$,", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie man ihn dann verwahret und seine Burgen bricht,", "tokens": ["Wie", "man", "ihn", "dann", "ver\u00b7wah\u00b7ret", "und", "sei\u00b7ne", "Bur\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bis er von allem Zwange die Edeln ledig spricht.", "tokens": ["Bis", "er", "von", "al\u00b7lem", "Zwan\u00b7ge", "die", "E\u00b7deln", "le\u00b7dig", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann fahre wohl, Landfriede! dann, Lehndienst, gute Nacht!", "tokens": ["Dann", "fah\u00b7re", "wohl", ",", "Land\u00b7frie\u00b7de", "!", "dann", ",", "Lehn\u00b7dienst", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "NN", "$.", "ADV", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann ist\u2019s der freie Ritter, der alle Welt verlacht.", "tokens": ["Dann", "ist's", "der", "frei\u00b7e", "Rit\u00b7ter", ",", "der", "al\u00b7le", "Welt", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Schon sank die Nacht hernieder, die K\u00f6n\u2019ge sind zur Ruh,", "tokens": ["Schon", "sank", "die", "Nacht", "her\u00b7nie\u00b7der", ",", "die", "K\u00f6n'\u00b7ge", "sind", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schon kr\u00e4hen jetzt die H\u00e4hne dem nahen Morgen zu,", "tokens": ["Schon", "kr\u00e4\u00b7hen", "jetzt", "die", "H\u00e4h\u00b7ne", "dem", "na\u00b7hen", "Mor\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da schallt mit scharfem Sto\u00dfe das W\u00e4chterhorn vom Thurm,", "tokens": ["Da", "schallt", "mit", "schar\u00b7fem", "Sto\u00b7\u00dfe", "das", "W\u00e4ch\u00b7ter\u00b7horn", "vom", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wohlauf, wohlauf, ihr Schl\u00e4fer! das Horn verk\u00fcndet Sturm.", "tokens": ["Wohl\u00b7auf", ",", "wohl\u00b7auf", ",", "ihr", "Schl\u00e4\u00b7fer", "!", "das", "Horn", "ver\u00b7k\u00fcn\u00b7det", "Sturm", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "In Nacht und Nebel drau\u00dfen, da wogt es wie ein Meer", "tokens": ["In", "Nacht", "und", "Ne\u00b7bel", "drau\u00b7\u00dfen", ",", "da", "wogt", "es", "wie", "ein", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und zieht von allen Seiten sich um das St\u00e4dtlein her;", "tokens": ["Und", "zieht", "von", "al\u00b7len", "Sei\u00b7ten", "sich", "um", "das", "St\u00e4dt\u00b7lein", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verhaltne M\u00e4nnerstimmen, verworrner Gang und Drang,", "tokens": ["Ver\u00b7halt\u00b7ne", "M\u00e4n\u00b7ner\u00b7stim\u00b7men", ",", "ver\u00b7worr\u00b7ner", "Gang", "und", "Drang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hufschlag und Rossesschnauben und dumpfer Waffenklang!", "tokens": ["Huf\u00b7schlag", "und", "Ros\u00b7ses\u00b7schnau\u00b7ben", "und", "dum\u00b7pfer", "Waf\u00b7fen\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Und als das Fr\u00fchroth leuchtet und als der Nebel sinkt,", "tokens": ["Und", "als", "das", "Fr\u00fch\u00b7roth", "leuch\u00b7tet", "und", "als", "der", "Ne\u00b7bel", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hei! wie es da von Speeren, von Morgensternen blinkt!", "tokens": ["Hei", "!", "wie", "es", "da", "von", "Spee\u00b7ren", ",", "von", "Mor\u00b7gens\u00b7ter\u00b7nen", "blinkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "ADV", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Des ganzen Gaues Bauern stehn um den Ort geschaart,", "tokens": ["Des", "gan\u00b7zen", "Gau\u00b7es", "Bau\u00b7ern", "stehn", "um", "den", "Ort", "ge\u00b7schaart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und mitten h\u00e4lt zu Rosse der alte Rauschebart.", "tokens": ["Und", "mit\u00b7ten", "h\u00e4lt", "zu", "Ros\u00b7se", "der", "al\u00b7te", "Rau\u00b7sche\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Schlegler m\u00f6chten schirmen das St\u00e4dtlein und das Schlo\u00df,", "tokens": ["Die", "Schleg\u00b7ler", "m\u00f6ch\u00b7ten", "schir\u00b7men", "das", "St\u00e4dt\u00b7lein", "und", "das", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie werfen von den Th\u00fcrmen mit Steinen und Gescho\u00df.", "tokens": ["Sie", "wer\u00b7fen", "von", "den", "Th\u00fcr\u00b7men", "mit", "Stei\u00b7nen", "und", "Ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u201enur sachte! \u2014 ruft der Greiner \u2014 euch wird das Bad geheitzt,", "tokens": ["\u201e", "nur", "sach\u00b7te", "!", "ruft", "der", "Grei\u00b7ner", "euch", "wird", "das", "Bad", "ge\u00b7heitzt", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "$(", "PPER", "VAFIN", "ART", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aufdampfen soll\u2019s und qualmen, da\u00df euch\u2019s die Augen beitzt!\u201c", "tokens": ["Auf\u00b7damp\u00b7fen", "soll's", "und", "qual\u00b7men", ",", "da\u00df", "euch's", "die", "Au\u00b7gen", "beitzt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "KON", "VVFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Rings um die alten mauern ist Holz und Stroh geh\u00e4uft,", "tokens": ["Rings", "um", "die", "al\u00b7ten", "mau\u00b7ern", "ist", "Holz", "und", "Stroh", "ge\u00b7h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dunkler Nacht geschichtet und wohl mit Theer betr\u00e4uft,", "tokens": ["In", "dunk\u00b7ler", "Nacht", "ge\u00b7schich\u00b7tet", "und", "wohl", "mit", "Theer", "be\u00b7tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Drein schie\u00dft man gl\u00fch\u2019nde Pfeile, wie raschelt\u2019s da im Stroh!", "tokens": ["Drein", "schie\u00dft", "man", "gl\u00fch'n\u00b7de", "Pfei\u00b7le", ",", "wie", "ra\u00b7schelt's", "da", "im", "Stroh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,", "PWAV", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Drein wirft man feur\u2019ge Kr\u00e4nze, wie flackert\u2019s lichterloh!", "tokens": ["Drein", "wirft", "man", "feur'\u00b7ge", "Kr\u00e4n\u00b7ze", ",", "wie", "fla\u00b7ckert's", "lich\u00b7ter\u00b7loh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,", "PWAV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Und noch von allen Enden wird Vorrath zugef\u00fchrt,", "tokens": ["Und", "noch", "von", "al\u00b7len", "En\u00b7den", "wird", "Vor\u00b7rath", "zu\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von all den r\u00fcst\u2019gen Bauern wird emsig nachgesch\u00fcrt,", "tokens": ["Von", "all", "den", "r\u00fcst'\u00b7gen", "Bau\u00b7ern", "wird", "em\u00b7sig", "nach\u00b7ge\u00b7sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bis h\u00f6her, immer h\u00f6her die Flamme leckt und schweift,", "tokens": ["Bis", "h\u00f6\u00b7her", ",", "im\u00b7mer", "h\u00f6\u00b7her", "die", "Flam\u00b7me", "leckt", "und", "schweift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und schon mit lust\u2019gem Prasseln der Th\u00fcrme Dach ergreift.", "tokens": ["Und", "schon", "mit", "lust'\u00b7gem", "Pras\u00b7seln", "der", "Th\u00fcr\u00b7me", "Dach", "er\u00b7greift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Ein Thor ist frei gelassen, so hat\u2019s der Graf beliebt,", "tokens": ["Ein", "Thor", "ist", "frei", "ge\u00b7las\u00b7sen", ",", "so", "hat's", "der", "Graf", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort h\u00f6rt man wie der Riegel sich leise, lose schiebt.", "tokens": ["Dort", "h\u00f6rt", "man", "wie", "der", "Rie\u00b7gel", "sich", "lei\u00b7se", ",", "lo\u00b7se", "schiebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ART", "NN", "PRF", "ADJD", "$,", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort st\u00fcrzen wohl, verzweifelnd, d