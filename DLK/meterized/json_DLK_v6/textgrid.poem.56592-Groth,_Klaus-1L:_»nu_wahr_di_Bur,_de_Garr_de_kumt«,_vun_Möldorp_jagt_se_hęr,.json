{"textgrid.poem.56592": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbnu wahr di Bur, de Garr de kumt\u00ab, vun M\u00f6ldorp jagt se h\u0119r,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnu wahr di Bur, de Garr de kumt\u00ab, vun M\u00f6ldorp jagt se h\u0119r,", "tokens": ["\u00bb", "nu", "wahr", "di", "Bur", ",", "de", "Garr", "de", "kumt", "\u00ab", ",", "vun", "M\u00f6l\u00b7dorp", "jagt", "se", "h\u0119r", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "NE", "NN", "$,", "FM.da", "FM.da", "FM.da", "FM.da", "$(", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "De Helm un Panzers schint as Gold, as S\u00fclwer schint de P\u0119r.", "tokens": ["De", "Helm", "un", "Pan\u00b7zers", "schint", "as", "Gold", ",", "as", "S\u00fcl\u00b7wer", "schint", "de", "P\u0119r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Kong Hans un all wat Adel kumt mit groten Larm un Schall,", "tokens": ["Kong", "Hans", "un", "all", "wat", "A\u00b7del", "kumt", "mit", "gro\u00b7ten", "Larm", "un", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "PIAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "De Wulf de lurt mit w\u00fccke Burn bi Braken achtern Wall.", "tokens": ["De", "Wulf", "de", "lurt", "mit", "w\u00fc\u00b7cke", "Burn", "bi", "Bra\u00b7ken", "ach\u00b7tern", "Wall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "APPR", "VVFIN", "NE", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Vun M\u00f6ldorp trock dat swart hendal, wul d\u00f6rtig dusent Mann:", "tokens": ["Vun", "M\u00f6l\u00b7dorp", "trock", "dat", "swart", "hen\u00b7dal", ",", "wul", "d\u00f6r\u00b7tig", "du\u00b7sent", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Vun W\u00f6rden il en l\u00fctten Tropp, en M\u00e4den gung v\u0153ran.", "tokens": ["Vun", "W\u00f6r\u00b7den", "il", "en", "l\u00fct\u00b7ten", "Tropp", ",", "en", "M\u00e4\u00b7den", "gung", "v\u0153ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "\u00bbso h\u00f6lp uns Herr, du hest dat Rik in Himmel un op Eer!\u00ab", "tokens": ["\u00bb", "so", "h\u00f6lp", "uns", "Herr", ",", "du", "hest", "dat", "Rik", "in", "Him\u00b7mel", "un", "op", "E\u00b7er", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wulf Isebrand st\u00f6rtt ut de Schanz, twee Hunnert achterh\u0119r.", "tokens": ["Wulf", "I\u00b7se\u00b7brand", "st\u00f6rtt", "ut", "de", "Schanz", ",", "twee", "Hun\u00b7nert", "acht\u00b7er\u00b7h\u0119r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Un op de Panzers fulln de Sl\u00e4g', un R\u00fcters in den Sand,", "tokens": ["Un", "op", "de", "Pan\u00b7zers", "fulln", "de", "Sl\u00e4g'", ",", "un", "R\u00fc\u00b7ters", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "FM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Un vun de Geest dar kaamn de Burn, un de Floth keem \u0153wert Land.", "tokens": ["Un", "vun", "de", "Geest", "dar", "kaamn", "de", "Burn", ",", "un", "de", "Floth", "keem", "\u0153wert", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Un dal vun H\u0119ben full de Snee, op P\u0119r un Minsch de Sl\u00e4g',", "tokens": ["Un", "dal", "vun", "H\u0119\u00b7ben", "full", "de", "Snee", ",", "op", "P\u0119r", "un", "Minsch", "de", "Sl\u00e4g'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Blank war dat Moor un witt de Geest, un bl\u00f6di warn de St\u0119g'.", "tokens": ["Blank", "war", "dat", "Moor", "un", "witt", "de", "Geest", ",", "un", "bl\u00f6\u00b7di", "warn", "de", "St\u0119g'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "FM", "FM", "FM", "FM", "$,", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "De Buern schregen: st\u0119kt de P\u0119r un schont de R\u00fcterknechts!", "tokens": ["De", "Bu\u00b7ern", "schre\u00b7gen", ":", "st\u0119kt", "de", "P\u0119r", "un", "schont", "de", "R\u00fc\u00b7ter\u00b7knechts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Un sprungn barfot mit Kluwerst\u00f6ck un slogen links un rechts.", "tokens": ["Un", "sprungn", "bar\u00b7fot", "mit", "Klu\u00b7wer\u00b7st\u00f6ck", "un", "slo\u00b7gen", "links", "un", "rechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NN", "FM", "FM", "FM", "FM", "ADV", "$."], "meter": "+-+--+---+-+--", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Un reten inne Gr\u00f6ben dal un st\u00f6rtten se in Slamm,", "tokens": ["Un", "re\u00b7ten", "in\u00b7ne", "Gr\u00f6\u00b7ben", "dal", "un", "st\u00f6rt\u00b7ten", "se", "in", "Slamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Bet Minsch un Veh sik dr\u00e4ngn un drungn all langs den smallen Damm.", "tokens": ["Bet", "Minsch", "un", "Veh", "sik", "dr\u00e4ngn", "un", "drungn", "all", "langs", "den", "smal\u00b7len", "Damm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "FM", "FM", "FM", "FM", "ADV", "PIAT", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbnu wahr die Garr, de ", "tokens": ["\u00bb", "nu", "wahr", "die", "Garr", ",", "de"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "$,", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vun H\u0119ben fallt de Snee heraf, de Floth de stiggt vun nerrn.", "tokens": ["Vun", "H\u0119\u00b7ben", "fallt", "de", "Snee", "he\u00b7raf", ",", "de", "Floth", "de", "stiggt", "vun", "nerrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "Un wit ut alle D\u00f6rpen h\u0119r kumt H\u00f6lp un frischen Moth:", "tokens": ["Un", "wit", "ut", "al\u00b7le", "D\u00f6r\u00b7pen", "h\u0119r", "kumt", "H\u00f6lp", "un", "fri\u00b7schen", "Moth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbnu schont de P\u0119r \u2013 de ridt wi noch \u2013 un slat de R\u00fcters dot!\u00ab", "tokens": ["\u00bb", "nu", "schont", "de", "P\u0119r", "\u2013", "de", "ridt", "wi", "noch", "\u2013", "un", "slat", "de", "R\u00fc\u00b7ters", "dot", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "NE", "NE", "$(", "FM", "FM", "FM", "ADV", "$(", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "In Slick un Slamm sack menni Herr, de sunst op Siden leeg,", "tokens": ["In", "Slick", "un", "Slamm", "sack", "men\u00b7ni", "Herr", ",", "de", "sunst", "op", "Si\u00b7den", "leeg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "FM", "FM", "FM", "FM", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Int Swinmoor liggt nu menni Een, de harr en golden Weeg.", "tokens": ["Int", "Swin\u00b7moor", "liggt", "nu", "men\u00b7ni", "E\u00b7en", ",", "de", "harr", "en", "gol\u00b7den", "Weeg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.12": {"line.1": {"text": "Keen Nam so grot int Holstenland un nich in D\u00e4nnemark,", "tokens": ["Ke\u00b7en", "Nam", "so", "grot", "int", "Hols\u00b7ten\u00b7land", "un", "nich", "in", "D\u00e4n\u00b7ne\u00b7mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "FM", "FM", "FM", "PTKNEG", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dar ligt se nu ahn Kr\u00fcz un Steen, dar ligt se ahn en Sark.", "tokens": ["Dar", "ligt", "se", "nu", "ahn", "Kr\u00fcz", "un", "Steen", ",", "dar", "ligt", "se", "ahn", "en", "Sark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "FM", "FM", "FM", "FM", "FM", "NN", "$,", "PTKVZ", "VVFIN", "FM", "FM", "FM", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.13": {"line.1": {"text": "De Garr de full mit Junker Slenz, so grot un stolt he weer,", "tokens": ["De", "Garr", "de", "full", "mit", "Jun\u00b7ker", "Slenz", ",", "so", "grot", "un", "stolt", "he", "weer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "APPR", "NE", "NE", "$,", "ADV", "ADJD", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "De lange Reimer Wimerst\u0119d de keem un steek em d\u0153r.", "tokens": ["De", "lan\u00b7ge", "Rei\u00b7mer", "Wi\u00b7mer\u00b7st\u0119d", "de", "keem", "un", "steek", "em", "d\u0153r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Mit nauer Noth, in Angst un Sorg keem K\u00f6nig Hans dervan;", "tokens": ["Mit", "nau\u00b7er", "Noth", ",", "in", "Angst", "un", "Sorg", "keem", "K\u00f6\u00b7nig", "Hans", "der\u00b7van", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "FM", "NN", "ADJA", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "In M\u00f6ldorp leet he Beer un Win un Bradens inne Pann.", "tokens": ["In", "M\u00f6l\u00b7dorp", "leet", "he", "Beer", "un", "Win", "un", "Bra\u00b7dens", "in\u00b7ne", "Pann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.15": {"line.1": {"text": "Dat gev en Smaus na Noth un Dod, un ", "tokens": ["Dat", "gev", "en", "Smaus", "na", "Noth", "un", "Dod", ",", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dat mak de D\u00fcwels Isebrand un de Dusendd\u00fcwelswarf!", "tokens": ["Dat", "mak", "de", "D\u00fc\u00b7wels", "I\u00b7se\u00b7brand", "un", "de", "Du\u00b7send\u00b7d\u00fc\u00b7wels\u00b7warf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}