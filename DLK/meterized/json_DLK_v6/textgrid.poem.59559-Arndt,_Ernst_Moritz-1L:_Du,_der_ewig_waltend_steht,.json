{"textgrid.poem.59559": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, der ewig waltend steht,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der ewig waltend steht,", "tokens": ["Du", ",", "der", "e\u00b7wig", "wal\u00b7tend", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re, Vater, mein Gebet:", "tokens": ["H\u00f6\u00b7re", ",", "Va\u00b7ter", ",", "mein", "Ge\u00b7bet", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Denn mich dr\u00fcckt die b\u00f6se Stunde,", "tokens": ["Denn", "mich", "dr\u00fcckt", "die", "b\u00f6\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mich brennt die hei\u00dfe Wunde,", "tokens": ["Denn", "mich", "brennt", "die", "hei\u00b7\u00dfe", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tief im Herzen brennt sie sehr,", "tokens": ["Tief", "im", "Her\u00b7zen", "brennt", "sie", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4rst du nicht, w\u00e4r' ich nicht mehr.", "tokens": ["W\u00e4rst", "du", "nicht", ",", "w\u00e4r'", "ich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn du, alter treuer Gott,", "tokens": ["Denn", "du", ",", "al\u00b7ter", "treu\u00b7er", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alter lieber deutscher Gott,", "tokens": ["Al\u00b7ter", "lie\u00b7ber", "deut\u00b7scher", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast mit M\u00e4nnern und mit Rossen", "tokens": ["Hast", "mit", "M\u00e4n\u00b7nern", "und", "mit", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber mich dich ausgegossen,", "tokens": ["\u00dc\u00b7ber", "mich", "dich", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich in Staub getreten sehr,", "tokens": ["Mich", "in", "Staub", "ge\u00b7tre\u00b7ten", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich bin kein Freier mehr.", "tokens": ["Und", "ich", "bin", "kein", "Frei\u00b7er", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein eitles, schlechtes Volk", "tokens": ["Und", "ein", "eit\u00b7les", ",", "schlech\u00b7tes", "Volk"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00e4ut mit Henkerbeil und Dolch,", "tokens": ["Dr\u00e4ut", "mit", "Hen\u00b7ker\u00b7beil", "und", "Dolch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00e4ut mit Galgen und mit Stricken,", "tokens": ["Dr\u00e4ut", "mit", "Gal\u00b7gen", "und", "mit", "Stri\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich mu\u00df mich schweigend b\u00fccken:", "tokens": ["Und", "ich", "mu\u00df", "mich", "schwei\u00b7gend", "b\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die vor meinen V\u00e4tern flohn,", "tokens": ["Die", "vor", "mei\u00b7nen", "V\u00e4\u00b7tern", "flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprechen meiner Ehre Hohn.", "tokens": ["Spre\u00b7chen", "mei\u00b7ner", "Eh\u00b7re", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Eignen stehn dabei,", "tokens": ["Und", "die", "Eig\u00b7nen", "stehn", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freveln mit in B\u00fcberei,", "tokens": ["Fre\u00b7veln", "mit", "in", "B\u00fc\u00b7be\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Knechtisch wedeln deutsche Hunde,", "tokens": ["Knech\u00b7tisch", "we\u00b7deln", "deut\u00b7sche", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wedeln sich das Brot zum Munde,", "tokens": ["We\u00b7deln", "sich", "das", "Brot", "zum", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schreien gegen Vaterland,", "tokens": ["Schrei\u00b7en", "ge\u00b7gen", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gegen dich in Trug und Schand';", "tokens": ["Ge\u00b7gen", "dich", "in", "Trug", "und", "Schand'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schalken gegen Recht und Licht", "tokens": ["Schal\u00b7ken", "ge\u00b7gen", "Recht", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Weiberangesicht,", "tokens": ["Mit", "dem", "Wei\u00b7be\u00b7ran\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wert, da\u00df sie mit schlaffen H\u00e4nden", "tokens": ["Wert", ",", "da\u00df", "sie", "mit", "schlaf\u00b7fen", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nadel, Kn\u00e4ul und Spindel wenden.", "tokens": ["Na\u00b7del", ",", "Kn\u00e4ul", "und", "Spin\u00b7del", "wen\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O die Buben! Da\u00df dein Strahl", "tokens": ["O", "die", "Bu\u00b7ben", "!", "Da\u00df", "dein", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie zerschmettre tausendmal!", "tokens": ["Sie", "zer\u00b7schmett\u00b7re", "tau\u00b7send\u00b7mal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann wann der Bandit sich kr\u00e4nzt", "tokens": ["Dann", "wann", "der", "Ban\u00b7dit", "sich", "kr\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und der P\u00f6bel hundeschw\u00e4nzt,", "tokens": ["Und", "der", "P\u00f6\u00b7bel", "hun\u00b7de\u00b7schw\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Toren Kron' und Zepter halten,", "tokens": ["To\u00b7ren", "Kron'", "und", "Zep\u00b7ter", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann verstehen wir dein Walten,", "tokens": ["Dann", "ver\u00b7ste\u00b7hen", "wir", "dein", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tod und Jammer \u00fcberall", "tokens": ["Tod", "und", "Jam\u00b7mer", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutet's mit verborgnem Schall.", "tokens": ["Deu\u00b7tet's", "mit", "ver\u00b7borg\u00b7nem", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber z\u00fcchtigst du gleich schwer,", "tokens": ["A\u00b7ber", "z\u00fcch\u00b7tigst", "du", "gleich", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ssest du doch nimmermehr,", "tokens": ["L\u00e4s\u00b7sest", "du", "doch", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast die Sehnsucht uns gegeben", "tokens": ["Hast", "die", "Sehn\u00b7sucht", "uns", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem lichten G\u00f6tterleben,", "tokens": ["Nach", "dem", "lich\u00b7ten", "G\u00f6t\u00b7ter\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem hochgebornen Recht,", "tokens": ["Nach", "dem", "hoch\u00b7ge\u00b7bor\u00b7nen", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nennst uns selber dein Geschlecht.", "tokens": ["Nennst", "uns", "sel\u00b7ber", "dein", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf zur Tugend und zum Licht", "tokens": ["Auf", "zur", "Tu\u00b7gend", "und", "zum", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du uns das Herz gericht't:", "tokens": ["Hast", "du", "uns", "das", "Herz", "ge\u00b7richt't", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also klinget dein Gebot.", "tokens": ["Al\u00b7so", "klin\u00b7get", "dein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Denn des Mannes ganzen Wert", "tokens": ["Denn", "des", "Man\u00b7nes", "gan\u00b7zen", "Wert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt die Zunge und das Schwert;", "tokens": ["H\u00e4lt", "die", "Zun\u00b7ge", "und", "das", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will das letzte nicht mehr schneiden,", "tokens": ["Will", "das", "letz\u00b7te", "nicht", "mehr", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df die erste schon es leiden:", "tokens": ["Mu\u00df", "die", "ers\u00b7te", "schon", "es", "lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4nnerarm und M\u00e4nnerwort", "tokens": ["M\u00e4n\u00b7ner\u00b7arm", "und", "M\u00e4n\u00b7ner\u00b7wort"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Gl\u00fccks- und Freiheitshort.", "tokens": ["Ist", "der", "Gl\u00fccks", "und", "Frei\u00b7heits\u00b7hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}