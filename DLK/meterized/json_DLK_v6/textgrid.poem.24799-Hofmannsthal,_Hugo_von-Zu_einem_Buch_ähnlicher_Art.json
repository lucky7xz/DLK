{"textgrid.poem.24799": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Zu einem Buch \u00e4hnlicher Art", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Merkt auf, merkt auf! Die Zeit ist sonderbar,", "tokens": ["Merkt", "auf", ",", "merkt", "auf", "!", "Die", "Zeit", "ist", "son\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sonderbare Kinder hat sie: Uns!", "tokens": ["Und", "son\u00b7der\u00b7ba\u00b7re", "Kin\u00b7der", "hat", "sie", ":", "Uns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "$.", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer allzusehr verliebt ist in das S\u00fc\u00dfe,", "tokens": ["Wer", "all\u00b7zu\u00b7sehr", "ver\u00b7liebt", "ist", "in", "das", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ertr\u00e4gt uns nicht, denn unsre Art ist herb,", "tokens": ["Er\u00b7tr\u00e4gt", "uns", "nicht", ",", "denn", "uns\u00b7re", "Art", "ist", "herb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und unsre Unterhaltung wunderlich.", "tokens": ["Und", "uns\u00b7re", "Un\u00b7ter\u00b7hal\u00b7tung", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbschlagt eine kleine B\u00fchne auf im Zimmer,", "tokens": ["\u00bb", "schlagt", "ei\u00b7ne", "klei\u00b7ne", "B\u00fch\u00b7ne", "auf", "im", "Zim\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn die Haustochter will Theater spielen!\u00ab", "tokens": ["Denn", "die", "Haus\u00b7toch\u00b7ter", "will", "The\u00b7a\u00b7ter", "spie\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Meint ihr, sie wird als kleine Muse kommen,", "tokens": ["Meint", "ihr", ",", "sie", "wird", "als", "klei\u00b7ne", "Mu\u00b7se", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit offnem Haar, und in den blo\u00dfen Armen", "tokens": ["Mit", "off\u00b7nem", "Haar", ",", "und", "in", "den", "blo\u00b7\u00dfen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wird eine leichte goldne Leier liegen?", "tokens": ["Wird", "ei\u00b7ne", "leich\u00b7te", "gold\u00b7ne", "Lei\u00b7er", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Meint ihr, als Sch\u00e4ferin, ein wei\u00dfes Lamm", "tokens": ["Meint", "ihr", ",", "als", "Sch\u00e4\u00b7fe\u00b7rin", ",", "ein", "wei\u00b7\u00dfes", "Lamm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Am blauen Seidenband und auf den Lippen", "tokens": ["Am", "blau\u00b7en", "Sei\u00b7den\u00b7band", "und", "auf", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein L\u00e4cheln, s\u00fc\u00df und billig wie die Reime", "tokens": ["Ein", "L\u00e4\u00b7cheln", ",", "s\u00fc\u00df", "und", "bil\u00b7lig", "wie", "die", "Rei\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In Sch\u00e4ferspielen? Auf! und geht hinaus!", "tokens": ["In", "Sch\u00e4\u00b7fer\u00b7spie\u00b7len", "?", "Auf", "!", "und", "geht", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "$.", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Geht fort, ich bitt euch, wenn ihr das erwartet!", "tokens": ["Geht", "fort", ",", "ich", "bitt", "euch", ",", "wenn", "ihr", "das", "er\u00b7war\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ihr k\u00f6nnt uns nicht ertragen, wir sind anders!", "tokens": ["Ihr", "k\u00f6nnt", "uns", "nicht", "er\u00b7tra\u00b7gen", ",", "wir", "sind", "an\u00b7ders", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wir haben aus dem Leben, das wir leben,", "tokens": ["Wir", "ha\u00b7ben", "aus", "dem", "Le\u00b7ben", ",", "das", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein Spiel gemacht, und unsere Wahrheit gleitet", "tokens": ["Ein", "Spiel", "ge\u00b7macht", ",", "und", "un\u00b7se\u00b7re", "Wahr\u00b7heit", "glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Mit unserer Kom\u00f6die durcheinander", "tokens": ["Mit", "un\u00b7se\u00b7rer", "Ko\u00b7m\u00f6\u00b7die", "durch\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wie eines Taschenspielers hohle Becher \u2013", "tokens": ["Wie", "ei\u00b7nes", "Ta\u00b7schen\u00b7spie\u00b7lers", "hoh\u00b7le", "Be\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Je mehr ihr hinseht, desto mehr betrogen!", "tokens": ["Je", "mehr", "ihr", "hin\u00b7seht", ",", "des\u00b7to", "mehr", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Wir geben kleine Fetzen unsres Selbst", "tokens": ["Wir", "ge\u00b7ben", "klei\u00b7ne", "Fet\u00b7zen", "uns\u00b7res", "Selbst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "F\u00fcr Puppenkleider. Wie die wahren Worte", "tokens": ["F\u00fcr", "Pup\u00b7pen\u00b7klei\u00b7der", ".", "Wie", "die", "wah\u00b7ren", "Wor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "(an denen L\u00e4cheln oder Tr\u00e4nen h\u00e4ngen", "tokens": ["(", "an", "de\u00b7nen", "L\u00e4\u00b7cheln", "o\u00b7der", "Tr\u00e4\u00b7nen", "h\u00e4n\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PRELS", "NN", "KON", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Gleich Tau an einem Busch mit rauhen Bl\u00e4ttern)", "tokens": ["Gleich", "Tau", "an", "ei\u00b7nem", "Busch", "mit", "rau\u00b7hen", "Bl\u00e4t\u00b7tern", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Erschrecken m\u00fcssen, wenn sie sich erkennen,", "tokens": ["Er\u00b7schre\u00b7cken", "m\u00fcs\u00b7sen", ",", "wenn", "sie", "sich", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMINF", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "In dieses Spiel verflochten, halb geschminkt,", "tokens": ["In", "die\u00b7ses", "Spiel", "ver\u00b7floch\u00b7ten", ",", "halb", "ge\u00b7schminkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Halb noch sich selber gleich, und so entfremdet", "tokens": ["Halb", "noch", "sich", "sel\u00b7ber", "gleich", ",", "und", "so", "ent\u00b7frem\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "PRF", "ADV", "ADV", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der gro\u00dfen Unschuld, die sie fr\u00fcher hatten!", "tokens": ["Der", "gro\u00b7\u00dfen", "Un\u00b7schuld", ",", "die", "sie", "fr\u00fc\u00b7her", "hat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ward je ein so verworrnes Spiel gespielt?", "tokens": ["Ward", "je", "ein", "so", "ver\u00b7worr\u00b7nes", "Spiel", "ge\u00b7spielt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es stiehlt uns von uns selbst und ist nicht lieblich", "tokens": ["Es", "stiehlt", "uns", "von", "uns", "selbst", "und", "ist", "nicht", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "ADV", "KON", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Tanzen oder auf dem Wasser Singen,", "tokens": ["Wie", "Tan\u00b7zen", "o\u00b7der", "auf", "dem", "Was\u00b7ser", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch ist es das reichste an Verf\u00fchrung", "tokens": ["Und", "doch", "ist", "es", "das", "reichs\u00b7te", "an", "Ver\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Von allen Spielen, die wir Kinder wissen,", "tokens": ["Von", "al\u00b7len", "Spie\u00b7len", ",", "die", "wir", "Kin\u00b7der", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir Kinder dieser sonderbaren Zeit.", "tokens": ["Wir", "Kin\u00b7der", "die\u00b7ser", "son\u00b7der\u00b7ba\u00b7ren", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was wollt ihr noch? So sind wir nun einmal,", "tokens": ["Was", "wollt", "ihr", "noch", "?", "So", "sind", "wir", "nun", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch wollt ihr wirklich solche Dinge h\u00f6ren,", "tokens": ["Doch", "wollt", "ihr", "wirk\u00b7lich", "sol\u00b7che", "Din\u00b7ge", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleibt immerhin! Wir lassen uns nicht st\u00f6ren.", "tokens": ["Bleibt", "im\u00b7mer\u00b7hin", "!", "Wir", "las\u00b7sen", "uns", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}