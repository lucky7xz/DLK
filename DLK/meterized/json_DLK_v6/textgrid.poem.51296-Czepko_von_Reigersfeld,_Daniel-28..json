{"textgrid.poem.51296": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Erde, welcher Mensch, der dich mit F\u00fc\u00dfen tritt,", "tokens": ["O", "Er\u00b7de", ",", "wel\u00b7cher", "Mensch", ",", "der", "dich", "mit", "F\u00fc\u00b7\u00dfen", "tritt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dich bes\u00e4t und pfl\u00fcgt, der, was du hast vergraben,", "tokens": ["Der", "dich", "be\u00b7s\u00e4t", "und", "pfl\u00fcgt", ",", "der", ",", "was", "du", "hast", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "VVFIN", "$,", "PRELS", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sucht, und dich starck verwundt, umb Geld und Gut zu haben,", "tokens": ["Sucht", ",", "und", "dich", "starck", "ver\u00b7wundt", ",", "umb", "Geld", "und", "Gut", "zu", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPER", "ADJD", "VVPP", "$,", "KOUI", "NN", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nihmt dieser Gutthat wahr, damit er wird besch\u00fctt.", "tokens": ["Nihmt", "die\u00b7ser", "Gut\u00b7that", "wahr", ",", "da\u00b7mit", "er", "wird", "be\u00b7sch\u00fctt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df Gra\u00df und Kraut entsteht, da\u00df Gold und mancher Stein", "tokens": ["Da\u00df", "Gra\u00df", "und", "Kraut", "ent\u00b7steht", ",", "da\u00df", "Gold", "und", "man\u00b7cher", "Stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,", "KOUS", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll Tugend ist und Krafft, da\u00df alles wird erhalten,", "tokens": ["Voll", "Tu\u00b7gend", "ist", "und", "Krafft", ",", "da\u00df", "al\u00b7les", "wird", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "KON", "NN", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df alles wird ern\u00e4hrt, da\u00df wir selbst sind und alten,", "tokens": ["Da\u00df", "al\u00b7les", "wird", "er\u00b7n\u00e4hrt", ",", "da\u00df", "wir", "selbst", "sind", "und", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ADV", "VAFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df unser Geist sich regt, o Erd, ist dein allein.", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "sich", "regt", ",", "o", "Erd", ",", "ist", "dein", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus dir entspringt die Lufft, aus der das Leben geht,", "tokens": ["Aus", "dir", "ent\u00b7springt", "die", "Lufft", ",", "aus", "der", "das", "Le\u00b7ben", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Mensch, du selbst bist Erd' und solst zur Erde werden,", "tokens": ["O", "Mensch", ",", "du", "selbst", "bist", "Erd'", "und", "solst", "zur", "Er\u00b7de", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADV", "VAFIN", "NN", "KON", "VMFIN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erd ist dein Unterhalt! O Wunder! aus der Erden", "tokens": ["Erd", "ist", "dein", "Un\u00b7ter\u00b7halt", "!", "O", "Wun\u00b7der", "!", "aus", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "NE", "NN", "$.", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wird Salz gemacht, drauff selbst des Himmels Feste steht.", "tokens": ["Wird", "Salz", "ge\u00b7macht", ",", "drauff", "selbst", "des", "Him\u00b7mels", "Fes\u00b7te", "steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,", "PAV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}