{"textgrid.poem.44391": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Napoleon", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So stehst du still, du unruhvolles Herz,", "tokens": ["So", "stehst", "du", "still", ",", "du", "un\u00b7ruh\u00b7vol\u00b7les", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bist gegangen zu der stillen Erde?", "tokens": ["Und", "bist", "ge\u00b7gan\u00b7gen", "zu", "der", "stil\u00b7len", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was f\u00fcnfzig Jahr, voll Hoheit und Beschwerde,", "tokens": ["Was", "f\u00fcnf\u00b7zig", "Jahr", ",", "voll", "Ho\u00b7heit", "und", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Heldenlust nicht gab und Heldenschmerz,", "tokens": ["Voll", "Hel\u00b7den\u00b7lust", "nicht", "gab", "und", "Hel\u00b7den\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKNEG", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist dir geworden in der stillen Erde,", "tokens": ["Ist", "dir", "ge\u00b7wor\u00b7den", "in", "der", "stil\u00b7len", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VAPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Sohn des Schicksals stiegest du hinab,", "tokens": ["Ein", "Sohn", "des", "Schick\u00b7sals", "stie\u00b7gest", "du", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verh\u00fcllt wie deine Mutter, sei dein Grab.", "tokens": ["Ver\u00b7h\u00fcllt", "wie", "dei\u00b7ne", "Mut\u00b7ter", ",", "sei", "dein", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Fieber warst du einer kranken Zeit,", "tokens": ["Das", "Fie\u00b7ber", "warst", "du", "ei\u00b7ner", "kran\u00b7ken", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bestimmt vielleicht, des \u00dcbels Sitz zu heben,", "tokens": ["Be\u00b7stimmt", "viel\u00b7leicht", ",", "des", "\u00dc\u00b7bels", "Sitz", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So flammtest du durchs aufgeregte Leben;", "tokens": ["So", "flamm\u00b7test", "du", "durchs", "auf\u00b7ge\u00b7reg\u00b7te", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wie des Krankenlagers \u00c4ngstlichkeit", "tokens": ["Doch", "wie", "des", "Kran\u00b7ken\u00b7la\u00b7gers", "\u00c4ngst\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Schienst du der Feind allein auch aller Ruh", "tokens": ["Schienst", "du", "der", "Feind", "al\u00b7lein", "auch", "al\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und trugst die Schuld, die fr\u00fcher war, als du.", "tokens": ["Und", "trugst", "die", "Schuld", ",", "die", "fr\u00fc\u00b7her", "war", ",", "als", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was sie ges\u00fcndiget ohn Unterla\u00df,", "tokens": ["Was", "sie", "ge\u00b7s\u00fcn\u00b7di\u00b7get", "ohn", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was sie gefrevelt seit den fr\u00fchsten Tagen,", "tokens": ["Was", "sie", "ge\u00b7fre\u00b7velt", "seit", "den", "fr\u00fchs\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ward all zusammen auf dein Haupt getragen,", "tokens": ["Ward", "all", "zu\u00b7sam\u00b7men", "auf", "dein", "Haupt", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du duldetest f\u00fcr alle aller Ha\u00df,", "tokens": ["Du", "dul\u00b7de\u00b7test", "f\u00fcr", "al\u00b7le", "al\u00b7ler", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dich lie\u00dfen sie nach jenem Schimmer jagen,", "tokens": ["Dich", "lie\u00b7\u00dfen", "sie", "nach", "je\u00b7nem", "Schim\u00b7mer", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In dem sich jeder selber gern gesonnt,", "tokens": ["In", "dem", "sich", "je\u00b7der", "sel\u00b7ber", "gern", "ge\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie du, gewollt, nur nicht, wie du, gekonnt.", "tokens": ["Wie", "du", ",", "ge\u00b7wollt", ",", "nur", "nicht", ",", "wie", "du", ",", "ge\u00b7konnt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VMPP", "$,", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn, seit du fort, flie\u00dft nun nicht mehr das Blut,", "tokens": ["Denn", ",", "seit", "du", "fort", ",", "flie\u00dft", "nun", "nicht", "mehr", "das", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKVZ", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "In dem vor dir schon alle Felder rannen?", "tokens": ["In", "dem", "vor", "dir", "schon", "al\u00b7le", "Fel\u00b7der", "ran\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ward Lohn den wider dich vereinten Mannen?", "tokens": ["Ward", "Lohn", "den", "wi\u00b7der", "dich", "ver\u00b7ein\u00b7ten", "Man\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist heilig das von dir bedrohte Gut?", "tokens": ["Ist", "hei\u00b7lig", "das", "von", "dir", "be\u00b7droh\u00b7te", "Gut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ward Tyrannei entfernt mit dem Tyrannen?", "tokens": ["Ward", "Ty\u00b7ran\u00b7nei", "ent\u00b7fernt", "mit", "dem", "Ty\u00b7ran\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist auf der freien Erde, seit du fort,", "tokens": ["Ist", "auf", "der", "frei\u00b7en", "Er\u00b7de", ",", "seit", "du", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nun wieder frei Gedanke, Meinung, Wort?", "tokens": ["Nun", "wie\u00b7der", "frei", "Ge\u00b7dan\u00b7ke", ",", "Mei\u00b7nung", ",", "Wort", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dich lieben kann ich nicht, dein hartes Amt", "tokens": ["Dich", "lie\u00b7ben", "kann", "ich", "nicht", ",", "dein", "har\u00b7tes", "Amt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War, eine Gei\u00dfel Gottes sein hienieden,", "tokens": ["War", ",", "ei\u00b7ne", "Gei\u00b7\u00dfel", "Got\u00b7tes", "sein", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Schwert hast du gebracht und nicht den Frieden,", "tokens": ["Das", "Schwert", "hast", "du", "ge\u00b7bracht", "und", "nicht", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genug hat dich die Welt darob verdammt;", "tokens": ["Ge\u00b7nug", "hat", "dich", "die", "Welt", "da\u00b7rob", "ver\u00b7dammt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch jetzt sei Urteil vom Gef\u00fchl geschieden!", "tokens": ["Doch", "jetzt", "sei", "Ur\u00b7teil", "vom", "Ge\u00b7f\u00fchl", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Leben liebt und ha\u00dft, der Toten Ruhm", "tokens": ["Das", "Le\u00b7ben", "liebt", "und", "ha\u00dft", ",", "der", "To\u00b7ten", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist der Geschichte heilig Eigentum.", "tokens": ["Ist", "der", "Ge\u00b7schich\u00b7te", "hei\u00b7lig", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Zum mindsten wardst du strahlend hingestellt,", "tokens": ["Zum", "minds\u00b7ten", "wardst", "du", "strah\u00b7lend", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu kleiden unsrer Nacktheit ekle Bl\u00f6\u00dfe,", "tokens": ["Zu", "klei\u00b7den", "uns\u00b7rer", "Nackt\u00b7heit", "ek\u00b7le", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu zeigen, da\u00df noch Ganzheit, Hoheit, Gr\u00f6\u00dfe", "tokens": ["Zu", "zei\u00b7gen", ",", "da\u00df", "noch", "Ganz\u00b7heit", ",", "Ho\u00b7heit", ",", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedenkbar sei in unsrer St\u00fcckelwelt,", "tokens": ["Ge\u00b7denk\u00b7bar", "sei", "in", "uns\u00b7rer", "St\u00fc\u00b7ckel\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sonst wohl selbst im eignen Nichts zerfl\u00f6sse,", "tokens": ["Die", "sonst", "wohl", "selbst", "im", "eig\u00b7nen", "Nichts", "zer\u00b7fl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df noch die Gattung da, die starker Hand", "tokens": ["Da\u00df", "noch", "die", "Gat\u00b7tung", "da", ",", "die", "star\u00b7ker", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bei Cann\u00e4 schlug, bei Thermopyl\u00e4 stand.", "tokens": ["Bei", "Can\u00b7n\u00e4", "schlug", ",", "bei", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und so tritt hin denn zu der Helden Zahl,", "tokens": ["Und", "so", "tritt", "hin", "denn", "zu", "der", "Hel\u00b7den", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die annoch lebet auf der Nachwelt Zungen,", "tokens": ["Die", "an\u00b7noch", "le\u00b7bet", "auf", "der", "Nach\u00b7welt", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Alexander, der die Welt bezwungen,", "tokens": ["Zum", "A\u00b7lex\u00b7an\u00b7der", ",", "der", "die", "Welt", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum C\u00e4sar, der mit tadelnswertrer Wahl", "tokens": ["Zum", "C\u00e4\u00b7sar", ",", "der", "mit", "ta\u00b7delns\u00b7wert\u00b7rer", "Wahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am Rubicon der Herrschaft vorgedrungen,", "tokens": ["Am", "Ru\u00b7bi\u00b7con", "der", "Herr\u00b7schaft", "vor\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum \u2013 Stellt kein Held sich mehr zum Gleichnis ein?", "tokens": ["Zum", "\u2013", "Stellt", "kein", "Held", "sich", "mehr", "zum", "Gleich\u00b7nis", "ein", "?"], "tok