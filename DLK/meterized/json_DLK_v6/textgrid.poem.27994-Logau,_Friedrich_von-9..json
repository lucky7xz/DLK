{"textgrid.poem.27994": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Jungfern, euer Leib, den wo Gewalt verletzet,", "tokens": ["Ihr", "Jung\u00b7fern", ",", "eu\u00b7er", "Leib", ",", "den", "wo", "Ge\u00b7walt", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird Ehren-lose nicht mit Billigkeit gesch\u00e4tzet.", "tokens": ["Wird", "Eh\u00b7ren\u00b7lo\u00b7se", "nicht", "mit", "Bil\u00b7lig\u00b7keit", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Cunninna wei\u00df es wol; wer an um Gunst sie spricht,", "tokens": ["Cun\u00b7nin\u00b7na", "wei\u00df", "es", "wol", ";", "wer", "an", "um", "Gunst", "sie", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "PWS", "APPR", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem gibt sie die und schreyt: O nun, O nein, O nicht!", "tokens": ["Dem", "gibt", "sie", "die", "und", "schreyt", ":", "O", "nun", ",", "O", "nein", ",", "O", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "KON", "VVFIN", "$.", "NE", "ADV", "$,", "NE", "PTKANT", "$,", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}