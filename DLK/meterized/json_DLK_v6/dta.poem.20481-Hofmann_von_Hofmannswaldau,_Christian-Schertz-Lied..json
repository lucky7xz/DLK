{"dta.poem.20481": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als die Venus neulich sasse", "tokens": ["Als", "die", "Ve\u00b7nus", "neu\u00b7lich", "sas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem bade nackt und blo\u00df/", "tokens": ["In", "dem", "ba\u00b7de", "nackt", "und", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADJD", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Cupido aus der schoo\u00df/", "tokens": ["Und", "Cu\u00b7pi\u00b7do", "aus", "der", "schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Von dem liebes-zucker asse/", "tokens": ["Von", "dem", "lie\u00b7bes\u00b7zu\u00b7cker", "as\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeigte sie dem kleinen knaben", "tokens": ["Zeig\u00b7te", "sie", "dem", "klei\u00b7nen", "kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles/ was die frauen haben.", "tokens": ["Al\u00b7les", "/", "was", "die", "frau\u00b7en", "ha\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "PWS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Marmol-h\u00fcgel sah er liegen/", "tokens": ["Mar\u00b7mol\u00b7h\u00fc\u00b7gel", "sah", "er", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von begierden auffgebaut;", "tokens": ["Von", "be\u00b7gier\u00b7den", "auff\u00b7ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach zur mutter \u00fcberlaut:", "tokens": ["Sprach", "zur", "mut\u00b7ter", "\u00fc\u00b7berl\u00b7aut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn werd ich dergleichen kriegen/", "tokens": ["Wenn", "werd", "ich", "derg\u00b7lei\u00b7chen", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mich auch die sch\u00e4ferinnen/", "tokens": ["Da\u00df", "mich", "auch", "die", "sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die damenlieb gewinnen?", "tokens": ["Und", "die", "da\u00b7men\u00b7lieb", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Venus lacht aus vollem munde", "tokens": ["Ve\u00b7nus", "lacht", "aus", "vol\u00b7lem", "mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uber ihren kleinen sohn:", "tokens": ["U\u00b7ber", "ih\u00b7ren", "klei\u00b7nen", "sohn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie sah und merckte schon/", "tokens": ["Denn", "sie", "sah", "und", "merck\u00b7te", "schon", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er was davon verstunde/", "tokens": ["Da\u00df", "er", "was", "da\u00b7von", "ver\u00b7stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PAV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprach: du hast wohl andre sachen/", "tokens": ["Sprach", ":", "du", "hast", "wohl", "and\u00b7re", "sa\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verliebter k\u00f6nnen machen.", "tokens": ["Die", "ver\u00b7lieb\u00b7ter", "k\u00f6n\u00b7nen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unterdessen lie\u00df sie spielen", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "lie\u00df", "sie", "spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine hand auff ihrer brust:", "tokens": ["Sei\u00b7ne", "hand", "auff", "ih\u00b7rer", "brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie merckte/ da\u00df er lust", "tokens": ["Denn", "sie", "merck\u00b7te", "/", "da\u00df", "er", "lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hatte weiter nachzuf\u00fchlen/", "tokens": ["Hat\u00b7te", "wei\u00b7ter", "nach\u00b7zu\u00b7f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df ihr endlich dieser kleine", "tokens": ["Bi\u00df", "ihr", "end\u00b7lich", "die\u00b7ser", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kam an ihre zarte beine.", "tokens": ["Kam", "an", "ih\u00b7re", "zar\u00b7te", "bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als er sich an sie geschmieget/", "tokens": ["Als", "er", "sich", "an", "sie", "ge\u00b7schmie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sprach er: Liebes m\u00fctterlein/", "tokens": ["Sprach", "er", ":", "Lie\u00b7bes", "m\u00fct\u00b7ter\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer hat an das dicke bein", "tokens": ["Wer", "hat", "an", "das", "di\u00b7cke", "bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch die wunde zugef\u00fcget?", "tokens": ["Euch", "die", "wun\u00b7de", "zu\u00b7ge\u00b7f\u00fc\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcst ihr weiber denn auff erden", "tokens": ["M\u00fcst", "ihr", "wei\u00b7ber", "denn", "auff", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Alle so verwundet werden?", "tokens": ["Al\u00b7le", "so", "ver\u00b7wun\u00b7det", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Venus konte nichts mehr sagen/", "tokens": ["Ve\u00b7nus", "kon\u00b7te", "nichts", "mehr", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als: Du kleiner b\u00f6sewicht/", "tokens": ["Als", ":", "Du", "klei\u00b7ner", "b\u00f6\u00b7se\u00b7wicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Packe dich/ du solst noch nicht", "tokens": ["Pa\u00b7cke", "dich", "/", "du", "solst", "noch", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VMFIN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dergleichen sachen fragen.", "tokens": ["Nach", "derg\u00b7lei\u00b7chen", "sa\u00b7chen", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wunden/ die von liebes-pfeilen", "tokens": ["Wun\u00b7den", "/", "die", "von", "lie\u00b7bes\u00b7pfei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ART", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommen/ die sind nicht zu heilen.", "tokens": ["Kom\u00b7men", "/", "die", "sind", "nicht", "zu", "hei\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}