{"textgrid.poem.37218": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Cythereens Fest", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von erhabnen Marmorstufen", "tokens": ["Von", "er\u00b7hab\u00b7nen", "Mar\u00b7mor\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Tempelhalle l\u00e4\u00dft", "tokens": ["Sei\u00b7ner", "Tem\u00b7pel\u00b7hal\u00b7le", "l\u00e4\u00dft"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amor Cythereens Fest", "tokens": ["A\u00b7mor", "Cy\u00b7ther\u00b7eens", "Fest"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aus in alle Winde rufen.", "tokens": ["Aus", "in", "al\u00b7le", "Win\u00b7de", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Musen, Chariten, Najaden,", "tokens": ["Mu\u00b7sen", ",", "Cha\u00b7ri\u00b7ten", ",", "Na\u00b7ja\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Was von g\u00f6ttlicher Natur", "tokens": ["Was", "von", "g\u00f6tt\u00b7li\u00b7cher", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Olymp und Meer und Flur", "tokens": ["In", "O\u00b7lymp", "und", "Meer", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebet, wird dazu geladen.", "tokens": ["Le\u00b7bet", ",", "wird", "da\u00b7zu", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Selbst was Himmlischen entspro\u00dfen,", "tokens": ["Selbst", "was", "Himm\u00b7li\u00b7schen", "ent\u00b7spro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den G\u00f6ttern sonst nicht zecht,", "tokens": ["Mit", "den", "G\u00f6t\u00b7tern", "sonst", "nicht", "zecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird gepriesen, \u2013 das Geschlecht", "tokens": ["Wird", "ge\u00b7prie\u00b7sen", ",", "\u2013", "das", "Ge\u00b7schlecht"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Menschen ausgeschlo\u00dfen.", "tokens": ["Nur", "der", "Men\u00b7schen", "aus\u00b7ge\u00b7schlo\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Neugier lockt auch Chloen. Friedlich", "tokens": ["Neu\u00b7gier", "lockt", "auch", "Chloen", ".", "Fried\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ADV", "NE", "$.", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "L\u00e4\u00dft sie Amor selber ein.", "tokens": ["L\u00e4\u00dft", "sie", "A\u00b7mor", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbvon der Sippschaft mu\u00df sie sein,\u00ab", "tokens": ["\u00bb", "von", "der", "Sipp\u00b7schaft", "mu\u00df", "sie", "sein", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VMFIN", "PPER", "VAINF", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht er, \u00bbsie ist gar zu niedlich.\u00ab", "tokens": ["Spricht", "er", ",", "\u00bb", "sie", "ist", "gar", "zu", "nied\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}