{"dta.poem.14995": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Feindes List und Spott.", "tokens": ["Durch", "Fein\u00b7des", "List", "und", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott wird es wohl verwalten,", "tokens": ["Gott", "wird", "es", "wohl", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist der Freiheit Gott.", "tokens": ["Er", "ist", "der", "Frei\u00b7heit", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df nur den W\u00fcthrich drohen,", "tokens": ["La\u00df", "nur", "den", "W\u00fcth\u00b7rich", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Dort reicht er nicht hinauf.", "tokens": ["Dort", "reicht", "er", "nicht", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einst bricht in heil'gen Lohen,", "tokens": ["Einst", "bricht", "in", "heil'\u00b7gen", "Lo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch deine Freiheit auf.", "tokens": ["Doch", "dei\u00b7ne", "Frei\u00b7heit", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Glimmend durch lange Schmerzen,", "tokens": ["Glim\u00b7mend", "durch", "lan\u00b7ge", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Hat sie der Tod verkl\u00e4rt.", "tokens": ["Hat", "sie", "der", "Tod", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Millionen Herzen,", "tokens": ["Aus", "Mil\u00b7lion\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit edlem Blut gen\u00e4hrt.", "tokens": ["Mit", "ed\u00b7lem", "Blut", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wird seinen Thron zermalmen,", "tokens": ["Wird", "sei\u00b7nen", "Thron", "zer\u00b7mal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schmelzt deine Fesseln los,", "tokens": ["Schmelzt", "dei\u00b7ne", "Fes\u00b7seln", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und pflanzt die gl\u00fch'nden Palmen", "tokens": ["Und", "pflanzt", "die", "gl\u00fch'n\u00b7den", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf deutscher Helden Moos.", "tokens": ["Auf", "deut\u00b7scher", "Hel\u00b7den", "Moos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum la\u00df dich nicht zerspalten", "tokens": ["Drum", "la\u00df", "dich", "nicht", "zer\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Feindes List und Spott.", "tokens": ["Durch", "Fein\u00b7des", "List", "und", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wird es wohl verwalten,", "tokens": ["Gott", "wird", "es", "wohl", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist der Freiheit Gott.", "tokens": ["Er", "ist", "der", "Frei\u00b7heit", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}