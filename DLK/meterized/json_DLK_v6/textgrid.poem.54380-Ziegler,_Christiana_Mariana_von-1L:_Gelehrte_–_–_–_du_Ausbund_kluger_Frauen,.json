{"textgrid.poem.54380": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gelehrte \u2013 \u2013 \u2013 du Ausbund kluger Frauen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelehrte \u2013 \u2013 \u2013 du Ausbund kluger Frauen,", "tokens": ["Ge\u00b7lehr\u00b7te", "\u2013", "\u2013", "\u2013", "du", "Aus\u00b7bund", "klu\u00b7ger", "Frau\u00b7en", ","], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "$(", "$(", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was l\u00e4\u00dft uns nicht dein Buch f\u00fcr nette Lieder schauen!", "tokens": ["Was", "l\u00e4\u00dft", "uns", "nicht", "dein", "Buch", "f\u00fcr", "net\u00b7te", "Lie\u00b7der", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Einfall ist so sch\u00f6n, so geistreich, so beliebt,", "tokens": ["Dein", "Ein\u00b7fall", "ist", "so", "sch\u00f6n", ",", "so", "geist\u00b7reich", ",", "so", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dir der Leser gleich gerechten Beyfall giebt.", "tokens": ["Da\u00df", "dir", "der", "Le\u00b7ser", "gleich", "ge\u00b7rech\u00b7ten", "Bey\u00b7fall", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Apollo mu\u00df dir selbst die Hand im Dichten f\u00fchren;", "tokens": ["A\u00b7pol\u00b7lo", "mu\u00df", "dir", "selbst", "die", "Hand", "im", "Dich\u00b7ten", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Du kannst den Thon so rein, so sanft die Seiten r\u00fchren,", "tokens": ["Du", "kannst", "den", "Thon", "so", "rein", ",", "so", "sanft", "die", "Sei\u00b7ten", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df uns dein holdes Spiel auch in der Fern ergetzt,", "tokens": ["Da\u00df", "uns", "dein", "hol\u00b7des", "Spiel", "auch", "in", "der", "Fern", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und unser wallend Herz in Glut und Flammen setzt.", "tokens": ["Und", "un\u00b7ser", "wal\u00b7lend", "Herz", "in", "Glut", "und", "Flam\u00b7men", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es nimmt ein jeder Theil an den Ergetzungsstunden:", "tokens": ["Es", "nimmt", "ein", "je\u00b7der", "Theil", "an", "den", "Er\u00b7get\u00b7zungs\u00b7stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der Titel ist mit Recht zu diesem Buch erfunden.", "tokens": ["Der", "Ti\u00b7tel", "ist", "mit", "Recht", "zu", "die\u00b7sem", "Buch", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Drum fahre ferner fort, brauch den gesch\u00e4rften Kiel,", "tokens": ["Drum", "fah\u00b7re", "fer\u00b7ner", "fort", ",", "brauch", "den", "ge\u00b7sch\u00e4rf\u00b7ten", "Kiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nicht, da\u00df ich dich nach Art der Schmeichler loben will;", "tokens": ["Nicht", ",", "da\u00df", "ich", "dich", "nach", "Art", "der", "Schmeich\u00b7ler", "lo\u00b7ben", "will", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es ist der M\u00fche wehrt; dein emsiges Bem\u00fchen", "tokens": ["Es", "ist", "der", "M\u00fc\u00b7he", "wehrt", ";", "dein", "em\u00b7si\u00b7ges", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist dir bereits zum Ruhm in aller Welt gediehen.", "tokens": ["Ist", "dir", "be\u00b7reits", "zum", "Ruhm", "in", "al\u00b7ler", "Welt", "ge\u00b7die\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie tr\u00e4gt sich Fama nicht mit deinem grossen Geist,", "tokens": ["Wie", "tr\u00e4gt", "sich", "Fa\u00b7ma", "nicht", "mit", "dei\u00b7nem", "gros\u00b7sen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "NN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der allem Frauenvolk ein rechtes Muster hei\u00dft!", "tokens": ["Der", "al\u00b7lem", "Frau\u00b7en\u00b7volk", "ein", "rech\u00b7tes", "Mus\u00b7ter", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und wie? wie k\u00f6nnt ich dich, Geehrte, wohl vergessen,", "tokens": ["Und", "wie", "?", "wie", "k\u00f6nnt", "ich", "dich", ",", "Ge\u00b7ehr\u00b7te", ",", "wohl", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du bist in meiner Gunst l\u00e4ngst oben an gesessen.", "tokens": ["Du", "bist", "in", "mei\u00b7ner", "Gunst", "l\u00e4ngst", "o\u00b7ben", "an", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als ich das erstemal dein liebstes Siegel brach,", "tokens": ["Als", "ich", "das", "ers\u00b7te\u00b7mal", "dein", "liebs\u00b7tes", "Sie\u00b7gel", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So folgte sie dir gleich mit starken Schritten nach.", "tokens": ["So", "folg\u00b7te", "sie", "dir", "gleich", "mit", "star\u00b7ken", "Schrit\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Antwort bin ich dir in etwas schuldig blieben;", "tokens": ["Die", "Ant\u00b7wort", "bin", "ich", "dir", "in", "et\u00b7was", "schul\u00b7dig", "blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "APPR", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein Vorsatz war es nicht; ein Brief ist bald geschrieben.", "tokens": ["Mein", "Vor\u00b7satz", "war", "es", "nicht", ";", "ein", "Brief", "ist", "bald", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich fuhr bald da bald dort bey Sommerszeit herum,", "tokens": ["Ich", "fuhr", "bald", "da", "bald", "dort", "bey", "Som\u00b7mers\u00b7zeit", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und sah mich hier und dar nach klugen Leuten um.", "tokens": ["Und", "sah", "mich", "hier", "und", "dar", "nach", "klu\u00b7gen", "Leu\u00b7ten", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "PTKVZ", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch als ich wieder kam; erwege nur das Schrecken,", "tokens": ["Doch", "als", "ich", "wie\u00b7der", "kam", ";", "er\u00b7we\u00b7ge", "nur", "das", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So suchte mich der Tod mit Sand und Stein zu decken.", "tokens": ["So", "such\u00b7te", "mich", "der", "Tod", "mit", "Sand", "und", "Stein", "zu", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da galt kein laufen nicht, er holte mich doch ein;", "tokens": ["Da", "galt", "kein", "lau\u00b7fen", "nicht", ",", "er", "hol\u00b7te", "mich", "doch", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Jedoch der Himmel half und willigte nicht drein.", "tokens": ["Je\u00b7doch", "der", "Him\u00b7mel", "half", "und", "wil\u00b7lig\u00b7te", "nicht", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Entgieng ich gleich der Wut, nicht g\u00e4nzlich zu erblassen,", "tokens": ["Ent\u00b7gieng", "ich", "gleich", "der", "Wut", ",", "nicht", "g\u00e4nz\u00b7lich", "zu", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So must ich doch dem Kopf Zeit zum Erholen lassen.", "tokens": ["So", "must", "ich", "doch", "dem", "Kopf", "Zeit", "zum", "Er\u00b7ho\u00b7len", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Die Lust zum Dichten schwand, Geist, Feuer, Muth und Sinn", "tokens": ["Die", "Lust", "zum", "Dich\u00b7ten", "schwand", ",", "Geist", ",", "Feu\u00b7er", ",", "Muth", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Verlie\u00df den matten Leib; doch da die Furcht dahin,", "tokens": ["Ver\u00b7lie\u00df", "den", "mat\u00b7ten", "Leib", ";", "doch", "da", "die", "Furcht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ergreif ich dieses Blat, nach deinem Wohl zu fragen,", "tokens": ["Er\u00b7greif", "ich", "die\u00b7ses", "Blat", ",", "nach", "dei\u00b7nem", "Wohl", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und will ergebnen Dank vor das Geschenke sagen,", "tokens": ["Und", "will", "er\u00b7geb\u00b7nen", "Dank", "vor", "das", "Ge\u00b7schen\u00b7ke", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Auch zeigen, da\u00df ich nicht nur blo\u00df in Worten bin.", "tokens": ["Auch", "zei\u00b7gen", ",", "da\u00df", "ich", "nicht", "nur", "blo\u00df", "in", "Wor\u00b7ten", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die dir verpflichtete, geneigte Zieglerin.", "tokens": ["Die", "dir", "ver\u00b7pflich\u00b7te\u00b7te", ",", "ge\u00b7neig\u00b7te", "Zieg\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}