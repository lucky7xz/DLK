{"dta.poem.12063": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Empfindung ist vom Ding ein Zeichen, von Empfindung", "tokens": ["Emp\u00b7fin\u00b7dung", "ist", "vom", "Ding", "ein", "Zei\u00b7chen", ",", "von", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zeichen war das Wort in erster Spracherfindung.", "tokens": ["Ein", "Zei\u00b7chen", "war", "das", "Wort", "in", "ers\u00b7ter", "Spra\u00b7cher\u00b7fin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun ist ein Zeichen vom Begriff das Wort allein,", "tokens": ["Nun", "ist", "ein", "Zei\u00b7chen", "vom", "Be\u00b7griff", "das", "Wort", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Empfindung f\u00fcgt sich nur nothd\u00fcrftig drein.", "tokens": ["Und", "die", "Emp\u00b7fin\u00b7dung", "f\u00fcgt", "sich", "nur", "noth\u00b7d\u00fcrf\u00b7tig", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Dinges Leben hat sich aus dem Wort verloren,", "tokens": ["Des", "Din\u00b7ges", "Le\u00b7ben", "hat", "sich", "aus", "dem", "Wort", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie die Empfindung zum Begriff sich umgeboren.", "tokens": ["Wie", "die", "Emp\u00b7fin\u00b7dung", "zum", "Be\u00b7griff", "sich", "um\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn er zu h\u00f6herer Empfindung sich erhebt,", "tokens": ["Wenn", "er", "zu", "h\u00f6\u00b7he\u00b7rer", "Emp\u00b7fin\u00b7dung", "sich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann ist mit dem Begriff wieder das Wort belebt.", "tokens": ["Dann", "ist", "mit", "dem", "Be\u00b7griff", "wie\u00b7der", "das", "Wort", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kein todtes Zeichen ist, kein Bild vom Ding das Wort,", "tokens": ["Kein", "tod\u00b7tes", "Zei\u00b7chen", "ist", ",", "kein", "Bild", "vom", "Ding", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "$,", "PIAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist im Geist das Ding, des Geistes Zauberhort.", "tokens": ["Es", "ist", "im", "Geist", "das", "Ding", ",", "des", "Geis\u00b7tes", "Zau\u00b7ber\u00b7hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Des Dinges Wesen selbst ist in das Wort gebannt;", "tokens": ["Des", "Din\u00b7ges", "We\u00b7sen", "selbst", "ist", "in", "das", "Wort", "ge\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschaffen ist das Ding, sowie das Wort genannt.", "tokens": ["Ge\u00b7schaf\u00b7fen", "ist", "das", "Ding", ",", "so\u00b7wie", "das", "Wort", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "La\u00dft uns, eh wir durchs Wort das Wesen schaffen k\u00f6nnen,", "tokens": ["La\u00dft", "uns", ",", "eh", "wir", "durchs", "Wort", "das", "We\u00b7sen", "schaf\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zaubrin Fantasie Scheinbildersch\u00f6pfung g\u00f6nnen!", "tokens": ["Der", "Zaub\u00b7rin", "Fan\u00b7ta\u00b7sie", "Schein\u00b7bil\u00b7der\u00b7sch\u00f6p\u00b7fung", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}