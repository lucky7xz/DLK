{"textgrid.poem.29585": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mein Sinn bi\u00dfher gezeugt, und die Schrifft an Tag hin legte,", "tokens": ["Was", "mein", "Sinn", "bi\u00df\u00b7her", "ge\u00b7zeugt", ",", "und", "die", "Schrifft", "an", "Tag", "hin", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "$,", "KON", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Liegt dahin, ob mans verwarff, oder ob es iemand pflegte.", "tokens": ["Liegt", "da\u00b7hin", ",", "ob", "mans", "ver\u00b7warff", ",", "o\u00b7der", "ob", "es", "ie\u00b7mand", "pfleg\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "KOUS", "PIS", "VVFIN", "$,", "KON", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Taug iemanden diese Zucht, kan sich noch Geschwister finden;", "tokens": ["Taug", "ie\u00b7man\u00b7den", "die\u00b7se", "Zucht", ",", "kan", "sich", "noch", "Ge\u00b7schwis\u00b7ter", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "NN", "$,", "VMFIN", "PRF", "ADV", "NN", "VVINF", "$."], "meter": "+---+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da\u00df sie sch\u00f6ner werden seyn, wil ich mich doch nicht verbinden.", "tokens": ["Da\u00df", "sie", "sch\u00f6\u00b7ner", "wer\u00b7den", "seyn", ",", "wil", "ich", "mich", "doch", "nicht", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "VAINF", "$,", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}