{"textgrid.poem.33765": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Leichenfeier", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig mit gesenkten Fl\u00fcgeln", "tokens": ["Trau\u00b7rig", "mit", "ge\u00b7senk\u00b7ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt der Legionen Aar", "tokens": ["Schwebt", "der", "Le\u00b7gi\u00b7o\u00b7nen", "Aar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Tiber dunklen H\u00fcgeln", "tokens": ["Von", "der", "Ti\u00b7ber", "dunk\u00b7len", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um den flammenden Altar.", "tokens": ["Um", "den", "flam\u00b7men\u00b7den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Sarg mit Lorbeerzweigen", "tokens": ["Auf", "dem", "Sarg", "mit", "Lor\u00b7beer\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt der Kaiser bleich und stumm.", "tokens": ["Liegt", "der", "Kai\u00b7ser", "bleich", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Krieger schau'n mit Schweigen", "tokens": ["Sei\u00b7ne", "Krie\u00b7ger", "schau'n", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf das Todesheiligtum.", "tokens": ["Auf", "das", "To\u00b7des\u00b7hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4hrend sie den Holzsto\u00df z\u00fcnden,", "tokens": ["W\u00e4h\u00b7rend", "sie", "den", "Holz\u00b7sto\u00df", "z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft ein wei\u00dfer Priesterchor,", "tokens": ["Ruft", "ein", "wei\u00b7\u00dfer", "Pries\u00b7ter\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "C\u00e4sars Ankunft zu verk\u00fcnden,", "tokens": ["C\u00e4\u00b7sars", "An\u00b7kunft", "zu", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Sternen dies empor:", "tokens": ["Zu", "den", "Ster\u00b7nen", "dies", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehmt ihn auf in eure Mitte,", "tokens": ["Nehmt", "ihn", "auf", "in", "eu\u00b7re", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen Siegesgott der Welt,", "tokens": ["Die\u00b7sen", "Sie\u00b7ges\u00b7gott", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der sie einst im Segensschritte", "tokens": ["Der", "sie", "einst", "im", "Se\u00b7gens\u00b7schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einer Sonne gleich erhellt!", "tokens": ["Ei\u00b7ner", "Son\u00b7ne", "gleich", "er\u00b7hellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Z\u00fcngelnd schlagen auf die Flammen", "tokens": ["Z\u00fcn\u00b7gelnd", "schla\u00b7gen", "auf", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Sarg, ein gl\u00fchend Meer,", "tokens": ["Um", "den", "Sarg", ",", "ein", "gl\u00fc\u00b7hend", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwert und Schilde schl\u00e4gt zusammen", "tokens": ["Schwert", "und", "Schil\u00b7de", "schl\u00e4gt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laut mit Klageruf das Heer.", "tokens": ["Laut", "mit", "Kla\u00b7ge\u00b7ruf", "das", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fahl wie Asche liegt der Tote,", "tokens": ["Fahl", "wie", "A\u00b7sche", "liegt", "der", "To\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Matt verglimmt der Scheiterhauf,", "tokens": ["Matt", "ver\u00b7glimmt", "der", "Schei\u00b7ter\u00b7hauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch der Aar, sein Fl\u00fcgelbote,", "tokens": ["Doch", "der", "Aar", ",", "sein", "Fl\u00fc\u00b7gel\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Schwingt sich zu den G\u00f6ttern auf.", "tokens": ["Schwingt", "sich", "zu", "den", "G\u00f6t\u00b7tern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}