{"dta.poem.13753": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "da:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nur der Degen blieb da droben", "tokens": ["Nur", "der", "De\u00b7gen", "blieb", "da", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam liegen \u00fcber'm Grab;", "tokens": ["Ein\u00b7sam", "lie\u00b7gen", "\u00fc\u00b7ber'm", "Grab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201esei denn Hab' und Gut zerstoben,", "tokens": ["\u201e", "sei", "denn", "Hab'", "und", "Gut", "zer\u00b7sto\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "KON", "NN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich dich, du Schwerdt, nur hab'!\u201c", "tokens": ["Wenn", "ich", "dich", ",", "du", "Schwerdt", ",", "nur", "hab'", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "NE", "NE", "$,", "ADV", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich fa\u00dft' es, \u2014 Leute w\u00fchlten", "tokens": ["Und", "ich", "fa\u00dft'", "es", ",", "Leu\u00b7te", "w\u00fchl\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "$(", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueber'n Berg, hinab, hinauf,", "tokens": ["Ue\u00b7ber'n", "Berg", ",", "hin\u00b7ab", ",", "hin\u00b7auf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob sie f\u00fcr verr\u00fcckt mich hielten \u2014", "tokens": ["Ob", "sie", "f\u00fcr", "ver\u00b7r\u00fcckt", "mich", "hiel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir ging hell die Sonne auf.", "tokens": ["Mir", "ging", "hell", "die", "Son\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}