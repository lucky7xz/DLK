{"textgrid.poem.34263": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Du weis' und gerecht stets in demselben Glanz", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Du weis' und gerecht stets in demselben Glanz", "tokens": ["Die", "Du", "weis'", "und", "ge\u00b7recht", "stets", "in", "dem\u00b7sel\u00b7ben", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADJD", "ADV", "APPR", "PDAT", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Herrschst, Anbethung verschm\u00e4hst, anbethungsw\u00fcrdig sein", "tokens": ["Herrschst", ",", "An\u00b7be\u00b7thung", "ver\u00b7schm\u00e4hst", ",", "an\u00b7be\u00b7thungs\u00b7w\u00fcr\u00b7dig", "sein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "VVFIN", "$,", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ohne Lorbeer voll Bluts, th\u00f6richte Helden lehrst,", "tokens": ["Oh\u00b7ne", "Lor\u00b7beer", "voll", "Bluts", ",", "th\u00f6\u00b7rich\u00b7te", "Hel\u00b7den", "lehrst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Hin, hinauf zu Dir fleugt mein Lied.", "tokens": ["Hin", ",", "hin\u00b7auf", "zu", "Dir", "fleugt", "mein", "Lied", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Furchtsam weihet es Dir dieses betr\u00e4nte Bild", "tokens": ["Furcht\u00b7sam", "wei\u00b7het", "es", "Dir", "die\u00b7ses", "be\u00b7tr\u00e4n\u00b7te", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch der Mitternacht Graun schl\u00e4ngelnder Blizze, Bluts,", "tokens": ["Durch", "der", "Mit\u00b7ter\u00b7nacht", "Graun", "schl\u00e4n\u00b7geln\u00b7der", "Bliz\u00b7ze", ",", "Bluts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das durch Blumen und Gras rinnt, wie die Quelle rinnt,", "tokens": ["Das", "durch", "Blu\u00b7men", "und", "Gras", "rinnt", ",", "wie", "die", "Quel\u00b7le", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und des Sterbtages der Natur.", "tokens": ["Und", "des", "Sterb\u00b7ta\u00b7ges", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Mit des Fr\u00fchlinges Pracht, mit seinem ersten Schmuk", "tokens": ["Mit", "des", "Fr\u00fch\u00b7lin\u00b7ges", "Pracht", ",", "mit", "sei\u00b7nem", "ers\u00b7ten", "Schmuk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Kr\u00e4nzt' ihr gl\u00fckliches Haupt, schm\u00fckte die stolze Brust", "tokens": ["Kr\u00e4nzt'", "ihr", "gl\u00fck\u00b7li\u00b7ches", "Haupt", ",", "schm\u00fck\u00b7te", "die", "stol\u00b7ze", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Meine Muse, wenn Dir, wenn auf di\u00df Bild, wenn Dir", "tokens": ["Mei\u00b7ne", "Mu\u00b7se", ",", "wenn", "Dir", ",", "wenn", "auf", "di\u00df", "Bild", ",", "wenn", "Dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "$,", "KOUS", "APPR", "PDS", "NN", "$,", "KOUS", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Eine g\u00f6ttliche Tr\u00e4n' entfiel.", "tokens": ["Ei\u00b7ne", "g\u00f6tt\u00b7li\u00b7che", "Tr\u00e4n'", "ent\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Denn Du hassest den Krieg, hassest den pr\u00e4chtgen Mord,", "tokens": ["Denn", "Du", "has\u00b7sest", "den", "Krieg", ",", "has\u00b7sest", "den", "pr\u00e4cht\u00b7gen", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Winkst dem Hunger zu fliehn, betest zu Gott f\u00fcrs Land", "tokens": ["Winkst", "dem", "Hun\u00b7ger", "zu", "fliehn", ",", "be\u00b7test", "zu", "Gott", "f\u00fcrs", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "NN", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und Dein Flehen verscheucht Abbadon, da\u00df das Schwerdt", "tokens": ["Und", "Dein", "Fle\u00b7hen", "ver\u00b7scheucht", "Ab\u00b7ba\u00b7don", ",", "da\u00df", "das", "Schwerdt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NE", "$,", "KOUS", "ART", "NN"], "meter": "--+--++-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus der bebenden Hand ihm sinkt.", "tokens": ["Aus", "der", "be\u00b7ben\u00b7den", "Hand", "ihm", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kann er hart genug einst, Dich uns zu rauben, sein?", "tokens": ["Kann", "er", "hart", "ge\u00b7nug", "einst", ",", "Dich", "uns", "zu", "rau\u00b7ben", ",", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "ADV", "$,", "PPER", "PPER", "PTKZU", "VVINF", "$,", "VAINF", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Solch ein L\u00e4cheln wie Deins sehen und t\u00f6dten? Traurt,", "tokens": ["Solch", "ein", "L\u00e4\u00b7cheln", "wie", "Deins", "se\u00b7hen", "und", "t\u00f6d\u00b7ten", "?", "Traurt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ART", "NN", "KOKOM", "NN", "VVINF", "KON", "VVINF", "$.", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bebt sein Innerstes nicht, wenn er ein Bild von Gott,", "tokens": ["Bebt", "sein", "In\u00b7ners\u00b7tes", "nicht", ",", "wenn", "er", "ein", "Bild", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Lebe, Mutter der Welt! siehe, der V\u00f6lker Wohl", "tokens": ["Le\u00b7be", ",", "Mut\u00b7ter", "der", "Welt", "!", "sie\u00b7he", ",", "der", "V\u00f6l\u00b7ker", "Wohl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "ART", "NN", "$.", "VVIMP", "$,", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Fleht, es fleht Ihr Gebet, still in die Nacht geschluchst:", "tokens": ["Fleht", ",", "es", "fleht", "Ihr", "Ge\u00b7bet", ",", "still", "in", "die", "Nacht", "ge\u00b7schluchst", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Lebe! die Du an Huld gleichest der Gottheit, sei", "tokens": ["Le\u00b7be", "!", "die", "Du", "an", "Huld", "glei\u00b7chest", "der", "Got\u00b7theit", ",", "sei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$.", "PRELS", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$,", "VAFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "An Unsterblichkeit auch ihr gleich.", "tokens": ["An", "U\u00b7nsterb\u00b7lich\u00b7keit", "auch", "ihr", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPER", "ADV", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Denn ich seh es im Geist, um Deine schwarze Gruft", "tokens": ["Denn", "ich", "seh", "es", "im", "Geist", ",", "um", "Dei\u00b7ne", "schwar\u00b7ze", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUI", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dr\u00e4ngt ein sprachloser Krei\u00df; Schluchsen und Seufzen trennt", "tokens": ["Dr\u00e4ngt", "ein", "sprach\u00b7lo\u00b7ser", "Krei\u00df", ";", "Schluch\u00b7sen", "und", "Seuf\u00b7zen", "trennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "NN", "KON", "NN", "VVFIN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Die nachhallende Luft, Schluchsen und Heulen t\u00f6nt", "tokens": ["Die", "nach\u00b7hal\u00b7len\u00b7de", "Luft", ",", "Schluch\u00b7sen", "und", "Heu\u00b7len", "t\u00f6nt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Von dem Belt bis zum schwarzen Meer.", "tokens": ["Von", "dem", "Belt", "bis", "zum", "schwar\u00b7zen", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Trostlo\u00df raufet der Greis das ihm gebliebne Haar,", "tokens": ["Trost\u00b7lo\u00df", "rau\u00b7fet", "der", "Greis", "das", "ihm", "ge\u00b7blieb\u00b7ne", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirft sein heiliges Haar ausgerauft auf Dein Grab:", "tokens": ["Wirft", "sein", "hei\u00b7li\u00b7ges", "Haar", "aus\u00b7ger\u00b7auft", "auf", "Dein", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dreimal k\u00fc\u00dft er den Staub der Deine Leiche dekt,", "tokens": ["Drei\u00b7mal", "k\u00fc\u00dft", "er", "den", "Staub", "der", "Dei\u00b7ne", "Lei\u00b7che", "dekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Dreimal weinet er laut und ruft:", "tokens": ["Drei\u00b7mal", "wei\u00b7net", "er", "laut", "und", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "\u00bbwarum zeugtest du mich, du, der du mich gezeugt?", "tokens": ["\u00bb", "wa\u00b7rum", "zeug\u00b7test", "du", "mich", ",", "du", ",", "der", "du", "mich", "ge\u00b7zeugt", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PRF", "$,", "PPER", "$,", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Warum zeugete ich, du, den ich zeugte, dich?", "tokens": ["Wa\u00b7rum", "zeu\u00b7ge\u00b7te", "ich", ",", "du", ",", "den", "ich", "zeug\u00b7te", ",", "dich", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df mein Auge soll sehn, Sohn, da\u00df dein Auge soll", "tokens": ["Da\u00df", "mein", "Au\u00b7ge", "soll", "sehn", ",", "Sohn", ",", "da\u00df", "dein", "Au\u00b7ge", "soll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,", "NN", "$,", "KOUS", "PPOSAT", "NN", "VMFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}}}}