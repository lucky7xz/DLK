{"textgrid.poem.61608": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gisella", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht sch\u00f6n noch rein von Fehle;", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "noch", "rein", "von", "Feh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ob die Welt auch hart gesinnt", "tokens": ["Doch", "ob", "die", "Welt", "auch", "hart", "ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich unter die Verlornen z\u00e4hle:", "tokens": ["Dich", "un\u00b7ter", "die", "Ver\u00b7lor\u00b7nen", "z\u00e4h\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du r\u00fchrst das Herz mir, armes Kind.", "tokens": ["Du", "r\u00fchrst", "das", "Herz", "mir", ",", "ar\u00b7mes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn mitten unter Schein und L\u00fcge,", "tokens": ["Denn", "mit\u00b7ten", "un\u00b7ter", "Schein", "und", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdeckt von leichter Schlacke nur,", "tokens": ["Ver\u00b7deckt", "von", "leich\u00b7ter", "Schla\u00b7cke", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erkenn' ich noch an dir die Z\u00fcge", "tokens": ["Er\u00b7kenn'", "ich", "noch", "an", "dir", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der gottbegnadeten Natur.", "tokens": ["Der", "gott\u00b7be\u00b7gna\u00b7de\u00b7ten", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schien je ein hold Gef\u00e4\u00df erkoren", "tokens": ["Schien", "je", "ein", "hold", "Ge\u00b7f\u00e4\u00df", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Dienst der Kunst, die du erw\u00e4hlt,", "tokens": ["Zum", "Dienst", "der", "Kunst", ",", "die", "du", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So warest du's, der angeboren,", "tokens": ["So", "wa\u00b7rest", "du's", ",", "der", "an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was hundert Vielbekr\u00e4nzten fehlt:", "tokens": ["Was", "hun\u00b7dert", "Viel\u00b7be\u00b7kr\u00e4nz\u00b7ten", "fehlt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der stumme Zauber der Geb\u00e4rde,", "tokens": ["Der", "stum\u00b7me", "Zau\u00b7ber", "der", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stimme, welche Tr\u00e4nen spricht,", "tokens": ["Die", "Stim\u00b7me", ",", "wel\u00b7che", "Tr\u00e4\u00b7nen", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr alles Gl\u00fcck und Weh der Erde", "tokens": ["F\u00fcr", "al\u00b7les", "Gl\u00fcck", "und", "Weh", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schrei, der aus dem Herzen bricht.", "tokens": ["Der", "Schrei", ",", "der", "aus", "dem", "Her\u00b7zen", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch weit ist's von der Kraft zum Siege,", "tokens": ["Doch", "weit", "ist's", "von", "der", "Kraft", "zum", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihr das Gl\u00fcck die Hand nicht bot;", "tokens": ["Wenn", "ihr", "das", "Gl\u00fcck", "die", "Hand", "nicht", "bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl stand die Mus' an deiner Wiege,", "tokens": ["Wohl", "stand", "die", "Mus'", "an", "dei\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch bei der Muse stand die Not.", "tokens": ["Doch", "bei", "der", "Mu\u00b7se", "stand", "die", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und was als lichtes Angebinde", "tokens": ["Und", "was", "als", "lich\u00b7tes", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die andre h\u00fcllt' es, ach, dem Kinde", "tokens": ["Die", "and\u00b7re", "h\u00fcllt'", "es", ",", "ach", ",", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$,", "ITJ", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Dust und Spinnweb trostlos ein.", "tokens": ["In", "Dust", "und", "Spinn\u00b7web", "trost\u00b7los", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kein Sternbild sahst du leitend funkeln,", "tokens": ["Kein", "Stern\u00b7bild", "sahst", "du", "lei\u00b7tend", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein F\u00fchrer ging dir treu voran;", "tokens": ["Kein", "F\u00fch\u00b7rer", "ging", "dir", "treu", "vo\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du tastetest allein im Dunkeln", "tokens": ["Du", "tas\u00b7te\u00b7test", "al\u00b7lein", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich fort auf ungewisser Bahn.", "tokens": ["Dich", "fort", "auf", "un\u00b7ge\u00b7wis\u00b7ser", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Jung, ratlos, ohne Schutz und Pflege,", "tokens": ["Jung", ",", "rat\u00b7los", ",", "oh\u00b7ne", "Schutz", "und", "Pfle\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom eignen hei\u00dfen Blut verwirrt \u2013", "tokens": ["Vom", "eig\u00b7nen", "hei\u00b7\u00dfen", "Blut", "ver\u00b7wirrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hebt den Stein auf, wenn vom Wege", "tokens": ["Wer", "hebt", "den", "Stein", "auf", ",", "wenn", "vom", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ahnungslos dein Fu\u00df verirrt!", "tokens": ["Sich", "ah\u00b7nungs\u00b7los", "dein", "Fu\u00df", "ver\u00b7irrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Emporgebl\u00fcht auf sonn'gen Matten,", "tokens": ["Em\u00b7por\u00b7ge\u00b7bl\u00fcht", "auf", "sonn'\u00b7gen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4rst du vielleicht der Stolz der Flur,", "tokens": ["W\u00e4rst", "du", "viel\u00b7leicht", "der", "Stolz", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun brachtest du's, verbla\u00dft im Schatten,", "tokens": ["Nun", "brach\u00b7test", "du's", ",", "ver\u00b7bla\u00dft", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einer kargen Bl\u00fcte nur.", "tokens": ["Zu", "ei\u00b7ner", "kar\u00b7gen", "Bl\u00fc\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Perle, die, vom Staub gereinigt,", "tokens": ["Die", "Per\u00b7le", ",", "die", ",", "vom", "Staub", "ge\u00b7rei\u00b7nigt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr keine Krone zu gering,", "tokens": ["F\u00fcr", "kei\u00b7ne", "Kro\u00b7ne", "zu", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verloren unter Sand und Steinicht", "tokens": ["Ver\u00b7lo\u00b7ren", "un\u00b7ter", "Sand", "und", "Stei\u00b7nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zertrat sie, wer vor\u00fcberging.", "tokens": ["Zer\u00b7trat", "sie", ",", "wer", "vor\u00b7\u00fc\u00b7ber\u00b7ging", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und doch, um die zersprungnen St\u00fccke", "tokens": ["Und", "doch", ",", "um", "die", "zer\u00b7sprung\u00b7nen", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spielt noch ein Glanz so echter Art,", "tokens": ["Spielt", "noch", "ein", "Glanz", "so", "ech\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich die Tr\u00e4ne nicht zerdr\u00fccke", "tokens": ["Da\u00df", "ich", "die", "Tr\u00e4\u00b7ne", "nicht", "zer\u00b7dr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um das, was hier verloren ward.", "tokens": ["Um", "das", ",", "was", "hier", "ver\u00b7lo\u00b7ren", "ward", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ach, fast bed\u00fcnkt's mich jetzt ein Segen,", "tokens": ["Ach", ",", "fast", "be\u00b7d\u00fcnkt's", "mich", "jetzt", "ein", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du dahingehst leichtgeherzt,", "tokens": ["Da\u00df", "du", "da\u00b7hin\u00b7gehst", "leicht\u00b7ge\u00b7herzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum ahnend, ", "tokens": ["Kaum", "ah\u00b7nend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und welche Zukunft du verscherzt.", "tokens": ["Und", "wel\u00b7che", "Zu\u00b7kunft", "du", "ver\u00b7scherzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}