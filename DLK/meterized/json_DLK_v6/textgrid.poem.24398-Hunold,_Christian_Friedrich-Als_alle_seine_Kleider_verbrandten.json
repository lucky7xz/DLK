{"textgrid.poem.24398": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als alle seine Kleider verbrandten", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun steh ich nackend da. Mein Elend ist sehr gro\u00df.", "tokens": ["Nun", "steh", "ich", "na\u00b7ckend", "da", ".", "Mein", "E\u00b7lend", "ist", "sehr", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r ich in Eden doch/ so gieng ich gerne blo\u00df.", "tokens": ["W\u00e4r", "ich", "in", "E\u00b7den", "doch", "/", "so", "gieng", "ich", "ger\u00b7ne", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch mu\u00df ich/ wie ein Mensch/ mein Gott/ auf Erden leben/", "tokens": ["Doch", "mu\u00df", "ich", "/", "wie", "ein", "Mensch", "/", "mein", "Gott", "/", "auf", "Er\u00b7den", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$(", "KOKOM", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wirst du mir auch ein Fell/ wie Adam wieder geben.", "tokens": ["Wirst", "du", "mir", "auch", "ein", "Fell", "/", "wie", "A\u00b7dam", "wie\u00b7der", "ge\u00b7ben",