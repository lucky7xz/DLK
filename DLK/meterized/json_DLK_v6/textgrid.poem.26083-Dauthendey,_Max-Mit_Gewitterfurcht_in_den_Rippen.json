{"textgrid.poem.26083": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mit Gewitterfurcht in den Rippen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Berg ist vom Gewitter umlauert, der Flu\u00df steht fest wie angemauert.", "tokens": ["Der", "Berg", "ist", "vom", "Ge\u00b7wit\u00b7ter", "um\u00b7lau\u00b7ert", ",", "der", "Flu\u00df", "steht", "fest", "wie", "an\u00b7ge\u00b7mau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "VVPP", "$."], "meter": "-+-+-+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Flu\u00df und Abend und Berg erwarten den flatternden Ritter,", "tokens": ["Flu\u00df", "und", "A\u00b7bend", "und", "Berg", "er\u00b7war\u00b7ten", "den", "flat\u00b7tern\u00b7den", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Den ersten Fr\u00fchlingsblitz, von dem der Efeu im Garten", "tokens": ["Den", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7blitz", ",", "von", "dem", "der", "E\u00b7feu", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Vorgef\u00fchl schon rauschend erschauert.", "tokens": ["Im", "Vor\u00b7ge\u00b7f\u00fchl", "schon", "rau\u00b7schend", "er\u00b7schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf der Pappelinsel im Flu\u00df fl\u00fcchten die Amseln scheu,", "tokens": ["Auf", "der", "Pap\u00b7pe\u00b7lin\u00b7sel", "im", "Flu\u00df", "fl\u00fcch\u00b7ten", "die", "Am\u00b7seln", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--++--+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Als ob bald die Insel im Blitzstrahl versinken mu\u00df,", "tokens": ["Als", "ob", "bald", "die", "In\u00b7sel", "im", "Blitz\u00b7strahl", "ver\u00b7sin\u00b7ken", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.7": {"text": "F\u00e4llt der wie ein Schu\u00df aus den Bergen ins Tal.", "tokens": ["F\u00e4llt", "der", "wie", "ein", "Schu\u00df", "aus", "den", "Ber\u00b7gen", "ins", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann, mit Gewitterfurcht in den Rippen,", "tokens": ["Dann", ",", "mit", "Ge\u00b7wit\u00b7ter\u00b7furcht", "in", "den", "Rip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "H\u00e4lt manche die Lippen hin zum ersten Ku\u00df.", "tokens": ["H\u00e4lt", "man\u00b7che", "die", "Lip\u00b7pen", "hin", "zum", "ers\u00b7ten", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}