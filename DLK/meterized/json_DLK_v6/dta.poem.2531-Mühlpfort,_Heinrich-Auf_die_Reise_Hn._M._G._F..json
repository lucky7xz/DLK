{"dta.poem.2531": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Reise Hn. M. G. F.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So f\u00e4llt ein himmlisch Feu'r/ mein Freund/ auf seine Zunge/", "tokens": ["So", "f\u00e4llt", "ein", "himm\u00b7lisch", "Feu'r", "/", "mein", "Freund", "/", "auf", "sei\u00b7ne", "Zun\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich da der lichte Blitz um die Apostel schwebt/", "tokens": ["Gleich", "da", "der", "lich\u00b7te", "Blitz", "um", "die", "A\u00b7pos\u00b7tel", "schwebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ein zertheilter Strahl in ihre Seele drunge/", "tokens": ["Als", "ein", "zer\u00b7theil\u00b7ter", "Strahl", "in", "ih\u00b7re", "See\u00b7le", "drun\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Krafft von oben ab sie hat auffs neu belebt.", "tokens": ["Und", "Krafft", "von", "o\u00b7ben", "ab", "sie", "hat", "auffs", "neu", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADV", "APPR", "PPER", "VAFIN", "APPRART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wil der Tr\u00f6ster Jhn in seinen Weinberg senden/", "tokens": ["So", "wil", "der", "Tr\u00f6s\u00b7ter", "Jhn", "in", "sei\u00b7nen", "Wein\u00b7berg", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auff eben diesen Tag/ da er gesendet ist;", "tokens": ["Auff", "e\u00b7ben", "die\u00b7sen", "Tag", "/", "da", "er", "ge\u00b7sen\u00b7det", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Lehrer/ der gelehrt an jeden Ort und Enden/", "tokens": ["Der", "Leh\u00b7rer", "/", "der", "ge\u00b7lehrt", "an", "je\u00b7den", "Ort", "und", "En\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVPP", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den h\u00f6rt man wie er auch durch seine Lippen list.", "tokens": ["Den", "h\u00f6rt", "man", "wie", "er", "auch", "durch", "sei\u00b7ne", "Lip\u00b7pen", "list", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "KOKOM", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ruff in der Cantzeley de\u00df Himmels unterschrieben!", "tokens": ["Ruff", "in", "der", "Cant\u00b7ze\u00b7ley", "de\u00df", "Him\u00b7mels", "un\u00b7ter\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.10": {"text": "Und Schlu\u00df den eintzig nur macht die Dreyfaltigkeit!", "tokens": ["Und", "Schlu\u00df", "den", "eint\u00b7zig", "nur", "macht", "die", "Drey\u00b7fal\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Verwaltung/ die blo\u00df heist der Seelen Wohlfart lieben!", "tokens": ["Ver\u00b7wal\u00b7tung", "/", "die", "blo\u00df", "heist", "der", "See\u00b7len", "Wohl\u00b7fart", "lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADV", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Arbeit welche zu de\u00df Lebens Heil bereit!", "tokens": ["Und", "Ar\u00b7beit", "wel\u00b7che", "zu", "de\u00df", "Le\u00b7bens", "Heil", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRELS", "APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verwunderns voller Tag! dreymahl-begl\u00fcckte Stunden!", "tokens": ["Ver\u00b7wun\u00b7derns", "vol\u00b7ler", "Tag", "!", "drey\u00b7mahl\u00b7be\u00b7gl\u00fcck\u00b7te", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Licht/ da\u00df den Anfang macht zu einem grossen Werck!", "tokens": ["Licht", "/", "da\u00df", "den", "An\u00b7fang", "macht", "zu", "ei\u00b7nem", "gros\u00b7sen", "Werck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Und Au\u00dfgang/ den gewi\u00df kein Menschen-Witz erfunden;", "tokens": ["Und", "Au\u00df\u00b7gang", "/", "den", "ge\u00b7wi\u00df", "kein", "Men\u00b7schen\u00b7Witz", "er\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "ART", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nein/ der alleine k\u00f6mt von Zions heilgem Berg.", "tokens": ["Nein", "/", "der", "al\u00b7lei\u00b7ne", "k\u00f6mt", "von", "Zi\u00b7ons", "heil\u00b7gem", "Berg", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ART", "ADV", "VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Mensch ist ohne GOtt mit seinem Dencken nichtig/", "tokens": ["Ein", "Mensch", "ist", "oh\u00b7ne", "Gott", "mit", "sei\u00b7nem", "Den\u00b7cken", "nich\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und einer Harffen gleich/ die falsch und ungestimmt:", "tokens": ["Und", "ei\u00b7ner", "Harf\u00b7fen", "gleich", "/", "die", "falsch", "und", "un\u00b7ge\u00b7stimmt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$(", "ART", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Wei\u00dfheit dieser Welt als wie ein Schatten fl\u00fcchtig/", "tokens": ["Die", "Wei\u00df\u00b7heit", "die\u00b7ser", "Welt", "als", "wie", "ein", "Schat\u00b7ten", "fl\u00fcch\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "KOUS", "KOKOM", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dem Regenbogen gleich/ der bald sein Ende nimmt.", "tokens": ["Dem", "Re\u00b7gen\u00b7bo\u00b7gen", "gleich", "/", "der", "bald", "sein", "En\u00b7de", "nimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hingegen wenn der Geist de\u00df Herren in uns f\u00e4hret/", "tokens": ["Hin\u00b7ge\u00b7gen", "wenn", "der", "Geist", "de\u00df", "Her\u00b7ren", "in", "uns", "f\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und sein lebendig Wort in Hertz und Adern wacht/", "tokens": ["Und", "sein", "le\u00b7ben\u00b7dig", "Wort", "in", "Hertz", "und", "A\u00b7dern", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wird uns neue Krafft und neue Macht gewehret/", "tokens": ["So", "wird", "uns", "neu\u00b7e", "Krafft", "und", "neu\u00b7e", "Macht", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die unsre Bl\u00f6digkeit hier noch zu Engeln macht.", "tokens": ["Die", "uns\u00b7re", "Bl\u00f6\u00b7dig\u00b7keit", "hier", "noch", "zu", "En\u00b7geln", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer h\u00e4tt\u2019 es au\u00dfgedacht/ wer h\u00e4tte di\u00df geschlossen/", "tokens": ["Wer", "h\u00e4tt'", "es", "au\u00df\u00b7ge\u00b7dacht", "/", "wer", "h\u00e4t\u00b7te", "di\u00df", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$(", "PWS", "VAFIN", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df er/ ", "tokens": ["Da\u00df", "er", "/"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Da\u00df er die Mutter-Brust/ die er bi\u00dfher genossen/", "tokens": ["Da\u00df", "er", "die", "Mut\u00b7ter\u00b7Brust", "/", "die", "er", "bi\u00df\u00b7her", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Und was sonst Bre\u00dflau hegt setzt au\u00df der Augen Schein?", "tokens": ["Und", "was", "sonst", "Bre\u00df\u00b7lau", "hegt", "setzt", "au\u00df", "der", "Au\u00b7gen", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch GOttes Stimme rufft ihn zu den nahen Polen/", "tokens": ["Doch", "Got\u00b7tes", "Stim\u00b7me", "rufft", "ihn", "zu", "den", "na\u00b7hen", "Po\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Er fol zu Schlitigheim bedienen das Altar/", "tokens": ["Er", "fol", "zu", "Schli\u00b7tig\u00b7heim", "be\u00b7die\u00b7nen", "das", "Al\u00b7tar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Hertzen anzugl\u00fchn/ Feu'r au\u00df dem Himmel holen.", "tokens": ["Die", "Hert\u00b7zen", "an\u00b7zu\u00b7gl\u00fchn", "/", "Feu'r", "au\u00df", "dem", "Him\u00b7mel", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$(", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und treulich Tag und Nacht der Heerde nehmen war.", "tokens": ["Und", "treu\u00b7lich", "Tag", "und", "Nacht", "der", "Heer\u00b7de", "neh\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "KON", "NN", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wohlan/ er geh getrost und g\u00fcrte seine Lenden/", "tokens": ["Wo\u00b7hlan", "/", "er", "geh", "ge\u00b7trost", "und", "g\u00fcr\u00b7te", "sei\u00b7ne", "Len\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sein Beystand h\u00fcllet ihn in Stern und Flammen ein/", "tokens": ["Sein", "Beys\u00b7tand", "h\u00fcl\u00b7let", "ihn", "in", "Stern", "und", "Flam\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und wird dem Hertzen Krafft/ der Zunge Wortesenden/", "tokens": ["Und", "wird", "dem", "Hert\u00b7zen", "Krafft", "/", "der", "Zun\u00b7ge", "Wor\u00b7te\u00b7sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df Frucht und Nutzen folgt im Wandel der Gemein.", "tokens": ["Da\u00df", "Frucht", "und", "Nut\u00b7zen", "folgt", "im", "Wan\u00b7del", "der", "Ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er sey Basilius/ wenn ihn Gefahr bedreuet/", "tokens": ["Er", "sey", "Ba\u00b7si\u00b7lius", "/", "wenn", "ihn", "Ge\u00b7fahr", "be\u00b7dreu\u00b7et", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$(", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Und ein Ambrosius/ wenn es zum tr\u00f6sten k\u00f6mmt/", "tokens": ["Und", "ein", "A\u00b7mbro\u00b7si\u00b7us", "/", "wenn", "es", "zum", "tr\u00f6s\u00b7ten", "k\u00f6mmt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "$(", "KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Ein hurtig Cyprian/ der auch den Tod nicht scheuet/", "tokens": ["Ein", "hur\u00b7tig", "Cyp\u00b7ri\u00b7an", "/", "der", "auch", "den", "Tod", "nicht", "scheu\u00b7et", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "$(", "ART", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Bernhard dem sein Hertz in Jesus Liebe glimmt.", "tokens": ["Ein", "Bern\u00b7hard", "dem", "sein", "Hertz", "in", "Je\u00b7sus", "Lie\u00b7be", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "PPOSAT", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er weise seinem Volck/ das ihm ist untergeben/", "tokens": ["Er", "wei\u00b7se", "sei\u00b7nem", "Volck", "/", "das", "ihm", "ist", "un\u00b7ter\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PRELS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und dessen Seelen sind vertrauet seiner Hand/", "tokens": ["Und", "des\u00b7sen", "See\u00b7len", "sind", "ver\u00b7trau\u00b7et", "sei\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "VAFIN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie unser Heyland sey/ Weg/ Wahrheit und das Leben/", "tokens": ["Wie", "un\u00b7ser", "Hey\u00b7land", "sey", "/", "Weg", "/", "Wahr\u00b7heit", "und", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$(", "NN", "$(", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da\u00df der nicht irren kan/ der dieses hat erkant.", "tokens": ["Da\u00df", "der", "nicht", "ir\u00b7ren", "kan", "/", "der", "die\u00b7ses", "hat", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PTKNEG", "VVINF", "VMFIN", "$(", "ART", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Er sey gletch einem Thau/ der f\u00fcr der Sonnen-Hitze/", "tokens": ["Er", "sey", "gletch", "ei\u00b7nem", "Thau", "/", "der", "f\u00fcr", "der", "Son\u00b7nen\u00b7Hit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die welcken Kr\u00e4uter st\u00e4rckt mit seinem Perlen-Safft.", "tokens": ["Die", "wel\u00b7