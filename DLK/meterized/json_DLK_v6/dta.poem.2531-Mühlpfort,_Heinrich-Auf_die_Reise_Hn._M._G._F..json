ter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "So halt er fleissig an mit Beten/ Tr\u00f6sten/ Lehren/", "tokens": ["So", "halt", "er", "fleis\u00b7sig", "an", "mit", "Be\u00b7ten", "/", "Tr\u00f6s\u00b7ten", "/", "Leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und f\u00f6rdre/ was er kan/ der Andacht ihren Lauf.", "tokens": ["Und", "f\u00f6rd\u00b7re", "/", "was", "er", "kan", "/", "der", "An\u00b7dacht", "ih\u00b7ren", "Lauf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Er sey ein solcher Artzt/ der b\u00f6se Sch\u00e4den brennen", "tokens": ["Er", "sey", "ein", "sol\u00b7cher", "Artzt", "/", "der", "b\u00f6\u00b7se", "Sch\u00e4\u00b7den", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und Unbu\u00dffertige mit Essig beitzen mu\u00df.", "tokens": ["Und", "Un\u00b7bu\u00df\u00b7fer\u00b7ti\u00b7ge", "mit", "Es\u00b7sig", "beit\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Hingegen wo er auch kan Reu und Leid erkennen/", "tokens": ["Hin\u00b7ge\u00b7gen", "wo", "er", "auch", "kan", "Reu