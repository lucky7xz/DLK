{"dta.poem.9203": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Als Coridon dem Florindo an das Knie k\u00fctzelte/ in  \n Meynung als h\u00e4tte er die Marilis darbey.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein knie thut mir abscheulich weh/", "tokens": ["Mein", "knie", "thut", "mir", "ab\u00b7scheu\u00b7lich", "weh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es juckt mich wo ich geh und steh/", "tokens": ["Es", "juckt", "mich", "wo", "ich", "geh", "und", "steh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df nicht/ was ich dencke:", "tokens": ["Ich", "wei\u00df", "nicht", "/", "was", "ich", "den\u00b7cke", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es krappelt stets und wo ich bin/", "tokens": ["Es", "krap\u00b7pelt", "stets", "und", "wo", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da f\u00e4hret mirs bald oben hin/", "tokens": ["Da", "f\u00e4h\u00b7ret", "mirs", "bald", "o\u00b7ben", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald sitzt mirs im gelencke.", "tokens": ["Bald", "sitzt", "mirs", "im", "ge\u00b7len\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Denn eine junggesellen hand/", "tokens": ["Denn", "ei\u00b7ne", "jung\u00b7ge\u00b7sel\u00b7len", "hand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hat mich gestern so verbrandt", "tokens": ["Die", "hat", "mich", "ge\u00b7stern", "so", "ver\u00b7brandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit ihrem complimenten/", "tokens": ["Mit", "ih\u00b7rem", "com\u00b7pli\u00b7men\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie griff mir dran/ und macht es doch", "tokens": ["Sie", "griff", "mir", "dran", "/", "und", "macht", "es", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$(", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wundersch\u00f6n/ als wann wir noch", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", "/", "als", "wann", "wir", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "KON", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wohl einander kennten.", "tokens": ["So", "wohl", "ein\u00b7an\u00b7der", "kenn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Es ist f\u00fcrwar ein artger schwanck/", "tokens": ["Es", "ist", "f\u00fcr\u00b7war", "ein", "art\u00b7ger", "schwanck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein knie das must ohns henckers-danck", "tokens": ["Mein", "knie", "das", "must", "ohns", "hen\u00b7cker\u00b7sdanck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein jungfer-knie bedeuten/", "tokens": ["Ein", "jung\u00b7fer\u00b7knie", "be\u00b7deu\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da kriegte mich der narr dabey/", "tokens": ["Da", "krieg\u00b7te", "mich", "der", "narr", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und machte seine l\u00f6ffeley", "tokens": ["Und", "mach\u00b7te", "sei\u00b7ne", "l\u00f6f\u00b7fe\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gantz heimlich vor den leuten.", "tokens": ["Gantz", "heim\u00b7lich", "vor", "den", "leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Die worte waren schrecklich krum/", "tokens": ["Die", "wor\u00b7te", "wa\u00b7ren", "schreck\u00b7lich", "krum", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie flogen in der stuben rum", "tokens": ["Sie", "flo\u00b7gen", "in", "der", "stu\u00b7ben", "rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lauter ungeziefer/", "tokens": ["Wie", "lau\u00b7ter", "un\u00b7ge\u00b7zie\u00b7fer", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Monsieur der hat ein hau\u00df gebaut", "tokens": ["Mon\u00b7si\u00b7eur", "der", "hat", "ein", "hau\u00df", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Von butter-milch und sauer kraut/", "tokens": ["Von", "but\u00b7ter\u00b7milch", "und", "sau\u00b7er", "kraut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und deckt es nun mit schiefer.", "tokens": ["Und", "deckt", "es", "nun", "mit", "schie\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Er fand bey mir zwar guten platz/", "tokens": ["Er", "fand", "bey", "mir", "zwar", "gu\u00b7ten", "platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dacht ich ja/ mein lieber schatz/", "tokens": ["Doch", "dacht", "ich", "ja", "/", "mein", "lie\u00b7ber", "schatz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Je seyd ihr nicht ein flegel?", "tokens": ["Je", "seyd", "ihr", "nicht", "ein", "fle\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcrwahr/ wo ihr euch nicht beqvemt/", "tokens": ["F\u00fcr\u00b7wahr", "/", "wo", "ihr", "euch", "nicht", "be\u00b7qvemt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00f6fflich euren abschied nehmt/", "tokens": ["Und", "h\u00f6ff\u00b7lich", "eu\u00b7ren", "ab\u00b7schied", "nehmt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So hust ihr auf den schlegel:", "tokens": ["So", "hust", "ihr", "auf", "den", "schle\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Er sah mir die gedancken an/", "tokens": ["Er", "sah", "mir", "die", "ge\u00b7dan\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum gieng er als ein nasser hahn/", "tokens": ["Drum", "gieng", "er", "als", "ein", "nas\u00b7ser", "hahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kratzte mit den f\u00fcssen/", "tokens": ["Und", "kratz\u00b7te", "mit", "den", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich hab hingegen grosse m\u00fch/", "tokens": ["Ich", "hab", "hin\u00b7ge\u00b7gen", "gros\u00b7se", "m\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und werde noch zu meinem knie", "tokens": ["Und", "wer\u00b7de", "noch", "zu", "mei\u00b7nem", "knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den zahn- artzt brauchen m\u00fcssen.", "tokens": ["Den", "zahn", "artzt", "brau\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}