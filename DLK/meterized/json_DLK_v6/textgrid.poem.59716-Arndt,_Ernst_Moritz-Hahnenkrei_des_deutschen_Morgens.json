{"textgrid.poem.59716": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Hahnenkrei des deutschen Morgens", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat mir ein goldkammiger Hahn gekr\u00e4ht,", "tokens": ["Hat", "mir", "ein", "gold\u00b7kam\u00b7mi\u00b7ger", "Hahn", "ge\u00b7kr\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der der Zeiten und V\u00f6lker Geheimnis singt \u2013", "tokens": ["Der", "der", "Zei\u00b7ten", "und", "V\u00f6l\u00b7ker", "Ge\u00b7heim\u00b7nis", "singt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "NN", "NN", "VVFIN", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ihr wi\u00dft, es wird nimmer zu Wind verweht,", "tokens": ["Ihr", "wi\u00dft", ",", "es", "wird", "nim\u00b7mer", "zu", "Wind", "ver\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was der kluge Schnabel der Weisheit klingt \u2013", "tokens": ["Was", "der", "klu\u00b7ge", "Schna\u00b7bel", "der", "Weis\u00b7heit", "klingt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er sang aus verborgener Zukunft Wolke", "tokens": ["Er", "sang", "aus", "ver\u00b7bor\u00b7ge\u00b7ner", "Zu\u00b7kunft", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mir Wunderrunen vom deutschen Volke.", "tokens": ["Mir", "Wun\u00b7der\u00b7ru\u00b7nen", "vom", "deut\u00b7schen", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er kr\u00e4hte \u2013 sein goldiger Kamm ward bleich \u2013", "tokens": ["Er", "kr\u00e4h\u00b7te", "\u2013", "sein", "gol\u00b7di\u00b7ger", "Kamm", "ward", "bleich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mir der deutschen Treue geschwundene Kraft,", "tokens": ["Mir", "der", "deut\u00b7schen", "Treu\u00b7e", "ge\u00b7schwun\u00b7de\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Leichenges\u00e4nge vom heiligen Reich,", "tokens": ["Die", "Lei\u00b7chen\u00b7ge\u00b7s\u00e4n\u00b7ge", "vom", "hei\u00b7li\u00b7gen", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von verrosteten Degen der Ritterschaft,", "tokens": ["Von", "ver\u00b7ros\u00b7te\u00b7ten", "De\u00b7gen", "der", "Rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Von gebrochenen T\u00fcrmen, geschleiften Wehren", "tokens": ["Von", "ge\u00b7bro\u00b7che\u00b7nen", "T\u00fcr\u00b7men", ",", "ge\u00b7schleif\u00b7ten", "Weh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und des Kaiserpurpurs zerrissenen Ehren.", "tokens": ["Und", "des", "Kai\u00b7ser\u00b7pur\u00b7purs", "zer\u00b7ris\u00b7se\u00b7nen", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So kr\u00e4ht' er mir traurig vom d\u00fcrren Ast", "tokens": ["So", "kr\u00e4ht'", "er", "mir", "trau\u00b7rig", "vom", "d\u00fcr\u00b7ren", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Schandejahrhunderte Weh und Ach,", "tokens": ["Der", "Schan\u00b7de\u00b7jah\u00b7rhun\u00b7der\u00b7te", "Weh", "und", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er kr\u00e4hte, da\u00df unter der T\u00f6ne Last", "tokens": ["Er", "kr\u00e4h\u00b7te", ",", "da\u00df", "un\u00b7ter", "der", "T\u00f6\u00b7ne", "Last"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vom eisigen Jammer das Herz mir brach,", "tokens": ["Vom", "ei\u00b7si\u00b7gen", "Jam\u00b7mer", "das", "Herz", "mir", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da\u00df mir mit mordlich scharfen Harpunen", "tokens": ["Da\u00df", "mir", "mit", "mord\u00b7lich", "schar\u00b7fen", "Har\u00b7pu\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Brust durchschossen des Sanges Runen.", "tokens": ["Die", "Brust", "durch\u00b7schos\u00b7sen", "des", "San\u00b7ges", "Ru\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch sieh! Bald fliegt er auf gr\u00fcnen Baum,", "tokens": ["Doch", "sieh", "!", "Bald", "fliegt", "er", "auf", "gr\u00fc\u00b7nen", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bald kr\u00e4ht er von bl\u00fchendem Zweig sein Lied,", "tokens": ["Bald", "kr\u00e4ht", "er", "von", "bl\u00fc\u00b7hen\u00b7dem", "Zweig", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das hell, ein leuchtender Zeitentraum,", "tokens": ["Das", "hell", ",", "ein", "leuch\u00b7ten\u00b7der", "Zei\u00b7ten\u00b7traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Zukunft sonnige Bahnen zieht:", "tokens": ["Der", "Zu\u00b7kunft", "son\u00b7ni\u00b7ge", "Bah\u00b7nen", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er kr\u00e4ht gar lustig aus heitrer Wolke", "tokens": ["Er", "kr\u00e4ht", "gar", "lus\u00b7tig", "aus", "hei\u00b7trer", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verj\u00fcngte Freuden dem deutschen Volke.", "tokens": ["Ver\u00b7j\u00fcng\u00b7te", "Freu\u00b7den", "dem", "deut\u00b7schen", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Er kr\u00e4hte: \u00bbDer d\u00fcstern Jahrhunderte Lauf", "tokens": ["Er", "kr\u00e4h\u00b7te", ":", "\u00bb", "Der", "d\u00fcs\u00b7tern", "Jahr\u00b7hun\u00b7der\u00b7te", "Lauf"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verrann, Germaniens Luft wird klar,", "tokens": ["Ver\u00b7rann", ",", "Ger\u00b7ma\u00b7ni\u00b7ens", "Luft", "wird", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Neu wachen die Heinriche, Friedriche auf,", "tokens": ["Neu", "wa\u00b7chen", "die", "Hein\u00b7ri\u00b7che", ",", "Fried\u00b7ri\u00b7che", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Mit ihnen der Seher, der Helden Schar,", "tokens": ["Mit", "ih\u00b7nen", "der", "Se\u00b7her", ",", "der", "Hel\u00b7den", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die deutsche Sonne mit gl\u00e4nzenden Tagen", "tokens": ["Die", "deut\u00b7sche", "Son\u00b7ne", "mit", "gl\u00e4n\u00b7zen\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lenkt \u00fcber die H\u00e4upter der V\u00f6lker den Wagen.\u00ab", "tokens": ["Lenkt", "\u00fc\u00b7ber", "die", "H\u00e4up\u00b7ter", "der", "V\u00f6l\u00b7ker", "den", "Wa\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "O Goldkamm, du gl\u00fcckverk\u00fcndender Hahn,", "tokens": ["O", "Gold\u00b7kamm", ",", "du", "gl\u00fcck\u00b7ver\u00b7k\u00fcn\u00b7den\u00b7der", "Hahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So singst und klingst du vom gr\u00fcnen Ast \u2013", "tokens": ["So", "singst", "und", "klingst", "du", "vom", "gr\u00fc\u00b7nen", "Ast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O s\u00fc\u00dfer, heiliger, deutscher Wahn!", "tokens": ["O", "s\u00fc\u00b7\u00dfer", ",", "hei\u00b7li\u00b7ger", ",", "deut\u00b7scher", "Wahn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich halte die Herrlichkeit fest umfa\u00dft:", "tokens": ["Ich", "hal\u00b7te", "die", "Herr\u00b7lich\u00b7keit", "fest", "um\u00b7fa\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Was seine Runen geklungen haben,", "tokens": ["Was", "sei\u00b7ne", "Ru\u00b7nen", "ge\u00b7klun\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Weissagung soll mir kein Grab begraben.", "tokens": ["Die", "Weis\u00b7sa\u00b7gung", "soll", "mir", "kein", "Grab", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}