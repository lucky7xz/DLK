{"textgrid.poem.37629": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sag, wo kommst du S\u00e4nger her?", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag, wo kommst du S\u00e4nger her?", "tokens": ["Sag", ",", "wo", "kommst", "du", "S\u00e4n\u00b7ger", "her", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Apostel f\u00fchr' ich ein.", "tokens": ["Den", "A\u00b7pos\u00b7tel", "f\u00fchr'", "ich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sag, was will denn seine Lehr?", "tokens": ["Sag", ",", "was", "will", "denn", "sei\u00b7ne", "Lehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies zu fassen bin nicht rein.", "tokens": ["Dies", "zu", "fas\u00b7sen", "bin", "nicht", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist doch klug wie keiner hier?", "tokens": ["Bist", "doch", "klug", "wie", "kei\u00b7ner", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nig h\u00f6r': das sagt nicht viel.", "tokens": ["K\u00f6\u00b7nig", "h\u00f6r'", ":", "das", "sagt", "nicht", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PDS", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist als K\u00e4mpfer \u00fcber mir?", "tokens": ["Bist", "als", "K\u00e4mp\u00b7fer", "\u00fc\u00b7ber", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Herr, das ist ein kleines Ziel.", "tokens": ["Herr", ",", "das", "ist", "ein", "klei\u00b7nes", "Ziel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "S\u00e4nger sprich wie alt du bist?", "tokens": ["S\u00e4n\u00b7ger", "sprich", "wie", "alt", "du", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wei\u00df ich es doch selber nicht.", "tokens": ["Wei\u00df", "ich", "es", "doch", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sage der Vergn\u00fcgung List?", "tokens": ["Sa\u00b7ge", "der", "Ver\u00b7gn\u00fc\u00b7gung", "List", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ich bewahr' mein Lebenslicht.", "tokens": ["Ich", "be\u00b7wahr'", "mein", "Le\u00b7bens\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Sprich, wer hat es dir gebracht?", "tokens": ["Sprich", ",", "wer", "hat", "es", "dir", "ge\u00b7bracht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Weiber drei, alt, unbekannt,", "tokens": ["Wei\u00b7ber", "drei", ",", "alt", ",", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "CARD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "In der gro\u00dfen Mitternacht,", "tokens": ["In", "der", "gro\u00b7\u00dfen", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Die mich in die Welt gesandt.", "tokens": ["Die", "mich", "in", "die", "Welt", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Eh du dieses Licht verbrannt,", "tokens": ["Eh", "du", "die\u00b7ses", "Licht", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sprach die Eine, die's mir bot,", "tokens": ["Sprach", "die", "Ei\u00b7ne", ",", "die's", "mir", "bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Es ist Nornengeist genannt,", "tokens": ["Es", "ist", "Nor\u00b7nen\u00b7geist", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Bist du, Kindlein, auch nicht todt;", "tokens": ["Bist", "du", ",", "Kin\u00b7dlein", ",", "auch", "nicht", "todt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Angez\u00fcndet hab' ich's nie,", "tokens": ["An\u00b7ge\u00b7z\u00fcn\u00b7det", "hab'", "ich's", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Nimmer habe ich gelebt,", "tokens": ["Nim\u00b7mer", "ha\u00b7be", "ich", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Andrer Thaten, andrer M\u00fch,", "tokens": ["A\u00b7ndrer", "Tha\u00b7ten", ",", "an\u00b7drer", "M\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Mein Gesang mit Lust erhebt.", "tokens": ["Mein", "Ge\u00b7sang", "mit", "Lust", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "S\u00e4nger, mir verkauf dein Licht,", "tokens": ["S\u00e4n\u00b7ger", ",", "mir", "ver\u00b7kauf", "dein", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Ich bewahr' es treulich dir,", "tokens": ["Ich", "be\u00b7wahr'", "es", "treu\u00b7lich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Gebe dir ein Goldgewicht", "tokens": ["Ge\u00b7be", "dir", "ein", "Gold\u00b7ge\u00b7wicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.28": {"text": "Wie es keiner giebt, daf\u00fcr.", "tokens": ["Wie", "es", "kei\u00b7ner", "giebt", ",", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "$,", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Sieh dies Gold und es versuch,", "tokens": ["Sieh", "dies", "Gold", "und", "es", "ver\u00b7such", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Nirgend giebt es mehr solch Gold!", "tokens": ["Nir\u00b7gend", "giebt", "es", "mehr", "solch", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Zwar es ruht darauf ein Fluch,", "tokens": ["Zwar", "es", "ruht", "da\u00b7rauf", "ein", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Denn es gab der Schande Sold.", "tokens": ["Denn", "es", "gab", "der", "Schan\u00b7de", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Dieses goldne W\u00fcrfelpaar", "tokens": ["Die\u00b7ses", "gold\u00b7ne", "W\u00fcr\u00b7fel\u00b7paar"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Hat verloost des Herren Kleid,", "tokens": ["Hat", "ver\u00b7lo\u00b7ost", "des", "Her\u00b7ren", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Der f\u00fcr uns gekommen war", "tokens": ["Der", "f\u00fcr", "uns", "ge\u00b7kom\u00b7men", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Und gelitten alles Leid.", "tokens": ["Und", "ge\u00b7lit\u00b7ten", "al\u00b7les", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Nun so nimm von diesem Wein,", "tokens": ["Nun", "so", "nimm", "von", "die\u00b7sem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Nimm von diesem Brode an,", "tokens": ["Nimm", "von", "die\u00b7sem", "Bro\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Denn du magst wohl heilig sein", "tokens": ["Denn", "du", "magst", "wohl", "hei\u00b7lig", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Und du bist ein guter Mann.", "tokens": ["Und", "du", "bist", "ein", "gu\u00b7ter", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Nein, ein andrer giebt mir Wein,", "tokens": ["Nein", ",", "ein", "an\u00b7drer", "giebt", "mir", "Wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Nein, ein andrer giebt mir Brod,", "tokens": ["Nein", ",", "ein", "an\u00b7drer", "giebt", "mir", "Brod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Wenn mein Mund ganz n\u00fcchtern rein,", "tokens": ["Wenn", "mein", "Mund", "ganz", "n\u00fcch\u00b7tern", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Geh ich also in den Tod.", "tokens": ["Geh", "ich", "al\u00b7so", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Der Apostel bringt sein Blut,", "tokens": ["Der", "A\u00b7pos\u00b7tel", "bringt", "sein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.46": {"text": "Bringt des Herren Fleisch dazu,", "tokens": ["Bringt", "des", "Her\u00b7ren", "Fleisch", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "K\u00f6nig, lege ab die Wuth,", "tokens": ["K\u00f6\u00b7nig", ",", "le\u00b7ge", "ab", "die", "Wuth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Wirf dich nieder dort in Ruh.", "tokens": ["Wirf", "dich", "nie\u00b7der", "dort", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Mit dem Kelche kam er her,", "tokens": ["Mit", "dem", "Kel\u00b7che", "kam", "er", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Brach mit seiner Hand das Brod,", "tokens": ["Brach", "mit", "sei\u00b7ner", "Hand", "das", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Und so schuf er neue Lehr'", "tokens": ["Und", "so", "schuf", "er", "neu\u00b7e", "Lehr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "In der alten Heiden Noth.", "tokens": ["In", "der", "al\u00b7ten", "Hei\u00b7den", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Seht den Tisch des Herrn bereit,", "tokens": ["Seht", "den", "Tisch", "des", "Herrn", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Doch es fehlet noch das Licht,", "tokens": ["Doch", "es", "feh\u00b7let", "noch", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Aus ist alter S\u00e4nger Zeit,", "tokens": ["Aus", "ist", "al\u00b7ter", "S\u00e4n\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Und der alte S\u00e4nger spricht:", "tokens": ["Und", "der", "al\u00b7te", "S\u00e4n\u00b7ger", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.57": {"text": "Altes Feuer werde frei,", "tokens": ["Al\u00b7tes", "Feu\u00b7er", "wer\u00b7de", "frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Alte Liebe frei heraus,", "tokens": ["Al\u00b7te", "Lie\u00b7be", "frei", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Da\u00df ich hier ein Zeuge sei,", "tokens": ["Da\u00df", "ich", "hier", "ein", "Zeu\u00b7ge", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Leuchte ich in diesem Haus.", "tokens": ["Leuch\u00b7te", "ich", "in", "die\u00b7sem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Also z\u00fcndet er sein Licht,", "tokens": ["Al\u00b7so", "z\u00fcn\u00b7det", "er", "sein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Bringt es zu dem Abendmahl", "tokens": ["Bringt", "es", "zu", "dem", "A\u00b7bend\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Und empf\u00e4ngt was ihm gebricht,", "tokens": ["Und", "emp\u00b7f\u00e4ngt", "was", "ihm", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Christi Leib und Gnadenmahl.", "tokens": ["Chris\u00b7ti", "Leib", "und", "Gna\u00b7den\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Also hatte er gelebt,", "tokens": ["Al\u00b7so", "hat\u00b7te", "er", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.66": {"text": "Selig sank er in den Tod,", "tokens": ["Se\u00b7lig", "sank", "er", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Neues Blut ihn neu belebt,", "tokens": ["Neu\u00b7es", "Blut", "ihn", "neu", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.68": {"text": "Neuen Leib giebt ihm das Brod,", "tokens": ["Neu\u00b7en", "Leib", "giebt", "ihm", "das", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.69": {"text": "Das Gespinnst der Nornendrei", "tokens": ["Das", "Ge\u00b7spinnst", "der", "Nor\u00b7nen\u00b7drei"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.70": {"text": "Brennt wie Spinnweb an dem Licht,", "tokens": ["Brennt", "wie", "Spinn\u00b7web", "an", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Altes Leben ist nun frei,", "tokens": ["Al\u00b7tes", "Le\u00b7ben", "ist", "nun", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "Und vom Schicksal keiner spricht.", "tokens": ["Und", "vom", "Schick\u00b7sal", "kei\u00b7ner", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}