{"textgrid.poem.49251": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei sehr gegr\u00fc\u00dft, du Stadt, in Holland auserkoren", "tokens": ["Sei", "sehr", "ge\u00b7gr\u00fc\u00dft", ",", "du", "Stadt", ",", "in", "Hol\u00b7land", "au\u00b7ser\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "PPER", "NN", "$,", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor allen, weil du mir Demophilen geboren;", "tokens": ["Vor", "al\u00b7len", ",", "weil", "du", "mir", "De\u00b7mo\u00b7phi\u00b7len", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "KOUS", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "-+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich aber bin jetzund den Wellen unterthan,", "tokens": ["Ich", "a\u00b7ber", "bin", "je\u00b7tzund", "den", "Wel\u00b7len", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df ich nicht mit ihr nach Notturft reden kan.", "tokens": ["So", "da\u00df", "ich", "nicht", "mit", "ihr", "nach", "Not\u00b7turft", "re\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Doch bitt' ich, weil die Wind' jetzt mich von hinnen tragen,", "tokens": ["Doch", "bitt'", "ich", ",", "weil", "die", "Wind'", "jetzt", "mich", "von", "hin\u00b7nen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du wollst di\u00df treue Wort ihr in die Augen sagen:", "tokens": ["Du", "wollst", "di\u00df", "treu\u00b7e", "Wort", "ihr", "in", "die", "Au\u00b7gen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob gleich ich von der See hier ganz umringet bin,", "tokens": ["Ob", "gleich", "ich", "von", "der", "See", "hier", "ganz", "um\u00b7rin\u00b7get", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hitz' und brenn' ich doch noch immer wie vorhin.", "tokens": ["So", "hitz'", "und", "brenn'", "ich", "doch", "noch", "im\u00b7mer", "wie", "vor\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}