{"textgrid.poem.52899": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdies war die Stelle\u00ab, sprach ein greiser Krieger,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdies war die Stelle\u00ab, sprach ein greiser Krieger,", "tokens": ["\u00bb", "dies", "war", "die", "Stel\u00b7le", "\u00ab", ",", "sprach", "ein", "grei\u00b7ser", "Krie\u00b7ger", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwo wir im Winter \u00fcber sind gesetzt;", "tokens": ["\u00bb", "wo", "wir", "im", "Win\u00b7ter", "\u00fc\u00b7ber", "sind", "ge\u00b7setzt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "APPRART", "NN", "APPR", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier haben wir zum ersten Mal als Sieger", "tokens": ["Hier", "ha\u00b7ben", "wir", "zum", "ers\u00b7ten", "Mal", "als", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ihrer Schwelle unser Schwert gewetzt.", "tokens": ["Auf", "ih\u00b7rer", "Schwel\u00b7le", "un\u00b7ser", "Schwert", "ge\u00b7wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Herr \u2013 Eine Lust! Der Alt' auf seinem Schimmel,", "tokens": ["Herr", "\u2013", "Ei\u00b7ne", "Lust", "!", "Der", "Alt'", "auf", "sei\u00b7nem", "Schim\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Dort sprengt' er in die eisbedeckte Flut,", "tokens": ["Dort", "sprengt'", "er", "in", "die", "eis\u00b7be\u00b7deck\u00b7te", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und in den Wellen spiegelte der Himmel", "tokens": ["Und", "in", "den", "Wel\u00b7len", "spie\u00b7gel\u00b7te", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hell seine Sterne ab und unsren Mut.\u00ab", "tokens": ["Hell", "sei\u00b7ne", "Ster\u00b7ne", "ab", "und", "un\u00b7sren", "Mut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nachdenklich sah ich in das dunkle Wasser,", "tokens": ["Nach\u00b7denk\u00b7lich", "sah", "ich", "in", "das", "dunk\u00b7le", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das tr\u00e4umend durch die stille Talschlucht zog.", "tokens": ["Das", "tr\u00e4u\u00b7mend", "durch", "die", "stil\u00b7le", "Tal\u00b7schlucht", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Bilder alle der Franzosenhasser,", "tokens": ["Die", "Bil\u00b7der", "al\u00b7le", "der", "Fran\u00b7zo\u00b7sen\u00b7has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Friedlich zu Fu\u00dfe und zu Rosse hoch,", "tokens": ["Fried\u00b7lich", "zu", "Fu\u00b7\u00dfe", "und", "zu", "Ros\u00b7se", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die zahmen, die mit Wort und Reimen streiten,", "tokens": ["Die", "zah\u00b7men", ",", "die", "mit", "Wort", "und", "Rei\u00b7men", "strei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die wilden, die der Kampf ins Feuer trug,", "tokens": ["Die", "wil\u00b7den", ",", "die", "der", "Kampf", "ins", "Feu\u00b7er", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich sah gespenstig sie hin\u00fcberschreiten,", "tokens": ["Ich", "sah", "ge\u00b7spens\u00b7tig", "sie", "hin\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gen Westen zu ein langer Pilgerzug.", "tokens": ["Gen", "Wes\u00b7ten", "zu", "ein", "lan\u00b7ger", "Pil\u00b7ger\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Grau nickten die zerbrochnen Ritterschl\u00f6sser", "tokens": ["Grau", "nick\u00b7ten", "die", "zer\u00b7broch\u00b7nen", "Rit\u00b7ter\u00b7schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hernieder an den \u00bbfreien, deutschen\u00ab Strand ...", "tokens": ["Her\u00b7nie\u00b7der", "an", "den", "\u00bb", "frei\u00b7en", ",", "deut\u00b7schen", "\u00ab", "Strand", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "$(", "ADJA", "$,", "ADJA", "$(", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War jene Zeit, so fragt' ich, deutscher, besser", "tokens": ["War", "je\u00b7ne", "Zeit", ",", "so", "fragt'", "ich", ",", "deut\u00b7scher", ",", "bes\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und freier, da ihr stolzes Haupt noch stand,", "tokens": ["Und", "frei\u00b7er", ",", "da", "ihr", "stol\u00b7zes", "Haupt", "noch", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da Sang und Klang von ihren S\u00f6llern t\u00f6nte", "tokens": ["Da", "Sang", "und", "Klang", "von", "ih\u00b7ren", "S\u00f6l\u00b7lern", "t\u00f6n\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Jammer aus dem dunklen Burgverlies,", "tokens": ["Und", "Jam\u00b7mer", "aus", "dem", "dunk\u00b7len", "Burg\u00b7ver\u00b7lies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da frech der Edle die Vasallen h\u00f6hnte", "tokens": ["Da", "frech", "der", "Ed\u00b7le", "die", "Va\u00b7sal\u00b7len", "h\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Wanderer am Wege niederstie\u00df?", "tokens": ["Und", "Wan\u00b7de\u00b7rer", "am", "We\u00b7ge", "nie\u00b7ders\u00b7tie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jene Zeit, da mit dem F\u00fcrstenschwerte", "tokens": ["Und", "je\u00b7ne", "Zeit", ",", "da", "mit", "dem", "F\u00fcrs\u00b7ten\u00b7schwer\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Krummstab eines m\u00e4cht'gen Pfaffen focht?", "tokens": ["Der", "Krumm\u00b7stab", "ei\u00b7nes", "m\u00e4cht'\u00b7gen", "Pfaf\u00b7fen", "focht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jene, da die freie, deutsche Erde", "tokens": ["Und", "je\u00b7ne", ",", "da", "die", "frei\u00b7e", ",", "deut\u00b7sche", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "$,", "KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein k\u00fchner R\u00f6mer spielend unterjocht \u2013 ? \u2013", "tokens": ["Ein", "k\u00fch\u00b7ner", "R\u00f6\u00b7mer", "spie\u00b7lend", "un\u00b7ter\u00b7jocht", "\u2013", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$(", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbfrei\u00ab war der Rhein, da er durch \u00f6de Steine", "tokens": ["\u00bb", "frei", "\u00ab", "war", "der", "Rhein", ",", "da", "er", "durch", "\u00f6\u00b7de", "Stei\u00b7ne"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$(", "VAFIN", "ART", "NE", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Noch unbewohnt sich selbst die Bahnen brach,", "tokens": ["Noch", "un\u00b7be\u00b7wohnt", "sich", "selbst", "die", "Bah\u00b7nen", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbdeutsch\u00ab war der Rhein, da hier im Eichenhaine", "tokens": ["\u00bb", "deutsch", "\u00ab", "war", "der", "Rhein", ",", "da", "hier", "im", "Ei\u00b7chen\u00b7hai\u00b7ne"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$(", "VAFIN", "ART", "NE", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein wildes Volk auf B\u00e4renh\u00e4uten lag!", "tokens": ["Ein", "wil\u00b7des", "Volk", "auf", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ten", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Geht mir mit Euren Liedern f\u00fcr und wider!", "tokens": ["Geht", "mir", "mit", "Eu\u00b7ren", "Lie\u00b7dern", "f\u00fcr", "und", "wi\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geduldig ist das lumpige Papier,", "tokens": ["Ge\u00b7dul\u00b7dig", "ist", "das", "lum\u00b7pi\u00b7ge", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleichgiltig str\u00f6mt und k\u00fchl die Welle nieder,", "tokens": ["Gleich\u00b7gil\u00b7tig", "str\u00f6mt", "und", "k\u00fchl", "die", "Wel\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Taub f\u00fcr der Menschen Zank um Mir und Dir,", "tokens": ["Taub", "f\u00fcr", "der", "Men\u00b7schen", "Zank", "um", "Mir", "und", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "APPR", "NE", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Franzmann beut sie schmeichlerisch den R\u00fccken", "tokens": ["Dem", "Franz\u00b7mann", "beut", "sie", "schmeich\u00b7le\u00b7risch", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und tr\u00e4gt den Deutschen, wirft er sich hinein:", "tokens": ["Und", "tr\u00e4gt", "den", "Deut\u00b7schen", ",", "wirft", "er", "sich", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Rhein, wie Ihr, l\u00e4\u00dft sich von jedem dr\u00fccken,", "tokens": ["Der", "Rhein", ",", "wie", "Ihr", ",", "l\u00e4\u00dft", "sich", "von", "je\u00b7dem", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PWAV", "PPER", "$,", "VVFIN", "PRF", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Drum hei\u00dft er auch der freie deutsche Rhein.", "tokens": ["Drum", "hei\u00dft", "er", "auch", "der", "frei\u00b7e", "deut\u00b7sche", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dumpf grollend ging die Woge mir zu F\u00fc\u00dfen,", "tokens": ["Dumpf", "grol\u00b7lend", "ging", "die", "Wo\u00b7ge", "mir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als w\u00fc\u00dfte sie, was meine Lippe schalt.", "tokens": ["Als", "w\u00fc\u00df\u00b7te", "sie", ",", "was", "mei\u00b7ne", "Lip\u00b7pe", "schalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da tauchte abw\u00e4rts, unter B\u00f6llersch\u00fcssen,", "tokens": ["Da", "tauch\u00b7te", "ab\u00b7w\u00e4rts", ",", "un\u00b7ter", "B\u00f6l\u00b7ler\u00b7sch\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Nachtbild auf von riesiger Gestalt;", "tokens": ["Ein", "Nacht\u00b7bild", "auf", "von", "rie\u00b7si\u00b7ger", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Strom entgegen w\u00e4lzte sich im D\u00fcstern", "tokens": ["Dem", "Strom", "ent\u00b7ge\u00b7gen", "w\u00e4lz\u00b7te", "sich", "im", "D\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Donnerton der D\u00e4mpfer her von fern,", "tokens": ["Mit", "Don\u00b7ner\u00b7ton", "der", "D\u00e4mp\u00b7fer", "her", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "ADV", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Rauch und Schaum entspr\u00fchte seinen N\u00fcstern,", "tokens": ["Und", "Rauch", "und", "Schaum", "ent\u00b7spr\u00fch\u00b7te", "sei\u00b7nen", "N\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und hoch am Maste hing es wie ein Stern.", "tokens": ["Und", "hoch", "am", "Mas\u00b7te", "hing", "es", "wie", "ein", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Stern einer neuen Zeit! Sei mir willkommen!", "tokens": ["Stern", "ei\u00b7ner", "neu\u00b7en", "Zeit", "!", "Sei", "mir", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du gehst zur richtigen Minute auf,", "tokens": ["Du", "gehst", "zur", "rich\u00b7ti\u00b7gen", "Mi\u00b7nu\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heran mit Deinen Wundern komm geschwommen,", "tokens": ["He\u00b7ran", "mit", "Dei\u00b7nen", "Wun\u00b7dern", "komm", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entgegen dem gewohnten Wellen-Lauf,", "tokens": ["Ent\u00b7ge\u00b7gen", "dem", "ge\u00b7wohn\u00b7ten", "Wel\u00b7len\u00b7Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erwecke sie, die hier am Ufer tr\u00e4umen,", "tokens": ["Er\u00b7we\u00b7cke", "sie", ",", "die", "hier", "am", "U\u00b7fer", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und rei\u00df sie fort mit Deiner R\u00e4der Kraft!", "tokens": ["Und", "rei\u00df", "sie", "fort", "mit", "Dei\u00b7ner", "R\u00e4\u00b7der", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ja, brausen mu\u00df, wie Du, die Zeit und sch\u00e4umen,", "tokens": ["Ja", ",", "brau\u00b7sen", "mu\u00df", ",", "wie", "Du", ",", "die", "Zeit", "und", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "VMFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Eh' sie den neuen Geist lebendig schafft!", "tokens": ["Eh'", "sie", "den", "neu\u00b7en", "Geist", "le\u00b7ben\u00b7dig", "schafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Strom-auf und nieder schwinge Deine Fahnen,", "tokens": ["Strom\u00b7auf", "und", "nie\u00b7der", "schwin\u00b7ge", "Dei\u00b7ne", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trag hin und her Dein Feuer durch die Welt,", "tokens": ["Trag", "hin", "und", "her", "Dein", "Feu\u00b7er", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sei mit den eisernen Gedanken-Bahnen", "tokens": ["Sei", "mit", "den", "ei\u00b7ser\u00b7nen", "Ge\u00b7dan\u00b7ken\u00b7Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Blitz, der uns die graue Nacht erhellt,", "tokens": ["Der", "Blitz", ",", "der", "uns", "die", "grau\u00b7e", "Nacht", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Band, das uns Geschiedene vereinet,", "tokens": ["Das", "Band", ",", "das", "uns", "Ge\u00b7schie\u00b7de\u00b7ne", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Hand, die uns durch Rad und Ruder lenkt \u2013", "tokens": ["Die", "Hand", ",", "die", "uns", "durch", "Rad", "und", "Ru\u00b7der", "lenkt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann wird er \u00bbfrei\u00ab, doch freier, als Ihr meinet,", "tokens": ["Dann", "wird", "er", "\u00bb", "frei", "\u00ab", ",", "doch", "frei\u00b7er", ",", "als", "Ihr", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "ADJD", "$(", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann wird er \u00bbdeutsch\u00ab, doch deutscher, als Ihr denkt!", "tokens": ["Dann", "wird", "er", "\u00bb", "deutsch", "\u00ab", ",", "doch", "deut\u00b7scher", ",", "als", "Ihr", "denkt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "ADJD", "$(", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Auf, frommes K\u00f6ln, auf, heitres Mainz, erwache,", "tokens": ["Auf", ",", "from\u00b7mes", "K\u00f6ln", ",", "auf", ",", "heit\u00b7res", "Mainz", ",", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Du, junges Mannheim, mache Dich bereit;", "tokens": ["Du", ",", "jun\u00b7ges", "Mann\u00b7heim", ",", "ma\u00b7che", "Dich", "be\u00b7reit", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Stadt zu Stadt, den wachsenden, entfache", "tokens": ["Von", "Stadt", "zu", "Stadt", ",", "den", "wach\u00b7sen\u00b7den", ",", "ent\u00b7fa\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich die Aurora einer neuen Zeit!", "tokens": ["Sich", "die", "Au\u00b7ro\u00b7ra", "ei\u00b7ner", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Ihr, die uns von deutscher Lebensader", "tokens": ["Und", "Ihr", ",", "die", "uns", "von", "deut\u00b7scher", "Le\u00b7ben\u00b7sa\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So viel geschwatzt, \u2013 da\u00df sie zu reich nicht quillt!", "tokens": ["So", "viel", "ge\u00b7schwatzt", ",", "\u2013", "da\u00df", "sie", "zu", "reich", "nicht", "quillt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "$(", "KOUS", "PPER", "PTKA", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr sch\u00fcrtet drin und drau\u00dfen an dem Hader,", "tokens": ["Ihr", "sch\u00fcr\u00b7tet", "drin", "und", "drau\u00b7\u00dfen", "an", "dem", "Ha\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie, wenn er, ", "tokens": ["Wie", ",", "wenn", "er", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Ihr habt's beschworen, seht nun, da\u00df Ihr's zwinget,", "tokens": ["Ihr", "habt's", "be\u00b7schwo\u00b7ren", ",", "seht", "nun", ",", "da\u00df", "Ih\u00b7r's", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sonst w\u00e4chst das Kind Euch alten \u00fcbers Haupt;", "tokens": ["Sonst", "w\u00e4chst", "das", "Kind", "Euch", "al\u00b7ten", "\u00fc\u00b7bers", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort fliegt es hin, ein Vogel leicht beschwinget,", "tokens": ["Dort", "fliegt", "es", "hin", ",", "ein", "Vo\u00b7gel", "leicht", "be\u00b7schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unhemmbar, stark, am Ziel, eh Ihr es glaubt.", "tokens": ["Un\u00b7hemm\u00b7bar", ",", "stark", ",", "am", "Ziel", ",", "eh", "Ihr", "es", "glaubt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+-+-+