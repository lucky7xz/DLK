{"dta.poem.23647": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Abend wird es wieder:", "tokens": ["A\u00b7bend", "wird", "es", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ueber Wald und Feld", "tokens": ["Ue\u00b7ber", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "S\u00e4uselt Frieden nieder,", "tokens": ["S\u00e4u\u00b7selt", "Frie\u00b7den", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und es ruht die Welt.", "tokens": ["Und", "es", "ruht", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nur der Bach ergie\u00dfet", "tokens": ["Nur", "der", "Bach", "er\u00b7gie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich am Felsen dort,", "tokens": ["Sich", "am", "Fel\u00b7sen", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und er braust und flie\u00dfet", "tokens": ["Und", "er", "braust", "und", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Immer, immer fort.", "tokens": ["Im\u00b7mer", ",", "im\u00b7mer", "fort", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und kein Abend bringet", "tokens": ["Und", "kein", "A\u00b7bend", "brin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Frieden ihm und Ruh,", "tokens": ["Frie\u00b7den", "ihm", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Keine Glocke klinget", "tokens": ["Kei\u00b7ne", "Glo\u00b7cke", "klin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihm ein Rastlied zu.", "tokens": ["Ihm", "ein", "Rast\u00b7lied", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "So, in deinem Streben", "tokens": ["So", ",", "in", "dei\u00b7nem", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bist, mein Herz, auch du:", "tokens": ["Bist", ",", "mein", "Herz", ",", "auch", "du", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gott nur kann dir geben", "tokens": ["Gott", "nur", "kann", "dir", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wahre Abendruh.", "tokens": ["Wah\u00b7re", "A\u00b7ben\u00b7druh", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}