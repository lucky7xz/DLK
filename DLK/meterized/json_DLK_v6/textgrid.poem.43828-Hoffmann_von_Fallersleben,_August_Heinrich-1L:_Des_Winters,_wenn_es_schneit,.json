{"textgrid.poem.43828": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Winters, wenn es schneit,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Winters, wenn es schneit,", "tokens": ["Des", "Win\u00b7ters", ",", "wenn", "es", "schneit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dann ist gar b\u00f6se Zeit:", "tokens": ["Dann", "ist", "gar", "b\u00f6\u00b7se", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die armen, armen V\u00f6gelein,", "tokens": ["Die", "ar\u00b7men", ",", "ar\u00b7men", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die thun mir gar zu leid!", "tokens": ["Die", "thun", "mir", "gar", "zu", "leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, k\u00f6nnt' ich locken sie,", "tokens": ["Ach", ",", "k\u00f6nnt'", "ich", "lo\u00b7cken", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und w\u00fc\u00dft' ich doch nur wie?", "tokens": ["Und", "w\u00fc\u00dft'", "ich", "doch", "nur", "wie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die armen, armen V\u00f6gelein,", "tokens": ["Die", "ar\u00b7men", ",", "ar\u00b7men", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sollten hungern nie!", "tokens": ["Sie", "soll\u00b7ten", "hun\u00b7gern", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O nehmet hier Quartier!", "tokens": ["O", "neh\u00b7met", "hier", "Quar\u00b7tier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "O kommt! Was zaudert ihr?", "tokens": ["O", "kommt", "!", "Was", "zau\u00b7dert", "ihr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr armen, armen V\u00f6gelein,", "tokens": ["Ihr", "ar\u00b7men", ",", "ar\u00b7men", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt her und speist bei mir! \u2013", "tokens": ["Kommt", "her", "und", "speist", "bei", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Futter streut' ich aus,", "tokens": ["Das", "Fut\u00b7ter", "streut'", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da kamen sie an's Haus,", "tokens": ["Da", "ka\u00b7men", "sie", "an's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die armen, armen V\u00f6gelein,", "tokens": ["Die", "ar\u00b7men", ",", "ar\u00b7men", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hielten einen Schmaus.", "tokens": ["Sie", "hiel\u00b7ten", "ei\u00b7nen", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O gl\u00fccklich Jedermann,", "tokens": ["O", "gl\u00fcck\u00b7lich", "Je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer geben mag und kann;", "tokens": ["Wer", "ge\u00b7ben", "mag", "und", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr armen, armen V\u00f6gelein,", "tokens": ["Ihr", "ar\u00b7men", ",", "ar\u00b7men", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nehmt meine Gaben an!", "tokens": ["Nehmt", "mei\u00b7ne", "Ga\u00b7ben", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}