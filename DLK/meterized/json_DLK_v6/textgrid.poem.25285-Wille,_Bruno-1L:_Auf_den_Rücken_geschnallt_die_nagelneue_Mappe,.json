{"textgrid.poem.25285": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf den R\u00fccken geschnallt die nagelneue Mappe,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den R\u00fccken geschnallt die nagelneue Mappe,", "tokens": ["Auf", "den", "R\u00fc\u00b7cken", "ge\u00b7schnallt", "die", "na\u00b7gel\u00b7neu\u00b7e", "Map\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Fibel und Schiefertafel unter der gro\u00dfen Klappe,", "tokens": ["Fi\u00b7bel", "und", "Schie\u00b7fer\u00b7ta\u00b7fel", "un\u00b7ter", "der", "gro\u00b7\u00dfen", "Klap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Schwamm und Schieferstift bammelnd an F\u00e4dchen", "tokens": ["Schwamm", "und", "Schie\u00b7fers\u00b7tift", "bam\u00b7melnd", "an", "F\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "APPR", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Trollt ich mit kleinen Knaben und M\u00e4dchen", "tokens": ["Trollt", "ich", "mit", "klei\u00b7nen", "Kna\u00b7ben", "und", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Zur Schule nach ", "tokens": ["Zur", "Schu\u00b7le", "nach"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und war doch ein Greis,", "tokens": ["Und", "war", "doch", "ein", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Mit Haaren schlohwei\u00df", "tokens": ["Mit", "Haa\u00b7ren", "schloh\u00b7wei\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und wallendem Bart.", "tokens": ["Und", "wal\u00b7len\u00b7dem", "Bart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Bald hockt ich auf niedriger Klassenbank", "tokens": ["Bald", "hockt", "ich", "auf", "nied\u00b7ri\u00b7ger", "Klas\u00b7sen\u00b7bank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zwischen Ofen und Klassenschrank;", "tokens": ["Zwi\u00b7schen", "O\u00b7fen", "und", "Klas\u00b7sen\u00b7schrank", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Herr Lehrer sa\u00df auf dem Katheder.", "tokens": ["Der", "Herr", "Leh\u00b7rer", "sa\u00df", "auf", "dem", "Ka\u00b7the\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Laut und deutlich mu\u00dfte nun jeder", "tokens": ["Laut", "und", "deut\u00b7lich", "mu\u00df\u00b7te", "nun", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "ADJD", "VMFIN", "ADV", "PIAT"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Aus der Fibel buchstabieren,", "tokens": ["Aus", "der", "Fi\u00b7bel", "buch\u00b7sta\u00b7bie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Artikulieren, deklamieren.", "tokens": ["Ar\u00b7ti\u00b7ku\u00b7lie\u00b7ren", ",", "de\u00b7kla\u00b7mie\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Vom pl\u00e4rrenden Chorus hallte das Zimmer:", "tokens": ["Vom", "pl\u00e4r\u00b7ren\u00b7den", "Cho\u00b7rus", "hall\u00b7te", "das", "Zim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u00bbi, m: Im! Im/ mer.", "tokens": ["\u00bb", "i", ",", "m", ":", "Im", "!", "Im", "/", "mer", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "NE", "$,", "FM.la", "$.", "APPRART", "$.", "APPRART", "$(", "ADV", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.9": {"text": "Ni, m: Nim! Nim/ mer!\u00ab", "tokens": ["Ni", ",", "m", ":", "Nim", "!", "Nim", "/", "mer", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$,", "FM.la", "$.", "NE", "$.", "NE", "$(", "ADV", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ich stammelte mit, zerstreut, verlegen,", "tokens": ["Ich", "stam\u00b7mel\u00b7te", "mit", ",", "zer\u00b7streut", ",", "ver\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVPP", "$,", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wagte kein Auge vom Buch zu bewegen,", "tokens": ["Wag\u00b7te", "kein", "Au\u00b7ge", "vom", "Buch", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Wu\u00dfte vor Scham mich nicht zu lassen.", "tokens": ["Wu\u00df\u00b7te", "vor", "Scham", "mich", "nicht", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Was tat ich nur hier? Ich konnt es nicht fassen.", "tokens": ["Was", "tat", "ich", "nur", "hier", "?", "Ich", "konnt", "es", "nicht", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Hatte Bibliotheken durchstudiert,", "tokens": ["Hat\u00b7te", "Bib\u00b7lio\u00b7the\u00b7ken", "durch\u00b7stu\u00b7diert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "War B\u00fccherverfasser, ein Denker, ein Dichter ...", "tokens": ["War", "B\u00fc\u00b7cher\u00b7ver\u00b7fas\u00b7ser", ",", "ein", "Den\u00b7ker", ",", "ein", "Dich\u00b7ter", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.17": {"text": "Was tat ich hier zwischen dem Fibelgelichter?", "tokens": ["Was", "tat", "ich", "hier", "zwi\u00b7schen", "dem", "Fi\u00b7bel\u00b7ge\u00b7lich\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Urpl\u00f6tzlich sah ich zu meinem Schrecken", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "sah", "ich", "zu", "mei\u00b7nem", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Des Herren Lehrers hochw\u00fcrdigen Bauch", "tokens": ["Des", "Her\u00b7ren", "Leh\u00b7rers", "hoch\u00b7w\u00fcr\u00b7di\u00b7gen", "Bauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.20": {"text": "Vor meinen Platz sich pflanzen und recken.", "tokens": ["Vor", "mei\u00b7nen", "Platz", "sich", "pflan\u00b7zen", "und", "re\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "\u00bbnun, Brunochen\u00ab, sprach er, \u00bbsag du's auch!", "tokens": ["\u00bb", "nun", ",", "Bru\u00b7no\u00b7chen", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "sag", "du's", "auch", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Ein kleines Blauveilchen ...?\u00ab", "tokens": ["Ein", "klei\u00b7nes", "Blau\u00b7veil\u00b7chen", "...", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$.", "$("], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.23": {"text": "Ich erhob mich verbl\u00fcfft, mit Zittern und Zagen;", "tokens": ["Ich", "er\u00b7hob", "mich", "ver\u00b7bl\u00fcfft", ",", "mit", "Zit\u00b7tern", "und", "Za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Was sollt ich sagen? Ein kleines Blauveilchen?", "tokens": ["Was", "sollt", "ich", "sa\u00b7gen", "?", "Ein", "klei\u00b7nes", "Blau\u00b7veil\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-++-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Auf einmal erwachte, Zeile f\u00fcr Zeilchen,", "tokens": ["Auf", "ein\u00b7mal", "er\u00b7wach\u00b7te", ",", "Zei\u00b7le", "f\u00fcr", "Zeil\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Die Fabel aus meinen Kindertagen,", "tokens": ["Die", "Fa\u00b7bel", "aus", "mei\u00b7nen", "Kin\u00b7der\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Und ich konnte mechanisch sagen:", "tokens": ["Und", "ich", "konn\u00b7te", "me\u00b7cha\u00b7nisch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "\u00bbein/ klei/ nes/ Blau/ veil/ chen", "tokens": ["\u00bb", "ein", "/", "klei", "/", "nes", "/", "Blau", "/", "veil", "/", "chen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "ART", "$(", "XY", "$(", "ADJA", "$(", "NN", "$(", "KOUS", "$(", "NE"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.29": {"text": "Stand eben erst ein Weilchen", "tokens": ["Stand", "e\u00b7ben", "erst", "ein", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Unten im Tal am Bach.\u00ab", "tokens": ["Un\u00b7ten", "im", "Tal", "am", "Bach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.31": {"text": "Da dacht es nach und sprach:", "tokens": ["Da", "dacht", "es", "nach", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "\u00bbda\u00df ich hier unten bl\u00fch,", "tokens": ["\u00bb", "da\u00df", "ich", "hier", "un\u00b7ten", "bl\u00fch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Lohnt sich kaum der M\u00fch;", "