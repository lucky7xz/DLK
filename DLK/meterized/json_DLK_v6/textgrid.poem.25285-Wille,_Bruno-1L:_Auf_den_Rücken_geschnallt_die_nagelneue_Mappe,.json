"iambic.tetra"}, "line.40": {"text": "Und wie gesagt, so getan;", "tokens": ["Und", "wie", "ge\u00b7sagt", ",", "so", "ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Aus dem Wiesenland", "tokens": ["Aus", "dem", "Wie\u00b7sen\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.42": {"text": "Mit eigener Hand", "tokens": ["Mit", "ei\u00b7ge\u00b7ner", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.43": {"text": "Zieht es ein Beinchen nach dem andern", "tokens": ["Zieht", "es", "ein", "Be\u00b7in\u00b7chen", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Und begibt sich aufs Wandern.", "tokens": ["Und", "be\u00b7gibt", "sich", "aufs", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.45": {"text": "\u00bbdr\u00fcben der H\u00fcgel w\u00e4r mir schon recht!", "tokens": ["\u00bb", "dr\u00fc\u00b7ben", "der", "H\u00fc\u00b7gel", "w\u00e4r", "mir", "schon", "recht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Wenn ich den erreichen m\u00f6cht,", "tokens": ["Wenn", "ich", "den", "er\u00b7rei\u00b7chen", "m\u00f6cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": [