{"textgrid.poem.25515": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn der Schlaf, um Mitternacht,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Schlaf, um Mitternacht,", "tokens": ["Wenn", "der", "Schlaf", ",", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest auf allen Augen lieget,", "tokens": ["Fest", "auf", "al\u00b7len", "Au\u00b7gen", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn allein dein M\u00e4dchen wacht,", "tokens": ["Wenn", "al\u00b7lein", "dein", "M\u00e4d\u00b7chen", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und auf langen leisen Zehn", "tokens": ["Und", "auf", "lan\u00b7gen", "lei\u00b7sen", "Zehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Sich bei jedem Schritte wieget:", "tokens": ["Sich", "bei", "je\u00b7dem", "Schrit\u00b7te", "wie\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werden wir uns wieder sehn.", "tokens": ["Wer\u00b7den", "wir", "uns", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie? das w\u00e4ren bis dahin,", "tokens": ["Wie", "?", "das", "w\u00e4\u00b7ren", "bis", "da\u00b7hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PDS", "VAFIN", "APPR", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Funfzehn, ganze funfzehn Stunden?", "tokens": ["Funf\u00b7zehn", ",", "gan\u00b7ze", "funf\u00b7zehn", "Stun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ha! wenn ich nicht bei dir bin,", "tokens": ["Ha", "!", "wenn", "ich", "nicht", "bei", "dir", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schleicht der Zeiger an der Uhr;", "tokens": ["Schleicht", "der", "Zei\u00b7ger", "an", "der", "Uhr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Schnell verfliegen die Secunden,", "tokens": ["Schnell", "ver\u00b7flie\u00b7gen", "die", "Se\u00b7cun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn ich bei dir sitze, nur.", "tokens": ["Wenn", "ich", "bei", "dir", "sit\u00b7ze", ",", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Was vertreibt mir diese Zeit?", "tokens": ["Was", "ver\u00b7treibt", "mir", "die\u00b7se", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ach! kein Lesen und kein Schreiben!", "tokens": ["Ach", "!", "kein", "Le\u00b7sen", "und", "kein", "Schrei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Eher noch als ", "tokens": ["E\u00b7her", "noch", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ist's mit Nanten!) m\u00f6chte mir", "tokens": ["Ist's", "mit", "Nan\u00b7ten", "!", ")", "m\u00f6ch\u00b7te", "mir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "APPR", "NN", "$.", "$(", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Sie spricht wenigstens von dir.", "tokens": ["Sie", "spricht", "we\u00b7nigs\u00b7tens", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}