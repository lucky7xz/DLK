{"dta.poem.23433": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nos fronti\u00e8res du Rhin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir tr\u00e4nken freilich lieber Thee", "tokens": ["Wir", "tr\u00e4n\u00b7ken", "frei\u00b7lich", "lie\u00b7ber", "Thee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als andere Getr\u00e4nke \u2014", "tokens": ["Als", "an\u00b7de\u00b7re", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Habt Dank f\u00fcr eure Soir", "tokens": ["Habt", "Dank", "f\u00fcr", "eu\u00b7re", "Soir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und euere Geschenke!", "tokens": ["Und", "eu\u00b7e\u00b7re", "Ge\u00b7schen\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ihr wollt uns euer bischen Trank", "tokens": ["Ihr", "wollt", "uns", "eu\u00b7er", "bi\u00b7schen", "Trank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu theuer stets verkaufen,", "tokens": ["Zu", "theu\u00b7er", "stets", "ver\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wollt daf\u00fcr den Rhein \u2014 habt Dank!", "tokens": ["Ihr", "wollt", "da\u00b7f\u00fcr", "den", "Rhein", "habt", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ART", "NE", "$(", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da mag der Teufel saufen!", "tokens": ["Da", "mag", "der", "Teu\u00b7fel", "sau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}