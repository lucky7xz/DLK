{"textgrid.poem.62846": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lang sah ein Fisch, den die Natur mit Fl\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang sah ein Fisch, den die Natur mit Fl\u00fcgeln", "tokens": ["Lang", "sah", "ein", "Fisch", ",", "den", "die", "Na\u00b7tur", "mit", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von d\u00fcnnem Schleyer ausgeschm\u00fcckt,", "tokens": ["Von", "d\u00fcn\u00b7nem", "Schle\u00b7yer", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den hohen Ph\u00f6bus sich im Weltmeer spiegeln,", "tokens": ["Den", "ho\u00b7hen", "Ph\u00f6\u00b7bus", "sich", "im", "Welt\u00b7meer", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und endlich rief er ganz entz\u00fcckt:", "tokens": ["Und", "end\u00b7lich", "rief", "er", "ganz", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mu\u00df, ich mu\u00df dich in der N\u00e4he,", "tokens": ["Ich", "mu\u00df", ",", "ich", "mu\u00df", "dich", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohlth\u00e4ter aller Wesen, sehn!", "tokens": ["Wohlt\u00b7h\u00e4\u00b7ter", "al\u00b7ler", "We\u00b7sen", ",", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er schwingt sich k\u00fchn in die lazurne H\u00f6he:", "tokens": ["Er", "schwingt", "sich", "k\u00fchn", "in", "die", "la\u00b7zur\u00b7ne", "H\u00f6\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie gro\u00df bist du, wie liebenswerth, wie sch\u00f6n!", "tokens": ["Wie", "gro\u00df", "bist", "du", ",", "wie", "lie\u00b7bens\u00b7werth", ",", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie wunderbar sind deine Stralen,", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "sind", "dei\u00b7ne", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die jeden Tropfen in dem Ocean", "tokens": ["Die", "je\u00b7den", "Trop\u00b7fen", "in", "dem", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit deinem hehren Bild bemalen!", "tokens": ["Mit", "dei\u00b7nem", "heh\u00b7ren", "Bild", "be\u00b7ma\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "O seelig, wer dein Antlitz schauen kann!", "tokens": ["O", "see\u00b7lig", ",", "wer", "dein", "Ant\u00b7litz", "schau\u00b7en", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Itzt f\u00fchlt er nur und opfert stille Thr\u00e4nen.", "tokens": ["Itzt", "f\u00fchlt", "er", "nur", "und", "op\u00b7fert", "stil\u00b7le", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Doch pl\u00f6zlich deckt sein Aug ein d\u00fcstrer Flor:", "tokens": ["Doch", "pl\u00f6z\u00b7lich", "deckt", "sein", "Aug", "ein", "d\u00fcst\u00b7rer", "Flor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Fl\u00fcgel ausgedorrte Sehnen", "tokens": ["Der", "Fl\u00fc\u00b7gel", "aus\u00b7ge\u00b7dorr\u00b7te", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Versagen ihm. Er sinkt, rafft sich empor,", "tokens": ["Ver\u00b7sa\u00b7gen", "ihm", ".", "Er", "sinkt", ",", "rafft", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sinkt tiefer, st\u00fcrzt entgeistert nieder,", "tokens": ["Sinkt", "tie\u00b7fer", ",", "st\u00fcrzt", "ent\u00b7geis\u00b7tert", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und fand, der Seele gleich, die jenseits unsrer Welt", "tokens": ["Und", "fand", ",", "der", "See\u00b7le", "gleich", ",", "die", "jen\u00b7seits", "uns\u00b7rer", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Gottheit schauen will, und aus den Wolken f\u00e4llt,", "tokens": ["Die", "Got\u00b7theit", "schau\u00b7en", "will", ",", "und", "aus", "den", "Wol\u00b7ken", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In seinem Element sich wieder.", "tokens": ["In", "sei\u00b7nem", "E\u00b7le\u00b7ment", "sich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}