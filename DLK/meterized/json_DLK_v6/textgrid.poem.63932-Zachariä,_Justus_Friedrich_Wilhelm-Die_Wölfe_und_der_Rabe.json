{"textgrid.poem.63932": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die W\u00f6lfe und der Rabe", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween W\u00f6lfe, die sehr hungrig waren,", "tokens": ["Zween", "W\u00f6l\u00b7fe", ",", "die", "sehr", "hung\u00b7rig", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begaben sich mit viel Gefahren", "tokens": ["Be\u00b7ga\u00b7ben", "sich", "mit", "viel", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu einem Schafstall. Jeder nahm", "tokens": ["Zu", "ei\u00b7nem", "Schaf\u00b7stall", ".", "Je\u00b7der", "nahm"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich nach Belieben da sein Lamm,", "tokens": ["Sich", "nach", "Be\u00b7lie\u00b7ben", "da", "sein", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eilten d'rauf zum Wald hinein,", "tokens": ["Und", "eil\u00b7ten", "d'\u00b7rauf", "zum", "Wald", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von Niemand mehr gest\u00f6rt zu seyn.", "tokens": ["Von", "Nie\u00b7mand", "mehr", "ge\u00b7st\u00f6rt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Rabe sah's von ohngef\u00e4hr,", "tokens": ["Ein", "Ra\u00b7be", "sah's", "von", "ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Flog hurtig hinter ihnen her;", "tokens": ["Flog", "hur\u00b7tig", "hin\u00b7ter", "ih\u00b7nen", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und als sie nun, nach langem Schmachten,", "tokens": ["Und", "als", "sie", "nun", ",", "nach", "lan\u00b7gem", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich \u00fcber ihre Beute machten,", "tokens": ["Sich", "\u00fc\u00b7ber", "ih\u00b7re", "Beu\u00b7te", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Rief er von einem Baum herab:", "tokens": ["Rief", "er", "von", "ei\u00b7nem", "Baum", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihr Herr'n, gebt mir doch auch was ab!", "tokens": ["Ihr", "Herr'n", ",", "gebt", "mir", "doch", "auch", "was", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr werdet mich doch nicht vergessen,", "tokens": ["Ihr", "wer\u00b7det", "mich", "doch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und Alles so allein hier fressen?", "tokens": ["Und", "Al\u00b7les", "so", "al\u00b7lein", "hier", "fres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich habe k\u00fchn und unverzagt", "tokens": ["Ich", "ha\u00b7be", "k\u00fchn", "und", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mein Leben ja mit euch gewagt;", "tokens": ["Mein", "Le\u00b7ben", "ja", "mit", "euch", "ge\u00b7wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "D'rum gebt mir mein geb\u00fchrend St\u00fcck,", "tokens": ["D'\u00b7rum", "gebt", "mir", "mein", "ge\u00b7b\u00fch\u00b7rend", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "VVPP", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Und weist mich hungrig nicht zur\u00fcck.", "tokens": ["Und", "weist", "mich", "hung\u00b7rig", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ja, sprach ein Wolf, du hast geflogen,", "tokens": ["Ja", ",", "sprach", "ein", "Wolf", ",", "du", "hast", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NE", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und bist uns treulich nachgezogen,", "tokens": ["Und", "bist", "uns", "treu\u00b7lich", "nach\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Doch, guter Freund, aus Eigennutz,", "tokens": ["Doch", ",", "gu\u00b7ter", "Freund", ",", "aus", "Ei\u00b7gen\u00b7nutz", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und wahrlich nicht zu unserm Schutz!", "tokens": ["Und", "wahr\u00b7lich", "nicht", "zu", "un\u00b7serm", "Schutz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}