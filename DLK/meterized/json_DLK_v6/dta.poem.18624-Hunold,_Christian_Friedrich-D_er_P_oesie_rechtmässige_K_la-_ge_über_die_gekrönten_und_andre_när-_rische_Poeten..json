{"dta.poem.18624": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D er  P oesie rechtm\u00e4ssige  K la-  \n ge \u00fcber die gekr\u00f6nten und andre n\u00e4r-  \n rische Poeten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.57", "en:0.28", "sv:0.14"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Apollo la\u00df dir doch in tieffer Demuth klagen/", "tokens": ["A\u00b7pol\u00b7lo", "la\u00df", "dir", "doch", "in", "tief\u00b7fer", "De\u00b7muth", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie meine Reinlichkeit so sehr beflecket ist/", "tokens": ["Wie", "mei\u00b7ne", "Rein\u00b7lich\u00b7keit", "so", "sehr", "be\u00b7fle\u00b7cket", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie meine Jungferschafft sich l\u00e4st zu Grabe tragen/", "tokens": ["Wie", "mei\u00b7ne", "Jung\u00b7fer\u00b7schafft", "sich", "l\u00e4st", "zu", "Gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn du/ gerechter F\u00fcrst/ nicht meine Rettung bist.", "tokens": ["Wenn", "du", "/", "ge\u00b7rech\u00b7ter", "F\u00fcrst", "/", "nicht", "mei\u00b7ne", "Ret\u00b7tung", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ADJD", "NN", "$(", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Nahme/ welchen ja die G\u00f6tter selber f\u00fchren/", "tokens": ["Mein", "Nah\u00b7me", "/", "wel\u00b7chen", "ja", "die", "G\u00f6t\u00b7ter", "sel\u00b7ber", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PWAT", "ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df ein ver\u00e4chtlich Wort in vielen Ohren seyn:", "tokens": ["Mu\u00df", "ein", "ver\u00b7\u00e4cht\u00b7lich", "Wort", "in", "vie\u00b7len", "Oh\u00b7ren", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJD", "NN", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wil sich jeder Narr mit meinen Schmucke Zieren/", "tokens": ["Es", "wil", "sich", "je\u00b7der", "Narr", "mit", "mei\u00b7nen", "Schmu\u00b7cke", "Zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und manches Irrlicht prahlt durch den entlehnten Schein.", "tokens": ["Und", "man\u00b7ches", "Irr\u00b7licht", "prahlt", "durch", "den", "ent\u00b7lehn\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach schaue grosser F\u00fcrst/ wie diese Lorbeer-Crone/", "tokens": ["Ach", "schau\u00b7e", "gros\u00b7ser", "F\u00fcrst", "/", "wie", "die\u00b7se", "Lor\u00b7beer\u00b7Cro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJA", "NN", "$(", "KOKOM", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So deine Gnade mir auff meine Scheitel setzt/", "tokens": ["So", "dei\u00b7ne", "Gna\u00b7de", "mir", "auff", "mei\u00b7ne", "Schei\u00b7tel", "setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Ungeschicklichkeit anitzo tr\u00e4gt zu Lohne/", "tokens": ["Die", "Un\u00b7ge\u00b7schick\u00b7lich\u00b7keit", "a\u00b7nit\u00b7zo", "tr\u00e4gt", "zu", "Loh\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df sie die Klugheit auch vor einen Stroh-Krantz sch\u00e4tzt.", "tokens": ["Da\u00df", "sie", "die", "Klug\u00b7heit", "auch", "vor", "ei\u00b7nen", "Stroh\u00b7Krantz", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich edle Poesie bin von der Kunst beneidet/", "tokens": ["Ich", "ed\u00b7le", "Poe\u00b7sie", "bin", "von", "der", "Kunst", "be\u00b7nei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Da\u00df mich ein hoher Geist sonst was Galantes hie\u00df/", "tokens": ["Da\u00df", "mich", "ein", "ho\u00b7her", "Geist", "sonst", "was", "Ga\u00b7lan\u00b7tes", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "PWS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil mancher T\u00f6lpel sich mit meinen Zierath kleidet/", "tokens": ["Weil", "man\u00b7cher", "T\u00f6l\u00b7pel", "sich", "mit", "mei\u00b7nen", "Zie\u00b7rath", "klei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem doch nat\u00fcrlicher die Narren-Kappe lie\u00df.", "tokens": ["Dem", "doch", "na\u00b7t\u00fcr\u00b7li\u00b7cher", "die", "Nar\u00b7ren\u00b7Kap\u00b7pe", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sonst war ich die Geburth der hocherhabnen Sinnen/", "tokens": ["Sonst", "war", "ich", "die", "Ge\u00b7burth", "der", "ho\u00b7cher\u00b7hab\u00b7nen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Helden nennten sich mir gl\u00fccklich unterthan:", "tokens": ["Die", "Hel\u00b7den", "nenn\u00b7ten", "sich", "mir", "gl\u00fcck\u00b7lich", "un\u00b7ter\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nun denckt ein Eselskopff mein Kleinod zu gewinnen/", "tokens": ["Nun", "denckt", "ein", "E\u00b7sels\u00b7kopff", "mein", "Klei\u00b7nod", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Man sieht mich \u00fcberall nur vor ein Hurkind an.", "tokens": ["Man", "sieht", "mich", "\u00fc\u00b7be\u00b7rall", "nur", "vor", "ein", "Hur\u00b7kind", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie Schalck und Schelm zuvor nur Ehren Titel hiessen/", "tokens": ["Wie", "Schalck", "und", "Schelm", "zu\u00b7vor", "nur", "Eh\u00b7ren", "Ti\u00b7tel", "hies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "ADV", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So schlisset Schimpff und Schand anitzt ihr Wappen ein:", "tokens": ["So", "schlis\u00b7set", "Schimpff", "und", "Schand", "a\u00b7nitzt", "ihr", "Wap\u00b7pen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wird/ was mich zuerst vor G\u00f6ttlich hat gepriesen/", "tokens": ["So", "wird", "/", "was", "mich", "zu\u00b7erst", "vor", "G\u00f6tt\u00b7lich", "hat", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "PWS", "PPER", "ADV", "APPR", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Durch Kriepel der Vernunfft mein Spott und Tadel seyn.", "tokens": ["Durch", "Krie\u00b7pel", "der", "Ver\u00b7nunfft", "mein", "Spott", "und", "Ta\u00b7del", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.25": {"text": "Es kan gantz Indien nicht so viel Affen hecken/", "tokens": ["Es", "kan", "gantz", "In\u00b7di\u00b7en", "nicht", "so", "viel", "Af\u00b7fen", "he\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NE", "PTKNEG", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als wie der Hundes-Stern Poeten-W\u00fcrmer zeugt:", "tokens": ["Als", "wie", "der", "Hun\u00b7des\u00b7S\u00b7tern", "Po\u00b7e\u00b7ten\u00b7W\u00fcr\u00b7mer", "zeugt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Die frische Saat kan nicht so voller Hasen stecken/", "tokens": ["Die", "fri\u00b7sche", "Saat", "kan", "nicht", "so", "vol\u00b7ler", "Ha\u00b7sen", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als eine grosse Zahl in meinen Garten steigt.", "tokens": ["Als", "ei\u00b7ne", "gros\u00b7se", "Zahl", "in", "mei\u00b7nen", "Gar\u00b7ten", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da sol vor Kraut und Kohl der Wei\u00dfheit-Blume gr\u00fcnen/", "tokens": ["Da", "sol", "vor", "Kraut", "und", "Kohl", "der", "Wei\u00df\u00b7heit\u00b7Blu\u00b7me", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die ihnen Thorheit doch/ wie Kr\u00f6ten-Gifft gewehrt/", "tokens": ["Die", "ih\u00b7nen", "Thor\u00b7heit", "doch", "/", "wie", "Kr\u00f6\u00b7ten\u00b7Gifft", "ge\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADV", "$(", "KOKOM", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie kan Minervens Kost in dem Gehirne dienen/", "tokens": ["Wie", "kan", "Mi\u00b7ner\u00b7vens", "Kost", "in", "dem", "Ge\u00b7hir\u00b7ne", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das steten Aberwitz/ wie Raupen-Nester nehrt?", "tokens": ["Das", "ste\u00b7ten", "A\u00b7ber\u00b7witz", "/", "wie", "Rau\u00b7pen\u00b7Nes\u00b7ter