{"textgrid.poem.40539": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Harfen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch der Seele Tiefen klingend", "tokens": ["Durch", "der", "See\u00b7le", "Tie\u00b7fen", "klin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weht in mir ein Harfenpaar,", "tokens": ["Weht", "in", "mir", "ein", "Har\u00b7fen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brausend t\u00f6nt das Spiel der einen,", "tokens": ["Brau\u00b7send", "t\u00f6nt", "das", "Spiel", "der", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "ART", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das der andern sanft und klar;", "tokens": ["Das", "der", "an\u00b7dern", "sanft", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwei der Kr\u00e4fte, die sich hassen,", "tokens": ["Zwei", "der", "Kr\u00e4f\u00b7te", ",", "die", "sich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geben ihnen Klang und Laut,", "tokens": ["Ge\u00b7ben", "ih\u00b7nen", "Klang", "und", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den Saiten wettert diese,", "tokens": ["In", "den", "Sai\u00b7ten", "wet\u00b7tert", "die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jene k\u00fc\u00dft sie leis' und traut.", "tokens": ["Je\u00b7ne", "k\u00fc\u00dft", "sie", "leis'", "und", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie von Fels auf Felsbett st\u00fcrzend", "tokens": ["Wie", "von", "Fels", "auf", "Fels\u00b7bett", "st\u00fcr\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild der Katarakt erdr\u00f6hnt,", "tokens": ["Wild", "der", "Ka\u00b7ta\u00b7rakt", "er\u00b7dr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie, wenn Donnerkeile rasen,", "tokens": ["Wie", ",", "wenn", "Don\u00b7ner\u00b7kei\u00b7le", "ra\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dumpf es durch die Bergschlucht st\u00f6hnt,", "tokens": ["Dumpf", "es", "durch", "die", "Berg\u00b7schlucht", "st\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Sturz der fessellosen", "tokens": ["Wie", "der", "Sturz", "der", "fes\u00b7sel\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schneelavin' im Thal verhallt,", "tokens": ["Schnee\u00b7la\u00b7vin'", "im", "Thal", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also auch die eine Harfe", "tokens": ["Al\u00b7so", "auch", "die", "ei\u00b7ne", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir im Busen dr\u00f6hnend schallt.", "tokens": ["Mir", "im", "Bu\u00b7sen", "dr\u00f6h\u00b7nend", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie \u00fcber Rosenhaine", "tokens": ["Doch", "wie", "\u00fc\u00b7ber", "Ro\u00b7sen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zefir haucht den Morgenku\u00df,", "tokens": ["Ze\u00b7fir", "haucht", "den", "Mor\u00b7gen\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie aus fernen, fernen Welten", "tokens": ["Wie", "aus", "fer\u00b7nen", ",", "fer\u00b7nen", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Geliebten leiser Gru\u00df,", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", "lei\u00b7ser", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie bei Nacht sich's still harmonisch", "tokens": ["Wie", "bei", "Nacht", "sich's", "still", "har\u00b7mo\u00b7nisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NE", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Cypressenwipfeln regt,", "tokens": ["In", "Cyp\u00b7res\u00b7sen\u00b7wip\u00b7feln", "regt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "T\u00f6nt der andern Harfe Lispeln,", "tokens": ["T\u00f6nt", "der", "an\u00b7dern", "Har\u00b7fe", "Lis\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zart von milder Kraft bewegt.", "tokens": ["Zart", "von", "mil\u00b7der", "Kraft", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4tte doch die beiden Kr\u00e4fte", "tokens": ["H\u00e4t\u00b7te", "doch", "die", "bei\u00b7den", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleiches Streben hold vereint!", "tokens": ["Glei\u00b7ches", "Stre\u00b7ben", "hold", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbesiegbar, unvers\u00f6hnbar", "tokens": ["Un\u00b7be\u00b7sieg\u00b7bar", ",", "un\u00b7ver\u00b7s\u00f6hn\u00b7bar"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiben sie sich ewig feind;", "tokens": ["Blei\u00b7ben", "sie", "sich", "e\u00b7wig", "feind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis die letzte Sait' in Tr\u00fcmmer,", "tokens": ["Bis", "die", "letz\u00b7te", "Sait'", "in", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jede Harf' in Staub zerbricht,", "tokens": ["Je\u00b7de", "Ha\u00b7rf'", "in", "Staub", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Dann befeinden sie sich nimmer,", "tokens": ["Dann", "be\u00b7fein\u00b7den", "sie", "sich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber, ach \u2013 sie t\u00f6nen nicht!", "tokens": ["A\u00b7ber", ",", "ach", "\u2013", "sie", "t\u00f6\u00b7nen", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "XY", "$(", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}