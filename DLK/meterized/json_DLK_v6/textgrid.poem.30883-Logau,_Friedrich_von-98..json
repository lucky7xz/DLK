{"textgrid.poem.30883": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frey von eigner Lieb und Gunst,", "tokens": ["Frey", "von", "eig\u00b7ner", "Lieb", "und", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich von aussen und von innen", "tokens": ["Sich", "von", "aus\u00b7sen", "und", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "VVINF", "KON", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kennen, ist das beste k\u00fcnnen", "tokens": ["Ken\u00b7nen", ",", "ist", "das", "bes\u00b7te", "k\u00fcn\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und passirt f\u00fcr alle Kunst.", "tokens": ["Und", "pas\u00b7sirt", "f\u00fcr", "al\u00b7le", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Andrer Leute M\u00e4ngel richten,", "tokens": ["A\u00b7ndrer", "Leu\u00b7te", "M\u00e4n\u00b7gel", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine schlichten,", "tokens": ["Sei\u00b7ne", "schlich\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Tieff zu andren sehen ein,", "tokens": ["Tieff", "zu", "an\u00b7dren", "se\u00b7hen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihme selbsten fremde seyn,", "tokens": ["Ih\u00b7me", "selbs\u00b7ten", "frem\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Taug mit nichten.", "tokens": ["Taug", "mit", "nich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Viel zu z\u00e4rtlich buhlt ihm der,", "tokens": ["Viel", "zu", "z\u00e4rt\u00b7lich", "buhlt", "ihm", "der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "VVFIN", "PPER", "ART", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Der sich in sich selbst verliebet,", "tokens": ["Der", "sich", "in", "sich", "selbst", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df er alles g\u00fcnnt und gibet", "tokens": ["Da\u00df", "er", "al\u00b7les", "g\u00fcnnt", "und", "gi\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ihm, was sonsten andrer w\u00e4r,", "tokens": ["Ihm", ",", "was", "sons\u00b7ten", "an\u00b7drer", "w\u00e4r", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWS", "VMFIN", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der ihm nichts nicht ab kan schlagen", "tokens": ["Der", "ihm", "nichts", "nicht", "ab", "kan", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIS", "PTKNEG", "PTKVZ", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Zum behagen,", "tokens": ["Zum", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Der sich, wie er sich gebildt,", "tokens": ["Der", "sich", ",", "wie", "er", "sich", "ge\u00b7bildt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wann er nicht bey andren gilt,", "tokens": ["Wann", "er", "nicht", "bey", "an\u00b7dren", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Wil beklagen.", "tokens": ["Wil", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Andrer Mann hat auch ein Haupt,", "tokens": ["A\u00b7ndrer", "Mann", "hat", "auch", "ein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Sein Gehirn und sein Gemercke;", "tokens": ["Sein", "Ge\u00b7hirn", "und", "sein", "Ge\u00b7mer\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie? wann ihm auch deine Wercke", "tokens": ["Wie", "?", "wann", "ihm", "auch", "dei\u00b7ne", "Wer\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PWAV", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Durch zu suchen w\u00e4r erlaubt?", "tokens": ["Durch", "zu", "su\u00b7chen", "w\u00e4r", "er\u00b7laubt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wer die Zung auff Hohn au\u00dfstrecket,", "tokens": ["Wer", "die", "Zung", "auff", "Hohn", "au\u00df\u00b7str\u00b7e\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.24": {"text": "Der erwecket", "tokens": ["Der", "er\u00b7we\u00b7cket"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Einen, der den Kopff hebt auff", "tokens": ["Ei\u00b7nen", ",", "der", "den", "Kopff", "hebt", "auff"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Und ihm auch f\u00fcr seinen Lauff", "tokens": ["Und", "ihm", "auch", "f\u00fcr", "sei\u00b7nen", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Lichter stecket.", "tokens": ["Lich\u00b7ter", "ste\u00b7cket", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Wem der Himmel was geschenckt,", "tokens": ["Wem", "der", "Him\u00b7mel", "was", "ge\u00b7schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PWS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Dencke nicht, er seys alleine;", "tokens": ["Den\u00b7cke", "nicht", ",", "er", "seys", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Andrem ist von solchem Scheine", "tokens": ["A\u00b7ndrem", "ist", "von", "sol\u00b7chem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Auch vielleicht was zugelenckt.", "tokens": ["Auch", "viel\u00b7leicht", "was", "zu\u00b7ge\u00b7lenckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Viel ist manchem zugezehlet;", "tokens": ["Viel", "ist", "man\u00b7chem", "zu\u00b7ge\u00b7zeh\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Viel noch fehlet,", "tokens": ["Viel", "noch", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.34": {"text": "Da\u00df er noch nicht alles hat:", "tokens": ["Da\u00df", "er", "noch", "nicht", "al\u00b7les", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Gott hat keinen ohne Rath", "tokens": ["Gott", "hat", "kei\u00b7nen", "oh\u00b7ne", "Rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "So gewehlet.", "tokens": ["So", "ge\u00b7weh\u00b7let", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}