{"dta.poem.23367": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Petitionsrecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Beten und das Bitten ist erlaubt,", "tokens": ["Das", "Be\u00b7ten", "und", "das", "Bit\u00b7ten", "ist", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja, und erlaubt ist Alles \u00fcberhaupt,", "tokens": ["Ja", ",", "und", "er\u00b7laubt", "ist", "Al\u00b7les", "\u00fc\u00b7ber\u00b7haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VVPP", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was niemals n\u00fctzt den armen Unterthanen.", "tokens": ["Was", "nie\u00b7mals", "n\u00fctzt", "den", "ar\u00b7men", "Un\u00b7ter\u00b7tha\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn wie an ein Versprechen etwa mahnen,", "tokens": ["Wenn", "wie", "an", "ein", "Ver\u00b7spre\u00b7chen", "et\u00b7wa", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gesetzlich bitten, was wir fordern k\u00f6nnen,", "tokens": ["Ge\u00b7setz\u00b7lich", "bit\u00b7ten", ",", "was", "wir", "for\u00b7dern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PRELS", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da will man uns das Bitten auch nicht g\u00f6nnen,", "tokens": ["Da", "will", "man", "uns", "das", "Bit\u00b7ten", "auch", "nicht", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Man weist uns ab mit kaltem Hohn zuletzt:", "tokens": ["Man", "weist", "uns", "ab", "mit", "kal\u00b7tem", "Hohn", "zu\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihr habt die Form verletzt.", "tokens": ["Ihr", "habt", "die", "Form", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Herr der Welten h\u00f6ret unser Flehn,", "tokens": ["Der", "Herr", "der", "Wel\u00b7ten", "h\u00f6\u00b7ret", "un\u00b7ser", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er naht und ist bereit uns beizustehn,", "tokens": ["Er", "naht", "und", "ist", "be\u00b7reit", "uns", "bei\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er fordert was wir bitten kaum noch wollten,", "tokens": ["Er", "for\u00b7dert", "was", "wir", "bit\u00b7ten", "kaum", "noch", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "ADV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erf\u00fcllt was wir nach Recht verlangen sollten.", "tokens": ["Er\u00b7f\u00fcllt", "was", "wir", "nach", "Recht", "ver\u00b7lan\u00b7gen", "soll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu jenen, die ihr heiligstes Versprechen", "tokens": ["Zu", "je\u00b7nen", ",", "die", "ihr", "hei\u00b7ligs\u00b7tes", "Ver\u00b7spre\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebrochen haben und noch heute brechen,", "tokens": ["Ge\u00b7bro\u00b7chen", "ha\u00b7ben", "und", "noch", "heu\u00b7te", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Spricht er ein allerh\u00f6chstes Wort zuletzt:", "tokens": ["Spricht", "er", "ein", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "Wort", "zu\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihr habt das Recht verletzt.", "tokens": ["Ihr", "habt", "das", "Recht", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}