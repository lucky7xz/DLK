{"textgrid.poem.39720": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist alles stumm und leer;", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist alles stumm und leer;", "tokens": ["Ist", "al\u00b7les", "stumm", "und", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nichts macht mir Freude mehr;", "tokens": ["Nichts", "macht", "mir", "Freu\u00b7de", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcfte, sie d\u00fcften nicht,", "tokens": ["D\u00fcf\u00b7te", ",", "sie", "d\u00fcf\u00b7ten", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "L\u00fcfte, sie l\u00fcften nicht;", "tokens": ["L\u00fcf\u00b7te", ",", "sie", "l\u00fcf\u00b7ten", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mein Herz so schwer!", "tokens": ["Mein", "Herz", "so", "schwer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ist alles \u00f6d' und hin;", "tokens": ["Ist", "al\u00b7les", "\u00f6d'", "und", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bange mein Herz und Sinn;", "tokens": ["Ban\u00b7ge", "mein", "Herz", "und", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "M\u00f6chte, nicht wei\u00df ich, was;", "tokens": ["M\u00f6ch\u00b7te", ",", "nicht", "wei\u00df", "ich", ",", "was", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "$,", "PTKNEG", "VVFIN", "PPER", "$,", "PWS", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Treibt mich ohn' Unterla\u00df,", "tokens": ["Treibt", "mich", "ohn'", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wei\u00df nicht, wohin!", "tokens": ["Wei\u00df", "nicht", ",", "wo\u00b7hin", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ein Bild von Meisterhand", "tokens": ["Ein", "Bild", "von", "Meis\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat mir den Sinn gebannt;", "tokens": ["Hat", "mir", "den", "Sinn", "ge\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit ich das holde sah,", "tokens": ["Seit", "ich", "das", "hol\u00b7de", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist's fern und ewig nah,", "tokens": ["Ist's", "fern", "und", "e\u00b7wig", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir anverwandt.", "tokens": ["Mir", "an\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein Klang im Herzen ruht,", "tokens": ["Ein", "Klang", "im", "Her\u00b7zen", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der noch erquickt den Muth,", "tokens": ["Der", "noch", "er\u00b7quickt", "den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Fl\u00f6tenhauch ein Wort,", "tokens": ["Wie", "Fl\u00f6\u00b7ten\u00b7hauch", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "T\u00f6net noch leise fort,", "tokens": ["T\u00f6\u00b7net", "noch", "lei\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Stillt Thr\u00e4nenfluth.", "tokens": ["Stillt", "Thr\u00e4\u00b7nen\u00b7fluth", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Fr\u00fchlinges Blumen treu", "tokens": ["Fr\u00fch\u00b7lin\u00b7ges", "Blu\u00b7men", "treu"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJD"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kommen zur\u00fcck auf's Neu;", "tokens": ["Kom\u00b7men", "zu\u00b7r\u00fcck", "auf's", "Neu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nicht so der Liebe Gl\u00fcck,", "tokens": ["Nicht", "so", "der", "Lie\u00b7be", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach, es kommt nicht zur\u00fcck \u2013", "tokens": ["Ach", ",", "es", "kommt", "nicht", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sch\u00f6n, doch nicht treu!", "tokens": ["Sch\u00f6n", ",", "doch", "nicht", "treu", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Kann Lieb' so unlieb sein,", "tokens": ["Kann", "Lieb'", "so", "un\u00b7lieb", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von mir so fern, was mein?", "tokens": ["Von", "mir", "so", "fern", ",", "was", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann Lust so schmerzlich sein,", "tokens": ["Kann", "Lust", "so", "schmerz\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Untreu so herzlich sein?", "tokens": ["Un\u00b7treu", "so", "herz\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Wonn', o Pein!", "tokens": ["O", "Wonn'", ",", "o", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ph\u00f6nix der Lieblichkeit,", "tokens": ["Ph\u00f6\u00b7nix", "der", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dich tr\u00e4gt dein Fittig weit", "tokens": ["Dich", "tr\u00e4gt", "dein", "Fit\u00b7tig", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hin zu der Sonne Strahl,", "tokens": ["Hin", "zu", "der", "Son\u00b7ne", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Ach was ist dir zumal", "tokens": ["Ach", "was", "ist", "dir", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VAFIN", "PPER", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mein einsam Leid!", "tokens": ["Mein", "ein\u00b7sam", "Leid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}