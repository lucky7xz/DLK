{"textgrid.poem.34772": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaff mich nicht ab, wenn auch den Durst", "tokens": ["Schaff", "mich", "nicht", "ab", ",", "wenn", "auch", "den", "Durst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gel\u00f6scht der holde Trunk;", "tokens": ["Ge\u00b7l\u00f6scht", "der", "hol\u00b7de", "Trunk", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Behalt mich noch ein Vierteljahr,", "tokens": ["Be\u00b7halt", "mich", "noch", "ein", "Vier\u00b7tel\u00b7jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann hab auch ich genung.", "tokens": ["Dann", "hab", "auch", "ich", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kannst du nicht mehr Geliebte sein,", "tokens": ["Kannst", "du", "nicht", "mehr", "Ge\u00b7lieb\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei Freundin mir sodann;", "tokens": ["Sei", "Freun\u00b7din", "mir", "so\u00b7dann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat man die Liebe durchgeliebt,", "tokens": ["Hat", "man", "die", "Lie\u00b7be", "durch\u00b7ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4ngt man die Freundschaft an.", "tokens": ["F\u00e4ngt", "man", "die", "Freund\u00b7schaft", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}