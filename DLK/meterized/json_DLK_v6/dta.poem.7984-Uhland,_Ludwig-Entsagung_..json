{"dta.poem.7984": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Entsagung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wer entwandelt durch den Garten", "tokens": ["Wer", "ent\u00b7wan\u00b7delt", "durch", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Sterne bleichem Schein?", "tokens": ["Bei", "der", "Ster\u00b7ne", "blei\u00b7chem", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat er S\u00fc\u00dfes zu erwarten?", "tokens": ["Hat", "er", "S\u00fc\u00b7\u00dfes", "zu", "er\u00b7war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Nacht ihm selig seyn?", "tokens": ["Wird", "die", "Nacht", "ihm", "se\u00b7lig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! der Harfner ist\u2019s, er sinkt", "tokens": ["Ach", "!", "der", "Harf\u00b7ner", "ist's", ",", "er", "sinkt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nieder an des Thurmes Fu\u00dfe,", "tokens": ["Nie\u00b7der", "an", "des", "Thur\u00b7mes", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo es sp\u00e4t herunterblinkt,", "tokens": ["Wo", "es", "sp\u00e4t", "her\u00b7un\u00b7ter\u00b7blinkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und beginnt zum Saitengru\u00dfe:", "tokens": ["Und", "be\u00b7ginnt", "zum", "Sai\u00b7ten\u00b7gru\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201elausche, Jungfrau, aus der H\u00f6he", "tokens": ["\u201e", "lau\u00b7sche", ",", "Jung\u00b7frau", ",", "aus", "der", "H\u00f6\u00b7he"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJA", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Einem Liede, dir geweiht!", "tokens": ["Ei\u00b7nem", "Lie\u00b7de", ",", "dir", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ein Traum dich lind umwehe", "tokens": ["Da\u00df", "ein", "Traum", "dich", "lind", "um\u00b7we\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Kindheit Rosenzeit.", "tokens": ["Aus", "der", "Kind\u00b7heit", "Ro\u00b7sen\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Abendglocke Klang", "tokens": ["Mit", "der", "A\u00b7bend\u00b7glo\u00b7cke", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kam ich, will vor Tage gehen,", "tokens": ["Kam", "ich", ",", "will", "vor", "Ta\u00b7ge", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das Schlo\u00df, dem ich entsprang,", "tokens": ["Und", "das", "Schlo\u00df", ",", "dem", "ich", "ent\u00b7sprang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht im Sonnenstrale sehen.", "tokens": ["Nicht", "im", "Son\u00b7nen\u00b7stra\u00b7le", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem kerzenhellen Saale,", "tokens": ["Von", "dem", "ker\u00b7zen\u00b7hel\u00b7len", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wo du throntest, blieb ich fern,", "tokens": ["Wo", "du", "thron\u00b7test", ",", "blieb", "ich", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo um dich bei\u2019m reichen Mahle", "tokens": ["Wo", "um", "dich", "bei'm", "rei\u00b7chen", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudig sa\u00dfen edle Herrn.", "tokens": ["Freu\u00b7dig", "sa\u00b7\u00dfen", "ed\u00b7le", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Freude nur vertraut,", "tokens": ["Mit", "der", "Freu\u00b7de", "nur", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4tten Frohes sie begehret,", "tokens": ["H\u00e4t\u00b7ten", "Fro\u00b7hes", "sie", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Nicht der Liebe Klagelaut,", "tokens": ["Nicht", "der", "Lie\u00b7be", "Kla\u00b7ge\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht der Kindheit Recht geehret.", "tokens": ["Nicht", "der", "Kind\u00b7heit", "Recht", "ge\u00b7eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bange D\u00e4mmerung entweiche!", "tokens": ["Ban\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "ent\u00b7wei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcstre B\u00e4ume, gl\u00e4nzet neu!", "tokens": ["D\u00fcst\u00b7re", "B\u00e4u\u00b7me", ",", "gl\u00e4n\u00b7zet", "neu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich in dem Zauberreiche", "tokens": ["Da\u00df", "ich", "in", "dem", "Zau\u00b7ber\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Meiner Kindheit selig sey.", "tokens": ["Mei\u00b7ner", "Kind\u00b7heit", "se\u00b7lig", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinken will ich in den Klee,", "tokens": ["Sin\u00b7ken", "will", "ich", "in", "den", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis das Kind mit leichtem Schritte", "tokens": ["Bis", "das", "Kind", "mit", "leich\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wandle her, die sch\u00f6ne Fee,", "tokens": ["Wand\u00b7le", "her", ",", "die", "sch\u00f6\u00b7ne", "Fee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mit Blumen mich besch\u00fctte.", "tokens": ["Und", "mit", "Blu\u00b7men", "mich", "be\u00b7sch\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja! die Zeit ist hingeflogen,", "tokens": ["Ja", "!", "die", "Zeit", "ist", "hin\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Erinnrung weichet nie;", "tokens": ["Die", "E\u00b7rinn\u00b7rung", "wei\u00b7chet", "nie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein lichter Regenbogen", "tokens": ["Als", "ein", "lich\u00b7ter", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Steht auf tr\u00fcben Wolken sie.", "tokens": ["Steht", "auf", "tr\u00fc\u00b7ben", "Wol\u00b7ken", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauen flieht mein s\u00fc\u00dfer Schmerz,", "tokens": ["Schau\u00b7en", "flieht", "mein", "s\u00fc\u00b7\u00dfer", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df nicht die Erinnrung schwinde.", "tokens": ["Da\u00df", "nicht", "die", "E\u00b7rinn\u00b7rung", "schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sage das nur, ob dein Herz", "tokens": ["Sa\u00b7ge", "das", "nur", ",", "ob", "dein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Noch der Kindheit Lust empfinde?\u201c", "tokens": ["Noch", "der", "Kind\u00b7heit", "Lust", "emp\u00b7fin\u00b7de", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und es schwieg der Sohn der Lieder,", "tokens": ["Und", "es", "schwieg", "der", "Sohn", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der am Fu\u00df des Thurmes sa\u00df;", "tokens": ["Der", "am", "Fu\u00df", "des", "Thur\u00b7mes", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom Fenster klang es nieder,", "tokens": ["Und", "vom", "Fens\u00b7ter", "klang", "es", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es gl\u00e4nzt\u2019 im dunkeln Gras.", "tokens": ["Und", "es", "gl\u00e4nzt'", "im", "dun\u00b7keln", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201enimm den Ring, und denke mein,", "tokens": ["\u201e", "nimm", "den", "Ring", ",", "und", "den\u00b7ke", "mein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denk an unsrer Kindheit Sch\u00f6ne!", "tokens": ["Denk", "an", "uns\u00b7rer", "Kind\u00b7heit", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm ihn hin! ein Edelstein", "tokens": ["Nimm", "ihn", "hin", "!", "ein", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "PTKVZ", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00e4nzt darauf und eine Thr\u00e4ne.\u201c", "tokens": ["Gl\u00e4nzt", "da\u00b7rauf", "und", "ei\u00b7ne", "Thr\u00e4\u00b7ne", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PAV", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}