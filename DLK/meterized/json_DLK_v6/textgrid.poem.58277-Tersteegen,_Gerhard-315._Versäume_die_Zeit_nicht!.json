{"textgrid.poem.58277": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "315. Vers\u00e4ume die Zeit nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gnadenzeit ist kurz, das Werk ist gro\u00df und wichtig;", "tokens": ["Die", "Gna\u00b7den\u00b7zeit", "ist", "kurz", ",", "das", "Werk", "ist", "gro\u00df", "und", "wich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seel', halte dich nicht auf mit Dingen, die doch nichtig!", "tokens": ["Seel'", ",", "hal\u00b7te", "dich", "nicht", "auf", "mit", "Din\u00b7gen", ",", "die", "doch", "nich\u00b7tig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verleugnen ist dein Werk und deinen Gott zu lieben;", "tokens": ["Ver\u00b7leug\u00b7nen", "ist", "dein", "Werk", "und", "dei\u00b7nen", "Gott", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wohl dem, der seine Zeit mit diesem Werk vertrieben!", "tokens": ["Wohl", "dem", ",", "der", "sei\u00b7ne", "Zeit", "mit", "die\u00b7sem", "Werk", "ver\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}