{"textgrid.poem.65760": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "34", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum verhie\u00dfest du so heitern Tag,", "tokens": ["Wa\u00b7rum", "ver\u00b7hie\u00b7\u00dfest", "du", "so", "hei\u00b7tern", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00dfest ohne Mantel mich verreisen,", "tokens": ["Und", "lie\u00b7\u00dfest", "oh\u00b7ne", "Man\u00b7tel", "mich", "ver\u00b7rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df unterwegs mich trifft der Wolken Schmach,", "tokens": ["Da\u00df", "un\u00b7ter\u00b7wegs", "mich", "trifft", "der", "Wol\u00b7ken", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die deine Pracht mit faulem Dunst umkreisen?", "tokens": ["Die", "dei\u00b7ne", "Pracht", "mit", "fau\u00b7lem", "Dunst", "um\u00b7krei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht ist's genug, da\u00df du den Regen mir", "tokens": ["Nicht", "ist's", "ge\u00b7nug", ",", "da\u00df", "du", "den", "Re\u00b7gen", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Wolken brechend hauchst von sturmgepeitschten Wangen;", "tokens": ["Durch", "Wol\u00b7ken", "bre\u00b7chend", "hauchst", "von", "sturm\u00b7ge\u00b7peitschten", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Denn niemand dankt wohl f\u00fcr den Balsam dir,", "tokens": ["Denn", "nie\u00b7mand", "dankt", "wohl", "f\u00fcr", "den", "Bal\u00b7sam", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Wunden heilt und nicht des Unglimpfs Bangen:", "tokens": ["Der", "Wun\u00b7den", "heilt", "und", "nicht", "des", "Un\u00b7glimpfs", "Ban\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Noch kann dein Sch\u00e4men meinen Gram zerstreun.", "tokens": ["Noch", "kann", "dein", "Sch\u00e4\u00b7men", "mei\u00b7nen", "Gram", "zer\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nur schwachen Trost gew\u00e4hrt des Sch\u00e4digers Bereu'n", "tokens": ["Nur", "schwa\u00b7chen", "Trost", "ge\u00b7w\u00e4hrt", "des", "Sch\u00e4\u00b7di\u00b7gers", "Be\u00b7reu'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dem, der des Schadens Dorn tr\u00e4gt in der Brust.", "tokens": ["Dem", ",", "der", "des", "Scha\u00b7dens", "Dorn", "tr\u00e4gt", "in", "der", "Brust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch ach! die Tr\u00e4ne, die dein Auge netzt,", "tokens": ["Doch", "ach", "!", "die", "Tr\u00e4\u00b7ne", ",", "die", "dein", "Au\u00b7ge", "netzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie reiche Perl' ist's nicht, die allen Fehl ersetzt!", "tokens": ["Wie", "rei\u00b7che", "Perl'", "ist's", "nicht", ",", "die", "al\u00b7len", "Fehl", "er\u00b7setzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "PTKNEG", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}