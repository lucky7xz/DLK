{"textgrid.poem.40166": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: O mein Geliebter, in die Kissen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O mein Geliebter, in die Kissen", "tokens": ["O", "mein", "Ge\u00b7lieb\u00b7ter", ",", "in", "die", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ausweinen will ich meine Lust!", "tokens": ["aus\u00b7wei\u00b7nen", "will", "ich", "mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "darf sagen nicht, nur wissen, wissen,", "tokens": ["darf", "sa\u00b7gen", "nicht", ",", "nur", "wis\u00b7sen", ",", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "PTKNEG", "$,", "ADV", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "was also wogt in dunkler Brust!", "tokens": ["was", "al\u00b7so", "wogt", "in", "dunk\u00b7ler", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch bin ich wachend dir enthalten,", "tokens": ["Doch", "bin", "ich", "wa\u00b7chend", "dir", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "so will ich mich im Traum dir nahn,", "tokens": ["so", "will", "ich", "mich", "im", "Traum", "dir", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mich wie die Rose dir entfalten", "tokens": ["mich", "wie", "die", "Ro\u00b7se", "dir", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und deine Sonnenglut empfahn", "tokens": ["und", "dei\u00b7ne", "Son\u00b7nen\u00b7glut", "em\u00b7pfahn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "und deine Flammenk\u00fcsse trinken,", "tokens": ["und", "dei\u00b7ne", "Flam\u00b7men\u00b7k\u00fcs\u00b7se", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dir Flammen k\u00fcssend wiederspr\u00fchn:", "tokens": ["dir", "Flam\u00b7men", "k\u00fcs\u00b7send", "wie\u00b7der\u00b7spr\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bis lodernd wir zusammensinken,", "tokens": ["bis", "lo\u00b7dernd", "wir", "zu\u00b7sam\u00b7men\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in langen Schauern stumm vergl\u00fchn.", "tokens": ["in", "lan\u00b7gen", "Schau\u00b7ern", "stumm", "ver\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oh stiller Sehnsucht bange Wonne \u2013", "tokens": ["Oh", "stil\u00b7ler", "Sehn\u00b7sucht", "ban\u00b7ge", "Won\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "o stiller Sehnsucht sel'ge Qual \u2013", "tokens": ["o", "stil\u00b7ler", "Sehn\u00b7sucht", "sel'\u00b7ge", "Qual", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "oh Traum du, meiner N\u00e4chte Sonne \u2013", "tokens": ["oh", "Traum", "du", ",", "mei\u00b7ner", "N\u00e4ch\u00b7te", "Son\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "$,", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "o mein Geliebter \u2013 \u2013 mein Gemahl!", "tokens": ["o", "mein", "Ge\u00b7lieb\u00b7ter", "\u2013", "\u2013", "mein", "Ge\u00b7mahl", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "NN", "$(", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}