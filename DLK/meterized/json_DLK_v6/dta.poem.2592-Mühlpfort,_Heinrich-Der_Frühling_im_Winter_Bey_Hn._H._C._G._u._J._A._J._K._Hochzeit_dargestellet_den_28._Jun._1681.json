{"dta.poem.2592": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling im Winter/  \n  Bey Hn. H. C. G. u. J. A. J. K. Hochzeit  \n dargestellet/ den 28. Jun. 1681", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje spielet auch im Schnee die g\u00fctige Natur/", "tokens": ["Wie", "spie\u00b7let", "auch", "im", "Schnee", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeigt sie Lilien aus den gerollten Ballen?", "tokens": ["Und", "zeigt", "sie", "Li\u00b7li\u00b7en", "aus", "den", "ge\u00b7roll\u00b7ten", "Bal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist denn Adonis Blut auff kaltes Ei\u00df gefallen/", "tokens": ["Ist", "denn", "A\u00b7do\u00b7nis", "Blut", "auff", "kal\u00b7tes", "Ei\u00df", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entsprost die ", "tokens": ["Ent\u00b7sprost", "die"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So mu\u00df der G\u00f6tter Rath was sonderbahres handeln/", "tokens": ["So", "mu\u00df", "der", "G\u00f6t\u00b7ter", "Rath", "was", "son\u00b7der\u00b7bah\u00b7res", "han\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "PWS", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in des Fr\u00fchlings Lust des Winters Frost verwandeln.", "tokens": ["Und", "in", "des", "Fr\u00fch\u00b7lings", "Lust", "des", "Win\u00b7ters", "Frost", "ver\u00b7wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt was Cupido nur nach neulich hat gethan/", "tokens": ["H\u00f6rt", "was", "Cu\u00b7pi\u00b7do", "nur", "nach", "neu\u00b7lich", "hat", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "NE", "ADV", "APPR", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Als Venus und ihr Volck der Schlittenfarth genossen/", "tokens": ["Als", "Ve\u00b7nus", "und", "ihr", "Volck", "der", "Schlit\u00b7ten\u00b7farth", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat sie den kleinen Schalck ins Cabinett geschlossen/", "tokens": ["Hat", "sie", "den", "klei\u00b7nen", "Schalck", "ins", "Ca\u00b7bi\u00b7nett", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df ihn M\u00fcssiggang nicht wo verleiten kan/", "tokens": ["Und", "da\u00df", "ihn", "M\u00fcs\u00b7sig\u00b7gang", "nicht", "wo", "ver\u00b7lei\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PTKNEG", "PWAV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Legt sie ihm Arbeit auff/ zu der er sich sol schicken", "tokens": ["Legt", "sie", "ihm", "Ar\u00b7beit", "auff", "/", "zu", "der", "er", "sich", "sol", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "APPR", "$(", "APPR", "PRELS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ein gewisses Theil von Schwanen-Federn pfl\u00fccken.", "tokens": ["Und", "ein", "ge\u00b7wis\u00b7ses", "Theil", "von", "Schwa\u00b7nen\u00b7Fe\u00b7dern", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Gast/ dem Spiel und Schertz ein s\u00fcsses Handwerck war/", "tokens": ["Der", "Gast", "/", "dem", "Spiel", "und", "Schertz", "ein", "s\u00fcs\u00b7ses", "Hand\u00b7werck", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dessen Zeit-vertreib bey sch\u00f6nem Frauenzimmer/", "tokens": ["Und", "des\u00b7sen", "Zeit\u00b7ver\u00b7treib", "bey", "sch\u00f6\u00b7nem", "Frau\u00b7en\u00b7zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ergrimmte bey sich selbst/ und sprach/ di\u00df thu ich nimmer", "tokens": ["Er\u00b7grimm\u00b7te", "bey", "sich", "selbst", "/", "und", "sprach", "/", "di\u00df", "thu", "ich", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "$(", "KON", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verschleust der Winter mir denn meine Freuden gar/", "tokens": ["Ver\u00b7schleust", "der", "Win\u00b7ter", "mir", "denn", "mei\u00b7ne", "Freu\u00b7den", "gar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Mutter lebt in Lust/ ich sol in Einsamkeiten", "tokens": ["Die", "Mut\u00b7ter", "lebt", "in", "Lust", "/", "ich", "sol", "in", "Ein\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$(", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df was sonst Nymsen thun mit meiner Hand bereiten.", "tokens": ["Di\u00df", "was", "sonst", "Nym\u00b7sen", "thun", "mit", "mei\u00b7ner", "Hand", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ADV", "NN", "VVINF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er geht drauff hin und her/ er sucht bald di\u00df bald das", "tokens": ["Er", "geht", "drauff", "hin", "und", "her", "/", "er", "sucht", "bald", "di\u00df", "bald", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "KON", "PTKVZ", "$(", "PPER", "VVFIN", "ADV", "PDS", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df da\u00df er ohngefehr zu einem Schrancken kommen/", "tokens": ["Bi\u00df", "da\u00df", "er", "ohn\u00b7ge\u00b7fehr", "zu", "ei\u00b7nem", "Schran\u00b7cken", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da er in langer Reyh viel Gl\u00e4ser wahrgenommen/", "tokens": ["Da", "er", "in", "lan\u00b7ger", "Reyh", "viel", "Gl\u00e4\u00b7ser", "wahr\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er siht den Vorath an und denckt welch einen Spa\u00df", "tokens": ["Er", "siht", "den", "Vo\u00b7rath", "an", "und", "denckt", "welch", "ei\u00b7nen", "Spa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "L\u00e4st mir der Himmel zu/ trotz aller Winter Wettern", "tokens": ["L\u00e4st", "mir", "der", "Him\u00b7mel", "zu", "/", "trotz", "al\u00b7ler", "Win\u00b7ter", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "$(", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wil als Venus selbst mich heute mehr verg\u00f6ttern.", "tokens": ["Ich", "wil", "als", "Ve\u00b7nus", "selbst", "mich", "heu\u00b7te", "mehr", "ver\u00b7g\u00f6t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "ADV", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kaum hat er ein Gef\u00e4\u00df in etwas auffgemacht/", "tokens": ["Kaum", "hat", "er", "ein", "Ge\u00b7f\u00e4\u00df", "in", "et\u00b7was", "auff\u00b7ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ausgeh\u00f6let war von reinesten Crystallen/", "tokens": ["Das", "aus\u00b7ge\u00b7h\u00f6\u00b7let", "war", "von", "rei\u00b7nes\u00b7ten", "Crys\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als so ein starck Geruch den F\u00fcrwitz angefallen/", "tokens": ["Als", "so", "ein", "starck", "Ge\u00b7ruch", "den", "F\u00fcr\u00b7witz", "an\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er als wie entz\u00fcckt/ springt/ singet/ tantzt und lacht", "tokens": ["Da\u00df", "er", "als", "wie", "ent\u00b7z\u00fcckt", "/", "springt", "/", "sin\u00b7get", "/", "tantzt", "und", "lacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "KOKOM", "VVPP", "$(", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja noch begieriger aus einer andern Flaschen/", "tokens": ["Ja", "noch", "be\u00b7gie\u00b7ri\u00b7ger", "aus", "ei\u00b7ner", "an\u00b7dern", "Fla\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denckt von dem Nectar-Tranck der Mutter was zu naschen.", "tokens": ["Denckt", "von", "dem", "Nec\u00b7ta\u00b7rTranck", "der", "Mut\u00b7ter", "was", "zu", "na\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Was Pal", "tokens": ["Was", "Pal"], "token_info": ["word", "word"], "pos": ["PWS", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Und was aus Syrien von Basam k\u00f6mmt geflossen/", "tokens": ["Und", "was", "aus", "Sy\u00b7ri\u00b7en", "von", "Ba\u00b7sam", "k\u00f6mmt", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NE", "APPR", "NE", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was sonst Egypten-Land auff Gr\u00e4ber hat gegossen", "t