{"dta.poem.1697": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es hielten mich Verdru\u00df und K\u00fcmmernis begraben/", "tokens": ["Es", "hiel\u00b7ten", "mich", "Ver\u00b7dru\u00df", "und", "K\u00fcm\u00b7mer\u00b7nis", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schien fast meiner selbst nicht Sorg und Acht zu haben/", "tokens": ["Ich", "schien", "fast", "mei\u00b7ner", "selbst", "nicht", "Sorg", "und", "Acht", "zu", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADV", "PTKNEG", "NN", "KON", "CARD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Unmutt-volles Haubt war von Gedancken schwer/", "tokens": ["Mein", "Un\u00b7mut\u00b7tvol\u00b7les", "Haubt", "war", "von", "Ge\u00b7dan\u00b7cken", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es irrten/ wei\u00df nicht wo/ die Sinnen hin und her.", "tokens": ["Es", "irr\u00b7ten", "/", "wei\u00df", "nicht", "wo", "/", "die", "Sin\u00b7nen", "hin", "und", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "PTKNEG", "PWAV", "$(", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was nicht zu \u00e4ndern steht/ und man doch bessern solte/", "tokens": ["Was", "nicht", "zu", "\u00e4n\u00b7dern", "steht", "/", "und", "man", "doch", "bes\u00b7sern", "sol\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$(", "KON", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was \u00fcber Kr\u00e4ffte steigt/ und man doch zwingen wolte/", "tokens": ["Was", "\u00fc\u00b7ber", "Kr\u00e4ff\u00b7te", "steigt", "/", "und", "man", "doch", "zwin\u00b7gen", "wol\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$(", "KON", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Betr\u00fcbte meinen Geist/ der Leib zugleich empfand", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "mei\u00b7nen", "Geist", "/", "der", "Leib", "zu\u00b7gleich", "emp\u00b7fand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie das Gem\u00fctte nicht in seiner Ordnung stand/", "tokens": ["Wie", "das", "Ge\u00b7m\u00fct\u00b7te", "nicht", "in", "sei\u00b7ner", "Ord\u00b7nung", "stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "nd f\u00fchlte manche Last/ die auff dem Hertzen steckte/", "tokens": ["nd", "f\u00fchl\u00b7te", "man\u00b7che", "Last", "/", "die", "auff", "dem", "Hert\u00b7zen", "steck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ls eine M\u00e4yen-Post vom Quei\u00dfe mich erweckte/", "tokens": ["ls", "ei\u00b7ne", "M\u00e4yen\u00b7Post", "vom", "Que\u00b7i\u00b7\u00dfe", "mich", "er\u00b7weck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "en unbedachten Schlaf/ dem ich ergeben blieb", "tokens": ["en", "un\u00b7be\u00b7dach\u00b7ten", "Schlaf", "/", "dem", "ich", "er\u00b7ge\u00b7ben", "blieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "ADJA", "NN", "$(", "PRELS", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "on so geraumer Zeit/ aus dem Gehirne trieb/", "tokens": ["on", "so", "ge\u00b7rau\u00b7mer", "Zeit", "/", "aus", "dem", "Ge\u00b7hir\u00b7ne", "trieb", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$(", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ie Feder in die Hand/ den Wunsch ins Hertze spielte/", "tokens": ["ie", "Fe\u00b7der", "in", "die", "Hand", "/", "den", "Wunsch", "ins", "Hert\u00b7ze", "spiel\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPRART", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "en nichts als alte Treu und Redligkeit erzielte:", "tokens": ["en", "nichts", "als", "al\u00b7te", "Treu", "und", "Red\u00b7lig\u00b7keit", "er\u00b7ziel\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "er izt die Blumen zehlt/ seh auch der Fr\u00fcchte Lust/", "tokens": ["er", "izt", "die", "Blu\u00b7men", "zehlt", "/", "seh", "auch", "der", "Fr\u00fcch\u00b7te", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$(", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "em Friede-Garten sey kein Winter-Sturm bewust!", "tokens": ["em", "Frie\u00b7de\u00b7Gar\u00b7ten", "sey", "kein", "Win\u00b7ter\u00b7Sturm", "be\u00b7wust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}