{"textgrid.poem.40124": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie ist erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist erwacht,", "tokens": ["Sie", "ist", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "des Winters einzige Blume.", "tokens": ["des", "Win\u00b7ters", "ein\u00b7zi\u00b7ge", "Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Tod und Nacht", "tokens": ["In", "Tod", "und", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "tr\u00e4umte die stumme", "tokens": ["tr\u00e4um\u00b7te", "die", "stum\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Botin des Fr\u00fchlings", "tokens": ["Bo\u00b7tin", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "von Licht und Leben.", "tokens": ["von", "Licht", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie sie sich ", "tokens": ["Wie", "sie", "sich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "alle die sprie\u00dfenden Spitzen,", "tokens": ["al\u00b7le", "die", "sprie\u00b7\u00dfen\u00b7den", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "zum Himmel bange", "tokens": ["zum", "Him\u00b7mel", "ban\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "bebend sich richten!", "tokens": ["be\u00b7bend", "sich", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "aber droben", "tokens": ["a\u00b7ber", "dro\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "die Sonne schl\u00e4ft.", "tokens": ["die", "Son\u00b7ne", "schl\u00e4ft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Roh durchs Land die ", "tokens": ["Roh", "durchs", "Land", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "lachen kalt der schlichten", "tokens": ["la\u00b7chen", "kalt", "der", "schlich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "furchtsam strebenden Zarten,", "tokens": ["furcht\u00b7sam", "stre\u00b7ben\u00b7den", "Zar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "heulen ein Lied von Krieg und Streit:", "tokens": ["heu\u00b7len", "ein", "Lied", "von", "Krieg", "und", "Streit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Nur die Starken, Harten", "tokens": ["Nur", "die", "Star\u00b7ken", ",", "Har\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "preiset der Reigen", "tokens": ["prei\u00b7set", "der", "Rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "der eisernen Zeit!", "tokens": ["der", "ei\u00b7ser\u00b7nen", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Duftlos neigen sich", "tokens": ["Duft\u00b7los", "nei\u00b7gen", "sich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "die wei\u00dfen reinen", "tokens": ["die", "wei\u00b7\u00dfen", "rei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "scheuen K\u00f6pfchen", "tokens": ["scheu\u00b7en", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "zur Erde wieder", "tokens": ["zur", "Er\u00b7de", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "entsagend nieder", "tokens": ["ent\u00b7sa\u00b7gend", "nie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "und weinen", "tokens": ["und", "wei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "selber ins Grab sich.", "tokens": ["sel\u00b7ber", "ins", "Grab", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PRF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch nicht minder,", "tokens": ["Doch", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "du einsame Blume,", "tokens": ["du", "ein\u00b7sa\u00b7me", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "tr\u00f6stet dein Bl\u00fchen", "tokens": ["tr\u00f6s\u00b7tet", "dein", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Menschenkinder.", "tokens": ["die", "Men\u00b7schen\u00b7kin\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nicht ist vergebens", "tokens": ["Nicht", "ist", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ADV"], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "dein kurzes M\u00fchen:", "tokens": ["dein", "kur\u00b7zes", "M\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "alles des Lebens", "tokens": ["al\u00b7les", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Brausen und Gl\u00fchen,", "tokens": ["Brau\u00b7sen", "und", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "das uns der Fr\u00fchling schickt,", "tokens": ["das", "uns", "der", "Fr\u00fch\u00b7ling", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "du f\u00fchlst es nahn!", "tokens": ["du", "f\u00fchlst", "es", "nahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Mit neuem Glauben blickt", "tokens": ["Mit", "neu\u00b7em", "Glau\u00b7ben", "blickt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "auf seine Bahn,", "tokens": ["auf", "sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "winkt ihm Dein Gru\u00df,", "tokens": ["winkt", "ihm", "Dein", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "rastlos wandernd der Mensch.", "tokens": ["rast\u00b7los", "wan\u00b7dernd", "der", "Mensch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Keimt doch zitternd in Ihm auch", "tokens": ["Keimt", "doch", "zit\u00b7ternd", "in", "Ihm", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "manche lautere Blume", "tokens": ["man\u00b7che", "lau\u00b7te\u00b7re", "Blu\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "aus dem dunklen Grunde des Herzens,", "tokens": ["aus", "dem", "dunk\u00b7len", "Grun\u00b7de", "des", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die verbl\u00fchen mu\u00df,", "tokens": ["die", "ver\u00b7bl\u00fc\u00b7hen", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "ehe die andern", "tokens": ["e\u00b7he", "die", "an\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "sicher strebenden,", "tokens": ["si\u00b7cher", "stre\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "m\u00e4chtiger treibenden", "tokens": ["m\u00e4ch\u00b7ti\u00b7ger", "trei\u00b7ben\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Wurzeln sich regen:", "tokens": ["Wur\u00b7zeln", "sich", "re\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}