{"dta.poem.10507": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Gaben mir Rath und gute Lehren,", "tokens": ["Ga\u00b7ben", "mir", "Rath", "und", "gu\u00b7te", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Uebersch\u00fctteten mich mit Ehren,", "tokens": ["Ue\u00b7ber\u00b7sch\u00fct\u00b7te\u00b7ten", "mich", "mit", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sagten, da\u00df ich nur warten sollt',", "tokens": ["Sag\u00b7ten", ",", "da\u00df", "ich", "nur", "war\u00b7ten", "sollt'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Haben mich protegiren gewollt.", "tokens": ["Ha\u00b7ben", "mich", "pro\u00b7te\u00b7gi\u00b7ren", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "VMPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Aber bei all ihrem Protegiren,", "tokens": ["A\u00b7ber", "bei", "all", "ih\u00b7rem", "Pro\u00b7te\u00b7gi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4tte ich k\u00f6nnen vor Hunger krepiren,", "tokens": ["H\u00e4t\u00b7te", "ich", "k\u00f6n\u00b7nen", "vor", "Hun\u00b7ger", "kre\u00b7pi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "W\u00e4r' nicht gekommen ein braver Mann,", "tokens": ["W\u00e4r'", "nicht", "ge\u00b7kom\u00b7men", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wacker nahm er sich meiner an.", "tokens": ["Wa\u00b7cker", "nahm", "er", "sich", "mei\u00b7ner", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PPOSAT", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Braver Mann! Er schafft mir zu essen!", "tokens": ["Bra\u00b7ver", "Mann", "!", "Er", "schafft", "mir", "zu", "es\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will es ihm nie und nimmer vergessen!", "tokens": ["Will", "es", "ihm", "nie", "und", "nim\u00b7mer", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schade, da\u00df ich ihn nicht k\u00fcssen kann!", "tokens": ["Scha\u00b7de", ",", "da\u00df", "ich", "ihn", "nicht", "k\u00fcs\u00b7sen", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Denn ich bin selbst dieser brave Mann.", "tokens": ["Denn", "ich", "bin", "selbst", "die\u00b7ser", "bra\u00b7ve", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}