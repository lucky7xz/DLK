{"dta.poem.3981": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XlI.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Verachte nicht mein schlecht Papier,", "tokens": ["Ver\u00b7ach\u00b7te", "nicht", "mein", "schlecht", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fromme Freundschafft schreibet dir.", "tokens": ["Die", "from\u00b7me", "Freund\u00b7schafft", "schrei\u00b7bet", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er\u00f6ffne es mit treuen H\u00e4nden,", "tokens": ["Er\u00b7\u00f6ff\u00b7ne", "es", "mit", "treu\u00b7en", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht kan nichts bessers senden.", "tokens": ["Die", "Sehn\u00b7sucht", "kan", "nichts", "bes\u00b7sers", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}