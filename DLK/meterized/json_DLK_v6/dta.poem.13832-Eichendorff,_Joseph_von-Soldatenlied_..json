{"dta.poem.13832": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das sind meine lieben Reiter,", "tokens": ["Das", "sind", "mei\u00b7ne", "lie\u00b7ben", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die rufen hinaus zur Schlacht,", "tokens": ["Die", "ru\u00b7fen", "hin\u00b7aus", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das sind meine lustigen Reiter,", "tokens": ["Das", "sind", "mei\u00b7ne", "lus\u00b7ti\u00b7gen", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun, Liebchen, gute Nacht!", "tokens": ["Nun", ",", "Lieb\u00b7chen", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie wird es da vorne so heiter,", "tokens": ["Wie", "wird", "es", "da", "vor\u00b7ne", "so", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie spr\u00fchet der Morgenwind,", "tokens": ["Wie", "spr\u00fc\u00b7het", "der", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "In den Sieg, in den Tod und weiter,", "tokens": ["In", "den", "Sieg", ",", "in", "den", "Tod", "und", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "KON", "ADV", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Bis da\u00df wir im Himmel sind!", "tokens": ["Bis", "da\u00df", "wir", "im", "Him\u00b7mel", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}