{"textgrid.poem.61983": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Was ist das Beste?", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Liedlein t\u00f6nt von Ferne:", "tokens": ["Ein", "Lied\u00b7lein", "t\u00f6nt", "von", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ist das Liebste Dir? \u2013", "tokens": ["Was", "ist", "das", "Liebs\u00b7te", "Dir", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen und die Sterne,", "tokens": ["Die", "Au\u00b7gen", "und", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sind das Liebste mir.", "tokens": ["Sie", "sind", "das", "Liebs\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Liedlein t\u00f6nt von Ferne:", "tokens": ["Ein", "Lied\u00b7lein", "t\u00f6nt", "von", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ist das Sch\u00f6nste hier?", "tokens": ["Was", "ist", "das", "Sch\u00f6ns\u00b7te", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Sch\u00f6nste und das Liebste,", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "und", "das", "Liebs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist der Himmel mir! \u2013", "tokens": ["Das", "ist", "der", "Him\u00b7mel", "mir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sprich, was ist das Gr\u00f6\u00dfte", "tokens": ["Und", "sprich", ",", "was", "ist", "das", "Gr\u00f6\u00df\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Allertreu'ste Dir? \u2013", "tokens": ["Und", "Al\u00b7ler\u00b7treu'\u00b7ste", "Dir", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Gr\u00f6\u00dfte ist der Glaube,", "tokens": ["Das", "Gr\u00f6\u00df\u00b7te", "ist", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Treu'ste Tugend mir.", "tokens": ["Das", "Treu'\u00b7ste", "Tu\u00b7gend", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und weiter t\u00f6nt es ferne:", "tokens": ["Und", "wei\u00b7ter", "t\u00f6nt", "es", "fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ist das Beste hier?", "tokens": ["Was", "ist", "das", "Bes\u00b7te", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Beste ist die Mutter,", "tokens": ["Das", "Bes\u00b7te", "ist", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Allerliebste mir!", "tokens": ["Das", "Al\u00b7ler\u00b7liebs\u00b7te", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}