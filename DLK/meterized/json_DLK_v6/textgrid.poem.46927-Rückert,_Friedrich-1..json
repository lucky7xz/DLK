{"textgrid.poem.46927": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gipfel von dem Helikon ist hoch", "tokens": ["Der", "Gip\u00b7fel", "von", "dem", "He\u00b7li\u00b7kon", "ist", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhaben \u00fcber dem Gebiet der Gr\u00fcfte;", "tokens": ["Er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "dem", "Ge\u00b7biet", "der", "Gr\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wie sein Haupt frei trinket Himmelsl\u00fcfte,", "tokens": ["Doch", ",", "wie", "sein", "Haupt", "frei", "trin\u00b7ket", "Him\u00b7mel\u00b7sl\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPOSAT", "NN", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit F\u00fc\u00dfen steht er auf der Erde doch.", "tokens": ["Mit", "F\u00fc\u00b7\u00dfen", "steht", "er", "auf", "der", "Er\u00b7de", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wollte mich entziehn der Erde Joch,", "tokens": ["Ich", "woll\u00b7te", "mich", "ent\u00b7ziehn", "der", "Er\u00b7de", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich bergend in die h\u00f6chste seiner Kl\u00fcfte;", "tokens": ["Mich", "ber\u00b7gend", "in", "die", "h\u00f6chs\u00b7te", "sei\u00b7ner", "Kl\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Doch als die Erde sch\u00fctterte, da pr\u00fcfte", "tokens": ["Doch", "als", "die", "Er\u00b7de", "sch\u00fct\u00b7ter\u00b7te", ",", "da", "pr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich auch den Sto\u00df dort, wo ich mich verkroch.", "tokens": ["Ich", "auch", "den", "Sto\u00df", "dort", ",", "wo", "ich", "mich", "ver\u00b7kroch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Drum will ich l\u00e4nger nicht gleich einem Diebe", "tokens": ["Drum", "will", "ich", "l\u00e4n\u00b7ger", "nicht", "gleich", "ei\u00b7nem", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verborgen hier (schon that ich's allzulang)", "tokens": ["Ver\u00b7bor\u00b7gen", "hier", "(", "schon", "that", "ich's", "all\u00b7zu\u00b7lang", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "ADV", "VVFIN", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Umnebeln mich mit geisterhafter Liebe.", "tokens": ["Um\u00b7ne\u00b7beln", "mich", "mit", "geis\u00b7ter\u00b7haf\u00b7ter", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ich will hinunter in des Lebens Drang,", "tokens": ["Ich", "will", "hin\u00b7un\u00b7ter", "in", "des", "Le\u00b7bens", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Eingreifen in das irdische Getriebe,", "tokens": ["Ein\u00b7grei\u00b7fen", "in", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wo nicht durch Thaten, doch durch irdischern Gesang.", "tokens": ["Wo", "nicht", "durch", "Tha\u00b7ten", ",", "doch", "durch", "ir\u00b7di\u00b7schern", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "APPR", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}