{"textgrid.poem.39276": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[wohl mag das Lied auch schmelzen hold und sinnig]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl mag das Lied auch schmelzen hold und sinnig,", "tokens": ["Wohl", "mag", "das", "Lied", "auch", "schmel\u00b7zen", "hold", "und", "sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn's gilt, der Frauen Lockenhaupt zu zieren,", "tokens": ["Wenn's", "gilt", ",", "der", "Frau\u00b7en", "Lo\u00b7cken\u00b7haupt", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nimmer soll's die heil'ge Kraft verlieren,", "tokens": ["Doch", "nim\u00b7mer", "soll's", "die", "heil'\u00b7ge", "Kraft", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ihm verliehen, stolz zugleich und minnig.", "tokens": ["Die", "ihm", "ver\u00b7lie\u00b7hen", ",", "stolz", "zu\u00b7gleich", "und", "min\u00b7nig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und was du minnst im Busen herzensinnig,", "tokens": ["Und", "was", "du", "minnst", "im", "Bu\u00b7sen", "her\u00b7zen\u00b7sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das magst du auch zum Stoff der Lieder k\u00fcren,", "tokens": ["Das", "magst", "du", "auch", "zum", "Stoff", "der", "Lie\u00b7der", "k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn was in Liebe mir das Herz mag r\u00fchren,", "tokens": ["Denn", "was", "in", "Lie\u00b7be", "mir", "das", "Herz", "mag", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem treu mit Schwert und Lied ergeben bin ich.", "tokens": ["Dem", "treu", "mit", "Schwert", "und", "Lied", "er\u00b7ge\u00b7ben", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Doch eher mag ihr Feuer mich verzehren,", "tokens": ["Doch", "e\u00b7her", "mag", "ihr", "Feu\u00b7er", "mich", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh' in der Minne Diensten ich zum Knechte", "tokens": ["Eh'", "in", "der", "Min\u00b7ne", "Diens\u00b7ten", "ich", "zum", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich wandle um, in Liedern und in Taten;", "tokens": ["Mich", "wand\u00b7le", "um", ",", "in", "Lie\u00b7dern", "und", "in", "Ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des k\u00fchnen Herzens mut'gem Drange wehren,", "tokens": ["Des", "k\u00fch\u00b7nen", "Her\u00b7zens", "mut'\u00b7gem", "Dran\u00b7ge", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das hei\u00dft zernichten seine edlen Rechte,", "tokens": ["Das", "hei\u00dft", "zer\u00b7nich\u00b7ten", "sei\u00b7ne", "ed\u00b7len", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hei\u00dft die Mannheit an ein Weib verraten.", "tokens": ["Das", "hei\u00dft", "die", "Mann\u00b7heit", "an", "ein", "Weib", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}