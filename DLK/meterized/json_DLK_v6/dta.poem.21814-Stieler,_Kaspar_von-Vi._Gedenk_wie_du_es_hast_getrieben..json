{"dta.poem.21814": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Gedenk wie du es hast getrieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich als du h\u00e4ttest still gesessen/", "tokens": ["Gleich", "als", "du", "h\u00e4t\u00b7test", "still", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als dir annoch das junge Bluht", "tokens": ["als", "dir", "an\u00b7noch", "das", "jun\u00b7ge", "Bluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in deinem frischem Herzen wallte:", "tokens": ["in", "dei\u00b7nem", "fri\u00b7schem", "Her\u00b7zen", "wall\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seht/ Kinder/ wie der Alte/ Kalte", "tokens": ["Seht", "/", "Kin\u00b7der", "/", "wie", "der", "Al\u00b7te", "/", "Kal\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$(", "NN", "$(", "KOKOM", "ART", "NN", "$(", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "die Heiligkeit nu hat gefressen", "tokens": ["die", "Hei\u00b7lig\u00b7keit", "nu", "hat", "ge\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie kunnt\u2019 er doch in seiner Jugend", "tokens": ["Wie", "kunnt'", "er", "doch", "in", "sei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den jungen M\u00e4gdchen schleichen nach", "tokens": ["den", "jun\u00b7gen", "M\u00e4gd\u00b7chen", "schlei\u00b7chen", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie wust\u2019 er sie so sch\u00f6n zu gr\u00fcssen", "tokens": ["wie", "wust'", "er", "sie", "so", "sch\u00f6n", "zu", "gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wie hielt\u2019 er gern mit ihnen Sprach\u2019", "tokens": ["wie", "hielt'", "er", "gern", "mit", "ih\u00b7nen", "Sprach'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und kunnte weidlich sie zerk\u00fcssen!", "tokens": ["und", "kunn\u00b7te", "weid\u00b7lich", "sie", "zer\u00b7k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "iezt ist er keusch und lehret Tugend.", "tokens": ["iezt", "ist", "er", "keusch", "und", "leh\u00b7ret", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r/ Alter/ denk auff deine Zeiten/", "tokens": ["H\u00f6r", "/", "Al\u00b7ter", "/", "denk", "auff", "dei\u00b7ne", "Zei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und denk/ da\u00df ich in diesen bin.", "tokens": ["und", "denk", "/", "da\u00df", "ich", "in", "die\u00b7sen", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "APPR", "PDS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werde mich auch ernstlich halten/", "tokens": ["Ich", "wer\u00b7de", "mich", "auch", "ernst\u00b7lich", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wenn einst mein runzel-striemig Kinn", "tokens": ["wenn", "einst", "mein", "run\u00b7zel\u00b7strie\u00b7mig", "Kinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "in grauen Borsten wird veralten:", "tokens": ["in", "grau\u00b7en", "Bors\u00b7ten", "wird", "ver\u00b7al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "denn wil ich auch auff Erbar streiten.", "tokens": ["denn", "wil", "ich", "auch", "auff", "Er\u00b7bar", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Wer wei\u00df/ was unter deinen Haaren/", "tokens": ["Wer", "wei\u00df", "/", "was", "un\u00b7ter", "dei\u00b7nen", "Haa\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dem alten Schnee/ verborgen ist?", "tokens": ["dem", "al\u00b7ten", "Schnee", "/", "ver\u00b7bor\u00b7gen", "ist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Alte sein auch offters Gekken/", "tokens": ["die", "Al\u00b7te", "sein", "auch", "off\u00b7ters", "Gek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "doch wissen sie mit Wizz und Lust", "tokens": ["doch", "wis\u00b7sen", "sie", "mit", "Wizz", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Narren-Kappe zu verstekken.", "tokens": ["die", "Nar\u00b7ren\u00b7Kap\u00b7pe", "zu", "vers\u00b7tek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man hat der Tohrheit viel erfahren.", "tokens": ["Man", "hat", "der", "Tohr\u00b7heit", "viel", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir Jungen k\u00f6nnen nicht verschweigen/", "tokens": ["Wir", "Jun\u00b7gen", "k\u00f6n\u00b7nen", "nicht", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wenn uns ein Gl\u00fckk willkommen heist.", "tokens": ["wenn", "uns", "ein", "Gl\u00fckk", "will\u00b7kom\u00b7men", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Straks m\u00fcssens alle Leute wissen/", "tokens": ["Straks", "m\u00fcs\u00b7sens", "al\u00b7le", "Leu\u00b7te", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "denn wird es an uns mi\u00dfgepreist.", "tokens": ["denn", "wird", "es", "an", "uns", "mi\u00df\u00b7ge\u00b7preist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir folgen Alten auff den F\u00fcssen/", "tokens": ["Wir", "fol\u00b7gen", "Al\u00b7ten", "auff", "den", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und man wil uns des Lasters zeugen.", "tokens": ["und", "man", "wil", "uns", "des", "Las\u00b7ters", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}