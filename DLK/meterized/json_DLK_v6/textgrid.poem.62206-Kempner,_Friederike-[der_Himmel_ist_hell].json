{"textgrid.poem.62206": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[der Himmel ist hell]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist hell,", "tokens": ["Der", "Him\u00b7mel", "ist", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Das Feld, es ist wei\u00df,", "tokens": ["Das", "Feld", ",", "es", "ist", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es leuchten so kalt in der Ferne", "tokens": ["Es", "leuch\u00b7ten", "so", "kalt", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Unz\u00e4hlige silberne Sterne.", "tokens": ["Un\u00b7z\u00e4h\u00b7li\u00b7ge", "sil\u00b7ber\u00b7ne", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Nacht ist lang,", "tokens": ["Die", "Nacht", "ist", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Traum ist bang,", "tokens": ["Der", "Traum", "ist", "bang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Viel Geister, sie fehlen hienieden,", "tokens": ["Viel", "Geis\u00b7ter", ",", "sie", "feh\u00b7len", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geb' Gott den Fehlenden Frieden!", "tokens": ["Geb'", "Gott", "den", "Feh\u00b7len\u00b7den", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}