{"textgrid.poem.36274": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fchende", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lockend schwillt der Mund der Rosen,", "tokens": ["Lo\u00b7ckend", "schwillt", "der", "Mund", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6ffnet dir des Duftes F\u00fclle;", "tokens": ["\u00d6ff\u00b7net", "dir", "des", "Duf\u00b7tes", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du s\u00fc\u00df begeistert kosen,", "tokens": ["Willst", "du", "s\u00fc\u00df", "be\u00b7geis\u00b7tert", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sticht ein Dorn aus Blumenh\u00fclle,", "tokens": ["Sticht", "ein", "Dorn", "aus", "Blu\u00b7men\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spottet noch, wenn Schmerzen tosen.", "tokens": ["Spot\u00b7tet", "noch", ",", "wenn", "Schmer\u00b7zen", "to\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00fcrft' ich sagen, wie ich liebe,", "tokens": ["D\u00fcrft'", "ich", "sa\u00b7gen", ",", "wie", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ungest\u00f6rt im leisen Bunde,", "tokens": ["Un\u00b7ge\u00b7st\u00f6rt", "im", "lei\u00b7sen", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus dich atmen, Glut der Triebe,", "tokens": ["Aus", "dich", "at\u00b7men", ",", "Glut", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Klagt' ich nicht mehr: Rosamunde,", "tokens": ["Klagt'", "ich", "nicht", "mehr", ":", "Ro\u00b7sa\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wilde Scherze scherzt die Liebe.", "tokens": ["Wil\u00b7de", "Scher\u00b7ze", "scherzt", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hei\u00dfer f\u00fchl' ich stets die Wunden,", "tokens": ["Hei\u00b7\u00dfer", "f\u00fchl'", "ich", "stets", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00f6he gern den Blitz der Augen,", "tokens": ["Fl\u00f6\u00b7he", "gern", "den", "Blitz", "der", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann zerrissen nie gesunden,", "tokens": ["Kann", "zer\u00b7ris\u00b7sen", "nie", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis ich Mund an Mund darf saugen", "tokens": ["Bis", "ich", "Mund", "an", "Mund", "darf", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00dfen Tod von Rosamunden.", "tokens": ["S\u00fc\u00b7\u00dfen", "Tod", "von", "Ro\u00b7sa\u00b7mun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sie ferne mich vertriebe,", "tokens": ["Wenn", "sie", "fer\u00b7ne", "mich", "ver\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seufzt' ich ferne nach der Frucht,", "tokens": ["Seufzt'", "ich", "fer\u00b7ne", "nach", "der", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die mir ewig Blume bliebe.", "tokens": ["Die", "mir", "e\u00b7wig", "Blu\u00b7me", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Fliehend hindert mich die Flucht,", "tokens": ["Flie\u00b7hend", "hin\u00b7dert", "mich", "die", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wild und wilder scherzt die Liebe.", "tokens": ["Wild", "und", "wil\u00b7der", "scherzt", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Anmut will den Mut mir rauben,", "tokens": ["An\u00b7mut", "will", "den", "Mut", "mir", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will in Unmut mich versenken,", "tokens": ["Will", "in", "Un\u00b7mut", "mich", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Wehmut nichts erlauben!", "tokens": ["Mei\u00b7ner", "Weh\u00b7mut", "nichts", "er\u00b7lau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neuen Traum will Treu' erdenken,", "tokens": ["Neu\u00b7en", "Traum", "will", "Treu'", "er\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Reue scheut zu glauben.", "tokens": ["A\u00b7ber", "Reu\u00b7e", "scheut", "zu", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn mein Herz, von Leid nun tr\u00fcbe,", "tokens": ["Wenn", "mein", "Herz", ",", "von", "Leid", "nun", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr entsagt' und nichts mehr sagte,", "tokens": ["Ihr", "ent\u00b7sagt'", "und", "nichts", "mehr", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im Mi\u00dfverst\u00e4ndnis bliebe,", "tokens": ["Und", "im", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7nis", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00fcrdet ihr ger\u00fchrt euch fragen:", "tokens": ["W\u00fcr\u00b7det", "ihr", "ge\u00b7r\u00fchrt", "euch", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Scherzt so wilden Scherz die Liebe?", "tokens": ["Scherzt", "so", "wil\u00b7den", "Scherz", "die", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}