{"textgrid.poem.54528": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kind! nicht gebohren Kind/ doch schon der Welt Verlangen!", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind! nicht gebohren Kind/ doch schon der Welt Verlangen!", "tokens": ["Kind", "!", "nicht", "ge\u00b7boh\u00b7ren", "Kind", "/", "doch", "schon", "der", "Welt", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKNEG", "VVPP", "NN", "$(", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kind das noch eh als Mensch der Menschen Tr\u00f6ster heist/", "tokens": ["Kind", "das", "noch", "eh", "als", "Mensch", "der", "Men\u00b7schen", "Tr\u00f6s\u00b7ter", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "KOUS", "KOUS", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das dieses Kind besucht/ das voll von Flamm und Geist/", "tokens": ["Das", "die\u00b7ses", "Kind", "be\u00b7sucht", "/", "das", "voll", "von", "Flamm", "und", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "VVPP", "$(", "ART", "ADJD", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich in der Mutter Leib mit H\u00fcpffen hat empfangen!", "tokens": ["Dich", "in", "der", "Mut\u00b7ter", "Leib", "mit", "H\u00fcpf\u00b7fen", "hat", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schau Kind/ schau Freuden-Kind/ wie mit bethr\u00e4nten Wangen", "tokens": ["Schau", "Kind", "/", "schau", "Freu\u00b7den\u00b7Kind", "/", "wie", "mit", "be\u00b7thr\u00e4n\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$(", "ADJD", "NN", "$(", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich in der Wehmuth schwimm/ in Wehmuth die mich beist/", "tokens": ["Ich", "in", "der", "Weh\u00b7muth", "schwimm", "/", "in", "Weh\u00b7muth", "die", "mich", "beist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$(", "APPR", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In Wehmuth/ die mein Hertz umschlossen h\u00e4lt/ und reist.", "tokens": ["In", "Weh\u00b7muth", "/", "die", "mein", "Hertz", "um\u00b7schlos\u00b7sen", "h\u00e4lt", "/", "und", "reist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "PPOSAT", "NN", "VVINF", "VVFIN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ich offt w\u00fcnsch' ich w\u00e4r in Mutterleib vergangen.", "tokens": ["Da\u00df", "ich", "offt", "w\u00fcn\u00b7sch'", "ich", "w\u00e4r", "in", "Mut\u00b7ter\u00b7leib", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "O komm doch s\u00fcsses Kind/ du kanst mich fr\u00f6lich machen!", "tokens": ["O", "komm", "doch", "s\u00fcs\u00b7ses", "Kind", "/", "du", "kanst", "mich", "fr\u00f6\u00b7lich", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN", "$(", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Komm/ komm/ mein Geist wird dir mit Lust entgegen gehn/", "tokens": ["Komm", "/", "komm", "/", "mein", "Geist", "wird", "dir", "mit", "Lust", "ent\u00b7ge\u00b7gen", "gehn", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ob er liegt/ durch dich in lauter Wonn auffstehn!", "tokens": ["Und", "ob", "er", "liegt", "/", "durch", "dich", "in", "lau\u00b7ter", "Wonn", "auffs\u00b7tehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "APPR", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gleich wie Elisabeth f\u00fchlt ihre Frucht auffwachen/", "tokens": ["Gleich", "wie", "E\u00b7lis\u00b7a\u00b7beth", "f\u00fchlt", "ih\u00b7re", "Frucht", "auff\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wen du besuchst/ f\u00fchlt/ ob er schon nicht sieht/", "tokens": ["Wen", "du", "be\u00b7suchst", "/", "f\u00fchlt", "/", "ob", "er", "schon", "nicht", "sieht", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "VVFIN", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dich Lebens-Krafft/ durch die das Leben bl\u00fcht.", "tokens": ["Dich", "Le\u00b7bens\u00b7Krafft", "/", "durch", "die", "das", "Le\u00b7ben", "bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}