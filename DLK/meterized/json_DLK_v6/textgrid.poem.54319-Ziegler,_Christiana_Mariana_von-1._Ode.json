{"textgrid.poem.54319": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verh\u00e4ngni\u00df! warum l\u00e4\u00dft dein Schlu\u00df", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", "!", "wa\u00b7rum", "l\u00e4\u00dft", "dein", "Schlu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nicht noch weiterhin verschieben?", "tokens": ["Sich", "nicht", "noch", "wei\u00b7ter\u00b7hin", "ver\u00b7schie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach! da\u00df die F\u00fcrstin sterben mu\u00df,", "tokens": ["Ach", "!", "da\u00df", "die", "F\u00fcrs\u00b7tin", "ster\u00b7ben", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wir so treu, so z\u00e4rtlich lieben!", "tokens": ["Die", "wir", "so", "treu", ",", "so", "z\u00e4rt\u00b7lich", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer Ihren Ruhm nur h\u00f6rt, der betet Sie schon an,", "tokens": ["Wer", "Ih\u00b7ren", "Ruhm", "nur", "h\u00f6rt", ",", "der", "be\u00b7tet", "Sie", "schon", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wer Ihr dient, der preist sein Gl\u00fccke.", "tokens": ["Und", "wer", "Ihr", "dient", ",", "der", "preist", "sein", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O! da\u00df doch unser Wunsch dich nicht bezwingen kann,", "tokens": ["O", "!", "da\u00df", "doch", "un\u00b7ser", "Wunsch", "dich", "nicht", "be\u00b7zwin\u00b7gen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dich, unerbittliches Geschicke!", "tokens": ["Dich", ",", "un\u00b7er\u00b7bitt\u00b7li\u00b7ches", "Ge\u00b7schi\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umsonst st\u00e4rkt man hier die Natur,", "tokens": ["Um\u00b7sonst", "st\u00e4rkt", "man", "hier", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie soll durch deine Macht erliegen.", "tokens": ["Sie", "soll", "durch", "dei\u00b7ne", "Macht", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00f6nig seufzt. O! schenk Ihm nur", "tokens": ["Der", "K\u00f6\u00b7nig", "seufzt", ".", "O", "!", "schenk", "Ihm", "nur"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Carolinen sein Vergn\u00fcgen.", "tokens": ["In", "Ca\u00b7ro\u00b7li\u00b7nen", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der selbsten Gnad erweist, verlangt sie nun von dir;", "tokens": ["Der", "selbs\u00b7ten", "Gnad", "er\u00b7weist", ",", "ver\u00b7langt", "sie", "nun", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Majestet weicht hier der Liebe;", "tokens": ["Die", "Ma\u00b7jes\u00b7tet", "weicht", "hier", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Er zieht die Z\u00e4rtlichkeit der strengen Hoheit f\u00fcr,", "tokens": ["Er", "zieht", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "der", "stren\u00b7gen", "Ho\u00b7heit", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und folget ihrem edlen Triebe.", "tokens": ["Und", "fol\u00b7get", "ih\u00b7rem", "ed\u00b7len", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Kummer qu\u00e4lt die matte Brust", "tokens": ["Der", "Kum\u00b7mer", "qu\u00e4lt", "die", "mat\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6chstbest\u00fcrzten F\u00fcrstenkinder;", "tokens": ["Der", "h\u00f6chst\u00b7be\u00b7st\u00fcrz\u00b7ten", "F\u00fcrs\u00b7ten\u00b7kin\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sehn den schrecklichen Verlust;", "tokens": ["Sie", "sehn", "den", "schreck\u00b7li\u00b7chen", "Ver\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Schmerz dabey ist nicht gelinder:", "tokens": ["Ihr", "Schmerz", "da\u00b7bey", "ist", "nicht", "ge\u00b7lin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz vergi\u00dft den Stand, und denkt nur an sein Gl\u00fcck", "tokens": ["Das", "Herz", "ver\u00b7gi\u00dft", "den", "Stand", ",", "und", "denkt", "nur", "an", "sein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An Ihre m\u00fctterlichen Lehren;", "tokens": ["An", "Ih\u00b7re", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es ruft Sie von der Bahn der Sterblichkeit zur\u00fcck,", "tokens": ["Es", "ruft", "Sie", "von", "der", "Bahn", "der", "Sterb\u00b7lich\u00b7keit", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie hier noch l\u00e4nger zu verehren.", "tokens": ["Sie", "hier", "noch", "l\u00e4n\u00b7ger", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn dieses nicht genug bewegt,", "tokens": ["Wenn", "die\u00b7ses", "nicht", "ge\u00b7nug", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sieh, wie manches Land hier weinet;", "tokens": ["So", "sieh", ",", "wie", "man\u00b7ches", "Land", "hier", "wei\u00b7net", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PWAV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie tief die Zeitung niederschl\u00e4gt,", "tokens": ["Wie", "tief", "die", "Zei\u00b7tung", "nie\u00b7der\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie traurig jedermann erscheinet;", "tokens": ["Wie", "trau\u00b7rig", "je\u00b7der\u00b7mann", "er\u00b7schei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er fraget \u00e4ngstlich nach: Lebt unsre F\u00fcrstin noch?", "tokens": ["Er", "fra\u00b7get", "\u00e4ngst\u00b7lich", "nach", ":", "Lebt", "uns\u00b7re", "F\u00fcrs\u00b7tin", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann f\u00e4ngt er kr\u00e4ftig an zubeten:", "tokens": ["Dann", "f\u00e4ngt", "er", "kr\u00e4f\u00b7tig", "an", "zu\u00b7be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Entrei\u00df Sie der Gefahr, la\u00df mich viel lieber doch", "tokens": ["Ent\u00b7rei\u00df", "Sie", "der", "Ge\u00b7fahr", ",", "la\u00df", "mich", "viel", "lie\u00b7ber", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fcr Sie des Todes Thal betreten!", "tokens": ["F\u00fcr", "Sie", "des", "To\u00b7des", "Thal", "be\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was \u00f6ffnet sich? wen seh ich dort?", "tokens": ["Was", "\u00f6ff\u00b7net", "sich", "?", "wen", "seh", "ich", "dort", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gottesfurcht liegt auf den Knien;", "tokens": ["Die", "Got\u00b7tes\u00b7furcht", "liegt", "auf", "den", "Kni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Altar raucht, und jedes Wort", "tokens": ["Der", "Al\u00b7tar", "raucht", ",", "und", "je\u00b7des", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht Sie der Gruft noch zu entziehen.", "tokens": ["Sucht", "Sie", "der", "Gruft", "noch", "zu", "ent\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Heldengeist lehrt uns im Glauben standhaft seyn,", "tokens": ["Ihr", "Hel\u00b7den\u00b7geist", "lehrt", "uns", "im", "Glau\u00b7ben", "stand\u00b7haft", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie hilft sein G\u00f6ttlich Reich erweitern,", "tokens": ["Sie", "hilft", "sein", "G\u00f6tt\u00b7lich", "Reich", "er\u00b7wei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sucht der Armen Nacht durch Ihren Gnadenschein", "tokens": ["Und", "sucht", "der", "Ar\u00b7men", "Nacht", "durch", "Ih\u00b7ren", "Gna\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit Schutz und Beystand zu erheitern.", "tokens": ["Mit", "Schutz", "und", "Beys\u00b7tand", "zu", "er\u00b7hei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Wissenschaften zittern schon,", "tokens": ["Die", "Wis\u00b7sen\u00b7schaf\u00b7ten", "zit\u00b7tern", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehn ihr seltnes Gl\u00fcck verschwinden.", "tokens": ["Und", "sehn", "ihr", "selt\u00b7nes", "Gl\u00fcck", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie rief sie selbst an ihren Thron,", "tokens": ["Sie", "rief", "sie", "selbst", "an", "ih\u00b7ren", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df sie sichern Zutritt finden;", "tokens": ["Und", "lie\u00df", "sie", "si\u00b7chern", "Zu\u00b7tritt", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr schneller Geist drang durch bis in den tiefsten Grund,", "tokens": ["Ihr", "schnel\u00b7ler", "Geist", "drang", "durch", "bis", "in", "den", "tiefs\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Wahrheit selbsten einzusehen;", "tokens": ["Die", "Wahr\u00b7heit", "selbs\u00b7ten", "ein\u00b7zu\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr pr\u00e4chtig Denkmal macht der sp\u00e4ten Nachwelt kund", "tokens": ["Ihr", "pr\u00e4ch\u00b7tig", "Denk\u00b7mal", "macht", "der", "sp\u00e4\u00b7ten", "Nach\u00b7welt", "kund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie eifrig es von ihr geschehen.", "tokens": ["Wie", "eif\u00b7rig", "es", "von", "ihr", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer h\u00f6rt nicht, wie die Staatskunst klagt,", "tokens": ["Wer", "h\u00f6rt", "nicht", ",", "wie", "die", "Staats\u00b7kunst", "klagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da Tod und Leben um Sie k\u00e4mpfen;", "tokens": ["Da", "Tod", "und", "Le\u00b7ben", "um", "Sie", "k\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie viel Ihr Heldenmuth gewagt,", "tokens": ["Wie", "viel", "Ihr", "Hel\u00b7den\u00b7muth", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Frechheit wilde Brut zu d\u00e4mpfen?", "tokens": ["Der", "Frech\u00b7heit", "wil\u00b7de", "Brut", "zu", "d\u00e4mp\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie hoch das kluge Reich den weisen Rath gesch\u00e4tzt,", "tokens": ["Wie", "hoch", "das", "klu\u00b7ge", "Reich", "den", "wei\u00b7sen", "Rath", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn Sie den schweren Scepter f\u00fchrte;", "tokens": ["Wenn", "Sie", "den", "schwe\u00b7ren", "Scep\u00b7ter", "f\u00fchr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sich das freye Volk an Ihrer Huld ergetzt,", "tokens": ["Wie", "sich", "das", "frey\u00b7e", "Volk", "an", "Ih\u00b7rer", "Huld", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn Sie recht K\u00f6niglich regierte.", "tokens": ["Wenn", "Sie", "recht", "K\u00f6\u00b7nig\u00b7lich", "re\u00b7gier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch, wie? du bleibst vor diesmahl taub;", "tokens": ["Doch", ",", "wie", "?", "du", "bleibst", "vor", "dies\u00b7mahl", "taub", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "$.", "PPER", "VVFIN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wilst den harten Schlu\u00df nicht \u00e4ndern,", "tokens": ["Du", "wilst", "den", "har\u00b7ten", "Schlu\u00df", "nicht", "\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wirst durch den verwegnen Raub", "tokens": ["Und", "wirst", "durch", "den", "ver\u00b7weg\u00b7nen", "Raub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die lange Quaal von vielen L\u00e4ndern!", "tokens": ["Die", "lan\u00b7ge", "Qua\u00b7al", "von", "vie\u00b7len", "L\u00e4n\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ja! Caroline stirbt! Jedoch, Sie stirbt vergn\u00fcgt,", "tokens": ["Ja", "!", "Ca\u00b7ro\u00b7li\u00b7ne", "stirbt", "!", "Je\u00b7doch", ",", "Sie", "stirbt", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "VVFIN", "$.", "ADV", "$,", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und kr\u00f6nt durch diesen Tod Ihr Leben;", "tokens": ["Und", "kr\u00f6nt", "durch", "die\u00b7sen", "Tod", "Ihr", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr K\u00f6niglicher Geist hat r\u00fchmlich obgesiegt,", "tokens": ["Ihr", "K\u00f6\u00b7nig\u00b7li\u00b7cher", "Geist", "hat", "r\u00fchm\u00b7lich", "ob\u00b7ge\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihm wird die Ewigkeit gegeben.", "tokens": ["Ihm", "wird", "die", "E\u00b7wig\u00b7keit", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}