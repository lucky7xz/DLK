{"textgrid.poem.40588": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heimw\u00e4rts kam ein Klephte aus dem Kampfe,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heimw\u00e4rts kam ein Klephte aus dem Kampfe,", "tokens": ["Heim\u00b7w\u00e4rts", "kam", "ein", "Kleph\u00b7te", "aus", "dem", "Kamp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An die Brust sinkt ihm die treue Gattin,", "tokens": ["An", "die", "Brust", "sinkt", "ihm", "die", "treu\u00b7e", "Gat\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und zwei Knaben frisch und freudig rufen:", "tokens": ["Und", "zwei", "Kna\u00b7ben", "frisch", "und", "freu\u00b7dig", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbgott gr\u00fc\u00df', Vater! dachtest du auch unser?\u00ab", "tokens": ["\u00bb", "gott", "gr\u00fc\u00df'", ",", "Va\u00b7ter", "!", "dach\u00b7test", "du", "auch", "un\u00b7ser", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch das dritt' und kleinste in der Wiege", "tokens": ["Doch", "das", "dritt'", "und", "kleins\u00b7te", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "KON", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Streckt die zarten H\u00e4ndchen ihm entgegen.", "tokens": ["Streckt", "die", "zar\u00b7ten", "H\u00e4nd\u00b7chen", "ihm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und er spricht zum Kn\u00e4blein in der Wiege:", "tokens": ["Und", "er", "spricht", "zum", "Kn\u00e4\u00b7blein", "in", "der", "Wie\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbarmer Schalk, mich dauert deine Bl\u00f6\u00dfe,", "tokens": ["\u00bb", "ar\u00b7mer", "Schalk", ",", "mich", "dau\u00b7ert", "dei\u00b7ne", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Brachte Stoff, zu decken deine Nacktheit,", "tokens": ["Brach\u00b7te", "Stoff", ",", "zu", "de\u00b7cken", "dei\u00b7ne", "Nackt\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00fctterchen soll Windeln draus dir schneiden.\u00ab", "tokens": ["M\u00fct\u00b7ter\u00b7chen", "soll", "Win\u00b7deln", "draus", "dir", "schnei\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "NN", "PAV", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zog aus dem Tornister einen Turban.", "tokens": ["Zog", "aus", "dem", "Tor\u00b7nis\u00b7ter", "ei\u00b7nen", "Tur\u00b7ban", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann zum zweiten sprach er l\u00e4chelnd also:", "tokens": ["Dann", "zum", "zwei\u00b7ten", "sprach", "er", "l\u00e4\u00b7chelnd", "al\u00b7so", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbgern, ich wei\u00df es, spielst du mit dem Balle,", "tokens": ["\u00bb", "gern", ",", "ich", "wei\u00df", "es", ",", "spielst", "du", "mit", "dem", "Bal\u00b7le", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Habe dir gebracht drei runde B\u00e4lle,", "tokens": ["Ha\u00b7be", "dir", "ge\u00b7bracht", "drei", "run\u00b7de", "B\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bring' viel solcher B\u00e4ll' einst deinen S\u00f6hnen", "tokens": ["Bring'", "viel", "sol\u00b7cher", "B\u00e4ll'", "einst", "dei\u00b7nen", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PIAT", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und hoch in die L\u00fcfte la\u00df sie fliegen!\u00ab", "tokens": ["Und", "hoch", "in", "die", "L\u00fcf\u00b7te", "la\u00df", "sie", "flie\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und er zog heraus drei T\u00fcrkensch\u00e4del.", "tokens": ["Und", "er", "zog", "he\u00b7raus", "drei", "T\u00fcr\u00b7ken\u00b7sch\u00e4\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "K\u00fc\u00dft dann auf die Stirn den dritten, \u00e4lt'sten,", "tokens": ["K\u00fc\u00dft", "dann", "auf", "die", "Stirn", "den", "drit\u00b7ten", ",", "\u00e4lt'\u00b7sten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Schnallt ein blankes Schwert ihm um die Lenden,", "tokens": ["Schnallt", "ein", "blan\u00b7kes", "Schwert", "ihm", "um", "die", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00e4ngt ihm eine B\u00fcchse auf die Schultern,", "tokens": ["H\u00e4ngt", "ihm", "ei\u00b7ne", "B\u00fcch\u00b7se", "auf", "die", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Also sprechend: \u00bbAuf, wir ziehn zusammen!", "tokens": ["Al\u00b7so", "spre\u00b7chend", ":", "\u00bb", "Auf", ",", "wir", "ziehn", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "$(", "APPR", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Freut, ihr Andern, euch auf unsre R\u00fcckkehr!", "tokens": ["Freut", ",", "ihr", "An\u00b7dern", ",", "euch", "auf", "uns\u00b7re", "R\u00fcck\u00b7kehr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "$,", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Doppelt wiegt die Beute, die wir bringen,", "tokens": ["Dop\u00b7pelt", "wiegt", "die", "Beu\u00b7te", ",", "die", "wir", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Windeln f\u00fcr die Kinder von zehn D\u00f6rfern,", "tokens": ["Win\u00b7deln", "f\u00fcr", "die", "Kin\u00b7der", "von", "zehn", "D\u00f6r\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "B\u00e4lle f\u00fcr die ganze Nachbarschaft.\u00ab", "tokens": ["B\u00e4l\u00b7le", "f\u00fcr", "die", "gan\u00b7ze", "Nach\u00b7bar\u00b7schaft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}