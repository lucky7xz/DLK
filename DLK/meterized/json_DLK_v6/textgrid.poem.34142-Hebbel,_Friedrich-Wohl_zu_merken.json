{"textgrid.poem.34142": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wohl zu merken", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lichter gie\u00dfen, ist Eins, und Lichter brauchen, ein Zweites!", "tokens": ["Lich\u00b7ter", "gie\u00b7\u00dfen", ",", "ist", "Eins", ",", "und", "Lich\u00b7ter", "brau\u00b7chen", ",", "ein", "Zwei\u00b7tes", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VAFIN", "NN", "$,", "KON", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Merk' es dir, emsiger Freund, der du die Fackel dir borgst,", "tokens": ["Merk'", "es", "dir", ",", "em\u00b7si\u00b7ger", "Freund", ",", "der", "du", "die", "Fa\u00b7ckel", "dir", "borgst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Und nun, Winkel nach Winkel mit ihrer H\u00fclfe durchkriechend,", "tokens": ["Und", "nun", ",", "Win\u00b7kel", "nach", "Win\u00b7kel", "mit", "ih\u00b7rer", "H\u00fcl\u00b7fe", "durch\u00b7krie\u00b7chend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jenem, der sie dir lieh, keck an die Seite dich stellst!", "tokens": ["Je\u00b7nem", ",", "der", "sie", "dir", "lieh", ",", "keck", "an", "die", "Sei\u00b7te", "dich", "stellst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Der die Sonne erschuf, wird ewig ein Anderer bleiben,", "tokens": ["Der", "die", "Son\u00b7ne", "er\u00b7schuf", ",", "wird", "e\u00b7wig", "ein", "An\u00b7de\u00b7rer", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Als der flei\u00dfige Mann, der die Veduten uns malt.", "tokens": ["Als", "der", "flei\u00b7\u00dfi\u00b7ge", "Mann", ",", "der", "die", "Ve\u00b7du\u00b7ten", "uns", "malt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}