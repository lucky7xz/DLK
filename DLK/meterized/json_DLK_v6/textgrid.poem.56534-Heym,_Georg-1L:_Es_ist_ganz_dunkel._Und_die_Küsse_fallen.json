{"textgrid.poem.56534": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist ganz dunkel. Und die K\u00fcsse fallen", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ganz dunkel. Und die K\u00fcsse fallen", "tokens": ["Es", "ist", "ganz", "dun\u00b7kel", ".", "Und", "die", "K\u00fcs\u00b7se", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hei\u00dfer Tau im d\u00e4mmernden Gemach.", "tokens": ["Wie", "hei\u00b7\u00dfer", "Tau", "im", "d\u00e4m\u00b7mern\u00b7den", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wollust Fackeln brennen auf und wallen", "tokens": ["Der", "Wol\u00b7lust", "Fa\u00b7ckeln", "bren\u00b7nen", "auf", "und", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit roter Glut dem dunklen Abend nach.", "tokens": ["Mit", "ro\u00b7ter", "Glut", "dem", "dunk\u00b7len", "A\u00b7bend", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Fieber jagt ihr Blut mit wei\u00dfem Brand,", "tokens": ["Das", "Fie\u00b7ber", "jagt", "ihr", "Blut", "mit", "wei\u00b7\u00dfem", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie sich halb schon seinem Durst gew\u00e4hrt.", "tokens": ["Da\u00df", "sie", "sich", "halb", "schon", "sei\u00b7nem", "Durst", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie bebt auf seinem Scho\u00df, da seine Hand", "tokens": ["Sie", "bebt", "auf", "sei\u00b7nem", "Scho\u00df", ",", "da", "sei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihrem Hemd nach ihren Br\u00fcsten f\u00e4hrt.", "tokens": ["In", "ih\u00b7rem", "Hemd", "nach", "ih\u00b7ren", "Br\u00fcs\u00b7ten", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hinten, im Vorhang, in der Dunkelheit", "tokens": ["Hin\u00b7ten", ",", "im", "Vor\u00b7hang", ",", "in", "der", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Steht auf das Bett, der Hafen ihrer Gier.", "tokens": ["Steht", "auf", "das", "Bett", ",", "der", "Ha\u00b7fen", "ih\u00b7rer", "Gier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Wolken auf dem Meere lagert breit", "tokens": ["Wie", "Wol\u00b7ken", "auf", "dem", "Mee\u00b7re", "la\u00b7gert", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darauf der Dunst von schwarzem Elixier.", "tokens": ["Da\u00b7rauf", "der", "Dunst", "von", "schwar\u00b7zem", "E\u00b7li\u00b7xier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie wird es sein? Sie friert in seinem Arm,", "tokens": ["Wie", "wird", "es", "sein", "?", "Sie", "friert", "in", "sei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VAINF", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ihren nackten Leib hin\u00fcbertr\u00e4gt.", "tokens": ["Der", "ih\u00b7ren", "nack\u00b7ten", "Leib", "hin\u00b7\u00fc\u00b7ber\u00b7tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es zittert auf in ihrem Scho\u00dfe warm,", "tokens": ["Es", "zit\u00b7tert", "auf", "in", "ih\u00b7rem", "Scho\u00b7\u00dfe", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um den er wild die beiden Arme schl\u00e4gt.", "tokens": ["Um", "den", "er", "wild", "die", "bei\u00b7den", "Ar\u00b7me", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "ADJD", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ihr blondes Haar brennt durch die Nacht, darein", "tokens": ["Ihr", "blon\u00b7des", "Haar", "brennt", "durch", "die", "Nacht", ",", "da\u00b7rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die tiefe Hand des feuchten Dunkels w\u00fchlt.", "tokens": ["Die", "tie\u00b7fe", "Hand", "des", "feuch\u00b7ten", "Dun\u00b7kels", "w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sturm der Wollust l\u00e4\u00dft sie leise schrein,", "tokens": ["Der", "Sturm", "der", "Wol\u00b7lust", "l\u00e4\u00dft", "sie", "lei\u00b7se", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da seinen Bi\u00df sie in den Br\u00fcsten f\u00fchlt.", "tokens": ["Da", "sei\u00b7nen", "Bi\u00df", "sie", "in", "den", "Br\u00fcs\u00b7ten", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}