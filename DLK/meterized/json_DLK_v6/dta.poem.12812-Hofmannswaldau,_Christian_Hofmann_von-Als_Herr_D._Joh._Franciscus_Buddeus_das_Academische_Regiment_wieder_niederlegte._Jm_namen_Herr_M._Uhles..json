{"dta.poem.12812": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So stimmen wir denn mit einander ein:", "tokens": ["So", "stim\u00b7men", "wir", "denn", "mit", "ein\u00b7an\u00b7der", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dieser Gro\u00dfe Mann,", "tokens": ["Da\u00df", "die\u00b7ser", "Gro\u00b7\u00dfe", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den selbst der neid mit keinem rechte tadeln kan,", "tokens": ["Den", "selbst", "der", "neid", "mit", "kei\u00b7nem", "rech\u00b7te", "ta\u00b7deln", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PIAT", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sch\u00f6ne Saal-Athen,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Saa\u00b7lA\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo sich gerechtigkeit und weisheit stets erh\u00f6hn,", "tokens": ["Wo", "sich", "ge\u00b7rech\u00b7tig\u00b7keit", "und", "weis\u00b7heit", "stets", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch viele jahre m\u00f6g erfreun.", "tokens": ["Noch", "vie\u00b7le", "jah\u00b7re", "m\u00f6g", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum, schwestern! auf! kommt, stimmet mit mir ein.", "tokens": ["Drum", ",", "schwes\u00b7tern", "!", "auf", "!", "kommt", ",", "stim\u00b7met", "mit", "mir", "ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "$.", "PTKVZ", "$.", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}