{"textgrid.poem.43201": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chst meiner Gondel steht ein M\u00f6nch, der leise", "tokens": ["N\u00e4chst", "mei\u00b7ner", "Gon\u00b7del", "steht", "ein", "M\u00f6nch", ",", "der", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebete seufzt. Er starret vor sich nieder,", "tokens": ["Ge\u00b7be\u00b7te", "seufzt", ".", "Er", "star\u00b7ret", "vor", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch sein Gefolge murmelt Todtenlieder.", "tokens": ["Auch", "sein", "Ge\u00b7fol\u00b7ge", "mur\u00b7melt", "Tod\u00b7ten\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wellen singen ihre alte Weise.", "tokens": ["Die", "Wel\u00b7len", "sin\u00b7gen", "ih\u00b7re", "al\u00b7te", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den M\u00f6nch und eine Todte tr\u00e4gt die kleine", "tokens": ["Den", "M\u00f6nch", "und", "ei\u00b7ne", "Tod\u00b7te", "tr\u00e4gt", "die", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schwarzbeflaggte Gondel, um das Kissen", "tokens": ["Und", "schwarz\u00b7be\u00b7flagg\u00b7te", "Gon\u00b7del", ",", "um", "das", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des stillen M\u00e4dchens flattert windzerrissen", "tokens": ["Des", "stil\u00b7len", "M\u00e4d\u00b7chens", "flat\u00b7tert", "wind\u00b7zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schwarzer Schleier, und umh\u00fcllt die Reine.", "tokens": ["Ein", "schwar\u00b7zer", "Schlei\u00b7er", ",", "und", "um\u00b7h\u00fcllt", "die", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Kranz von Rosen schm\u00fcckt ihr Haupt, das bleiche,", "tokens": ["Ein", "Kranz", "von", "Ro\u00b7sen", "schm\u00fcckt", "ihr", "Haupt", ",", "das", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die blonden Locken gleiten auf den Fluthen,", "tokens": ["Die", "blon\u00b7den", "Lo\u00b7cken", "glei\u00b7ten", "auf", "den", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie sie einst bei s\u00fc\u00dfen Tr\u00e4umen ruhten,", "tokens": ["Und", "wie", "sie", "einst", "bei", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruh'n jetzt die H\u00e4nde dieser holden Leiche.", "tokens": ["Ruh'n", "jetzt", "die", "H\u00e4n\u00b7de", "die\u00b7ser", "hol\u00b7den", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In alten Angeln knarret rostend, lose", "tokens": ["In", "al\u00b7ten", "An\u00b7geln", "knar\u00b7ret", "ros\u00b7tend", ",", "lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Friedhofsthor ... und wie den Sarg sie heben,", "tokens": ["Das", "Fried\u00b7hofs\u00b7thor", "...", "und", "wie", "den", "Sarg", "sie", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den jungen Leib der Erde r\u00fcckzugeben,", "tokens": ["Den", "jun\u00b7gen", "Leib", "der", "Er\u00b7de", "r\u00fcck\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sinkt in die Fluthen eine wei\u00dfe Rose...", "tokens": ["Sinkt", "in", "die", "Flut\u00b7hen", "ei\u00b7ne", "wei\u00b7\u00dfe", "Ro\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}