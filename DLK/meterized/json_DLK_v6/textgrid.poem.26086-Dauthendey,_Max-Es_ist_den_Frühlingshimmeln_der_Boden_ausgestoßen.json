{"textgrid.poem.26086": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es ist den Fr\u00fchlingshimmeln der Boden ausgesto\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Platzregen, der sich \u00fcberschl\u00e4gt,", "tokens": ["Platz\u00b7re\u00b7gen", ",", "der", "sich", "\u00fc\u00b7bersc\u00b7hl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00dcber das Pflaster wie ein T\u00e4nzer fegt,", "tokens": ["\u00dc\u00b7ber", "das", "Pflas\u00b7ter", "wie", "ein", "T\u00e4n\u00b7zer", "fegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie ein gewaltiges graues Wasserspiel.", "tokens": ["Wie", "ein", "ge\u00b7wal\u00b7ti\u00b7ges", "grau\u00b7es", "Was\u00b7ser\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und jeder Regenstrahl nimmt einen Stein zum Ziel.", "tokens": ["Und", "je\u00b7der", "Re\u00b7gen\u00b7strahl", "nimmt", "ei\u00b7nen", "Stein", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie ein Kranz steht der Regen auf der Steine Kopf,", "tokens": ["Wie", "ein", "Kranz", "steht", "der", "Re\u00b7gen", "auf", "der", "Stei\u00b7ne", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Das helle Pflaster hat einen grauen Wasserschopf.", "tokens": ["Das", "hel\u00b7le", "Pflas\u00b7ter", "hat", "ei\u00b7nen", "grau\u00b7en", "Was\u00b7ser\u00b7schopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Es ist der Regen wie S\u00e4ulen auf die Erde geschossen,", "tokens": ["Es", "ist", "der", "Re\u00b7gen", "wie", "S\u00e4u\u00b7len", "auf", "die", "Er\u00b7de", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOKOM", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Es sind die Wolkenf\u00e4sser in Str\u00f6men \u00fcbergeflossen;", "tokens": ["Es", "sind", "die", "Wol\u00b7ken\u00b7f\u00e4s\u00b7ser", "in", "Str\u00f6\u00b7men", "\u00fc\u00b7ber\u00b7ge\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und ist den Fr\u00fchlingshimmeln der Boden ausgesto\u00dfen.", "tokens": ["Und", "ist", "den", "Fr\u00fch\u00b7lings\u00b7him\u00b7meln", "der", "Bo\u00b7den", "aus\u00b7ge\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}