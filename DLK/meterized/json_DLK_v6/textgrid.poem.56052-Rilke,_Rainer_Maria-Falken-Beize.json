{"textgrid.poem.56052": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Falken-Beize", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaiser sein hei\u00dft unverwandelt vieles", "tokens": ["Kai\u00b7ser", "sein", "hei\u00dft", "un\u00b7ver\u00b7wan\u00b7delt", "vie\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAINF", "VVFIN", "ADJD", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcberstehen bei geheimer Tat:", "tokens": ["\u00fc\u00b7bers\u00b7te\u00b7hen", "bei", "ge\u00b7hei\u00b7mer", "Tat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "wenn der Kanzler nachts den Turm betrat,", "tokens": ["wenn", "der", "Kanz\u00b7ler", "nachts", "den", "Turm", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "fand er ", "tokens": ["fand", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "k\u00fchnen f\u00fcrstlichen Traktat", "tokens": ["k\u00fch\u00b7nen", "f\u00fcrst\u00b7li\u00b7chen", "Trak\u00b7tat"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "in den eingeneigten Schreiber sagen;", "tokens": ["in", "den", "ein\u00b7ge\u00b7neig\u00b7ten", "Schrei\u00b7ber", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "denn er hatte im entlegnen Saale", "tokens": ["denn", "er", "hat\u00b7te", "im", "ent\u00b7leg\u00b7nen", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "selber n\u00e4chtelang und viele Male", "tokens": ["sel\u00b7ber", "n\u00e4ch\u00b7tel\u00b7ang", "und", "vie\u00b7le", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "das noch ungewohnte Tier getragen,", "tokens": ["das", "noch", "un\u00b7ge\u00b7wohn\u00b7te", "Tier", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "wenn es fremd war, neu und aufgebr\u00e4ut.", "tokens": ["wenn", "es", "fremd", "war", ",", "neu", "und", "auf\u00b7ge\u00b7br\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und er hatte dann sich nie gescheut,", "tokens": ["Und", "er", "hat\u00b7te", "dann", "sich", "nie", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Pl\u00e4ne, welche in ihm aufgesprungen,", "tokens": ["Pl\u00e4\u00b7ne", ",", "wel\u00b7che", "in", "ihm", "auf\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "oder z\u00e4rtlicher Erinnerungen", "tokens": ["o\u00b7der", "z\u00e4rt\u00b7li\u00b7cher", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "tieftiefinneres Gel\u00e4ut", "tokens": ["tief\u00b7tie\u00b7fin\u00b7ne\u00b7res", "Ge\u00b7l\u00e4ut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "zu verachten, um des bangen jungen", "tokens": ["zu", "ver\u00b7ach\u00b7ten", ",", "um", "des", "ban\u00b7gen", "jun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVFIN", "$,", "KOUI", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Falken willen, dessen Blut und Sorgen", "tokens": ["Fal\u00b7ken", "wil\u00b7len", ",", "des\u00b7sen", "Blut", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zu begreifen er sich nicht erlie\u00df.", "tokens": ["zu", "be\u00b7grei\u00b7fen", "er", "sich", "nicht", "er\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Daf\u00fcr war er auch wie mitgehoben,", "tokens": ["Da\u00b7f\u00fcr", "war", "er", "auch", "wie", "mit\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wenn der Vogel, den die Herren loben,", "tokens": ["wenn", "der", "Vo\u00b7gel", ",", "den", "die", "Her\u00b7ren", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "gl\u00e4nzend von der Hand geworfen, oben", "tokens": ["gl\u00e4n\u00b7zend", "von", "der", "Hand", "ge\u00b7wor\u00b7fen", ",", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "in dem mitgef\u00fchlten Fr\u00fchlingsmorgen", "tokens": ["in", "dem", "mit\u00b7ge\u00b7f\u00fchl\u00b7ten", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "wie ein Engel auf den Reiher stie\u00df.", "tokens": ["wie", "ein", "En\u00b7gel", "auf", "den", "Rei\u00b7her", "stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}