{"textgrid.poem.47088": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah! Sie stand im Ost; zur Seite standen", "tokens": ["Ich", "sah", "!", "Sie", "stand", "im", "Ost", ";", "zur", "Sei\u00b7te", "stan\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Berggipfel ihr, gleich harrendem Altare;", "tokens": ["Berg\u00b7gip\u00b7fel", "ihr", ",", "gleich", "har\u00b7ren\u00b7dem", "Al\u00b7ta\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Tauperlen waren Kron' in ihrem Haare,", "tokens": ["Tau\u00b7per\u00b7len", "wa\u00b7ren", "Kron'", "in", "ih\u00b7rem", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Morgennebel flo\u00df ihr zu Gewanden.", "tokens": ["Und", "Mor\u00b7gen\u00b7ne\u00b7bel", "flo\u00df", "ihr", "zu", "Ge\u00b7wan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf ihren Wangen, statt der Schminke, fanden", "tokens": ["Auf", "ih\u00b7ren", "Wan\u00b7gen", ",", "statt", "der", "Schmin\u00b7ke", ",", "fan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwei Morgenr\u00f6ten sich, zwei dunkelklare;", "tokens": ["Zwei", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten", "sich", ",", "zwei", "dun\u00b7kel\u00b7kla\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "PRF", "$,", "CARD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Blicken ward zu Morgensternenpaare,", "tokens": ["Ihr", "Bli\u00b7cken", "ward", "zu", "Mor\u00b7gens\u00b7ter\u00b7nen\u00b7paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Davor die Blick' all andrer Sterne schwanden.", "tokens": ["Da\u00b7vor", "die", "Blick'", "all", "an\u00b7drer", "Ster\u00b7ne", "schwan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Glutmeer lag zu F\u00fc\u00dfen ihr; sie tauchte", "tokens": ["Ein", "Glut\u00b7meer", "lag", "zu", "F\u00fc\u00b7\u00dfen", "ihr", ";", "sie", "tauch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PPOSAT", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drein mit der Opferschal' und lie\u00df als Sonne", "tokens": ["Drein", "mit", "der", "Op\u00b7fer\u00b7schal'", "und", "lie\u00df", "als", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Glanzgu\u00df auf des Berghaupts Altar glimmen.", "tokens": ["Den", "Glanz\u00b7gu\u00df", "auf", "des", "Berg\u00b7haupts", "Al\u00b7tar", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann, wie sie mit dem Odem erdw\u00e4rts hauchte,", "tokens": ["Dann", ",", "wie", "sie", "mit", "dem", "O\u00b7dem", "erd\u00b7w\u00e4rts", "hauch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erwachten tausend Kehlen dort in Wonne,", "tokens": ["Er\u00b7wach\u00b7ten", "tau\u00b7send", "Keh\u00b7len", "dort", "in", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit mir den Morgenhymnus anzustimmen.", "tokens": ["Mit", "mir", "den", "Mor\u00b7gen\u00b7hym\u00b7nus", "an\u00b7zu\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}