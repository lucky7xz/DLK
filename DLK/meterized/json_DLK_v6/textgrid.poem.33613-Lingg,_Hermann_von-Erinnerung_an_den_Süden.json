{"textgrid.poem.33613": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung an den S\u00fcden", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch im Herbst, wie wart ihr sch\u00f6n,", "tokens": ["Noch", "im", "Herbst", ",", "wie", "wart", "ihr", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen \u00fcber den Ruinen,", "tokens": ["Blu\u00b7men", "\u00fc\u00b7ber", "den", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn in lichten \u00c4therh\u00f6h'n", "tokens": ["Wenn", "in", "lich\u00b7ten", "\u00c4\u00b7ther\u00b7h\u00f6h'n"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Fr\u00fch der erste Stern erschienen!", "tokens": ["Fr\u00fch", "der", "ers\u00b7te", "Stern", "er\u00b7schie\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie gerne stundenlang", "tokens": ["O", "wie", "ger\u00b7ne", "stun\u00b7den\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weilt' ich bei den Tempelh\u00fcgeln,", "tokens": ["Weilt'", "ich", "bei", "den", "Tem\u00b7pel\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis auf dunklen Adlerfl\u00fcgeln", "tokens": ["Bis", "auf", "dunk\u00b7len", "Ad\u00b7ler\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die Nacht herniederschwang.", "tokens": ["Sich", "die", "Nacht", "her\u00b7nie\u00b7der\u00b7schwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}