{"dta.poem.7374": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Fr\u00e4ulein von Rodenschild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Sind denn so schw\u00fcl die N\u00e4cht' im April?", "tokens": ["Sind", "denn", "so", "schw\u00fcl", "die", "N\u00e4cht'", "im", "Ap\u00b7ril", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Oder ist so siedend jungfr\u00e4ulich' Blut?", "tokens": ["O\u00b7der", "ist", "so", "sie\u00b7dend", "jung\u00b7fr\u00e4u\u00b7lich'", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Sie schlie\u00dft die Wimper, sie liegt so still,", "tokens": ["Sie", "schlie\u00dft", "die", "Wim\u00b7per", ",", "sie", "liegt", "so", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und horcht des Herzens pochender Fluth.", "tokens": ["Und", "horcht", "des", "Her\u00b7zens", "po\u00b7chen\u00b7der", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u201eo will es denn nimmer und nimmer tagen!", "tokens": ["\u201e", "o", "will", "es", "denn", "nim\u00b7mer", "und", "nim\u00b7mer", "ta\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "PPER", "ADV", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "O will denn nicht endlich die Stunde schlagen!", "tokens": ["O", "will", "denn", "nicht", "end\u00b7lich", "die", "Stun\u00b7de", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ich wache, und selbst der Seiger ruht!", "tokens": ["Ich", "wa\u00b7che", ",", "und", "selbst", "der", "Sei\u00b7ger", "ruht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch horch! es summt, eins, zwei und drei, \u2014", "tokens": ["Doch", "horch", "!", "es", "summt", ",", "eins", ",", "zwei", "und", "drei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "$,", "PIS", "$,", "CARD", "KON", "CARD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch immer fort? \u2014 sechs, sieben und acht,", "tokens": ["Noch", "im\u00b7mer", "fort", "?", "sechs", ",", "sie\u00b7ben", "und", "acht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "$(", "CARD", "$,", "CARD", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Elf, zw\u00f6lf, \u2014 o Himmel, war das ein Schrei?", "tokens": ["Elf", ",", "zw\u00f6lf", ",", "o", "Him\u00b7mel", ",", "war", "das", "ein", "Schrei", "?"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "CARD", "$,", "$(", "FM", "NN", "$,", "VAFIN", "PDS", "ART", "NN", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Doch nein, Gesang steigt \u00fcber der Wacht,", "tokens": ["Doch", "nein", ",", "Ge\u00b7sang", "steigt", "\u00fc\u00b7ber", "der", "Wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nun wird mir's klar, mit frommem Munde", "tokens": ["Nun", "wird", "mir's", "klar", ",", "mit", "from\u00b7mem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Begr\u00fc\u00dft das Hausgesinde die Stunde, ", "tokens": ["Be\u00b7gr\u00fc\u00dft", "das", "Haus\u00b7ge\u00b7sin\u00b7de", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Anbrach die hochheilige Osternacht.\u201c", "tokens": ["An\u00b7brach", "die", "hoch\u00b7hei\u00b7li\u00b7ge", "Os\u00b7ter\u00b7nacht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}}}}}