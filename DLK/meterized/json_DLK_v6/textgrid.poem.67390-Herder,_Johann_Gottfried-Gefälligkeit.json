{"textgrid.poem.67390": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Gef\u00e4lligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aspasia, sie schenkte meinem Leben", "tokens": ["As\u00b7pa\u00b7sia", ",", "sie", "schenk\u00b7te", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zwar ", "tokens": ["Zwar"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Doch warum ", "tokens": ["Doch", "wa\u00b7rum"], "token_info": ["word", "word"], "pos": ["KON", "PWAV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und litt ich nicht vielleicht aus eigner Schuld?", "tokens": ["Und", "litt", "ich", "nicht", "viel\u00b7leicht", "aus", "eig\u00b7ner", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbauch Andern,\u00ab sprach ich, \u00bbwerd' ein sch\u00f6nes Leben!\u00ab", "tokens": ["\u00bb", "auch", "An\u00b7dern", ",", "\u00ab", "sprach", "ich", ",", "\u00bb", "werd'", "ein", "sch\u00f6\u00b7nes", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJA", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sah ihr Gl\u00fcck und sah es mit Geduld.", "tokens": ["Und", "sah", "ihr", "Gl\u00fcck", "und", "sah", "es", "mit", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Andern, eben auch, wie ich, entlassen,", "tokens": ["Die", "An\u00b7dern", ",", "e\u00b7ben", "auch", ",", "wie", "ich", ",", "ent\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verfolgten sie mit heft'ger Ungeduld.", "tokens": ["Ver\u00b7folg\u00b7ten", "sie", "mit", "heft'\u00b7ger", "Un\u00b7ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwie?\u00ab sprach Aspasia; \u00bbmit Zank und Hassen", "tokens": ["\u00bb", "wie", "?", "\u00ab", "sprach", "As\u00b7pa\u00b7sia", ";", "\u00bb", "mit", "Zank", "und", "Has\u00b7sen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "$.", "$(", "VVFIN", "NE", "$.", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wollt Ihr erzwingen Artigkeit und Huld?", "tokens": ["Wollt", "Ihr", "er\u00b7zwin\u00b7gen", "Ar\u00b7tig\u00b7keit", "und", "Huld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Eine droht, der Andre will erblassen;", "tokens": ["Der", "Ei\u00b7ne", "droht", ",", "der", "And\u00b7re", "will", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Unholde, traget Eurer Sitten Schuld!\u00ab", "tokens": ["Un\u00b7hol\u00b7de", ",", "tra\u00b7get", "Eu\u00b7rer", "Sit\u00b7ten", "Schuld", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbaspasia,\u00ab sprach ich, \u00bbnimm diese Rosen", "tokens": ["\u00bb", "as\u00b7pa\u00b7sia", ",", "\u00ab", "sprach", "ich", ",", "\u00bb", "nimm", "die\u00b7se", "Ro\u00b7sen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "NE", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVIMP", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und sei mir hold, so lange Dir's gef\u00e4llt!", "tokens": ["Und", "sei", "mir", "hold", ",", "so", "lan\u00b7ge", "Dir's", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unwerth ist Dein, wer Dich mit Liebekosen", "tokens": ["Un\u00b7werth", "ist", "Dein", ",", "wer", "Dich", "mit", "Lie\u00b7be\u00b7ko\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "$,", "PWS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Forderung f\u00fcr seine Sklavin h\u00e4lt.\u00ab", "tokens": ["Und", "For\u00b7de\u00b7rung", "f\u00fcr", "sei\u00b7ne", "Skla\u00b7vin", "h\u00e4lt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie neigte sich, die Wange voller Rosen:", "tokens": ["Sie", "neig\u00b7te", "sich", ",", "die", "Wan\u00b7ge", "vol\u00b7ler", "Ro\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwer zu gefallen wei\u00df, ist Herr der Welt.\u00ab", "tokens": ["\u00bb", "wer", "zu", "ge\u00b7fal\u00b7len", "wei\u00df", ",", "ist", "Herr", "der", "Welt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PTKZU", "VVINF", "VVFIN", "$,", "VAFIN", "NN", "ART", "NN", "$.", "$("], "meter": "---+-+-+-+", "measure": "zehnsilber"}}}}}