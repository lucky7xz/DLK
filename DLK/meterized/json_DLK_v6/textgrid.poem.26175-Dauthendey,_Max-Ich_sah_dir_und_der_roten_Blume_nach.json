{"textgrid.poem.26175": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich sah dir und der roten Blume nach", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stand in der D\u00e4mmerung ziegelrot", "tokens": ["Es", "stand", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "zie\u00b7gel\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die erste Mohnblume \u00fcber den Weg,", "tokens": ["Die", "ers\u00b7te", "Mohn\u00b7blu\u00b7me", "\u00fc\u00b7ber", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sich im Halbdunkel noch deinem Aug' anbot.", "tokens": ["Die", "sich", "im", "Halb\u00b7dun\u00b7kel", "noch", "dei\u00b7nem", "Aug'", "an\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du nahmst sie mit. Die Mohnblum' brannte noch,", "tokens": ["Du", "nahmst", "sie", "mit", ".", "Die", "Mohn\u00b7blum'", "brann\u00b7te", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als l\u00e4ngst die Nacht in alle B\u00e4ume kroch;", "tokens": ["Als", "l\u00e4ngst", "die", "Nacht", "in", "al\u00b7le", "B\u00e4u\u00b7me", "kroch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hieltst sie wie eine kleine Fackel hoch.", "tokens": ["Hieltst", "sie", "wie", "ei\u00b7ne", "klei\u00b7ne", "Fa\u00b7ckel", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu Haus trugst du sie leuchtend ins Gemach.", "tokens": ["Zu", "Haus", "trugst", "du", "sie", "leuch\u00b7tend", "ins", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah dir und der roten Blume nach. \u2013", "tokens": ["Ich", "sah", "dir", "und", "der", "ro\u00b7ten", "Blu\u00b7me", "nach", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du tr\u00e4gst mir ewig neues Feuer unters Dach.", "tokens": ["Du", "tr\u00e4gst", "mir", "e\u00b7wig", "neu\u00b7es", "Feu\u00b7er", "un\u00b7ters", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}