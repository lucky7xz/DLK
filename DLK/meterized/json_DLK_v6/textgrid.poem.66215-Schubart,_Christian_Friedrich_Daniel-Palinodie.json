{"textgrid.poem.66215": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Palinodie", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der gro\u00dfe", "tokens": ["Wer", "ist", "der", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Herkulische Kolosse,", "tokens": ["Her\u00b7ku\u00b7li\u00b7sche", "Ko\u00b7los\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der seinen H\u00fcgel Fleisch auf Knochens\u00e4ulen hebt,", "tokens": ["Der", "sei\u00b7nen", "H\u00fc\u00b7gel", "Fleisch", "auf", "Kno\u00b7chen\u00b7s\u00e4u\u00b7len", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Boden stampft, der weit herum erbebt?", "tokens": ["Den", "Bo\u00b7den", "stampft", ",", "der", "weit", "he\u00b7rum", "er\u00b7bebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist ", "tokens": ["Es", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Im tiefen Auge Flammenblick.", "tokens": ["Im", "tie\u00b7fen", "Au\u00b7ge", "Flam\u00b7men\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schaum flie\u00dft von seinem Bart. Er br\u00fcllt herab: Ich z\u00fcrne,", "tokens": ["Schaum", "flie\u00dft", "von", "sei\u00b7nem", "Bart", ".", "Er", "br\u00fcllt", "her\u00b7ab", ":", "Ich", "z\u00fcr\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Verwegner, \u00fcber dich! \u2013 Zur\u00fcck!", "tokens": ["Ver\u00b7weg\u00b7ner", ",", "\u00fc\u00b7ber", "dich", "!", "\u2013", "Zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sonst sto\u00df' ich dir die scharfgeschliffnen Zacken", "tokens": ["Sonst", "sto\u00df'", "ich", "dir", "die", "scharf\u00b7ge\u00b7schliff\u00b7nen", "Za\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Des m\u00e4chtigen Tridentes in den Nacken! \u2013", "tokens": ["Des", "m\u00e4ch\u00b7ti\u00b7gen", "Tri\u00b7den\u00b7tes", "in", "den", "Na\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie angedonnert stand ich da", "tokens": ["Wie", "an\u00b7ge\u00b7don\u00b7nert", "stand", "ich", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und zitterte und sah", "tokens": ["Und", "zit\u00b7ter\u00b7te", "und", "sah"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Voll \u00e4ngstlicher Erwartung nieder. \u2013", "tokens": ["Voll", "\u00e4ngst\u00b7li\u00b7cher", "Er\u00b7war\u00b7tung", "nie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Furchtbare erhob sich wieder", "tokens": ["Der", "Furcht\u00b7ba\u00b7re", "er\u00b7hob", "sich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und sprach: Du wagst es, du allein,", "tokens": ["Und", "sprach", ":", "Du", "wagst", "es", ",", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht in den Lobgesang des Friedens einzustimmen?", "tokens": ["Nicht", "in", "den", "Lob\u00b7ge\u00b7sang", "des", "Frie\u00b7dens", "ein\u00b7zu\u00b7stim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Verschweigst des Sultans Schmach? Ich w\u00fcrd' ergrimmen,", "tokens": ["Ver\u00b7schweigst", "des", "Sul\u00b7tans", "Schmach", "?", "Ich", "w\u00fcrd'", "er\u00b7grim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Jedoch du bist f\u00fcr meinen Grimm zu klein. \u2013", "tokens": ["Je\u00b7doch", "du", "bist", "f\u00fcr", "mei\u00b7nen", "Grimm", "zu", "klein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "PPOSAT", "NE", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ja, Friede ist's! Es w\u00e4lzen meine Fluthen", "tokens": ["Ja", ",", "Frie\u00b7de", "ist's", "!", "Es", "w\u00e4l\u00b7zen", "mei\u00b7ne", "Flut\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Nicht mehr der T\u00fcrken Aeser fort.", "tokens": ["Nicht", "mehr", "der", "T\u00fcr\u00b7ken", "A\u00b7e\u00b7ser", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Und keine Flotte schwimmt auf Meerschaum hin, um dort", "tokens": ["Und", "kei\u00b7ne", "Flot\u00b7te", "schwimmt", "auf", "Meer\u00b7schaum", "hin", ",", "um", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "KOUI", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Im Archipelagus zu donnern und zu bluten.", "tokens": ["Im", "Ar\u00b7chi\u00b7pe\u00b7la\u00b7gus", "zu", "don\u00b7nern", "und", "zu", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ha, Katharina siegt! Die G\u00f6ttin ist es werth!", "tokens": ["Ha", ",", "Ka\u00b7tha\u00b7ri\u00b7na", "siegt", "!", "Die", "G\u00f6t\u00b7tin", "ist", "es", "werth", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "H\u00f6rst du die Friedensjubel rasen?", "tokens": ["H\u00f6rst", "du", "die", "Frie\u00b7dens\u00b7ju\u00b7bel", "ra\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wohl dir, Tatar, nun kann dein Pferd", "tokens": ["Wohl", "dir", ",", "Ta\u00b7tar", ",", "nun", "kann", "dein", "Pferd"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "NN", "$,", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "So frei, wie du, auf sichern Steppen grasen.", "tokens": ["So", "frei", ",", "wie", "du", ",", "auf", "si\u00b7chern", "Step\u00b7pen", "gra\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Auf schwarzen Meere seh ich schon", "tokens": ["Auf", "schwar\u00b7zen", "Mee\u00b7re", "seh", "ich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Moskowiten Flaggen wehen.", "tokens": ["Der", "Mos\u00b7ko\u00b7wi\u00b7ten", "Flag\u00b7gen", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Der gro\u00dfe Peter scheint vom nahen Wolkenthron,", "tokens": ["Der", "gro\u00b7\u00dfe", "Pe\u00b7ter", "scheint", "vom", "na\u00b7hen", "Wol\u00b7ken\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als Genius, den Schwimmern zuzusehen.", "tokens": ["Als", "Ge\u00b7nius", ",", "den", "Schwim\u00b7mern", "zu\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Mit vorgepre\u00dftem Auge sieht Sultan", "tokens": ["Mit", "vor\u00b7ge\u00b7pre\u00df\u00b7tem", "Au\u00b7ge", "sieht", "Sul\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Den blassen Mond in Machmuths Fahne an", "tokens": ["Den", "blas\u00b7sen", "Mond", "in", "Mach\u00b7muths", "Fah\u00b7ne", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und h\u00f6rt auf ihren B\u00e4uchen,", "tokens": ["Und", "h\u00f6rt", "auf", "ih\u00b7ren", "B\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die \u00fcberwundne Schaar der Janitscharen keuchen.", "tokens": ["Die", "\u00fc\u00b7berw\u00b7und\u00b7ne", "Schaar", "der", "Ja\u00b7nit\u00b7scha\u00b7ren", "keu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Erdkreis schweigt: es stutzt der Ocean", "tokens": ["Der", "Erd\u00b7kreis", "schweigt", ":", "es", "stutzt", "der", "O\u00b7cean"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und alles weit umher staunt Katharinen an.", "tokens": ["Und", "al\u00b7les", "weit", "um\u00b7her", "staunt", "Ka\u00b7tha\u00b7ri\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "PTKVZ", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}