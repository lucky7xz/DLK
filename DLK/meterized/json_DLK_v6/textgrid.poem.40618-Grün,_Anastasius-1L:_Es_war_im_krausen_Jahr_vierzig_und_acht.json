{"textgrid.poem.40618": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war im krausen Jahr vierzig und acht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war im krausen Jahr vierzig und acht", "tokens": ["Es", "war", "im", "krau\u00b7sen", "Jahr", "vier\u00b7zig", "und", "acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "CARD", "KON", "CARD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das jenes Riesenfeuer angefacht,", "tokens": ["Das", "je\u00b7nes", "Rie\u00b7sen\u00b7feu\u00b7er", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Draus sich der Ph\u00f6nix Deutschland schwingen sollte;", "tokens": ["Draus", "sich", "der", "Ph\u00f6\u00b7nix", "Deutschland", "schwin\u00b7gen", "soll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "NN", "NE", "VVINF", "VMFIN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Doch wie die Lohe stieg, die Windsbraut grollte,", "tokens": ["Doch", "wie", "die", "Lo\u00b7he", "stieg", ",", "die", "Winds\u00b7braut", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die L\u00e4uterung, sie wollte noch nicht kommen,", "tokens": ["Die", "L\u00e4u\u00b7te\u00b7rung", ",", "sie", "woll\u00b7te", "noch", "nicht", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drob manches Herz, auch meines, tief beklommen.", "tokens": ["Drob", "man\u00b7ches", "Herz", ",", "auch", "mei\u00b7nes", ",", "tief", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PPOSAT", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Riesensturm, \u2013 der Stra\u00dfenstaub nur hasche?", "tokens": ["Ein", "Rie\u00b7sen\u00b7sturm", ",", "\u2013", "der", "Stra\u00b7\u00dfen\u00b7staub", "nur", "ha\u00b7sche", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Weltenbrand, \u2013 und all sein Rest nur Asche? \u2013", "tokens": ["Ein", "Wel\u00b7ten\u00b7brand", ",", "\u2013", "und", "all", "sein", "Rest", "nur", "A\u00b7sche", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "$(", "KON", "PIAT", "PPOSAT", "NN", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So vor mich sinnend war ich eingetreten", "tokens": ["So", "vor", "mich", "sin\u00b7nend", "war", "ich", "ein\u00b7ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVPP", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Dom der Helden, Weisen und Poeten.", "tokens": ["Im", "Dom", "der", "Hel\u00b7den", ",", "Wei\u00b7sen", "und", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Marmorboden gl\u00e4nzte blank und helle,", "tokens": ["Der", "Mar\u00b7mor\u00b7bo\u00b7den", "gl\u00e4nz\u00b7te", "blank", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein glattes Spiegelmeer, das zu befahren", "tokens": ["Ein", "glat\u00b7tes", "Spie\u00b7gel\u00b7meer", ",", "das", "zu", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein stattlich Schiffsgeschwader an der Schwelle", "tokens": ["Ein", "statt\u00b7lich", "Schiffs\u00b7ge\u00b7schwa\u00b7der", "an", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor Anker lag f\u00fcr die profanen Scharen;", "tokens": ["Vor", "An\u00b7ker", "lag", "f\u00fcr", "die", "pro\u00b7fa\u00b7nen", "Scha\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Pantoffel, Filzschuh, Wollgalloschen schienen", "tokens": ["Pan\u00b7tof\u00b7fel", ",", "Filz\u00b7schuh", ",", "Woll\u00b7gal\u00b7lo\u00b7schen", "schie\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Fregatt' und Slup hier, Brigg und Brigantinen;", "tokens": ["Fre\u00b7gatt'", "und", "Slup", "hier", ",", "Brigg", "und", "Bri\u00b7gan\u00b7ti\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "ADV", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der deutschen Flotte mocht' ich hoffend denken,", "tokens": ["Der", "deut\u00b7schen", "Flot\u00b7te", "mocht'", "ich", "hof\u00b7fend", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u2013 Jetzt m\u00fc\u00dft' in Wehmut ich das Auge senken. \u2013", "tokens": ["\u2013", "Jetzt", "m\u00fc\u00dft'", "in", "Weh\u00b7mut", "ich", "das", "Au\u00b7ge", "sen\u00b7ken", ".", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "APPR", "NN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mein Boot bestieg auch ich, wie's anbefohlen,", "tokens": ["Mein", "Boot", "be\u00b7stieg", "auch", "ich", ",", "wie's", "an\u00b7be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Behutsam steuert ich dahin und gr\u00fc\u00dfte", "tokens": ["Be\u00b7hut\u00b7sam", "steu\u00b7ert", "ich", "da\u00b7hin", "und", "gr\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bekannte H\u00e4upter rings auf den Konsolen;", "tokens": ["Be\u00b7kann\u00b7te", "H\u00e4up\u00b7ter", "rings", "auf", "den", "Kon\u00b7so\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Vertraut schien mir zu nicken manche B\u00fcste,", "tokens": ["Ver\u00b7traut", "schien", "mir", "zu", "ni\u00b7cken", "man\u00b7che", "B\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Befeuernd, tr\u00f6stend flo\u00df aus Marmormunde", "tokens": ["Be\u00b7feu\u00b7ernd", ",", "tr\u00f6s\u00b7tend", "flo\u00df", "aus", "Mar\u00b7mor\u00b7mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Noch manch unsterblich Wort, manch heil'ge Kunde.", "tokens": ["Noch", "manch", "uns\u00b7terb\u00b7lich", "Wort", ",", "manch", "heil'\u00b7ge", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich hielt das Auge mir gefangen", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "hielt", "das", "Au\u00b7ge", "mir", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bildni\u00df, nicht erhofft in dieser Runde,", "tokens": ["Ein", "Bild\u00b7ni\u00df", ",", "nicht", "er\u00b7hofft", "in", "die\u00b7ser", "Run\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Antlitz, drauf der M\u00f6nch und Krieger rangen,", "tokens": ["Ein", "Ant\u00b7litz", ",", "drauf", "der", "M\u00f6nch", "und", "Krie\u00b7ger", "ran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Prophetenstirne bei des Schalkes Wangen.", "tokens": ["Pro\u00b7phe\u00b7tens\u00b7tir\u00b7ne", "bei", "des", "Schal\u00b7kes", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich rief in Lust: \u00bbWillkommen, Gottwillkommen!", "tokens": ["Ich", "rief", "in", "Lust", ":", "\u00bb", "Will\u00b7kom\u00b7men", ",", "Gott\u00b7will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "$(", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ei, Doktor Martin, Fr\u00f6hlichster der Frommen,", "tokens": ["Ei", ",", "Dok\u00b7tor", "Mar\u00b7tin", ",", "Fr\u00f6h\u00b7lichs\u00b7ter", "der", "From\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Als ich hierher vor Jahren kam im Wandern", "tokens": ["Als", "ich", "hier\u00b7her", "vor", "Jah\u00b7ren", "kam", "im", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da irrtet Ihr noch vor dem Thor mit Andern,", "tokens": ["Da", "irr\u00b7tet", "Ihr", "noch", "vor", "dem", "Thor", "mit", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch wann? und wie? und welche der Walk\u00fcren", "tokens": ["Doch", "wann", "?", "und", "wie", "?", "und", "wel\u00b7che", "der", "Wal\u00b7k\u00fc\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "KON", "PWAV", "$.", "KON", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hat es gewagt, Euch in die\u00df Haus zu f\u00fchren?\u00ab", "tokens": ["Hat", "es", "ge\u00b7wagt", ",", "Euch", "in", "die\u00df", "Haus", "zu", "f\u00fch\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "APPR", "PDS", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da strich's um die olymp'sche Lutherstirne", "tokens": ["Da", "strich's", "um", "die", "o\u00b7lym\u00b7p'\u00b7sche", "Lu\u00b7ther\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie heitres L\u00e4cheln und wie milde Trauer,", "tokens": ["Wie", "heit\u00b7res", "L\u00e4\u00b7cheln", "und", "wie", "mil\u00b7de", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleichwie im Wechselspiel am Alpenfirne", "tokens": ["Gleich\u00b7wie", "im", "Wech\u00b7sel\u00b7spiel", "am", "Al\u00b7pen\u00b7fir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald Sonnenblicke ziehn, bald Regenschauer;", "tokens": ["Bald", "Son\u00b7nen\u00b7bli\u00b7cke", "ziehn", ",", "bald", "Re\u00b7gen\u00b7schau\u00b7er", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und also lie\u00df vernehmen sich die Stimme:", "tokens": ["Und", "al\u00b7so", "lie\u00df", "ver\u00b7neh\u00b7men", "sich", "die", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbes war zur Zeit, als schon in schw\u00e4cherm Grimme", "tokens": ["\u00bb", "es", "war", "zur", "Zeit", ",", "als", "schon", "in", "schw\u00e4\u00b7cherm", "Grim\u00b7me"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "APPRART", "NN", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Winter rang mit ersten Fr\u00fchlingsl\u00fcften,", "tokens": ["Der", "Win\u00b7ter", "rang", "mit", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da h\u00f6rt' ich donnernd \u00fcber unsern Gr\u00fcften", "tokens": ["Da", "h\u00f6rt'", "ich", "don\u00b7nernd", "\u00fc\u00b7ber", "un\u00b7sern", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Durch Deutschland hin ein Hochgewitter rollen,", "tokens": ["Durch", "Deutschland", "hin", "ein", "Hoch\u00b7ge\u00b7wit\u00b7ter", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gesang und Schwertgeklirr, Gejauchz' und Grollen:", "tokens": ["Ge\u00b7sang", "und", "Schwert\u00b7ge\u00b7klirr", ",", "Ge\u00b7jauch\u00b7z'", "und", "Grol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Des Rothbarts Stunde, dacht' ich, sei gekommen;", "tokens": ["Des", "Roth\u00b7barts", "Stun\u00b7de", ",", "dacht'", "ich", ",", "sei", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von Heimatdrang f\u00fchlt' ich mein Herz entglommen.", "tokens": ["Von", "Hei\u00b7mat\u00b7drang", "f\u00fchlt'", "ich", "mein", "Herz", "ent\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "wor