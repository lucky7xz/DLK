{"dta.poem.16315": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bist du gest\u00fcrzt und hat der Sturz dir nicht geschadet,", "tokens": ["Bist", "du", "ge\u00b7st\u00fcrzt", "und", "hat", "der", "Sturz", "dir", "nicht", "ge\u00b7scha\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denke: dismal hat der Himmel dir gegnadet.", "tokens": ["So", "den\u00b7ke", ":", "dis\u00b7mal", "hat", "der", "Him\u00b7mel", "dir", "ge\u00b7gna\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Gnade hast du nicht verdient, verdiene sie!", "tokens": ["Die", "Gna\u00b7de", "hast", "du", "nicht", "ver\u00b7dient", ",", "ver\u00b7die\u00b7ne", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steh auf mit Zuversicht und falle nie mehr, nie!", "tokens": ["Steh", "auf", "mit", "Zu\u00b7ver\u00b7sicht", "und", "fal\u00b7le", "nie", "mehr", ",", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "KON", "VVFIN", "AD