{"dta.poem.6686": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch mitten in der sch\u00f6nsten Melodey,", "tokens": ["Doch", "mit\u00b7ten", "in", "der", "sch\u00f6ns\u00b7ten", "Me\u00b7lo\u00b7dey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die m\u00e4chtig, uns die Sinnen zu entf\u00fchren,", "tokens": ["Die", "m\u00e4ch\u00b7tig", ",", "uns", "die", "Sin\u00b7nen", "zu", "ent\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kan dieses uns am kr\u00e4fftigsten probiren,", "tokens": ["Kan", "die\u00b7ses", "uns", "am", "kr\u00e4ff\u00b7tigs\u00b7ten", "pro\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "PPER", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df unsre Seele doch allein nur Meister sey.", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "doch", "al\u00b7lein", "nur", "Meis\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie kan sich selbst von der Betrachtung lencken,", "tokens": ["Sie", "kan", "sich", "selbst", "von", "der", "Be\u00b7trach\u00b7tung", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie kan auf etwas anders dencken.", "tokens": ["Sie", "kan", "auf", "et\u00b7was", "an\u00b7ders", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch eben den Gesang, der durch die Munterkeit", "tokens": ["Durch", "e\u00b7ben", "den", "Ge\u00b7sang", ",", "der", "durch", "die", "Mun\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch Lust und Munterkeit in ihr erregt,", "tokens": ["Auch", "Lust", "und", "Mun\u00b7ter\u00b7keit", "in", "ihr", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wird sie zur anderen, auf andre Art bewegt.", "tokens": ["Wird", "sie", "zur", "an\u00b7de\u00b7ren", ",", "auf", "and\u00b7re", "Art", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Ton, der einmal uns gefiel, kan uns mi\u00dffallen.", "tokens": ["Der", "Ton", ",", "der", "ein\u00b7mal", "uns", "ge\u00b7fiel", ",", "kan", "uns", "mi\u00df\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "L\u00e4sst eine Frau, l\u00e4sst eine Mutter Klagen", "tokens": ["L\u00e4sst", "ei\u00b7ne", "Frau", ",", "l\u00e4sst", "ei\u00b7ne", "Mut\u00b7ter", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Bey des Geliebten Sarg erschallen;", "tokens": ["Bey", "des", "Ge\u00b7lieb\u00b7ten", "Sarg", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.13": {"text": "So mehrt die fr\u00f6lichste Music derselben Plagen:", "tokens": ["So", "mehrt", "die", "fr\u00f6\u00b7lichs\u00b7te", "Mu\u00b7sic", "der\u00b7sel\u00b7ben", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sodann kan nichts als Schluchzen, Seuffzen, Heulen", "tokens": ["So\u00b7dann", "kan", "nichts", "als", "Schluch\u00b7zen", ",", "Seuff\u00b7zen", ",", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PIS", "KOKOM", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Derselben Trost ertheilen.", "tokens": ["Der\u00b7sel\u00b7ben", "Trost", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}