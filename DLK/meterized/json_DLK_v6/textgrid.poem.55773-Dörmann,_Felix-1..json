{"textgrid.poem.55773": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ruht auf mir meiner S\u00fcnden Zicht,", "tokens": ["Noch", "ruht", "auf", "mir", "mei\u00b7ner", "S\u00fcn\u00b7den", "Zicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Fackel der Freiheit, noch flammt sie mir nicht,", "tokens": ["Die", "Fa\u00b7ckel", "der", "Frei\u00b7heit", ",", "noch", "flammt", "sie", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Noch klirren die Ketten mir mahnend am Fu\u00df,", "tokens": ["Noch", "klir\u00b7ren", "die", "Ket\u00b7ten", "mir", "mah\u00b7nend", "am", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch peitscht mich der Br\u00fcder ver\u00e4chtlicher Gru\u00df,", "tokens": ["Noch", "peitscht", "mich", "der", "Br\u00fc\u00b7der", "ver\u00b7\u00e4cht\u00b7li\u00b7cher", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Noch scheut der Geringste den Druck meiner Hand,", "tokens": ["Noch", "scheut", "der", "Ge\u00b7rings\u00b7te", "den", "Druck", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch bin ich versto\u00dfen, vervehmt und verbannt:", "tokens": ["Noch", "bin", "ich", "ver\u00b7sto\u00b7\u00dfen", ",", "ver\u00b7vehmt", "und", "ver\u00b7bannt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "So war meine Reue noch z\u00e4rtlich und weich,", "tokens": ["So", "war", "mei\u00b7ne", "Reu\u00b7e", "noch", "z\u00e4rt\u00b7lich", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So war meine Bu\u00dfe noch schw\u00e4chlich und bleich:", "tokens": ["So", "war", "mei\u00b7ne", "Bu\u00b7\u00dfe", "noch", "schw\u00e4ch\u00b7lich", "und", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Wohlan! so verh\u00e4ngt mir die zehnfache Pein, \u2013", "tokens": ["Wo\u00b7hlan", "!", "so", "ver\u00b7h\u00e4ngt", "mir", "die", "zehn\u00b7fa\u00b7che", "Pein", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich will sie ertragen und dankbar sein.", "tokens": ["Ich", "will", "sie", "er\u00b7tra\u00b7gen", "und", "dank\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "ADJD", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}