{"textgrid.poem.40677": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dunkeln mu\u00df der Himmel rings im Runde,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunkeln mu\u00df der Himmel rings im Runde,", "tokens": ["Dun\u00b7keln", "mu\u00df", "der", "Him\u00b7mel", "rings", "im", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df sein Sternenglanz zu leuchten wage;", "tokens": ["Da\u00df", "sein", "Ster\u00b7nen\u00b7glanz", "zu", "leuch\u00b7ten", "wa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "St\u00fcrmen mu\u00df das Meer bis tief zum Grunde,", "tokens": ["St\u00fcr\u00b7men", "mu\u00df", "das", "Meer", "bis", "tief", "zum", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "APPR", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ans Land es seine Perlen trage;", "tokens": ["Da\u00df", "ans", "Land", "es", "sei\u00b7ne", "Per\u00b7len", "tra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Klaffen mu\u00df des Berges offne Wunde,", "tokens": ["Klaf\u00b7fen", "mu\u00df", "des", "Ber\u00b7ges", "off\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df sein Goldgehalt ersteh' zu Tage;", "tokens": ["Da\u00df", "sein", "Gold\u00b7ge\u00b7halt", "er\u00b7steh'", "zu", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dunkle Stunden m\u00fcssen offenbaren,", "tokens": ["Dunk\u00b7le", "Stun\u00b7den", "m\u00fcs\u00b7sen", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Was ein Herz des Gro\u00dfen birgt und Klaren.", "tokens": ["Was", "ein", "Herz", "des", "Gro\u00b7\u00dfen", "birgt", "und", "Kla\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}