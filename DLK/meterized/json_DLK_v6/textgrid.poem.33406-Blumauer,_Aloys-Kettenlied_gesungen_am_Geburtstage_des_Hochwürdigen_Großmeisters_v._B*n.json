{"textgrid.poem.33406": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Kettenlied gesungen am Geburtstage des Hochw\u00fcrdigen Gro\u00dfmeisters v. B*n", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Br\u00fcder, und reicht euch die H\u00e4nde!", "tokens": ["Auf", "Br\u00fc\u00b7der", ",", "und", "reicht", "euch", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein R\u00e4thsel ist Anfang und Ende", "tokens": ["Ein", "R\u00e4th\u00b7sel", "ist", "An\u00b7fang", "und", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "An unserer Kette dem Blick;", "tokens": ["An", "un\u00b7se\u00b7rer", "Ket\u00b7te", "dem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Von Osten aus gehen die Glieder,", "tokens": ["Von", "Os\u00b7ten", "aus", "ge\u00b7hen", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kehren im Kreise dann wieder", "tokens": ["Und", "keh\u00b7ren", "im", "Krei\u00b7se", "dann", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Von Westen nach Osten zur\u00fcck.", "tokens": ["Von", "Wes\u00b7ten", "nach", "Os\u00b7ten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Doch h\u00e4ngen wir alle im Kreise", "tokens": ["Doch", "h\u00e4n\u00b7gen", "wir", "al\u00b7le", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "An Einem, der liebend und weise", "tokens": ["An", "Ei\u00b7nem", ",", "der", "lie\u00b7bend", "und", "wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Uns Br\u00fcder versammelt umher:", "tokens": ["Uns", "Br\u00fc\u00b7der", "ver\u00b7sam\u00b7melt", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Und wenn wir die Kette dann schlingen,", "tokens": ["Und", "wenn", "wir", "die", "Ket\u00b7te", "dann", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "So seh'n wir nach Osten, und singen:", "tokens": ["So", "seh'n", "wir", "nach", "Os\u00b7ten", ",", "und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Ihr Anfang und Ende ist \u2013 Er!", "tokens": ["Ihr", "An\u00b7fang", "und", "En\u00b7de", "ist", "\u2013", "Er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "$(", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}