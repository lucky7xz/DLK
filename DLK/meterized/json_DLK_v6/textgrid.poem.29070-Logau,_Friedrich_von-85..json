{"textgrid.poem.29070": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sagt, die Welt sey falsch? Hierzu geh\u00f6rt Beweis;", "tokens": ["Wer", "sagt", ",", "die", "Welt", "sey", "falsch", "?", "Hier\u00b7zu", "ge\u00b7h\u00f6rt", "Be\u00b7weis", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$.", "PAV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist Cristallen-rein und meistens ietzund wei\u00df.", "tokens": ["Sie", "ist", "Cris\u00b7tal\u00b7len\u00b7rein", "und", "meis\u00b7tens", "ie\u00b7tzund", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}