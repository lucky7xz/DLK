{"textgrid.poem.60260": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings Tod", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum, o L\u00fcfte, fl\u00fcstert ihr so bang?", "tokens": ["Wa\u00b7rum", ",", "o", "L\u00fcf\u00b7te", ",", "fl\u00fcs\u00b7tert", "ihr", "so", "bang", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle Haine weht die Trauerkunde,", "tokens": ["Durch", "al\u00b7le", "Hai\u00b7ne", "weht", "die", "Trau\u00b7er\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00f6rrisch klagt der tr\u00fcben Welle Gang:", "tokens": ["Und", "st\u00f6r\u00b7risch", "klagt", "der", "tr\u00fc\u00b7ben", "Wel\u00b7le", "Gang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist des holden Fr\u00fchlings Todesstunde!", "tokens": ["Das", "ist", "des", "hol\u00b7den", "Fr\u00fch\u00b7lings", "To\u00b7dess\u00b7tun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Himmel, finster und gewitterschw\u00fcl,", "tokens": ["Der", "Him\u00b7mel", ",", "fins\u00b7ter", "und", "ge\u00b7wit\u00b7ter\u00b7schw\u00fcl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umh\u00fcllt sich tief, da\u00df er sein Leid verhehle,", "tokens": ["Um\u00b7h\u00fcllt", "sich", "tief", ",", "da\u00df", "er", "sein", "Leid", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und an des Lenzes gr\u00fcnem Sterbepf\u00fchl", "tokens": ["Und", "an", "des", "Len\u00b7zes", "gr\u00fc\u00b7nem", "Ster\u00b7be\u00b7pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weint noch sein Kind, sein liebstes, Philomele.", "tokens": ["Weint", "noch", "sein", "Kind", ",", "sein", "liebs\u00b7tes", ",", "Phi\u00b7lo\u00b7me\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn so der Lenz frohlocket, schmerzlich ahnt", "tokens": ["Wenn", "so", "der", "Lenz", "froh\u00b7lo\u00b7cket", ",", "schmerz\u00b7lich", "ahnt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Herz sein Paradies, das uns verloren,", "tokens": ["Das", "Herz", "sein", "Pa\u00b7ra\u00b7dies", ",", "das", "uns", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und weil er uns zu laut daran gemahnt,", "tokens": ["Und", "weil", "er", "uns", "zu", "laut", "da\u00b7ran", "ge\u00b7mahnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKA", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00dft ihn der hei\u00dfe Sonnenpfeil durchbohren.", "tokens": ["Mu\u00dft", "ihn", "der", "hei\u00b7\u00dfe", "Son\u00b7nen\u00b7pfeil", "durch\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Himmel blitzt, und Donnerwolken fliehn,", "tokens": ["Der", "Him\u00b7mel", "blitzt", ",", "und", "Don\u00b7ner\u00b7wol\u00b7ken", "fliehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die lauten St\u00fcrme durch die Haine tosen;", "tokens": ["Die", "lau\u00b7ten", "St\u00fcr\u00b7me", "durch", "die", "Hai\u00b7ne", "to\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch l\u00e4chelnd stirbt der holde Lenz dahin,", "tokens": ["Doch", "l\u00e4\u00b7chelnd", "stirbt", "der", "hol\u00b7de", "Lenz", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Herzblut still verstr\u00f6mend, seine Rosen.", "tokens": ["Sein", "Herz\u00b7blut", "still", "ver\u00b7str\u00f6\u00b7mend", ",", "sei\u00b7ne", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}