{"textgrid.poem.58919": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "138. Der beste Handel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem, der sich bek\u00fcmmert nicht", "tokens": ["Wohl", "dem", ",", "der", "sich", "be\u00b7k\u00fcm\u00b7mert", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um das, was hier und da geschicht,", "tokens": ["Um", "das", ",", "was", "hier", "und", "da", "ge\u00b7schicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "$,", "PRELS", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mit sich selbst und Gott nur handelt,", "tokens": ["Der", "mit", "sich", "selbst", "und", "Gott", "nur", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Geist vor dessen Augen wandelt!", "tokens": ["Im", "Geist", "vor", "des\u00b7sen", "Au\u00b7gen", "wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}