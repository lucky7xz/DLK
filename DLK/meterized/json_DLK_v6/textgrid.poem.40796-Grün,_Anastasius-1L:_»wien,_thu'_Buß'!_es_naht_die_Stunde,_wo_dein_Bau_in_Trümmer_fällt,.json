{"textgrid.poem.40796": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwien, thu' Bu\u00df'! es naht die Stunde, wo dein Bau in Tr\u00fcmmer f\u00e4llt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwien, thu' Bu\u00df'! es naht die Stunde, wo dein Bau in Tr\u00fcmmer f\u00e4llt,", "tokens": ["\u00bb", "wi\u00b7en", ",", "thu'", "Bu\u00df'", "!", "es", "naht", "die", "Stun\u00b7de", ",", "wo", "dein", "Bau", "in", "Tr\u00fcm\u00b7mer", "f\u00e4llt", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Deine Zinnen gleich der Erde und kein Stein am andern h\u00e4lt!\u00ab", "tokens": ["Dei\u00b7ne", "Zin\u00b7nen", "gleich", "der", "Er\u00b7de", "und", "kein", "Stein", "am", "an\u00b7dern", "h\u00e4lt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "KON", "PIAT", "NN", "APPRART", "ADJA", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also rief ein Mann am Marktplatz, und wir lachten laut ihn aus,", "tokens": ["Al\u00b7so", "rief", "ein", "Mann", "am", "Markt\u00b7platz", ",", "und", "wir", "lach\u00b7ten", "laut", "ihn", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber den Propheten sperrte eilend man ins Narrenhaus.", "tokens": ["A\u00b7ber", "den", "Pro\u00b7phe\u00b7ten", "sperr\u00b7te", "ei\u00b7lend", "man", "ins", "Nar\u00b7ren\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PIS", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Doch bei stiller Nacht umwogte mir das Aug' ein seltner Traum:", "tokens": ["Doch", "bei", "stil\u00b7ler", "Nacht", "um\u00b7wog\u00b7te", "mir", "das", "Aug'", "ein", "selt\u00b7ner", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ich bewohnt' auf einem Berge einer H\u00fctte d\u00fcrft'gen Raum,", "tokens": ["Ich", "be\u00b7wohnt'", "auf", "ei\u00b7nem", "Ber\u00b7ge", "ei\u00b7ner", "H\u00fct\u00b7te", "d\u00fcrft'\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mir zu F\u00fc\u00dfen weithin dehnte sich die Kaiserstadt umher,", "tokens": ["Mir", "zu", "F\u00fc\u00b7\u00dfen", "weit\u00b7hin", "dehn\u00b7te", "sich", "die", "Kai\u00b7ser\u00b7stadt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch in Schutt und Staub zerfallen, ein gewalt'ges Tr\u00fcmmermeer!", "tokens": ["Doch", "in", "Schutt", "und", "Staub", "zer\u00b7fal\u00b7len", ",", "ein", "ge\u00b7walt'\u00b7ges", "Tr\u00fcm\u00b7mer\u00b7meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Horch, an meine stille Pforte pocht des Fremdlings Schaulust an,", "tokens": ["Horch", ",", "an", "mei\u00b7ne", "stil\u00b7le", "Pfor\u00b7te", "pocht", "des", "Fremd\u00b7lings", "Schau\u00b7lust", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df ich ihr, f\u00fcr d\u00fcrft'ge Gabe, F\u00fchrer durch die Tr\u00fcmmerbahn,", "tokens": ["Da\u00df", "ich", "ihr", ",", "f\u00fcr", "d\u00fcrft'\u00b7ge", "Ga\u00b7be", ",", "F\u00fch\u00b7rer", "durch", "die", "Tr\u00fcm\u00b7mer\u00b7bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Deuter sei zerfallner Gr\u00f6\u00dfe, wo ein jeder Stein und Staub", "tokens": ["Deu\u00b7ter", "sei", "zer\u00b7fall\u00b7ner", "Gr\u00f6\u00b7\u00dfe", ",", "wo", "ein", "je\u00b7der", "Stein", "und", "Staub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,", "PWAV", "ART", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mahnend spricht von sch\u00f6nen Tagen, wie vom Lenz das d\u00fcrre Laub. \u2013", "tokens": ["Mah\u00b7nend", "spricht", "von", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ",", "wie", "vom", "Lenz", "das", "d\u00fcr\u00b7re", "Laub", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Herr, gebt Acht, da\u00df eine Schlange pl\u00f6tzlich aus dem Schutt nicht blitzt!", "tokens": ["Herr", ",", "gebt", "Acht", ",", "da\u00df", "ei\u00b7ne", "Schlan\u00b7ge", "pl\u00f6tz\u00b7lich", "aus", "dem", "Schutt", "nicht", "blitzt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "CARD", "$,", "KOUS", "ART", "NN", "ADJD", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seht euch vor, da\u00df ihr die Glieder nicht am Dorngestr\u00fcpp' dort ritzt!", "tokens": ["Seht", "euch", "vor", ",", "da\u00df", "ihr", "die", "Glie\u00b7der", "nicht", "am", "Dorn\u00b7ge\u00b7str\u00fcpp'", "dort", "ritzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Reicht mir jetzt die Hand! Beschwerlich steigt durchs Schuttger\u00f6ll sich's hier!", "tokens": ["Reicht", "mir", "jetzt", "die", "Hand", "!", "Be\u00b7schwer\u00b7lich", "steigt", "durchs", "Schutt\u00b7ge\u00b7r\u00f6ll", "sich's", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ADJD", "VVFIN", "APPRART", "NN", "NE", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auf dem Tr\u00fcmmerh\u00fcgel finden doch ein Bischen Aussicht wir!", "tokens": ["Auf", "dem", "Tr\u00fcm\u00b7mer\u00b7h\u00fc\u00b7gel", "fin\u00b7den", "doch", "ein", "Bi\u00b7schen", "Aus\u00b7sicht", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Seht euch um, ob's einem Buche hoher Psalmen hier nicht gleicht,", "tokens": ["Seht", "euch", "um", ",", "ob's", "ei\u00b7nem", "Bu\u00b7che", "ho\u00b7her", "Psal\u00b7men", "hier", "nicht", "gleicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dran die Zeit das Blatt zermorschte und die ganze Schrift gebleicht,", "tokens": ["Dran", "die", "Zeit", "das", "Blatt", "zer\u00b7morschte", "und", "die", "gan\u00b7ze", "Schrift", "ge\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Hier und dort nur blieben W\u00e4nde, wie manch einzeln lesbar Wort,", "tokens": ["Hier", "und", "dort", "nur", "blie\u00b7ben", "W\u00e4n\u00b7de", ",", "wie", "manch", "ein\u00b7zeln", "les\u00b7bar", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADV", "VVFIN", "NN", "$,", "PWAV", "PIAT", "ADJD", "ADJD", "NN", "$,"], "meter": "--+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und gleichwie ein einzler Buchstab' eine S\u00e4ule hier und dort.", "tokens": ["Und", "gleich\u00b7wie", "ein", "einz\u00b7ler", "Buch\u00b7stab'", "ei\u00b7ne", "S\u00e4u\u00b7le", "hier", "und", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ART", "ADJA", "NN", "ART", "NN", "ADV", "KON", "ADV", "$."], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}}, "stanza.6": {"line.1": {"text": "Rathet doch, wo jetzt wir stehen? \u2013 Ei nun, auf dem Stephansthurm!", "tokens": ["Ra\u00b7thet", "doch", ",", "wo", "jetzt", "wir", "ste\u00b7hen", "?", "\u2013", "Ei", "nun", ",", "auf", "dem", "Ste\u00b7phanst\u00b7hurm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$.", "$(", "NN", "ADV", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Von der hohen Himmelspappel, die gef\u00e4llt der grimme Sturm,", "tokens": ["Von", "der", "ho\u00b7hen", "Him\u00b7mel\u00b7spap\u00b7pel", ",", "die", "ge\u00b7f\u00e4llt", "der", "grim\u00b7me", "Sturm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ist's zwar nur der niedre Strunk noch, der im Boden wurzelnd steht;", "tokens": ["Ist's", "zwar", "nur", "der", "nied\u00b7re", "Strunk", "noch", ",", "der", "im", "Bo\u00b7den", "wur\u00b7zelnd", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "ADV", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn der Stamm, die Zweig' und Bl\u00e4tter liegen rings als Schutt ges\u00e4t!", "tokens": ["Denn", "der", "Stamm", ",", "die", "Zweig'", "und", "Bl\u00e4t\u00b7ter", "lie\u00b7gen", "rings", "als", "Schutt", "ge\u00b7s\u00e4t", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Schlank und stolz einst, wie die Pappel, stieg in Wolken er hinein,", "tokens": ["Schlank", "und", "stolz", "einst", ",", "wie", "die", "Pap\u00b7pel", ",", "stieg", "in", "Wol\u00b7ken", "er", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Leichtes Ast- und Laubwerk formte Menschengeist aus spr\u00f6dem Stein!", "tokens": ["Leich\u00b7tes", "Ast", "und", "Laub\u00b7werk", "form\u00b7te", "Men\u00b7schen\u00b7geist", "aus", "spr\u00f6\u00b7dem", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "TRUNC", "KON", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O wie zwischen Zweig' und Bl\u00e4ttern, hoch mit lautem hellem Schall", "tokens": ["O", "wie", "zwi\u00b7schen", "Zweig'", "und", "Bl\u00e4t\u00b7tern", ",", "hoch", "mit", "lau\u00b7tem", "hel\u00b7lem", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "APPR", "NN", "KON", "NN", "$,", "ADJD", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Oben die gewalt'ge Glocke schlug als Riesennachtigall!", "tokens": ["O\u00b7ben", "die", "ge\u00b7walt'\u00b7ge", "Glo\u00b7cke", "schlug", "als", "Rie\u00b7sen\u00b7nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Seht den Stein, bemoost am Boden! Wer wohl n\u00e4hm' an ihm es wahr,", "tokens": ["Seht", "den", "Stein", ",", "be\u00b7moost", "am", "Bo\u00b7den", "!", "Wer", "wohl", "n\u00e4hm'", "an", "ihm", "es", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$.", "PWS", "ADV", "VVFIN", "APPR", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df er Bruderschaft und Zwiesprach hielt in L\u00fcften mit dem Aar!", "tokens": ["Da\u00df", "er", "Bru\u00b7der\u00b7schaft", "und", "Zwie\u00b7sprach", "hielt", "in", "L\u00fcf\u00b7ten", "mit", "dem", "Aar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch im Raum noch, wo der Aether tausend Jahr' fast nicht gekreist,", "tokens": ["Doch", "im", "Raum", "noch", ",", "wo", "der", "A\u00b7e\u00b7ther", "tau\u00b7send", "Jahr'", "fast", "nicht", "ge\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$,", "PWAV", "ART", "NN", "CARD", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "--+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ragt als leise licht're S\u00e4ule, sichtbar kaum, des Thurmes Geist! \u2013", "tokens": ["Ragt", "als", "lei\u00b7se", "licht'\u00b7re", "S\u00e4u\u00b7le", ",", "sicht\u00b7bar", "kaum", ",", "des", "Thur\u00b7mes", "Geist", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KOKOM", "ADJD", "ADJA", "NN", "$,", "ADJD", "ADV", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Hebt empor euch auf den Zehen! K\u00f6nnt ihr jene Eichen sehn,", "tokens": ["Hebt", "em\u00b7por", "euch", "auf", "den", "Ze\u00b7hen", "!", "K\u00f6nnt", "ihr", "je\u00b7ne", "Ei\u00b7chen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "APPR", "ART", "CARD", "$.", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die wie Reihn von Grenadieren jenseits an der Donau stehn?", "tokens": ["Die", "wie", "Reihn", "von", "Gre\u00b7na\u00b7die\u00b7ren", "jen\u00b7seits", "an", "der", "Do\u00b7nau", "stehn", "?"], "tok