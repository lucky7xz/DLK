{"textgrid.poem.41324": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsichtvollen", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt ein Volk, das immer lernen sollte,", "tokens": ["Es", "gibt", "ein", "Volk", ",", "das", "im\u00b7mer", "ler\u00b7nen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und immer lehrt.", "tokens": ["Und", "im\u00b7mer", "lehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das ist das Volk, das man nie h\u00f6ren wollte,", "tokens": ["Das", "ist", "das", "Volk", ",", "das", "man", "nie", "h\u00f6\u00b7ren", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und t\u00e4glich h\u00f6rt.", "tokens": ["Und", "t\u00e4g\u00b7lich", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}