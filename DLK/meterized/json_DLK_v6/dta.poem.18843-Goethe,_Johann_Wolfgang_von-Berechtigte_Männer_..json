{"dta.poem.18843": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Berechtigte M\u00e4nner .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Seine Todten mag der Feind betrauern:", "tokens": ["Sei\u00b7ne", "Tod\u00b7ten", "mag", "der", "Feind", "be\u00b7trau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn sie liegen ohne Wiederkehren;", "tokens": ["Denn", "sie", "lie\u00b7gen", "oh\u00b7ne", "Wie\u00b7der\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unsre Br\u00fcder sollt ihr nicht bedauern:", "tokens": ["Uns\u00b7re", "Br\u00fc\u00b7der", "sollt", "ihr", "nicht", "be\u00b7dau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn sie wandeln \u00fcber jenen Sph\u00e4ren.", "tokens": ["Denn", "sie", "wan\u00b7deln", "\u00fc\u00b7ber", "je\u00b7nen", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Die Planeten haben alle sieben", "tokens": ["Die", "Pla\u00b7ne\u00b7ten", "ha\u00b7ben", "al\u00b7le", "sie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "CARD"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die metallnen Thore weit gethan,", "tokens": ["Die", "me\u00b7tall\u00b7nen", "Tho\u00b7re", "weit", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und schon klopfen die verkl\u00e4rten Lieben", "tokens": ["Und", "schon", "klop\u00b7fen", "die", "ver\u00b7kl\u00e4r\u00b7ten", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Paradieses Pforten k\u00fchnlich an.", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses", "Pfor\u00b7ten", "k\u00fchn\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Finden, ungehofft und \u00fcbergl\u00fccklich,", "tokens": ["Fin\u00b7den", ",", "un\u00b7ge\u00b7hofft", "und", "\u00fc\u00b7berg\u00b7l\u00fcck\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herrlichkeiten, die mein Flug ber\u00fchrt,", "tokens": ["Herr\u00b7lich\u00b7kei\u00b7ten", ",", "die", "mein", "Flug", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als das Wunderpferd mich augenblicklich", "tokens": ["Als", "das", "Wun\u00b7der\u00b7pferd", "mich", "au\u00b7gen\u00b7blick\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Himmel alle durchgef\u00fchrt.", "tokens": ["Durch", "die", "Him\u00b7mel", "al\u00b7le", "durch\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Weisheitsbaum an Baum cypresseragend", "tokens": ["Weis\u00b7heits\u00b7baum", "an", "Baum", "cyp\u00b7res\u00b7se\u00b7ra\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heben Aepfel goldner Zierd empor,", "tokens": ["He\u00b7ben", "A\u00b7e\u00b7pfel", "gold\u00b7ner", "Zierd", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lebensb\u00e4ume breite Schatten schlagend", "tokens": ["Le\u00b7bens\u00b7b\u00e4u\u00b7me", "brei\u00b7te", "Schat\u00b7ten", "schla\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Decken Blumensitz und Kr\u00e4uter-Flor.", "tokens": ["De\u00b7cken", "Blu\u00b7men\u00b7sitz", "und", "Kr\u00e4u\u00b7ter\u00b7Flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und nun bringt ein s\u00fcsser Wind von Osten", "tokens": ["Und", "nun", "bringt", "ein", "s\u00fcs\u00b7ser", "Wind", "von", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hergef\u00fchrt die Himmels-M\u00e4dchen-Schaar;", "tokens": ["Her\u00b7ge\u00b7f\u00fchrt", "die", "Him\u00b7mels\u00b7M\u00e4d\u00b7chen\u00b7Schaar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den Augen f\u00e4ngst du an zu kosten,", "tokens": ["Mit", "den", "Au\u00b7gen", "f\u00e4ngst", "du", "an", "zu", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schon der Anblick s\u00e4ttigt ganz und gar.", "tokens": ["Schon", "der", "An\u00b7blick", "s\u00e4t\u00b7tigt", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Forschend stehn sie was du unternahmest?", "tokens": ["For\u00b7schend", "stehn", "sie", "was", "du", "un\u00b7ter\u00b7nah\u00b7mest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Grosse Plane? f\u00e4hrlich blutigen Straus?", "tokens": ["Gros\u00b7se", "Pla\u00b7ne", "?", "f\u00e4hr\u00b7lich", "blu\u00b7ti\u00b7gen", "Straus", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dass du Held seyst sehn sie, weil du kamest;", "tokens": ["Dass", "du", "Held", "seyst", "sehn", "sie", ",", "weil", "du", "ka\u00b7mest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Welch ein Held du seyst? sie forschen\u2019s aus.", "tokens": ["Welch", "ein", "Held", "du", "seyst", "?", "sie", "for\u00b7schen's", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VAFIN", "$.", "PPER", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und sie sehn es bald an deinen Wunden,", "tokens": ["Und", "sie", "sehn", "es", "bald", "an", "dei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die sich selbst ein Ehrendenkmal schreibt.", "tokens": ["Die", "sich", "selbst", "ein", "Eh\u00b7ren\u00b7denk\u00b7mal", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gl\u00fcck und Hoheit alles ist verschwunden,", "tokens": ["Gl\u00fcck", "und", "Ho\u00b7heit", "al\u00b7les", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur die Wunde f\u00fcr den Glauben bleibt.", "tokens": ["Nur", "die", "Wun\u00b7de", "f\u00fcr", "den", "Glau\u00b7ben", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "F\u00fchren zu Chiosken dich und Lauben,", "tokens": ["F\u00fch\u00b7ren", "zu", "Chi\u00b7os\u00b7ken", "dich", "und", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00e4ulenreich von buntem Lichtgestein,", "tokens": ["\u00e4u\u00b7len\u00b7reich", "von", "bun\u00b7tem", "Licht\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und zum edlen Saft verkl\u00e4rter Trauben", "tokens": ["Und", "zum", "ed\u00b7len", "Saft", "ver\u00b7kl\u00e4r\u00b7ter", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Laden sie mit Nippen freundlich ein.", "tokens": ["La\u00b7den", "sie", "mit", "Nip\u00b7pen", "freund\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "J\u00fcngling! mehr als J\u00fcngling bist willkommen!", "tokens": ["J\u00fcng\u00b7ling", "!", "mehr", "als", "J\u00fcng\u00b7ling", "bist", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "KOUS", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle sind wie alle licht und klar;", "tokens": ["Al\u00b7le", "sind", "wie", "al\u00b7le", "licht", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "PIAT", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hast du Eine dir an\u2019s Herz genommen;", "tokens": ["Hast", "du", "Ei\u00b7ne", "dir", "an's", "Herz", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Herrinn, Freundinn ist sie deiner Schaar.", "tokens": ["Her\u00b7rinn", ",", "Freun\u00b7dinn", "ist", "sie", "dei\u00b7ner", "Schaar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Doch die allertrefflichste gef\u00e4llt sich", "tokens": ["Doch", "die", "al\u00b7ler\u00b7treff\u00b7lichs\u00b7te", "ge\u00b7f\u00e4llt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PRF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keineswegs in solchen Herrlichkeiten,", "tokens": ["Kei\u00b7nes\u00b7wegs", "in", "sol\u00b7chen", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heiter, neidlos, redlich unterh\u00e4lt dich", "tokens": ["Hei\u00b7ter", ",", "neid\u00b7los", ",", "red\u00b7lich", "un\u00b7ter\u00b7h\u00e4lt", "dich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von den mannigfalt\u2019gen andrer Trefflichkeiten.", "tokens": ["Von", "den", "man\u00b7nig\u00b7falt'\u00b7gen", "an\u00b7drer", "Treff\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Eine f\u00fchrt dich zu der andern Schmause,", "tokens": ["Ei\u00b7ne", "f\u00fchrt", "dich", "zu", "der", "an\u00b7dern", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den sich jede \u00e4usserst ausersinnt.", "tokens": ["Den", "sich", "je\u00b7de", "\u00e4us\u00b7serst", "aus\u00b7er\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Viele Frauen hast\u2019 und Ruh im Hause,", "tokens": ["Vie\u00b7le", "Frau\u00b7en", "hast'", "und", "Ruh", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Werth dass man darob das Paradies gewinnt.", "tokens": ["Werth", "dass", "man", "da\u00b7rob", "das", "Pa\u00b7ra\u00b7dies", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.12": {"line.1": {"text": "Und so schicke dich in diesen Frieden:", "tokens": ["Und", "so", "schi\u00b7cke", "dich", "in", "die\u00b7sen", "Frie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn du kannst ihn weiter nicht vertauschen;", "tokens": ["Denn", "du", "kannst", "ihn", "wei\u00b7ter", "nicht", "ver\u00b7tau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Solche M\u00e4dchen werden nicht erm\u00fcden,", "tokens": ["Sol\u00b7che", "M\u00e4d\u00b7chen", "wer\u00b7den", "nicht", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Solche Weine werden nicht berauschen.", "tokens": ["Sol\u00b7che", "Wei\u00b7ne", "wer\u00b7den", "nicht", "be\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Und so war das Wenige zu melden", "tokens": ["Und", "so", "war", "das", "We\u00b7ni\u00b7ge", "zu", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "PIAT", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie der sel\u2019ge Musulman sich br\u00fcstet.", "tokens": ["Wie", "der", "sel'\u00b7ge", "Mu\u00b7sul\u00b7man", "sich", "br\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Paradies der M\u00e4nner Glaubenshelden", "tokens": ["Pa\u00b7ra\u00b7dies", "der", "M\u00e4n\u00b7ner", "G