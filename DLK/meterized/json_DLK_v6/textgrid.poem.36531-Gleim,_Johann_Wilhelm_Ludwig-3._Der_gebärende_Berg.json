{"textgrid.poem.36531": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. Der geb\u00e4rende Berg", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Berg, der seines Leibes B\u00fcrde", "tokens": ["Ein", "Berg", ",", "der", "sei\u00b7nes", "Lei\u00b7bes", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geb\u00e4ren wollte, krachte;", "tokens": ["Ge\u00b7b\u00e4\u00b7ren", "woll\u00b7te", ",", "krach\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Land umher erzitterte, man dachte,", "tokens": ["Das", "Land", "um\u00b7her", "er\u00b7zit\u00b7ter\u00b7te", ",", "man", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er ein Ungeheu'r geb\u00e4ren w\u00fcrde.", "tokens": ["Da\u00df", "er", "ein", "Un\u00b7ge\u00b7heu'r", "ge\u00b7b\u00e4\u00b7ren", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was war's? Was kam heraus?", "tokens": ["Was", "wa\u00b7r's", "?", "Was", "kam", "he\u00b7raus", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O Wunder! \u2013 ", "tokens": ["O", "Wun\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}}}}