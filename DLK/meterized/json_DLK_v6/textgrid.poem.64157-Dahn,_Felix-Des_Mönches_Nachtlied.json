{"textgrid.poem.64157": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Des M\u00f6nches Nachtlied", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann alle Stimmen schweigen,", "tokens": ["Wann", "al\u00b7le", "Stim\u00b7men", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die laut den Tag gemacht,", "tokens": ["Die", "laut", "den", "Tag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und still im Sternenreigen", "tokens": ["Und", "still", "im", "Ster\u00b7nen\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Himmel geht die Nacht \u2013", "tokens": ["Am", "Him\u00b7mel", "geht", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann schwebt aus duft'ger Ferne,", "tokens": ["Dann", "schwebt", "aus", "duft'\u00b7ger", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dunkler Wolken Tor,", "tokens": ["Aus", "dunk\u00b7ler", "Wol\u00b7ken", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der lieblichste der Sterne,", "tokens": ["Der", "lieb\u00b7lichs\u00b7te", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Bild schwebt mir empor:", "tokens": ["Dein", "Bild", "schwebt", "mir", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Befreit von Erdenstaube,", "tokens": ["Be\u00b7freit", "von", "Er\u00b7dens\u00b7tau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Himmelshauch umweht,", "tokens": ["Von", "Him\u00b7mels\u00b7hauch", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So heilig wie der Glaube,", "tokens": ["So", "hei\u00b7lig", "wie", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So rein wie das Gebet.", "tokens": ["So", "rein", "wie", "das", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "In deinen Z\u00fcgen malet", "tokens": ["In", "dei\u00b7nen", "Z\u00fc\u00b7gen", "ma\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich sel'ge Traurigkeit:", "tokens": ["Sich", "sel'\u00b7ge", "Trau\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Auge widerstrahlet", "tokens": ["Dein", "Au\u00b7ge", "wi\u00b7der\u00b7strah\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott und Unendlichkeit.", "tokens": ["Gott", "und", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Da legen alle Fluten", "tokens": ["Da", "le\u00b7gen", "al\u00b7le", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Welt und Leben sich,", "tokens": ["Von", "Welt", "und", "Le\u00b7ben", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es l\u00f6schen selbst die Gluten,", "tokens": ["Es", "l\u00f6\u00b7schen", "selbst", "die", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mich verzehrt um dich.", "tokens": ["Die", "mich", "ver\u00b7zehrt", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich falte meine H\u00e4nde", "tokens": ["Ich", "fal\u00b7te", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fromm wie ich nie geglaubt:", "tokens": ["Fromm", "wie", "ich", "nie", "ge\u00b7glaubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Segen sonder Ende", "tokens": ["O", "Se\u00b7gen", "son\u00b7der", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf dein geliebtes Haupt!", "tokens": ["Auf", "dein", "ge\u00b7lieb\u00b7tes", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}