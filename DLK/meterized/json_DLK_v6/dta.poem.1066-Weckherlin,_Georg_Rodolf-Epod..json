{"dta.poem.1066": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Darf dan deiner Hochheit grund", "tokens": ["Darf", "dan", "dei\u00b7ner", "Hoch\u00b7heit", "grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zuber\u00fchren ich mich wagen;", "tokens": ["Zu\u00b7be\u00b7r\u00fch\u00b7ren", "ich", "mich", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00f6rcht ich/ das ich anzuklagen", "tokens": ["F\u00f6rcht", "ich", "/", "das", "ich", "an\u00b7zu\u00b7kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "PRELS", "PPER", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch deiner Z\u00fcchtigkeit mund/", "tokens": ["Durch", "dei\u00b7ner", "Z\u00fcch\u00b7tig\u00b7keit", "mund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Welche vngezwungen ehret/", "tokens": ["Wel\u00b7che", "vn\u00b7ge\u00b7zwun\u00b7gen", "eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd gern dienet jedem stand/", "tokens": ["Vnd", "gern", "die\u00b7net", "je\u00b7dem", "stand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also den wahren verstand", "tokens": ["Al\u00b7so", "den", "wah\u00b7ren", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Der herren hochheit vnslehret/", "tokens": ["Der", "her\u00b7ren", "hoch\u00b7heit", "vn\u00b7sleh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Das auch mehr durch gunst da\u0303 prange\u0303/", "tokens": ["Das", "auch", "mehr", "durch", "gunst", "d\u00e3", "prang\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "APPR", "FM", "FM", "FM", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Der menschen lieb zu erlangen.", "tokens": ["Der", "men\u00b7schen", "lieb", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}