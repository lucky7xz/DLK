{"textgrid.poem.52239": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Schlesischen Gebirge", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnun werden gr\u00fcn die Brombeerhecken;", "tokens": ["\u00bb", "nun", "wer\u00b7den", "gr\u00fcn", "die", "Brom\u00b7beer\u00b7he\u00b7cken", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier schon ein Veilchen \u2013 welch ein Fest!", "tokens": ["Hier", "schon", "ein", "Veil\u00b7chen", "\u2013", "welch", "ein", "Fest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Amsel sucht sich d\u00fcrre Stecken,", "tokens": ["Die", "Am\u00b7sel", "sucht", "sich", "d\u00fcr\u00b7re", "Ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch der Buchfink baut sein Nest.", "tokens": ["Und", "auch", "der", "Buch\u00b7fink", "baut", "sein", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Schnee ist \u00fcberall gewichen,", "tokens": ["Der", "Schnee", "ist", "\u00fc\u00b7be\u00b7rall", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Koppe nur sieht wei\u00df ins Tal;", "tokens": ["Die", "Kop\u00b7pe", "nur", "sieht", "wei\u00df", "ins", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ich habe mich von Haus geschlichen,", "tokens": ["Ich", "ha\u00b7be", "mich", "von", "Haus", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier ist der Ort \u2013 ich wag's ein einmal:", "tokens": ["Hier", "ist", "der", "Ort", "\u2013", "ich", "wag's", "ein", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "R\u00fcbezahl!", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt' er's? ich seh' ihm dreist entgegen!", "tokens": ["H\u00f6rt'", "er's", "?", "ich", "seh'", "ihm", "dreist", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist nicht b\u00f6s! Auf diesen Block", "tokens": ["Er", "ist", "nicht", "b\u00f6s", "!", "Auf", "die\u00b7sen", "Block"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich mein Leinwandp\u00e4ckchen legen \u2013", "tokens": ["Will", "ich", "mein", "Lein\u00b7wand\u00b7p\u00e4ck\u00b7chen", "le\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist ein richt'ges volles Schock!", "tokens": ["Es", "ist", "ein", "richt'\u00b7ges", "vol\u00b7les", "Schock", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fein! Ja, daf\u00fcr kann ich stehen!", "tokens": ["Und", "fein", "!", "Ja", ",", "da\u00b7f\u00fcr", "kann", "ich", "ste\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PTKANT", "$,", "PAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein be\u00dfres wird geweht im Tal \u2013", "tokens": ["Kein", "be\u00df\u00b7res", "wird", "ge\u00b7weht", "im", "Tal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4\u00dft sich immer noch nicht sehen!", "tokens": ["Er", "l\u00e4\u00dft", "sich", "im\u00b7mer", "noch", "nicht", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drum frischen Mutes noch einmal:", "tokens": ["Drum", "fri\u00b7schen", "Mu\u00b7tes", "noch", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "R\u00fcbezahl!", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Kein Laut! \u2013 Ich bin ins Holz gegangen,", "tokens": ["Kein", "Laut", "!", "\u2013", "Ich", "bin", "ins", "Holz", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "$(", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er uns hilft in unsrer Not!", "tokens": ["Da\u00df", "er", "uns", "hilft", "in", "uns\u00b7rer", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, meiner Mutter blasse Wangen \u2013", "tokens": ["O", ",", "mei\u00b7ner", "Mut\u00b7ter", "blas\u00b7se", "Wan\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im ganzen Haus kein St\u00fcckchen Brot!", "tokens": ["Im", "gan\u00b7zen", "Haus", "kein", "St\u00fcck\u00b7chen", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vater schritt zu Markt mit Fluchen \u2013", "tokens": ["Der", "Va\u00b7ter", "schritt", "zu", "Markt", "mit", "Flu\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00e4nd' er auch K\u00e4ufer nur einmal!", "tokens": ["F\u00e4nd'", "er", "auch", "K\u00e4u\u00b7fer", "nur", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will's mit R\u00fcbezahl versuchen \u2013", "tokens": ["Ich", "will's", "mit", "R\u00fc\u00b7be\u00b7zahl", "ver\u00b7su\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo bleibt er nur? Zum drittenmal:", "tokens": ["Wo", "bleibt", "er", "nur", "?", "Zum", "drit\u00b7ten\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$.", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "R\u00fcbezahl!", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Er half so vielen schon vorzeiten \u2013", "tokens": ["Er", "half", "so", "vie\u00b7len", "schon", "vor\u00b7zei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gro\u00dfmutter hat mir's oft erz\u00e4hlt!", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "hat", "mir's", "oft", "er\u00b7z\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, er ist gut den armen Leuten,", "tokens": ["Ja", ",", "er", "ist", "gut", "den", "ar\u00b7men", "Leu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die unverschuldet Elend qu\u00e4lt!", "tokens": ["Die", "un\u00b7ver\u00b7schul\u00b7det", "E\u00b7lend", "qu\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bin ich froh denn hergelaufen", "tokens": ["So", "bin", "ich", "froh", "denn", "her\u00b7ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit meiner richt'gen Ellenzahl!", "tokens": ["Mit", "mei\u00b7ner", "richt'\u00b7gen", "El\u00b7len\u00b7zahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will nicht betteln, will verkaufen!", "tokens": ["Ich", "will", "nicht", "bet\u00b7teln", ",", "will", "ver\u00b7kau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O, da\u00df er k\u00e4me! R\u00fcbezahl!", "tokens": ["O", ",", "da\u00df", "er", "k\u00e4\u00b7me", "!", "R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "R\u00fcbezahl!", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wenn dieses P\u00e4ckchen ihm gefiele,", "tokens": ["Wenn", "die\u00b7ses", "P\u00e4ck\u00b7chen", "ihm", "ge\u00b7fie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht gar b\u00e4t' er mehr sich aus!", "tokens": ["Viel\u00b7leicht", "gar", "b\u00e4t'", "er", "mehr", "sich", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das w\u00e4r' mir recht! Ach, gar zu viele,", "tokens": ["Das", "w\u00e4r'", "mir", "recht", "!", "Ach", ",", "gar", "zu", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$.", "ITJ", "$,", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich sch\u00f6ne liegen noch zu Haus!", "tokens": ["Gleich", "sch\u00f6\u00b7ne", "lie\u00b7gen", "noch", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die n\u00e4hm' er alle bis zum letzten!", "tokens": ["Die", "n\u00e4hm'", "er", "al\u00b7le", "bis", "zum", "letz\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, fiel auf dies doch seine Wahl!", "tokens": ["Ach", ",", "fiel", "auf", "dies", "doch", "sei\u00b7ne", "Wahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "APPR", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da l\u00f6st' ich ein selbst die versetzten \u2013", "tokens": ["Da", "l\u00f6st'", "ich", "ein", "selbst", "die", "ver\u00b7setz\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das w\u00e4r' ein Jubel! R\u00fcbezahl!", "tokens": ["Das", "w\u00e4r'", "ein", "Ju\u00b7bel", "!", "R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "R\u00fcbezahl!", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Dann tr\u00e4t' ich froh ins kleine Zimmer,", "tokens": ["Dann", "tr\u00e4t'", "ich", "froh", "ins", "klei\u00b7ne", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und riefe: Vater, Geld genug!", "tokens": ["Und", "rie\u00b7fe", ":", "Va\u00b7ter", ",", "Geld", "ge\u00b7nug", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann flucht' er nicht, dann sagt' er nimmer:", "tokens": ["Dann", "flucht'", "er", "nicht", ",", "dann", "sagt'", "er", "nim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich web' euch nur ein Hungertuch!", "tokens": ["Ich", "web'", "euch", "nur", "ein", "Hun\u00b7ger\u00b7tuch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann l\u00e4chelte die Mutter wieder,", "tokens": ["Dann", "l\u00e4\u00b7chel\u00b7te", "die", "Mut\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und tischt' uns auf ein reichlich Mahl;", "tokens": ["Und", "tischt'", "uns", "auf", "ein", "reich\u00b7lich", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann jauchzten meine kleinen Br\u00fcder \u2013", "tokens": ["Dann", "jauchz\u00b7ten", "mei\u00b7ne", "klei\u00b7nen", "Br\u00fc\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O k\u00e4m', o k\u00e4m' er! R\u00fcbezahl!", "tokens": ["O", "k\u00e4m'", ",", "o", "k\u00e4m'", "er", "!", "R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "R\u00fcbezahl!\u00ab", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "So rief der dreizehnj\u00e4hr'ge Knabe;", "tokens": ["So", "rief", "der", "drei\u00b7zehn\u00b7j\u00e4hr'\u00b7ge", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So stand und rief er, matt und bleich.", "tokens": ["So", "stand", "und", "rief", "er", ",", "matt", "und", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst! Nur dann und wann ein Rabe", "tokens": ["Um\u00b7sonst", "!", "Nur", "dann", "und", "wann", "ein", "Ra\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "ADV", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flog durch des Gnomen altes Reich.", "tokens": ["Flog", "durch", "des", "Gno\u00b7men", "al\u00b7tes", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So stand und pa\u00dft' er Stund' auf Stunde,", "tokens": ["So", "stand", "und", "pa\u00dft'", "er", "Stund'", "auf", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df es dunkel ward im Tal,", "tokens": ["Bis", "da\u00df", "es", "dun\u00b7kel", "ward", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und er halblaut mit zuckendem Munde", "tokens": ["Und", "er", "hal\u00b7blaut", "mit", "zu\u00b7cken\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Ausrief durch Tr\u00e4nen noch einmal:", "tokens": ["Aus\u00b7rief", "durch", "Tr\u00e4\u00b7nen", "noch", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "R\u00fcbezahl!", "tokens": ["R\u00fc\u00b7be\u00b7zahl", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Dann lie\u00df er still das buschige Fleckchen,", "tokens": ["Dann", "lie\u00df", "er", "still", "das", "bu\u00b7schi\u00b7ge", "Fleck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und zitterte und sagte: Hu!", "tokens": ["Und", "zit\u00b7ter\u00b7te", "und", "sag\u00b7te", ":", "Hu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schritt mit seinem Leinwandp\u00e4ckchen", "tokens": ["Und", "schritt", "mit", "sei\u00b7nem", "Lein\u00b7wand\u00b7p\u00e4ck\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Jammer seiner Heimat zu.", "tokens": ["Dem", "Jam\u00b7mer", "sei\u00b7ner", "Hei\u00b7mat", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Oft ruht' er aus auf moos'gen Steinen,", "tokens": ["Oft", "ruht'", "er", "aus", "auf", "moos'\u00b7gen", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Matt von der B\u00fcrde, die er trug.", "tokens": ["Matt", "von", "der", "B\u00fcr\u00b7de", ",", "die", "er", "trug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich glaub', sein Vater webt dem Kleinen", "tokens": ["Ich", "glaub'", ",", "sein", "Va\u00b7ter", "webt", "dem", "Klei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Hunger- bald das Leichentuch!", "tokens": ["Zum", "Hun\u00b7ger", "bald", "das", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u2013 R\u00fcbezahl?!", "tokens": ["\u2013", "R\u00fc\u00b7be\u00b7zahl", "?!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}