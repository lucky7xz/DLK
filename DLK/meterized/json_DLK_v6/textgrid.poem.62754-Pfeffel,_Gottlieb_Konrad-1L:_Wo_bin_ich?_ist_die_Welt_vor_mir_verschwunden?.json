{"textgrid.poem.62754": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo bin ich? ist die Welt vor mir verschwunden?", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bin ich? ist die Welt vor mir verschwunden?", "tokens": ["Wo", "bin", "ich", "?", "ist", "die", "Welt", "vor", "mir", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie? oder h\u00e4lt der Abgrund mich gebunden?", "tokens": ["Wie", "?", "o\u00b7der", "h\u00e4lt", "der", "Ab\u00b7grund", "mich", "ge\u00b7bun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Sonne! Warum ziehst du deine Blicke", "tokens": ["O", "Son\u00b7ne", "!", "Wa\u00b7rum", "ziehst", "du", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von mir zur\u00fccke?", "tokens": ["Von", "mir", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo bist du, Hofnung, letztes Gut des Lebens?", "tokens": ["Wo", "bist", "du", ",", "Hof\u00b7nung", ",", "letz\u00b7tes", "Gut", "des", "Le\u00b7bens", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch auch nach dir tappt meine Hand vergebens;", "tokens": ["Doch", "auch", "nach", "dir", "tappt", "mei\u00b7ne", "Hand", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch du verbirgst nun deine holden Strahlen", "tokens": ["Auch", "du", "ver\u00b7birgst", "nun", "dei\u00b7ne", "hol\u00b7den", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor meinen Quaalen.", "tokens": ["Vor", "mei\u00b7nen", "Qua\u00b7a\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Die blasse Schwermuth, die mein Fleisch gefressen,", "tokens": ["Die", "blas\u00b7se", "Schwer\u00b7muth", ",", "die", "mein", "Fleisch", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umwindet meine Schl\u00e4fe mit Cypressen.", "tokens": ["Um\u00b7win\u00b7det", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "mit", "Cyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sind verwelkt, die Rosen und die Myrten,", "tokens": ["Sie", "sind", "ver\u00b7welkt", ",", "die", "Ro\u00b7sen", "und", "die", "Myr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sonst sie zierten.", "tokens": ["Die", "sonst", "sie", "zier\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wie? soll die Nacht des Grabes meinen Tagen", "tokens": ["Wie", "?", "soll", "die", "Nacht", "des", "Gra\u00b7bes", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VMFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ewig alle Heiterkeit versagen?", "tokens": ["Auf", "e\u00b7wig", "al\u00b7le", "Hei\u00b7ter\u00b7keit", "ver\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch rei\u00dft kein Tod die ekle Seele", "tokens": ["Und", "den\u00b7noch", "rei\u00dft", "kein", "Tod", "die", "ek\u00b7le", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus ihrer H\u00f6le.", "tokens": ["Aus", "ih\u00b7rer", "H\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Verh\u00e4ngni\u00df, la\u00df nun ab, mich zu versuchen,", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "la\u00df", "nun", "ab", ",", "mich", "zu", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sonst mu\u00df ich \u2013 nein, ich kann dich nicht verfluchen.", "tokens": ["Sonst", "mu\u00df", "ich", "\u2013", "nein", ",", "ich", "kann", "dich", "nicht", "ver\u00b7flu\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bald mein Geist die heil'ge Wahrheit denket:", "tokens": ["So", "bald", "mein", "Geist", "die", "heil'\u00b7ge", "Wahr\u00b7heit", "den\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Gott dich lenket.", "tokens": ["Da\u00df", "Gott", "dich", "len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Bey wem willst du den Herrn der Welt verklagen?", "tokens": ["Bey", "wem", "willst", "du", "den", "Herrn", "der", "Welt", "ver\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beth\u00f6rte Seele, sch\u00e4me dich zu zagen.", "tokens": ["Be\u00b7th\u00f6r\u00b7te", "See\u00b7le", ",", "sch\u00e4\u00b7me", "dich", "zu", "za\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sey weise, so entspringt dir aus dem Leiden", "tokens": ["Sey", "wei\u00b7se", ",", "so", "ent\u00b7springt", "dir", "aus", "dem", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Quell der Freuden.", "tokens": ["Ein", "Quell", "der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "O Weisheit! komm, erscheine meinem Herzen,", "tokens": ["O", "Weis\u00b7heit", "!", "komm", ",", "er\u00b7schei\u00b7ne", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geu\u00df deinen Balsam \u00fcber meine Schmerzen:", "tokens": ["Geu\u00df", "dei\u00b7nen", "Bal\u00b7sam", "\u00fc\u00b7ber", "mei\u00b7ne", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur du allein kannst mit dem Schicksal ringen,", "tokens": ["Nur", "du", "al\u00b7lein", "kannst", "mit", "dem", "Schick\u00b7sal", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und es bezwingen!", "tokens": ["Und", "es", "be\u00b7zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Du bist die Weisheit, dich mu\u00df ich erw\u00e4hlen,", "tokens": ["Du", "bist", "die", "Weis\u00b7heit", ",", "dich", "mu\u00df", "ich", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Tugend! dich, du Gottheit in den Seelen.", "tokens": ["O", "Tu\u00b7gend", "!", "dich", ",", "du", "Got\u00b7theit", "in", "den", "See\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "$,", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Komm, Sch\u00f6pferin der wahren ew'gen Wonne,", "tokens": ["Komm", ",", "Sch\u00f6p\u00b7fe\u00b7rin", "der", "wah\u00b7ren", "ew'\u00b7gen", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sey meine Sonne.", "tokens": ["Sey", "mei\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "O, selig! wer an deinem Busen lebet,", "tokens": ["O", ",", "se\u00b7lig", "!", "wer", "an", "dei\u00b7nem", "Bu\u00b7sen", "le\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der zittert nicht, wenn gleich der Weltkreis bebet;", "tokens": ["Der", "zit\u00b7tert", "nicht", ",", "wenn", "gleich", "der", "Welt\u00b7kreis", "be\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wenn Plagen Gottes sein Gebein zermalmen,", "tokens": ["Wenn", "Pla\u00b7gen", "Got\u00b7tes", "sein", "Ge\u00b7bein", "zer\u00b7mal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So singt er Psalmen.", "tokens": ["So", "singt", "er", "Psal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Verzagte Seele! d\u00e4mpfe deinen Kummer.", "tokens": ["Ver\u00b7zag\u00b7te", "See\u00b7le", "!", "d\u00e4mp\u00b7fe", "dei\u00b7nen", "Kum\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben ist ja nur ein kurzer Schlummer.", "tokens": ["Das", "Le\u00b7ben", "ist", "ja", "nur", "ein", "kur\u00b7zer", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald wird der Tod die kalte Rechte strecken,", "tokens": ["Bald", "wird", "der", "Tod", "die", "kal\u00b7te", "Rech\u00b7te", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich aufzuwecken.", "tokens": ["Dich", "auf\u00b7zu\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}