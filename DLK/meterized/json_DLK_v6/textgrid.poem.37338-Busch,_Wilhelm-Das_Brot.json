{"textgrid.poem.37338": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Brot", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er sa\u00df beim Fr\u00fchst\u00fcck \u00e4u\u00dferst gr\u00e4mlich,", "tokens": ["Er", "sa\u00df", "beim", "Fr\u00fch\u00b7st\u00fcck", "\u00e4u\u00b7\u00dferst", "gr\u00e4m\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprach ein Kr\u00fcmchen Brot vernehmlich:", "tokens": ["Da", "sprach", "ein", "Kr\u00fcm\u00b7chen", "Brot", "ver\u00b7nehm\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aha, so ist es mit dem Orden", "tokens": ["A\u00b7ha", ",", "so", "ist", "es", "mit", "dem", "Or\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr diesmal wieder nichts geworden.", "tokens": ["F\u00fcr", "dies\u00b7mal", "wie\u00b7der", "nichts", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "PIS", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, Freund, wer seinen Blick erweitert", "tokens": ["Ja", ",", "Freund", ",", "wer", "sei\u00b7nen", "Blick", "er\u00b7wei\u00b7tert"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PWS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schaut nach hinten und nach vorn,", "tokens": ["Und", "schaut", "nach", "hin\u00b7ten", "und", "nach", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der preist den Kummer, denn er l\u00e4utert.", "tokens": ["Der", "preist", "den", "Kum\u00b7mer", ",", "denn", "er", "l\u00e4u\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich selber war ein Weizenkorn.", "tokens": ["Ich", "sel\u00b7ber", "war", "ein", "Wei\u00b7zen\u00b7korn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit vielen, die mir anverwandt,", "tokens": ["Mit", "vie\u00b7len", ",", "die", "mir", "an\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lag ich im rauhen Ackerland.", "tokens": ["Lag", "ich", "im", "rau\u00b7hen", "A\u00b7cker\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bedr\u00fcckt von einem Erdenklo\u00df,", "tokens": ["Be\u00b7dr\u00fcckt", "von", "ei\u00b7nem", "Er\u00b7den\u00b7klo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Macht ich mich mutig strebend los.", "tokens": ["Macht", "ich", "mich", "mu\u00b7tig", "stre\u00b7bend", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Gleich kam ein alter Has gehupft", "tokens": ["Gleich", "kam", "ein", "al\u00b7ter", "Has", "ge\u00b7hupft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und hat mich an der Nas gezupft;", "tokens": ["Und", "hat", "mich", "an", "der", "Nas", "ge\u00b7zupft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und als es Winter ward, verfror,", "tokens": ["Und", "als", "es", "Win\u00b7ter", "ward", ",", "ver\u00b7fror", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Was peinlich ist, mein linkes Ohr;", "tokens": ["Was", "pein\u00b7lich", "ist", ",", "mein", "lin\u00b7kes", "Ohr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und als ich reif mit meiner Sippe,", "tokens": ["Und", "als", "ich", "reif", "mit", "mei\u00b7ner", "Sip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "O weh, da hat mit seiner Hippe", "tokens": ["O", "weh", ",", "da", "hat", "mit", "sei\u00b7ner", "Hip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Hans uns rutschweg abges\u00e4belt", "tokens": ["Der", "Hans", "uns", "rut\u00b7schweg", "ab\u00b7ge\u00b7s\u00e4\u00b7belt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und zum Ersticken festgeknebelt", "tokens": ["Und", "zum", "Er\u00b7sti\u00b7cken", "fest\u00b7ge\u00b7kne\u00b7belt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und auf die Tenne fortgeschafft,", "tokens": ["Und", "auf", "die", "Ten\u00b7ne", "fort\u00b7ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wo ihrer vier mit voller Kraft", "tokens": ["Wo", "ih\u00b7rer", "vier", "mit", "vol\u00b7ler", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "CARD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "In regelrechtem Flegeltakte", "tokens": ["In", "re\u00b7gel\u00b7rech\u00b7tem", "Fle\u00b7gel\u00b7tak\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Uns klopften, da\u00df die Schwarte knackte.", "tokens": ["Uns", "klopf\u00b7ten", ",", "da\u00df", "die", "Schwar\u00b7te", "knack\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ein Esel trug uns nach der M\u00fchle.", "tokens": ["Ein", "E\u00b7sel", "trug", "uns", "nach", "der", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ich sage dir, das sind Gef\u00fchle,", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "das", "sind", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wenn man, zerrieben und gedrillt", "tokens": ["Wenn", "man", ",", "zer\u00b7rie\u00b7ben", "und", "ge\u00b7drillt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Zum allerfeinsten Staubgebild,", "tokens": ["Zum", "al\u00b7ler\u00b7feins\u00b7ten", "Staub\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Sich kaum besinnt und fast vergi\u00dft,", "tokens": ["Sich", "kaum", "be\u00b7sinnt", "und", "fast", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ob Sonntag oder Montag ist.", "tokens": ["Ob", "Sonn\u00b7tag", "o\u00b7der", "Mon\u00b7tag", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und schlie\u00dflich schob der B\u00e4ckermeister,", "tokens": ["Und", "schlie\u00df\u00b7lich", "schob", "der", "B\u00e4\u00b7cker\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Nachdem wir erst als z\u00e4her Kleister", "tokens": ["Nach\u00b7dem", "wir", "erst", "als", "z\u00e4\u00b7her", "Kleis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "In seinem Troge ba\u00df gehudelt,", "tokens": ["In", "sei\u00b7nem", "Tro\u00b7ge", "ba\u00df", "ge\u00b7hu\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Vermengt, geknetet und vernudelt,", "tokens": ["Ver\u00b7mengt", ",", "ge\u00b7kne\u00b7tet", "und", "ver\u00b7nu\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Uns in des Ofens h\u00f6chste Glut.", "tokens": ["Uns", "in", "des", "O\u00b7fens", "h\u00f6chs\u00b7te", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Jetzt sind wir Brot. Ist das nicht gut?", "tokens": ["Jetzt", "sind", "wir", "Brot", ".", "Ist", "das", "nicht", "gut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Frischauf, du hast genug, mein Lieber,", "tokens": ["Frisc\u00b7hauf", ",", "du", "hast", "ge\u00b7nug", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Greif zu und schneide nicht zu knapp", "tokens": ["Greif", "zu", "und", "schnei\u00b7de", "nicht", "zu", "knapp"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und streiche t\u00fcchtig Butter dr\u00fcber", "tokens": ["Und", "strei\u00b7che", "t\u00fcch\u00b7tig", "But\u00b7ter", "dr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und gib den andern auch was ab.", "tokens": ["Und", "gib", "den", "an\u00b7dern", "auch", "was", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}