{"textgrid.poem.32752": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch irr' ich einsam, ohne Gef\u00e4hrtin noch", "tokens": ["Noch", "irr'", "ich", "ein\u00b7sam", ",", "oh\u00b7ne", "Ge\u00b7f\u00e4hr\u00b7tin", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUI", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchs tr\u00fcbe Leben; weine noch ungeteilt", "tokens": ["Durchs", "tr\u00fc\u00b7be", "Le\u00b7ben", ";", "wei\u00b7ne", "noch", "un\u00b7ge\u00b7teilt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Freude Thr\u00e4nen und des Kummers,", "tokens": ["Der", "Freu\u00b7de", "Thr\u00e4\u00b7nen", "und", "des", "Kum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der vertraulichen Abendd\u00e4mmrung.", "tokens": ["In", "der", "ver\u00b7trau\u00b7li\u00b7chen", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zwar oft in Stunden heller Begeisterung", "tokens": ["Zwar", "oft", "in", "Stun\u00b7den", "hel\u00b7ler", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stieg eine Bildung nieder, und l\u00e4chelte;", "tokens": ["Stieg", "ei\u00b7ne", "Bil\u00b7dung", "nie\u00b7der", ",", "und", "l\u00e4\u00b7chel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Voll Sehnsucht seufzt' ich: Komm, Erw\u00e4hlte!", "tokens": ["Voll", "Sehn\u00b7sucht", "seufzt'", "ich", ":", "Komm", ",", "Er\u00b7w\u00e4hl\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "$.", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber in Schatten zerflo\u00df die T\u00e4uschung.", "tokens": ["A\u00b7ber", "in", "Schat\u00b7ten", "zer\u00b7flo\u00df", "die", "T\u00e4u\u00b7schung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Umsonst, o Daphne, f\u00fchrte die Liebe dich", "tokens": ["Um\u00b7sonst", ",", "o", "Daph\u00b7ne", ",", "f\u00fchr\u00b7te", "die", "Lie\u00b7be", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entgegen mir, umgaukelt von Hoffnungen;", "tokens": ["Ent\u00b7ge\u00b7gen", "mir", ",", "um\u00b7gau\u00b7kelt", "von", "Hoff\u00b7nun\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wink des Sch\u00f6pfers! und sie st\u00fcrzten", "tokens": ["Ein", "Wink", "des", "Sch\u00f6p\u00b7fers", "!", "und", "sie", "st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief in der ewigen Trennung Abgrund!", "tokens": ["Tief", "in", "der", "e\u00b7wi\u00b7gen", "Tren\u00b7nung", "Ab\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Da\u00df du es heiltest, flehte mein wundes Herz", "tokens": ["Da\u00df", "du", "es", "heil\u00b7test", ",", "fleh\u00b7te", "mein", "wun\u00b7des", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir oft, Elise, weinte dir schweigend nach;", "tokens": ["Dir", "oft", ",", "E\u00b7li\u00b7se", ",", "wein\u00b7te", "dir", "schwei\u00b7gend", "nach", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NE", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mitleidig sahst du mich, und bebtest", "tokens": ["Mit\u00b7lei\u00b7dig", "sahst", "du", "mich", ",", "und", "beb\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! in den Arm des geliebtern J\u00fcnglings.", "tokens": ["Ach", "!", "in", "den", "Arm", "des", "ge\u00b7lieb\u00b7tern", "J\u00fcng\u00b7lings", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Unsichtbar schwebt um jegliche Seel', als Freund,", "tokens": ["Un\u00b7sicht\u00b7bar", "schwebt", "um", "jeg\u00b7li\u00b7che", "Seel'", ",", "als", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PIAT", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Engel Gottes, bildet der Tugend sie,", "tokens": ["Ein", "En\u00b7gel", "Got\u00b7tes", ",", "bil\u00b7det", "der", "Tu\u00b7gend", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Folgt ihr bis an die Nacht des Grabes,", "tokens": ["Folgt", "ihr", "bis", "an", "die", "Nacht", "des", "Gra\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Winkt ihr, und schwingt sich mit ihr zu Gott auf.", "tokens": ["Winkt", "ihr", ",", "und", "schwingt", "sich", "mit", "ihr", "zu", "Gott", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+++", "measure": "zehnsilber"}}, "stanza.6": {"line.1": {"text": "Doch wessen Herzen h\u00f6here Seligkeit", "tokens": ["Doch", "wes\u00b7sen", "Her\u00b7zen", "h\u00f6\u00b7he\u00b7re", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon hier des Richters lohnende Schale wog,", "tokens": ["Schon", "hier", "des", "Rich\u00b7ters", "loh\u00b7nen\u00b7de", "Scha\u00b7le", "wog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem eilt aus Edens Flur ein Engel,", "tokens": ["Dem", "eilt", "aus", "E\u00b7dens", "Flur", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sichtbar, in M\u00e4dchengestalt, entgegen.", "tokens": ["Sicht\u00b7bar", ",", "in", "M\u00e4d\u00b7chen\u00b7ge\u00b7stalt", ",", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "NN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Und jeder Wonne schlie\u00dft sich sein Busen auf,", "tokens": ["Und", "je\u00b7der", "Won\u00b7ne", "schlie\u00dft", "sich", "sein", "Bu\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Fr\u00fchlingsauen wandelt die Sch\u00f6pfung sich,", "tokens": ["In", "Fr\u00fch\u00b7lin\u00b7gsau\u00b7en", "wan\u00b7delt", "die", "Sch\u00f6p\u00b7fung", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Leicht wird ihm jede Pflicht, und heller", "tokens": ["Leicht", "wird", "ihm", "je\u00b7de", "Pflicht", ",", "und", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PIAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Winket dem Waller die Siegespalme.", "tokens": ["Win\u00b7ket", "dem", "Wal\u00b7ler", "die", "Sie\u00b7ge\u00b7spal\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "La\u00df laute St\u00fcrme toben! Ihr schweigen sie.", "tokens": ["La\u00df", "lau\u00b7te", "St\u00fcr\u00b7me", "to\u00b7ben", "!", "Ihr", "schwei\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "La\u00df bange Thr\u00e4nen rinnen! Sie k\u00fc\u00dft sie auf.", "tokens": ["La\u00df", "ban\u00b7ge", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "!", "Sie", "k\u00fc\u00dft", "sie", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "VVINF", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, \u00f6ffnet sich das Grab, so folgt ihm", "tokens": ["Und", ",", "\u00f6ff\u00b7net", "sich", "das", "Grab", ",", "so", "folgt", "ihm"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald der geliebte, getreue Geist nach. \u2013", "tokens": ["Bald", "der", "ge\u00b7lieb\u00b7te", ",", "ge\u00b7treu\u00b7e", "Geist", "nach", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "O Gott! Wenn reines Herzens ich bin vor dir,", "tokens": ["O", "Gott", "!", "Wenn", "rei\u00b7nes", "Her\u00b7zens", "ich", "bin", "vor", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "ADJA", "NN", "PPER", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn wert ich dieser himmlischen Freundin bin;", "tokens": ["Wenn", "wert", "ich", "die\u00b7ser", "himm\u00b7li\u00b7schen", "Freun\u00b7din", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PDAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So sende sie aus Edens Fluren,", "tokens": ["So", "sen\u00b7de", "sie", "aus", "E\u00b7dens", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mit ihr dir entgegen wandle!", "tokens": ["Da\u00df", "ich", "mit", "ihr", "dir", "ent\u00b7ge\u00b7gen", "wand\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}