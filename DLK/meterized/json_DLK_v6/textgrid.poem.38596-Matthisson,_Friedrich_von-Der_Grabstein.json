{"textgrid.poem.38596": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Grabstein", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bemooster Stein, im heiligen Gefilde", "tokens": ["Be\u00b7moos\u00b7ter", "Stein", ",", "im", "hei\u00b7li\u00b7gen", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Aussaat Gottes, sei mir froh gegr\u00fc\u00dft!", "tokens": ["Der", "Aus\u00b7saat", "Got\u00b7tes", ",", "sei", "mir", "froh", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O du, auf den des Abendhimmels Milde", "tokens": ["O", "du", ",", "auf", "den", "des", "A\u00b7bend\u00b7him\u00b7mels", "Mil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So freundlich sich ergie\u00dft!", "tokens": ["So", "freund\u00b7lich", "sich", "er\u00b7gie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seit Jahren schweigen dir die Klaget\u00f6ne", "tokens": ["Seit", "Jah\u00b7ren", "schwei\u00b7gen", "dir", "die", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Freundes schon; auch sein Gebein ist Staub!", "tokens": ["Des", "Freun\u00b7des", "schon", ";", "auch", "sein", "Ge\u00b7bein", "ist", "Staub", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir streut kein M\u00e4dchen mehr, mit frommer Thr\u00e4ne,", "tokens": ["Dir", "streut", "kein", "M\u00e4d\u00b7chen", "mehr", ",", "mit", "from\u00b7mer", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Fr\u00fchlings Erstlingslaub!", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Erst\u00b7lings\u00b7laub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer nennt mir deinen Schlummrer! Halbverwittert,", "tokens": ["Wer", "nennt", "mir", "dei\u00b7nen", "Schlumm\u00b7rer", "!", "Halb\u00b7ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blieb dir des Todtenkopfes Zierde nur,", "tokens": ["Blieb", "dir", "des", "Tod\u00b7ten\u00b7kop\u00b7fes", "Zier\u00b7de", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Schrift erlosch, und Wintergr\u00fcn umzittert", "tokens": ["Die", "Schrift", "er\u00b7losch", ",", "und", "Win\u00b7ter\u00b7gr\u00fcn", "um\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Namens dunkle Spur!", "tokens": ["Des", "Na\u00b7mens", "dunk\u00b7le", "Spur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dir eil' ich zu, des Weltget\u00fcmmels m\u00fcde,", "tokens": ["Dir", "eil'", "ich", "zu", ",", "des", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mels", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann durchs Geb\u00fcsch die Abendr\u00f6the bebt,", "tokens": ["Wann", "durchs", "Ge\u00b7b\u00fcsch", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Altar der Hofnung! wo Jehovas Friede", "tokens": ["Al\u00b7tar", "der", "Hof\u00b7nung", "!", "wo", "Je\u00b7ho\u00b7vas", "Frie\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PWAV", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Engelfl\u00fcgeln schwebt!", "tokens": ["Auf", "En\u00b7gel\u00b7fl\u00fc\u00b7geln", "schwebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}