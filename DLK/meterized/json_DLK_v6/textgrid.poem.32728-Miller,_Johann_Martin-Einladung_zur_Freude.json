{"textgrid.poem.32728": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Einladung zur Freude", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch bl\u00fchen uns zu Kr\u00e4nzen", "tokens": ["Noch", "bl\u00fc\u00b7hen", "uns", "zu", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Blumen auf der Flur;", "tokens": ["Die", "Blu\u00b7men", "auf", "der", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch locken uns zu T\u00e4nzen", "tokens": ["Noch", "lo\u00b7cken", "uns", "zu", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die S\u00e4nger der Natur.", "tokens": ["Die", "S\u00e4n\u00b7ger", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch rieselt uns gelinde", "tokens": ["Noch", "rie\u00b7selt", "uns", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der kleine Schmerlenbach,", "tokens": ["Der", "klei\u00b7ne", "Schmer\u00b7len\u00b7bach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die belaubte Linde", "tokens": ["Und", "die", "be\u00b7laub\u00b7te", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "W\u00f6lbt uns ein Schattendach.", "tokens": ["W\u00f6lbt", "uns", "ein", "Schat\u00b7ten\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da stellen sich am Feste", "tokens": ["Da", "stel\u00b7len", "sich", "am", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die jungen Bauren ein", "tokens": ["Die", "jun\u00b7gen", "Bau\u00b7ren", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schm\u00fccken sich aufs beste,", "tokens": ["Und", "schm\u00fc\u00b7cken", "sich", "aufs", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Liebchen wert zu sein.", "tokens": ["Dem", "Lieb\u00b7chen", "wert", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie tanzen, bis ins Tr\u00fcbe", "tokens": ["Sie", "tan\u00b7zen", ",", "bis", "ins", "Tr\u00fc\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Abendr\u00f6te sinkt,", "tokens": ["Die", "A\u00b7bend\u00b7r\u00f6\u00b7te", "sinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bla\u00df, wie bange Liebe,", "tokens": ["Und", "bla\u00df", ",", "wie", "ban\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Mond am Himmel blinkt.", "tokens": ["Der", "Mond", "am", "Him\u00b7mel", "blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohlauf, ihr St\u00e4dterinnen!", "tokens": ["Wohl\u00b7auf", ",", "ihr", "St\u00e4d\u00b7te\u00b7rin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00dft einen Reihentanz", "tokens": ["La\u00dft", "ei\u00b7nen", "Rei\u00b7hen\u00b7tanz"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihnen uns beginnen,", "tokens": ["Mit", "ih\u00b7nen", "uns", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Pfl\u00fcckt euren Blumenkranz!", "tokens": ["Pfl\u00fcckt", "eu\u00b7ren", "Blu\u00b7men\u00b7kranz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ziert mit roten B\u00e4ndern", "tokens": ["Und", "ziert", "mit", "ro\u00b7ten", "B\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das wei\u00dfe Sch\u00e4ferkleid!", "tokens": ["Das", "wei\u00b7\u00dfe", "Sch\u00e4\u00b7fer\u00b7kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn bald wird sich ver\u00e4ndern", "tokens": ["Denn", "bald", "wird", "sich", "ver\u00b7\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die wonnigliche Zeit.", "tokens": ["Die", "won\u00b7nig\u00b7li\u00b7che", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bald locket uns zur Linde", "tokens": ["Bald", "lo\u00b7cket", "uns", "zur", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein k\u00fchler Schatten mehr;", "tokens": ["Kein", "k\u00fch\u00b7ler", "Schat\u00b7ten", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald streifen rauhe Winde", "tokens": ["Bald", "strei\u00b7fen", "rau\u00b7he", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zweige bl\u00e4tterleer;", "tokens": ["Die", "Zwei\u00b7ge", "bl\u00e4t\u00b7ter\u00b7leer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die jungen Blumen sinken", "tokens": ["Die", "jun\u00b7gen", "Blu\u00b7men", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald unter Reif und Schnee,", "tokens": ["Bald", "un\u00b7ter", "Reif", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Perlentropfen blinken", "tokens": ["Und", "Per\u00b7len\u00b7trop\u00b7fen", "blin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht mehr am wei\u00dfen Klee.", "tokens": ["Nicht", "mehr", "am", "wei\u00b7\u00dfen", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald rauschen uns die B\u00e4che", "tokens": ["Bald", "rau\u00b7schen", "uns", "die", "B\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mehr, im Gras versteckt,", "tokens": ["Nicht", "mehr", ",", "im", "Gras", "ver\u00b7steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ihre Spiegelfl\u00e4che", "tokens": ["Wenn", "ih\u00b7re", "Spie\u00b7gel\u00b7fl\u00e4\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verwelktes Laub bedeckt.", "tokens": ["Ver\u00b7welk\u00b7tes", "Laub", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Vogel all verstummen,", "tokens": ["Die", "Vo\u00b7gel", "all", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Hain wird liederleer,", "tokens": ["Der", "Hain", "wird", "lie\u00b7der\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und kleine Bienen summen", "tokens": ["Und", "klei\u00b7ne", "Bie\u00b7nen", "sum\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht mehr im Gras' umher.", "tokens": ["Nicht", "mehr", "im", "Gras'", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann eilen nicht mit Kr\u00e4nzen", "tokens": ["Dann", "ei\u00b7len", "nicht", "mit", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Bux und Rosmarin", "tokens": ["Von", "Bux", "und", "Ros\u00b7ma\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu frohen deutschen T\u00e4nzen", "tokens": ["Zu", "fro\u00b7hen", "deut\u00b7schen", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die D\u00f6rferinnen hin.", "tokens": ["Die", "D\u00f6r\u00b7fe\u00b7rin\u00b7nen", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um ihre H\u00fctte st\u00fcrmet", "tokens": ["Um", "ih\u00b7re", "H\u00fct\u00b7te", "st\u00fcr\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der rauhe Winter nur,", "tokens": ["Der", "rau\u00b7he", "Win\u00b7ter", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Schneegest\u00f6ber t\u00fcrmet", "tokens": ["Und", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "t\u00fcr\u00b7met"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich auf der nackten Flur.", "tokens": ["Sich", "auf", "der", "nack\u00b7ten", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O kommt! auf euren Wangen", "tokens": ["O", "kommt", "!", "auf", "eu\u00b7ren", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist in Aurorens Pracht", "tokens": ["Ist", "in", "Au\u00b7ro\u00b7rens", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jugend aufgegangen,", "tokens": ["Die", "Ju\u00b7gend", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und euer Auge lacht.", "tokens": ["Und", "eu\u00b7er", "Au\u00b7ge", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sch\u00f6n, wie der junge Morgen,", "tokens": ["Sch\u00f6n", ",", "wie", "der", "jun\u00b7ge", "Mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist euer Angesicht,", "tokens": ["Ist", "eu\u00b7er", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und kennt des Alters Sorgen", "tokens": ["Und", "kennt", "des", "Al\u00b7ters", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und seine Runzeln nicht.", "tokens": ["Und", "sei\u00b7ne", "Run\u00b7zeln", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch, liebe St\u00e4dterinnen,", "tokens": ["Doch", ",", "lie\u00b7be", "St\u00e4d\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sommer eilt dahin;", "tokens": ["Der", "Som\u00b7mer", "eilt", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald wird der Herbst beginnen,", "tokens": ["Bald", "wird", "der", "Herbst", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und jede Freud' entfliehn.", "tokens": ["Und", "je\u00b7de", "Freud'", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein kalter Odem scheuchet", "tokens": ["Sein", "kal\u00b7ter", "O\u00b7dem", "scheu\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Freuden weg, und ach!", "tokens": ["Die", "Freu\u00b7den", "weg", ",", "und", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "XY", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit schweren Schritten keuchet", "tokens": ["Mit", "schwe\u00b7ren", "Schrit\u00b7ten", "keuc\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der alte Winter nach.", "tokens": ["Der", "al\u00b7te", "Win\u00b7ter", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dann seufzen wir vergebens", "tokens": ["Dann", "seuf\u00b7zen", "wir", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns jeden Augenblick", "tokens": ["Uns", "je\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des ungeno\u00dfnen Lebens", "tokens": ["Des", "un\u00b7ge\u00b7no\u00df\u00b7nen", "Le\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit bangem Ach zur\u00fcck.", "tokens": ["Mit", "ban\u00b7gem", "Ach", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum folget mir und weihet", "tokens": ["Drum", "fol\u00b7get", "mir", "und", "wei\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Euch jetzt der Fr\u00f6hlichkeit!", "tokens": ["Euch", "jetzt", "der", "Fr\u00f6h\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dann, im Winter, freuet", "tokens": ["Und", "dann", ",", "im", "Win\u00b7ter", ",", "freu\u00b7et"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch der geno\u00dfnen Zeit.", "tokens": ["Euch", "der", "ge\u00b7no\u00df\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}