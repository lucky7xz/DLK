{"textgrid.poem.48598": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. An eine Jungfrau, auf dero Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne, schaut! Der Sonnenschein", "tokens": ["Sch\u00f6\u00b7ne", ",", "schaut", "!", "Der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "will heut' euer eigen sein.", "tokens": ["will", "heut'", "eu\u00b7er", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Strahlen g\u00fcldnes Licht", "tokens": ["Ih\u00b7rer", "Strah\u00b7len", "g\u00fcld\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "giebt sich heut' in eure Pflicht.", "tokens": ["giebt", "sich", "heut'", "in", "eu\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie ist froh, als wie sie war,", "tokens": ["Sie", "ist", "froh", ",", "als", "wie", "sie", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da sie euch zur Welt gebar,", "tokens": ["da", "sie", "euch", "zur", "Welt", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie sie noch in euch gebiert,", "tokens": ["wie", "sie", "noch", "in", "euch", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "was euch keuschen Jungfern ziert.", "tokens": ["was", "euch", "keu\u00b7schen", "Jung\u00b7fern", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Euer sch\u00f6ner Name macht,", "tokens": ["Eu\u00b7er", "sch\u00f6\u00b7ner", "Na\u00b7me", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df sie mehr als fr\u00f6lich lacht.", "tokens": ["da\u00df", "sie", "mehr", "als", "fr\u00f6\u00b7lich", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lacht auch fr\u00f6lich, edle Zier!", "tokens": ["Lacht", "auch", "fr\u00f6\u00b7lich", ",", "ed\u00b7le", "Zier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Glanzes Glanz seid ihr.", "tokens": ["Ih\u00b7res", "Glan\u00b7zes", "Glanz", "seid", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unterdessen seht nicht an,", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "seht", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ich euch nichts schenken kan!", "tokens": ["da\u00df", "ich", "euch", "nichts", "schen\u00b7ken", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4ubt, was euer Herze spricht:", "tokens": ["Gl\u00e4ubt", ",", "was", "eu\u00b7er", "Her\u00b7ze", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe sieht auf Gaben nicht!", "tokens": ["Lie\u00b7be", "sieht", "auf", "Ga\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Di\u00df Band setz' ich bei euch ein,", "tokens": ["Di\u00df", "Band", "setz'", "ich", "bei", "euch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "da\u00df es soll ein Pfand nur sein.", "tokens": ["da\u00df", "es", "soll", "ein", "Pfand", "nur", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin wol versichert mir:", "tokens": ["Ich", "bin", "wol", "ver\u00b7si\u00b7chert", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Pfand und Band, das seid selbst ihr.", "tokens": ["Pfand", "und", "Band", ",", "das", "seid", "selbst", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PDS", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}