{"textgrid.poem.28657": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fusca ist zwar m\u00e4chtig sch\u00f6n, pfleget aber f\u00fcrzunehmen", "tokens": ["Fus\u00b7ca", "ist", "zwar", "m\u00e4ch\u00b7tig", "sch\u00f6n", ",", "pfle\u00b7get", "a\u00b7ber", "f\u00fcr\u00b7zu\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "ADJD", "$,", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sachen, die ihr sch\u00f6nes Wei\u00df ziemlich schw\u00e4rtzen und besch\u00e4men;", "tokens": ["Sa\u00b7chen", ",", "die", "ihr", "sch\u00f6\u00b7nes", "Wei\u00df", "ziem\u00b7lich", "schw\u00e4rt\u00b7zen", "und", "be\u00b7sch\u00e4\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sch\u00f6ner kummt ein sch\u00f6nes Bild, wann es steht in schwartzen R\u00e4men.", "tokens": ["Sch\u00f6\u00b7ner", "kummt", "ein", "sch\u00f6\u00b7nes", "Bild", ",", "wann", "es", "steht", "in", "schwart\u00b7zen", "R\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}