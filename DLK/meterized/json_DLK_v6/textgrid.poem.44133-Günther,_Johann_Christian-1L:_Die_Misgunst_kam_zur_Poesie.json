{"textgrid.poem.44133": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Misgunst kam zur Poesie", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Misgunst kam zur Poesie", "tokens": ["Die", "Mis\u00b7gunst", "kam", "zur", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sah ihr h\u00f6hnisch ins Gesichte:", "tokens": ["Und", "sah", "ihr", "h\u00f6h\u00b7nisch", "ins", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun, sprach sie, gieb dir weiter M\u00fch,", "tokens": ["Nun", ",", "sprach", "sie", ",", "gieb", "dir", "wei\u00b7ter", "M\u00fch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df deine Mariane dichte;", "tokens": ["Da\u00df", "dei\u00b7ne", "Ma\u00b7ri\u00b7a\u00b7ne", "dich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geh, stecke Brust und Sinnen an,", "tokens": ["Geh", ",", "ste\u00b7cke", "Brust", "und", "Sin\u00b7nen", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will die Flamme bald ersticken,", "tokens": ["Ich", "will", "die", "Flam\u00b7me", "bald", "er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dieweil ein Strahl von meinen Blicken", "tokens": ["Die\u00b7weil", "ein", "Strahl", "von", "mei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ganze Lust versalzen kan.", "tokens": ["Die", "gan\u00b7ze", "Lust", "ver\u00b7sal\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schweig, sprach die Poesie im Zorn,", "tokens": ["Schweig", ",", "sprach", "die", "Poe\u00b7sie", "im", "Zorn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wenn Frauen Wiz und Wei\u00dfheit zeigen,", "tokens": ["Wenn", "Frau\u00b7en", "Wiz", "und", "Wei\u00df\u00b7heit", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's M\u00e4nnern leicht ein Augendorn.", "tokens": ["Ist's", "M\u00e4n\u00b7nern", "leicht", "ein", "Au\u00b7gen\u00b7dorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was schadet Marianens Schweigen?", "tokens": ["Was", "scha\u00b7det", "Ma\u00b7ri\u00b7a\u00b7nens", "Schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich seh, man stiehlt mir ihre Gunst,", "tokens": ["Ich", "seh", ",", "man", "stiehlt", "mir", "ih\u00b7re", "Gunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie soll mich ha\u00dfen und verschw\u00f6ren;", "tokens": ["Sie", "soll", "mich", "ha\u00b7\u00dfen", "und", "ver\u00b7schw\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie thut es, doch mit Zwang und Z\u00e4hren,", "tokens": ["Sie", "thut", "es", ",", "doch", "mit", "Zwang", "und", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, Ehre gnug vor meine Kunst!", "tokens": ["Ach", ",", "Eh\u00b7re", "gnug", "vor", "mei\u00b7ne", "Kunst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}