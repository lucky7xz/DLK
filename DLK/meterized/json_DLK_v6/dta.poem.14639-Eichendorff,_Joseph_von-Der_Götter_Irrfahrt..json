{"dta.poem.14639": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, da hebt er Felsenspitzen", "tokens": ["Sieh", ",", "da", "hebt", "er", "Fel\u00b7sen\u00b7spit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Langsam aus der Wasser Grund,", "tokens": ["Lang\u00b7sam", "aus", "der", "Was\u00b7ser", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erschrocken aus den Ritzen", "tokens": ["Und", "er\u00b7schro\u00b7cken", "aus", "den", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schie\u00dfen schupp'ge Schlangen bunt;", "tokens": ["Schie\u00b7\u00dfen", "schupp'\u00b7ge", "Schlan\u00b7gen", "bunt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ringelnd Ungeth\u00fcm der Tiefen,", "tokens": ["Rin\u00b7gelnd", "Un\u00b7ge\u00b7th\u00fcm", "der", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im \u00f6den Wogen-Haus", "tokens": ["Die", "im", "\u00f6\u00b7den", "Wo\u00b7gen\u00b7Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der gr\u00fcnen D\u00e4mm'rung schliefen,", "tokens": ["In", "der", "gr\u00fc\u00b7nen", "D\u00e4m\u00b7m'\u00b7rung", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "St\u00fcrzen sich in's Meer hinaus.", "tokens": ["St\u00fcr\u00b7zen", "sich", "in's", "Meer", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}