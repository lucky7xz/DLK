{"dta.poem.2266": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anleitung  \n zum vergn\u00fcgten und gelassenen  \n Sterben.  \n Beym Anfange des 1747 Jahres entworfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Gott! du Wesen aller Wesen,", "tokens": ["O", "Gott", "!", "du", "We\u00b7sen", "al\u00b7ler", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das anders denkt, als alle Welt,", "tokens": ["Das", "an\u00b7ders", "denkt", ",", "als", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der anders ist, und anders wirkt, als je ein Geist sich", "tokens": ["Der", "an\u00b7ders", "ist", ",", "und", "an\u00b7ders", "wirkt", ",", "als", "je", "ein", "Geist", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "$,", "KON", "ADV", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "vorgestellt,", "tokens": ["vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der Geist und K\u00f6rper, Welt und Sonnen, zu seines Da-", "tokens": ["Der", "Geist", "und", "K\u00f6r\u00b7per", ",", "Welt", "und", "Son\u00b7nen", ",", "zu", "sei\u00b7nes", "Da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "seyns Prob', erlesen!", "tokens": ["seyns", "Prob'", ",", "er\u00b7le\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "O Gott! du lebst in deinen Werken, die Seele siehet dei-", "tokens": ["O", "Gott", "!", "du", "lebst", "in", "dei\u00b7nen", "Wer\u00b7ken", ",", "die", "See\u00b7le", "sie\u00b7het", "dei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "ne Spur", "tokens": ["ne", "Spur"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "In allen deinen Kreaturen, und in der wirkenden Natur.", "tokens": ["In", "al\u00b7len", "dei\u00b7nen", "Kre\u00b7a\u00b7tu\u00b7ren", ",", "und", "in", "der", "wir\u00b7ken\u00b7den", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+--+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Die Mittel dazu sind die Sinnen, wodurch wir, mit der", "tokens": ["Die", "Mit\u00b7tel", "da\u00b7zu", "sind", "die", "Sin\u00b7nen", ",", "wo\u00b7durch", "wir", ",", "mit", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PAV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "$,", "APPR", "ART"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.11": {"text": "Welt verbunden,", "tokens": ["Welt", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Durch die gel\u00e4uterte Vernunft, derselben Sch\u00f6pfer aus-", "tokens": ["Durch", "die", "ge\u00b7l\u00e4u\u00b7ter\u00b7te", "Ver\u00b7nunft", ",", "der\u00b7sel\u00b7ben", "Sch\u00f6p\u00b7fer", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "gefunden.", "tokens": ["ge\u00b7fun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Durch Ordnung, den Zusammenhang, durch Pracht und", "tokens": ["Durch", "Ord\u00b7nung", ",", "den", "Zu\u00b7sam\u00b7men\u00b7hang", ",", "durch", "Pracht", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Absicht wird der Grund,", "tokens": ["Ab\u00b7sicht", "wird", "der", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Ein weises, liebreichs, m\u00e4chtigs Wesen, vern\u00fcnftigen", "tokens": ["Ein", "wei\u00b7ses", ",", "lieb\u00b7reichs", ",", "m\u00e4ch\u00b7tigs", "We\u00b7sen", ",", "ver\u00b7n\u00fcnf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Gesch\u00f6pfen kund,", "tokens": ["Ge\u00b7sch\u00f6p\u00b7fen", "kund", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Die, wenn sie, Herr, von deinen Werken, von Welt und", "tokens": ["Die", ",", "wenn", "sie", ",", "Herr", ",", "von", "dei\u00b7nen", "Wer\u00b7ken", ",", "von", "Welt", "und"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Sinnen nichtes w\u00fc\u00dften,", "tokens": ["Sin\u00b7nen", "nich\u00b7tes", "w\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "In ewiger Unwissenheit von deinem Daseyn bleiben m\u00fc\u00dften.", "tokens": ["In", "e\u00b7wi\u00b7ger", "Un\u00b7wis\u00b7sen\u00b7heit", "von", "dei\u00b7nem", "Da\u00b7seyn", "blei\u00b7ben", "m\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.21": {"text": "Wie k\u00f6nnten immermehr Begriffe, Gedanken, Bilder und", "tokens": ["Wie", "k\u00f6nn\u00b7ten", "im\u00b7mer\u00b7mehr", "Be\u00b7grif\u00b7fe", ",", "Ge\u00b7dan\u00b7ken", ",", "Bil\u00b7der", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Jdeen", "tokens": ["Jdeen"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "In einer sinnen-k\u00f6rperlosen und leeren Menschenseel\u2019 ent-", "tokens": ["In", "ei\u00b7ner", "sin\u00b7nen\u00b7k\u00f6r\u00b7per\u00b7lo\u00b7sen", "und", "lee\u00b7ren", "Men\u00b7schen\u00b7seel'", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "stehen?", "tokens": ["ste\u00b7hen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Der kl\u00fcgste Geist m\u00fc\u00dft\u2019 ohne Kraft, ohn\u2019 Ueberlegung", "tokens": ["Der", "kl\u00fcgs\u00b7te", "Geist", "m\u00fc\u00dft'", "oh\u00b7ne", "Kraft", ",", "ohn'", "Ue\u00b7ber\u00b7le\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und Vergn\u00fcgen,", "tokens": ["und", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Ein nimmerdenkend Wesen bleiben, kein Geist seyn, ewig", "tokens": ["Ein", "nim\u00b7mer\u00b7den\u00b7kend", "We\u00b7sen", "blei\u00b7ben", ",", "kein", "Geist", "seyn", ",", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "PIAT", "NN", "VAINF", "$,", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "brache liegen.", "tokens": ["bra\u00b7che", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Mu\u00df eine Seele denn nicht billig den Werth, o Herr,", "tokens": ["Mu\u00df", "ei\u00b7ne", "See\u00b7le", "denn", "nicht", "bil\u00b7lig", "den", "Werth", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "von deinen Werken,", "tokens": ["von", "dei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Die deine Herrlichkeit uns zeigen, mit Andacht, Dank", "tokens": ["Die", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "uns", "zei\u00b7gen", ",", "mit", "An\u00b7dacht", ",", "Dank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVINF", "$,", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "und Ehrfurcht merken?", "tokens": ["und", "Ehr\u00b7furcht", "mer\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "Dein Schaffen, Formen und Erhalten, dein unbegreifli-", "tokens": ["Dein", "Schaf\u00b7fen", ",", "For\u00b7men", "und", "Er\u00b7hal\u00b7ten", ",", "dein", "un\u00b7be\u00b7greif\u00b7li"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "ches Regieren", "tokens": ["ches", "Re\u00b7gie\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.35": {"text": "So ungeheurer Himmelsk\u00f6rper, der Welt und aller Wesen", "tokens": ["So", "un\u00b7ge\u00b7heu\u00b7rer", "Him\u00b7mels\u00b7k\u00f6r\u00b7per", ",", "der", "Welt", "und", "al\u00b7ler", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.36": {"text": "sp\u00fcren,", "tokens": ["sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Bewundern und dich anzubeten, und zwar dich ewgen", "tokens": ["Be\u00b7wun\u00b7dern", "und", "dich", "an\u00b7zu\u00b7be\u00b7ten", ",", "und", "zwar", "dich", "ew\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "PPER", "VVIZU", "$,", "KON", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Gott allein", "tokens": ["Gott", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Tief unterw\u00fcrfig zu verehren, gereizet und verbunden seyn?", "tokens": ["Tief", "un\u00b7ter\u00b7w\u00fcr\u00b7fig", "zu", "ver\u00b7eh\u00b7ren", ",", "ge\u00b7rei\u00b7zet", "und", "ver\u00b7bun\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKZU", "VVFIN", "$,", "VVPP", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.40": {"text": "Du rufst Gesch\u00f6pfen, und sie kommen; du sprichst: ver-", "tokens": ["Du", "rufst", "Ge\u00b7sch\u00f6p\u00b7fen", ",", "und", "sie", "kom\u00b7men", ";", "du", "sprichst", ":", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "PPER", "VVINF", "$.", "PPER", "VVFIN", "$.", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "geht, und sie vergehen,", "tokens": ["geht", ",", "und", "sie", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.42": {"text": "Da andre denn, auf deinen Wink, aufs neu, an ihrer", "tokens": ["Da", "and\u00b7re", "denn", ",", "auf", "dei\u00b7nen", "Wink", ",", "aufs", "neu", ",", "an", "ih\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJD", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Stell', entstehen.", "tokens": ["Stell'", ",", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.44": {"text": "Wobey jedoch vern\u00fcnftge Wesen auf deine Liebe sich ver-", "tokens": ["Wo\u00b7bey", "je\u00b7doch", "ver\u00b7n\u00fcnft\u00b7ge", "We\u00b7sen", "auf", "dei\u00b7ne", "Lie\u00b7be", "sich", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.45": {"text": "lassen,", "tokens": ["las\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.46": {"text": "Und, da\u00df sie nicht vernichtigt werden, zu deiner Ehr\u2019 im", "tokens": ["Und", ",", "da\u00df", "sie", "nicht", "ver\u00b7nich\u00b7tigt", "wer\u00b7den", ",", "zu", "dei\u00b7ner", "Ehr'", "im"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "VAINF", "$,", "APPR", "PPOSAT", "NN", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.47": {"text": "Glauben fassen.", "tokens": ["Glau\u00b7ben", "fas\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.48": {"text": "Sie sehen die Zerst\u00f6rlichkeit der K\u00f6rper, und ihr Aendern an", "tokens": ["Sie", "se\u00b7hen", "die", "Zer\u00b7st\u00f6r\u00b7lich\u00b7keit", "der", "K\u00f6r\u00b7per", ",", "und", "ihr", "A\u00b7en\u00b7dern", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.49": {"text": "Als Folgen deiner weisen Ordnung, doch die den Geist", "tokens": ["Als", "Fol\u00b7gen", "dei\u00b7ner", "wei\u00b7sen", "Ord\u00b7nung", ",", "doch", "die", "den", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ART", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "nicht treffen kann.", "tokens": ["nicht", "tref\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.51": {"text": "O gro\u00dfer Trost, in dem wir sterben, (da wir doch alle", "tokens": ["O", "gro\u00b7\u00dfer", "Trost", ",", "in", "dem", "wir", "ster\u00b7ben", ",", "(", "da", "wir", "doch", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word