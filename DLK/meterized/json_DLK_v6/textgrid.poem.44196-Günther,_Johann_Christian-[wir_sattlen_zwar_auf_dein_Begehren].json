{"textgrid.poem.44196": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[wir sattlen zwar auf dein Begehren]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sattlen zwar auf dein Begehren,", "tokens": ["Wir", "satt\u00b7len", "zwar", "auf", "dein", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gelehrter Freund, den Pegasus", "tokens": ["Ge\u00b7lehr\u00b7ter", "Freund", ",", "den", "Pe\u00b7ga\u00b7sus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollen dir die Pflicht gew\u00e4hren,", "tokens": ["Und", "wol\u00b7len", "dir", "die", "Pflicht", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ohnedem nicht schweigen mu\u00df.", "tokens": ["Die", "oh\u00b7ne\u00b7dem", "nicht", "schwei\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch las dich um Verzeihung bitten,", "tokens": ["Doch", "las", "dich", "um", "Ver\u00b7zei\u00b7hung", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wofern er schlechte Spr\u00fcnge macht,", "tokens": ["Wo\u00b7fern", "er", "schlech\u00b7te", "Spr\u00fcn\u00b7ge", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man hat ihn gleich zusamt dem Schlitten", "tokens": ["Man", "hat", "ihn", "gleich", "zu\u00b7samt", "dem", "Schlit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Gerberwa\u00dfer heimgebracht.", "tokens": ["Vom", "Ger\u00b7ber\u00b7wa\u00b7\u00dfer", "heim\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er drabt doch noch. Nun, so geschwinde", "tokens": ["Er", "drabt", "doch", "noch", ".", "Nun", ",", "so", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "ADV", "$,", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt wohl kein Doctor an den Rang", "tokens": ["Kommt", "wohl", "kein", "Doc\u00b7tor", "an", "den", "Rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keine Jungemagd zum Kinde", "tokens": ["Und", "kei\u00b7ne", "Jun\u00b7ge\u00b7magd", "zum", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als unser Klepper in den Gang;", "tokens": ["Als", "un\u00b7ser", "Klep\u00b7per", "in", "den", "Gang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns giebt die Lustfahrt auf dem Ei\u00dfe", "tokens": ["Uns", "giebt", "die", "Lust\u00b7fahrt", "auf", "dem", "Ei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Reimes Stof und Hauptwerck ein,", "tokens": ["Des", "Rei\u00b7mes", "Stof", "und", "Haupt\u00b7werck", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum wollen wir, doch stummer Weise,", "tokens": ["Drum", "wol\u00b7len", "wir", ",", "doch", "stum\u00b7mer", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Pursche Zeitvertreib beschreyn.", "tokens": ["Der", "Pur\u00b7sche", "Zeit\u00b7ver\u00b7treib", "be\u00b7schreyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gesellschaft, Lehrer, Geld, Patronen,", "tokens": ["Ge\u00b7sell\u00b7schaft", ",", "Leh\u00b7rer", ",", "Geld", ",", "Pat\u00b7ro\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Land, Auferziehung, Leib und Zeit", "tokens": ["Land", ",", "Auf\u00b7er\u00b7zie\u00b7hung", ",", "Leib", "und", "Zeit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht warlich unter den Personen", "tokens": ["Macht", "war\u00b7lich", "un\u00b7ter", "den", "Per\u00b7so\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ihrer Lust viel Unterscheid.", "tokens": ["Und", "ih\u00b7rer", "Lust", "viel", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Regel gilt auf hohen Schulen:", "tokens": ["Die", "Re\u00b7gel", "gilt", "auf", "ho\u00b7hen", "Schu\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So mancher Kopf, so mancher Sinn,", "tokens": ["So", "man\u00b7cher", "Kopf", ",", "so", "man\u00b7cher", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So manches Garn, so manche Spulen,", "tokens": ["So", "man\u00b7ches", "Garn", ",", "so", "man\u00b7che", "Spu\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So mancher Bart, so manches Kinn.", "tokens": ["So", "man\u00b7cher", "Bart", ",", "so", "man\u00b7ches", "Kinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der eine will die Sprache fa\u00dfen", "tokens": ["Der", "ei\u00b7ne", "will", "die", "Spra\u00b7che", "fa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und exhauriret das Latein,", "tokens": ["Und", "ex\u00b7hau\u00b7ri\u00b7ret", "das", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Bis die von ihm bewegten Ga\u00dfen", "tokens": ["Bis", "die", "von", "ihm", "be\u00b7weg\u00b7ten", "Ga\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des guten Kopfes Zeugen seyn;", "tokens": ["Des", "gu\u00b7ten", "Kop\u00b7fes", "Zeu\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der conjugirt beym vollen Glase", "tokens": ["Der", "con\u00b7ju\u00b7girt", "beym", "vol\u00b7len", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das deutsche ", "tokens": ["Das", "deut\u00b7sche"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und jenem trichtert eine Laase", "tokens": ["Und", "je\u00b7nem", "trich\u00b7tert", "ei\u00b7ne", "Laa\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Bieres Wei\u00dfheit in den Bauch.", "tokens": ["Des", "Bie\u00b7res", "Wei\u00df\u00b7heit", "in", "den", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der sucht die Feder nett zu f\u00fchren", "tokens": ["Der", "sucht", "die", "Fe\u00b7der", "nett", "zu", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schreibt, wie folgt, an sein ", "tokens": ["Und", "schreibt", ",", "wie", "folgt", ",", "an", "sein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VVFIN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will den ", "tokens": ["Ich", "will", "den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und lege mich darauf gar sehr;", "tokens": ["Und", "le\u00b7ge", "mich", "da\u00b7rauf", "gar", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun weis ich wohl, da\u00df ihn ", "tokens": ["Nun", "weis", "ich", "wohl", ",", "da\u00df", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKVZ", "PPER", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Recht rein und flie\u00dfend lehren kan,", "tokens": ["Recht", "rein", "und", "flie\u00b7\u00dfend", "leh\u00b7ren", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum sprech ich sie nun ", "tokens": ["Drum", "sprech", "ich", "sie", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Um einen guten Handgrif an.", "tokens": ["Um", "ei\u00b7nen", "gu\u00b7ten", "Hand\u00b7grif", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die andern lieben die Geschichte", "tokens": ["Die", "an\u00b7dern", "lie\u00b7ben", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaun sich nach Talandern um,", "tokens": ["Und", "schaun", "sich", "nach", "Ta\u00b7lan\u00b7dern", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Menant verderbet ihr Gesichte,", "tokens": ["Me\u00b7nant", "ver\u00b7der\u00b7bet", "ihr", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sizen sich beym Ziegler krumm", "tokens": ["Sie", "si\u00b7zen", "sich", "beym", "Zieg\u00b7ler", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sind darinnen so beschlagen,", "tokens": ["Und", "sind", "da\u00b7rin\u00b7nen", "so", "be\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df, wenn man nur vom Niesen spricht,", "tokens": ["Da\u00df", ",", "wenn", "man", "nur", "vom", "Nie\u00b7sen", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So k\u00f6nnen sie uns hurtig sagen,", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "uns", "hur\u00b7tig", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie viel in Pegu Mord geschicht.", "tokens": ["Wie", "viel", "in", "Pe\u00b7gu", "Mord", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dem dritten schreibt des Vaters Willen:", "tokens": ["Dem", "drit\u00b7ten", "schreibt", "des", "Va\u00b7ters", "Wil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sohn, treibe die Geographie!", "tokens": ["Sohn", ",", "trei\u00b7be", "die", "Geo\u00b7gra\u00b7phie", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der Sohn ist fl\u00fcchtig zum Erf\u00fcllen,", "tokens": ["Der", "Sohn", "ist", "fl\u00fcch\u00b7tig", "zum", "Er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zieht Sporn und Stiefeln an die Knie", "tokens": ["Zieht", "Sporn", "und", "Stie\u00b7feln", "an", "die", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nimmt ein Duzend Reisebr\u00fcder", "tokens": ["Und", "nimmt", "ein", "Du\u00b7zend", "Rei\u00b7se\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht nach Stettriz auf das Land", "tokens": ["Und", "zieht", "nach", "Stet\u00b7triz", "auf", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schreibet aus der Schencke wieder:", "tokens": ["Und", "schrei\u00b7bet", "aus", "der", "Schen\u00b7cke", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich mache mir die Welt bekand.", "tokens": ["Ich", "ma\u00b7che", "mir", "die", "Welt", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie geht es in den Facult\u00e4ten?", "tokens": ["Wie", "geht", "es", "in", "den", "Fa\u00b7cul\u00b7t\u00e4\u00b7ten", 