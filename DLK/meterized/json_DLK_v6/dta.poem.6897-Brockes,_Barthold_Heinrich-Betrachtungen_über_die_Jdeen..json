{"dta.poem.6897": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denn die Jdeen sind in unsrer Seele,", "tokens": ["Denn", "die", "Jdeen", "sind", "in", "uns\u00b7rer", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und zwar in selbiger viel eher noch formirt,", "tokens": ["Und", "zwar", "in", "sel\u00b7bi\u00b7ger", "viel", "e\u00b7her", "noch", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bevor ein Strich, ein Druck, vom C\u00f6rper sie ber\u00fchrt.", "tokens": ["Be\u00b7vor", "ein", "Strich", ",", "ein", "Druck", ",", "vom", "C\u00f6r\u00b7per", "sie", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es sind gantz unterschiedne Sachen.", "tokens": ["Es", "sind", "gantz", "un\u00b7ter\u00b7schied\u00b7ne", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Gegenw\u00fcrff\u2019 uns einen Eindruck machen;", "tokens": ["Wenn", "Ge\u00b7gen\u00b7w\u00fcrff'", "uns", "ei\u00b7nen", "Ein\u00b7druck", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Empf\u00e4ngt man ihrer Bilder Heer,", "tokens": ["Emp\u00b7f\u00e4ngt", "man", "ih\u00b7rer", "Bil\u00b7der", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf eine Tafel, welche leer:", "tokens": ["Auf", "ei\u00b7ne", "Ta\u00b7fel", ",", "wel\u00b7che", "leer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hierdurch ist aber uns noch vom Verstand", "tokens": ["Hier\u00b7durch", "ist", "a\u00b7ber", "uns", "noch", "vom", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nicht die geringste Spur bekannt.", "tokens": ["Nicht", "die", "ge\u00b7rings\u00b7te", "Spur", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wodurch erkennet man in der Empfindlichkeit", "tokens": ["Wo\u00b7durch", "er\u00b7ken\u00b7net", "man", "in", "der", "Emp\u00b7find\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So unterschiedner Ding so grossen Unterscheid?", "tokens": ["So", "un\u00b7ter\u00b7schied\u00b7ner", "Ding", "so", "gros\u00b7sen", "Un\u00b7ter\u00b7scheid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn unser Werckzeug blos ger\u00fchrt ist durchs Bewegen;", "tokens": ["Wenn", "un\u00b7ser", "Wer\u00b7ck\u00b7zeug", "blos", "ge\u00b7r\u00fchrt", "ist", "durchs", "Be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "So sp\u00fcret man, wie sich zum Schlu\u00df und Uberlegen", "tokens": ["So", "sp\u00fc\u00b7ret", "man", ",", "wie", "sich", "zum", "Schlu\u00df", "und", "U\u00b7ber\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "PRF", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verschiedene Jdeen in uns regen:", "tokens": ["Ver\u00b7schie\u00b7de\u00b7ne", "Jdeen", "in", "uns", "re\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Wovon die ersten blos in unserm Geist gebohren:", "tokens": ["Wo\u00b7von", "die", "ers\u00b7ten", "blos", "in", "un\u00b7serm", "Geist", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die, f\u00fcr vern\u00fcnfftige Gesch\u00f6pffe nur erkohren,", "tokens": ["Die", ",", "f\u00fcr", "ver\u00b7n\u00fcnff\u00b7ti\u00b7ge", "Ge\u00b7sch\u00f6pf\u00b7fe", "nur", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sich immer an und bey einander finden,", "tokens": ["Sich", "im\u00b7mer", "an", "und", "bey", "ein\u00b7an\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKVZ", "KON", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Jedoch sich nie mit unsern Sinnen binden.", "tokens": ["Je\u00b7doch", "sich", "nie", "mit", "un\u00b7sern", "Sin\u00b7nen", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}