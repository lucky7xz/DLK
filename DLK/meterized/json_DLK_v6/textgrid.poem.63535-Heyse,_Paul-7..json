{"textgrid.poem.63535": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnt' ich dir gefallen,", "tokens": ["O", "k\u00f6nnt'", "ich", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meiner Seele Traum!", "tokens": ["Du", "mei\u00b7ner", "See\u00b7le", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00e4hlte dich von allen,", "tokens": ["Ich", "w\u00e4hl\u00b7te", "dich", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du aber kennst mich kaum.", "tokens": ["Du", "a\u00b7ber", "kennst", "mich", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel holde Fraun auf Erden gehn,", "tokens": ["Viel", "hol\u00b7de", "Fraun", "auf", "Er\u00b7den", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin nicht schmuck, ich bin nicht sch\u00f6n \u2013", "tokens": ["Ich", "bin", "nicht", "schmuck", ",", "ich", "bin", "nicht", "sch\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie k\u00f6nnt' ich dir gefallen,", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du meiner Seele Traum!", "tokens": ["Du", "mei\u00b7ner", "See\u00b7le", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zum k\u00fchlen Grund ich gehe,", "tokens": ["Zum", "k\u00fch\u00b7len", "Grund", "ich", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht eine Linde gr\u00fcn.", "tokens": ["Da", "steht", "ei\u00b7ne", "Lin\u00b7de", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am Stamm wohl in die H\u00f6he", "tokens": ["Am", "Stamm", "wohl", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wilden Rosen bl\u00fchn.", "tokens": ["Die", "wil\u00b7den", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Schattennacht vereint die Zwei \u2013", "tokens": ["In", "Schat\u00b7ten\u00b7nacht", "ver\u00b7eint", "die", "Zwei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich denke dein und mein dabei!", "tokens": ["Ich", "den\u00b7ke", "dein", "und", "mein", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "KON", "PPOSAT", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum k\u00fchlen Grund ich gehe,", "tokens": ["Zum", "k\u00fch\u00b7len", "Grund", "ich", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da steht eine Linde gr\u00fcn.", "tokens": ["Da", "steht", "ei\u00b7ne", "Lin\u00b7de", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Zusammen bl\u00fchn und sterben,", "tokens": ["Zu\u00b7sam\u00b7men", "bl\u00fchn", "und", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was kann wohl s\u00fc\u00dfer sein?", "tokens": ["Was", "kann", "wohl", "s\u00fc\u00b7\u00dfer", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcrft' ich dies Los erwerben,", "tokens": ["D\u00fcrft'", "ich", "dies", "Los", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich lachte jeder Pein.", "tokens": ["Ich", "lach\u00b7te", "je\u00b7der", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und tr\u00e4fe dich ein Wetterstrahl,", "tokens": ["Und", "tr\u00e4\u00b7fe", "dich", "ein", "Wet\u00b7ter\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verloderten wir Zwei zumal \u2013", "tokens": ["Ver\u00b7lo\u00b7der\u00b7ten", "wir", "Zwei", "zu\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "KOUS", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Zusammen bl\u00fchn und sterben,", "tokens": ["Zu\u00b7sam\u00b7men", "bl\u00fchn", "und", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was kann wohl s\u00fc\u00dfer sein!", "tokens": ["Was", "kann", "wohl", "s\u00fc\u00b7\u00dfer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}