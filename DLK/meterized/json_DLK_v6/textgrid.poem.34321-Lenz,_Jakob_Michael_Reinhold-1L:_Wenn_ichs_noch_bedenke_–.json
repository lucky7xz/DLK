{"textgrid.poem.34321": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ichs noch bedenke \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ichs noch bedenke \u2013", "tokens": ["Wenn", "ichs", "noch", "be\u00b7den\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf der langen Seereis' \u2013 \u00fcberall \u2013", "tokens": ["Auf", "der", "lan\u00b7gen", "See\u00b7reis'", "\u2013", "\u00fc\u00b7be\u00b7rall", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die Luft so feucht war, gab sie Wein,", "tokens": ["Wo", "die", "Luft", "so", "feucht", "war", ",", "gab", "sie", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auf Madera, an dem frohen Cap \u2013", "tokens": ["Auf", "Ma\u00b7de\u00b7ra", ",", "an", "dem", "fro\u00b7hen", "Cap", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo sie scharf war, wuchsen Cokusn\u00fcsse \u2013", "tokens": ["Wo", "sie", "scharf", "war", ",", "wuch\u00b7sen", "Co\u00b7kus\u00b7n\u00fcs\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo es kalt war, fl\u00f6zte sie uns Holz zu.", "tokens": ["Wo", "es", "kalt", "war", ",", "fl\u00f6z\u00b7te", "sie", "uns", "Holz", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Riesen sahen wir, wie David,", "tokens": ["Rie\u00b7sen", "sa\u00b7hen", "wir", ",", "wie", "Da\u00b7vid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und bezwungen sie mit kleinen Steinchen;", "tokens": ["Und", "be\u00b7zwun\u00b7gen", "sie", "mit", "klei\u00b7nen", "Stein\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wilde Teufel sahen wir, sie sangen", "tokens": ["Wil\u00b7de", "Teu\u00b7fel", "sa\u00b7hen", "wir", ",", "sie", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Uns die kauderwelschen Friedenslieder", "tokens": ["Uns", "die", "kau\u00b7der\u00b7wel\u00b7schen", "Frie\u00b7dens\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Da\u00df wir ihrer Gutheit lachten \u2013", "tokens": ["Da\u00df", "wir", "ih\u00b7rer", "Gut\u00b7heit", "lach\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ach! wohlth\u00e4tige Natur!", "tokens": ["Ach", "!", "wohlt\u00b7h\u00e4\u00b7ti\u00b7ge", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ost- und Westw\u00e4rts \u2013 Ueberall!", "tokens": ["Ost", "und", "West\u00b7w\u00e4rts", "\u2013", "Ue\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Sieh! die\u00df lezte Scheitchen Holz", "tokens": ["Sieh", "!", "die\u00df", "lez\u00b7te", "Scheit\u00b7chen", "Holz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PDS", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Leg ich auf \u2013 Sein Rauch verschwindet", "tokens": ["Leg", "ich", "auf", "\u2013", "Sein", "Rauch", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "$(", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "In die Luft \u2013 und Niemand meldet sich \u2013 \u2013", "tokens": ["In", "die", "Luft", "\u2013", "und", "Nie\u00b7mand", "mel\u00b7det", "sich", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "PIS", "VVFIN", "PRF", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Allbedenkende Natur!", "tokens": ["All\u00b7be\u00b7den\u00b7ken\u00b7de", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.18": {"text": "Hast du mich vergessen?", "tokens": ["Hast", "du", "mich", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}