{"textgrid.poem.35184": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Oberfl\u00e4chlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk nicht, das Leben sei ein Spiel!", "tokens": ["Denk", "nicht", ",", "das", "Le\u00b7ben", "sei", "ein", "Spiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es meints gar ernst, ja, mehr als ernst.", "tokens": ["Es", "meints", "gar", "ernst", ",", "ja", ",", "mehr", "als", "ernst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PTKANT", "$,", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erforsche seinen Zweck, sein Ziel,", "tokens": ["Er\u00b7for\u00b7sche", "sei\u00b7nen", "Zweck", ",", "sein", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit du es begreifen lernst!", "tokens": ["Da\u00b7mit", "du", "es", "be\u00b7grei\u00b7fen", "lernst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gehst behaglich hier spazieren,", "tokens": ["Du", "gehst", "be\u00b7hag\u00b7lich", "hier", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Machst dirs so viel wie m\u00f6glich leicht", "tokens": ["Machst", "dirs", "so", "viel", "wie", "m\u00f6g\u00b7lich", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "KOKOM", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und glaubst was wunder zu verlieren,", "tokens": ["Und", "glaubst", "was", "wun\u00b7der", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sich ein Tag nicht folgsam zeigt.", "tokens": ["Wenn", "sich", "ein", "Tag", "nicht", "folg\u00b7sam", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und brauchst du irgend welche Sorgen,", "tokens": ["Und", "brauchst", "du", "ir\u00b7gend", "wel\u00b7che", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So mu\u00df die Erde sie dir borgen.", "tokens": ["So", "mu\u00df", "die", "Er\u00b7de", "sie", "dir", "bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du gehst auf einem weiten Moor,", "tokens": ["Du", "gehst", "auf", "ei\u00b7nem", "wei\u00b7ten", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das du wohl fest und sicher nennst,", "tokens": ["Das", "du", "wohl", "fest", "und", "si\u00b7cher", "nennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKVZ", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur weil du seinen Blumenflor", "tokens": ["Nur", "weil", "du", "sei\u00b7nen", "Blu\u00b7men\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht als zum Sumpf geh\u00f6rig kennst.", "tokens": ["Nicht", "als", "zum", "Sumpf", "ge\u00b7h\u00f6\u00b7rig", "kennst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sollst hin\u00fcber, sollst dich retten", "tokens": ["Du", "sollst", "hin\u00b7\u00fc\u00b7ber", ",", "sollst", "dich", "ret\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und bist verloren, bleibst du stehn;", "tokens": ["Und", "bist", "ver\u00b7lo\u00b7ren", ",", "bleibst", "du", "stehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wirst du gehalten von den Kletten,", "tokens": ["Wirst", "du", "ge\u00b7hal\u00b7ten", "von", "den", "Klet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sinkst du ein, mu\u00dft untergehn.", "tokens": ["So", "sinkst", "du", "ein", ",", "mu\u00dft", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und zieht dich das Verderben nieder,", "tokens": ["Und", "zieht", "dich", "das", "Ver\u00b7der\u00b7ben", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So giebt es dich dann niemals wieder.", "tokens": ["So", "giebt", "es", "dich", "dann", "nie\u00b7mals", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denk nicht, das Leben sei ein Spiel;", "tokens": ["Denk", "nicht", ",", "das", "Le\u00b7ben", "sei", "ein", "Spiel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist die Rettung vor dem Tod,", "tokens": ["Es", "ist", "die", "Ret\u00b7tung", "vor", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schritt um Schritt, bis an das Ziel", "tokens": ["Der", "Schritt", "um", "Schritt", ",", "bis", "an", "das", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stets unter deinen F\u00fc\u00dfen droht.", "tokens": ["Stets", "un\u00b7ter", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gehst dar\u00fcber, t\u00e4glich, st\u00fcndlich", "tokens": ["Du", "gehst", "da\u00b7r\u00fc\u00b7ber", ",", "t\u00e4g\u00b7lich", ",", "st\u00fcnd\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und siehst es nicht, wie tief es ist;", "tokens": ["Und", "siehst", "es", "nicht", ",", "wie", "tief", "es", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist ja grad so unergr\u00fcndlich,", "tokens": ["Es", "ist", "ja", "grad", "so", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil du so oberfl\u00e4chlich bist.", "tokens": ["Weil", "du", "so", "o\u00b7berf\u00b7l\u00e4ch\u00b7lich", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O, denke tiefer dich ins Leben,", "tokens": ["O", ",", "den\u00b7ke", "tie\u00b7fer", "dich", "ins", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dann kanns f\u00fcr dich noch Rettung geben!", "tokens": ["Dann", "kanns", "f\u00fcr", "dich", "noch", "Ret\u00b7tung", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}